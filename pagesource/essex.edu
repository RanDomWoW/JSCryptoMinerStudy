    margin: 0;
    padding: 20px;
    padding-left: 115px;
    min-height: 60px;
    color: #444;
    background: #eeeeee;
}


/*---Start of CSS editing-------------*/
/* top menu styling */
#pe-top-bar .menu>li>a {
    padding: 10px 0px 0px 0px;
    margin: 0px 0px 0px 0px;
    height: 50px;
    line-height: 30px;
    border: none;
    position: relative;
    font-weight: 700;
}

#pe-top-bar {
    line-height: 40px;
    font-size: 12px;
    background: #fff;
    color: #000;
    border-bottom: 1px solid #e3e3e3;
    font-family: 'Droid Sans', sans-serif;
}

.menu li a {
    display: block;
    line-height: 1.1;
    font-weight: 600;
    padding: 0px 0px 0px 0px;
    color: inherit;
    margin: 0px 0px 0px 0px;

}

/*---Start of CSS editing-------------*/
/* social icons */
#pe-footer .pe-social-icons span.fa {
    background: #ffffff;
}

/*---Start of CSS editing-------------*/
/* right sidebar menu */
.menu li a {
    display: block;
    line-height: 1.9;
    font-weight: 500;
    padding: 4px 0px 4px 0px;
    color: #444444;
    margin: 0px 0px 0px 0px;
}

.menu>li>a {
    border-top: 1px dashed #fdb913;
}

/*---Start of CSS editing-------------*/
/* Page Header styling */

.page-header>h1/*, .page-header>h2*/ {
    display: block;
    color: #e9a707;
    font-family: Droid Sans,Arial,Helvetica,sans-serif;
    font-weight: 700;
    font-size: 30px;
    letter-spacing: normal;
    text-transform: none;
    line-height: 1.1;
    margin: 0;
}

