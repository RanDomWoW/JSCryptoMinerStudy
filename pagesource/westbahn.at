			'th:not(.skip-highlight), td:not(.skip-highlight)',
			'mouseenter mouseleave',
			function(e)
			{
				var $this = $(this);
				var index = $this.index();
				var $parent = $this.parent(); // tr
				var $column = $('th, td', $table).filter(':nth-child(' + (index + 1) + ')'); // column
				var $activeColumn = $('thead th', $table).eq(index); // first th in column
				var $activeRow = $('td', $parent).eq(0); // first td in row
				var action = e.type == 'mouseenter' ? 'addClass' : 'removeClass';

				$parent[action]('highlight');
				$column[action]('highlight');
				$this[action]('active');
				$activeColumn[action]('active');
				$activeRow[action]('active');
			}
		);
	});
