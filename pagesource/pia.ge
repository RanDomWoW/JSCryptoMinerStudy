	<script>

		$(document).ready(function(){

			$.fn.ajaxlink();
			$.fn.ajaxform();
			var y = 1;
			window.onscroll = function(ev) {

			    if ((window.innerHeight + window.scrollY) >= document.documentElement.offsetHeight) {
			        if($('button#load_more').length) $('button#load_more').trigger("click");
			        if($('button#load_more2').length) $('button#load_more2').trigger("click");
			    }
			}


			// LIVE
			var dataString = '';

			
			$.ajax({
				type: "GET",
