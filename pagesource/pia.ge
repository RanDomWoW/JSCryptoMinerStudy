				cache: false,
				data: dataString,
				success: function (html) {
				
				}
			});
			// END LIVE

			

			$(window).on('load', function(){
				//$.removeCookie('mobile-banner');
				if(!$.cookie('mobile-banner') && $(window).width() < 700 && $('.mobile-banner ins a').length){
					$('#revese-countdown')
					  .prop('number', 4)
					  .animateNumber(
					    {
					      number: 0,
					      numberStep: function(now, tween) {
					        var target = $(tween.elem),
					            rounded_now = Math.round(now);

					        target.text(now === tween.end ? blurOut() : rounded_now);
					      }
					    },
					    4000,
					    'linear'
				  	);

					blurIn();
					$('.mobile-banner').show();
					$.cookie('mobile-banner',1);
				} 

				if($.cookie('live-embed') && $('#live-embed').length) {
					//$('#live-embed').remove();
				} else {
					$('#live-embed').fadeIn();
				}


				if($('.640-banner ins a').length || $('.640-banner ins iframe').length){
					$('.640-banner').fadeIn();
				} 
	
			});

			$('.close-slide-banner').click(function(){
				$('.slide-banner').css('left', '-400px');
				$.cookie('pia_400_250', 1, { expires : 1 });
			});

			$("#informer-block-mobile-toggle").click(function() {
				if ($("#informer-block-mobile").hasClass('opened')) {
					$("#informer-block-mobile").addClass("closed");
					$("#informer-block-mobile").removeClass("opened");
					$("#informer-block-mobile").find("#informer-block-mobile-toggle span i").attr("class", "fa fa-angle-up");
				} else {
					$("#informer-block-mobile").addClass("opened");
					$("#informer-block-mobile").removeClass("closed");
					$("#informer-block-mobile").find("#informer-block-mobile-toggle span i").attr("class", "fa fa-angle-down");
				}
			});


			window.onload = function(){
				$('#loader').hide();
			};
			window.onbeforeunload = function() {
				$('#loader').show();
			};




			var sidebar = $("#sidebar");
			var sidebar_offset_left = sidebar.offset().left;
			var footerHeight = document.querySelector('footer').offsetHeight;

			var scrolled = false;


		    $(window).scroll(function() {

		    	var scrollBottom = $(document).height() - $(window).height() - $(window).scrollTop();

		    	var whenToFix = sidebar.height() - $(this).scrollTop() - $(window).height() + sidebar.offset().top;
		    	var debugg = sidebar.offset().top+'-'+sidebar.height()+'-'+$(window).height()+'-'+$(this).scrollTop();

		    	sidebar.attr('data-offsettop', whenToFix);

		    	if (whenToFix <= 0) {
		    		sidebar.addClass('fixed');
		    		sidebar.css('left', sidebar_offset_left);
		    	}

				if (sidebar.offset().top < 220){
		    		sidebar.removeClass('fixed');
		    		sidebar.css('left', 'auto');
		    	}

		    	if (scrollBottom <= footerHeight - 5) {
		    		sidebar.addClass('absoluted');
		    		//$(".footer1").css('bottom', '50px');
		    	} else {
		    		sidebar.removeClass('absoluted');
		    		//$(".footer1").css('bottom', '-250px');
		    	}

				if ($(this).scrollTop() > 200){
					
					$('header').addClass("scrolled");
					$('.site-nav.scrolled').addClass("show");

					var scrollspeed = parseInt(2.5),
					offset = $(this).scrollTop() / scrollspeed;
					//sidebar.css('transform',   'translateY(' + offset + 'px)');


				} else {
					$('header').removeClass("scrolled");
					$('.site-nav.scrolled').removeClass("show");

				}

				if($(this).scrollTop() > 100 && scrolled === false){
                    scrolled = true;
                    $('.verticals').addClass('opened');
                    $('.verticals').removeClass('closed');
                    $('#informer-block-toggle').find('span').html('<i class="fa fa-angle-down"></i>');
                    $('#informer-block-mobile-toggle').find('span').html('<i class="fa fa-angle-down"></i>');
                    //alert(scrolled);
                }else if($(this).scrollTop() < 100){
                    $('.verticals').addClass('closed');
                    $('.verticals').removeClass('opened');
                    $('#informer-block-toggle').find('span').html('<i class="fa fa-angle-up"></i>');
                    $('#informer-block-mobile-toggle').find('span').html('<i class="fa fa-angle-up"></i>');
                    scrolled = false;
                }

                if($('.slide-banner ins a').length || $('.slide-banner ins img').length || $('.slide-banner ins iframe').length) {
	                if ($(this).scrollTop() > 300){
						if(!$.cookie('pia_400_250')){
							$('.slide-banner').css('left', '15px');
						}
					}else{
						$('.slide-banner').css('left', '-400px');
					}
				}

			});
			

		    if ($(window).width() < 700) {
			   $('.desktop').remove();
			}


        });

	</script>



