    <script>
         $(window).load(function() {
            // gotop		
            $(".gotop").hide();
                $(function () {
                    $(window).scroll(function () {
                        if ($(this).scrollTop() > ($("#site-body").offset().top)) {
                            $('.gotop').fadeIn();
                        } else {
                            $('.gotop').fadeOut();
                    }
                });
                
                $('body span.gotop').click(function () {
                    $('body,html').animate({
                        scrollTop: $("#site-body").offset().top
                    }, 900);
                    return false;
                });
            });            
        });


         $(document).ready(function () {
        
            $('#nav-bar li .drop-ico').each(function(){
                if($(this).next('.nav-sub').find('a').length==0){
                    $(this).hide();	
                }	
            });
            $('.list-focus').bxSlider({
                touchEnabled:false,
                minSlides:5,
                maxSlides:5,
                slideWidth: 192,
                infiniteLoop:false

            });
        
            $('.tin-tuc ul.older-news').each(function(){
                $(this).find('li:eq(1)').addClass('second');
            });
            $('.ban-doc ul.older-news').each(function(){
                $(this).find('li:eq(1)').addClass('second');
                $(this).find('li:eq(2)').addClass('third');
                $(this).find('li:eq(3)').addClass('forth');
            });

            function insertClear() {
                var divClear = '<div class="clearfix"></div>';                    // Create element with HTML  
                
                $(".ban-doc ul.older-news li.third").after(divClear);          // Insert new elements after <img>
            }

            $('.phap-luat ul.older-news').each(function(){
                $(this).find('li:eq(2)').addClass('third');
               
            });
            $('.older-video .list4-hr').each(function(){
                $(this).find('li:eq(4)').addClass('fifth');
               
            });
            $('.cat-menu').each(function(){
                if($(this).next('.sub-cat').find('a').length==0){
                    $(this).hide();	
                }	
            });

            $("#__MB_FILTER_DATE").datepicker({
                dateFormat:'yy-mm-dd',
                maxDate:0,
                onSelect: function(dateText, inst) {
                    window.location	=	window.location.href	+	'&fv='+dateText;
                }
            });
            // search keywords style
            var myString = "" ;
            if (myString!=''){
                myArray = myString.split(" ");
                for(i=0;i<myArray.length;i++)
                {
                    $('.search-list li').highlight(myArray[i]);
                } 					   
            }

         });


