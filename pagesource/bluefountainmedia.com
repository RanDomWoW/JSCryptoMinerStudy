<script data-cfasync=“false" src="/js/swiper.jquery.min.js"></script>
<script>

        jQuery(document).ready(function() {

            var activeSlide = 0;

            var bannerSliderTop = new Swiper('.js-gallery-top', {
                preloadImages: false,
                lazyLoading: true,
                lazyLoadingInPrevNext: true,
                slidesPerView: 1,
            });

            var bannerRoundSlider = new Swiper('.js-gallery-thumbs', {
                centeredSlides: true,
                slidesPerView: 1,
                touchRatio: 0.2,
                nextButton: '.js-gallery-thumbs .swiper-button-next',
                prevButton: '.js-gallery-thumbs .swiper-button-prev',
                pagination : '.banner-backgrounds .swiper-pagination',
                paginationClickable: true,
                lazyLoadingInPrevNext: true,
                onSlideChangeEnd: function(swiper) {
                    activeSlide = swiper.activeIndex;
                },
                paginationBulletRender: function (swiper, index, className) {

                    if (index === 0) {
                        return (
                            '<div class="'+ className +'">' +
                            '<img class="bullet-img" src="/images/sections/BannerProjectsSlider/images/sk-logo.png" alt="">' +
                            '<img class="bullet-img-active" src="/images/sections/BannerProjectsSlider/images/sk-logo-active.png" alt="">' +
                            '</div>'
                        )
                    } else if (index === 1) {
                        return (
                            '<div class="'+ className +'">' +
                            '<img class="bullet-img" src="/images/sections/BannerProjectsSlider/images/nuna-logo.png" alt="">' +
                            '<img class="bullet-img-active" src="/images/sections/BannerProjectsSlider/images/nuna-logo-active.png" alt="">' +
                            '</div>'
                        )
                    } else if (index === 2) {
                        return (
                            '<div class="'+ className +'">' +
                            '<img class="bullet-img" src="/images/sections/BannerProjectsSlider/images/thenewschool-logo-bullet.png" alt="">' +
                            '<img class="bullet-img-active" src="/images/sections/BannerProjectsSlider/images/thenewschool-logo-bullet-active.png" alt="">' +
                            '</div>'
                        )
                    } else if (index === 3) {
                        return (
                            '<div class="'+ className +'">' +
                            '<img class="bullet-img" src="/images/sections/BannerProjectsSlider/images/wholesome-tab-logo.png" alt="">' +
                            '<img class="bullet-img-active" src="/images/sections/BannerProjectsSlider/images/wholesome-tab-logo-active.png" alt="">' +
                            '</div>'
                        )
                    }

                }
            });

            bannerRoundSlider.params.control = bannerSliderTop;

            var awardsSlider = new Swiper('.js-awards-container', {
                nextButton: '.awards-container .swiper-button-next',
                prevButton: '.awards-container .swiper-button-prev',
            });


            $('.js-inner-tabs-slider').each(function(index, item) {

                var progressMax;
                var progressLine;
                var _slider = $(item);
                var pagination = _slider.find('.js-pagination-slider');

                var sliderTabs = new Swiper(_slider, {
                    pagination : _slider.find('.swiper-pagination'),
                    paginationClickable: true,
                    preloadImages: false,
                    lazyLoading: true,
                    nextButton: _slider.find('.swiper-button-next'),
                    prevButton: _slider.find('.swiper-button-prev'),
                    noSwipingClass: 'pagination-progress',
                    nested: true,
                    paginationBulletRender: function (swiper, index, className) {
                        return (
                            '<div class="'+ className +' swiper-slide">'+
                            '<span>'+
                            $(_slider.find('.swiper-slide')[index]).attr('data-tab-text') +
                            '</span>'+
                            '</div>'
                        )
                    },
                    onPaginationRendered: function (swiper, paginationContainer) {
                        progressMax = paginationContainer.clientWidth;
                        progressLine = _slider.find('.pp-line');
                    },
                    onProgress: function (swiper, progress) {

                        var progressWidth = swiper.bullets[swiper.snapIndex].clientWidth;
                        var progressPosition = (progressMax * progress) - (progressWidth * progress);

                        progressLine.css({
                            width: progressWidth,
                            transform: 'translate3d('+ progressPosition +'px, 0, 0)'
                        });

                    },
                });

                var paginationSlider = new Swiper(pagination, {
                    grabCursor: false,
                    simulateTouch: false,
                    breakpoints: {
                        1154: {
                            slidesPerView: 1,
                            grabCursor: true,
                            simulateTouch: true,
                        }
                    }
                });

                sliderTabs.params.control = paginationSlider;
                paginationSlider.params.control = sliderTabs;

            });

            var linksForToggleTabs = $('.banner-round-container .plus-link');

            var swiperTabs = new Swiper('.js-swiper-tabs', {
                direction: 'vertical',
                slidesPerView: 1,
                touchRatio: 0,
                initialSlide: activeSlide,
            });

            swiperTabs.detachEvents();
            bannerSliderTop.params.control = swiperTabs;

            var caseStudyIsOpen = false;

            linksForToggleTabs.on('click', function(){

                caseStudyIsOpen = !caseStudyIsOpen;

                linksForToggleTabs
                    .toggleClass('pl-active')
                    .text(caseStudyIsOpen ? 'hide case study' : 'view case study');

                $('.banner-tabs-slider').toggleClass('tsc-hidden');

                var scrollTo = $('.js-gallery-top')[0].clientHeight;
                var banner_zoom = ($('.banner-container').css('zoom') == 0.8) ? scrollTo * .2 - 10 : 0;

                body.stop().animate({
                    scrollTop: caseStudyIsOpen ? scrollTo - banner_zoom + $('.header').height() : 0
                }, 500, 'swing');

                setTimeout(function() {

                    if ($(window).width() > 500) {
                        body.stop().animate({
                            scrollTop: $(window).scrollTop() - $('.header')[0].clientHeight
                        }, 500, 'swing');
                    }

                }, 500)
            });

            $('.banner-tabs-slider .red-close-btn').on('click', function() {

                caseStudyIsOpen = false;

                linksForToggleTabs.removeClass('pl-active').text('view case study');

                $('.banner-tabs-slider').addClass('tsc-hidden');

                body.stop().animate({scrollTop: 0}, 500, 'swing');

            });

            var gallerySlider = new Swiper('.js-gallery-slider', {
                pagination: '.js-gallery-slider .swiper-pagination',
                effect: 'coverflow',
                grabCursor: true,
                centeredSlides: true,
                slidesPerView: 'auto',
                loop: true,
                coverflow: {
                    rotate: 50,
                    stretch: 0,
                    depth: 100,
                    modifier: 1,
                    slideShadows : true
                }
            });

            var projectsContainers = $('.pp-item-container');
            var projectsExpanded = $('.project-expanded-section');
            var closeSectionButton = projectsExpanded.find('.red-close-btn');
            var oldScroll = 0;

            var body = $("html, body");

            projectsContainers.on('click', function() {
                var _this = $(this);
                var _section = _this.next('.project-expanded-section');
                var _slider = _section.find('.js-pes-slider');

                $('.pp-pl-item').not(_this).removeClass('pp-active');
                _this.parents('.pp-pl-item').toggleClass('pp-active');

                projectsExpanded.not(_section).slideUp(500).removeClass('pes-expanded');

                _section.slideDown(500).addClass('pes-expanded');

                var projectsSlider = new Swiper(_slider, {
                    pagination : _section.find('.pes-tabs-container'),
                    paginationClickable: true,
                    slidesPerView: 1,
                    spaceBetween: 1,
                    paginationBulletRender: function (swiper, index, className) {
                        return (
                            '<div class="pes-tab '+ className +'">' +
                            '<img src="'+ $(swiper.slides[index]).attr('data-tab-image') +'" alt="">' +
                            '</div>'
                        )
                    }
                });

                oldScroll = $(window).scrollTop();

                setTimeout(function() {
                    _section.find('.pes-content').css({
                        'min-height': _section.find('.pes-tabs-container')[0].clientHeight
                    });

                    var scrollTo = _section.offset().top - $('.header').height();

                    body.stop().animate({scrollTop: scrollTo}, 500, 'swing');
                }, 500);

            });

            closeSectionButton.on('click', function() {
                projectsExpanded.parents('.pp-pl-item').removeClass('pp-active');

                projectsExpanded.slideUp(500).removeClass('pes-expanded');

                body.stop().animate({scrollTop: oldScroll}, 500, 'swing');
            });

            $('.bc-anchor').on('click', function() {
                var scrollTo = ($(this).offset().top);

                body.stop().animate({scrollTop: scrollTo}, 500, 'swing');
            });

            $('.pp-pl-item').on('mouseover', function() {

                var _this = $(this);

                $('.pp-pl-item').not(_this).addClass('pp-item-hovered');

                _this.removeClass('pp-item-hovered');

            }).on('mouseout', function() {

                $('.pp-pl-item').removeClass('pp-item-hovered');

            })

        });


    </script>
