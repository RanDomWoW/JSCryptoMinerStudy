<SCRIPT type=text/javascript>
  function el(object){
		return document.getElementById(object);
			}
function selectTag(theyear,selfObj){
	// 操作标签
	var lastyear = parseInt(el('lastyear').value);
	var tag = document.getElementById("tags").getElementsByTagName("li");
	var taglength = tag.length;
	for(i=0; i<taglength; i++){
		if (tag[i].getAttribute("name").indexOf('tag') != -1)
		{
			tag[i].className = "";
		}
		
	}
	selfObj.parentNode.className = "selectTag";
	// 操作内容
	for(i=lastyear; j=document.getElementById("tagContent"+i); i--){
		j.style.display = "none";
	}
	showContent='tagContent'+theyear;
	document.getElementById(showContent).style.display = "block";
	graduatelist(2005,lastyear,theyear);
	//document.getElementById('nowtag').value=theyear;
	
}
function graduatelist(startyear,endyear,theyear)
	{
		
		//apic=theyear+'.gif';
		//el('bybg').innerHTML="<img src='/static/images/alevel/"+apic+"' border='0' usemap='#Map'/>";
		for (i=startyear;i<=endyear;i++ )
		{
			divid='bavev_fae'+i;
			if (i==theyear)
			{
				if (el(divid))
				{
					el(divid).style.display="block";
				}
				else{
					get_graduate_list_data(theyear);
				}
			}
			else
			{
				if (el(divid))
				{
					el(divid).style.display="none";
				}
			}
		}
	}
function get_graduate_list_data(theyear){
	$.ajax({
		url:"/get_graduate_list_data/",
		type:"post",
		secureuri :false,
		cache :false,  
		async : true, //默认为true 异步   
		dataType : 'json',
		data : {'thisyear':theyear},
		success : function (data, status)
		{
			var data_array =JSON.stringify(data);
			var data_array=eval('('+data_array+')');
			var status = data_array['status'];
			if (status != 'ok')
			{	
				alert(data_array['info']);
			}
			else{
				var new_html = $('#bavev_fae_div').html() + data_array['html'];
				$('#bavev_fae_div').html(new_html);
				divid='bavev_fae'+theyear;
				el(divid).style.display="block";
			}
		},
		error: function (data, status, e)
		{
			alert(e);
		}
	});
}

function gototags(direct){
//direct:-1 往左
//direct:1 往右
	// 操作标签
	theyear=parseInt(el('nowtag').value);
	var lastyear = parseInt(el('lastyear').value);
	if (direct==1)
	{
		if (theyear-5>=2005)
		{
			hidtab='tag'+(theyear);
			showtab='tag'+(theyear-5)
			el(hidtab).style.display='none';
			el(showtab).style.display='block';
			el('nowtag').value=theyear-1;
		}
	}
	if (direct==-1)
	{
		if (theyear+1<=lastyear)
		{
			hidtab='tag'+(theyear-4);
			showtab='tag'+(theyear+1)
			el(hidtab).style.display='none';
			el(showtab).style.display='block';
			el('nowtag').value=theyear+1;
		}
	}

	
	
}
</SCRIPT>

