<script>

$(document).ready(function () {
		$(".pro_pic").each(function () {  
					DrawImage(this, 235, 150);  
					$(this).css('padding-top', (150 - $(this).height())/2-2);
					//$(this).css('padding-left', (240 - $(this).width())/2-2);
					
            });  
            $(".works_block").imageScroller({
                next: "btnNext",
                prev: "btnPrev",
                frame: "works_pic",
                child: "li",
                timer: 6000,
                speed:'normal',
                easing:'',
                auto: true
            });
        });

function DrawImage(ImgD, FitWidth, FitHeight) {  
	var image = new Image();  
	image.src = ImgD.src;  
	if (image.width > 0 && image.height > 0) {  
		if (image.width / image.height >= FitWidth / FitHeight) {  
			if (image.width > FitWidth) {  
				ImgD.width = FitWidth;  
				ImgD.height = (image.height * FitWidth) / image.width;  
			} else {  
				ImgD.width = image.width;  
				ImgD.height = image.height;  
			}  
		} else {  
			if (image.height > FitHeight) {  
				ImgD.height = FitHeight;  
				ImgD.width = (image.width * FitHeight) / image.height;  
			} else {  
				ImgD.width = image.width;  
				ImgD.height = image.height;  
			}  
		}  
	}
	
}  

</script>
<script>
function urlencode(url)
{
	url=url.replace(/\%/g,"%25");
	url=url.replace(/\#/g,"%23");
	url=url.replace(/\&/g,"%26");
	return url;
 }
function openURL(url)
{
	//alert( urlencode(url));

	if(url.indexOf('.pdf') != -1 || url.indexOf('.pub') != -1)
    {
		//theurl="https://view.officeapps.live.com/op/view.aspx?src="+urlencode(url)
		window.open(url,"_blank");
	}
	else
    {
		theurl="https://view.officeapps.live.com/op/view.aspx?src="+urlencode(url)
		window.open(urlencode(theurl),"_blank");
	}
}
</script>
