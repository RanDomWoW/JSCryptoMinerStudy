    <script>
        $(document).ready(function () {
            heightWindow()
        })

        function heightWindow() {


            if ((parseInt($("body").outerHeight(true)) + parseInt($("footer").outerHeight(true))) < $(window).height()) {

                $("footer").addClass("fixFooter")
            }

            else {

