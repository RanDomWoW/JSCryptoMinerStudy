    <script type="text/javascript">

        var selectedTab = 0;
        var prm = Sys.WebForms.PageRequestManager.getInstance();
        var wayType = "mainPlaceHolder_internalFirstWay";

        prm.add_endRequest(function () {

            $(".custom-select").customselect({
                noresultstext: "بدون نتیجه",
            })

            $(".tab").Tabs({ defaultActiveIndex: selectedTab })

            $("#" + wayType).prop("checked", true)
    
            if (wayType == "mainPlaceHolder_internalSecoundWay") {

                    $(".date.back .disabled").hide()
                }

        });

        $(window).load(function () {

            $('#nivo-slider').nivoSlider({

                prevText: '',
                nextText: '',
                controlNav: false
            });

        });

        $(document).ready(function () {

            $(".tab").Tabs()

            $("body").on("click", ".searchBox > ul > li", function () {
                
                selectedTab = $(this).index();
              wayType = "mainPlaceHolder_internalFirstWay";
            })

            $(".custom-select").customselect({
                noresultstext: "بدون نتیجه",
            })

            $("body").on("click", ".slcWay label", function () {

                wayType = $(this).attr("for") == "mainPlaceHolder_internalSecoundWay" ? "mainPlaceHolder_internalSecoundWay" : "mainPlaceHolder_internalFirstWay"
         
                if ($(this).attr("for") == "mainPlaceHolder_internalSecoundWay") {

                    $(".date.back .disabled").hide()
                }
                else
                    $(".date.back .disabled").show()
            })
        })

        function resetTab() {
            $("#mainPlaceHolder_internalFirstWay").prop("checked", true);
        }
    </script>

