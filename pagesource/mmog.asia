<script src="http://www.mmog.asia/template/default/js/jquery-1.7.4.dotdotdot.min.js"></script> 
<script>   
    	var email_valid       = false;
	var image_success     = '<img src="http://www.mmog.asia/template/default/images/en/reg/icon_correct.png" style="vertical-align:text-top;"/>';
	var image_error       = '<img src="http://www.mmog.asia/template/default/images/en/reg/icon_wrong.png" style="vertical-align:text-top;"/>';	
	var image_notice      = '<img src="http://www.mmog.asia/template/default/images/en/reg/icon_alert2.png" style="vertical-align:text-top;"/>';

    	function check_submit_newsletter_form(){
		
		is_valid = true;
		//Submit form checking
		email_valid       = check_email(document.getElementById('email').value);
		if(!email_valid){
			document.getElementById('div_email_error').innerHTML = image_error + " Invalid email address." ;
			document.getElementById('div_email_error_2').innerHTML = image_error + " Invalid email address." ;
			document.getElementById('div_email_correct').innerHTML = "" ;
			
			is_valid = false;
		}
                if(is_valid){
                    //alert(is_valid);
			document.UpdateNewsletter.submit();			
		}
                
        }
	function check_email(value){		
		
		if((value!='Enter Valid Email')){
			document.getElementById('email').value = (document.getElementById('email').value).toLowerCase();
			document.getElementById('email').value = (document.getElementById('email').value).replace(/\s/g,'');
		}
				
		var value = document.getElementById('email').value;
		document.getElementById('div_email_correct').innerHTML = '';
                document.getElementById('div_email_error').innerHTML = '';
                document.getElementById('div_email_error_2').innerHTML = '';
                
		//is empty?
		if((value == '')){
			document.getElementById('email').value = '';
			document.getElementById('div_email_error').innerHTML = image_error + " Please enter email address." ;
			document.getElementById('div_email_error_2').innerHTML = image_error + " Please enter email address." ;
			document.getElementById('div_email_correct').innerHTML = "";
			email_valid = false;
			return false;
			
		}
		
		if(value.indexOf("@") < 1 || value.indexOf(".") < 1 || value.indexOf(" ") > 0){		
			
			document.getElementById('div_email_error').innerHTML = image_error + " Invalid email address." ;
			document.getElementById('div_email_error_2').innerHTML = image_error + " Invalid email address." ;
			document.getElementById('div_email_correct').innerHTML = "";
			email_valid = false;
			return false;
		}

		//is valid email format?
		var emailRegEx = /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$/i;
		if(document.getElementById('email').value.search(emailRegEx) == -1){
			document.getElementById('div_email_error').innerHTML = image_error + " Invalid email address." ;
			document.getElementById('div_email_error_2').innerHTML = image_error + " Invalid email address." ;
			document.getElementById('div_email_correct').innerHTML = "";
			email_valid = false;
			return false;
		}
		
		//is email available?
		$.ajax({
		    type: "POST",
		    url: "http://www.mmog.asia/user/ajax_lib",
		    data: { email: value, action : "check_newsletter_email"},
		    cache: false,
		    success: function(response_text){
                       //alert(response_text);
			    response_text = response_text.trim();	                            
			    if(response_text == 'success'){
				    document.getElementById('div_email_error').innerHTML = "" ;
				    document.getElementById('div_email_error_2').innerHTML = "" ;
				    document.getElementById('div_email_correct').innerHTML = image_success;
			    }
			    else
			    if(response_text == 'fail'){
				    document.getElementById('div_email_correct').innerHTML = "" ;
				    document.getElementById('div_email_error').innerHTML = image_error + " A user with this email already exists." ;
				    document.getElementById('div_email_error_2').innerHTML = image_error + " A user with this email already exists." ;
				    email_valid = false;
				    return false;
			    }				
			    else{
				    document.getElementById('div_email_correct').innerHTML = "" ;
				    document.getElementById('div_email_error').innerHTML = image_error + " Unknown error, please try again later." ;
				    document.getElementById('div_email_error_2').innerHTML = image_error + " Unknown error, please try again later." ;
				    email_valid = false;
				    return false;
			    }

		    }
		});

	    email_valid = true;

	    return true;
	}	
    
        $(document).ready(function() {
                var category = "1";
                if (matchMedia) {
                    var mq = window.matchMedia("(min-width: 500px)");
                    mq.addListener(WidthChange);
                    if (WidthChange(mq)===1){
                        category = "1";
                    }            

                    mq = window.matchMedia("(min-width: 768px)");
                    mq.addListener(WidthChange);
                    if (WidthChange(mq)===1){
                        category = "2";
                    }

                    mq = window.matchMedia("(min-width: 1024px)");
                    mq.addListener(WidthChange);
                    if (WidthChange(mq)===1){
                        category = "3";
                    }
                }

                switch (category){
                    case "1":
                        ShowMoreChar(200);
                        break;
                    case "2":
                        ShowMoreChar(200);
                        break;
                    case "3":
                        ShowMoreChar(200);
                        break;
                    default:
                        break;
                }


        });

        // media query event handler

        // media query change
        function WidthChange(mq) {
        //<500px
            if (mq.matches) {
                return 1;
            } else {
               return 0;
            }

        }
        function ShowMoreChar(showChar){
                //var showChar = 200;
                var ellipsestext = "...";
                var moretext = "more";
                var lesstext = "less";
                $('.more').each(function() {
                        var content = $(this).html();

                        if(content.length > showChar) {

                                var c = content.substr(0, showChar);
                                //var h = content.substr(showChar -1, content.length - showChar);
                                var h = content.substr(showChar, content.length - showChar);


                                //var html = c + '<span class="moreellipses">' + ellipsestext+ '&nbsp;</span><span class="morecontent"><span>' + h + '</span>&nbsp;&nbsp;<a href="" class="morelink">' + moretext + '</a></span>';
                                var html = c + '... more';//+ '<span class="moreellipses">' + ellipsestext+ '&nbsp;</span><span class="morecontent"><span>' + h + '</span>&nbsp;&nbsp;<a href="" class="morelink">' + moretext + '</a></span>';

                                $(this).html(html);
                        }

                });

                $(".morelink").click(function(){
                    //for no less
                        if($(this).hasClass("less")) {
                                $(this).removeClass("less");
                                $(this).html(moretext);
                        } else {
                                $(this).addClass("less");
                                $(this).html('');
                        }

                        $(this).parent().prev().toggle();
                        $(this).prev().toggle();
                        return false;

                });
        }

