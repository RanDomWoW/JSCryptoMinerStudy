    <script type="text/javascript" charset="utf-8">
        window.onload = function () {
            var pagerYanContUlUz = $('.pager-yan-cont > ul').width();
            $('.pager-yan-cont > ul').css('left', (734 - pagerYanContUlUz) / 2);
            $('.db-pager > ul').css('left', (480 - pagerYanContUlUz) / 2);

            var tdSolHeight = $('.td_sol').height();
            var tdSagHeight = $('.td_sag').height();
            if (tdSolHeight > tdSagHeight)
                $('.td_sag').height(tdSolHeight);
            else
                $('.td_sol').height(tdSagHeight);
        }

        $(document).ready(function () {
            var scrollTop = $(document).scrollTop();
            var isFixed = 0;
            var sayfaWidth = $(window).width();
            var sayfaHeight = $(window).height();
            var leftAltFix = 0;
            var leftMenuHeight = $('.left-menu-cont').height();


            console.log('left menu yukseklik' + leftMenuHeight);
            $(document).scroll(function () {
                //üst ve yan fixed menüyü ayarlama.
                var sctionsYukseklik = $('.sctions').height();
                var leftBotter = $('.sctions').height() + 170;
                scrollTop = $(document).scrollTop();

                if (scrollTop > 300 && isFixed == 0) {
                    isFixed = 1;
                    $('.menu-conts').addClass('menu-cont-active');
                    $('.left-menu-cont').addClass('left-menu-fixed');
                }
                else if (scrollTop < 300 && isFixed == 1) {
                    isFixed = 0;
                    $('.menu-conts').removeClass('menu-cont-active');
                    $('.left-menu-cont').removeClass('left-menu-fixed');
                }
                var altsinir = leftBotter - leftMenuHeight;
                //console.log('scroll top...:' + scrollTop);
                //console.log('alt sini...:' + altsinir);
                if (scrollTop > altsinir && leftAltFix == 0) {
                    console.log('scroll top alt sınırı geçti.');
                    $('.left-menu-cont').removeClass('left-menu-fixed');
                    $('.left-menu-cont').addClass('left-menu-bottom');
                    leftAltFix = 1;
                }
                else if (scrollTop <= altsinir && leftAltFix == 1) {
                    console.log('scroll top alt sınırı geri geldi.');
                    $('.left-menu-cont').removeClass('left-menu-bottom');
                    $('.left-menu-cont').addClass('left-menu-fixed');
                    leftAltFix = 0;
                }
            });

            //top-menu açılır menü----------------------------
            $(window).resize(function () {
                top_menu_boyut();
            });

            function top_menu_boyut() {
                sayfaWidth = $(window).width();
                sayfaHeight = $(window).height();

                var leftSabit = -((sayfaWidth - 1210) / 2 + 347);
                $('.top-menu-detay').width(sayfaWidth);
                $('ul.top-menu>li').eq(0).children('.top-menu-detay').css({ 'left': (leftSabit) });
                $('ul.top-menu>li').eq(1).children('.top-menu-detay').css({ 'left': (leftSabit - 152) });
                $('ul.top-menu>li').eq(2).children('.top-menu-detay').css({ 'left': (leftSabit - 291) });
                $('ul.top-menu>li').eq(3).children('.top-menu-detay').css({ 'left': (leftSabit - 437) });
                $('ul.top-menu>li').eq(4).children('.top-menu-detay').css({ 'left': (leftSabit - 610) });
                $('ul.top-menu>li').eq(5).children('.top-menu-detay').css({ 'left': (leftSabit - 744) });

                $('.ilet-modal-strec').width(sayfaWidth);
                $('.ilet-modal-strec').height(sayfaHeight);
            }
            top_menu_boyut();


            //alt menu gösterme---------------
            //rev 11.03.2014 start
            $('ul.top-menu>li').hover(function () {
                $(this).children('.top-menu-detay').css('display', 'block');
            }, function () {
                $(this).children('.top-menu-detay').css('display', 'none');
            });
            //rev 11.03.2014 end


            //
            $('.top-menu-cont').hover(function () {
                if ($.browser.msie) {
                    if ($.browser.version == "8.0") {
                        $(this).parent().parent().css('z-index', 500);
                    }
                }
                else {
                    $(this).parent().parent().css('z-index', 3);
                }

            }, function () {
                $(this).parent().parent().css('z-index', 1);
            });

            //akordiyon menü açılıp kapatma
            $('.akr-list').css('display', 'block');
            $('.akr-list>li').children('div').slideUp();
            //$('.akr-list>li>nav').click(function () {
            //    $('.ilceler').css('display', 'none');
            //    $(this).children('h1').toggleClass('h1-opened');
            //    $(this).parent().children('div').slideToggle();
            //    //$(this).parent().children('.li-detay').slideDown();
            //});

            //akordiyon menü açılıp kapatma
            $('.akr-list1').css('display', 'block');
            $('.akr-list1>li').children('div').slideUp();
            $('.akr-list1>li>nav').click(function () {
                $(this).children('h1').toggleClass('h1-opened');
                $(this).parent().children('div').slideToggle();
                //$(this).parent().children('.li-detay').slideDown();
            });


            //arama sonuç aç
            $('.ara-wrap input[type="text"]').focusin(function () {
                $(this).parent().parent().children('.ara-acilir').fadeIn();
            });

            $('.ara-wrap input[type="text"]').focusout(function () {
                $(this).parent().parent().children('.ara-acilir').fadeOut();
            });
            $('.ara-close').click(function () {
                $('.ara-acilir').fadeOut();
            });



            //harf süzme açılır

            $('.harf-list').slideUp();
            $('.muht-pager-cont > section').click(function () {
                $(this).children('.harf-list').slideToggle();
            })


            //iletişim modal açılış
            $('.list-cont > ul > li,.flip-open').click(function () {
                $('html').css('overflow', 'hidden');
                $('.sctions').addClass('sctions-modal');
                top_menu_boyut();
                $('.ilet-modal-strec').fadeIn();
            });

            $('.mdl-close').click(function () {
                $('html').css('overflow', 'auto');
                $('.sctions').removeClass('sctions-modal');
                top_menu_boyut();
                $('.ilet-modal-strec').fadeOut();
            });
            //ieft menu cont.

            //place holder.........................
            if ($.browser.msie) { //brovser versiyon kontrol.
                if ($.browser.version == "9.0" || $.browser.version == "8.0") {
                    $('select').css('background-image', 'none');
                    $('[placeholder]').focus(function () {
                        var input = $(this);
                        if (input.val() == input.attr('placeholder')) {
                            input.val('');
                            input.removeClass('placeholder');
                        }
                    }).blur(function () {
                        var input = $(this);
                        if (input.val() == '' || input.val() == input.attr('placeholder')) {
                            input.addClass('placeholder');
                            input.val(input.attr('placeholder'));
                        }
                    }).blur();
                }
            }

            if ($.browser.msie) {
                if ($.browser.version == "8.0") {
                    $('#ie8css').attr('href', '/assets/css/ie8.css');
                    $('#ie8js').attr('src', '/assets/js/html5shiv.js');
                }
                $('label').click(function () {
                    $(this).children('span').toggleClass('togl-chk');
                    if ($(this).parent().children('input').attr('checked') == false) {
                        $(this).parent().children('input').attr('checked') = false;
                    }
                    else
                        $(this).parent().children('input').attr('checked') = true;

                });

            }

            //--------------------------baskanlik section
            var aktifSc = 0;
            $('.bsk-dt-list').height($('.bsk-dt-list>li').eq(aktifSc).height() + 20);

            $('ul.baskan-list > li').click(function () {
                var mn_index = $(this).index();
                if (mn_index != 2) {
                    $('ul.bsk-dt-list > li').eq(aktifSc).fadeOut();
                    aktifSc = mn_index;
                    $('ul.bsk-dt-list > li').eq(aktifSc).fadeIn();
                    $('.bsk-dt-list').height($('.bsk-dt-list>li').eq(aktifSc).height() + 20);
                }
            });

            //etkinlik takvimi resim değişimi
            var yol;
            $('.etk-menu > li').hover(function () {
                yol = $(this).children('a').children('img').attr('src');
                var dosyaAdi = yol.split('/')[6];
                var yeniYol = '/assets/img/icons/etkinlikt/h/' + dosyaAdi;
                $(this).children('a').children('img').attr('src', yeniYol);
                console.log(dosyaAdi);

            }, function () {
                $(this).children('a').children('img').attr('src', yol);
            });

            //ulaşım resim değişimi

            var yolOto;
            $('.otobus-nav-cont > li').hover(function () {
                if (!$(this).hasClass('on-active')) {
                    $(this).children('.img-n').fadeOut();
                    $(this).children('.img-h').fadeIn();
                }

            }, function () {
                if (!$(this).hasClass('on-active')) {
                    $(this).children('.img-n').fadeIn();
                    $(this).children('.img-h').fadeOut();
                }
            });

            $('.fancybox').fancybox();

            //if ($.browser.webkit) {
            //    $('body').css('text-shadow', '0px 0px 1px rgba(150, 150, 150, 0.59)');
            //}

            // aramadaki listenini içeriğini imputa basar.
            $('.otobus-acilir-list > li').click(function () {
                $(this).parent().parent().parent().children('.ulasim_ara').children('input').val($(this).children('p').html());
            });

            $('.hatarama-list > li').click(function () {
                $(this).parent().parent().parent().children('.hat_arama_cn').children('input').val($(this).children('p').html());
            });


            //otobussaatleri tıklayarak section açma
            var aktifOsTab = 0
            $('ul.otobus-nav-cont > li').eq(aktifOsTab).addClass('on-active');
            $('.oto-saat-cont>div').eq(aktifOsTab).fadeIn(100, function () {
                $('.oto-saat-cont').height($('.oto-saat-cont>div').eq(aktifOsTab).height() + 20);
            });


            $('ul.otobus-nav-cont > li').click(function () {
                $('ul.otobus-nav-cont > li').eq(aktifOsTab).removeClass('on-active');
                $('.oto-saat-cont>div').eq(aktifOsTab).fadeOut();
                aktifOsTab = $(this).index();
                $('ul.otobus-nav-cont > li').eq(aktifOsTab).addClass('on-active');
                $('.oto-saat-cont>div').eq(aktifOsTab).fadeIn();
                $('.oto-saat-cont').height($('.oto-saat-cont>div').eq(aktifOsTab).height() + 20);
            });

            //otobussaatleri tıklayarak section açma
            var aktifDbTab = 0
            $('.db-right>div').eq(aktifDbTab).fadeIn(100, function () {
                $('.db-right').height($('.db-right>div').eq(aktifDbTab).height() + 40);
            });


            //$('ul.db-mn-list> li').click(function () {
            //    $('.db-right>div').eq(aktifDbTab).fadeOut();
            //    aktifDbTab = $(this).index();
            //    $('.db-right>div').eq(aktifDbTab).fadeIn();
            //    $('.db-right').height($('.db-right>div').eq(aktifDbTab).height() + 40);
            //});

        });
    </script>
