	<script>
		$(function(){
			$('.countdown').each(function(i, el){
				var $el = $(el);
				var id = $el.attr('id').split('-')[1];
				var timestamp = parseInt(id);
				timestamp *= 1000; //unix timestamp in milliseconds; unambiguous UTC
				$el.countdown(timestamp, function(event){
					$('#numdays-' + id).html(event.strftime('%D'));
					$('#numhours-' + id).html(event.strftime('%H'));
					$('#nummins-' + id).html(event.strftime('%M'));
					$('#dayslabel-' + id).html( 'Day' + ( ( event.strftime('%D') != 1 ) ? 's' : '' ) );
					$('#hourslabel-' + id).html( 'Hour' + ( ( event.strftime('%H') != 1 ) ? 's' : '' ) );
					$('#minslabel-' + id).html( 'Minute' + ( ( event.strftime('%M') != 1 ) ? 's' : '' ) );
				});
			});

			//This will load the rest of the carousel images besides the first which is loaded by default.
			$('.carousel-item').each(function(){
				if( !$(this).find('img').length )
				{
					var id = $(this).data('id');
					var photo = $(this).data('photo');
					var title = $(this).data('title');

					$(this).find('a').html('<img src="https://cdn2.zayconfresh.com'+photo+'" alt="'+title+'" id="carousel-img-'+id+'">');
				}
			});
		});
	</script>

