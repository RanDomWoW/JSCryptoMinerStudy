<script>
    $(function(){
        $('img[name="seal"]').addClass("center-block");

	    $('#footer-login-form-email, #footer-login-form-password').keypress(function(e){
		    if (e.which === 13) {
			    $('#footer-login-form-submit').trigger('click');
		    }
	    });

	    $('#footer-login-form-submit').click(function(e){
		    e.preventDefault();

		    var $error1 = $('#footer-login-form-error-message1');
		    var $error2 = $('#footer-login-form-error-message2');
		    $error1.hide();
		    $error2.hide();

		    var email = $('#footer-login-form-email').val();
		    var password = $('#footer-login-form-password').val();

		    if ( email.length === 0 ){
			    $error1.text('Email cannot be blank').show();
		    }

		    if ( password.length === 0 ) {
			    $error2.text('Password cannot be blank').show();
		    }

		    if ( email.length > 0 && password.length > 0 ) {
			    $('#footer-login-form').submit();
		    }
	    });

	    $('.show-hide').click(function(e){
		    e.preventDefault();

		    var state = $(this).data('state');
		    var $element = $('#footer-login-form-password');

		    if( state === 'password' )
		    {
			    $element.attr('type', 'text');
			    $(this).data('state', 'text').text('hide');
		    }
		    else
	        {
			    $element.attr('type', 'password');
			    $(this).data('state', 'password').text('show');
		    }
	    });

	    $('#navbar-login-trigger').click(function(e){
	    	e.preventDefault();
	    });

	    $(window).click(function(e){
	    	var $target = $(e.target);

	    	if( $target.hasClass( 'login-trigger' ) )
		    {
		    	e.preventDefault();

		    	if( $(window).width() < 450 )
			    {
			    	window.location = '/login';
			    	return true;
			    }

			    toggleLoginWindow( $('#login-form-wrapper').is(':visible') );
		    }
		    else if( $target.attr('id') === 'login-form-close' )
		    {
			    e.preventDefault();

			    toggleLoginWindow( true );
		    }
		    else if(
		    	$target.parents('#login-form-wrapper').length === 0 &&
			    $('#login-form-wrapper').is(':visible')
		    ) {
		    	toggleLoginWindow( true );
		    }
	    });

	    $(window).on('resize', function(){
		    var $form_wrapper = $('#login-form-wrapper');

	    	if( $form_wrapper.is(':visible') )
		    {
			    var offset = getTopAndLeft( $('#navbar-login-trigger').offset() );

			    $form_wrapper
				    .css('top', offset.top)
				    .css('left', offset.left);
		    }
	    });

	    function toggleLoginWindow( is_visible ){
		    var $form_wrapper = $('#login-form-wrapper');
		    var $login_button = $('#navbar-login-trigger');

		    if( is_visible )
		    {
			    $form_wrapper.hide();

			    $login_button
				    .removeClass('btn-success')
				    .addClass('btn-default');

			    $('#homepage-carousel')
				    .carousel('next')
				    .carousel('cycle');
		    }
		    else
		    {
		    	var offset = getTopAndLeft( $login_button.offset() );

			    $form_wrapper
				    .css('top', offset.top)
				    .css('left', offset.left);

			    $form_wrapper.show();

			    $('#footer-login-form-customer-name').focus();

			    $login_button
				    .removeClass('btn-default')
				    .addClass('btn-success');

			    if( $(window).width() > 768 &&
				    $(window).scrollTop() > 180
			    ) {
				    $('html').animate({scrollTop:0}, 'fast');
			    }

			    $('#homepage-carousel').carousel('pause');
		    }
	    }

	    function getTopAndLeft( offset )
	    {
	    	var top = 15;
	    	var left = 0;

		    if( $(window).width() > 768 )
		    {
			    left = offset.left - $('#login-form-wrapper').width() + 50;
			    top = offset.top + 55;
		    }

		    return {
			    top: top,
			    left: left
		    };
	    }
    });
</script>

