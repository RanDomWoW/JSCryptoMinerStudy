		if(breakpoint.value == 'mobile'){
		 	swatchContainerWidth = 140;
		 	disArrowLen = 5;
		}else{
			swatchContainerWidth = 224;
			disArrowLen = 8;
		}
		//console.debug("breakpoint:"+ breakpoint.value + " disArrowLen:"+disArrowLen + " swatchContainerWidth:"+swatchContainerWidth);
		
		// Dynamically display left/right arrow and related style class.
		var swatchPrdLenArray = dojo.query("input[id^='displaySwatchLength_']");
		for(var j = 0; j<swatchPrdLenArray.length; j++){
			var productSwatchLenId = dojo.attr(swatchPrdLenArray[j],"id");
			var lengthVal = dojo.attr(productSwatchLenId,'value');
			var productId = dojo.attr(productSwatchLenId,'data-product-id');
			var leftArrowSpan = 'spanCarouselLeft_'+productId;
			var leftArrowDiv = 'divCarouselLeftArrow_'+productId;
			var rightArrow = 'spanCarouselRight_'+productId;
			var rightEndSpanArrow = 'endSpanCarouselRight_'+productId;
			var leftArrowSpanId = document.getElementById(leftArrowSpan);
			var leftArrowDivId = document.getElementById(leftArrowDiv);
			var rightArrowId = document.getElementById(rightArrow);
			var rightArrowEndSpanId = document.getElementById(rightEndSpanArrow);
			if(lengthVal > disArrowLen){
				if(leftArrowSpanId != null){
					leftArrowSpanId.style.display = "";
				}
				if(leftArrowDivId != null){
					leftArrowDivId.className = "slider-container";
					leftArrowDivId.style.display = "";
				}
				if(rightArrowId != null){
					rightArrowId.style.display = "";
				}
				
			}else{
				if(leftArrowDivId != null){
					leftArrowDivId.className = "slider-container no-arrows";
					leftArrowDivId.style.display = "";
				}
			}
			if(rightArrowEndSpanId != null){
				$("div#"+rightEndSpanArrow ).replaceWith('</div>');
			}
		}
		
	});
	
	
	
	
</script>		



