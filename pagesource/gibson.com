    <script type="text/javascript">
        function getVids(pid) {
            //console.log("get vids for " + pid);

            $.get (
                "https://www.googleapis.com/youtube/v3/playlistItems", {
                    part: 'snippet',
                    maxResults: 5,
                    playlistId: pid,
                    key: 'AIzaSyCuKZpPqYZ-QMZv-KnP9jJ1oYu7R8IFBMQ'
                },
                function (data) {
                    var results;
                    $.each(data.items, function (i, item) {
                        var videoid = item.snippet.resourceId.videoId;

                        var li = $(document.createElement('li'));
                        var aitem = $(document.createElement('a')).addClass("item colorbox-popup-video").attr('href', 'https://www.youtube.com/watch?v=' + videoid).attr('target', '_blank');

                        var thumb = $('<div class="thumb" />');

                        var imgthumb = $('<img class="video-thumb" src="' + item.snippet.thumbnails.medium.url + '">');
                        var imgplay = $('<img class="video-play" id="videoPlayThumb" src="http://images.gibson.com/Site-Images/Page-Elements/VideoPlay-Small_Button.png" />');

                        thumb.append(imgthumb).append(imgplay);

                        var description = $('<div class="description" />');

                        var title = $('<div class="title" />');
                        title.text(item.snippet.title);

                        var summary = $('<div class="summary" />');
                        summary.text(item.snippet.description.substring(0, 200) + "...");

                        description.append(title).append(summary);

