var pagenum = 1;
function imagedisplay()
{
  clearTimeout(imagedisplaytimeout);
  $.get("/default/imagedisplaynew",{pagenum:pagenum}, function(content){
      if(content != ''){
        $(".css-logos").append(content);
        pagenum++;
        imagedisplaytimeout = setTimeout(imagedisplay, 10000);
      }
  });

}

startmarquee(100, 35, 4500);
function startmarquee(lh, speed, delay) {

    var p = false;
    var t;
    var o = $(".css-logos")[0];
    //var o = $(".css-corphire .css-box .css-text")[0];
    o.innerHTML += o.innerHTML;
    o.style.marginTop = 0;
    o.onmouseover = function() {
        p = true;
    }
    o.onmouseout = function() {
        p = false;
    }

    function start() {
        t = setInterval(scrolling, speed);
        if (!p) o.style.marginTop = parseInt(o.style.marginTop) - 1 + "px";
    }

    function scrolling() {
        if (parseInt(o.style.marginTop) % lh != 0) {
            o.style.marginTop = parseInt(o.style.marginTop) - 1 + "px";
            if (Math.abs(parseInt(o.style.marginTop)) >= o.scrollHeight / 2) o.style.marginTop = 0;
        } else {
            clearInterval(t);
            setTimeout(start, delay);
        }
    }
    setTimeout(start, delay);
}
