    function strtotime(date){
      return date.getFullYear()+'-'+(date.getMonth()+1)+'-'+date.getDate();
    }
    function beforeShowDay(date) {
            var d = strtotime(date);
            var d_temp=date.getTime();
            var d_temp_end=(d_temp)+86399000;

        if(json && d in json){
          if(d_temp>temp){
            return [true, 'hasData',''];
          }else if(d_temp<temp){
            if(temp<d_temp_end){
              return [true,'hasData_last ui-datepicker-intoday',''];
            }
            else{
              return [true, 'hasData_last',''];
            }

          }
            return [true, '', ''];
        }
        else{
          return [false, '', ''];
        }

    }
