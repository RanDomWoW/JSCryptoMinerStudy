  function firstline (){
	$(".nano > .nano-content").scroll(function() {
        var dHight = $(this).height(), scrollHeight = $(this)[0].scrollHeight, scrollTop = $(this)[0].scrollTop;
        if (scrollTop + dHight >= scrollHeight) {
            var divFirst = $("#divFirst")
              , pageIndex = parseInt(divFirst.data("pageindex")) + 1;
            if ( pageIndex < 1 || isNaN(pageIndex))
                return;
            $.ajax({ type: "get",url: "/firstnews.aspx?action=post&p=" + pageIndex + "&pagesize=30",success: function(data) {
                  	divFirst.data("pageindex",pageIndex)
					if(data.length>0){
						var datalist=$('#ulFirst');
						if (datalist.length > 0) {
							datalist.html(datalist.html() + data);
						}
					}
                }
            });
        }
    });
  }
    
function loadnews(type,cur) {
    var posturl = '/top/handlers/Handler.ashx?action=newslist-all&p=1&url=http://www.pedaily.cn/top/newslist.aspx?', loadmore = $('#news-loadmore'), newslistall = $('#news-list-all'), newslistother = $('#news-list-other'); ;
