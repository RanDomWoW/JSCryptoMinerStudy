vi.chbeck.beckshop.suggestFF = vi.chbeck.beckshop.suggestFF || (function() {

    function SuggestFFViewModel(suggestInputID, suggestOutputID, host, url) {
        this.suchVorschlaege = ko.observableArray([]);
        this.autorenVorschlaege = ko.observableArray([]);
        this.produktVorschlaege = ko.observableArray([]);
        this.serviceThemen = ko.observableArray([]);
        this.suggestInputID = suggestInputID;
        this.suggestInputField = _getElementById(suggestInputID);
        this.suggestInputField.setAttribute("autocomplete", "off");
        this.suggestInputField.setAttribute("data-bind", "valueUpdate: 'afterkeydown', event: { keyup: vi.chbeck.beckshop.suggestFF.suggestFFTriggerPressKey }");
        this.suggestOutputID = suggestOutputID;
        this.host = host;
        this.url = url;
        this.userInput = "";
        this.autoSuggestPageUrl = host + url;
    }

    function _getElementById(element) {
        if (element && element != "") {
            if ($("#" + element).length > 0) return $("#" + element)[0];
        }
    }

    //Extrahiert Autor und Titel aus den Produktdaten und gibt diese als Query-String zurück
    function getAuthorAndTitle(data) {
        var queryString = "";

        if (data.name) {
            queryString += removeTags(data.name);
        } 

        if (data.attributes.Urheberzeile) {
            queryString += " - " + removeTags(data.attributes.Urheberzeile);
        }

        return "&AuthorAndTitle=" + queryString;
    }

    //Entfernt HTML-Tags aus Strings, falls vorhanden
    function removeTags(text) {
        var regex = /<[^>]*>/g;
        return text.replace(regex, '');
    }

    return {
        suggestFFViewModel: null,

        timeoutUpdateModel: null,

        init: function(suggestInputID, suggestOutputID, host, url) {
            try {
                // Activates knockout.js
                vi.chbeck.beckshop.suggestFF.suggestFFViewModel = new SuggestFFViewModel(suggestInputID, suggestOutputID, host, url)
                $("body").click(function () {
                    $("#" + suggestOutputID).css("display", "none");
                });
                ko.applyBindings(vi.chbeck.beckshop.suggestFF.suggestFFViewModel);
            } catch (e) {
            }
        },

        clickOutputSuggestItem: function (data, event) {
            // is required for the tracking of factfinder
          //  vi.chbeck.beckshop.suggestFF.suggestFFViewModel.trackSuggestFactFinder(data.factFinderBasisUrl, data.searchParams.replace("/FACT-Finder/", ""), vi.chbeck.beckshop.suggestFF.suggestFFViewModel.userInput);
            // open the trefferlist with the remark that is called by a suggestion, this is required for factfindertracking
            
            var link = data.CallBackLink + "&isSuggestCall=true&userInput=" + vi.chbeck.beckshop.suggestFF.suggestFFViewModel.userInput;
            window.location.href = link;
        },

        clickOutputSuggestProductItem: function (data, event) {
            window.location.href = data.attributes.deeplink + getAuthorAndTitle(data);
        },

        suggestFFTriggerPressKey: function (data1, event1) {
            clearTimeout(vi.chbeck.beckshop.suggestFF.suggestFFViewModel.timeoutUpdateModel);
            vi.chbeck.beckshop.suggestFF.suggestFFViewModel.timeoutUpdateModel = setTimeout(function (data, event) {
                if (event.keyCode != 13 && event.keyCode != 9 && event.keyCode != 16 && event.keyCode != 17 && event.keyCode != 18 && event.keyCode != 19 && event.keyCode != 20 && event.keyCode != 27 && event.keyCode != 33 && event.keyCode != 34 && event.keyCode != 35 && event.keyCode != 36 && event.keyCode != 37 && event.keyCode != 38 && event.keyCode != 39 && event.keyCode != 40) {
                    vi.chbeck.beckshop.suggestFF.suggestFFViewModel.userInput = escape(event.currentTarget.value);
                    $.getJSON(encodeURI(vi.chbeck.beckshop.suggestFF.suggestFFViewModel.autoSuggestPageUrl + "?query=" + event.currentTarget.value))
                      .done(function (data) {
                          try {
                              vi.chbeck.beckshop.suggestFF.suggestFFViewModel.suchVorschlaege(data['Suchvorschlaege']);
                              vi.chbeck.beckshop.suggestFF.suggestFFViewModel.autorenVorschlaege(data['Autorenvorschlaege']);
                              vi.chbeck.beckshop.suggestFF.suggestFFViewModel.produktVorschlaege(data['Produktvorschlaege']);
                              vi.chbeck.beckshop.suggestFF.suggestFFViewModel.serviceThemen(data['ServiceThemen']);
                              if (data['Suchvorschlaege'].length == 0) {
                                  $('#SuchvorschlaegeTabelle').css('display', 'none');
                              } else {
                                  $('#SuchvorschlaegeTabelle').css('display', '');
                              }
                              if (data['Autorenvorschlaege'].length == 0) {
                                  $('#AutorenvorschlaegeTabelle').css('display', 'none');
                              } else {
                                  $('#AutorenvorschlaegeTabelle').css('display', '');
                              }
                              if (data['ServiceThemen'].length == 0) {
                                  $('#ServiceThemenTabelle').css('display', 'none');
                              } else {
                                  $('#ServiceThemenTabelle').css('display', '');
                              }
                              if (data['Produktvorschlaege'].length == 0) {
                                  $('#TitelvorschlaegeTabelle').css('display', 'none');
                              } else {
                                  $('#TitelvorschlaegeTabelle').css('display', '');
                              }

                              if (data['Suchvorschlaege'].length > 0 && data['Autorenvorschlaege'].length == 0 && data['ServiceThemen'].length == 0) {
                                  $('#AbschnittZeileSuche').css('display', 'none');
                              } else {
                                  $('#AbschnittZeileSuche').css('display', '');
                              }
                              if (data['Autorenvorschlaege'].length > 0 && data['ServiceThemen'].length == 0) {
                                  $('#AbschnittZeileAutoren').css('display', 'none');
                              } else {
                                  $('#AbschnittZeileAutoren').css('display', '');
                              }
                              if (data['Suchvorschlaege'].length == 0 && data['Autorenvorschlaege'].length == 0 && data['ServiceThemen'].length == 0) {
                                  $('#TabellenLinksSpalte').css('display', 'none');
                              } else {
                                  $('#TabellenLinksSpalte').css('display', '');
                              }

                              $('#' + vi.chbeck.beckshop.suggestFF.suggestFFViewModel.suggestOutputID).css('display', '');
                              if (data['Suchvorschlaege'].length == 0 && data['Autorenvorschlaege'].length == 0 && data['ServiceThemen'].length == 0 && data['Produktvorschlaege'].length == 0) {
                                  $('#' + vi.chbeck.beckshop.suggestFF.suggestFFViewModel.suggestOutputID).css('display', 'none');
                              }
                          } catch (ex) {
                          }
                      });
                }
            }, 300, data1, event1);

        }
    };
}());//]]>
