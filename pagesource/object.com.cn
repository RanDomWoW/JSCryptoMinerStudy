    <script type="text/javascript">
        seajs.use('/seajs/seajs2/module/slide.js', function (slide) {

            slide1 = new slide({
                wrap: $(".banner"),
                btnClass: 'adchick',
                animateType: 'opacity',
                time: 1000,
                interval: 5000,
                btnType: 'mouseover',
                slideType: 'TWEEN_SINE_EASEINOUT'
            })

        });

        seajs.use('/seajs/seajs2/module/tab.js', function (tab) {
            var tab = new tab({
                wrap: $(".gamebox"),
                btnClass: '.tit',
                contClass: '.gb_d',
                auto: false,
                eventType: 'mouseover'
            })
        });
		
		 var imgWheel = {
		   eleWrap:null,
		   childs:null,
		   timer:null,
		   timer2:null,
		   index:0,
		   interval:120,
		   w:210,
		   init:function(){
			   var _this = this;
			   this.eleWrap = $(".gb_d");
			   this.childs = this.eleWrap.find("li");
			   this.childs.each(function(){
					
					$(this).find("a").eq(0).css({zIndex:0});
			   		$(this).find("a").eq(1).css({zIndex:1,left:-(_this.w)});
			   })
			   this.start(0);
			   
		   },
		   start:function(n){
			   var startTime = 5000;
			   var _this = this;
			   this.timer = setTimeout(function(){
				   _this.doAction();
			   },startTime+n)
			   
		   },
		   getPre:function(){
			   if(!this.isFirst()){
				   this.index--;
			   }
			   return this.childs.eq(this.index);
		   },
		   getNext:function(){
			    if(!this.isLast()){
				   this.index++;
			   }
			   return this.childs.eq(this.index);
		   },
		   getCurr:function(){
			   return this.childs.eq(this.index);
		   },
		   isLast:function(){
			  return  this.index == this.childs.length-1;
		   },
		   isFirst:function(){
			   return this.index == 0;
		   },
		   doAction:function(){
			  var _this = this;
			  setTimeout(function(){
				 var ele = _this.getCurr();
				 ele.find("a").eq(1).animate({left:0},function(){
					ele.find("a").eq(0).css({left:-(_this.w)});ele.find("a").eq(0).insertAfter(ele.find("a").eq(1));
					ele.find("a").eq(0).css({zIndex:0});
					ele.find("a").eq(1).css({zIndex:1})
				});
				 if(!_this.isLast()){
					_this.getNext();
				  	_this.doAction();
				  }else{
					  _this.index=0;
					  _this.start(_this.interval);
				  }
	          },this.interval)
		   }
		   
	   }
	   imgWheel.init();
    </script>
