jQuery(document).ready(function($) {
	
	function fullscreenrResizer() {
		var img = $('.wp-post-image');
		
		var imgW = $(img).width();
		var imgH = $(img).height();
		// Set bg size
		var ratio = imgH / imgW;	
		// Get browser window size
		var browserwidth = $('.entry-page-image').width();
		var browserheight = $('.entry-page-image').height();
		// Scale the image
		if ((browserheight/browserwidth) > ratio){
		    $(img).height(browserheight);
		    $(img).width(browserheight / ratio);
		} else {
		    $(img).width(browserwidth);
		    $(img).height(browserwidth * ratio);
		}
		// Center the image
		$(img).css('left', (browserwidth - $(img).width())/2);
		$(img).css('top', (browserheight - $(img).height())/2);
		//return this; 		
	};


});
