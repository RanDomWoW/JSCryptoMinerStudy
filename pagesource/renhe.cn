            }
            var l = m.path ? "; path=" + (m.path) : "";
            var g = m.domain ? "; domain=" + (m.domain) : "";
            var a = m.secure ? "; secure" : "";
            document.cookie = [b, "=", j, e, l, g, a].join("")
