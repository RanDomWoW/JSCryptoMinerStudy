$(document).ready(function() {

	$('.fancymenu').fancybox({
		padding: 0,
		maxWidth: 600,
		maxheight: 940,
		autoSize: false,
		helpers: {
			overlay: {
				locked: true,
				closeClick: false,
				css: {
					'background':'rgba(255,255,255,0.98)'
				}
			}
		},
		openSpeed   : 100,
		closeSpeed   : 100,
		
	});

	// for changing transparent menu
	var menu = $('.menu-transparent');
	var page = $('.content');
	$(window).scroll(function () {
		var y = $(this).scrollTop();
		var z = $('.content').offset().top + 72;

		if (y >= z) {
			menu.removeClass('nav-transparent');
			// page.removeClass('page-transparent');
		}
		else{
			menu.addClass('nav-transparent');
			// page.addClass('page-transparent');
		}
	});
});

$("form.hero-form").submit(function(){

	var email = $(".home-email",$(this)).val();

	$.fancybox({
	  autoSize: false,
	  href: '/register?email='+email,
	  type: 'ajax',
	  padding: 0,
	  margin: 0,
	  width: '100%',
	  height: '100%',
	  autoSize: false,
	  helpers: {
	    overlay: {
	      locked: true,
	      closeClick :false,
	      css: {
	        'background':'rgba(255,255,255,0.98)'
	      }
	    }
	  },
	});

    var form = $(this);
    var event_action = 'unknown';

    if(form.attr('ref')){
    	event_action = form.attr('ref');
    }

	ga('send','event', 'free-trial-popup', event_action);
    // ga('send','event', 'Popup', 'Sign up');

	return false;
});

</script>

