<script type="text/javascript">
  

    $(document).on('ready',function () {

      $('#email_subscription_success').hide();
      $('#email_subscription_failed').hide();
      $('#email_subscription_error').hide();

        $('.email-modal').magnificPopup({
          type: 'inline',
          preloader: false,
          focus: '#username',
          modal: true
        });

        $(document).on('click', '.popup-modal-dismiss', function (e) {
          e.preventDefault();
          $.magnificPopup.close();

          var expDate = new Date();

          expDate.setTime(expDate.getTime() + (86400000)); // add 15 minutes

          $.cookie('email_subscription_closed', 1, { path: '/', expires: expDate });

        });


        if(!$.cookie('email_subscription_closed')){
          setTimeout( function(){ 
            $('.email-modal').magnificPopup('open')
          }  , 2000 );
        }

        $('#email_subscription_btn').click(function (e) {

            e.preventDefault();
          
              $.ajax({
                  url: '/user/email_subscription',
                  type: 'post',
                  data: $('form#email_subscription_form').serialize(),
                  success: function(data) {
                    if(data.success == true){
                      $('#email_subscription_success').show();
                      var expDate = new Date();
                      expDate.setTime(expDate.getTime() + (1080000000));
                      $.cookie('email_subscription_closed', 1, { path: '/', expires: expDate });                      
                    } else {
                      $('#email_subscription_failed').show();
                    }
                  },
                  error: function () {
                      $('#email_subscription_error').show();
                  }
              });

        });
        
      });

</script>
