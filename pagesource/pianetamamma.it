        {	
        	$("div.jq-adv-only-responsive").each(function() {
    			
    			var obj = $(this),
    				dataAdv = $(this).data("adv");
                
    			if ( (dataAdv.trim() != "") && ($("div#"+dataAdv).length == 0) )				
                {    				    
                    obj.attr('id', dataAdv);
                    
                    obj.removeClass("jq-adv-responsive").removeAttr("data-adv");
                }
    		});            
            
            $("div.jq-adv-responsive").each(function() {
    			
    			var obj = $(this),
    				dataAdv = $(this).data("adv"),
                    adv = $("div#"+dataAdv);
    			                
    			if ( (dataAdv.trim() != "") && ($("div#"+dataAdv).length > 0) )				
                {                
                    adv.appendTo(obj);
                    
                    obj.removeClass("jq-adv-responsive");
                }
    		});
            
            $("div.jq-adv-noresponsive").remove();
	    }
    };
</script>


