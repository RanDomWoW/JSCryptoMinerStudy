<script>
    window.addEventListener("resize", throttle(redrawGrid, 66), false);
    document.addEventListener('DOMContentLoaded', redrawGrid);

    function redrawGrid()
    {
        var grids = document.querySelectorAll('[data-match-heights]'),
            i;

        for (i = 0; i < grids.length; ++i) {
            evenGridHeights(grids[i]);
        }
    }

    function evenGridHeights(gridElement)
    {
        var gridSquares = gridElement.children,
            maxHeight = 0,
            i,
            thisHeight;

        for (i = 0; i < gridSquares.length; ++i) {
            gridSquares[i].style.height = 'auto';
            thisHeight = gridSquares[i].offsetHeight;
            if (thisHeight > maxHeight) maxHeight = thisHeight;
        }

        for (i = 0; i < gridSquares.length; ++i) {
            gridSquares[i].style.height = maxHeight+'px';
        }
    }

