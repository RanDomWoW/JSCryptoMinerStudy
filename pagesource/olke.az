<script type="text/javascript">

	$(document).ready(function(){
		var baseUrl = "http://olke.az";
		var hotNews;
		var newsObj = $(".hot_news");
		var hotNewsPerPage = 15;
		var hotNewsPart = 0;
		var from = hotNewsPerPage * hotNewsPart;
		var interval;
		var i;

		getHotNews(from,hotNewsPerPage);

		function getHotNews(from,count) {
			$.ajax({
				type: "POST",
				url: baseUrl+"/news/hot_news",
				dataType: "json",
				data: { "from": from, "count": count, "page": hotNewsPart },

				success: function(res) {
					hotNews = res;
					if (res.length >= 0) {
						i=0;
						newsObj.text(res[i].title);
						newsObj.attr("href",baseUrl + "/news/detail/" + res[i].slug_url);
						interval = setInterval(function(){
							i++;
							changeHotNews(res);
						},6000);
					}
				}

			})
		}

		function changeHotNews(data) {
			var last = (data.length - 1);
			i = (i > last)? i = 0 : i;
			i = (i < 0)? i = last : i;
			newsObj.text(data[i].title);
			newsObj.attr("href",baseUrl + "/news/detail/" + data[i].slug_url);
		}


		$("#nextHotNews").click(function(){
			clearInterval(interval);
			i++;
			changeHotNews(hotNews);
			interval = setInterval(function(){
							i++;
							changeHotNews(hotNews);
						},6000);
		})
		$("#prevHotNews").click(function(){
			clearInterval(interval);
			i--;
			changeHotNews(hotNews);
			interval = setInterval(function(){
							i++;
							changeHotNews(hotNews);
						},6000);
		})
		
	});

</script>

