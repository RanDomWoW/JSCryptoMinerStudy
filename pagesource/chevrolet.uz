<script>
    var lang = "";
    $(document).ready(function () {
        $("input[type='CustomDate']").datepicker({ format: "dd-mm-yyyy", todayBtn: true, autoclose: true, language: lang });
      
        $('select').selectize({
            sortField: 'text'
        });
       
