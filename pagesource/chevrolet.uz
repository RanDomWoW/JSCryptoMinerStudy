    $(function () {

        var url = window.location.pathname;
        var name;
        if (url.indexOf('/ru/') > -1 || url.indexOf('/ru') > -1) {
            name = 'Русский';
            lang = 'ru';
        }
        else if (url.indexOf('/uz/') > -1 || url.indexOf('/uz') > -1) {
            name = 'O`zbek';
            lang = 'uz';
        }
        else if (url.indexOf('/en/') > -1 || url.indexOf('/en') > -1) {
            name = 'English';
            lang = 'en';
        } else {
            name = 'Русский';
            lang = 'ru';
        }


        $('#langName').prepend(name);
        
        //setSelectLang("");

    });

    function changeLang(lang) {

        var url = window.location.pathname.replace('/en/', lang).replace('/ru/', lang).replace('/uz/', lang);


        if (!url.startsWith(lang)) {
            url = lang.replace('/', '') + url;
        }

        $(location).attr('pathname', url);
    }

    function setSelectLang(lname) {
        var valTheme = "classic";
        if (lname === '' && lang === '') {
            lang ='ru';
        }

        if (lang === 'ru') {

            //$("select").select2({ language: 'ru', theme : valTheme});
        }
        else if (lang === 'uz') {
            $("select").select2({ language: 'uz', theme: valTheme });
        }
        else if (lang === 'en') {
            $("select").select2({ language: 'en', theme: valTheme });
        } else {
            $("select").select2({ language: 'ru', theme: valTheme });
        }
    }
</script>
