//            }
            $('.country_list').mouseover(function(){
                _self = $(this);
                if (_self.hasClass('active')) {
                    return;
                }
                $('.country_list').removeClass('active');
                _self.addClass('active');

                var data_id = _self.attr('name');
                $.each(charte_id, function (index, value) {
                    if (value == data_id) {
                        $('#'+data_id).removeClass('dn');
                    }
                    else {
                        if (!$('#' + value).hasClass('dn')) {
                            $('#' + value).addClass('dn');
                        }
                    }
                });
            });

            var chart_option = {
                legend:{

                },
                tooltip : {trigger: 'axis'},
                xAxis : [
                    {
                        type : 'category',
                        boundaryGap : false,
                    }
                ],
                grid:[{
                    left:'20%'
                }],
                yAxis :[{
                    type : 'value',
                    axisLabel : {
                        interval: 'auto'    // {number}
                    }
                }]

            };

            $.post('/ajax/echart',{_token:'xWs0eX7HpAKBhtUceP0WHBVx9oABSbhOs7lKaupI'},function(re){
                for(var i in re){
                    var myCharts = echarts.init(document.getElementById('country_'+i));
                    chart_option.legend.data=re[i].legend;
                    chart_option.xAxis[0].data=re[i].dates;
                    chart_option.yAxis[0].axisLabel.formatter=re[i].unit+'{value}';
                    var series = re[i].data;
                    chart_option.series=[];
                    for(var j in series){
                        chart_option.series.push(series[j]);
                    }
                    myCharts.setOption(chart_option);
                }
            });

