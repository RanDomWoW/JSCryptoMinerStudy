            });
            $('#expert_list .swiper-button-next').click(function(){
                expert_list.unslider('next');
            });



            //haiwaifangchan
            $(".country_house a").mouseover(function(){
                var country_house = $(this);
                var name = country_house.attr('name');
                if(name){
                    $('.country_house a').removeClass('active');
                    $(this).addClass("active");
                    $(".house_content ul").hide();
                    $("#house_"+$(this).attr('name')).show();
                    if(name!='re'&&country_house.data('init')!='true'){
                        $("#house_"+name).find('img').each(function(){
                            $(this).attr('src',$(this).attr('data-src'));
                        });
                        country_house.data('init',true);
                    }
                }
            });



            var charte_id = ['country_6','country_16','country_7','country_17','country_11'];
