                nav:false,
            }).on('unslider.change', function(event, index, slide) {
                $('.pagination-item').each(function(){$(this).removeClass('active')});
                $($(".pagination-item")[index]).addClass("active");
            });
            $('.pagination-item').each(function(){
                var banner_slider = $(this);
                banner_slider.click(function(){
                    banner_swiper.unslider('animate:'+banner_slider.attr('data'));
