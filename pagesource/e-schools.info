<script>
$(document).ready(function(){

    $('.i-image_carousel .blackout, .i-image_carousel .plus').hover(function () {
        $('.i-image_carousel').addClass('hov');
    }, function () {
        $('.i-image_carousel').removeClass('hov');
    });

    var activeInd = 0;
    var isAnimated = false;

    $('.i-image_carousel').css('overflow', 'visible');
    $("#foo1").carouFredSel({
        auto: false,
        circular: true,
        items: {
            visible: 1
        },
        scroll: {
            items: 1,
            duration: 600,
            onBefore: function () {
            },
            onAfter: function (oldItems, newItems) {
                isAnimated = false;
            }
        },
        prev: {
            button: false//"#foo1_prev"
        },
        next: {
            button: false//"#foo1_next"
        },
        pagination: "#foo1_pag"
    });
    $('.i-image_carousel .blackout, .i-image_carousel .plus').click(function () {
        $("#foo1 .fancy-full:first").trigger('click');
    });

    $('#i-sl-tabs .t-item a').click(function () {
        var div = $(this).parent();
        if (!div.hasClass('act') && !$('#foo1').is(':animated')) {
            activeInd = div.parent().index();
            changeIndexTabs();
            $('#foo1_pag a:eq(' + activeInd + ')').trigger('click');
        }
        return false;
    });


    $("#foo1_next").click(function () {
        var ind = getNewInd(+1);
        $('#i-sl-tabs .t-item a:eq(' + ind + ')').trigger('click');
        return false;
    });
    $("#foo1_prev").click(function () {
        var ind = getNewInd(-1);
        $('#i-sl-tabs .t-item a:eq(' + ind + ')').trigger('click');
        return false;
    });

    function changeIndexTabs() {
        $('#i-sl-tabs .t-item').removeClass('act');
        $('#i-sl-tabs .t-item:eq(' + activeInd + ')').addClass('act');
        $('#i-sl-info .item').fadeOut(300);
        $('#i-sl-info .item:eq(' + activeInd + ')').fadeIn(600);
    }

    function getNewInd(n) {
        var ind = activeInd;
        ind += n;
        var l = $('.i-sl-tabs .t-item').length;
        if (ind < 0) ind = l - 1;
        if (ind > l - 1) ind = 0;
        return ind;
    }
});

</script>

                            

