            $("#ad_close").removeClass("fadeOutUp");
            $("#ad_close").removeClass("dn");
            $("#ad_close").addClass("fadeInDown");
        setTimeout(function(){
            // clearInterval(s)
            <!--隐藏-->
            /*背景*/
            $("#ad_bg").removeClass("fadeInDown");
            $("#ad_bg").addClass("fadeOutUp");
            setTimeout(function(){
                $("#ad_bg").addClass("dn");
            },500)

            /*广告图片*/
            $("#ad_img").removeClass("fadeInDown");
            $("#ad_img").addClass("fadeOutUp2");
            setTimeout(function(){
                $("#ad_img").addClass("dn");
            },300)

            /*关闭按钮*/
            $("#ad_close").removeClass("fadeInDown");
            $("#ad_close").addClass("fadeOutUp");
            setTimeout(function(){
                $("#ad_close").addClass("dn");
            },100)
            $.cookie('open_ad', '1'); 
        },5000)
        $("#ad_close").click(function(){
            <!--隐藏-->
            /*背景*/
            $("#ad_bg").removeClass("fadeInDown");
            $("#ad_bg").addClass("fadeOutUp");
            setTimeout(function(){
                $("#ad_bg").addClass("dn");
                $("#ad_bg").removeClass("fadeOutUp");
            },500)

            /*广告图片*/
            $("#ad_img").removeClass("fadeInDown");
            $("#ad_img").addClass("fadeOutUp2");
            setTimeout(function(){
                $("#ad_img").addClass("dn");
                $("#ad_img").removeClass("fadeOutUp2");
            },300)

            /*关闭按钮*/
            $("#ad_close").removeClass("fadeInDown");
            $("#ad_close").addClass("fadeOutUp");
            setTimeout(function(){
                $("#ad_close").addClass("dn");
                $("#ad_close").removeClass("fadeOutUp");
            },100)

        })



        });

    </script>



