	<script type="text/javascript">

    function check_resize(){
        var ancho = $('body').width();
        if(ancho<700){
            $('#ImgSlide2').css({height:ancho});
        }
    }
    
    $(window).resize(function() {
      check_resize();
    });

        $(function() {
            var ancho = $('body').width();
            if(ancho<700){
                $('#ImgSlide2').crossSlide({
                                  sleep: 5,
                                  fade: 1,
                                  shuffle: true
                                }, [
                                    { src: 'images/slidehome/101_slide-1_3658imagen_cell.jpg' }, { src: 'images/slidehome/94_shop-online_2269imagen_cell.jpg' }, { src: 'images/slidehome/95_slide-2_6276imagen_cell.jpg' }, { src: 'images/slidehome/83_slide-3_4685imagen_cell.jpg' }                  
                                ]);
            }            
            check_resize();
			var mov1 = $("#mov1");
			var mov2 = $("#mov2");
			var mov3 = $("#mov3");
			
            $('.kwicks').kwicks({
                max : 500,
                spacing : 0
            });
			
			mov1.css('left', '50px');
			mov1.css('opacity', '0');

			mov2.css('left', '50px');
			mov2.css('opacity', '0');
			
			mov3.css('left', '50px');
			mov3.css('opacity', '0');

			mov1.animate({left:'0', opacity: '1'}, 1000, function(){
				mov2.animate({left:'0', opacity: '1'}, 1000);
					mov3.animate({left:'0', opacity: '1'}, 1000);				
			});

			
        });
