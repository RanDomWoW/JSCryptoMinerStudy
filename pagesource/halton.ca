                $('a[id^=navitem0]').map(function() { if ($(this).attr('id') != unhoveredNavItem.attr('id')) { unhover($(this)); } 

});
                if (startId != null && hoveredNavItem != null && unhoveredNavItem != null && 

hoveredNavItem.attr('id') == unhoveredNavItem.attr('id')) {
                    clearTimeout(startId);
                    startId = null;
                }
                unhover(unhoveredNavItem);
                selectedNavItem = null;
                hoverDelay = oldHoverDelay;
            }, 250);
        });

