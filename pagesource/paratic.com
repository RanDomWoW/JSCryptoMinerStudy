              boxHtml += '<span class="boxTitle">'+params.name+' <i class="fa fa-fw fa-ellipsis-h"></i> <span data-value-type="fark" class="'+params.color+'">'+params.fark+'%</span><i class="fa fa-fw '+params.okk+'"></i></span>';
            boxHtml += '</div>';
            boxHtml += '<div class="piyasaMarqueeBoxColRow">';
               boxHtml += '<span class="boxPriceSatis">'+parseFloat(params.satis).toFixed(2)+' <i class="fa fa-usd"></i></span>';
               boxHtml += '<span class="boxPriceAlis">ZAMAN : '+z[0] + z[1] + ":" + z[2] + z[3] + ":" + z[4]+z[5]+'</span>';
            boxHtml += '</div>';
          boxHtml += '</a>';
        }else{
          var boxHtml = '<a href="https://piyasa.paratic.com'+params.slug+'" class="piyasaMarqueeBox">';
            boxHtml += '<div class="piyasaMarqueeBoxColRow">';
              boxHtml += '<span class="boxTitle">'+params.name+' <i class="fa fa-fw fa-ellipsis-h"></i> <span data-value-type="fark" class="'+params.color+'">'+params.fark+'%</span><i class="fa fa-fw '+params.okk+'"></i></span>';
            boxHtml += '</div>';
            boxHtml += '<div class="piyasaMarqueeBoxColRow">';
               boxHtml += '<span class="boxPriceSatis">'+parseFloat(params.satis).toFixed(2)+' &nbsp;&nbsp;</span>';
               boxHtml += '<span class="boxPriceAlis">ALIŞ : '+parseFloat(params.alis).toFixed(2)+'</span>';
            boxHtml += '</div>';
