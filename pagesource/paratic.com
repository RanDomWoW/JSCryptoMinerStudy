                $('.archive-infinite-trigger').before(html);
                $("time.timeago").timeago();
            }
          });

          archive_infinity_params.page++;
        });
      break;



      case "home":
        $.get(templateUrl+"/template-parts/infinite-home.php", desktop_home_infinity_params, function(resp){
            resp = JSON.parse(resp);
            $.each(resp, function(k,v){
              var html = "<div class='post_list_post_wrapper'>"+
                            "<div class='column'>"+
                                "<a href='" + v.link + "' title='" + v.baslik + "'>"+
                                    "<img src='"+ v.feat_img +"' alt='" + v.baslik + "' />"+
                                    "<img src='https://paratic.com/wp-content/themes/paratic/img/authorCovers/profilePictures/" + v.authornicename + ".jpg' />"+
                                "</a>"+
                            "</div>"+
                            
                            "<div class='column'>"+
                                "<div>"+
                                    "<a href='" + v.link + "' title='" + v.baslik + "'>"+
