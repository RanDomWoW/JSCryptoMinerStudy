      piyasaMarquee.initSlider();
    })

    $('.piyasaMarqueeBoxesContainer').hover(function(){
      piyasaMarquee.pauseAnimation();
    }, function(){
      piyasaMarquee.start(PM_totalWidth + 200);
    })
  }) // doc. ready..


  var piyasaMarquee = {
    initSlider : function(){
      if ($('.piyasaMarqueeBoxesContainer .piyasaMarqueeBox').length < 1){
        setTimeout(function(){
          $('.piyasaMarqueeBoxesContainer .piyasaMarqueeBox').each(function(k,v){
            PM_totalWidth += $(v).width() + 170;
          });
        })
      }else{
        $('.piyasaMarqueeBoxesContainer .piyasaMarqueeBox').each(function(k,v){
          PM_totalWidth += $(v).width() + 170;
        });
      }
      if ($(window).width() < 1000 ){
        $('.piyasaMarqueeBoxesContainer').css({width : PM_totalWidth + 1400 + "px"})
      }else{
        $('.piyasaMarqueeBoxesContainer').css({width : PM_totalWidth + 200 + "px"})
      }
      setTimeout(function(){
        piyasaMarquee.start(PM_totalWidth + 200);
      }, 3000)
    },
    start : function(PM_totalWidth){
      $('.piyasaMarqueeBoxesContainer').animate( { marginLeft : "-="+PM_totalWidth}, {easing: "linear",duration:150000} );
      piyasaMarquee.checkScroll(PM_totalWidth);
    },
    checkScroll : function(PM_totalWidth){
      var scrollChecker = setInterval(function(){
        if (parseInt($('.piyasaMarqueeBoxesContainer').css("margin-left")) < 0 - (PM_totalWidth - $(window).width())){
          $('.piyasaMarqueeBoxesContainer').stop().animate({marginLeft : "0px"}, 1000, 'easeInBack');
          piyasaMarquee.start(PM_totalWidth + 200);
          clearInterval(scrollChecker);
        }
      }, 3000);
    }, 
    pauseAnimation : function(){
      $('.piyasaMarqueeBoxesContainer').stop();
    }, 
    resumeAnimation : function(){
      $('.piyasaMarqueeBoxesContainer').animate( { marginLeft : "-="+PM_totalWidth}, {easing: "linear",duration:150000} );
      piyasaMarquee.checkScroll(PM_totalWidth);
    }
  }
</script>

<script>
  $(function(){
    $(window).on('scroll', function(){
      if( $(window).scrollTop() > 200 ){
        $('.piyasaMarquee').css({visibility : 'visible'});
        $('.piyasaMarquee.hid').css({bottom : "0px"})
        $('.desktopShareBar').css({bottom : "74px"});
      }else{
        $('.piyasaMarquee').css({visibility : 'hidden'});
        $('.piyasaMarquee.hid').css({bottom : "-300px"})
        $('.desktopShareBar').css({bottom : "0px"});
      }
    })
  });
</script>
