                                    "</a>"+
                                    "<span>"+
                                        "<i class='fa fa-fw fa-clock-o'></i> <time class='entry-date-published timeago' datetime='" + v.datetime + "'></time>"+
                                    "</span>"+
                                "</div>"+
                            "</div>"+
                        "</div>";
                  $('.desktop-home-infinite-container').append(html);
                  $("time.timeago").timeago();
            });

            desktop_home_infinity_params.page++;
          });
      break;

      case "home_mobile":
        
        $.get(templateUrl+"/template-parts/infinite-mobile-home.php", mobile_home_infinity_params, function(resp){
        }).done(function(resp){
          resp = JSON.parse(resp, true);
          
            for (i in resp)
            {

                var mobile_home_post_html  = "<div class='mobile_post_box'>"+
                                              "<a href='"+resp[i].link+"'>"+
                                                "<div class='mobile_post_box_darkness'></div>"+
                                                "<img src='"+resp[i].feat_img+"' />"+
                                                "<h2>"+resp[i].baslik+"</h2>"+
                                              "</a>"+
                                            "</div>";

                $('#mobile-home-posts-container').append(mobile_home_post_html);
            }
            mobile_home_infinity_params.page++;
          });
      break;


      case "cat":
        
        var jsonUrl = "https://paratic.com/wp-json/wp/v2/posts?page="+single_post_bottom_category_params.page+"&per_page="+single_post_bottom_category_params.per_page+"&categories="+single_post_bottom_category_params.categories;
        $.getJSON(jsonUrl, function(data){
          $(data).each(function(k,v){
            $.getJSON(v._links["wp:featuredmedia"][0].href, function(respp){
              var featured = respp.source_url;
              var html_b  = '<a href="'+v.link+'" class="np_cat_big_post">';
                  html_b += '<div class="col">';
                  html_b += '<img src="'+featured+'" />';
                  html_b += '</div>';
                  html_b += '<div class="col">' + v.title.rendered + '</div>';
                  html_b += '</a>';
                  if ( currentPostId != v.id ){
                    $(html_b).appendTo('#cat_bigs');
                  }

            }); // featured
          }); // each
          if (data.length < 8){
            $('.infinityTriggerButton').hide();
          }
          single_post_bottom_category_params.page++;
        }); // get json
      break;
    }
  }
