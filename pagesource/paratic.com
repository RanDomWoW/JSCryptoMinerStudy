        }

        // console.log(boxHtml);
        $('.piyasaMarquee .piyasaMarqueeCol:nth-of-type(2) .piyasaMarqueeBoxesContainer').append(boxHtml);

      });

      $(data[1]).each(function(k,v){
        if (v.id == "bitcoin" || v.id == "litecoin" || v.id == "ethereum"){
          var params = {
            okk       : (v.percent_change_24h >= 0) ? "fa-long-arrow-up" : "fa-long-arrow-down",
            color     : (v.percent_change_24h >= 1) ? "green" : "red",
            slug      : "/kripto-coin/"+v.id+"/",
            symbol    : v.id,
            price_try : v.price_try,
            price_usd : v.price_usd,
            fark      : v.percent_change_24h,
            name      : v.name
          }
          var boxHtml = '<a href="https://piyasa.paratic.com'+params.slug+'" class="piyasaMarqueeBox">';
              boxHtml += '<div class="piyasaMarqueeBoxColRow">';
                boxHtml += '<span class="boxTitle">'+params.name+' <i class="fa fa-fw fa-ellipsis-h"></i> <span data-value-type="fark" class="'+params.color+'">'+params.fark+'%</span><i class="fa fa-fw '+params.okk+'"></i></span>';
              boxHtml += '</div>';
              boxHtml += '<div class="piyasaMarqueeBoxColRow">';
                 boxHtml += '<span class="boxPriceSatis">'+parseFloat(params.price_usd).toFixed(2)+'<i class="fa fa-usd"></i> </span>';
                 boxHtml += '<span class="boxPriceAlis">TL : '+parseFloat(params.price_try).toFixed(2)+'<i class="fa fa-try"></i></span>';
              boxHtml += '</div>';
            boxHtml += '</a>';

            $('.piyasaMarquee .piyasaMarqueeCol:nth-of-type(2) .piyasaMarqueeBoxesContainer').append(boxHtml);
