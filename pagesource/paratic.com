      var convertedText = cleanText.replace(/\s+/g, '_');
      var sira = (tagname == "H2") ? counter + ". " : "<i class='fa fa-fw fa-circle'></i>" ;
      if (tagname == "H4") { sira = "<i class='fa fa-fw fa-circle-o'></i>";}
      $(v).attr('id', convertedText);
      var linkTemplate = "<a href='#"+convertedText+"' data-href='"+convertedText+"' title='"+cleanText+"' class='paratic_TOC_"+tagname+" navigationLinks'>"+sira+titleText+"</a>";
      $('#paratic_TOC_links').append(linkTemplate);
      if(tagname == "H3"){subCounter++;}
      if(tagname == "H2"){counter++; subCounter = 1;}
      }
    });
    var toptop = (isMobile()) ? 80 : 60;
    $('a.navigationLinks').click(function(){
        console.log($(this).attr('data-href'));
        window.location.hash = $(this).attr('data-href');
        $('html, body').stop().animate({
            scrollTop: $( $(this).attr('href') ).offset().top - toptop
        }, 500, 'easeInOutCirc');
