  function prepareHeadingIds(_ht){
    _ht = _ht.split('');
    var problemliChars = ["ı", "ş", "ç", "ö", "ü", "ğ", "İ", "Ş", "Ç", "Ö", "Ü", "Ğ", "?" , "!", " ", "."];
    //var problemsizChars= ["i", "s", "c", "o", "u", "g", "I", "S", "C", "O", "U", "G", ""  , "" , "_"];
    var problemsizChars= ["i", "s", "c", "o", "u", "g", "i", "s", "c", "o", "u", "g", ""  , "" , "-", ""];
    var returnText = "";
    $.grep(_ht, function(k,v){
      if ($.inArray(k ,problemliChars) > 0){
        _ht[v] = problemsizChars[$.inArray(k ,problemliChars)];
      }
      returnText += _ht[v].toLowerCase();
    });
    return returnText;
  }
  
