    $(function(){

        var restApiUrl = "https://paratic.com/wp-json/wp/v2/posts";
            restApiUrl+= "?_embed";
            restApiUrl+= "&per_page=10";
            restApiUrl+= "&offset=4";
            restApiUrl+= "&format=standart";
            restApiUrl+= "&exclude[]=81710&exclude[]=81751&exclude[]=83788&_"+ new Date().getTime();

        $.get(restApiUrl, function(sliderElements){

            $('.newsSliderWrapper .slide').attr('href', sliderElements[0].link).attr("title", sliderElements[0].title.rendered);;
            $('.newsSliderWrapper .slide .image').css({backgroundImage : "url("+sliderElements[0]._embedded["wp:featuredmedia"][0].source_url+")"});
            $('.newsSliderWrapper .slide .title').text(String.fromHtmlEntities(sliderElements[0].title.rendered));

            for (i in sliderElements){
                $('a.number[data-number="'+i+'"]').attr('href', sliderElements[i].link);
            }

            $('.newsSliderWrapper .number').on('mouseover', function(){
                if($(this).hasClass('active')){
                    return false;
                }
                $('.newsSliderWrapper .slide .image').css({ opacity: 0});
                $('.newsSliderWrapper .slide .title').css({ paddingBottom : "40px", opacity : 0});

                $('.newsSliderWrapper .number.active').removeClass('active');
                $(this).addClass('active');
                var orderNumber = $(this).attr('data-number');
                var thisSlide = sliderElements[orderNumber];

                $('.newsSliderWrapper .slide').attr('href', thisSlide.link).attr("title", thisSlide.title.rendered);
                $('.newsSliderWrapper .slide .image').css({backgroundImage : "url("+thisSlide._embedded["wp:featuredmedia"][0].source_url+")"});
                $('.newsSliderWrapper .slide .title').text(String.fromHtmlEntities(thisSlide.title.rendered));

                $('.newsSliderWrapper .slide .image').stop().animate({left: "0px", opacity: 1, height: '360px'}, 400);
