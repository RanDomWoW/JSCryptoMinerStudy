  $(function(){
    $.get("https://piyasa.paratic.com/tools/API/widgets.php?type=paraticbant", function(data){
      data = JSON.parse(data);
      $(data[0]).each(function(k,v){
        var params = {
          okk     : (v.dailyChangePercentageDirection == 1) ? "fa-long-arrow-up" : "fa-long-arrow-down",
          color   : (v.dailyChangePercentageDirection == 1) ? "green" : "red",
          slug    : "",
          symbol  : v.c,
          alis    : v.bid,
          satis   : v.ask,
          fark    : v.dailyChangePercentage
        }

        if ( v.c == "USD/TRL" ) { params.slug = "/doviz/dolar/"; params.name = "Dolar"; }
        if ( v.c == "EUR/TRL" ) { params.slug = "/doviz/euro/"; params.name = "Euro"; }
        if ( v.c == "GBP/TRL" ) { params.slug = "/doviz/sterlin/"; params.name = "Sterlin"; }

        if ( v.c == "EUR/USD" ) { params.slug = "/forex/parite/eur-usd/"; params.name = v.c; }
        if ( v.c == "GBP/USD" ) { params.slug = "/forex/parite/gbp-usd/"; params.name = v.c; }
        if ( v.c == "USD/JPY" ) { params.slug = "/forex/parite/usd-jpy/"; params.name = v.c; }

        if ( v.c == "SGLD"    ) { params.slug = "/altin/gram-altin/"; params.name = "Gram Altın";}
        if ( v.c == "SGCEYREK") { params.slug = "/altin/ceyrek-altin/"; params.name = "Çeyrek Altın"; }
        if ( v.c == "XAU/USD" ) { params.slug = "/forex/parite/xau-usd/"; params.name = "Altın ONS"; }

        if ( v.c == "COILUS,CFD" ) { params.slug = "/forex/emtia/ham-petrol/"; params.name = "Ham Petrol"; params.satis = v.last; params.alis = v.time; params.emtia = true; }
        if ( v.c == "SILVUS,CFD" ) { params.slug = "/forex/emtia/gumus/"; params.name = "Gümüş"; params.satis = v.last; params.alis = v.time; params.emtia = true; }
        if ( v.c == "COPPUS,CFD" ) { params.slug = "/forex/emtia/bakir/"; params.name = "Bakır"; params.satis = v.last; params.alis = v.time; params.emtia = true; }

        // console.log(params.name, params.slug);
        
        if (params.emtia){
          var z = params.alis.split("");
          var boxHtml = '<a href="https://piyasa.paratic.com'+params.slug+'" class="piyasaMarqueeBox">';
