    }
    if(h4 == 'evet'){
      basliklar += ', .entry-content h4'
    }
    console.log(basliklar);
    var counter    = 1;
    var subCounter = 1;

    $(basliklar).each(function(k,v){
      if($(v).parent().prop('class') != 'ilginizi-cekebilir-wrapper'){
      var tagname     = $(v).prop('tagName');
      var tabsize     = (tagname == 'H2') ? "" : "<span class='paratic_TOC_"+tagname+"'><i class='fa fa-fw fa-circle'></i></span>";
