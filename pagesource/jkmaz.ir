    <script type="text/javascript">
        $(document).ready(function () {
            $(document).on("click", ".NewsGroupBtn", function () {
                var GroupId = $(this).attr("data-id");
                $("#NewsGroupId").val(GroupId);
                $("#Step").val(1);
                $("#NewsList").addClass("DisableItems");
                $.get("/Home/ShowDocumentList", {
                    OrgId: $("#OranaizeId").val(),
                    Step: $("#Step").val(),
                    NewsGroupId: $("#NewsGroupId").val()
                }, function (data) {
                    $("#NewsList").html(data);
                    $("#NewsList").removeClass("DisableItems");
                });
            });

            $(document).on("click", ".Page-number", function () {
                var Step = $(this).attr("data-step");
                $("#Step").val(Step);
                $("#NewsList").addClass("DisableItems");
                $.get("/Home/ShowDocumentList", {
                    OrgId: $("#OranaizeId").val(),
                    Step: $("#Step").val(),
                    NewsGroupId: $("#NewsGroupId").val()
                }, function (data) {
                    $("#NewsList").html(data);
                    $("#NewsList").removeClass("DisableItems");
                });
            });

            $(document).on("click", ".previous", function () {
                var Step = $("#Step").val() - 1;
                $("#Step").val(Step);
                $("#NewsList").addClass("DisableItems");
                $.get("/Home/ShowDocumentList", {
                    OrgId: $("#OranaizeId").val(),
                    Step: $("#Step").val(),
                    NewsGroupId: $("#NewsGroupId").val()
                }, function (data) {
                    $("#NewsList").html(data);
                    $("#NewsList").removeClass("DisableItems");
                });
            });

            $(document).on("click", ".next", function () {
                var Step = parseInt($("#Step").val()) + 1;
                $("#Step").val(Step);
                $("#NewsList").addClass("DisableItems");
                $.get("/Home/ShowDocumentList", {
                    OrgId: $("#OranaizeId").val(),
                    Step: $("#Step").val(),
                    NewsGroupId: $("#NewsGroupId").val(),
                }, function (data) {
                    $("#NewsList").html(data);
                    $("#NewsList").removeClass("DisableItems");
                });
            });
        });
    </script>


