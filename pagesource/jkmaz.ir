<script type="text/javascript">

    $(document).ready(function () {
        $(document).on("click", ".Navigate-Slide", function () {
            $(".Navigate-Slide").removeClass("NavigateSlideSelect");
            $(this).addClass("NavigateSlideSelect");
            var SlideId = $(this).data("id");
            //$("#SlideImagesDiv").find(".ShowSlide").first().css("display", "none !important");
            $(".Image-Slide").removeClass("ShowSlide").addClass("HideSlide");
            $("#SlideImagesDiv").find("[data-id='" + SlideId + "']").removeClass("HideSlide").addClass("ShowSlide");
        });
        $(function () {
            setInterval(Slider, 5000);
        });
        function Slider() {
            var $Navigate = $("#NavigateSlideDiv").find(".NavigateSlideSelect");
            if ($Navigate.next().length == 0) {
                $Navigate.removeClass("NavigateSlideSelect");
                var $FirstTag = $("#NavigateSlideDiv").find(".Navigate-Slide").first();
                $FirstTag.addClass("NavigateSlideSelect");
                var SlideId = $FirstTag.data("id");
            }
            else {
                $Navigate.removeClass("NavigateSlideSelect").next().addClass("NavigateSlideSelect");
                var SlideId = $Navigate.next().data("id");
            }
            $(".Image-Slide").removeClass("ShowSlide").addClass("HideSlide");
            $("#SlideImagesDiv").find("[data-id='" + SlideId + "']").removeClass("HideSlide").addClass("ShowSlide");
        }
    });
</script>

