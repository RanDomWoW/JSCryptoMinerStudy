<script>
	var typingTimer;
	var doneTypingInterval = 1000;
	var $inputSearch = $("#search");
	var searchCountPageview = {'eshop': 0, 'gallery': 0, 'magazine': 0};

	$inputSearch.on('keyup', function () {
		clearTimeout(typingTimer);
		typingTimer = setTimeout(sendToTagManagerSuggest, doneTypingInterval);
	});

	$inputSearch.on('keydown', function () {
		clearTimeout(typingTimer);
	});

	function sendToTagManagerSuggest () {
		var url = 'https://www.megapixel.cz/naseptavac/?query=' + $inputSearch.val() + '&section=suggest';

		dataLayer.push({
			'event': 'ga.pageview',
			'pageTitle': 'Vyhledávání - našeptávač | Megapixel',
			'pageUrl': url,
			'pageType': 'search',
			'searchResultsCount': searchCountPageview
		});
	}
</script>
