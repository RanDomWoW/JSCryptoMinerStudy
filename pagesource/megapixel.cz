			<script>
				// newsletter
					if( $(window).width() > 580){
						if ($.cookie('mpxlNewsletter') == null || (parseInt($.cookie('mpxlNewsletter')) < (new Date).getTime())) {
							var tNl;
							clearTimeout(tNl);
							tNl = setTimeout(function(){
								nlBox = new $.pdBox();
								nlBox.width = '720';
								nlBox.className = 'nlBox animated slideInUp';
								nlBox.closeOverlay = false;
								nlBox.isOverlay = false;
								nlBox.openUrl('/newsletter');
								nlBox.onClose = function($box){
									var pathname = window.location.pathname;
		//				            ga('send', 'event', 'PopupNewsletter', 'close', pathname);
									if ($.cookie('mpxlNewsletter') == null) {
										var d = new Date;
										d.setDate(d.getDate() + 7);
										$.cookie('mpxlNewsletter', d.getTime(), { expires: 900 })

									} else {
										var d = new Date;
										d.setMonth(d.getMonth() + 6);
										$.cookie('mpxlNewsletter', d.getTime(), { expires: 900 });
									}
								};
							}, 1500);
						}
					}
			</script>



