<script>
	(function($) {
		$(function() {
			$('.group-select').on('click', function() {
				$('.group-select .menu').toggleClass('open');
			});
			$('.group-select .menu li').on('click', function() {
				$('.now-selected').text($(this).find('a').text());
			});
			$('body').on('shown.bs.tab', '.widgetGroup [data-toggle="tab"]', function() {
				var code = $(this).attr('href').replace('#layer', '');
				var html = '';
				$.getJSON('/tool/mallGoods.php', {
					code: code
				}).done(function(data) {
					if (0 == data.length) return false;
					html += '   <div class="owl-carousel owl-demo">';
					$.each(data, function (key, row) {
						html += '   <div class="item">';
						html += '       <a href="' + row.url + '">';
						html += '           <div class="item-image">';
						html += '               <div class="item-thumbnail"><img src="' + row.thumbnailFilePath + '" class="img-responsive" /></div>';
						html += '           </div>';
						html += '           <div class="item-info">';
						html += '               <div class="brand"><img src="/imgs/system/iso/iso_' + row.countryIsoCode.toLowerCase() + '.gif" alt="" />' + row.brandName + '</div>';
						html += '               <div class="title">' + row.goodsName + '</div>';
						html += '               <div class="price">';
						html += '                   <span class="price-new">' + row.price + '</span>';
						if(row._fixPrice) {
							html += '                   <span class="price-old">' + row.fixPrice + '</span>';
						}
						html += '               </div>';
						html += '           </div>';
						html += '       </a>';
						html += '   </div>';
					});
					html += '   </div>';
					var This = $('#layer' + code).html(html);
					if (0 < $('> div > .item', This).length) {
						$('> div', This).owlCarousel({
							loop: true,
//							margin: 10,
							nav: true, // 양 옆 버튼
							navSpeed: 500,
							navText: ["", ""], // 양 옆 버튼글자
							dots: false,
							autoplay: true, // 자동 슬라이드
							autoplayTimeout: 1500,
							autoplaySpeed: 1000,
							autoplayHoverPause: true,
							responsive: {
								0: {items: 2},
								480: {items: 2},
								768: {items: 3},
								992: {items: 4},
								1200: {items: 5}
							}
						});
					}
				});
			});

			$('.widgetGroup [data-toggle="tab"]:first').tab('show').trigger('shown.bs.tab');
		})
	})(jQuery);
</script>		</div>
