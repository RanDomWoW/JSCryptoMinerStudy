<script>
    var form = layui.form(),layer = layui.layer;


    $('.doCompalin').click(function () {
        var type=$('input[name="type"]').val();
        var content=$.trim($('textarea[name="content"]').val());
        if(content.length == 0){
            swal("请输入您想吐槽的内容")
            return false;
        }
        var concats=$.trim($('input[name="concats"]').val());
        if(concats.length == 0){
            swal("请输入联系人")
            return false;
        }

        var tel=$.trim($('input[name="tel"]').val());
        var reg6 = /^1[34578]\d{9}$/;
        var reg1 = /^((\(\d{3}\))|(\d{3}\-))?13[0-9]\d{8}|15[89]\d{8}/;
        var reg2 = /^([0-9]{3,4}-)?[0-9]{7,8}$/;
        var reg3 = /^(0[0-9]{2,3}\-)?([2-9][0-9]{6,7})+(\-[0-9]{1,4})?$/;
        var reg4 = /^((\d{11})|^((\d{7,8})|(\d{4}|\d{3})-(\d{7,8})|(\d{4}|\d{3})-(\d{7,8})-(\d{4}|\d{3}|\d{2}|\d{1})|(\d{7,8})-(\d{4}|\d{3}|\d{2}|\d{1}))$)$/;
        var reg5 = /^400-([0-9]){1}([0-9-]{7})$/;

        if(!(/^((0\d{2,3}-\d{7,8})|(1[35784]\d{9}))$/.test(tel))){
            swal("请输入正确的电话")
            return false;
        }
        var email=$.trim($('input[name="email"]').val());
        var reg = /^[A-Za-z0-9_\u4e00-\u9fa5]+@[a-zA-Z0-9_-]+(\.[a-zA-Z0-9_-]+)+$/;
        if(!reg.test(email) ){
            swal("请填写正确的邮箱")
            return false;
        }
        var addContent = $('.addContent').val()
        var addConcats =   $('.addConcats').val()
        var addTel = $('.addTel').val()
        var addEmail =  $('.addEmail').val()
        var addType=$('.addType').val()
        /*if(addType==type && addContent==content && concats==addConcats &&addTel==tel && email==addEmail){
            swal("休息一下再吐槽吧"); return false;
        }*/
        $('.setTimeS').removeClass('doCompalin');
        setTimeout(function(){
            $('.setTimeS').addClass('doCompalin');
            $('.setTimeS').removeClass('sss');
        }, 8000);

        $.post('/createComplain',{type:type,content:content,concats:concats,tel:tel,email:email},function(res){


            if(res.status==1){
                $('.addContent').val(content)
                $('.addConcats').val(concats)
                $('.addTel').val(tel)
                $('.addEmail').val(email)
                $('.addType').val(type);
                $('.setTimeS').addClass('sss');
                swal('吐槽成功');
                return false;
            }
            if(res.status==3){
                $('.addContent').val(content)
                $('.addConcats').val(concats)
                $('.addTel').val(tel)
                $('.addEmail').val(email)
                $('.addType').val(type);
                $('.setTimeS').addClass('sss');
                swal('休息30s再吐槽吧');
                return false;
            }
            else{
                swal('吐槽失败');
                return false;
            }
        },'json')
    })
    $('.sss').click(function () {
        swal('休息30s再吐槽吧');
        return false;
    });
</script><!-- 返回顶部结束 -->







