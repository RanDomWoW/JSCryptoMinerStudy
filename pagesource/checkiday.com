<script>
  documentReady(function() {
    $('.share-event').click(function(e) {
      var id = $(this).parent().data('mdl-for');
      // ga('send', 'event', [eventCategory], [eventAction], [eventLabel], [eventValue], [fieldsObject]);
      ga('send', 'event', 'share', this.text, id.toLowerCase());
    });

    // Set moment timezone to match server
    moment.tz.setDefault("America/Los_Angeles");

    // From: https://github.com/js-cookie/js-cookie/blob/master/SERVER_SIDE.md#php
    PHPCookies = Cookies.withConverter({
      write: function(value) {
        // Encode all characters according to the "encodeURIComponent" spec
        return encodeURIComponent(value)
          // Revert the characters that are unnecessarly encoded but are
          // allowed in a cookie value, except for the plus sign (%2B)
          .replace(/%(23|24|26|3A|3C|3E|3D|2F|3F|40|5B|5D|5E|60|7B|7D|7C)/g, decodeURIComponent);
      },
      read: function(value) {
        return value
          // Decode the plus sign to spaces first, otherwise "legit" encoded pluses
          // will be replaced incorrectly
          .replace(/\+/g, ' ')
          // Decode all characters according to the "encodeURIComponent" spec
          .replace(/(%[0-9A-Z]{2})+/g, decodeURIComponent);
      }
    });

    settings = PHPCookies.getJSON('settings');

    if (!settings) {
      settings = {
        adult: false,
        simple: false,
        tz: moment.tz.guess(),
      };

      var cookieOptions = {
        expires: 200,
        secure: true,
      };
      if (PHPCookies.set('settings', settings, cookieOptions)) {
        snackbar.push({
          message: 'Timezone ' + settings.tz + ' detected.',
          timeout: 7500,
          actionText: 'Reload',
          actionHandler: function() { location.reload(); },
        });
      }
    }

    // TODO update once this is fixed and released: https://github.com/google/material-design-lite/issues/4963
    /*
    componentHandler.registerUpgradedCallback('MaterialSnackbar', function(snackbar) {
      console.log('registerUpgradedCallback snackbar', snackbar);
    });
    */
    $('#snackbar').on('mdl-componentupgraded', function(e) {
      if (e.target === this) {
        $(this).off('mdl-componentupgraded');

        QueuedCallback(snackbar, function(e) {
          $('#snackbar')[0].MaterialSnackbar.showSnackbar(e);
        });
      }
    });

    componentHandler.registerUpgradedCallback('MaterialLayout', function() {
      // Prevent scroll-to-top
      $('.mdl-layout__content').scrollTop(prevScrollPos);
      // Scroll to #id if set.
      if (window.location.hash) window.location = window.location.hash;
      // To prevent ads from loading 'twice', load AdSense JS after MDL component #layout is upgraded.
      $.getScript('//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js').fail(function() {
        $('ins.adsbygoogle').each(function(i, obj) {
          $(obj).parent().html('<h2 class="mdl-card__supporting-text">Please <a href="https://help.getadblock.com/support/solutions/articles/6000055743">allow ads</a> to keep Checkiday free!</h2>');
        });
      }).always(function() {
        if (window.Masonry) $('#masonryGrid').masonry('layout'); // Reload Masonry to fit changed cards.
      });
    });

    // Copy to clipboard
    var clip = new ClipboardJS('.clip');
    clip.on('success', function() {
      snackbar.push({
        message: 'Copied to clipboard.',
      });
    });

    // Add target="_blank" to all external links without a target
    $("a[href^='http']").not("a[href^='https://www.checkiday.com']").not('a[target]').attr('target', '_blank');
  });
</script>
