        }

        function CheckEmailAvailability(email) {
            var returnval = null;
            $.ajax({
                type: "POST",
                url: '/Registrations/CheckEmailAvailability', // the URL of the controller action method
                data: { Email: email }, // optional data
                datatype: "JSON",
                async: false,
                success: function (result) {
                    if (result.ok) {
                        returnval = result.data;
                    }
                }
            });
            return returnval;
        }

