                if ($(".main-flotingbarwrapper").is(".active")) { $('.main-flotingbarwrapper').removeClass('active'); } else { $('.main-flotingbarwrapper').addClass('active'); }
                $('.signuplink').click();
            });

            // Out side click close script start
            $('.ac-menulink,.accountwrapper').click(function (event) {
                $('.accountwrapper').show();
                event.stopPropagation();
            });
            $("html").click(function () {
                $(".accountwrapper").hide();
            });
            // Out side click close script end 

            $('.loginlink2').click(function () {
                $('.loginwrapper2').show("slow");
                $('.signupwrapper').hide("slow");
                $('.forgotwrapper').hide("slow");
                $('.forgotlink-hide').hide("slow");
                $('.signuplink').show("slow");
                $('.loginlink2').show("slow");
                if ($(".loginlink2").is(".active")) { } else { $('.loginlink2').addClass('active'); }
                $('.signuplink').removeClass('active');
            });
            $('.signuplink').click(function () {
                $('.loginwrapper2').hide("slow");
                $('.signupwrapper').show("slow");
                $('.forgotwrapper').hide("slow");
                if ($(".signuplink").is(".active")) { } else { $('.signuplink').addClass('active'); }
                $('.loginlink2').removeClass('active');
            });

            $('.forgotlink').click(function () {
                $('.forgotwrapper').show("slow");
                $('.loginwrapper2').hide("slow");
                $('.loginlink2').hide("slow");
                $('.backtologinlink').show("slow");
                $('.signuplink').hide("slow");
                $('.forgotlink-hide').show("slow");

                if ($(".forgotlink").is(".active")) { } else { $('.forgotlink').addClass('active'); }
                $('.loginlink2').removeClass('active');
                if ($(".forgotlink-hide").is(".active")) { $('.forgotlink-hide').removeClass('active'); } else { $('.forgotlink-hide').addClass('active'); }
            });
