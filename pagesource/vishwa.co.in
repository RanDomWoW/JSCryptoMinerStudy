        function LoginValidate() {
            if ($.trim($('#LEmailId').val()) == '') { $('#LEmailId').focus().addClass("validate"); return false; } else { $('#LEmailId').focus().removeClass("validate"); }
            if (!Common.CheckEmail($.trim($('#LEmailId').val()))) { $('#LEmailId').val('').attr('placeholder', 'Please enter valid email.'); $('#LEmailId').addClass("validate"); return false; } else { $('#LEmailId').focus().removeClass("validate"); }
            if ($.trim($('#LPassword').val()) == '') { $('#LPassword').focus().addClass("validate"); return false; } else { $('#LPassword').focus().removeClass("validate"); }

