        function RegisterValidation() {
            if ($.trim($('#RName').val()) == '') { $('#RName').focus().addClass("validate"); return false; } else { $('#RName').focus().removeClass("validate"); }
            if ($.trim($('#RPhoneNo').val()) == '') { $('#RPhoneNo').focus().addClass("validate"); return false; } else { $('#RPhoneNo').focus().removeClass("validate"); }
            if ($.trim($('#REmailId').val()) == '') { $('#REmailId').focus().addClass("validate"); return false; } else { $('#REmailId').focus().removeClass("validate"); }
            if (!Common.CheckEmail($.trim($('#REmailId').val()))) { $('#REmailId').val('').attr('placeholder', 'Please enter valid email.'); $('#REmailId').addClass("validate"); return false; } else { $('#REmailId').focus().removeClass("validate"); }
            if (!CheckEmailAvailability($.trim($('#REmailId').val()))) { $('#REmailId').val('').attr('placeholder', 'Email address already exist.'); $('#REmailId').addClass("validate"); return false; } else { $('#REmailId').removeClass("validate"); }
            if ($.trim($('#RPassword').val()) == '') { $('#RPassword').focus().addClass("validate"); return false; } else { $('#RPassword').focus().removeClass("validate"); }
            if (!Common.ValidatePassword($('#RPassword').val())) { $('#spanRPassword').html('Reqired minimum 8 charecters (atleast one special charecter & one numeric charecter).'); $('#RPassword').focus(); return false; } else { $('#spanRPassword').html(''); $('#RPassword').focus().removeClass("red-border"); }
            if ($.trim($('#RConfirmPassword').val()) == '') { $('#RConfirmPassword').focus().addClass("validate"); return false; } else { $('#RConfirmPassword').focus().removeClass("validate"); }
            if ($('#RConfirmPassword').val() != $('#RPassword').val()) { $('#spanRConfirmPassword').html('Password and Confirm password should be match.'); $('#RConfirmPassword').focus().addClass("validate"); return false; } else { $('#spanRConfirmPassword').html(''); $('#RConfirmPassword').removeClass("validate"); }

            if ($('#RtxtCaptcha').val() == "") { $('#RtxtCaptcha').focus().addClass("validate"); return false; } else { $('#RtxtCaptcha').html(''); $('#RtxtCaptcha').removeClass("validate"); }
            var str = Common.GetCaptcha();
