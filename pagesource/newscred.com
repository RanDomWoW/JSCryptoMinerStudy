<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.min.js"></script>
<script>

	// main bindings
	jQuery(document).ready(function($) {

		// Diagonal Angle Parallax Functions
		function ltrParallax(intensity, element, angle) {
	      $(window).scroll(function() {
	        var scrollTop = $(window).scrollTop();
	        var imgPos = scrollTop / intensity + 'px';
	        element.css('transform', 'perspective(1px) rotate(-' + angle + 'deg) translateY(' + imgPos + ')');
	      });
   		 }

		function rtlParallax(intensity, element, angle) {
			$(window).scroll(function() {
				var scrollTop = $(window).scrollTop();
				var imgPos = scrollTop / intensity + 'px';
				element.css('transform', 'perspective(1px) rotate(' + angle + 'deg) translateY(' + imgPos + ')');
			});
		}

		// Back To Top Function
		function backToTop() {
			$(window).scroll(function() {
				var scroll = $(this).scrollTop();
				var heroHeight = $('.et_pb_section_0').height();

				if (scroll < heroHeight) {
					$('.back-to-top').addClass('hide');
				} else {
					$('.back-to-top').removeClass('hide');
				}
			});
		}

		backToTop();

		$('body').on('click', '.back-to-top', function() {
			$('html, body').animate({
				scrollTop: 0
			}, 'slow');
		})

		// Marketo Forms Placeholders
		$('.marketo-loader.mktoForm').ready(function() {
			setTimeout(function() {
				$('.mktoField').each(function() {
					var labelRaw = $(this).parents('.mktoFieldWrap').find('.mktoLabel').text();
					var label = labelRaw.replace(/[:*,]+/g, '');
					if ($(this).hasClass('mktoRequired')) {
						label = '*' + label;
					}

					$(this).attr('placeholder', label);
					if ($(this).is('select')) {
						$(this).find('option:first-child').text(label);
					}
				});
			}, 1000);
			console.log('form loaded');
		});

		// On Marketo Form Submission
		MktoForms2.whenReady(function (form) {
				form.onSuccess(function(values, followUpUrl) {
						// form.getFormElem().html('Thanks for reaching out!');
						console.log('thanks');
						alert("Thank you! You'll be hearing from us really soon.");
						$('.mktoField').val('');
						return false;
				});
		});

		// Homepage Add Diagonal Blocks
		$('.home .client-gallery-container').append('<div class="homepage-block ltr"></div>');

		$('.rtl-block').prepend('<div class="block rtl"></div>');
		$('.ltr-block').prepend('<div class="block ltr"></div>')

		ltrParallax(11, $('.t1-container .ltr'), 8);
		rtlParallax(11, $('.t1-container .rtl'), 8);
		ltrParallax(11, $('.t2-container .ltr'), 8);
		rtlParallax(11, $('.t2-container .rtl'), 8);

		// Homepage Create Mobile Menu from Existing Nav
		$('.nav__menu__item').each(function () {
			var navItem = $(this).clone();
			$('.nav__menu--mobile').append(navItem);
		});

		$('body').on('click', '#menu-toggle', function() {
			$('.mobile-menu-wrapper').toggleClass('open');
			$('body').toggleClass('mobile-menu-open');
		});

		// Close Modals
		$('body').on('click', '.close-modal', function() {
			$('body').removeClass('modal-open');
			$('.modal-overlay').html('');
		})


		// About Page Leadership Modals
		$('body').on('click', '.leadership-tiles .et_pb_team_member_image', function() {
			$('body').addClass('modal-open');
			var bio = $(this).parents('.et_pb_team_member').find('.et_pb_team_member_description .leadership-modal').clone();
			$('.modal-overlay').html(bio);
		})

		// Careers Page How We Work Modal
		$('body').on('click', '.ceo-letter-section .read-more', function() {
			$('body').addClass('modal-open');
			var howWeWork = $(this).parents('.et_pb_text_inner').find('.ceo-letter-modal').clone();
			$('.modal-overlay').html(howWeWork);
		})

		// Careers Page Testimonial Modal
		$('body').on('click', '.careers-testimonial-row .et_pb_image', function() {
			$('body').addClass('modal-open');
			var videoLink = $(this).find('img').attr('title');
			var videoModal =   "<div class='testimonial-modal'>\
								  <div class='close-modal'></div>\
								  <iframe src='" + videoLink + "' width='100%' height='100%' frameborder='0' webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>\
								</div>";
			$('.modal-overlay').html(videoModal);
		})

		// Careers Page hide content for job postings
		if (window.location.href.indexOf("careers/?gh_jid") > -1) {
			$('.et_pb_section, #sectionTitle h1').hide();
			$('#jobSection').show();
		}

		// Add 'Read more' to Insights blocks
		$('#insightsArticles').prepend('<a class="read-more" href="https://insights.newscred.com/" target="_blank">See all articles &#8594;</a>');

		// Add 'Load more' to Press
		$('.press-insights-section').append('<div class="load-more"><span>View more</span</div>');
		$('body').on('click', '.press-insights-section .load-more', function() {
			$('.press-insights-section .insights-post').removeClass('hidden');
			$(this).remove();
		})

		// Create Banner Cookie
		if ( !$.cookie('banner-read') ) {
			$.cookie('banner-read', false, {
				expires: 1,
				path: '/'
			});
			// console.log('creating cookie');
		}

		// Load Banner
		function loadBanner() {
			var bannerSource = $('#banner-message a').attr('href'),
				bannerText = $('#banner-message p').text(),
				bannerCTA = $('#banner-message h6').text();
				cookieBoo = $.cookie('banner-read');

			console.log(cookieBoo);

			if ( cookieBoo === 'false' ) {
				$('.header-banner').slideDown();
				$('.header-banner a').attr('href', bannerSource);
				$('.header-banner a:last-child').text(bannerText);
				$('.header-banner .banner-cta').text(bannerCTA);
			} else if (cookieBoo === 'true' ){
				$('.header-banner').hide();
			}
		}
		loadBanner();

		// Close Banner
		$('body').on('click', '.header-banner-closer', function() {
			$('.header-banner').addClass('closed');
			$.cookie('banner-read', true, {
				expires: 1,
				path: '/'
			});
		});



		// add tab triggers
		$('.et_pb_tabs_controls').append('\
			<div class="tab-trigger-row">\
				<div class="tab-trigger-1"></div>\
				<div class="tab-trigger-2"></div>\
				<div class="tab-trigger-3"></div>\
			</div>\
		');

		$('body').on('click', '#operationsAnchor .tab-trigger-row > div', function() {
			if ( $(this).hasClass('tab-trigger-1') ) {
				$('.et_pb_tab_0').trigger('click');
				$('.content-marketing-operations').removeClass('tab-1 tab-2 tab-3');
				$('.content-marketing-operations').addClass('tab-1');
			}
			if ( $(this).hasClass('tab-trigger-2') ) {
				$('.et_pb_tab_1').trigger('click');
				$('.content-marketing-operations').removeClass('tab-1 tab-2 tab-3');
				$('.content-marketing-operations').addClass('tab-2');
			}
			if ( $(this).hasClass('tab-trigger-3') ) {
				$('.et_pb_tab_2').trigger('click');
				$('.content-marketing-operations').removeClass('tab-1 tab-2 tab-3');
				$('.content-marketing-operations').addClass('tab-3');
			}
		});

		$('body').on('click', '#contentHubsAnchor .tab-trigger-row > div', function() {
			if ( $(this).hasClass('tab-trigger-1') ) {
				$('.et_pb_tab_3').trigger('click');
				$('.content-hubs').removeClass('tab-1 tab-2 tab-3');
				$('.content-hubs').addClass('tab-1');
			}
			if ( $(this).hasClass('tab-trigger-2') ) {
				$('.et_pb_tab_4').trigger('click');
				$('.content-hubs').removeClass('tab-1 tab-2 tab-3');
				$('.content-hubs').addClass('tab-2');
			}
			if ( $(this).hasClass('tab-trigger-3') ) {
				$('.et_pb_tab_5').trigger('click');
				$('.content-hubs').removeClass('tab-1 tab-2 tab-3');
				$('.content-hubs').addClass('tab-3');
			}
		});









	});







