            switch (eventObj.eventName) {
                case "login":
                    //gigya.accounts.setSSOToken();
                    cxenseOnLoginHandler(eventObj);
                    break;

                case "logout":
                    window.location.replace("/");
                    break;
                default:

            }
        }

onLoad();
