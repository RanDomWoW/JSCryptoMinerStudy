        $('#report_submit').on('click',function(){
            if($('#spec').val()==''){
                alert('反馈原因不能为空！');
            }else if($('#txtvalidateCode').val()==''){
                alert('验证码不能为空！');
            }else{
                
               $.post('http://'+window.location.host+'/feedback',{contentid:contentid,content:$('#jb_id').val(),remark:$('#other').val(),reason:$('#spec').val(),vertify_code:$('#txtvalidateCode').val(),ip:getIP,agent:agent},function(data){
                    
                    if(data.status == 138002){
                        alert("反馈类型不能为空！");
                    }else if(data.status == 138008){
                        alert("验证码错误！");
                        $('#imgCode').click();
                    }else if(data.status == 0){
                        $('#popjubao,#jubaobg').hide();
                        $('#imgCode').click();
                        $('#txtvalidateCode,#other,#spec').val('');
                        $('.radios').attr("checked",false);
                        alert('反馈成功！');
                    }else if(data.status == 103){
                        alert(data.message);
                    }else{
                        alert("反馈失败，请联系客服!");
                    }
                },'json');  
            }
           
        })
    })
