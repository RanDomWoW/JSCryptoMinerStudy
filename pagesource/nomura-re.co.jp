<script>
$(function(){
    $('#selectNav li').on('click', function() {
		var  headerHeight = $('header').outerHeight(),
		target = $('#selectNav'),
		position = target.offset().top-(headerHeight+40),
		tabNav = $('#selectNav li'),
		tabNavIndex = tabNav.index(this),
		tabBoxes = $('#selectBoxes > div');
		tabBoxes.hide();
		tabBoxes.eq(tabNavIndex).fadeToggle(800);
		tabNav.removeClass('is_current');
		$(this).addClass('is_current');
		$('html, body').animate({scrollTop:position}, 400);
	});
	
	//$('.innerItem').matchHeight();
	
});
</script>

