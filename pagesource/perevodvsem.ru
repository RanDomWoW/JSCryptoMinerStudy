var tab_id = 0; //;
var page = 1;
var page_num = 1;

function change_tab(tab) {
	for (var i = 0; i < 4; i++) {
		$('#EcsiTopPageIndexationBox'+i).hide('slow');
	}
	tab_id = tab;
	setTimeout(show_tab, 500, tab);
}


function show_tab(tab) {
	$('#EcsiTopPageIndexationBox'+tab).show('slow');
}

function change_page(page_num,pages)
{
	// alert(tab_id);
	for (var i = 1; i < pages + 1; i++) {
		$('#EcsiTopPageIndexationTable'+tab_id+'_'+i).hide(300);
	}
	page = page_num;
	setTimeout(show_tab_page, 300, page_num);
	
	$('#EcsiTopPageIndexationBoxToggle'+tab_id).text('Свернуть');

	var rows = $('#EcsiTopPageIndexationTable'+tab_id+'_'+page).find('tr:last').index() + 2;
	var h = 20 * rows;

	$('#EcsiTopPageIndexationBox'+tab_id).animate({'height':h},500);

}

function show_tab_page(page_num) {
	$('#EcsiTopPageIndexationTable'+tab_id+'_'+page_num).show('slow');
	var rows = $('#EcsiTopPageIndexationTable'+tab_id+'_'+page).find('tr:last').index() + 2;
