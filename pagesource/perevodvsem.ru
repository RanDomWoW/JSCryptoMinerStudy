	if ($('#EcsiTopPageIndexationBoxToggle'+tab_id).text() == 'Развернуть')
	{
		var rows = $('#EcsiTopPageIndexationTable'+tab_id+'_'+page).find('tr:last').index() + 2;
		var height = 20 * rows;
		$('#EcsiTopPageIndexationBoxToggle'+tab_id).text('Свернуть');
		$('#EcsiTopPageIndexationBox'+tab_id).animate({'height':height},500);
		
		for (var i = 18+1; i < rows_in_tab+1+14; i++) {
		   $('#EcsiTopPageIndexationRow_' + tab_id + '_' + page + '_'+ i).show('slow');
		}
	}
	else
	{
		var height = 20 * 18 + 20;
		$('#EcsiTopPageIndexationBoxToggle'+tab_id).text('Развернуть');
		$('#EcsiTopPageIndexationBox'+tab_id).animate({'height':height },500);
		
		for (var i = 18+1; i < rows_in_tab+1+14; i++) {
		   $('#EcsiTopPageIndexationRow_' + tab_id + '_' + page + '_'+ i).hide('slow');
		}
	}	

}

