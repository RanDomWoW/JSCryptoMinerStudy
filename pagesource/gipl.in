    <script type="text/javascript">

        $('a.checkchk').on('click', function () {
            var myModal = $('#myModal');

            var Value = $(this).closest('.text').find('.desc');
            var ImageDiv = $(this).closest('.text').find('.imgTestimonial');

            var ImageSrc = $(ImageDiv).attr('src');

            var Data = $(Value).text();
            $(myModal).find('.text').html(Data);
            $(myModal).find('.img-thumbnail').attr('src', ImageSrc);


            // and finally show the modal
            myModal.modal({ show: true });

            return false;
        });


        $('a.chkAwardDetails').on('click', function () {
            var myModalAward = $('#myModalAward');

            var awrdContent = $(this).closest('.news-content').find('.awardContent');
            var TitleDiv = $(this).closest('.news-content').find('h5');

            var DataAward = unescape($(awrdContent).html());
            var Title = $(TitleDiv).text();

            $(myModalAward).find('.text').html(DataAward);
            $(myModalAward).find('.modal-title').text(Title);

            myModalAward.modal({ show: true });

            return false;
        });

