    <script type="text/javascript" language="javascript">
        var _currentMainItem = null;
        var _currentMainItems = null;
        var _currentMainIndex = 0;
        window.disableRightAds = true;
        var _resetInterval = null;

        $(document).ready(function () {

            $('#mainPromoBox').mouseover(function (e) {
                stopMainTimer();
            });

            $('#AMbg').click(function (e) {
                $a = $('#divTabStrip a.current');
                window.open($a.attr('url'), '_blank');
            });

            $('#mainPromoBox').mouseleave(function (e) {
                startMainTimer();
            });

            $('#divTabStrip a.tab').mouseover(function (e) {
                resetMainItem($(this), e);
            });
            $('#divTabStrip a').each(function () {
                $a = $($('#divTabStrip a.tab')[_currentMainIndex]);
                $('#divCurrnetNewsTitle').attr('href', $a.attr('url'));
                $('#divCurrnetNewsTitle').css('color', $a.attr('text-color'));
                $('#divCurrnetNewsTitle').html( $a.attr('content-title'));
                $('#div_title2').css('color', $a.attr('text-color2'));
                $('#div_title2').html($a.attr('content-title2'));

            });

            startMainTimer();
        });

        function stopMainTimer() {
            if (_resetInterval) {
                clearInterval(_resetInterval);
            }
        }

        function startMainTimer() {
            _resetInterval = setInterval(
                resetMainItem, 5000);
        }

        function resetMainItem($a, e) {
            if (!$a) {
                _currentMainIndex++;
                if (_currentMainIndex >= $('#divTabStrip a.tab').length)
                    _currentMainIndex = 0;
                $a = $($('#divTabStrip a.tab')[_currentMainIndex]);
            }
            $a.addClass('current');
            $('.pic_holder').css('background', 'url("' + $a.attr('imageSource') + '")');
            $('.pic_holder').attr("title", $a.attr('content-title'));
            $('#divCurrnetNewsTitle').html($a.attr('content-title'));
            $('#divCurrnetNewsTitle').css("color", $a.attr('text-color'));
            $('#div_title2').css("color", $a.attr('text-color2'));
            $('#div_title2').html($a.attr('content-title2'));
            $('#divCurrnetNewsTitle').attr('href', $a.attr('url'));

            var actionText = ($a.attr('content-action-text') ? $a.attr('content-action-text') : 'קרא עוד');
            $('#divTabStrip a.tab').attr('class', 'tab');
            $a.addClass('current');
            if (e)
                e.preventDefault();


        }
    </script>
