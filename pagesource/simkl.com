	function mainBindPosterEvent(noClick){
		$('.rightPostersContainer').posterEvents({
			contEnter: '#SimklTVMainMostWatched',
            noClick: noClick
						,doStars: function (){
				$().SimklLP('showLoginPopup');
			}
			,chWL: function (){
				$().SimklLP('showLoginPopup');
			}
					});
	}

	function preDOM_MainTVPage(){
		$().pageLoader().markFunction('preDOM_MainTVPage'); 		/* for good page loading */

		$('div.SimklTVMainFrameArrows, .aj24Best').pageLoader({
			cont: 'global_div'
		});

		var cfObj = $('#currentFan');
		currentFan = parseInt(cfObj.val());
		/* fast clone */
		cBG = $('#bgContainer').clone();

		$('.SimklTVMainFrameArrowLeft, .SimklTVMainFrameArrowRight').click(function(event){
			event.stopPropagation();
			if (animationOn) return;
			hideFanAll();
			if ($(this).attr('class').indexOf('Left')!= -1) currentFan--; else currentFan++;
			if (typeof artData[currentFan] == 'undefined'){
				if (currentFan<0) currentFan = totalFan; else currentFan = 0;
			}
			cfObj.val(currentFan);

			showFanSlow();
		});

		$('.SimklTVMainFrameCircle').click(function(){
			if (animationOn) return;
			var id = parseInt($(this).attr('id').replace('FanCircle',''));
			if (id == currentFan) return; else {
				currentFan = id;
				cfObj.val(id);
			}
			hideFanAll();
			showFanSlow();
		});

		showFanSlow(true);

		mainBindPosterEvent(true);

		bindFilterDropDown(function(id){
			$('#filter_'+id.substr(0,id.indexOf('-'))).val(id.substr(id.indexOf('-')+1));
			mainAjaxReload('mostw');
		});

		$('span.MainSmallLinksToGenres').pageLoader({
			cont: 'tvMainTable',
			changedLoaded: 'mainblock',
			callback: function(){
				$('#headMenuTV, #headMenuAnime, #headMenuMovie').removeClass('Active');
				$('#headMenuGenres').addClass('Active');
			}
		});

		$('.SimklTVMainGenresTab').on('click',function(){
			$('.SimklTVMainGenresTab').toggleClass('Active');
			$('.SimklTVGenresPopupList').toggleClass('Hide');
		});

		$('.SimklTVPostersScrollWrap .frame').posterSly({
			user_id : 0
		});

		$('#bestofDay').on('click',function(){
			$('#bestofDayHref').click();
		});

        
		saveScrollPosition('mainScrollTop');

		lineAjaxButton();

        ga('set', 'nonInteraction', true);
        ga('send', 'event', {
            eventCategory: 'Visited website',
            eventAction  : 'Seen Tv Homepage'
        });
	}
	$(preDOM_MainTVPage);
</script>
