    function preDOM_HeaderLoad(){
        $().pageLoader().markFunction('preDOM_HeaderLoad');

        $('div.SimklHeaderMenuButton, div.SimklHeaderMenuMain, .ajFooterLinkInside').on("click.reload",function(event){ /* double reload */
            var urlParams = $().pageLoader().getUrlParams(document.URL).toLowerCase();

            if ($(this).attr('class').indexOf('Active')!=-1){
                if (urlParams == '//dashboard/'){
                    if ($(this).attr('class').indexOf('SimklHeaderMenuProfile')!=-1){ /* active is became faster then event is triggered */
                        $().pageLoader().reload('myProfile_table','mainblock',this);
                    }
                }else if (event.which != 2){
                    if ($('a',this).attr('href').indexOf('https') == -1) $().pageLoader().reload('global_div','',this);
                }
            }
        }).pageLoader({
            cont: 'global_div',
            activeClass: 'Active'
        });

        $(window).scroll(function(){
            if ($(this).scrollTop()>300){
                $('#SimklTVbackToTop').addClass("Active");
                $('.uv-icon').addClass("Active");
            }else{
                $('#SimklTVbackToTop').removeClass("Active");
                $('.uv-icon').removeClass("Active");
            }
        });

        
        $(document).ajaxStart(function() {
            stillCheking = true;
            if (!disableTopBar) $().pageLoader().onTopLoadBar();
        }).ajaxStop(function() {
            if (!disableTopBar) $().pageLoader().offTopLoadBar();
            disableTopBar = false;
            stillCheking = false;
        });

        /** preload for detail title, some bug */
        $().bigtext();

            }

