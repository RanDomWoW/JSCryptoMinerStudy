    function showFeaturePopup(id,showType,respType){
        $.ajax({
            url: '/ajax/header.php',
            dataType: 'html',
            type: 'POST',
            data: {
                'action': 'feeds',
                'id': id,
                'resp_type': respType,
                'show_type': showType
            },
            cache: false
        }).success(function(data) {
            $(data).prependTo('body')
        }).error(function(data){
            log(data.responseText);
        });
    }


    function saveScrollPosition(cookieName,callback,disableGoTo){
        /* go to position */
        if (disableGoTo == undefined){
            if (getCookie(cookieName)){
                $(window).scrollTop(parseInt(getCookie(cookieName)));
            }else{
                $(window).scrollTop(0);
            }
        }

        var posdiv = 40;
        var pos = Math.floor($(window).scrollTop()/posdiv);
        $(window).off('scroll.'+cookieName).bind('scroll.'+cookieName,function(){
            var scTop = Math.floor($(window).scrollTop()/posdiv);

            if (scTop!=pos){
                pos = scTop;
                setCookie(cookieName,(scTop*posdiv),600);
                if (callback!=undefined) callback(pos,posdiv);
            }
        });
        $().pageLoader().pushLeaveCallback(function(){
            $(window).unbind('scroll.'+cookieName);
        });
    }

    </script>
