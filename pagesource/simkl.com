<script src="//us.simkl.in/js/tv/jqury.perfect-scrollbar-0.4.1.with-mousewheel.min.js"></script>
<script type="text/javascript">
var searchInterval;
var searchlastIndexElem = -1;
var searchContIndex = 0;

function preDOM_HeaderSearch() {
	$().pageLoader().markFunction('preDOM_HeaderSearch');
    var $ki = $(".SimklTVSearchInput");
    $ki.focus();

    $ki.keypress(function () {
		$('.SimklTVSearchField').addClass("active");
	});

    if (window.chrome!=true){
		$('.SimklTVSearchBackground').css('background-color','rgba(12,15,17,0.9)');
	}

    $('#searchRButton').on('click',function(){
        switchSearchType($('#activeSearch'),$(this));
    });


	$('#SimklHeaderMenuSearch').on('mousedown',function(event) {
        $('body').addClass("SimklTVSearch");
		if (window.chrome){
			$('.SimklTVBodyBackgroundDiv').css({
					'-webkit-backface-visibility':'hidden',
					'-webkit-filter': 'blur(26px)'
				}
			);
		}

		$('.SimklTVSearchBackground').addClass("Active");
		$('.SimklTVSearchField').addClass("active");
		$(".SimklTVSearchInput").focus();

        
		searchInterval = setInterval(doGlobalSearch, 550);
        event.stopImmediatePropagation();
        event.preventDefault();
	})

	$('#SimklTVSearchTVScrollDiv, #SimklTVSearchAnimeScrollDiv, #SimklTVSearchMovScrollDiv').css('height',$(window).height()-240).perfectScrollbar({
		wheelSpeed: 35,
		wheelPropagation: false
	});

	$(window).resize(function(){
		$('#SimklTVSearchTVScrollDiv, #SimklTVSearchAnimeScrollDiv, #SimklTVSearchMovScrollDiv').css('height',$(window).height()-240);
	}).keydown(function(event){
        var $body = $('body');
		if (event.keyCode == 27 && $body.hasClass("SimklTVSearch")){
			event.preventDefault();
			$('.SimklTVSearchClose').click();
		}else  if (event.keyCode>36 && event.keyCode<41 && $body.hasClass("SimklTVSearch")){
            var $actSearch = $('#activeSearch');
			if (searchContIndex!=2 && $actSearch.val() == 'mov') searchContIndex = 2;

			var $oldElem = $('.SimklTVSearchTVScrollTable:eq('+searchContIndex+') .SimklTVSearchItem:eq('+searchlastIndexElem+')');
			var oldIndex = searchlastIndexElem;
			if (event.keyCode == 39 && searchContIndex == 0 && $actSearch.val() != 'mov') ++searchContIndex; else
			if (event.keyCode == 37 && searchContIndex == 1) --searchContIndex;

			var $newElem = $('.SimklTVSearchTVScrollTable:eq('+searchContIndex+') .SimklTVSearchItem:eq('+(event.keyCode == 40?++searchlastIndexElem:(event.keyCode == 38?--searchlastIndexElem:searchlastIndexElem))+')');

			$oldElem.removeClass('searchItemHovered').find('.SimklTVSearchImgTable img').removeClass('searchItemHoveredImg');
			if ($newElem.length && searchlastIndexElem>=-1){
				$newElem.toggleClass('searchItemHovered').find('.SimklTVSearchImgTable img').toggleClass('searchItemHoveredImg');
			}else{
				searchlastIndexElem = oldIndex;
			}
		}else if (event.keyCode == 13 && searchContIndex>-1 && $('body').hasClass("SimklTVSearch")){
			$('.SimklTVSearchTVScrollTable:eq('+searchContIndex+') .SimklTVSearchItem:eq('+searchlastIndexElem+')').click();
		}
	});


	$('.SimklTVSearchClose').click(function (event) {
		clearTimeout(searchInterval);
		$('.SimklTVBodyBackgroundDiv').css({
				'-webkit-backface-visibility':'visible',
				'-webkit-filter': ''
			}
		);
		$('.SimklTVSearchResultsTable').hide();
		$('#SimklTVSearchContainer, #SimklAnimeSearchContainer, #SimklMovSearchContainer').html('');
		$('.SimklTVSearchInput').val('');
		lastSearch = '';

		$('body').removeClass("SimklTVSearch");
		$('.SimklTVSearchBackground').removeClass("Active");
		$('.SimklTVSearchField').removeClass("active");
		event.preventDefault();
	});
}

function switchSearchType($as,$el){
	if ($as.val() == 'tv'){
		$('.SimklTVSearchField input').attr('placeholder','Type to find Movies, TMDB or IMDB ID\\URL');
		$el[0].innerHTML = 'TV & Anime &#187;';
		$el.css('margin-left',-140);
		$as.val('mov');
	}else{
		$('.SimklTVSearchField input').attr('placeholder','Type to find TV, Anime, TVDB or IMDB ID\\URL');
		$el[0].innerHTML = 'Movies &#187;';
		$el.css('margin-left','-80px');
		$as.val('tv');
	}
	$('.SimklTVSearchResultsTable').hide();
	lastSearch = '';
	searchlastIndexElem = -1;
	searchContIndex = 0;
	setCookie('searchActive',$as.val());
	$('.SimklTVSearchInput').focus();
}

function searchAddToMy(id,el,event){
	event.stopPropagation();
		$('.SimklTVSearchClose').click();
	$().SimklLP('showLoginPopup');
	}

function replTPL(tpl, obj, id) {
	tpl = tpl.replace('(aid)',id);
	tpl = tpl.replace('(id)',''+obj.url);
	tpl = tpl.replace('(year)',obj.year);
	tpl = tpl.replace('(rank)',obj.rank);
	tpl = tpl.replace('(eps)',obj.eps);
	tpl = typeof obj.inlist == 'undefined' ? tpl.replace('(inlist)','<a href="#" onclick="searchAddToMy(\''+id+'\',this,event); return false;">Add to watchlist</a>') : tpl.replace('(inlist)','');
	if (typeof obj.poster != 'undefined' && obj.poster) tpl = tpl.replace('(img)','<div style="width:48px; height: 60px;"><img class="lazy" data-original="//simkl.net/posters/'+obj.poster+'"/></div>'); else tpl = tpl.replace('(img)','');
	if (typeof obj.type != 'undefined') tpl = tpl.replace('(type)',obj.type);

    tpl = (typeof obj.titles.a7 != 'undefined' ? tpl.replace('(alt)',obj.titles.a7) :
        (typeof obj.titles.a_ != 'undefined' ? tpl.replace('(alt)',obj.titles.a_) : tpl.replace('(alt)','')));


	tpl = (typeof obj.titles.m  != 'undefined' ? tpl.replace('(title)',obj.titles.m) : '');


	return tpl;
}
function replCount(type,count){
	var e;
	if (type == 'tv') {
		e = $('.SimklTVSearchTVHeader .SimklTVSearchTypeTitle');
	}else if(type == 'mov'){
		e = $('.SimklTVSearchMovHeader .SimklTVSearchTypeTitle');
	}else{
		e = $('.SimklTVSearchAnimeHeader .SimklTVSearchTypeTitle');
	}
	e.html(e.html().replace(/\([0-9]+\)/i,'('+count+')'));
}

var lastSearchKey = '';
var keyPressInterval = 0;
var globalKeyTimeout = 0;
function storeLastKey(event){
	lastSearchKey = event.keyCode;
	keyPressInterval = Date.now() ;
	if (globalKeyTimeout) clearTimeout(globalKeyTimeout);
	globalKeyTimeout = setTimeout(doGlobalSearch,500);
}

var lastSearch = '';
function doGlobalSearch(data) {
	var intr = Date.now();
	if (stillCheking || (intr-keyPressInterval)<450) return;
	keyPressInterval = intr;

	var s = 'lastTop';
	if (data == undefined) s = $('.SimklTVSearchInput').val();

	if (s.length > 0 && lastSearch != s) {

		lastSearch = s;
		var actSearch = $('#activeSearch').val();

		$.ajax({
			url: '/ajax/search.php',
			dataType: 'json',
			type: 'POST',
			data: {
				's' : s,
				'as': actSearch
			},
			cache: false
		}).success(function (data) {
				if (data.err != undefined) {
					alert(data.err);
				} else {
					var totalCount = 0;
					var lastID = 0;

					if (actSearch == 'tv'){
						$('.SimklTVSearchResultsTable:eq(0)').show();
						$('#SimklTVSearchContainer, #SimklAnimeSearchContainer').html('');

						var aCount = 0, tCount = 0;
						if (data.counttv != undefined) tCount = data.counttv;
						if (data.countanime != undefined) aCount = data.countanime;

						replCount('tv',tCount);
						replCount('anime',aCount);

						/* TV */
						var cont = $('#SimklTVSearchContainer');
						if (tCount){
							var tpl = $('#SimklSearch_TPLTV')[0].innerHTML;
							for (var tv in data.tv) {
								var r = replTPL(tpl, data.tv[tv], tv);
								cont[0].innerHTML = cont[0].innerHTML+r;
								totalCount++;
								lastID = tv;

							}
						}else{
							cont[0].innerHTML = $('#SimklSearch_TPLNO')[0].innerHTML;
						}

						/* ANIME */
						cont = $('#SimklAnimeSearchContainer');
						if (aCount){
							var tpl = $('#SimklSearch_TPLAnime')[0].innerHTML;
							for (anime in data.anime) {
								var r = replTPL(tpl, data.anime[anime], anime);
								cont[0].innerHTML = cont[0].innerHTML+r;
								totalCount++;
								lastID = anime;

							}
						}else{
							cont[0].innerHTML = $('#SimklSearch_TPLNO')[0].innerHTML;
						}

						if (totalCount){ /* ajax */
							$('#SimklTVSearchContainer div, #SimklAnimeSearchContainer div').pageLoader({
								cont: 'global_div',
                                changedLoaded: 'search',
								permCallback: function(){
									$('.SimklTVSearchClose').click();
								}
							});


							$("#SimklTVSearchTVScrollDiv img.lazy").lazyload({
								container: $("#SimklTVSearchTVScrollDiv")
							});
							$("#SimklTVSearchAnimeScrollDiv img.lazy").lazyload({
								container: $("#SimklTVSearchAnimeScrollDiv")
							});

							/* open result immidiatly if result = 1 */
							if (totalCount == 1 && lastID && s.length>1){
								$('#goSearch'+lastID).click();
							}
						}
						$('#SimklTVSearchTVScrollDiv, #SimklTVSearchAnimeScrollDiv').scrollTop(0).perfectScrollbar('update');
					}else{
						$('.SimklTVSearchResultsTable:eq(1)').show();
						var $sCont = $('#SimklMovSearchContainer');
						$sCont.html('');

						var mCount = 0;
						if (data.countmov != undefined) mCount = data.countmov;

						replCount('mov',mCount);
						/* Mov */
						if (mCount){
							var tpl = $('#SimklSearch_TPLMov')[0].innerHTML;
							for ( var mov in data.mov) {
								var r = replTPL(tpl, data.mov[mov], mov);
								$sCont[0].innerHTML = $sCont[0].innerHTML+r;
								totalCount++;
								lastID = mov;

							}
						}else{
							$sCont[0].innerHTML = $('#SimklSearch_TPLNO')[0].innerHTML;
						}

						if (totalCount){ /* ajax */
							$('#SimklMovSearchContainer div').pageLoader({
								cont: 'global_div',
                                changedLoaded: 'search',
								permCallback: function(){
									$('.SimklTVSearchClose').click();
								}
							});

							$("#SimklTVSearchMovScrollDiv img.lazy").lazyload({
								container: $("#SimklTVSearchMovScrollDiv")
							});

							/* open result immidiatly if result = 1 */
							if (totalCount == 1 && lastID && s.length>1){
								$('#goSearch'+lastID).click();
							}
						}

						$('#SimklTVSearchMovScrollDiv').scrollTop(0).perfectScrollbar('update');
					}
				}

			}).error(function (data) {
				log(data.responseText);
			});
	}else if (s.length<1 && lastSearchKey == 8){
		$('.SimklTVSearchResultsTable').hide();
		$('#SimklTVSearchContainer, #SimklAnimeSearchContainer, #SimklMovSearchContainer').html('');
		lastSearch = '';
		replCount('tv','0');
		replCount('anime','0');
		replCount('mov','0');
	}
}

$(preDOM_HeaderSearch);

</script>
