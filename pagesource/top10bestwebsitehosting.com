        u.monitorAllComponentsLoaded();
        u.reportSSRComponents();
        u.splitAndLoadComponents();
        if (!u.pocMode()){
            u.buildCSSLibraries();
        }

                                // Webfont config variable, do not change the name!
        WebFontConfig = {
            google: { families: ["Open+Sans","Roboto+Slab:400,700","Roboto:400,300,700","Montserrat:400,300,700",":400,300,700",":400,300,700",":400,300,700"] }
        };

        // Load google fonts after components are loaded, otherwise it will block them and affect page score
        var webfontLoaded = false;

        var loadWebfont = function() {
            (function(d) {
                var wf = d.createElement('script'), s = d.scripts[0];
                wf.src = '//ajax.googleapis.com/ajax/libs/webfont/1.5.18/webfont.js';
                s.parentNode.insertBefore(wf, s);
            })(document);
            webfontLoaded = true;
        };
        var secondTemptToLoadFonts = function(){
            if(!webfontLoaded){
                loadWebfont(document);
            }
        };

        $(window).on('pageRendered', function (e) {
            loadWebfont();
        });
        
    }, u.poller('jquery'));
</script>

