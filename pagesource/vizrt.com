<script type="text/javascript">
$(document).ready(function(e) {
var active = false;
// stop normal submitting
$('#id37566').submit(function(e) {
e.preventDefault();
});
function stringToBoolean(string) {
switch(string.toLowerCase()){
case "true": case "yes": case "1": return true;
case "false": case "no": case "0": case null: return false;
default: return Boolean(string);
}
}
$('#button37566').on("click", function(e) {
//set some variables
if(!active) {
active = true;
var alertWindowError = $('#id37566 div.outputError');
var button = $(this);
var form = $('#id37566');
var closeButton = $('div.modal-footer a.btn', form);
var loadingGif = $('img.processing', form);
var dataString = form.serialize();
$.ajax({
type: "POST",
url: "http://www.vizrt.com/ContactMe",
data: dataString,
dataType: "json",
beforeSend: function() {
button.hide();
closeButton.hide();
loadingGif.show();
},
success: function(data) {
if(stringToBoolean(data.success)) {
closeButton.show().html('<i class="icon-ok"></i> Close');
$('div.modal-body',form).hide().after('<div class="alert alert-success clearfix" style="margin:0 15px 15px 15px; padding:10px;">' + data.message + '</div>')
} else {
button.show();
closeButton.show();
alertWindowError.html('<div class="alert alert-alert" style="margin: 5px 0;padding: 10px;">' + data.message + '</div>');
}
},
error: function(data) {
button.show();
closeButton.show();
alertWindowError.html("<div class='alert alert-alert' style='margin: 5px 0;padding: 10px;'> Oops! Something went wrong. Please try again, or <a href='//www.vizrt.com/contact/34401/Vizrt_Global_Headquarters'>email us</a> directly.</div>");
},
complete: function(func, settings) {
active = false;
loadingGif.hide();
}
});
}
});
