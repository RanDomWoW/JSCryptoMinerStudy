<script type="text/javascript">
$(document).ready(function(e) {
var active = false;
// stop normal submitting
$('#id21800').submit(function(e) {
e.preventDefault();
});
function stringToBoolean(string) {
switch(string.toLowerCase()){
case "true": case "yes": case "1": return true;
case "false": case "no": case "0": case null: return false;
default: return Boolean(string);
}
}
function process() {
//set some variables
if(!active) {
active = true;
var alertWindowError = $('#id21800 div.outputError');
var button = $('#regbutton');
var form = $('#id21800');
var loadingGif = $('img.processing', form);
var dataString = form.serialize();
$.ajax({
type: "POST",
url: "http://www.vizrt.com/ContactMe",
data: dataString,
dataType: "json",
beforeSend: function() {
button.hide();
loadingGif.show();
},
success: function(data) {
if(stringToBoolean(data.success)) {
form.hide().after('<div class="alert alert-success">' + data.message + '</div>')
} else {
button.show();
alertWindowError.html('<div class="alert alert-alert" style="margin: 5px 0;">' + data.message + '</div>');
}
},
error: function(data) {
button.show();
alertWindowError.html("<div class='alert alert-alert' style='margin: 5px 0;padding: 10px;'> Oops! Something went wrong. Please try again, or <a href='//www.vizrt.com/contact/34401/Vizrt_Global_Headquarters'>email us</a> directly. </div>");
},
complete: function(func, settings) {
active = false;
loadingGif.hide();
}
});
}
}
$('#button21800').on("click", function(e) {
process();
});
function valForm(form) {
var valid = true;
jQuery("input, textarea", form.children()).each(function () {
if (jQuery(this).attr("value") == ""){
valid = false;
}
});
return valid;
}
function submitForm() {
if (valForm($('#id21800'))) {
if ($('#subscr_chkbx').is(':checked')) {
var name = $("#id21800 input[name='fullname']").val();
var email = $("#id21800 input[type='email']").val();
$("input[name='cm-name']").val(name);
$("input[name='cm-ouyih-ouyih']").val(email);
$(function () {
$('#subForm').submit(function (e) {
e.preventDefault();
$.getJSON(
this.action + "?callback=?",
$(this).serialize(),
function (data) {
if (data.Status === 400) {
alert("Error: " + data.Message);
} else {
process();
}
});
});
});
$('#subForm').submit();
}
else {
process();
}
}
}
$('#regbutton').on("click", submitForm);
});
</script>
