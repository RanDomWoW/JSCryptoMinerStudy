jQuery(this).css({"color":"#000","opacity":"1"})
})
jQuery(".contactme dd select").focus(function(){
jQuery(this).css({"color":"#000","opacity":"1"})
if(this.value==""){
jQuery("option.autocountry",this).attr("selected","selected")
}
})
jQuery(".contactme dd select").blur(function(){
if(this.value==""){
jQuery(this).css({"color":"#000","opacity":".5"})
}
})
jQuery(".contactme select[name='cc']").change(function(){
var chosen=jQuery("select[name='cc'] option[value='"+this.value+"']",this.form).html()
jQuery("input[name='chosencountry']",this.form).attr("value",chosen)
})
})();
});
</script>
