<script>
$(function() {
var iframe = $('#video')[0],
player = $f(iframe);
var playerElement = $('#video');
var button = $("#vizvideo");
var width = $(window).width();
var container = $("#headerWrapper");
var playing = false;
player.addEvent('ready', function(player_id) {
player.addEvent('finish', onFinish);
});
function play() {
if(!playing) {
playing = true;
playerElement.fadeIn();
if(width > 979) {
$("div.middlebar").css("top", "+=180px");
container.css("height", "+=180px");
}
player.api("play");
}
}
function onFinish() {
playing = false;
playerElement.fadeOut();
if(width > 979) {
$("div.middlebar").css("top", "-=180px");
container.css("height", "-=180px");
}
}
$('#vizvideo').on('click', function(e) {
e.preventDefault();
play();
});
});
