<script>
	$(document).ready(function(){
	
		$('#menu-trigger').removeClass('open');
		$('.top-menu > li > a.link').removeClass('active');
	
		$('#menu-trigger').click(function(){
			$('#menu-trigger').toggleClass('open');
			$('#global-mobile-menu').slideToggle();
			if($('#global-mobile-login-nav').is(':visible')){
				$('#global-mobile-login-nav').slideUp();
				$('#global-mobile-login-button').removeClass('open');
			}; 
		});
		$('.top-menu > li > a.link.showSub').click(function() {
			$('.top-menu > li > a.link.showSub').not(this).removeClass('active');
			$(this).toggleClass('active');
			$(this).parent().siblings().find('ul.sub-menu').slideUp();
			$(this).next('ul.sub-menu').slideToggle();
			return false;
		});
		$('#global-mobile-login-button').click(function(){
			$(this).toggleClass('open');
			$('#global-mobile-login-nav').slideToggle('fast');	
			if($('#global-mobile-menu').is(':visible')){
				$('#global-mobile-menu').slideUp();
				$('#menu-trigger').removeClass('open');
			}; 
		});
	});
	
	$(document).ready(function() {
		var stickyNavTop = $('.utility-nav').offset().top;

		var stickyNav = function(){
		var scrollTop = $(window).scrollTop();

		if (scrollTop > stickyNavTop) { 
			$('.utility-nav').addClass('stuck');
		} else {
			$('.utility-nav').removeClass('stuck'); 
		}
		};

		stickyNav();

		$(window).scroll(function() {
		  stickyNav();
		});
		
		
		var loginDown = false;
		
		// Additional login support
		jQuery("#login").on('touchstart click', function(){

			if(loginDown != true){
				$("#global-login-nav").toggleClass("active-login");
				loginDown = true;
			}else{
				$("#global-login-nav").removeClass("active-login");
				loginDown = false;
			}
			return false;
		});	
});
	
	
</script>
