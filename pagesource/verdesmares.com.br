   jQuery( document ).ready(function() {
      jogosAoVivo();
     
       var wgt = jQuery('.barra-jogada');
       if( wgt.length>0 ){
         urlPlacar = jQuery('.barra-jogada .barra-jogada-hora').attr('href');
       }
     
       jQuery('#exibirConfrontos').click(function(){
            window.location.href = urlPlacar;
       });
       
       jQuery('#exibirConfrontos').hover(function(){
           $(this).css('cursor','pointer');
       });
     
    });
  
	function jogosAoVivo(){
        
        JogadaFacadeTempoReal.jogosAoVivo(today, {  
            callback:function(retorno){
                jQuery('#partidas').html(retorno);
                var jogos = eval(retorno);
                console.log(retorno);
                if(jogos != null && jogos.length > 0){						
                    jQuery('#exibirConfrontos').show();
                    jQuery('#confrontos').html('');
                    
                    var slideElements = "";
                    jQuery.each(jogos, function(i,o){
                        slideElements += montaJogosAovivo(o,i);
                    });
                    
                    jQuery('#confrontos').append(slideElements);
                    
                    /*define(['modules/plugins/slider-fade', 'jquery'], function(Index) {
                        if(!inited){
                            inited = true;
                            return $(function() {
                                return new Index.SliderFade({
                                    container: '.wgt-cmd-confronto',
                                    elements: '.placar'
                                });
                            });
                        }
                            
                    });*/	

                }else{
                    
                    jQuery('#exibirConfrontos').hide();
                }
                 
      
           }
        });
    }
  
    function atualizaJogosAoVivo(){
        var partidas = jQuery('#partidas').html();
        if(partidas != ""){
            JogadaFacadeTempoReal.atualizaJogos(partidas, {  
				  callback:function(retorno){
                    
						var jogosAtualizados = eval(retorno);
                    

						if(jogosAtualizados != null && jogosAtualizados.length > 0){						
								$('#exibirConfrontos').show();
								$('#confrontos').html('');
							
								var slideElementsAtualizados = "";
								jQuery.each(jogosAtualizados, function(i,o){
									slideElementsAtualizados += montaJogosAovivo(o,i);
								});
							
								jQuery('#confrontos').append(slideElementsAtualizados);
						 }
				}
			});	
		}
    }		
    
    window.alert = function() {};
    var html = ""; 
    function montaJogosAovivo(jogo,cont){
          var d=new Date(jogo.evento_hora);
          var h = d.getHours();
          var m = d.getMinutes().toString();
          var tam = m.length;
          var horaEventos = "";
          
          if(tam == 1){
            
           horaEventos = h.toString() + ":" + m.toString() + "0";
          }else{
           horaEventos = h.toString() + ":" + m.toString();
          }
        
          var status = 'Acompanhe ao vivo';	
          if (jogo.evento_status == 'acompanhe') {
            status = 'Acompanhe a partida' 
          }
          
          html = "<div class=\'barra-jogada\'>"+
                      "<a href=\'#\' class=\'barra-jogada-ant\'><</a>"+
                      "<a href=\'#\' class=\'barra-jogada-prox\'>></a>"+
                      "<a href=\'"+urlPlacar+jogo.evento_link+"\' class=\'barra-jogada-hora\'>às "+horaEventos+"</a>"+
            //"<a href=\'"+urlPlacar+jogo.evento_link+"\' class=\'barra-jogada-acompanhe\'> "+status+"</a>"+
                      
                      "<div class=\'barra-jogada-placar'\>"+
                          "<h2>"+jogo.clube_casa+"</h2>"+ 
                              "<img width=\'64px\' height=\'64px\' src=\'"+ jogo.clube_casa_escudo +"\' alt=\'"+ jogo.clube_casa +"\'>"+
                              "<h1>"+jogo.clube_casa_gols+"</h1>"+ "x" +"<h1>"+jogo.clube_visitante_gols+"</h1>" +
                          "<img width=\'64px\' height=\'64px\' src=\'"+ jogo.clube_visitante_escudo +"\' alt=\'"+ jogo.clube_visitante +"\'>"+
                          "<h2>"+jogo.clube_visitante+"</h2>"+
                      "</div>"+
                  "</div>";
        
    
         return html; 
    }
    
  interval = setInterval('atualizaJogosAoVivo()',60000);
</script>

      
    
    
