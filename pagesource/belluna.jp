<script type="text/javascript">
(function() {
	if (befastCDNCacheModule.isCDNCacheableContent()) {
		// CDNキャッシュされるコンテンツ
		console.log("This content is CDN cacheable.");
		//処理なし
	} else {
		// CDNキャッシュされないコンテンツ
		console.log("This content is not CDN cacheable.");
		// サーバ側から取得したカート点数と価格をクッキーに設定する。
		befastCDNCacheModule.setCookie(befastCDNCacheModule.COOKIE_CART_TOTALQUANTITY, "0点");
		befastCDNCacheModule.setCookie(befastCDNCacheModule.COOKIE_CART_ORDERPRICE, "￥0");
	}

	var toCommaNumString = function(n) {
		return String(n).replace( /(\d)(?=(\d\d\d)+(?!\d))/g, '$1,');
	};
	var writeContentIfExist = function(elementId, content) {
		var elem = document.getElementById(elementId);
		if (elem) {
			elem.innerHTML = content;
		}
	};

	if (befastCDNCacheModule.existsCookie("header_cart")) {
		// header_cartがある場合（ログイン前の場合）
		var headerCartValue = befastCDNCacheModule.getCookieValue("header_cart").split("-");
		console.log("* cookie header_cart = " + headerCartValue);
		if (headerCartValue.length >= 2) {
			// クッキー header_cart の値でカート投入件数と合計額を設定する。
			var dispTotalQuantity = headerCartValue[1] + "点";
			var dispOrderPrice = "￥" + toCommaNumString(headerCartValue[0]);
			writeContentIfExist("total_quantity_dlpoB", dispTotalQuantity);
			writeContentIfExist("order_price", dispOrderPrice);
			// ヘッダに設定された値をクッキーにも書き出す。
			befastCDNCacheModule.setCookie(befastCDNCacheModule.COOKIE_CART_TOTALQUANTITY, dispTotalQuantity);
			befastCDNCacheModule.setCookie(befastCDNCacheModule.COOKIE_CART_ORDERPRICE, dispOrderPrice);
		}
	} else if (befastCDNCacheModule.existsCookie(befastCDNCacheModule.COOKIE_CART_TOTALQUANTITY) 
	           && befastCDNCacheModule.existsCookie(befastCDNCacheModule.COOKIE_CART_ORDERPRICE)) {
		// header_cartがなく、befast.cartクッキーがある場合
		//   ログイン後の場合
		//   または、ログイン前でカート投入を１度もしていないかつ、非キャッシュコンテンツを1回以上表示した場合
		// クッキーに設定されている値でヘッダのカート投入件数と合計額を設定する。
		writeContentIfExist("total_quantity_dlpoB", befastCDNCacheModule.getCookieValue(befastCDNCacheModule.COOKIE_CART_TOTALQUANTITY));
		writeContentIfExist("order_price", befastCDNCacheModule.getCookieValue(befastCDNCacheModule.COOKIE_CART_ORDERPRICE));
	} else {
		// header_cartがない かつ、クッキーもない場合（ブラウザ起動直後にキャッシュ対象コンテンツを表示した場合）
		writeContentIfExist("total_quantity_dlpoB", "0点");
		writeContentIfExist("order_price", "￥0");
	}
})();
// 置換する文字列
var targetA = document.getElementById("total_quantity_dlpoB").innerHTML;

// 置換を実行し、textAfterに代入する
var textAfter = targetA.replace( /点/g , "" ) ;

document.getElementById('total_quantity_dlpoB').innerHTML = textAfter;
</script>

