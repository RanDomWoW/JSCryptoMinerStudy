    <script>
        // Click Tracking for the Slider Links
        function insertDataToDataLayer(actionType) {
            dataLayer.push({
                'event': 'onClickEvent',
                'eventCategory': 'home page slider',
                'eventAction': 'call out click',
                'eventLabel': actionType
            });
        };

        $(document).ready(function () {
            // Initiate Slider
            $('.bxslider').bxSlider({
                auto: true,
                autoDelay: 650,
                speed: 1500,
                mode: 'horizontal',
                captions: false,
                controls: false,
                adaptiveHeight: true,
                pager: true,
                touchEnabled: ($('.bxslider').children().length > 1) ? true : false,
                onSliderLoad: function (currentIndex) {
                    $("#homepage-slider").css("visibility", "visible");
                    $("#homepage-slider").css("opacity", "1");

                    // GTM Tracking for the initial slide impression
                    var gtmLabelInitialSlide = $(".active-slide").attr('name');
                    (function (activeSlide) {
                        dataLayer.push({
                            'event': 'onClickEvent',
                            'eventCategory': 'home page slider',
                            'eventAction': 'initial slide impression',
                            'eventLabel': activeSlide
                        });
                    })(gtmLabelInitialSlide)

                    //console.log('initial ' + gtmLabelInitialSlide);
                },
                onSlideBefore: function ($slideElement) {
                    $('#homepage-slider').find('.bx-viewport').find('.bxslider').children().removeClass('active-slide');
                    $slideElement.addClass('active-slide');
                }
            });

            $('.bx-pager-link').on('click', function () {

                // GTM Tracking - Tracks which circle is clicked
                var gtmLabelCircleClick = parseInt($(this).attr('data-slide-index')) + 1;
                dataLayer.push({
                    'event': 'onClickEvent',
                    'eventCategory': 'home page slider',
                    'eventAction': 'circle click',
                    'eventLabel': gtmLabelCircleClick
                });

                //console.log('circle click ' + gtmLabelCircleClick);
            });
        });
    </script>

