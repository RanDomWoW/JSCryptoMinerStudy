         slide = function () {
             var Slide_3 = new ScrollPic();
             Slide_3.id = 'J_Tab5_Nav'; //容器ID为必选参数
             Slide_3.prevId = 'J_Prev_5'; //按钮ID (上一个)
             Slide_3.nextId = 'J_Next_5'; //按钮ID (下一个)
             //以下皆是可选参数
             Slide_3.moveNum = 4; //每次滚动几个单元格 [默认1个]
             Slide_3.isLoop = false; //无缝循环滚动 [默认是]
             Slide_3.playTime = 2000; //自动播放时间间隔 [默认5秒]
             Slide_3.init();
             window.onload = function () {
                 var ls = jQuery("#J_Tab5_Nav li");
                 var len = ls.length;
                 var i = 0
                 for (i = 0; i < len; i++) {
                     if (ls[i].className == "fwb blue") {
                         var j = 0;
                         i = i + 1;
                         if (i > 5) {
                             j = i % 5 > 0 ? parseInt(i / 5) + 1 : parseInt(i / 5);
                             for (var k = 1; k < j; k++) {
                                 Slide_3.next();
                             }
                         }
                         break;
                     }
                 }
             }
         }
         slide();

