.sorting_link{
  padding: 6px 65px 6px 10px;
  border: 1px solid #dcdcdc;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
  background-color: #FCFCFC;
  display: inline-block;
  position: relative;
  text-decoration: none;
  color: #333;
  cursor: pointer;
  position: relative;
  font-size: 14px;
}



.sorting_arrow{
	float: right;
	border-left: 1px solid #dcdcdc;
	width: 28px;
	position: absolute;
	top: 0px;
	right: 0px;
	background-color: #dcdcdc;
	height: 100%;
}

.sorting_arrow:after{
  content: "";
  width: 7px;
  height: 5px;
  display: block;
  position: absolute;
  top: 50%;
  margin-top: -3px;
  left: 10px;
  background: url(/images/sorting_arrow.png) no-repeat center;
  -webkit-transition: all 0.2s ease;
  -moz-transition: all 0.2s ease;
  -ms-transition: all 0.2s ease;
  -o-transition: all 0.2s ease;
  transition: all 0.2s ease;
}

.sorting_inner{
  display: inline-block;
  position: relative;
  margin-top: 4px;
}

.sorting_inner:hover .sorting_arrow{
    background-color: #ff0042;
}

.sorting_inner:hover .sorting_link {
	border-color:#ff0042;
}


.sorting_list{
    display: none;
    list-style: none;
    position: absolute;
    overflow-y: scroll!important;
    max-height: 240px;
    width: 100%;
    z-index: 1000;
    background-color: white;
    box-shadow: 0px 3px 19px rgba(0, 0, 0, 0.30);
    border-bottom: 4px solid #ff0042;

}

.sorting_list li a{    
    font-size: 14px;
    color: #333;
       padding: 8px 10px 11px;
    border-top: 1px solid #E2DFD6;
    display: block;
    text-align: center;
}


.sorting_link_active .sorting_arrow:after{
  -moz-transform: rotate(180deg);
  -webkit-transform: rotate(180deg);
  -o-transform: rotate(180deg);
  -ms-transform: rotate(180deg);
  transform: rotate(180deg);
}


