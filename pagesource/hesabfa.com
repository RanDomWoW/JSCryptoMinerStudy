    <script type="text/javascript">
        $(function () {
            $('.img-thumbnail').bind('click', function () {
                var modal = document.createElement('div');
                document.body.appendChild(modal);

                var img = document.createElement('img');

                img.src = this.src;
                img.onload = function () {
                    var w = $(modal).width();
                    var h = $(modal).height() - 65;

                    var r1 = w / h;
                    var r2 = img.width / img.height;

                    var nw, nh;
                    if (r1 < r2) {
                        nw = w * .9;
                        nh = nw / r2;
                    } else {
                        nh = h * .9;
                        nw = nh * r2;
                    }


                    img.style.width = nw + 'px';
                    img.style.height = nh + 'px';
                    img.style.top = 65 + (h - nh) / 2 + 'px';

                };

                modal.appendChild(img);

                modal.className = 'site-image-back-modal';

                $(modal).bind('click', function () {
                    document.body.removeChild(modal);
                });
            });
        });


        $(document).ready(function () {
            var $goToTopBtn = $('#goToTopBtn');
            $goToTopBtn.hide();
            $('#clip-desc').on("inview", function (event, isInView) {
                if (isInView)
                    $("#clip").addClass('animated fadeInUp');
            });
            $('.featureThumbRow1, .featureThumbRow2').on("inview", function (event, isInView) {
                if (isInView) {
                    var i = 1, delay = [];
                    $(this).find('.featureThumb').each(function (i) {
                        i++;
                        var elem = $(this);
                        delay[i] = setTimeout(function () {
                            elem.addClass('animated fadeInRight');
                        }, 200 * i);
                    });
                }
            });
            $('#feature-container').on("inview", function (event, isInView) {
                if (isInView) {
                    var i = 1, delay = [];
                    $(".mobile-features .nav ul li").each(function (i) {
                        i++;
                        var elem = $(this);
                        delay[i] = setTimeout(function () {
                            elem.addClass('view');
                        }, 200 * i);
                    });
                }
            });
            $('#featuresList-title').on("inview", function (event, isInView) {
                if (isInView)
                    $(this).addClass('animated fadeInUp');
                else
                    $(this).removeClass('animated fadeInUp');
            });
            var counterViewed = false;
            $('#counters .row').on("inview", function (event, isInView) {
                if (isInView && !counterViewed) {
                    var $c1 = $('#counters .c1');
                    $('#counters .title').css("display","block");
                    $c1.prop('Counter', 0).animate({
                        Counter: $c1.data("count")
                    }, {
                        duration: 2000,
                        easing: 'swing',
                        step: function(now) {
                            $(this).text(Hesabfa.farsiDigit(Math.ceil(now)));
                        }
                    });

                    var $c2 = $('#counters .c2');
                    $c2.addClass("active");
                    $c2.prop('Counter', 0).animate({
                        Counter: $c2.data("count")
                    }, {
                        duration: 2500,
                        easing: 'swing',
                        step: function(now) {
                            $(this).text(Hesabfa.farsiDigit(Math.ceil(now)));
                        }
                    });

                    var $c3 = $('#counters .c3');
                    $c3.addClass("active");
                    $c3.prop('Counter', 0).animate({
                        Counter: $c3.data("count")
                    }, {
                        duration: 3300,
                        easing: 'swing',
                        step: function(now) {
                            $(this).text(Hesabfa.farsiDigit(Math.ceil(now)));
                        }
                    });
                    counterViewed = true;
                }
            });
            $('#mobile-icons-download, #mobile-icons-download1').on("inview", function (event, isInView) {
                if (isInView)
                    $("#mobile1").addClass('animated fadeIn');
                else
                    $("#mobile1").removeClass('animated fadeIn');
            });
            $('#freePeriod').on("inview", function (event, isInView) {
                if (isInView)
                    $(this).addClass('animated tada');
                else
                    $(this).removeClass('animated tada');
            });
            $('#comment-flag').on("inview", function (event, isInView) {
                if (isInView)
                    $(".customer-item").addClass('view');
                else
                    $(".customer-item").removeClass('view');
            });
            $(document).scroll(function () {
                var y = $(this).scrollTop();
                if (y > 500) {
                    $goToTopBtn.fadeIn();
                } else {
                    $goToTopBtn.fadeOut();
                }
                if (y > 200) {
                    $('body').addClass("sticky");
                    setTimeout(function () {
                        $('body').addClass("sticky-open");
                    }, 100);
                } else {
                    $('body').removeClass("sticky");
                    $('body').removeClass("sticky-open");
                }
            });
            $goToTopBtn.click(function () {
                $("html, body").animate({
                    scrollTop: 0
                }, 600);
                return false;
            });
            $('#customerCommentSlick').slick({
                centerMode: true,
                centerPadding: '60px',
                slidesToShow: 3,
                infinite: true,
                responsive: [
                  {
                      breakpoint: 768,
                      settings: {
                          arrows: false,
                          centerMode: true,
                          centerPadding: '40px',
                          slidesToShow: 3
                      }
                  },
                  {
                      breakpoint: 480,
                      settings: {
                          arrows: false,
                          centerMode: true,
                          centerPadding: '40px',
                          slidesToShow: 1
                      }
                  }
                ]
            });
            
        });
