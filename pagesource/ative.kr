//남은 시간
var timer={};
var remain_msec={};
var tiemTarget={};
function showRemainTime(obj, end_date, timer_flag, pid){

	if( !pid ){
		pid = "basic";
	}

	tiemTarget[pid] = obj;

	var now      = new Date().getTime();
    var end_date = new Date(end_date.replace(/-/g, '/')).getTime();

	remain_msec[pid] = end_date - now;

	if(timer_flag == true){
		timer[pid] = setInterval('setRemainTime("'+pid+'")', 1000);
	}else{
		setRemainTime(pid);
	}
}

// Set the unit values in milliseconds.
var msecPerMinute = 1000 * 60;
var msecPerHour = msecPerMinute * 60;
var msecPerDay = msecPerHour * 24;
function setRemainTime(pid){

	obj = tiemTarget[pid];
	remain_msec[pid] -= 1000;

	if( remain_msec[pid] < 0
		|| isNaN(remain_msec[pid]) )	{
		clearInterval(timer[pid]);
		var days = '0';
		var hours = '00';
		var minutes = '00';
		var seconds = '00';

	}else{

		// Get the difference in milliseconds.
		var interval = remain_msec[pid];

		// Calculate how many days the interval contains. Subtract that
		// many days from the interval to determine the remainder.
		var days = Math.floor(interval / msecPerDay );
		interval = interval - (days * msecPerDay );
		if(days.toString().length == 1){
			days = '0' + days.toString();
		}
		// Calculate the hours, minutes, and seconds.
		var hours = Math.floor(interval / msecPerHour );
		interval = interval - (hours * msecPerHour );
		if(hours.toString().length == 1){
			hours = '0' + hours.toString();
		}
		var minutes = Math.floor(interval / msecPerMinute );
		interval = interval - (minutes * msecPerMinute );
		if(minutes.toString().length == 1){
			minutes = '0' + minutes.toString();
		}
		var seconds = Math.floor(interval / 1000 );
		if(seconds.toString().length == 1){
			seconds = '0' + seconds.toString();
		}
	}

	$(obj).find('.days').html(days);
	$(obj).find('.hours').html(hours);
	$(obj).find('.minutes').html(minutes);
	$(obj).find('.seconds').html(seconds);
}
