	$(document).ready(function(){
		$( "#navigation" ).find('span').find('a')
		  .mouseover(function() {
			if($(this).hasClass("now") == false){
				$( this ).addClass( "active" );
			}
		  })
		  .mouseout(function() {
			if($(this).hasClass("now") == false){
				$( this ).removeClass( "active" );
			}
		  });

	});

	function goSearch(){
		var frm = document.search_form;
		if(frm.search_text.value==''){
			if( frm.search_text.placeholder==''){
				alert('검색어를 입력해 주세요');
				return false;
			}else{
				frm.search_text.value = frm.search_text.placeholder;
				return true;
			}
		}
		return true;
	}
</script>

