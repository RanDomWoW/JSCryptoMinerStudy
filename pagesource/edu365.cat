<script language="javascript" type="text/javascript">
function MostrarFecha()
   {
   var nombres_dias = new Array("Diumenge", "Dilluns", "Dimarts", "Dimecres", "Dijous", "Divendres", "Dissabte")
   var nombres_meses = new Array("de gener", "de febrer", "de març", "d'abril", "de maig", "de juny", "de juliol", "d'agost", "de setembre", "d'octubre", "de novembre", "de desembre")

   var fecha_actual = new Date()

   dia_mes = fecha_actual.getDate()   //dia del mes
   dia_semana = fecha_actual.getDay()   //dia de la semana
   mes = fecha_actual.getMonth() + 1
   anio = fecha_actual.getFullYear()

   //escribe en pagina
   document.write(nombres_dias[dia_semana] + ", " + dia_mes + " " + nombres_meses[mes - 1] + " de " + anio)
   }
</script>


