<script>
    jQuery(function(){
        $(".home-cta").click(function(e) {
            e.preventDefault();
            if(!$( "body" ).hasClass( "activate" )){
                $('body').addClass('activate');
                var CurParams = '';
                $('.start-box').fadeOut(1500);
                if(typeof currentParameters !== "undefined"){
                    CurParams = currentParameters;
                }
                if(!$( ".form-box" ).hasClass( "active" )){
                    $('.form-box').addClass('active').append( '<iframe src="https://go.touchbistro.com/homeform.html'+CurParams+'" class="homeiframe" style="max-width:100%;" name="homeiframe" scrolling="no" frameborder="0" marginheight="0px" marginwidth="0px" height="100%" width="100%"></iframe>' );
                }
                slowScroll('#intro',1500);
            }
            return false;
        });
        $(".x-box").click(function(e) {
            e.preventDefault();
            $('body').removeClass('activate');
            var CurParams = '';
            $('.start-box').fadeIn(1500);
            slowScroll('#top',1500);
            return false;
        });
        $('.nextsection-arrow').on('click', function(e){
            e.preventDefault();
            var id =  $(this).attr('href');
            slowScroll(id);
            return false;
        });
        $('.about-slider').slick({
            dots: false,
            speed: 200,
            arrows: false,
            accessibility: false,
        });
        $('.tab-buttons .tab').each( function( i ) {
            $(this).click(function(){
                $('.about-slider').slick('slickGoTo', i);
                $('.tab-buttons .tab').removeClass('current');
                $(this).addClass('current');
            })
        });
        $('.about-slider').on('beforeChange', function(event, slick, currentSlide, nextSlide){
            $('.tab-buttons .tab').removeClass('current');
            $( ".tab-buttons .tab[tab='"+nextSlide+"']" ).addClass('current');
        });
        $('.success-prop').click(function(){
            if( !$(this).hasClass('expand') ){
                $('.success-prop').removeClass('expand');
                $(this).addClass('expand');
            }else{
                $('.success-prop').removeClass('expand');
            }
            
        });
        $('.customers-slider').slick({
            slidesToShow: 3,
            dots: false,
            speed: 200,
            variableWidth: true,
            centerMode: true,
            arrows: false,
        });
        $('.customers-slide-nav-left').click(function() {
            $('.customers-slider').slick('slickPrev');
        });
        $('.customers-slide-nav-right').click(function() {
            $('.customers-slider').slick('slickNext');
        });
    });
    function slowScroll(id,speed){
        if(typeof speed !== "undefined" || isNaN(speed)){
           speed = 400;
        }
        var target = jQuery(id == '#' || id == "" ? 'html' : id);
        var position = target.offset().top;
        jQuery('body, html').animate({scrollTop:position}, speed, 'swing');
        return false;
    }
</script>