<script>
jQuery(document).ready(function($j) {
var delay = 1000 , additionalDelay = 10000 , applyDelayForClasses = 'div.fancybox-wrap'
, topOffsetForPopup = '0';
function isShownModelBox() {
var smthLikeFancyBox = jQuery(applyDelayForClasses);
if (typeof smthLikeFancyBox.css('display') != 'undefined' && smthLikeFancyBox.css('display') != 'none' ) {
return true;
}
return false;
}
function delayedShowExecution() {
// wait until something blocking is showing
if (isShownModelBox()) {
setTimeout(delayedShowExecution, delay);
} else {
performPopupOpen(additionalDelay);
}
}
function performPopupOpen(showWithDelay) {
if (typeof showWithDelay == 'number' && 100 <= showWithDelay) {
setTimeout(performPopupOpen, showWithDelay)
} else {
$j('#exitintent-popup').foundation('reveal', 'open');
Intenso.libs.ui.createCookie('ExitIntentPopupMigVapor',true,30);
try {
// fix position, if Foundation sums-up OY scroll
// has no effect in regular cases
jQuery('#exitintent-popup').css('top', topOffsetForPopup);
} catch (e) {
}
}
}
$j(function() {
$j(document).foundation();
if (Intenso.libs.ui.readCookie('ExitIntentPopupMigVapor') != 'true') {
delayedShowExecution();
}
});
$j(document).on('click tap touchstart', '.reveal-modal-bg', function() {
try {
if ($j('#exitintent-popup').is(":visible")) {
$j('#exitintent-popup').foundation('reveal', 'close');
}
} catch (e) {
}
})
});
jQuery(window).resize(function() {
try {
var exitintentPopup = jQuery('#exitintent-popup');
var exitintentPopupHeigth = exitintentPopup.height();
var windowHeight = jQuery(window).height();
if (typeof exitintentPopup != 'undefined' && (exitintentPopupHeigth * 1.5) >= windowHeight) {
var exitintentPopupHeigthScale = windowHeight / (exitintentPopupHeigth * 1.5);
if (0.52 >= exitintentPopupHeigthScale) {
exitintentPopupHeigthScale = 0.52;
}
exitintentPopup.css('transform', 'scale(' + exitintentPopupHeigthScale + ')');
} else {
exitintentPopup.css('transform', 'none');
}
} catch (e) {
}
});
</script>
