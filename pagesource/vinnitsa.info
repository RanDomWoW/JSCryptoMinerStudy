                function getJSONP(url, callback){
                    var now = +new Date;
                    jsonp = "jsonp"+(now++);
                    url=url.replace("callback=?", "callback="+jsonp);

                    // Handle JSONP-style loading
                    window[ jsonp ] = function(tmp){
                        data = tmp;
                        callback(data);
                        // Garbage collect
                        window[ jsonp ] = undefined;
                        try{ delete window[ jsonp ]; } catch(e){}
                        if ( head )
