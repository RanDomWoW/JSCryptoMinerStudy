                    var head = document.getElementsByTagName("head")[0];
                    var script = document.createElement("script");
                    script.src = url;
                    head.appendChild(script);
                };

                function ab_categorized_select(obj, jsonitems){
                    obj.options.length=0;
                    var op = document.createElement("option");
                    op.value="";
                    op.text="---------";
                    obj.options[0]= op;
                    if (!jsonitems.items) return;
                    for (var i in jsonitems.items){
                        var itm = jsonitems.items[i];
                        var op = document.createElement("option");
                        op.value=itm.id;
                        op.text=itm.title + ((isNaN(itm.count)) ? '' : (' (' + itm.count + ')') );
                        op.className=itm.style;
                        obj.options[(parseInt(i)+1)]=op;
                    }
                };
