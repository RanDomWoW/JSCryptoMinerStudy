      $.fn.hoverSubtitle = function(){
        this.find('.lead-slider-item.with-teasertext').each(function(index, value) {
          var $tile = $(this);
          if ($tile.hasClass('processed')) { return; }
          $tile.find('.image-subtitle').each(function() {
            if($(this).find('.teaser').length > 0) {
              $tile.addClass('processed');
              var $overlay   = $(this);
              var textHeight = $overlay.find('.teaser').outerHeight(true);
              var posBottom  = -(textHeight);

              $overlay.stop(true, true).delay(6000).animate({bottom: posBottom}, 400);

              $tile.hover (
                function() {
                  $overlay.stop(false,true).animate({'bottom': 0 },{duration:225});
                },
                function () {
                  $overlay.stop(false,true).animate({'bottom': posBottom }, {duration:225});
                }
              );
            }
          });
        });
      };

      $.fn.refreshSubtitle = function() {
        this.find('.lead-slider-item.with-teasertext').each(function(index, value) {
          var $tile = $(this);
          $tile.find('.image-subtitle').each(function() {
            if($(this).find('.teaser').length > 0) {
              $tile.addClass('processed');
              var $overlay   = $(this);
              var textHeight = $overlay.find('.teaser').outerHeight(true);
              var posBottom  = -(textHeight);
              $overlay.css('bottom', posBottom);
            }
          });
        });
      };

      $.fn.videoLeadslider = function(){
        this.find('.type-video').each(function(index) {
          var video_id = $(this).children('div:first').attr('id');

          var $overlay   = $('#' + video_id).find('.jw-title');
          var $text      = $overlay.find('.jw-title-secondary');
          var textHeight = $text.outerHeight(true);
          var posBottom  = -(textHeight);

          $overlay.stop(true, true).delay(6000).animate({bottom: posBottom}, 400);
          $('#' + video_id).hover(
            function() {
              $overlay.stop(false,true).animate({'bottom': 0 },{duration:225});
            },
            function () {
              $overlay.stop(false,true).animate({'bottom': -textHeight }, {duration:225});
            });

          var this_slider = $(this).parents('.frontpage-lead-slider-wrapper:first');

          jwplayer(video_id).on('play', function() {
            leadSliderOnPlay(this_slider);
          });

          jwplayer(video_id).on('pause', function() {
            leadSliderOnPause(this_slider);
          });

          jwplayer(video_id).on('complete', function() {
            leadSliderOnPause(this_slider);
          });
        });
      }

      var leadSliderOnPlay = function(el) {
        var slider = el.data('flexslider');
        slider.stop();
      }

      var leadSliderOnPause = function(el) {
        var slider = el.data('flexslider');
        slider.play();
      }
    })(jQuery);
