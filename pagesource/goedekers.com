<script type="text/javascript">


var testHandler = function(SC,render){
        for(var i =0; i< SC.page.products.length; i++)
        {
            var product = SC.page.products[i];
            if(product.price === 0 )
            {
                product.was = "<div class='no-was-price'></div>";
                product.map = "<a href="+product.link+"> See Options </a>";
                continue;
            }
            else if(product.c_cse == product.price)
            product.was = '$'+parseFloat(Math.round(Number(product.msrp) * 100) / 100).toFixed(2);
            else
            product.was = '$'+parseFloat(Math.round(Number(product.c_cse) * 100) / 100).toFixed(2);
            if(product.was =='$0.00')
            {
                product.was = '';
            }
            var map = product.c_map;
            //do ajax request for this products price
            product.map = "<div id="+product.id+">Please Wait </div>";
            var id = {'id':product.id};
            (function(_id){
                jQuery.ajax({
                    url: "/product/index/price",
                    type: "POST",
                    data: id
                }).success(function(data) {

                    var container = document.getElementById(_id.id);
                    if((container)!==null)
                    {
                        var updatedPrice = '$'+parseFloat(Math.round(Number(data) * 100) / 100).toFixed(2);
                        container.textContent=updatedPrice;
                    }
                });
            }(id));
        }
        render(SC);
    };

var limit = 4;
var width = screen.width;
if(width < 1200)
{
