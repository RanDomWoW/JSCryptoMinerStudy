			        	document.cookie = "showCouponCode=true";
			        	document.getElementById('coupon-open-close').innerHTML = "-";
			        	document.getElementById('emailSignupFlyoutContent').innerHTML = JSON.parse(data);
		        	}
		        	else {
		        		errorFlyout();
		        	}	
		        }, 
		        error: function(data){
		        	errorFlyout();
		        },
		        complete: function(data){
		        	jQuery('#loading-mask-flyout').hide();
		        } 
		     });
			}

			function validateFormMobile() {
				var x = jQuery('#fieldEmail_Flyout').val(); 
				var atpos = x.indexOf("@");
				var dotpos = x.lastIndexOf(".");

				if (atpos<1 || dotpos<atpos+2 || dotpos+2>=x.length) {
					jQuery('#noValid_flyout').show();
					return false;
				}
				else{
					flyoutEmailSend();
				}
			}

