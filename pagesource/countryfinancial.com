<script>
    var pagePath = '/content/cfin/en';
    var dl =  {};
	var pageName = '';
    var siteSection = '';
    var hier1 = '';
    var pagePathArray = pagePath.split("/");
    if(pagePathArray.length < 5){
        dl.hier1 = 'homepage';
        dl.siteSection = 'homepage';
        dl.pageName = 'homepage';
    }
    else{
		pagePathArray.splice(0, 3);
        dl.siteSection = pagePathArray[1];
        for(i=1;i < pagePathArray.length - 1;i++)
        {
            hier1 += pagePathArray[i];
            if(i < pagePathArray.length - 2){
                hier1 += ':';
            }
        }
        dl.pageName = hier1 + ':' + pagePathArray[pagePathArray.length-1];
        if(dl.pageName.charAt(0) === ':'){
            dl.pageName = dl.pageName.substr(1);
        }
        
        if(hier1.charAt(0) === ':'){
        	dl.hier1 = hier1.substr(1);
        }else{
        	dl.hier1 = hier1;
        }    
    }
</script>

