<script type="text/javascript">
$(document).ready(function() {
	$('.anasayfa_sayfa').delegate('a', 'click', function(event) {
		var sayfano = $(this).attr('href').split('sayfa-');
		$('#icerik').html('<div class="yukleniyor">&nbsp;</div>');
		$.ajax({
			url: 'index.php?sayfa=1',
			data: 'anasayfa_sayfano='+sayfano[1],
			type: 'POST',
			cache: false,
			success: function(data) {
				$('#icerik').html(data);
				$('html, body').animate({
					scrollTop: $("#icerik").offset().top
				 }, '2000');
			}
		});
		return false;
	});
});
</script>	
<script type="text/javascript">
$(document).ready(function() {
	$('body').delegate('.ana_menu', 'click', function(event) {
	$('#icerik').html('<div class="yukleniyor">&nbsp;</div>');
	var no=$(this).attr("href");
	var ids=$(this).attr("id");
		$.ajax({
			url: '/',
			data: 'sayfa='+ids,
			type: 'GET',
			cache: false,
			success: function(data) {
				$('#icerik').html(data);
				for (a=1;a<4;a++){
					$(".ana_menu").removeClass("aktif"); 
				}
				$("#"+ids).addClass("aktif"); 
			}
		});
	return false;
	});
});
</script>