	//tab
	 $("div.tabIndex a").each(function(index) {
		$(this).mouseenter(function() {
			$(this).siblings().removeClass("curr");
			$(this).addClass("curr");
			$("div.tabCon:eq(" + index + ")").show().siblings().hide();
		});

	});
	
	$("div.recomIndex a").each(function(index) {
		$(this).mouseenter(function() {
			$("div.recomIndex a").removeClass("curr");
			$(this).addClass("curr");
			$("div.recomCon li:eq(" + index + ")").show().siblings().hide();
		});

	});
})
</script>
