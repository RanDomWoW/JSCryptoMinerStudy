                        // makes it impossible for users to type the first letter in lowercase
                        $(function() {
                            $("#from, #dest, #from-period-ticket, #to-period-ticket, #from-real-time, #to-real-time").keyup(function(e) {
                                var code = (e.keyCode || e.which);
                                if(code == 37 || code == 39) {
                                    return;
                                } else if (code != 37 || code != 39) {
                                    toUpper(this);
                                }
                            });
                        });
                        function toUpper(obj) {
                            var mystring = obj.value;
                            var sp = mystring.split(' ');
                            var wl = 0;
                            var f ,r;
                            var word = new Array();
                            for (i = 0 ; i < sp.length ; i ++ ) {
                                f = sp[i].substring(0,1).toUpperCase();
                                r = sp[i].substring(1).toLowerCase();
                                word[i] = f+r;
                            }
                            newstring = word.join(' ');
                            obj.value = newstring;
                            return true;
                        }

