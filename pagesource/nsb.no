   </form><script type="text/javascript">
            var natDays = [];
            
                natDays.push("2018-03-25");
            
                natDays.push("2018-03-29");
            
                natDays.push("2018-03-30");
            
                natDays.push("2018-03-31");
            
                natDays.push("2018-04-01");
            
                natDays.push("2018-04-02");
            
                natDays.push("2018-05-01");
            
                natDays.push("2018-05-08");
            
                natDays.push("2018-05-10");
            
                natDays.push("2018-05-17");
            
                natDays.push("2018-05-19");
            
                natDays.push("2018-05-20");
            
                natDays.push("2018-05-21");
            

            $(function() {
                $('#nsb-travelplanner-search .nsb-search-toggle').click(function(e){
                    e.preventDefault();
                    $(this).toggleClass('nsb-js-open');
                    $(".nsb-js-travel-planner-fieldset").toggleClass('nsb-toggle-hide');
                    $('.nsb-js-travelers-top-summary-number, .nsb-js-travelers-top-summary-travelers').toggle();
                    var closeText = $(this).find('.nsb-js-search-toggle-toggle-text').data('close-text');
                    var openText = $(this).find('.nsb-js-search-toggle-toggle-text').data('open-text')
                    if ($(this).hasClass('nsb-js-open')) {
                        $(this).find('.nsb-js-search-toggle-toggle-text').text(closeText);
                    } else {
                        $(this).find('.nsb-js-search-toggle-toggle-text').text(openText);
                    }
                });

                
                if ($('summary-traveltime').hasClass('not-oneway')) {
                    $('#isReturn').val('return');
                }

                $('#booking-animalallergy-input').click(function() {
                    if ($(this).is(':checked')) {
                        $('#bookingPassengerExtrasPetfree').val('1');
                    } else {
                        $('#bookingPassengerExtrasPetfree').val('0');
                    }
                });

                

                // If oneway: disable return date and time inputs on submit
                $('#nsb-travelplanner-search').submit(function (e) {
                    if ($(this).valid()) {
                        
                        if ($('#toggle-oneway').is(':checked')) {
                            $('#date-return-input, #booking-date_return_hour').attr('disabled', 'disabled');
                            $('#booking-date_return_hour').attr('disabled', 'disabled');
                        }       
                        // disable input for oneway or return
                        $('#toggle-oneway').attr('disabled', 'disabled');
                        $('#toggle-return').attr('disabled', 'disabled');
                        // accessibility cookie checkbox not relevant
                        $('#booking-remember-accessibility').attr('disabled', 'disabled');
                    };
                });

                // Traveldate drawer
                $('.nsb-js-toggle-datepicker').click(function(e) {
                    $(this).closest('.nsb-drawer-traveltime-wrapper').siblings().find('.nsb-datepicker-wrapper').removeClass('nsb-datepicker-wrapper--active');
                    $(this).closest('.nsb-drawer-traveltime-wrapper').siblings().find('.nsb-js-toggle-datepicker').removeClass('nsb-datepicker-active');
                    $(this).closest('.nsb-drawer-traveltime-wrapper').find('.nsb-datepicker-wrapper').toggleClass('nsb-datepicker-wrapper--active');
                    $(this).toggleClass('nsb-datepicker-active');
                });

                // Toggle one-way or two-way
                if(!$('#toggle-return').is(':checked')) {
                    $('.nsb-js-toggle-return-fields').hide();
                }

                $('#toggle-return').on('click', function (e) {
                    $('.nsb-js-toggle-return-fields').show();

                    $('#nsb-datepicker-return').datepicker( 'option', 'disabled', false );
                    $('#nsb-datepicker-return').removeClass('inactive');

                    $('summary-traveltime').addClass('not-oneway');
                    $('#isReturn').val("return");
                    $(this).parents('.nsb-drawer-traveltime-wrapper').find('.nsb-js-toggle-datepicker').focus().click();
                });

                $('#toggle-oneway').on('click', function (e) {
                    $('#nsb-travelplanner-search').find('.nsb-js-toggle-return-fields').hide();
                    $('#nsb-datepicker-return').datepicker('option', 'disabled', true);
                    $('#nsb-datepicker-return').addClass('inactive');

                    $('summary-traveltime').removeClass('not-oneway');
                    $('#nsb-datepicker-oneway').datepicker('option', {maxDate: '+89d', beforeShowDay: clearCustomRange});
                    initializeDatepicker('oneway');
                    $('#isReturn').val("single");
                    $(this).parents('.nsb-drawer-traveltime-wrapper').find('.nsb-js-toggle-datepicker').focus().click();
                });

                function initTraveltimeSummary() {
                    var inputHour = $('#booking-date_outward_hour').val();
                    var inputDateOneWay = $('#date-oneway-input').val();
                    var inputDateOneWayString = inputDateOneWay !== undefined ? inputDateOneWay.substring(0, 6) : '';
                    var inputDateReturn = $('#date-return-input').val();
                    var inputDateReturnString = inputDateReturn !== undefined ? inputDateReturn.substring(0, 6) : '';

                    if (NSB.language == 'no') {
                        moment.locale('nb');
                    } else {
                        moment.locale('en');
                    }

                    // Search is not for now if (date is not todays date) or (date is same and hours are less)
                    if ($('#date-oneway-input').val() != $('#date-oneway-input').data('nowvalue') ||
                            ($('#date-oneway-input').val() == $('#date-oneway-input').data('nowvalue'))) {
                        $('.nsb-js-traveltime-summary').data('turn-date', moment(inputDateOneWay, 'DD.MM.YYYY').format('DD.MMM'));
                    } else {
                        $('.nsb-js-traveltime-summary').data('turn-date', $('.nsb-js-traveltime-summary').data('now'));
                    }

                    var isOutwardBefore = ($('#toggle-calculate-by-arrival').is(':checked'));

                    if (inputHour === 'now') {
                        $('.nsb-js-traveltime-summary').data('turn-date', $('.nsb-js-traveltime-summary').data('now'));
                    } else {
                        $('.nsb-js-traveltime-summary').data('turn-date', moment(inputDateOneWay, 'DD.MM.YYYY').format('DD.MMM'));
                    }

                    var isReturnBefore = ($('#toggle-return-by-arrival').is(':checked'));
                    if ($('#toggle-return').is(':checked')) {
                        if ($('#booking-date_return_hour').val() === 'now') {
                            $('.nsb-js-traveltime-summary').data('return-date', $('.nsb-js-traveltime-summary').data('now'));
                        } else {
                            $('.nsb-js-traveltime-summary').data('return-date', moment(inputDateReturnString, 'DD.MM.YYYY').format('DD.MMM'));
                        }
                    } else {
                        $('.nsb-js-traveltime-summary').data('return-date', '');
                    }
                    $('.nsb-js-traveltime-summary').trigger('changeDate')
                }

                $('#drawer-traveltime').change(function(){
                    initTraveltimeSummary();
                });

                initTraveltimeSummary();

                function initializeDatepicker (onewayOrReturn) {
                    disableDatePicker = onewayOrReturn == 'return' ?  true : false;
                    $('#nsb-datepicker-' + onewayOrReturn).datepicker({
                        altField: '#date-' + onewayOrReturn + '-input',
                        dateFormat: 'dd.mm.yy',
                        defaultDate: $('#date-' + onewayOrReturn + '-input').data('defaultdate'),
                        firstDay: 1,
                        minDate: 0,
                        disabled: disableDatePicker,
                        maxDate: '+89d',
                        beforeShowDay: markNationalDaysAndSundays
                    });
                }

                initializeDatepicker('oneway');
                initializeDatepicker('return');

                // Sync datepickers so that their minDate and maxDate stay within the same range
                $('#nsb-datepicker-oneway').datepicker('option', 'onSelect', function (dateText) {
                    // Sync datepickers so that their minDate and maxDate stay within the same range
                    if ($('#toggle-oneway').is(':checked')) {
                        $(this).datepicker('option', {setDate: dateText});
                    } else {
                        $(this).datepicker('option', {setDate: dateText, beforeShowDay: customRange});
                    }
                    $('#nsb-datepicker-return').datepicker('option', 'minDate', dateText);
                    initTraveltimeSummary();
                    // if another date than today is chosen, the time should change to 03:00
                    // and if today is selected again, the time should change to the current time
                    //updateDepartureTime();
                    $(this).change();
                    $('#date-oneway-input, #date-return-input').change();
                });

                $('#nsb-datepicker-return').datepicker('option', 'onSelect', function (dateText) {
                    // Sync datepickers so that their minDate and maxDate stay within the same range
                    $(this).datepicker('option', {setDate: dateText, beforeShowDay: customRange});
                    $('#nsb-datepicker-oneway').datepicker('option', {setDate: dateText, beforeShowDay: customRange});
                    $('#nsb-datepicker-oneway').datepicker('option', 'maxDate', dateText);
                    initTraveltimeSummary();
                    $(this).change();
                    $('#date-oneway-input, #date-return-input').change();
                });

                function initTravelersSummary() {
                    // Determine and show number of travelers
                    var numTravelers = parseInt($('#booking-num-travelers-select').val());
                    if (numTravelers < 10) {
                        $('#num-travelers-summary').text(numTravelers);
                    }

                    // Determine passenger type string based on 1-based index (not value!) of selected option for the first passenger
                    var firstPassengerTypeIndex = 0;
                    $('#booking-passenger_type-1').find('option').each(function (index) {
                        if ($(this).is(':selected')) {
                            firstPassengerTypeIndex = index;
                        }
                    });
                    var firstPassengerSelectedOption = $('#booking-passenger_type-1').find('option').eq(firstPassengerTypeIndex);
                    var firstPassengerTypeString = ((numTravelers == 1) ? firstPassengerSelectedOption.html() : firstPassengerSelectedOption.data('pluralstring'));

                    var firstPassengerType = $('#booking-passenger_type-1').val();
                    var isMoreThanOnePassengerType = false;
                    $('#booking-who-details').find('.booking-passenger').find('select').slice(0, numTravelers).each(function (index) {
                        if ($(this).val() != firstPassengerType) {
                            isMoreThanOnePassengerType = true;
                        }
                    });
                    if (isMoreThanOnePassengerType) {
                        $('#traveler-types-summary').html($('#traveler-types-summary').data('genericstring'));
                    } else {
                        $('#traveler-types-summary').html(firstPassengerTypeString);
                    }
                }

                $('#drawer-travelers').find('select, input').on('change blur', function (e) {
                    initTravelersSummary();
                });

                initTravelersSummary();

                // Show travel card input field
                $('#booking-who-details').find('.passenger-type').change(function (e) {
                    if ($(this).val() === '1+' || $(this).val() === '90+') {
                        $(this).closest('.booking-passenger').find('.passenger-card').removeClass('disabled').find('input').attr('disabled', false);
                    } else if ($(this).val() === '90') {
                        var passengerCardInput = $(this).closest('.booking-passenger').find('.passenger-card').addClass('disabled').find('input');
                        passengerCardInput.val($(this).children('option:selected').data('id'));
                        passengerCardInput.prop('disabled', false);
                    } else {
                        $(this).closest('.booking-passenger').find('.passenger-card').addClass('disabled').find('input').attr('disabled', 'disabled');
                    }

                    if ($(this).closest('#nsb-travelplanner-search').find('.nsb-js-invalid-customercard-error').is(":visible")) {
                        $(this).closest('#nsb-travelplanner-search').find('.nsb-js-invalid-customercard-error').hide();
                    }
                });

                // Show currently chosen amount of passenger type dropdowns
                $('#booking-num-travelers-select').on('change blur', function () {
                    if( $(this).val() > 0 && $(this).val() < 10) {
                        updateNumAvailTravelerTypeInputs();
                    }
                    updateAvailableSpecialLugaggeOptions();
                });


                function updateNumAvailTravelerTypeInputs() {
                    $('#booking-who-details').find('li.booking-passenger').each(function (index) {
                        if (index < parseInt($('#booking-num-travelers-select').val())) {
                            $(this).removeClass('disabled').find('select').attr('disabled', false);
                        } else {
                            $(this).addClass('disabled').find('select').attr('disabled', 'disabled');
                        }
                    });
                }

                function updateAvailableSpecialLugaggeOptions() {
                    // Update stroller options
                    $('#booking-passenger_extras_stroller').find('option').each(function (index) {
                        if (index <= parseInt($('#booking-num-travelers-select').val())) {
                            $(this).attr('disabled', false);
                        } else {
                            $(this).attr('disabled', 'disabled');
                        }
                    });

                    // Update stroller options
                    $('#booking-passenger_extras_closetodog').find('option').each(function (index) {
                        if (index <= parseInt($('#booking-num-travelers-select').val())) {
                            $(this).attr('disabled', false);
                        } else {
                            $(this).attr('disabled', 'disabled');
                        }
                    });

                    // Update stroller options
                    $('#booking-passenger_extras_animalsallowed').find('option').each(function (index) {
                        if (index <= parseInt($('#booking-num-travelers-select').val())) {
                            $(this).attr('disabled', false);
                        } else {
                            $(this).attr('disabled', 'disabled');
                        }
                    });
                }

                // Handling validation errors
                var errorParameter = getUrlParameter('error');
                if (errorParameter === '300') {
                    $('#nsb-drawer-trigger-travelers .nsb-drawer-call-to-action').click();
                }

                // Acquire former search criterias from session storage
                $(document).ready(function () {
                    if (typeof(Storage) !== 'undefined') {
                        // Stations
                        if (sessionStorage.bcFromStation) {
                            $('.nsb-js-travelplanner-search .nsb-js-travel-planner-station-from').val(sessionStorage.bcFromStation);
                        }

                        if (sessionStorage.bcToStation) {
                            $('.nsb-js-travelplanner-search .nsb-js-travel-planner-station-to').val(sessionStorage.bcToStation);
                        }

                        /*Dates*/
                        if (sessionStorage.bcDateOneWay && sessionStorage.bcDateReturn) {
                            // Set value in input-field and set date in datepicker
                            $('#date-oneway-input').val(sessionStorage.bcDateOneWay);
                            $('#nsb-datepicker-oneway').datepicker('setDate', sessionStorage.bcDateOneWay);
                            $('#date-return-input').val(sessionStorage.bcDateReturn);
                            $('#nsb-datepicker-return').datepicker('setDate', sessionStorage.bcDateReturn);

                            // Trigger click so return is shown in both base and drawer

                            $('.nsb-travel-planner-radio-input-wrapper.twoway input').click();

                            var now = moment().format('DD.MM.YYYY')

                            if (sessionStorage.bcDateOneWay > now) {
                                $('#booking-date_outward_hour').find("option[value='now']").addClass('hide');
                            }

                            if (sessionStorage.bcDateReturn > now) {
                                $('#booking-date_return_hour').find("option[value='now']").addClass('hide');
                            }

                            initTraveltimeSummary();

                        } else if (sessionStorage.bcDateOneWay) {
                            // Set value in input-field and set date in datepicker
                            $('#date-oneway-input').val(sessionStorage.bcDateOneWay);
                            $('#nsb-datepicker-oneway').datepicker('setDate', sessionStorage.bcDateOneWay);

                            // Set return as well --&gt; making the preselected date a valid one
                            $('#date-return-input').val(sessionStorage.bcDateOneWay);
                            $('#nsb-datepicker-return').datepicker('setDate', sessionStorage.bcDateOneWay);

                            var now = moment().format('DD.MM.YYYY')

                            if (sessionStorage.bcDateOneWay > now) {
                                $('#booking-date_outward_hour').find("option[value='now']").addClass('hide');
                            }

                            initTraveltimeSummary();

                        } else if (sessionStorage.bcDateReturn) {
                            // Set value in input-field and set date in datepicker
                            $('#date-return-input').val(sessionStorage.bcDateReturn);
                            $('#nsb-datepicker-return').datepicker('setDate', sessionStorage.bcDateReturn);

                            // Trigger click so return is shown in both base and drawer
                            $('.nsb-travel-planner-radio-input-wrapper.twoway input').click();

                            var now = moment().format('DD.MM.YYYY')

                            if (sessionStorage.bcDateReturn > now) {
                                $('#booking-date_return_hour').find("option[value='now']").addClass('hide');
                            }

                            initTraveltimeSummary();
                        }

                        /*Hours*/
                        if (sessionStorage.bcHourOneWay &&
                                ($('#date-oneway-input').val() != $('#date-oneway-input').data('nowvalue') ||
                                ($('#date-oneway-input').val() == $('#date-oneway-input').data('nowvalue') && sessionStorage.bcHourOneWay >
                                new Date().getHours()))) {
                            $('#booking-date_outward_hour').val(sessionStorage.bcHourOneWay);
                            $('#booking-date_hour_turn').val(sessionStorage.bcHourOneWay);
                            $('#booking-date_outward_hour, #booking-date_hour_turn').change();
                        }

                        if (sessionStorage.bcHourReturn) {
                            $('#booking-date_return_hour').val(sessionStorage.bcHourReturn);
                            $('#booking-date_hour_return').val(sessionStorage.bcHourReturn);
                            $('#booking-date_return_hour, #booking-date_hour_return').change();
                        }

                        /*Travelers*/
                        if (sessionStorage.bcNumberOfTravelers) {
                            $('#booking-num-travelers-select').val(sessionStorage.bcNumberOfTravelers);
                            $('#booking-num-travelers-select').change();
                        }

                        /*Passenger categories*/
                        if (sessionStorage.bcPassengerCategories) {
                            var passCatArray = sessionStorage.bcPassengerCategories.split(',');
                            var numPassCatArray = sessionStorage.bcNumberOfTravelers.split(',');

                            /*Reset result*/
                            $('.nsb-js-travelers').val(0);

                            /*Set values*/
                            for (var i = 0; i < passCatArray.length; i++) {
                                var passenger = $('.nsb-js-travelers[name=' + passCatArray[i] + ']');
                                passenger.val(numPassCatArray[i]);
                            }

                            /*Trigger change --&gt; handles "voksne" vs "personer"*/
                            $('#num-travelers-summary').text(updateTravelersSum($('.nsb-js-travelers')));
                            $('.nsb-js-travelers-top-summary-number').text(updateTravelersSum($('.nsb-js-travelers')));
                            updateEnableDisableButtonFunctionWrapper();
                            $('.nsb-js-traveler-types-summary').data('num-travelers', updateTravelersSum($('.nsb-js-travelers'))).trigger('changeTravelers');


                            var error = '';
                            if (error = '300'){
                                $('#nsb-travelplanner-search').find('.nsb-js-invalid-customercard-error').show();
                            }

                        }

                        /**Accessibility**/
                        /*Pet free*/
                        if (sessionStorage.bcPetFree && sessionStorage.bcPetFree === 'true') {
                            $('#booking-animalallergy-input').prop('checked', true);
                            updateOtherOptions($('#booking-animalallergy-input'));
                        }

                        /*Strollers*/
                        if (sessionStorage.bcStrollerCount && sessionStorage.bcStrollerCount > 0) {
                            $('#booking-passenger_extras_stroller').val(sessionStorage.bcStrollerCount);
                            updateOtherOptions($('#booking-passenger_extras_stroller'));
                        }

                        /*Dogs over 40cm*/
                        if (sessionStorage.bcDogsOver40Count && sessionStorage.bcDogsOver40Count > 0) {
                            $('#booking-passenger_extras_animalsallowed').val(sessionStorage.bcDogsOver40Count);
                            updateOtherOptions($('#booking-passenger_extras_animalsallowed'));
                        }

                        /*Pets under 40cm*/
                        if (sessionStorage.bcPetsUnder40Count && sessionStorage.bcPetsUnder40Count > 0) {
                            $('#booking-passenger_extras_closetodog').val(sessionStorage.bcPetsUnder40Count);
                            updateOtherOptions($('#booking-passenger_extras_closetodog'));
                        }

                        /*Bike Only*/
                        if (sessionStorage.bikeOnly && sessionStorage.bikeOnly > 0) {
                            $('#booking-passenger_extras_11').val(sessionStorage.bikeOnly);
                            updateOtherOptions($('#booking-passenger_extras_11'));
                        }

                        /*Bike trailer*/
                        if (sessionStorage.bikeTrailer && sessionStorage.bikeTrailer > 0) {
                            $('#booking-passenger_extras_55').val(sessionStorage.bikeTrailer);
                            updateOtherOptions($('#booking-passenger_extras_55'));
                        }

                        if (sessionStorage.bcTravelerCards) {
                            var travelerCardsArray = sessionStorage.bcTravelerCards.split(',');
                            for (var i = 0; i < travelerCardsArray.length; i ++) {
                                $('.nsb-js-travelers-card').val(i + 1);
                                updateTravelerCard($('.nsb-js-travelers-card'), true, travelerCardsArray[i]);
                            }
                        }

                        if (sessionStorage.bcEmployeeTickets) {
                            var employeeTicketsArray = sessionStorage.bcEmployeeTickets.split(',');
                            for (var i = 0; i < employeeTicketsArray.length; i ++) {
                                $('.nsb-js-travelers-employee').val(i + 1);
                                updateEmployeeSelect($('.nsb-js-travelers-employee'), true, employeeTicketsArray[i]);
                            }
                        }

                        if (!sessionStorage.bcNumberOfTravelers &&
                            !sessionStorage.bcPassengerCategories &&
                            !sessionStorage.bcEmployeeTickets &&
                            !sessionStorage.bcTravelerCards) {
                            $('#passengerCategory-1').val(1);
                        }


                        $('#num-travelers-summary').text(updateTravelersSum($('.nsb-js-travelers')));
                        $('.nsb-js-travelers-top-summary-number').text(updateTravelersSum($('.nsb-js-travelers')));
                        updateEnableDisableButtonFunctionWrapper();
                        updateTravelerTypesSummary();
                        $('.nsb-js-traveler-types-summary').data('num-travelers', updateTravelersSum($('.nsb-js-travelers'))).trigger('changeTravelers');
                    }
                });

                //Update for error text
                $('#date-oneway-input, #date-return-input').bind('change', function() {
                    $(this).focusout();
                });
            });
        </script><div id="booking-overlay" class="modal hide" tabindex="-1" role="dialog"
        aria-hidden="true">
      <div class="modal-content">
         <div class="modal-heading">Vennligst vent</div>
         <p>Vi søker etter ledige billetter.</p>
      </div>
