<script>
function colorChangingBackground() {
	var theHour = new Date().getHours();
	if ( theHour >= 1 && theHour < 5) {
        // If time is between 1am – 5am
        $('.featured-collection.dynamic-featured-collection').find('.page-graphic-margin').addClass('oneToFive');
    } else if (theHour >= 5 && theHour < 9) {
        // If time is between 5am – 9am
        $('.featured-collection.dynamic-featured-collection').find('.page-graphic-margin').addClass('fiveToNine');
    } else if (theHour >= 9 && theHour < 13) {
        // If time is between 9am – 1pm
        $('.featured-collection.dynamic-featured-collection').find('.page-graphic-margin').addClass('nineToThirteen');
     } else if (theHour >= 13 && theHour < 17) {
        // If time is between 1pm – 5pm
        $('.featured-collection.dynamic-featured-collection').find('.page-graphic-margin').addClass('thirteenToSeventeen');          
    } else if (theHour >= 17 && theHour < 21) {
        // If time is between 5pm – 9pm
        $('.featured-collection.dynamic-featured-collection').find('.page-graphic-margin').addClass('seventeenToTwentyOne');            
    } else if (theHour >= 21 && theHour < 25) {
        // If time is between 9pm – 1am
        $('.featured-collection.dynamic-featured-collection').find('.page-graphic-margin').addClass('twentyOneToTwentyFour');                 
    } else {
    	  $('.featured-collection.dynamic-featured-collection').find('.page-graphic-margin').addClass('twentyOneToTwentyFour');
    }
}

