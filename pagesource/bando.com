        });

        $('a.de-add-to-bag.atb-with-variant').on('click', function(e) {
              e.preventDefault();
              var theTitle = $(this).closest('.recommendations-product').find('.info').find('.title a').text();
              if ( $(this).attr('data-variantid') == 'blank' ) {
                  $(this).before('<p class="cart-upsell-error" style="margin-top: -10px; padding-top: 0px; text-align: center">Please select Size</p>');
              } else {
                  $(this).prev('.cart-upsell-error').remove();
                  easyDirectedAddToCartAjax( $(this) );
                  eventDirectedAdded(theTitle);
              }
        });

            
      }
</script>

