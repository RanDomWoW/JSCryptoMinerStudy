                    success: function(message) {
                      Shopify.country = '';
                    }
                  });



                },
                error: function(err) {
                  console.error('There was an error in getCountryCartAtr: ', err);
                }
              });
            }
            else{
              Shopify.country = '';
            }
        }
      });
  }

