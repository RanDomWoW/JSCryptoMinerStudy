                  <script>
function easyDirectedAddToCartAjax(_this) {
            var $this = _this, 
                  variantIDToAdd = $this.attr('data-variantid'),
                  directedEdgeItem = {
                        'id': variantIDToAdd, 
                        'quantity': 1          
                  };

            $.ajax({
                    url: '/cart/add.js',
                    type: 'post',
                    dataType: 'json',
                    data: {
                        quantity: 1,
                        id: variantIDToAdd
                    },                    
                    beforeSend: function(){ $this.val('Adding To Bag'); },
                    success: function(){
                        $("html, body").animate({ scrollTop: 0 }, "slow");
                        location.href= "/cart";
			
                    },
                    error: function(message) {
						if ( $this.hasClass('variant-needed-button') && variantIDToAdd == 'blank' ) {
								console.log(variantIDToAdd, 'variant id to add');
								$('.productselect-variant').addClass('cart-upsell-error-box');
								$('.cart-upsell-error').show();
						}	

                    }
            });    
}

function eventDirectedAdded(theTitle) {
      ga('send', 'event', {
        eventCategory: 'directededge_upsellpopup',
        eventAction: 'add_to_cart',
        eventLabel: theTitle,
        transport: 'beacon'
      });    
}
function directedEdgeUpsell() {
    $('.upsell-popup-wrap').find('.recommendations-product').each( function() {
      var $this = $(this);
        if ( $this.find('.directed-edge-select').length != 0 && $this.find('select.afor-selecter').length == 0  ) {

            var $directedEdgeSelect = $this.find('.directed-edge-select');
            $this.find('.select-wrapper-container').show().css('opacity', '1').prepend('<select class="afor-selecter"></select>');
            $directedEdgeSelect.find('li').each(function() {
                var thisValue = $(this).attr('data-variantvalue'),
                      thisAvailability = '',
                      thisIsDisabled = '',
                      thisLiText = $(this).text();

                if ( $(this).attr('data-variantavailable') == 'false' ) {
                     thisAvailability = '  - soldout';
                     thisIsDisabled = 'disabled';
                }
                      
                $this.find('select.afor-selecter').append('<option data-value-text="'+thisLiText+'" value="'+thisValue+'" data-variantavailable="" '+thisIsDisabled+'>'+ thisLiText + ' ' + thisAvailability +'</option>');                      
            }); 

            
            //$('.selector-wrapper').hide();            
        }
    });

        if ( $('.upsell-popup-wrap').find('.select-wrapper-wrapper').find('.directed-edge-select').length != 0  ) {
            $('.upsell-popup-wrap').find('.select-wrapper-wrapper').show();
            $('.upsell-popup-wrap').find('a.de-add-to-bag.atb-with-variant').show();
            $('.upsell-popup-wrap').find('.view-product-btn').hide();
        }

        $('.upsell-popup-wrap').find('.directed-edge-add-to-bag').show();

        $('.upsell-popup-wrap').find('a.de-add-to-bag.normal-product').on('click', function(e) {
                e.preventDefault();
                var theTitle = $(this).closest('.recommendations-product').find('.info').find('.title a').text();
                easyDirectedAddToCartAjax( $(this) ); 
                eventDirectedAdded(theTitle);
                
        });

        $('.upsell-popup-wrap').find('.select-wrapper-wrapper').find('select.afor-selecter').on('change', function() {
            $(this).find('option:first-child').prop('disabled', 'disabled');
            $(this).closest('.recommendations-product').find('.directed-edge-add-to-bag').find('.cart-upsell-error').remove();
