<script>
  $(document).ready(function() {
    $('.shopthelook-popup').on('click', function(e) {
        e.preventDefault();
        var theSTLURL = $(this).attr('data-handle').toLowerCase();

        var isMobile = false; //initiate as false
        

        shopthelookViewEvent(theSTLURL);

        if ( $(window).width() < 767 ) {

            window.location.href = '/products/'+theSTLURL+'?view=shopthelook-mobile';

        } else {

            $.ajax({
              url: '/products/'+theSTLURL+'?view=shopthelook',
              type: 'get',
              dataType: 'html',
              success: function( stlResult ) {
                  buildSTLQuickShop( stlResult );
                  // $('body').css('overflow-y','hidden');
                  $('body').addClass('overflow-no-scrolling');
                  $('.quickshop-closer').on('click', function() {
                      $('body').removeClass('overflow-no-scrolling');
                  });
              }
            });
        }



    });


    function buildSTLQuickShop(result){
      var svg = '//cdn.shopify.com/s/files/1/0787/5255/t/278/assets/quickshop-closer.svg?1205193808477080279';
      var quickShop = '\
    			<div class="bold-overlay qv-overlay qv-shopthelook">\
    			  <div id="bold-modal" class="bold-modalwindow">\
              <div class="inner-content">\
                <div>\
  				<a class="quickshop-closer">\
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="1559 867 14 14" class="cls-1-svg">\
                      <path id="X" class="cls-1" d="M19,6.4,17.6,5,12,10.6,6.4,5,5,6.4,10.6,12,5,17.6,6.4,19,12,13.4,17.6,19,19,17.6,13.4,12Z" transform="translate(1554 862)"/>\
                    </svg>\
    				</a>\
                </div>\
                '+result+'\
              </div>\
            </div>\
    			</div>';
      return $('body').append(quickShop);

    };

    function shopthelookViewEvent(theSTLURL){
      ga('send', 'event', {
        eventCategory: 'bold_shopthelook',
        eventAction: 'view',
        eventLabel: theSTLURL,
        transport: 'beacon'
      });
    };

  });
</script>


