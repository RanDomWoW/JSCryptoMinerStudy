<style type="text/css">

#categoryAttributeBox {
	display:none;
	position:absolute;
	width:600px;
	margin-left:-300px;
	min-height:300px;
	margin-top:-150px;
	top:50%;
	left:50%;
	
	border:1px solid #333;
	padding:15px;
	z-index:150;
	background:#fff;
	background-position:bottom right;
	background-repeat:no-repeat;
		
	border-radius:10px;
	box-shadow: 0px 0px 30px #444;
}

#categoryAttributeBox h1 {
	margin:0px;
}


#categoryAttributes a {
	display:block;
	float:left;
	padding:3px;
	margin:3px;
}

#categoryAttributeBox a.CloseBtn {
	display:block;
	float:right;
}

</style>

<script type="text/javascript">

	var selectedCategoryId = 0;
	var nextAttributeNameId = 0;
	var selectedAttributeNames = new Array();
	var selectedAttributeValues = new Array();
	var selectedCategoryURL = "";
	
	function selectAttributeValue(attributeNameId, attributeValueId) {

		//add selected values to array
		if (!contains(selectedAttributeValues, attributeValueId)) { selectedAttributeValues.push(attributeValueId); }
		if (!contains(selectedAttributeNames, attributeNameId)) { selectedAttributeNames.push(attributeNameId);	}
		
		loadAttributes(0);
	}

	function showAttributes(categoryId, initialAttributeNameId, categoryImage, categoryUrl) {
	
		//display category image
		$("#categoryAttributeBox").css("background-image", "url("+categoryImage+")");

		//set selected values
		selectedCategoryId = categoryId;
		selectedCategoryURL = categoryUrl;

		//clear any previously selected values
		selectedAttributeNames = new Array();
		selectedAttributeValues = new Array();

		//load the data
		loadAttributes(initialAttributeNameId);
	}

	function loadAttributes(initialAttributeNameId) {

		$("#categoryAttributeBox").show();
		$("#categoryAttributeHeading").html("");
		$("#categoryAttributes").html("loading...");

		var attributeNameId = 0;
		var attributeName = "";
		var attributeValueId = 0;
		var attributeValue = "";

		var selectedAttributeName = "";
		var valueList = "";
		var selectedAttributeList = "";
		var foundUnselectedAttribute = false;

		//build selection list
		for (var i = 0; i < selectedAttributeValues.length; i++) {
			if (selectedAttributeList != "") { selectedAttributeList += "," }
			selectedAttributeList += selectedAttributeValues[i];
		}


		var dataURL = "/_ajax/CategoryAttributesJSON.ashx?categoryid=" + selectedCategoryId + "&selectedAttributes=" + encodeURIComponent(selectedAttributeList);
		console.log("dataURL " + dataURL);

		//get the attribute data
		$.getJSON(dataURL, function (data) {

			if (data.length != 0) {

				$.each(data, function (key, val) {

					attributeNameId = val.AttributeNameId;
					attributeName = val.AttributeName;
					attributeValueId = val.AttributeValueId;
					attributeValue = val.AttributeValue;

					//only show the selected attribute
					//if initialAttributeNameId is 0, then an attribute has already been selected, so show the next in line
					if (attributeNameId == initialAttributeNameId || (initialAttributeNameId == 0 && !contains(selectedAttributeNames, attributeNameId))) {

						//set this value so we only show the current attribute name values
						initialAttributeNameId = attributeNameId;

						//label for the popup heading
						selectedAttributeName = attributeName;

						//html for the list
						valueList += "<a href=\"#\" onclick=\"selectAttributeValue(" + attributeNameId + "," + attributeValueId + ");return false;\">" + htmlEncode(attributeValue) + "</a>";

						foundUnselectedAttribute = true;
					}

				});


				if (foundUnselectedAttribute) {
					//display the values
					$("#categoryAttributeHeading").html("Select " + htmlEncode(selectedAttributeName));
					$("#categoryAttributes").html(valueList);
				} else {
					//attribute results are the ones we already selected, so just go to the results
					loadResults();
				}


			} else {

				if (selectedAttributeValues.length > 0) {
					//no more attributes available to select, so show results
					loadResults();

				} else {
					//we should never see this
					$("#categoryAttributeHeading").html("No results");
					$("#categoryAttributes").html("We could not find any values for the selected attribute.");
				}


			}

		});

	}


	function loadResults() {

		var selectedAttributeList = "";
		//build selection list
		for (var i = 0; i < selectedAttributeValues.length; i++) {
			if (selectedAttributeList != "") { selectedAttributeList += "," }
			selectedAttributeList += selectedAttributeValues[i];
		}

		//set a nice message
		$("#categoryAttributeHeading").html("All set!");
		$("#categoryAttributes").html("Loading product results...");

		//redirect to results - timeout is optional
		setTimeout(function () {
			location.href = selectedCategoryURL + "?AttrFilter=" + escape(selectedAttributeList);
		}, 1000);
	}


	function closeAttributeBox() {
	
		//clear any previously selected values
		selectedAttributeNames = new Array();
		selectedAttributeValues = new Array();
		selectedCategoryId = 0;
		selectedCategoryURL = "";

		//clear blocks
		$("#categoryAttributeHeading").html("");
		$("#categoryAttributes").html("");

		//hide it
		$('#categoryAttributeBox').hide();
	
	}


</script>

