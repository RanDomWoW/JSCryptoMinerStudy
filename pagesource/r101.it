<script>
	$('.b_sn .sep').socialLikes();

	var today = new Date("2018/03/18 00:54:46");
	var weekDay = (today.getDay() == 0) ? 6 : today.getDay() - 1;
	var timeDay = ((today.getHours() < 10 ? '0' : '') + today.getHours()) + ':' + ((today.getMinutes() < 10 ? '0' : '') + today.getMinutes());
	var palinsesto = '';

	function getOnAir() {
		try {
			if(palinsesto == '') {
				$.getJSON('/palinsesto/palinsesto.json', function(result) {
					if(result) {
						palinsesto = result;
						drawOnAir();
					}
				});
			}
			drawOnAir();
		} catch(e) {}
	}

	function updateOnAir() {
		try {
			today = new Date();
			weekDay = (today.getDay() == 0) ? 6 :  today.getDay() - 1;
			timeDay = ((today.getHours() < 10 ? '0' : '') +  today.getHours()) + ':' + ((today.getMinutes() < 10 ? '0' : '') +  today.getMinutes());

			if(palinsesto == '') {
				$.getJSON('/palinsesto/palinsesto.json', function(result) {
					if(result) {
						palinsesto = result;
						drawOnAir();
					}
				});
			} else {
				drawOnAir();
			}
		} catch(e) {}
	}

	function drawOnAir() {
		try {
			if(palinsesto != '') {
				$.each(palinsesto.data.giorni[weekDay].programma, function(i, prg) {
					try {
						var _time = prg.orario.split('-');
						var start = _time[0].trim();
						var end = (_time[1].trim() == '00:00') ? '24:00' :_time[1].trim();

						if(timeDay >= start && timeDay <= end) {
							$('.onAirHead img').attr('src', prg.path_img);
							$('.onAirHead .time').html(timeDay);
							$('.onAirHead .speakerH').html(prg.conduttori);
							false;
						}
					} catch(e) {}
				});
			}

			$('.onAirHead').show();
		} catch(e) {}
	}

	function drawNewsTGcom24() {
		$('.newsTGcom24').hide();

		try {
			$.getJSON('http://www.tgcom24.mediaset.it/flussi/ultimaora.sjson?callback=?', function(data) {
				if(data) {
					$('.newsTGcom24 .timeNews .time').html(' '+data.ultimaora.ora+' ');
					$('.newsTGcom24 .titleNews .tit a').html(data.ultimaora.titolo);
					$('.newsTGcom24').show();
				}
			});
		} catch(e) {}
	}

	$(function() {
		$('.onAirHead').hide();
		getOnAir();
		setInterval('updateOnAir()', 30000);
		drawNewsTGcom24();
		setInterval('drawNewsTGcom24()', 30000);

		$('.wrSidebarCarousel').slick({
			dots: true,
			slidesToShow: 1,
			slidesToScroll: 1
		});
	});
</script>

