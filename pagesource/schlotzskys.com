            </div>
            <div class="location-results-text">
                <a href="{{URL}}" class="loc-name">{{name}}</a>
                <span class="loc-hours">Open Until {{openUntil}}</span>
                <span class="loc-address">{{streetAddress}} {{locality}}, {{region}} {{postalCode}}</span>
                <span class="loc-phone">{{tel}}</span>
            </div>
            {{onlineOrdering}}
            {{onlineCatering}}
        </div>
        <div class="row">
            <div class="strike">
                <div class="icon-dist" style="opacity: 0; height: 25px;">
