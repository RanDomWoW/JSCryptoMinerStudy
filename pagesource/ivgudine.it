    <script type="text/javascript">
        //<![CDATA[
        var tendinaAperta = false;
        var sullaTenda = false;
        function tendinaVendite(){
            if( !tendinaAperta && !sullaTenda ){
                $("#vendite").removeClass( 'hover-menu' );
                $("#tendinavendite").hide();
                return;
            } 
            setTimeout( tendinaVendite, 1000 );
        }


        function numberFormat(number, decimals, dec_point, thousands_sep)
        {
            // http://kevin.vanzonneveld.net/techblog/article/javascript_equivalent_for_phps_number_format/
            number = (number + '').replace(/[^0-9+\-Ee.]/g, '');
            var n = !isFinite(+number) ? 0 : +number,
                prec = !isFinite(+decimals) ? 0 : Math.abs(decimals),
                sep = (typeof thousands_sep === 'undefined') ? ',' : thousands_sep,
                dec = (typeof dec_point === 'undefined') ? '.' : dec_point,
                s = '',
                toFixedFix = function (n, prec)
                {
                    var k = Math.pow(10, prec);
                    return '' + Math.round(n * k) / k;
                };
            // Fix for IE parseFloat(0.55).toFixed(0) = 0;
            s = (prec ? toFixedFix(n, prec) : '' + Math.round(n)).split('.');
            if (s[0].length > 3)
            {
                s[0] = s[0].replace(/\B(?=(?:\d{3})+(?!\d))/g, sep);
            }
            if ((s[1] || '').length < prec)
            {
                s[1] = s[1] || '';
                s[1] += new Array(prec - s[1].length + 1).join('0');
            }
            return s.join(dec);
        }


        jQuery(document).ready(function(){
            $( '#vendite' ).bind( "mouseover", 
                function() {
                    var offset = $("#vendite").offset();
                    $("#tendinavendite").css( { "top": offset.top + $("#vendite").height(), "left": offset.left } ).show();
                    $("#vendite").addClass( 'hover-menu' );
                    setTimeout( tendinaVendite, 1000 );
                    tendinaAperta = true;
                } );
            $( '#tendinavendite' ).bind( "mouseout", 
                function() {
                    tendinaAperta = false;
                    sullaTenda = false;
                } ).bind("mouseover", function(){
                    sullaTenda = true;
                    tendinaAperta = true;
                } );
            $( '#vendite' ).bind( "mouseout", 
                function() {
                    tendinaAperta = false;
                } );
        })
        //]]>
    </script>

