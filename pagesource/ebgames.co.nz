<script type="text/javascript">
        $(document).ready(function () {

            $(".ellipsis").ellipsis();

            $(".product-list-link").ebhintProduct();
            $(".product-thumbnail").clearEbhints();

            $(".hint").each(function () {
                var data = $(this).data();
                data.popupText = $(this).html();
                $(this).text(data.shortDisplay).ebhint();
            });

            $(".prd-stock-status a").on("click", function () {
                var container = $(this).parents(".prd-stock-status");
                container.toggleClass("expanded");

                if (container.hasClass("expanded")) {
                    var openHeight = (container.find(".delivery-option").length > 1) ? 63 : 32;
                    container.children(".drop-down").css("height", openHeight);
                } else {
                    container.children(".drop-down").css("height", 0);
                }
            });

            var heroOptions = {
                $SlideDuration: 1000,
                $HWA: false,
                $AutoPlay: true,
                $SlideWidth: 924,
                $SlideHeight: 345,
                $AutoPlayInterval: 5000,
                $ArrowNavigatorOptions: {
                    $Class: $JssorArrowNavigator$,
                    $ChanceToShow: 2
                },
                $BulletNavigatorOptions: {
                    $Class: $JssorBulletNavigator$,
                    $AutoCenter: 1,
                    $ChanceToShow: 2,
                    $SpacingX: 15,
                    $SpacingY: 15
                }
            };

            if ($('#heroCarousel').length > 0) {
                var heroCarousel = new $JssorSlider$("heroCarousel", heroOptions);
            }

            $('.productCarousel').each(function (index, element) {
                var chanceToShow = 2;

                if ($(element).find('.collection-page-product').length <= 5) {
                    chanceToShow = 0;
                }

                var heights = $('.collection-page-product', element).map(function () {
                    return $(this).outerHeight(true);
                }).get();

                var maxHeight = Math.max.apply(null, heights);

                $(element).height(maxHeight);
                $('.slides', element).height(maxHeight);

                var options = {
                    $ArrowKeyNavigation: true,
                    $Loop: true,
                    $HWA: false,
                    $SlideDuration: 150,
                    $MinDragOffsetToSlide: 20,
                    $SlideWidth: 165,
                    $SlideHeight: maxHeight,
                    $SlideSpacing: 9,
                    $DisplayPieces: 5,
                    $ArrowNavigatorOptions: {
                        $Class: $JssorArrowNavigator$,
                        $ChanceToShow: chanceToShow,
                        $Steps: 5,
                        $AutoCenter: 2
                    }
                };

                var slider = new $JssorSlider$(element.id, options);
                slider.$On($JssorSlider$.$EVT_PARK, function (slideIndex, fromIndex) {
                    var position = slideIndex++;

                    var page = Math.floor(position / 5) + 1;

                    var slides = slider.$SlidesCount();

                    if (slideIndex == fromIndex && slideIndex > 0)
                        slider.$PlayTo(0);

                    if (position + 5 >= slides) {
                        page = Math.ceil(slides / 5);
                    }

                    $(element).closest('.carousel-panel').find('.carousel-pager-current').text(page);
                });

                $(element).find('.packshot a').hoverHint("Click for more info");

                $(element).show();
            });
        });
    </script>
