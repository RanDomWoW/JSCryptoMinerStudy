<script language="javascript">


$(function(){
	$("area").each(function(){
			var $x=-100;
			var $y=-155;	
			var name=$(this).attr("title1");
			$(this).mouseover(function(e){
				var index_num=$(this).index();
				var dom="<div class='mapDiv'><p><span class='name'></span></p></div>";
				$("body").append(dom);
				$(".name").html(name);
				$(".num").text(index_num)
				$(".mapDiv").css({
						top: (e.pageY + $y)+"px",
						left: (e.pageX + $x)+"px"
					}).show("fast");
			}).mouseout(function(){
					$(".mapDiv").remove();
			}).mousemove(function(e,con){
					$(".mapDiv").css({
						top: (e.pageY + $y)+"px",
						left: (e.pageX + $x)+"px"
				});
			});
		});		   
});
</script>
