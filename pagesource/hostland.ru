    if($.cookie('ip_city') == undefined) {

        function returnIp(callback) {
            $.ajax({
                type: 'POST',
                url: '/ajax/get_ip.php',
                success: function(data) {
                    var ip_city = data;

                    if(callback && typeof(callback) === "function") {
                        callback(ip_city);
                    }
                }
            });
        }

        function callbackFun(city) {

            var city_arr = {'Saint Petersburg City': 'Санкт-Петербург', 'Saint Petersburg': 'Санкт-Петербург', 'Moscow City': 'Москва', 'Moscow': 'Москва', 'Moskva': 'Москва', 'Novosibirsk': 'Новосибирск', 'Samara': 'Самара', 'Voronezh': 'Воронеж', 'Kaliningrad': 'Калининград', 'Rostov': 'Ростов-на-Дону', 'Sverdlovsk': 'Екатеринбург', 'Tatarstan': 'Казань', 'Nizhegorod': 'Нижний Новгород', 'Volgograd': 'Волгоград', 'Omsk': 'Омск', 'Perm\'': 'Пермь','Krasnoyarsk': 'Красноярск','Chelyabinsk': 'Челябинск',};

            var city = city_arr[city];
            var city_tel;

            if (city == undefined) {
                city = 'Москва';
                city_tel = '+7 (495) 134-38-34';
            }
            
            var city_select = $(".chosen-select");
            var select_arr = city_select[0];

            $.each(select_arr, function(i, val) {
                if(city == val.label) {
                    city = val.label;
                    city_tel = val.dataset.phone;
                    return false;
                }
            });

            $(".chosen-select").val(city);
            $(".chosen-single span").html(city);
            $(".city_inp").val(city);
            $('.phone').text(city_tel);
            $.cookie('ip_city', null, {path: '/', expires: -1});
            $.cookie('ip_city', city, {path: '/',expires: 365});

            $(function() {
                $(".chosen-select").chosen({
                    no_results_text: "Город не найден",
                    disable_search: true

                }).change( function () {
                    var town = $(this).val();
                    // console.log(town);
                    var phoneTown = $(this).find('option:selected').data('phone');
                    // console.log(phoneTown);

                    $('.phone').text(phoneTown);
                    $.cookie('ip_city', null, {path: '/', expires: -1});
                    $.cookie('ip_city', town, {path: '/', expires: 365});
                    $(".city_inp").val(town);
                });
            });
        }

        returnIp(callbackFun);
    } else {
        var city_select = $(".chosen-select");
        var city_arr = city_select[0];
        var city_val;
        var city_tel;

        $.each(city_arr, function(i, val) {
            if($.cookie('ip_city') == val.label) {
                city_val = val.label;
                city_tel = val.dataset.phone;
            }
        });

        $(".chosen-select").val(city_val);
        $(".chosen-single span").html(city_val);
        $(".city_inp").val(city_val);
        $('.phone').text(city_tel);

        // Вывод телефонов и города 
