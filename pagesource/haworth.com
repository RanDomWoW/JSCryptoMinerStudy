<script src="https://cdnjs.cloudflare.com/ajax/libs/foggy/1.1.1/jquery.foggy.min.js"></script>
<script>
    function displayMailingList() {
        $('#theSite').foggy();
        $('#optInContainer').show();
    }

    var thirtyDays = new Date();
    thirtyDays.setDate(thirtyDays.getDate() + 90);

    $(document).ready(function () {
        var cookieVal = readCookie("optin");
        if (cookieVal == "false" || cookieVal == "" || cookieVal == null) {
            var expireVal = readCookie("optinExpiration");
            if (expireVal != null) {
                var expDate = Date.parse(expireVal);
                var now = Date.parse(new Date());
                if (expDate < now) {
                    displayOptIn();
                }
            }
            else {
                displayOptIn();
            }
        }
        $('#submitMailing').click(function (e) {
            if ($('#optInEmail').val() != '') {
                e.preventDefault();
                var optEmail = $('#optInEmail').val();
                if (validateEmail(optEmail)) {
                    submitEmail(optEmail, true);
                }
                else
                {
                    alert("Invalid Email. Please try again.");
                }
                return false;
            }
        });

        $('#optInFooterSubmit').click(function (e) {
            if ($('#optInFooterEmail').val() != '') {
                e.preventDefault();
                var optFootEmail = $('#optInFooterEmail').val();
                if (validateEmail(optFootEmail)) {
                    submitEmail(optFootEmail, false);
                }
                else {
                    alert("Invalid Email. Please try again.");
                }
                return false;
            }
        });
        function validateEmail(email) {
            var re = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;//Note the Two at symbols the second one is required to escape the razor engine
            return re.test(email);
        }
    });

    function submitEmail(oEmail, popup) {
        $.ajax({
            url: '/json/optin/subscribe/' + oEmail,
            type: 'POST',
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                if (data == "Exists") {
                    alert('The email address you entered has already been registered. Please try again.');
                    return false;
                }
                else {
                    document.cookie = "optin=true; path=/; expires=Tue, 1 Dec 2099 12:00:00 UTC";
                    if(popup) {
                        $('#optInContent').html("<h4>Thank You for registering!</h4><p>We look forward to sharing and connecting with you.</p>");
                    }
                    else {
                        alert("Thank You for registering!  We look forward to sharing and connecting with you.");
                    }
                }
            },
            error: function () {
                alert("Error");
            }
        });
    }

    function displayOptIn() {
        var optIn = readCookie("optinVisit")
        if (optIn == null) {
            document.cookie = "optinVisit=optin; path=/;";
        }
        else {
            document.cookie = "optinVisit=; path=/; expires=Thu, 01 Jan 1970 00:00:00 UTC";
            setTimeout(displayMailingList, 1000);
            $('#closeOptIn').click(function () {
                document.cookie = "optin=false; path=/; expires=" + thirtyDays.toUTCString();
                document.cookie = "optinExpiration=" + thirtyDays.toUTCString() + "; path=/; expires=" + thirtyDays.toUTCString();
                $('#optInContainer').hide();
            });
        }
    }

    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1, c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
        }
        return null;
    }
</script>
