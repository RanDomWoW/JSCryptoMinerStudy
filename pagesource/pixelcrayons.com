success: function(data)   // A function to be called if request succeeds
{
    var obj = jQuery.parseJSON(data);

    $('#loading').hide();
    //alert(obj.filename);
    if(obj.msg==1)
        {
            $("#message").html("<span style='color:green;' id='success'>File Uploaded Successfully...!!</span>");
            $('#hidFile').val(obj.filename);
        }
    else if(obj.msg==2)
        {
            $("#message").html("<span style='color:red;' id='success'>File already exists...!!</span>");
            $('#hidFile').val(obj.filename);
        }
    else if(obj.msg==3)
        {
            $("#message").html("<span style='color:red;' id='invalid'>Invalid file Size or Type<span>");
        }
    else
        {
            $("#message").html(obj.filename);
        }
},
complete: function()
{   //alert('call');
