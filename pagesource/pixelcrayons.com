<script>
                        function fileName() {
                            var file = document.getElementById("ffile").files;
                            file = file[0].name.slice(0, 11);
                            document.getElementById("upFile").innerHTML = file + '...';
                        }
                        var reqFooter = angular.module('frmReqFooter', [])
                        reqFooter.controller("ctrlReqFooter", function($scope, $http) {
                            $scope.value = "Request a Quote";
                            $scope.fprovide_url = $scope.fprovide_file = false;
                            $scope.footer = {"type": "footer"};


                            $scope.footersubmit = function() {
                                var companyNameDiv = angular.element(document.querySelector('#cnamediv'));
                                var companyName = angular.element(document.querySelector('#cname'));
                                var emailDiv = angular.element(document.querySelector('#femaildiv'));
                                var email = angular.element(document.querySelector('#femail')).val();
                                var fphoneDiv = angular.element(document.querySelector('#fphonediv'));
                                var fphone = angular.element(document.querySelector('#fphone')).val();
                                var freqdiv = angular.element(document.querySelector('#freqdiv'));
                                var freq = angular.element(document.querySelector('#freq'));
                                var hidFilef = angular.element(document.querySelector('#hidFilef'));
                                $scope.footer.hidFilef = hidFilef.val();
                                var error = false;
                              //  alert(companyName.val() == '');
                                if (companyName.val() == '') {
                                    companyNameDiv.addClass('active');
                                    error = true ;
                                } else {
                                    companyNameDiv.removeClass('active');
                                }                                
                                if (email == '')
                                {
                                    emailDiv.addClass('active');
                                    error = true;
                                }
                                else
                                {
                                    var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,17})+$/;
                                    if (!email.match(mailformat))
                                    {
                                        emailDiv.addClass('active');
                                        error = true;
                                    }
                                    else {
                                        emailDiv.removeClass('active');
                                    }
                                }
                                if (fphone == '')
                                {
                                    fphoneDiv.addClass('active');
                                    error = true;
                                }
                                else
                                {
                                    var phoneformat = /^(?=(.*\d){8,})[\d\(\)\s+-]{8,}$/;
//                                    var phoneformat = /^[\d().+]+$/;
                                    if (!fphone.match(phoneformat)) {
                                        fphoneDiv.addClass('active');
                                        error = true;
                                    }
                                    else {
                                        fphoneDiv.removeClass('active');
                                    }

                                }

                                if (freq.val() == '') {
                                    freqdiv.addClass('active');
                                    error = true;
                                } else {
                                    freqdiv.removeClass('active');
                                }
                                if (!error) {
                                    $scope.myButton = true;
                                    var file = angular.element(document.querySelector('#ffile')).prop("files")[0]; // Getting the properties of file from file field
                                    $scope.files = [];
                                    $scope.files.push(file);
                                    $scope.value = "processing...";
                                    $http({
                                        method: 'POST',
                                        url: SITE_ROOT_URL + 'ajax_act.php',
                                        headers: {'Content-Type': undefined}, // set the headers so angular passing info as form data (not request payload)
                                        transformRequest: function(data) {
                                            var formData = new FormData();
                                            formData.append('model', angular.toJson(data.model));
                                            formData.append('file', data.files[0]);
                                            return formData;
                                        },
                                        data: {model: $scope.footer, files: $scope.files}

                                    }).success(function(data) {
                                        window.location.href = SITE_ROOT_URL + 'thanks';
                                        $scope.myButton = false;

                                    });
                                }
                            };

                        });
                        angular.bootstrap(document.getElementById("footerForm"), ['frmReqFooter']);


