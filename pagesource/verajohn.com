<script type="text/javascript">
(function(){
	// jQuery ready
	$(desktopDreamWeeksAnalytics);

	function createFunctionWithTimeout(callback, opt_timeout) {
		var called = false;
		function fn() {
			if (!called) {
				called = true;
				callback();
			}
		}
		setTimeout(fn, opt_timeout || 2000);
		return fn;
	}

	function desktopDreamWeeksAnalytics() {
		if (typeof window.ga !== 'function') return;

		var eventCat = 'Dream weeks Fall 2016';
		var eventSent = false;
		var collapsibleTitles = ['Week 1', 'Week 2', 'Week 3', 'Week 4', 'Terms & Conditions'];

		$('.ticket-counter').closest('.static').find('.collapsible').each(function(ix, el) {
			var collapsibleTitle = collapsibleTitles[ix];
			var eventSent = false;
			var $depositBtn = $(this).find('.cta-deposit .button');

			// Collapsibles
			var $collapsibleTitle = $(this).find('.collapsible-title a');

			if ($collapsibleTitle.length) {
				$collapsibleTitle.on('click', function() {
					ga('send', {
						hitType: 'event',
						eventCategory: eventCat,
						eventAction: 'Click collapsible',
						eventLabel: collapsibleTitle
					});
				});

				var prioritizedEvent = $._data($collapsibleTitle[0]).events.click.pop();
				$._data($collapsibleTitle[0]).events.click.unshift(prioritizedEvent);
			}

			// Deposit
			if ($depositBtn.length) {
				$depositBtn.on('click', function(evt) {
					if (eventSent) return;
					evt.stopImmediatePropagation();

					ga('send', {
						hitType: 'event',
						eventCategory: eventCat,
						eventAction: 'Click deposit',
						eventLabel: collapsibleTitle,
						hitCallback: createFunctionWithTimeout(function(){
							eventSent = true;
							$depositBtn.trigger('click');
						})
					});
				});

				var prioritizedEvent = $._data($depositBtn[0]).events.click.pop();
				$._data($depositBtn[0]).events.click.unshift(prioritizedEvent);
			}
		});

		// News page
		var $newsPageBtn = $('#news .news-item[data-newsid="975"]:not(.detailed) .actions .button');

		if ($newsPageBtn.length) {
			$newsPageBtn.on('click', function(evt) {
				if (eventSent) return;
				evt.stopImmediatePropagation();

				ga('send', {
					hitType: 'event',
					eventCategory: eventCat,
					eventAction: 'Click news article',
					eventLabel: 'Read more button on news page',
					hitCallback: createFunctionWithTimeout(function(){
						eventSent = true;
						$newsPageBtn.trigger('click');
					})
				});
			});

			var prioritizedEvent = $._data($newsPageBtn[0]).events.click.pop();
			$._data($newsPageBtn[0]).events.click.unshift(prioritizedEvent);
		}

		// News article
		var $newsArticleBtn = $('#news .news-item[data-newsid="975"].detailed .actions .button');

		if ($newsArticleBtn.length) {
			$newsArticleBtn.on('click', function(evt) {
				if (eventSent) return;
				evt.stopImmediatePropagation();

				ga('send', {
					hitType: 'event',
					eventCategory: eventCat,
					eventAction: 'Click news article',
					eventLabel: 'Read more button in news article',
					hitCallback: createFunctionWithTimeout(function(){
						eventSent = true;
						$newsArticleBtn.trigger('click');
					})
				});
			});

			var prioritizedEvent = $._data($newsArticleBtn[0]).events.click.pop();
			$._data($newsArticleBtn[0]).events.click.unshift(prioritizedEvent);
		}

		// Raffle tickets
		var $raffleTickets = $('.balance a[href*="dream-weeks"]');

		if ($raffleTickets.length) {
			$raffleTickets.on('click', function(evt) {
				if (eventSent) return;
				evt.stopImmediatePropagation();

				ga('send', {
					hitType: 'event',
					eventCategory: eventCat,
					eventAction: 'Click balance',
					eventLabel: 'Raffle tickets',
					hitCallback: createFunctionWithTimeout(function(){
						eventSent = true;
						$raffleTickets.trigger('click');
					})
				});

				var prioritizedEvent = $._data($raffleTickets[0]).events.click.pop();
				$._data($raffleTickets[0]).events.click.unshift(prioritizedEvent);
			});
		}
	}
})();
