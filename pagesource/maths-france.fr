		<script type="text/javascript">

		function animate_one(e) {
			e.css('position','relative')
			e.css('top',Math.random()*100);
			e.css('left',Math.random()*300);
			e.fadeIn(500).fadeOut(500);
		}	
 
		$(document).ready(function () {
			var spans=$('#animation span');
			
			$.timer(1200, function (timer) {
				animate_one(spans.eq( Math.random()*spans.length));
				$.timer(100, function(timer) {
					timer.stop();
				}
				)
				animate_one(spans.eq( Math.random()*spans.length));
				animate_one(spans.eq( Math.random()*spans.length));
				animate_one(spans.eq( Math.random()*spans.length));
				animate_one(spans.eq( Math.random()*spans.length));
				animate_one(spans.eq( Math.random()*spans.length));
			});
			

		});

		</script>





