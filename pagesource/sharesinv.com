<script>
	$('.earnings_btn').on('click', function(){
		console.log($(this).text());
		if($(this).text() == 'Show more')
		{
			$('.earnings_hidden').show();
			$('.earnings_btn small').text('Show less');
		}
		else
		{
			$('.earnings_hidden').hide();
			$('.earnings_btn small').text('Show more');
		}
	});

	function login_form_action(){
		var username = $('#txtUsername').val();
		var password = $('#txtPassword').val();

		$.post("http://www.sharesinv.com/wp-content/themes/default/header/login_validate.php", { username: username, password: password },
			function(data){
				if(data == ""){
					alert("Invalid username and password.");
				}
				else{
					if ($('#rmb').attr('checked'))
					{
						$.cookie("login_username", username);
						$.cookie("login_password", password);
					} else {
						$.cookie("login_username", null);
						$.cookie("login_password", null);
					}

					if(data == "watchlist")//go to watchlist
						window.location="http://www.sharesinv.com/watchlist/";
					else
						window.location.reload(true);
				}
			});
	}

	function forgot_form_action(){
		var forgot_username 	= $('#forgot_username').val();
		var forgot_email		= $('#forgot_email').val();

		$.post("http://www.sharesinv.com/wp-content/themes/default/header/forget_validate.php", { forgot_username: forgot_username, forgot_email: forgot_email },
			function(data){

				if(data == "")
					alert("Invalid username / e-mail combination.");
				else
					alert("Your new password has been mailed to you. Please check your e-mail ("+ forgot_email +").");

			});
	}

	function selectItem(li) {
		if (li.extra)
			window.location="http://www.sharesinv.com/"+li.extra[1]+"/";
	}

	function formatItem(row) {
		return "<a href='?PHPSESSID=28270f9343331536c40a75878b219ed2'>["+row[1]+"]&nbsp;&nbsp;"+row[0]+"</a>";
	}

	$(document).ready(function() {

