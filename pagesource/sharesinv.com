		$("#s").autocomplete("http://www.sharesinv.com/wp-content/themes/default/ajax_search.php", { minChars:1, width:180, matchSubset:false, matchContains:false, cacheLength:10, onItemSelect:selectItem, formatItem:formatItem, selectOnly:1 });

		$("#verification").animate( { backgroundColor: '#92FFA5',color:'#009218', border: '1px solid #009218' }, 1000).animate( { color:'#009218',backgroundColor:  '#92FFA5',border: '1px solid #009218' }, 1000);

		$('#forgot_link').click(function() {
			$("#login_form").hide();
			$("#forgot_form").show();
