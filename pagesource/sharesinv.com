<script>
	$('.load-more-latest').on('click', function(){
		var offset = $('.ar_title').length;

		$.ajax({
			url: "http://www.sharesinv.com/get_aspire_article.php?offset=" + offset,
			dataType: "json"
		}).done(function(response) {
			if(response.length == 0) {
				$('.load-more-latest').css('display', 'none');
            } else {
				$.each(response, function(key, value) {
					var output = '<ul id="single-ar" ' + value['image_style'] + '>' +
						value['print_image'] +
						'<a class="ar_title" href="' + value['url'] + '?PHPSESSID=28270f9343331536c40a75878b219ed2" /?utm_source=si_home&utm_medium=si_desktop" rel="bookmark" title="Read full article">' + value['title'] + '</a>' +
						'<div style="clear:both"></div>' +
						'</ul>';

					$('.ar_title').eq(offset - 1).closest('ul').after(output);
					offset++;
				});
            }
		});
	});
</script>
