					<script>
						function trim(str, chars) {
							return ltrim(rtrim(str, chars), chars);
						}
						 
						function ltrim(str, chars) {
							chars = chars || "\\s";
							return str.replace(new RegExp("^[" + chars + "]+", "g"), "");
						}
						 
						function rtrim(str, chars) {
							chars = chars || "\\s";
							return str.replace(new RegExp("[" + chars + "]+$", "g"), "");
						}

						function check_text(f, msg){
							if(trim(f.value)==""){
								alert(msg);
								f.value="";
								f.focus();
								return false;
							}
							else{
								return true;
							}
						} 

						function checkLoginForm(obj){
							if(!check_text(obj.UserLogin, "請輸入登入名稱")) return false;
							if(!check_text(obj.UserPassword, "請輸入密碼")) return false;
						}

						function loginNow(obj){
							if(!check_text(obj.UserLogin, "請輸入登入名稱")) return false;
							if(!check_text(obj.UserPassword, "請輸入密碼")) return false;
							obj.submit();
						}
							
						function checkForgetForm(){
							obj = document.forgetForm;
							tmp = prompt("請輸入登入名稱或密碼", "");
							if(tmp!=null && trim(tmp)!=""){
								obj.UserLogin.value = tmp;
								obj.submit();
							}
						} 
					</script>
