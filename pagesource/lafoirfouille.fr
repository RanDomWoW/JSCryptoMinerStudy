		function sendGA(id) {
			posCode=$(".selected .posCode").val();
			posName=$(".selected #ppName strong").html();
			pos = posName + '_' + posCode ;
			
			if ( id!=null )
			{
				var page='HOMEPAGE';
				if ( page=='' )
					page='OTHERPAGE';
				if(id == 'header-contact') { 
					_gaq.push(['_trackEvent', page,  'header', 'contact']);
				}
				if(id == 'footer-contact') { 
					_gaq.push(['_trackEvent', page, 'footer', 'contact', 1]);
				}
				if(id == 'mainimage') { 
					_gaq.push(['_trackEvent', page, 'core', 'mainimage', 1]);
				}
				 if(id.startsWith('coupdecoeur-')) { 
					_gaq.push(['_trackEvent', page, 'coupdecoeur', 'product-'+id.replace('coupdecoeur-',''), 1]);
				 }	
				 if(id.startsWith('selection-')) { 
					_gaq.push(['_trackEvent', page, 'selection', 'product-'+id.replace('selection-',''), 1]);
				 }			  
				 if(id.startsWith('bestsale-')) { 
					_gaq.push(['_trackEvent', page, 'bestsale', 'product-'+id.replace('bestsale-',''), 1]);
				 }			  
				 if(id == 'contact-submit') { 
				_gaq.push(['_trackEvent', page, 'form', 'submit', 1]);
				}	
				 if(id == 'choix') { 
						_gaq.push(['_trackEvent','magasin','choix_magasin', pos, 1]); }
			}
		}
		
	</script>
