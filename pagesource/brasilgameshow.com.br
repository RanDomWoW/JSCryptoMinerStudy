<script>
    
  $(document).ready(function()
  {

    var today = new Date();

    var startDay = new Date('October 10, 2018');
    var msPerDay = 24 * 60 * 60 * 1000 ;
    var timeLeft = (startDay.getTime() - today.getTime());
    var e_daysLeft = timeLeft / msPerDay;
    var daysLeft = Math.ceil(e_daysLeft);

    var msg_numdias = '<b>' + daysLeft + '</b> dia';
    var msg_legenda = "Falta";

    if (daysLeft <= 0)
    {
      $('#dias').html('');
    }
    else if(daysLeft > 1)
    {
      msg_numdias += 's';
      msg_legenda += 'm';
    }

    msg_numdias += '';

    $('.leg_dias').html(msg_legenda);
    $('.num_dias').html(msg_numdias);

    //$('#Header #myCarousel').carousel
    $('#myCarousel').on('slid.bs.carousel', function () {
       $holder = $( "ol li.active" );
      $holder.removeClass('active');
      var idx = $('div.active').index('div.item');
//      console.log(idx);
      $('ol.carousel-indicators li[data-slide-to="'+ idx+'"]').addClass('active');
    });

    $('.carousel-indicators ol li').on("click",function(){
        $('.carousel-indicators ol li.active').removeClass("active");
        $(this).addClass("active");
    });

  });

</script>

