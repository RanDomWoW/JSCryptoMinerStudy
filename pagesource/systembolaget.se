    <div class="modal" id="modal-add-to-beverage-list" data-ng-controller="AddToBeverageListModalController as addToList" data-sb-modal>


    
    <div data-ng-show="!addToList.state.createNewBeverageListActive && !addToList.state.listCreatedActive && !addToList.state.listCreatedFailed && !addToList.state.addedToExistingList && !addToList.state.addedToExistingListFailed">

        <div class="title">
            L&#228;gg till i lista
        </div>

        <div class="">
            <div class="cmp-ajax-loader cmp-ajax-loader-inherit" data-ng-show="addToList.state.isLoadingLists"></div>
            <div class="text-content">
                <div class="description" data-ng-show="addToList.data.Product">
                    L&#228;gg till&#160; <strong>{{addToList.data.Product.ProductNameBold}}</strong> <span class="text-muted">{{addToList.data.Product.ProductNameThin}}</span>
                </div>
                <div class="description" data-ng-show="addToList.data.Products">
                    H&#228;r kan du l&#228;gga till&#160;produkterna i en lista
                </div>
            </div>
        </div>
        <div class="cmp-beveragelist-modal-list" data-ng-show="addToList.state.isPageReady">
            <ul>
                <li data-ng-repeat="bvrgList in addToList.serviceData.lists | orderBy:'Name' track by $index">
                    <button class="cmp-beveragelist-add" data-ng-click="addToList.addToList(bvrgList)">
                        <strong aria-hidden="true">{{bvrgList.Name}}</strong>
                        <span aria-hidden="true" class="text-muted pull-right">{{bvrgList.NumberOfItems || 0}}</span>
                        <span class="cmp-screen-reader-text"> {{bvrgList.Name}}</span>
                    </button>
                    <button class="cmp-beveragelist-modal-to-list icon-only-button pull-right" data-sb-button-link="{{bvrgList.BeverageListUrl}}">
                        <i class="icon icon-right"></i>
                        <span class="cmp-screen-reader-text"> {{bvrgList.Name}}</span>
                        <span aria-hidden="true"> {{bvrgList.Name}}</span>
                    </button>
                </li>
                <li class="message-empty small" data-ng-show="!addToList.state.isLoadingLists && addToList.serviceData.lists.length < 1">
                    Du har inga listor &#228;n
                </li>
            </ul>
        </div>
        <div class="actions">
            <button class="action primary" data-ng-click="addToList.close()">St&#228;ng</button>
            <button class="action primary" data-ng-click="addToList.toggleCreateNewList()">Skapa ny lista</button>
        </div>
    </div>

    
    <div data-ng-show="addToList.state.addedToExistingList">

        <div class="title">
            Tillagd
        </div>

        <div class="">
            <div class="cmp-ajax-loader cmp-ajax-loader-inherit" data-ng-show="addToList.state.isLoadingLists"></div>
            <div class="text-content">
                <div class="description" data-ng-show="addToList.data.Product">
                    Du har nu lagt till produkten <strong>{{addToList.data.Product.ProductNameBold}}</strong> <span class="text-muted">{{addToList.data.Product.ProductNameThin}}</span>
                </div>
                <div class="description" data-ng-show="addToList.data.Products">
                    Du har nu lagt till produkterna
                </div>
            </div>
        </div>
        <button class="action primary" data-ng-click="addToList.close()">Klar</button>
    </div>

    
    <div data-ng-show="addToList.state.addedToExistingListFailed">

        <div class="title">
            L&#228;gg till i lista
        </div>

        <div class="">
            <div class="cmp-ajax-loader cmp-ajax-loader-inherit" data-ng-show="addToList.state.isLoadingLists"></div>
            <div class="text-content">
                Det gick inte att l&#228;gga till i listan, troligtvis beror detta p&#229; att den &#228;r full
            </div>
        </div>
        <button class="action primary" data-ng-click="addToList.close()">St&#228;ng</button>
    </div>

    
    <div data-ng-show="addToList.state.createNewBeverageListActive">
        <form name="CreateBeverageListForm" novalidate data-ng-submit="addToList.createList(CreateBeverageListForm.$valid)">
            <div class="title">
                Skapa ny lista
            </div>
            <div class="text-content">
                <div class="description">
                    Skapa ny lista och l&#228;gg till&#160; <strong>{{addToList.data.Product.ProductNameBold}}</strong> <span class="text-muted">{{addToList.data.Product.ProductNameThin}}</span>
                </div>
                <fieldset>
                    <legend class="a11y">Skapa ny lista</legend>
                    <ul class="cmp-label-input-list cmp-label-input-list-modal" data-ng-class="{'show-validation-messages' : addToList.showValidationMessages}">
                        <li>
                            <label for="ListName">Namn</label>
                            <span class="input-holder">
                                <input type="text" id="ListName" spellcheck="false" maxlength="50" required data-ng-model="addToList.ListNameInput" />
                            </span>
                        </li>
                    </ul>
                </fieldset>
            </div>
            <div class="actions">
                <button class="action primary" data-ng-click="addToList.toggleCreateNewList()">Tillbaka</button>
                <button class="action primary" type="submit">Skapa</button>
            </div>
        </form>
    </div>


    
    <div data-ng-show="addToList.state.listCreatedActive">
        <div class="title">
            Ny lista skapad
        </div>
        <div class="text-content">
            <div class="description">
                Du har nu skapat listan och produkten har lagts till
            </div>
        </div>
        <div class="cmp-beveragelist-modal-list" data-ng-show="addToList.state.isPageReady">

            <ul>
                <li data-ng-repeat="bvrgList in addToList.serviceData.lists  | orderBy:'Name' track by $index">
                    <button class="cmp-beveragelist-add" disabled="disabled"><strong>{{bvrgList.Name}}</strong><span class="text-muted pull-right">{{bvrgList.NumberOfItems || 0}}</span></button>
                    <button class="cmp-beveragelist-modal-to-list icon-only-button pull-right" data-sb-button-link="{{bvrgList.BeverageListUrl}}"><i class="icon icon-right"></i>Gå till lista {{bvrgList.Name}}</button>
                </li>
            </ul>
        </div>
        <button class="action primary" data-ng-click="addToList.close()">Klar</button>
    </div>

    
    <div data-ng-show="addToList.state.listCreatedFailed">
        <div class="title">
            L&#228;gg till i lista
        </div>
        <div class="text-content">
            <div class="description">
                Det gick inte att skapa listan
            </div>
        </div>
        <button class="action primary" data-ng-click="addToList.close()">St&#228;ng</button>
    </div>
</div>


