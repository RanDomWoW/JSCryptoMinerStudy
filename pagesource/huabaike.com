    var st = $(document).scrollTop();
    if( st > 600 ){
      var cw=document.documentElement.clientWidth;
      if(cw>1268){
        h=(cw/2)+620;
      }else{
        h=cw-70;
      }
      $('#go-top').css({'left':h});
      $('#go-top').fadeIn(function(){
        $(this).removeClass('dn');
      });
    }else{
      $('#go-top').fadeOut(function(){
        $(this).addClass('dn');
      });
    } 

  });

  $('#go-top .go').on('click',function(){
    $('html,body').animate({'scrollTop':0},500);
