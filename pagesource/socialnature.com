(function($, window, googleClientId, googleClientScopes) {
  var socialShareService = {
    getFacebookReach: function() {
      return new Promise(function(resolve, reject) {
        FB.api('/me/friends', function(me) {
          var friends = (me.summary || {}).total_count || 0;
          resolve(friends);
        });
      });
    },

    checkFacebookPermission: function() {
      var service = this;
      return new Promise(function(resolve, reject) {
        FB.getLoginStatus(function(fbResponse) {
          if ( console.log ) console.log('FB.getLoginStatus', fbResponse);
          if ( fbResponse.status === 'connected' ) {
            // the user is logged in and has authenticated your
            // app, and response.authResponse supplies
            // the user's ID, a valid access token, a signed
            // request, and the time the access token
            // and signed request each expire
            var authResponse = fbResponse.authResponse;
            FB.api('/me?fields=friends,permissions', function(me) {
              if ( console.log ) console.log('FB.api(/me?fields=friends,permissions)', me);
              var permissions = {};
              me.permissions.data.filter(function (item){return item.status === 'granted';}).forEach(function(item){permissions[item.permission] = true;});
              var reach = me.friends.summary.total_count;
              if ( permissions.publish_actions && permissions.user_friends && permissions.email) {
                if ( console.log ) console.log('FB permissions granted');
                return resolve(authResponse, reach);
              }
                if ( console.log ) console.log('FB permissions failed', permissions);
              return reject('Invalid permissions');
            });
          } else {
            reject('Facebook not connected');
          }
        });
      });
    },

    facebookShare: function(channel, sampleKit, shareUrl, shareType, shareModel) {
      var service = this;
      return new Promise(function(resolve, reject) {
        service.checkFacebookPermission().then(function(authResponse, reach) {
          var ua = navigator.userAgent;
          var iOS = !!ua.match(/iPad/i) || !!ua.match(/iPhone/i);
          var webkit = !!ua.match(/WebKit/i);
          var iOSSafari = iOS && webkit && !ua.match(/CriOS/i);
          if ( iOSSafari ) {
            var redirectUri;
            if ( location.href.indexOf('?') > 0 ) {
              redirectUri = location.href + '&access_token=' + authResponse.accessToken + '&user_id=' + authResponse.userID;
            } else {
              redirectUri = location.href + '?access_token=' + authResponse.accessToken + '&user_id=' + authResponse.userID;
            }
            // Safari requires new window dialog
            window.open('https://www.facebook.com/dialog/share?' +
              'app_id=517804954986177' +
              '&display=popup' +
              '&href=' + encodeURIComponent(shareUrl).replace('localhost:5000', 'www.socialnature.com') +
              '&redirect_uri=' + encodeURIComponent(redirectUri), '_self');
          } else {
            FB.ui({
              method: 'share',
              href: shareUrl
            }, function(response) {
              console.log('FB Share response', response);
              if ( response && !response.error_message) {
                var post_id = response.post_id || response.id;
                window.snTrackEventsModule.trackEvent(sampleKit.Id, 'Review Share', 'Facebook', sampleKit.name );
                FB.api('/' + authResponse.userID + '_' + post_id + '?fields=message,permalink_url', function(postResponse) {
                  resolve({
                    Channel: 'facebook',
                    SampleKitId: sampleKit.Id,
                    AccessToken: authResponse.accessToken,
                    SocialId: authResponse.userID,
                    Message: postResponse.message || '',
                    PostId: post_id,
                    Reach: reach,
                    PostLink: postResponse.permalink_url || 'https://www.facebook.com/' + authResponse.userID + '_' + post_id
                  });
                });
              }
            })
          }
        }, function(error) {
          if ( error == 'Facebook not connected' ) {
            FB.login(function(response){
              if ( response.status === 'connected' ) {
                service.facebookShare(channel, sampleKit, shareUrl, shareType, shareModel);
              }
            }, {
              scope: 'public_profile, email, user_friends, publish_actions'
            });
          }
        });
      });
    },

    googleShare: function(sender, channel, sampleKit, shareUrl, shareType, shareModel) {
      var service = this;
      return new Promise(function(resolve, reject) {
        var opts = {
          calltoactionurl: shareUrl,
          contenturl: decodeURI(shareUrl),
          cookiepolicy: 'single_host_origin',
          onshare: function(data) {
            if ( data.action === 'shared' && data.status === 'completed' ) {
              var postId = data.post_id;
              gapi.client.load('plus', 'v1', function () {
                var request = gapi.client.plus.activities.get({
                  'activityId': postId
                });

                request
                  .execute(function (resp) {
                    if ( resp.error || !resp.url ) {
                      alert('Could not verify your post - make sure you\'re sharing publicly!');
                      return reject((resp.error || {}).message || 'Could not find url');
                    }
                    window.snTrackEventsModule.trackEvent(sampleKit.Id, 'Review Share', 'Google+', sampleKit.name );
                    resolve({
                      Channel: 'googleplus',
                      SampleKitId: sampleKit.Id,
                      AccessToken: '',
                      SocialId: '',
                      Message: resp.title || '',
                      PostId: postId,
                      Reach: 0,
                      PostLink: resp.url
                    });
                  }, function error(err) {
                    //console.log('failed to load post', err);
                  });
                });
            }
          },
          prefilltext: sampleKit.reviewMessage ? sampleKit.reviewMessage : sampleKit.name + ' by ' + sampleKit.company.name,
          scope: 'https://www.googleapis.com/auth/plus.login',
          clientId: googleClientId
        };
        window.gapi.interactivepost.render(sender, opts);
      });
    },

    doShare: function(sender, channel, shareUrl, shareType, shareModel, sampleKit) {
      var handleCallback = function(shareData) {
        if ( shareType === 'review' ) {
          shareData.ReviewId = shareModel;
          //console.log('recording share', shareData);
          $.post('/review/socialpost', shareData, function(){
            window.location.reload(true);
          });
        } else {
          shareData.SampleKitId = shareModel;
          //console.log('recording share', shareData);
          $.post('/product/socialpost', shareData, function(){
            //window.location.reload(true);
          });
        }
      };
      switch ( channel ) {
        case 'facebook':
          this.facebookShare(channel, sampleKit, shareUrl, shareType, shareModel).then(handleCallback);
          break;
        case 'googleplus':
          this.googleShare(sender, channel, sampleKit, shareUrl, shareType, shareModel).then(handleCallback);
          break;
        case 'pinterest':
          this.pinterestShare(channel, sampleKit, shareUrl, shareType, shareModel).then(handleCallback);
          break;
      }
    }
  };

  window.socialShareService = socialShareService;
