  // Featured Views Selector
  var $listViewSelector = $('.list-view-selector');
  var $tileViewSelector = $('.tile-view-selector');
  var $listings = $('#featured-listings-row');

  $(document).on('click', '.list-view-selector', function() {
    $listings.fadeTo(200, 0, function() {
      $('.featured-listing-container').addClass('list-view', 0);
    });

    $listings.fadeTo(200, 1);

    $listViewSelector.addClass('active');
    $tileViewSelector.removeClass('active');

    setCookie('features_view_type', 'list', 365);
  });

  $(document).on('click', '.tile-view-selector', function() {
    $listings.fadeTo(200, 0, function() {
      $('.featured-listing-container').removeClass('list-view', 0);
    });

    $listings.fadeTo(200, 1);

    $tileViewSelector.addClass('active');
    $listViewSelector.removeClass('active');

    setCookie('features_view_type', 'tile', 365);
  });
});</script><script>$(document).ready(function() {
  // Lazy Loading
  var featuredTemplate = _.template($('#featured-template').text());
  var $featuredListings = $('#featured-listings');
  var $featuredListingsRow = $('#featured-listings-row');
  var activeQuery = false

  $('body').css({
    'padding-bottom': '100px'
  });

  $(window).scroll(function() {
    if ($('#no-more-results').length === 0) {
      var featuresOnPage = $('.featured-listing').length;

      if (!activeQuery) {
        if ($(window).scrollTop() > ($(document).height() - ($('.footer').height() + ($(window).height()*1.5) + 100))) {
          activeQuery = true

          if ($('#loading-features').length === 0) {
            $featuredListingsRow.append($('#loading-features-template').text());
            $('#loading-features').slideDown('fast');
          }

          $.ajax({
            url: '/frontpage/fetch_features',
            type: 'GET',
            data: { features_on_page: featuresOnPage },
            success: function(response, status, xhr) {
              $('#loading-features').css({
                opacity:    0,
                transition: 'opacity 0.5s'
              }).slideUp(600, function() {
                $('#loading-features').remove();
              });

              if (response.allFeaturesLoaded === true) {
                $featuredListings.append($('#no-more-results-template').text());
                $('#no-more-results').fadeIn('slow');
                $('.featured-listing').last().addClass('last-item');

                ga('send', 'event', "Engagement", "Browse", "Features - Homepage [ Viewed All ]");
              } else {
                response.forEach(function(feature) {
                  $featuredListingsRow.append(featuredTemplate(feature));
                  $('.featured-listing-container[style*="display: none;"]').fadeIn('slow');
                });
              }

              activeQuery = false;
            },
            error: function(xhr, status, error) {
              console.log('An error occurred (details below):');
              console.log('- XHR:', xhr);
              console.log('- Status:', status);
              console.log('- Error:', error);
            },
          });
        }
      }
    }
  });

  // Google Analytics
  $(document).on('click', '#featured-listings .featured-listing', function() {
    ga('send', 'event', "Engagement", "Click", "Feature [Homepage]");
  });

  $(document).on('click', '#recommendations-box .recommended-item', function() {
    ga('send', 'event', "Engagement", "Click", "Recommendation [Box]");
  });

  $(document).on('click', '.see-more', function() {
    ga('send', 'event', "Engagement", "Browse", "Recommendations");
  });

  $(document).on('click', '#marketing-pane-ending-today', function() {
    ga('send', 'event', "Engagement", "Click", "Marketing Pane - Ending Today [" + (window.innerWidth <= 767 ? "Mobile" : "Other") + "]");
  });

  $(document).on('click', '#marketing-pane-amazing-deals', function() {
    ga('send', 'event', "Engagement", "Click", "Marketing Pane - Amazing Deals [" + (window.innerWidth <= 767 ? "Mobile" : "Other") + "]");
  });

  $(document).on('click', '#marketing-pane-hot-items', function() {
    ga('send', 'event', "Engagement", "Click", "Marketing Pane - Hot Items [" + (window.innerWidth <= 767 ? "Mobile" : "Other") + "]");
  });

  $(document).on('click', '#marketing-pane-new-today', function() {
