       <script>
    var slideInterval;
    
    function switchSliderPage(element,page){
	if(page<0)page=0;
	$('#'+element+' .item').removeClass('shown');
	
	$('#'+element+' .item').each(function(index){
	    if(index==page) {
		$(this).addClass('shown');
		$('#'+element+' .page-switch').removeClass('selected');
		$('#'+element+' .page-switch:eq('+page+')').addClass('selected');
	    }
	    
	});
	pauseSlide(); startSlide();
	
    }
    
    function switchSliderNextPage(element)
    {
	var page=0;
	var $switches = $('#'+element+' .slider-pages .page-switch');
	var count = $switches.length;
	$switches.each(function(index){
	    if($(this).hasClass('selected')) {
		if( (index+1) > (count-1)) page=0;
		else page=index+1;
	    }
	});
	switchSliderPage(element,page);
	
    }
    
    function pauseSlide()
    {
	 clearInterval(slideInterval); 
    }
    
    function startSlide()
    {
	 slideInterval = setInterval(function(){switchSliderNextPage('hlavni_banner_left')},5000);
    }
    
    $(document).ready(function(){
	startSlide();
	
    })
</script>
