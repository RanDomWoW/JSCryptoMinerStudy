{
	//L'utilisateur n'a pas encore de cookie de consentement
	var referrer_host = document.referrer.split('/')[2];
	if ( referrer_host != document.location.hostname )
	{ //si il vient d'un autre site
		//on désactive le tracking et on affiche la demande de consentement
		// article 6 deliberation http://www.cnil.fr/documentation/deliberations/deliberation/delib/300/ on se passe du consentement pour le dépot de cookie immédiat;
		if(typeof(disableStr) != 'undefined')
			window[disableStr] = false;
		window.onload = askConsent();
	}
