    $(document).on("click", "#locationSearchButton", function () {
        var url = "/find-a-location";
        var delimiter = "?";

        var zip = $("#locationSearchZip").val();
        var serviceLineId = $("#locationSearchServiceLine option:selected").val();

        if (serviceLineId) {
            url += delimiter + "serviceLineId=" + serviceLineId;
            delimiter = "&";
        }
        if (zip) {
            url += delimiter + "zipCode=" + zip;
            delimiter = "&";
        }
        window.location.href = url;
    });
