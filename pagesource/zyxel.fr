	<script type="text/javascript">
	$(function() {
		
		if (document.cookie.indexOf("cpidSTU") == -1) {
			if (!window.location.origin) {
				// older IE versions do not support window.location.orgigin; workaround...
				window.location.origin = window.location.protocol + "//" + window.location.hostname + (window.location.port ? ':' + window.location.port: '');
			};
			$.ajax({
				url : "https://www.studerus.ch/de/ajax/checkRS?o="+window.location.origin,
				type : "POST",
				xhrFields: { withCredentials:true },
				success : function(ret) {
					setCookie("cpidSTU",ret);
					doMixpanelInit(ret);
				}
			});
		} else {
			doMixpanelInit(getCookie('cpidSTU'));
		}

	});



function doMixpanelInit(cpidSTU) {
	if (cpidSTU.indexOf('_') > -1) {
		
		var accountID = cpidSTU.split('_')[0];
		var contactID = cpidSTU.split('_')[1];
	} else {
		
		var accountID = "";
		var contactID = cpidSTU;
	}



	

	
	mixpanel.register({"Last Visit": new Date().toISOString()});
	mixpanel.register_once({"First Visit": new Date().toISOString()});

	
	
	
	
	
	
		
	
	
	
		mixpanel.track("Page View", {"Page Name" : "General", "Subpage Name" : "index "});
	


	
    var pe = "General";
    var peSub = "index";
    var pgNewbrand = "";
   	


	
	

	// Mixpanel Tracking Search Result Clicks
	$('a.resultline').click(function(){
		pagename = "General";
		subpagename = "index ";
		type = $(this).attr('data-resulttype');
		target = $(this).attr('href');
		mixpanel.track("Search Result Clicked", {
			"Page Name" : pagename,
			"Subpage Name" : subpagename,
			"Search Result Type": type,
			"Target": target
		});
	});

	// Mixpanel Tracking Menu Clicks
	$('li.megamenu-trigger a').click(function(){
		pagename = "General";
		subpagename = "index ";
		type = $(this).parent().attr('id');
		mixpanel.track("Clicked Menu", {
			"Page Name" : pagename,
			"Subpage Name" : subpagename,
			"Menu Title": type
		});
	});

	$('.menuTitle a').click(function(){
		pagename = "General";
		subpagename = "index ";
		type = "Studerus " + $(this).html();
		mixpanel.track("Clicked Menu", {
			"Page Name" : pagename,
			"Subpage Name" : subpagename,
			"Menu Title": type
		});
	});

	// Mixpanel Tracking Supportportal Clicks
	$('.trackSupportClick').click(function(){
		pagename = "General";
		subpagename = "index ";
		type = $(this).attr('data-type');
		supcatID = $(this).attr('data-subcatid');
		supcatName = $(this).attr('data-subcatname');
		target = $(this).attr('data-target');
		if ( typeof( $(this).attr('data-target') ) != 'undefined' ){
			target = $(this).attr('data-target');}
		else if ( typeof( $(this).attr('href') ) != 'undefined' ){
			target = $(this).attr('href');}
		else {
			target = "";
		}
		artnr = $(this).attr('data-artnr');
		mixpanel.track("SupportClick", {
			"Page Name" : pagename,
			"Subpage Name" : subpagename,
			"Support Click Type": type,
			"Product ID": artnr,
			"Search Subcat1 Filter": supcatID,
			"Search Subcat1 Name": supcatName,
			"Target": target
		});
	});


	// Mixpanel Tracking ValueClicks
	$('.trackValueClick').click(function(){
		productartnr = $(this).attr('data-artnr');
		if ( typeof( $(this).attr('data-pgroup') ) != 'undefined' ){
			pgroup = $(this).attr('data-pgroup');}
		else {
			pgroup = pgNewbrand;
		}
		pagename = "General";
		subpagename = "index ";
		type = $(this).attr('data-type');
		target = $(this).attr('data-target');
		if ( typeof( $(this).attr('data-target') ) != 'undefined' ){
			target = $(this).attr('data-target');}
		else if ( typeof( $(this).attr('href') ) != 'undefined' ){
			target = $(this).attr('href');}
		else {
			target = "";
		}
		doValueClickTracking(pgroup,productartnr,pagename,subpagename,type,target);
	});

	// Mixpanel Tracking Plays
	$('.trackPlay').click(function(){
		productartnr = "";
		pgroup = $(this).attr('data-pgroup');
		pagename = "General";
		subpagename = "index ";
		type = $(this).attr('data-type');
		target = $(this).attr('data-target');
		doPlayTracking(pgroup,productartnr,pagename,subpagename,type,target);
	});

	// Mixpanel Tracking Registers
	$('.trackRegister').click(function(){
		productartnr = "";
		pgroup = $(this).attr('data-pgroup');
		pagename = "General";
		subpagename = "index ";
		type = $(this).attr('data-type');
		target = $(this).attr('data-target');
		doRegisterTracking(pgroup,productartnr,pagename,subpagename,type,target);
	});

	
	

	
	$(".menu-search").click(function() {
		mixpanel.track("Search Opened", {
		"Page Name" : "General.index",
		"Search Type" : "Zyxel global",
		});
	});

}
</script>

