<!-- Has Offers params forwarding code -->
    <script>
      // collect app links
      function getAppLinks() {
        var links = document.querySelectorAll("a");
        var mealPlanAppLinks = [];

        Array.prototype.forEach.call(links, function(link, i) {
          // skip the link if it's not an app link
          if (link.href.indexOf('cooksmarts.com') == -1 && link.href.indexOf('bit.ly') == -1) {
            return;
          }
          // skip popups, lest they misbehave
          if (link.href.indexOf('#popup') != -1) {
            return;
          }
          mealPlanAppLinks.push(link);
        });

        return mealPlanAppLinks;
      }

      // collect forms that post to app
      function getAppForms() {
        var forms = document.querySelectorAll("form");
        var mealPlanAppForms = [];

        Array.prototype.forEach.call(forms, function(form, i) {
          if (form.action.indexOf("cooksmarts.com") == -1 && form.action.indexOf('bit.ly') == -1) {
            return;
          }
          mealPlanAppForms.push(form);
        });

        return mealPlanAppForms;
      }

      // add set of params to a URL
      function addParams(url, paramsStr) {
        var strToAdd = paramsStr;
        if (url.indexOf("?") != -1) {
          strToAdd = "&" + paramsStr;
        } else {
          strToAdd = "?" + paramsStr;
        }
        url += strToAdd;
        return url;
      }

      // detect Has Offers data and forward the params
      // to the app
      function forwardHasOffersParams() {
        // Get current URL
        var url = window.location.href;

        // Check URL for Has Offers params
        if (url.indexOf("hoti=") != -1) {
          // Pull params from URL
          var questionMarkIndex = url.indexOf("?");
          var paramsStr = url.substring(questionMarkIndex + 1, url.length);

          // Attach params to any Rails app links
          var mealPlanLinks = getAppLinks();
          Array.prototype.forEach.call(mealPlanLinks, function(link, i){
            link.href = addParams(link.href, paramsStr);
          });

          // Update form actions
          var mealPlanForms = getAppForms();
          Array.prototype.forEach.call(mealPlanForms, function(form, i){
            form.action = addParams(form.action, paramsStr);
          });
        }
      }

      document.addEventListener("DOMContentLoaded", function() {
        forwardHasOffersParams();
      });
    </script>
<!-- end Has Offers params forwarding code -->
