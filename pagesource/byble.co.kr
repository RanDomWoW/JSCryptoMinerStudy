		// 메인 슬라이드 배너를 가운데 정렬 시키기 위해 window 사이즈에 따라 margin값을 적용하는 함수.
		function main_bxslide_magin() {
			var default_width = 1200;	// 사이트 기본 width px값
			max_width = parseInt($(".main_bxslider").closest('.bx-wrapper').css("max-width"));
			width = parseInt($('body').width());

			gab = max_width - width;
			if(max_width > width && width > default_width) {

				$(".main_bxslider").closest('.bx-wrapper').css("margin-left","-"+(gab/2)+"px");
			} else {

				Newgab = default_width+(((default_width-width)/2*1));
				if(max_width <= width) $(".main_bxslider").closest('.bx-wrapper').css("margin-left","0px");
				else if(width < default_width) $(".main_bxslider").closest('.bx-wrapper').css("margin-left", '-'+Newgab+"px");
			}
		}
		$(window).resize(main_bxslide_magin);
		$(document).ready(function() {
			$(".bx-loading").hide();
		});
	</script>




