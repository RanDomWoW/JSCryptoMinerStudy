            <div class="left">
    <h2 id="alert-message" data-defaulttitle="Compila il form ed entra in contatto con noi!">Compila il form ed entra in contatto con noi!</h2>
    <form method="post" action="#" id="contactform" novalidate>
        <div class="loader" id="loader" style="display:none;"></div> 
        <div class="alert-message" id="alert-message"></div> 
        <div class="row">
            <div class="col2"><input id="nome" name="name" placeholder="Nome (*)" type="text"></div>
            <div class="col2"><input id="cognome" name="surname" placeholder="Cognome (*)" type="text"></div>
        </div>
        <div class="row">
            <div class="col2"><input id="email" name="email" placeholder="Email (*)" type="text"></div>
            <div class="col2"><input id="azienda" name="company" placeholder="Azienda" type="text"></div>
        </div>
        <div class="row">
            <div class="col2"><input id="telefono" name="phone" placeholder="Telefono" type="text"></div>
            <div class="col2"><input id="website" name="webpage" placeholder="Sito Web" type="text"></div>
        </div>
        <div class="col1"><textarea cols="50" id="message" name="message" placeholder="La tua richiesta (*)" rows="4"></textarea></div>
        <div class="g-recaptcha" id="recaptcha" data-sitekey="6LddACYUAAAAAFIUrTsozJmmJDVcPSkezOge7QzU"></div>
                <div class="col1 required">
            <p>Campo obbligatorio.</p>
        </div>
        <div class="col1"><button type="submit" class="btn">Invia Messaggio</button></div>
    </form>
</div>

<script>
    $(document).ready(function() {

     /*   $('#contactform').validate({
            lang: 'it',
            rules: {
                name: 'required',
                surname: 'required',
                hiddenRecaptcha :'required',
                email: {
                    required: true,
                    email: true
                },
                message: 'required',
                "hiddenRecaptcha": {
                        required: function() {
                        if(grecaptcha.getResponse() == '') {
                            return true;
                        } else {
                            return false;
                            }
                            }
                    }
               }
            });*/

        $('#contactform').on('submit', function(e) {
				var c = '.' + $('#contactform').parent().attr('class');
				var captchaContainer = $('#recaptcha');
				captchaContainer.removeClass('error');
				$(c + ' input, ' + c + ' textarea').removeClass('error');
				e.preventDefault();
				var t = $(this);

				var alertMessage = $('#alert-message');
				alertMessage.html(alertMessage.data('defaulttitle'));
				var loader = $('#loader');
				loader.show();

				var postData = t.serializeArray();

				$.ajax({
						url : "/registration.php",
						type: "post",
						data : postData,
						dataType:'json',          
						success:function(data, textStatus, jqXHR) {
						if(data.status == -1) {
						if (data.form_errors.length > 0) { 
							$.each(data.form_errors, function(idx, el) {
							var o = $(c + ' :input[name='+el.field+']');
							if (o.length == 0) $(c + ':textarea[name='+el.field+']');
								o.addClass('error');
								if(el.field=='g-recaptcha-response'){
									captchaContainer.addClass('error');
								}
							});
					}

				} else { // (data.status == 1)
					alertMessage.html('Il tuo messaggio è stato inviato con successo');
					$('#nome').val('');
					$('#cognome').val('');
					$('#email').val('');
					$('#azienda').val('');
					$('#telefono').val('');
					$('#website').val('');
					$('#message').val('');
					}
					loader.hide();
					alertMessage.show();
					},
				error: function(jqXHR, textStatus, errorThrown) {
		   			console.log('error');
		   			alertMessage.html('Si è verificato un errore');
		   			loader.hide();
		   			alertMessage.show();
	   				}
				});
				return false;
				});

    });
</script>


