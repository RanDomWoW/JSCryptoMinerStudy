<script type="text/javascript">
     $(document).ready(function(){    
                    var $window = $(window);
            var $stickyEl = $('.banner-side-sticky');
            $window.scroll(function () {
                $stickyEl.toggleClass('sticky', ($window.scrollTop()+80) > $('div.lowpoint').offset().top);
            });

            // takeover banner placement 
            var groupTakeOver = jQuery('.banner-leaderboard-takeover-group');
            var topTakeOver = groupTakeOver.find('.takeover-leaderboard-top');
            var takeoverSidebars = groupTakeOver.find('.toptakeover');
            var margins = parseInt(groupTakeOver.css('marginBottom'));
            var timerId = setInterval(myTimer, 500);

            function myTimer() {
                takeoverSidebars.css('min-height',(topTakeOver.height()));
                if((topTakeOver.height()+margins) > 90){
                    clearInterval(timerId);
                }
            }
        });
    </script>
