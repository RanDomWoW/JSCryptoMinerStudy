<script>
            var page = 0;

            function setPage(page){
                this.page = page;
            }

            function setupListBanner(siteId,pageId,format,page,time){
                var element = format == 59051 ?  'sas_'+format :  'sas_'+page+'_'+format;
                var time = time+1;
                if(document.getElementById(element) !== null){
                    sas.cmd.push(function() {
                        sas.call("std", {
                            siteId:     siteId, // 
                            pageId:     pageId, // Page : Gaffa
                            formatId:   format, // Format : Multiple
                            tagId: element
                        }, {
                            onNoad: function(data){
                                if (data.formatId == 59051) {
                                    jQuery('#'+data.tagId).closest('.feed-item').hide();
                                    updateLayout();
                                }else{
                                    console.debug(data);
                                }
                            },
                            onLoad: function(data){
                                 if (data.formatId == 59051) {
                                    setTimeout(function () {
                                        var $this = jQuery('#'+data.tagId);
                                        var height = $this.height();
                                        if(!$this.find('img').prop('complete')){
                                            height += 169;
                                        }
                                        $this.closest('.banner-stream-native').height(height);
                                    }, feedOptions.loadingTimeExtender);
                                    updateLayout();
                                } 
                            }
                        });
                    });
                }
                else if(time <= 10){
                    setTimeout(function(){
                        setupListBanner(siteId,pageId,format,page,time);
                    }, 200); // check again in 200 ms
                }else{
                    console.debug('not creating since element '+element+ ' was not found in time');
                    updateLayout();
                }
            }

            function updateLayout(){
                setTimeout(function () {
                    //Arrange elements with ISOTOPE Plugin
                    feedHandler.arrangeElements();
                }, feedOptions.loadingTimeExtender);
            }
        </script>
