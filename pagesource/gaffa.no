<div class="feed-item  {{item.article_type}}">
<div class="feed-item-container">
<article class="feed-article  {{item.article_type}}" data-ng-cloak>
<a href="{{item.url}}" class="article-link">
<div class="article-image" data-ng-if="item.article_type == 'review'">
<div data-ng-if="item.review_type == '3'">
<img class="image-landscape" data-ng-src="{{item.image_urls.stream}}" alt="{{item.header}}" />
</div>
<div data-ng-if="item.review_type != '3'">
<img class="image-square" data-ng-src="{{item.image_urls.square}}" alt="{{item.header}}" />
</div>
</div>
<div class="article-image" data-ng-if="item.article_type !== 'review'">
<img data-ng-src="{{item.image_urls.stream}}" alt="{{item.header}}" />
</div>
<div class="article-content">
 <div class="article-info">
<span data-ng-if="item.article_type == 'news'">Nyhet</span>
<span data-ng-if="item.article_type == 'article'">Artikkel</span>
<span data-ng-if="item.article_type == 'review'">Anmeldelse</span>
<span>{{item.publish_date | amCalendar}}</span>
<span>{{item.publish_date | amDateFormat:'HH:mm'}}</span>
</div>
<p data-ng-if="item.article_type == 'review'" class="article-description"><b>{{item.artist}}<br />{{item.title}}</b></p>
<h1 class="article-title">{{item.header}}</h1>
<div data-ng-if="item.article_type == 'review'" data-ratings="" data-ng-rating="item.rating"></div>
<p data-ng-if="item.label" class="article-label">{{item.label}}</p>
</div>
</a>
</article>
</div>
</div>
</div>
</div>
