    <script type="text/javascript">
	function showToutelesoffre(item)
	{
    	// contenus
					$('touteslesoffres_secteur').hide();
				$('touteslesoffres_ville').hide();
		$('touteslesoffres_region').hide();
		$('touteslesoffres_departement').hide();
		$('touteslesoffres_'+item).show();

		// onglets
					$('onglet_secteur').removeClassName("active");
					$('onglet_region').removeClassName("active");
		$('onglet_ville').removeClassName("active");
		$('onglet_departement').removeClassName("active");
		$('onglet_'+item).addClassName("active");

		showToutelesoffre_page(1);
		if(item == 'secteur' || item == 'departement')
			$('pager_touteslesoffres').show();
		else
			$('pager_touteslesoffres').hide();
	}

	function showToutelesoffre_page(page)
	{
		if(page == 1) 
		{
			$('touteslesoffres_departement_page1').show();
			$('touteslesoffres_departement_page2').hide();
			$('touteslesoffres_ville_page1').show();
			$('touteslesoffres_region_page1').show();
							$('touteslesoffres_secteur_page1').show();
				$('touteslesoffres_secteur_page2').hide();
						$('bouton_page_2').removeClassName("active");
			$('bouton_page_1').addClassName("active");
				
		}
		else
		{
			$('touteslesoffres_departement_page1').hide();
			$('touteslesoffres_departement_page2').show();
			$('touteslesoffres_ville_page1').hide();
			$('touteslesoffres_region_page1').hide();
							$('touteslesoffres_secteur_page1').hide();
				$('touteslesoffres_secteur_page2').show();
						$('bouton_page_1').removeClassName("active");
			$('bouton_page_2').addClassName("active");
		}
	}
    </script>    
