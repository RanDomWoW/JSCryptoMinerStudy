<script type="text/ng-template" id="create-account.component.html">
    <div class="step-container-wrapper">
        <div class="step-container">
            <h2>Create your Account</h2>
            <h5>Signing up for RichMeetBeautiful is fast and free</h5>

            <div class="completed-sub-steps">
                <div class="completed-sub-step" ng-if="vm.accountDetails.sex" ng-click="vm.backToStep('sex')">
                    I&#039;m a
                    <strong>{{ 'enumusersex_' + vm.accountDetails.sex | translate }}</strong>
                </div>
                <div class="completed-sub-step" ng-if="vm.accountDetails.sugarType" ng-click="vm.backToStep('sugarType')">
                    Who&#039;s a
                    <strong>{{ vm.getSugarTitle(vm.accountDetails.sex, vm.accountDetails.sugarType) | translate }}</strong>
                </div>
                <div class="completed-sub-step" ng-if="vm.accountDetails.seekingSex" ng-click="vm.backToStep('seekingSex')">
                    Interested in
                    <strong>{{ 'interested_in.' + vm.accountDetails.seekingSex | translate }}</strong>
                </div>
                <div class="completed-sub-step" ng-if="vm.accountDetails.email" ng-click="vm.backToStep('email')">
                    E-mail:
                    <strong>{{ vm.accountDetails.email }}</strong>
                </div>
                <div class="completed-sub-step completed-sub-step-username"
                     ng-if="vm.accountDetails.username"
                     ng-click="vm.backToStep('username')"
                >
                    <span>Username:</span>
                    <strong class="long-text-fix">
                        {{ vm.accountDetails.username }}
                    </strong>
                </div>
                <div class="completed-sub-step" ng-if="vm.accountDetails.password" ng-click="vm.backToStep('password')">
                    Password:
                    <strong>{{ vm.accountDetails.password | passwordToAsterisk }}</strong>
                </div>

                <div class="completed-sub-step" ng-if="vm.accountDetails.birthDate" ng-click="vm.backToStep('birthDate')">
                    Birthday:
                    <strong>{{ vm.accountDetails.birthDate | date:'d MMMM yyyy' }}</strong>
                </div>

                <div class="completed-sub-step"
                     ng-if="vm.accountDetails.netWorth && vm.accountDetails.sugarType == 'daddy'"
                     ng-click="vm.backToStep('netWorth')"
                >
                    {{ 'profile.financials.net_worth' | translate }}:
                    <strong>{{ 'net_worth_' + vm.accountDetails.netWorth | translate }}</strong>
                </div>

                <div class="completed-sub-step"
                     ng-if="vm.accountDetails.annualIncome  && vm.accountDetails.sugarType == 'daddy'"
                     ng-click="vm.backToStep('annualIncome')"
                >
                    {{ 'profile.financials.annual_income' | translate }}:
                    <strong>{{ 'annual_income_' + vm.accountDetails.annualIncome | translate }}</strong>
                </div>

                <div class="completed-sub-step"
                     ng-if="vm.accountDetails.lifeStyle  && vm.accountDetails.sugarType == 'baby'"
                     ng-click="vm.backToStep('lifeStyle')"
                >
                    {{ 'profile.financials.life_style' | translate }}:
                    <strong>{{ 'life_style_' + vm.accountDetails.lifeStyle | translate }}</strong>
                </div>
            </div>

            <div class="current-sub-step" ng-if="vm.subStep == 'sex'">
                <label>I&#039;m a:</label>

                <div class="choose-container">
                    <div class="option" ng-click="vm.setSex('male')">
                        Man
                    </div>

                    <div class="option" ng-click="vm.setSex('female')">
                        Woman
                    </div>
                </div>
            </div>

            <div class="current-sub-step" ng-if="vm.subStep == 'sugarType'">
                <label>Who&#039;s a:</label>

                <div class="choose-container">
                    <div class="option" ng-click="vm.setSugarType('baby')">
                        SugarBaby
                    </div>
                    <div class="option" ng-click="vm.setSugarType('daddy')">
                        {{ vm.getSugarTitle(vm.accountDetails.sex, 'daddy') | translate }}
                    </div>
                </div>
            </div>

            <div class="current-sub-step" ng-if="vm.subStep == 'seekingSex'">
                <label>Interested in:</label>

                <div class="choose-container">
                    <div class="option" ng-click="vm.setSeekingSex('male')">
                        Men
                    </div>
                    <div class="option" ng-click="vm.setSeekingSex('female')">
                        Women
                    </div>
                    <div class="option" ng-click="vm.setSeekingSex('any')">
                        Both
                    </div>
                </div>
            </div>

            <div class="current-sub-step sub-step-email" ng-if="vm.subStep == 'email'">
                <form name="emailForm" ng-submit="emailForm.$valid && vm.setEmail()">

                                            <div
                                vc-recaptcha
                                key="'6Ld0EykUAAAAANPoLBFfJGCZV4Cbam0RoY-nk0Tc'"
                                on-success="vm.captchaSuccess()"
                        ></div>
                    
                    <div ng-if="vm.emailHasMistake || vm.emailUndeliverable">
                        <div ng-if="vm.emailHasMistake">
                            Did you mean <b>{{ vm.emailToReplace }}</b>?

                            <div class="confirmation-button-line">
                                <button class="btn btn-primary btn-cta text-uppercase" ng-click="vm.replaceEmail()">Yes</button>
                                <button class="btn btn-primary btn-cta text-uppercase" ng-click="vm.emailContinue()">No</button>
                            </div>
                        </div>

                        <div ng-if="!vm.emailHasMistake && vm.emailUndeliverable">
                            Email seems undeliverable. Are you sure it’s the correct one?

                            <div class="confirmation-button-line">
                                <button class="btn btn-primary btn-cta text-uppercase" ng-click="vm.emailContinue()">Yes</button>
                            </div>
                        </div>
                    </div>

                    <div class="input-group">
                        <input
                                type="email"
                                required
                                name="email"
                                class="form-control"
                                ng-model="vm.email"
                                placeholder="E-mail"
                        >
                        <span class="input-group-btn">
                            <button class="btn btn-primary btn-cta text-uppercase" ng-disabled="vm.emailHasMistake || vm.emailUndeliverable">
                                Send
                            </button>
                        </span>
                    </div>
                </form>
            </div>

            <div class="current-sub-step" ng-if="vm.subStep == 'username'">
                <form name="usernameForm" ng-submit="usernameForm.$valid && vm.setUsername()">
                    <div class="input-group">
                        <input
                                type="text"
                                required
                                maxlength="50"
                                name="username"
                                class="form-control"
                                ng-model="vm.username"
                                placeholder="Username"
                        >
                        <span class="input-group-btn">
                            <button class="btn btn-primary btn-cta text-uppercase">
                                Send
                            </button>
                        </span>
                    </div>
                </form>
            </div>

            <div class="current-sub-step" ng-if="vm.subStep == 'password'">
                <form name="passwordForm" ng-submit="passwordForm.$valid && vm.setPassword()">
                    <div class="input-group">
                        <input
                                type="password"
                                required
                                name="password"
                                class="form-control"
                                ng-model="vm.password"
                                placeholder="Password"
                                autocomplete="new-password"
                                minlength="6"
                        >
                        <span class="input-group-btn">
                            <button class="btn btn-primary btn-cta text-uppercase">
                                Send
                            </button>
                        </span>
                    </div>

                    <ng-password-meter password="vm.password"></ng-password-meter>
                </form>
            </div>

            <div class="current-sub-step sub-step-date" ng-if="vm.subStep == 'birthDate'">
                <form name="birthDateForm" ng-submit="birthDateForm.$valid && vm.setBirthDate()">
                    <div class="custom-form-group date-form-group">
                        <label>Birthday</label>

                        <div class="date-container">
                            <div class="select-wrapper">
                                <select
                                        ng-model="vm.birthDay"
                                        ng-change="vm.setBirthDate()"
                                        ng-options="option.text for option in vm.days track by option.id"
                                >
                                    <option value="">Day</option>
                                </select>

                                <i class="app-icon-arrow-down"></i>
                            </div>

                            <div class="select-wrapper">
                                <select
                                        ng-model="vm.birthMonth"
                                        ng-change="vm.setBirthDate()"
                                        ng-options="month as month.text for month in vm.months"
                                >
                                    <option value="">Month</option>
                                </select>

                                <i class="app-icon-arrow-down"></i>
                            </div>

                            <div class="select-wrapper year-wrapper">
                                <select
                                        ng-model="vm.birthYear"
                                        ng-change="vm.setBirthDate()"
                                        ng-options="year.id for year in vm.years"
                                >
                                    <option value="">Year</option>
                                </select>

                                <i class="app-icon-arrow-down"></i>
                            </div>
                        </div>
                    </div>
                </form>
            </div>

            <div class="current-sub-step" ng-if="vm.subStep == 'netWorth'">
                <div class="custom-form-group">
                    <div class="select-wrapper">
                        <select
                                ng-model="vm.netWorth"
                                ng-change="vm.setNetWorth()"
                                ng-options="option as ('net_worth_' + option) | translate for option in vm.enums.net_worth"
                        >
                            <option value="">{{ 'profile.financials.net_worth' | translate }}</option>
                        </select>

                        <i class="app-icon-arrow-down"></i>
                    </div>
                </div>
            </div>

            <div class="current-sub-step" ng-if="vm.subStep == 'annualIncome'">
                <div class="custom-form-group">
                    <div class="select-wrapper">
                        <select
                                ng-model="vm.annualIncome"
                                ng-change="vm.setAnnualIncome()"
                                ng-options="option as ('annual_income_' + option) | translate for option in vm.enums.annual_income"
                        >
                            <option value="">{{ 'profile.financials.annual_income' | translate }}</option>
                        </select>

                        <i class="app-icon-arrow-down"></i>
                    </div>
                </div>
            </div>

            <div class="current-sub-step" ng-if="vm.subStep == 'lifeStyle'">
                <div class="custom-form-group">
                    <div class="select-wrapper">
                        <select
                                ng-model="vm.lifeStyle"
                                ng-change="vm.setLifeStyle()"
                                ng-options="option as ('life_style_' + option) | translate for option in vm.enums.life_style"
                        >
                            <option value="">{{ 'profile.financials.life_style' | translate }}</option>
                        </select>

                        <i class="app-icon-arrow-down"></i>
                    </div>
                </div>
            </div>

            <div ng-if="!vm.subStep" class="continue-button-wrapper text-center">
                <a class="btn btn-primary btn-cta text-uppercase"
                   ng-click="vm.updateUser()"
                   ng-disabled="vm.sendingForm"
                >
                    Continue
                    <i class="app-icon-arrow-right"></i>
                </a>

                                
                <div class="terms-accept">
                    <div>By clicking, I accept the                     <a class="terms-anchor" target="_blank" href="/en/static/terms-and-conditions">
                        Terms of Use
                    </a>
                 and                     <a class="terms-anchor" target="_blank" href="/en/static/privacy-statement">
                        Privacy policy and Cookie policy
                    </a>
                </div>
                    <div>RichMeetBeautiful will send you email alerts and promotions.</div>
                </div>
            </div>
        </div>
    </div>
</script>
