        <script type="text/javascript">
            $(document).ready(function() {
                // Activate Bootstrap tooltips
                $('[data-toggle="tooltip"]').tooltip({
                    trigger: 'hover'
                });
            });

            function checkNewsletterSignupForm(myForm) {
                var errorMessage = "This form could not be sent because of the following reason(s):\n\n";
                var errorFound = false;

                if (myForm.subscription_name != null) {
                    if (myForm.subscription_name.value == '') {
                        errorMessage = errorMessage + "- No name given.\n";
                        errorFound = true;
                    }
                }
                if (myForm.subscription_email != null) {
                    if (myForm.subscription_email.value == '') {
                        errorMessage = errorMessage + "- Missing email address.\n";
                        errorFound = true;
                    }
                }
                if (errorFound) {
                    alert(errorMessage);
                    return false;
                }

                $.post(
                    $(myForm).attr('action'),
                    {
                        'email': $(myForm).find('[name=subscription_email]').val(),
                        'name': $(myForm).find('[name=subscription_name]').val(),
                        'language': $(myForm).find('[name=subscription_language] option:selected').val()
                    },
                    function() {
                        $(myForm).fadeOut('slow');
                        $('.js-subscribe-message').html("Thank you for your subscription!");
                    }
                );

                return false;
            }
        </script>
