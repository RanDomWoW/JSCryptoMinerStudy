<script>
	function checkForValidSearchTerm(){
		var s=s_gi('mormarriottvacationclubp'); 
		s.linkTrackVars='prop15,prop16';
		s.prop16='Marriott Vacation Club - Prospects';
		s.prop15='mvcp:globalnav:sitesearchbutton';
		s.tl(this,'o','mvcp:globalnav:sitesearchbutton');
		
		if (dijit.byId('form.input.navSearch').getValue().length != 0 && dojo.byId('form.input.navSearch').value != 'Search this website'){
			document.location = '/site-search/index.shtml?keyword=' + dijit.byId('form.input.navSearch').getValue();
		} 
		else { 
			dojo.byId('form.input.navSearch').style.color = 'red'; dojo.byId('form.input.navSearch').style.borderColor = 'red';
		}	
	}
	dojo.connect(document.body, "onkeyup", function(event){
		keyCode = event.keyCode;
		if ((keyCode == 13) && (document.activeElement != "[object HTMLBodyElement]")){
			navSearchValue = "";
			if (dojo.byId("form.input.navSearch")){
				navSearchValue = dijit.byId('form.input.navSearch').getValue();
			}
			if (navSearchValue.length != 0 && navSearchValue != "Search this website"){
				document.location = '/site-search/index.shtml?keyword=' + dijit.byId('form.input.navSearch').getValue().replace('&','|amp|');
			}
		}
	});
	
	function clearPreviousTermNav(){
		dojo.byId('form.input.navSearch').style.color = 'black'; 
		dojo.byId('form.input.navSearch').style.borderColor = '#d4d6d5'; 
		dojo.byId('form.input.navSearch').style.borderTopColor = '#6e706f'; 
		if  (dojo.byId('form.input.navSearch').value == '' || dojo.byId('form.input.navSearch').value == 'Search this website'){
			dojo.byId('form.input.navSearch').value = '';
		}
	}
	
	function replaceDefaultTextIfBlankNav(){
		if (dojo.byId('form.input.navSearch').value == ''){ dojo.byId('form.input.navSearch').value = 'Search this website'; }	
	}
	
</script>

