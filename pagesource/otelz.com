        var _paraBirimi = 'TRY';
        var _paraBirimiTextTasarimli = "";
        var _paraBirimiText = "";
        var _paraBirimiVal = "";
        $(document).ready(function () {
            mParaBirimiSet();
        });
        function mParaBirimiSet() {
            var ddlParaBirimi = document.getElementById("ddlParaBirimi1");
            $(".para-birimi-menu-item, .headerv3-ddl-currency div").each(function () {
                _paraBirimiVal = $(this).data("birimval");
                if (_paraBirimi == _paraBirimiVal) {
                    _paraBirimiVal = $(this).data("birimval");
                    _paraBirimiText = $(this).data("birim");
                    _paraBirimiTextTasarimli = $(this).data("birim");
                    $(ddlParaBirimi).html(_paraBirimiTextTasarimli);
                    $(".currency").text(_paraBirimiText);
                }
            });
        }
        function mParaBirimiDegis(bunu, preventReload) {
            $(bunu).addClass('selected');
            var ddlParaBirimi = document.getElementById("ddlParaBirimi1");
            var aktifParaBirimi = $(ddlParaBirimi).text();
            var paraBirimi = $(bunu).text();
            if (aktifParaBirimi == paraBirimi) {
                $(bunu).removeClass('selected');
                return;
            }
            var paraBirimiVal = $(bunu).data("birimval");
            if (_paraBirimi == paraBirimiVal) {
                $(bunu).removeClass('selected');
                $(ddlParaBirimi).trigger('click');
                return $.when(false);
            }
            _paraBirimi = paraBirimiVal;
            dataLayer.push({
                'Category': 'CurrencySelect',
                'Action': 'Select',
                'Label': paraBirimiVal,
                'NonInteraction': 'true',
                'event': 'gaEvent'
            });
          return $.ajax({
                type: "POST",
                url: "/Clients/WS/WSGenel.asmx/mParaBirimiCookie",
                data: "{paraBirimi:'" + paraBirimiVal + "'}",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    if (!preventReload) {
                        window.location.href = window.location.pathname;
                        return false;
                    } else {
                        _paraBirimi = paraBirimiVal;
                        mParaBirimiSet();
                        $(bunu).removeClass('selected');
                        $(ddlParaBirimi).trigger('click');
                        return true;
                    }
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert("işlem hatası");
                    $('.headerv3-ddl-currency div').removeClass('selected');
                    return false;
                }
            });
        }

        function mKaydetKampanya(bunu) {
            var txtEmail = document.getElementById("txtEmailForKampanya");
            var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            var mailDogruMu = re.test(txtEmail.value);
            if (txtEmail.value != "" && mailDogruMu) {
                OtelzWebUI.Clients.WS.WSGenel.mKampanyaHaberKaydet(txtEmail.value, mGetResKampanyaKayit);
            }
            else {
                $(txtEmail).addClass("hatali-input");
                $(txtEmail).click(function () {
                    $(this).removeClass("hatali-input");
                    $(this).unbind("click");
                });
            }
        }

        function mCallCenterSem() {
            dataLayer.push({
                'Category': 'HotelPage',
                'Action': 'CallCenterClicked',
                'NonInteraction': 'false',
                'event': 'gaEvent'
            });
            return true;
        }

        function mGetResKampanyaKayit() {
            dataLayer.push({
                'Category': 'HotelPage',
                'Action': 'NewsletterSend',
                'NonInteraction': 'false',
                'event': 'gaEvent'
            });

            var html = '<p style="color:#e53f45;">Tebrikler! Artık fırsatlar ayağınıza gelecek.</p>';
            $(".footerv3-right .footerv3-bigtext").html(html);
            $(".footerv3-right #txtEmailForKampanya").hide();
            $(".footerv3-right .dikdortgenbutton").hide();
        }

        // scroll event start
        function in_array(needle, haystack) {
            for (var i = 0, len = haystack.length; i < len; i++)
                if (haystack[i] == needle) return i;

            return -1;
        }
        var viewportHeight = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight || -1;
        var scrollYPosition, firstScrollYPosition = (document.documentElement && document.documentElement.scrollTop || document.body && document.body.scrollTop || 0);
        var documentHeight = Math.max(
			document.body.scrollHeight, document.documentElement.scrollHeight,
			document.body.offsetHeight, document.documentElement.offsetHeight,
			document.body.clientHeight, document.documentElement.clientHeight
		);

        var executedScrollPositions = [];
        var pageDown = false;

        window.onscroll = function () {
            scrollYPosition = (document.documentElement && document.documentElement.scrollTop || document.body && document.body.scrollTop || 0);
            if (Math.abs(scrollYPosition - firstScrollYPosition) >= viewportHeight) {
                var eventLine = Math.floor(scrollYPosition / viewportHeight);

                if (in_array(eventLine, executedScrollPositions) == -1) {
                    dataLayer.push({
                        'Category': 'Scroll',
                        'Action': document.URL,
                        'Label': (eventLine * viewportHeight) + " / " + (documentHeight - viewportHeight),
                        'Value': (eventLine * viewportHeight) / (documentHeight - viewportHeight) * 100,
                        'NonInteraction': 'TRUE',
                        'event': 'gaEvent'
                    });

                    executedScrollPositions.push(eventLine);
                }

                if (pageDown == false && scrollYPosition >= (documentHeight - viewportHeight)) {
                    dataLayer.push({
                        'Category': 'Scroll',
                        'Action': document.URL,
                        'Label': (eventLine * viewportHeight) + " / " + (documentHeight - viewportHeight),
                        'Value': 100,
                        'NonInteraction': 'TRUE',
                        'event': 'gaEvent'
                    });
                    pageDown = true;
                }
            }
        }

        // scroll event end
        var theurl = document.URL;
        function getFeedbackForm() {
            if ($('.feeder-popup-background').length < 1) {
                $('body').append('<div class="feeder-popup-background"></div>');
                
            }
