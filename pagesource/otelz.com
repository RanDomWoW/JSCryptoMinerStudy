        window.__latestResRecursiveCount = 0;
        window.__latestResRecursiveIndex = 0;
        window.__latestResData = [];
        $(document).ready(function () {
            latestResRecursiveGet(10)
        })
        function latestResRecursiveGet(max) {
           $.ajax({
               url: '/rest/latestres/' + max + '?format=json',
                method: 'GET',
                contentType: 'application/json; charset=UTF-8',
                dataType: 'json',
                success: function (res) {
                    if (res && res.length) {
                        window.__latestResData = window.__latestResData.concat(res);
                        if (window.__latestResRecursiveCount == 0) {
                            displayLatestResItems();
                            startShowLatestRes();
                        }
                        
                    }
                }
            })
        }
        function startShowLatestRes() {
            setTimeout(function () {
                displayLatestResItems();
                startShowLatestRes();
                window.__latestResRecursiveCount = window.__latestResRecursiveCount + 1;
                if (window.__latestResRecursiveCount % 36 == 0) {
                    latestResRecursiveGet(4);
                }
            }, 10000)
        }
        function displayLatestResItems() {
            var data = window.__latestResData;
            var len = data.length;
            var exists = $('.blc-item');
            var elen = exists.length;
            if (elen > 4) {
                for (var x = 4; x < elen; x++) {
                    $(exists[x]).remove();
                }
                elen = 4;
            }
            while (elen < 5) {
                if ((window.__latestResRecursiveIndex + 1) >= len)
                    window.__latestResRecursiveIndex = 0;

                var elm = renderLatestResItem(data[window.__latestResRecursiveIndex]);
                elm.hide().prependTo('#blc-items').fadeIn(2000);

                window.__latestResRecursiveIndex++;
                elen = elen + 1;
            }
        }
        function renderLatestResItem(item) {
            item.PlaceName = item.PlaceName || 'Türkiye';
            var html = '<a class="blc-item" href="/otel/'+ item.Code +'" target="_blank">'
                       + '<div class="blc-item-left"><img src="//img.otelz.com/s3/size(96,69,crop)/' + item.Path + '/115x115/' + item.Img + '"/></div>'
                       + '<div class="blc-item-right"><b>' + item.PlaceName + '</b> noktasında bir kullanıcı <span class="blc-facility-name">' + item.Name + '</span> tesisinde az önce yer ayırttı.</div>'
                      + '</a>';
            return $(html);
        }
    </script>
