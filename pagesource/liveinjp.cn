<script type="text/javascript">
    function CheckQuickLogin() {
        $(".quicklogin input[rel]").tipsy({ trigger: 'manual', gravity: 'w' });
        $(".quicklogin input[rel]").tipsy('hide');
        var txtAccount = $('.quicklogin input[id$=txtAccount]:eq(0)');
        var txtPassWord = $('.quicklogin input[id$=txtPassWord]:eq(0)');
        var regEmail = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w+)+$/;
        if ($(txtAccount).val() == "") {
            $(txtAccount).tipsy("show");
            return false;
        }
        else if (!regEmail.test($(txtAccount).val())) {
            $(txtAccount).tipsy("show");
            return false;
        }
        else {
            $(txtAccount).tipsy("hide");
        }
        var regPwd = /^\w{6,15}$/;
        if ($(txtPassWord).val() == "") {
            $(txtPassWord).tipsy("show");
            return false;
        }
        else if (!regPwd.test($(txtPassWord).val())) {
            $(txtPassWord).tipsy("show");
            return false;
        }
        else {
            $(txtPassWord).tipsy("hide");
        }
    }
</script>

