            function timeset() {
                var focusnum = $(".focuslist li").length;
                var focusindex = $(".focusbtn li").index($(".focusbtn .on"));
                if (focusnum > focusindex + 1) {
                    $(".focuslist li").eq(focusindex + 1).show().siblings().hide();
                    $(".focusbtn li").eq(focusindex + 1).addClass("on").siblings().removeClass("on");
                }
                else {
                    $(".focuslist li").eq(0).show().siblings().hide();
                    $(".focusbtn li").eq(0).addClass("on").siblings().removeClass("on");
                }
            };

            $(document).ready(function () {
                //按钮触碰事件
                $(".focusbtn li").bind("mouseover", function () {
                    $(this).addClass("on").siblings().removeClass("on");
                    var btnnum = $(".focusbtn li").index(this);
                    $(".focuslist li").eq(btnnum).show().siblings().hide();
                });

                //自动计时事件
                picTimer = setInterval(timeset, 4000);
                $(".focus").mouseover(function () {
                    clearInterval(picTimer);
                });
                $(".focus").bind("mouseout", function () {
                    picTimer = setInterval(timeset, 4000);
                });
            });
        </script>
