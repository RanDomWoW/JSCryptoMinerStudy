<html>
    <head>
        <meta charset="utf-8"/>
    </head>
    <body>
        <script type="text/javascript">
            var userAgent = navigator.userAgent,
                    requestUrl = 'http://www.gpsoo.net/',
                    isMobile = false;
            if(/Android|iPhone|iPod|iPad|Windows Phone|MQQBrowser/gi.test(userAgent)){
                isMobile = true;
            }
            if(requestUrl.indexOf('ga.gpsoo.net') > -1){
                location.href = requestUrl+'index_poliz.shtml';
            }else if('cn' == 'cn' && isMobile){
                // location.href = requestUrl+'index_m.shtml';
                location.href = requestUrl+'/m/index.html';
            }else if('ru' == 'cn'){
                location.href = requestUrl+'index_ru.shtml';
            }else if('en' == 'cn'){
                if(requestUrl.indexOf('www.cootrack.net') > -1){
                    location.href = requestUrl+'index_en_kks.shtml'; //¿µ¿­Ë¹
                }else{
                    location.href = requestUrl+'index_en.shtml';
                }
            }else{
                location.href = requestUrl+'index.shtml';
            }
        </script>
    </body>
</html>