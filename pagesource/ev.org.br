<script>
    $(function () {
        if (($('#hdBuscaCursosText').val() != '') && ($('#hdBuscaCursosValue').val() != '')) {
            var arrBuscaCursosText = JSON.parse($('#hdBuscaCursosText').val());
            var arrBuscaCursosValue = JSON.parse($('#hdBuscaCursosValue').val());
            $('#txtBusca').typeahead({
                hint: true,
                highlight: true,
                minLength: 1
            }, {
                name:'cursos',
                source: new Bloodhound({
                    datumTokenizer: Bloodhound.tokenizers.whitespace,
                    queryTokenizer: Bloodhound.tokenizers.whitespace,
                    local: arrBuscaCursosText
                })
            });
            $('#txtBusca').bind('typeahead:select', function (ev, suggestion) {
                var index = arrBuscaCursosText.indexOf(suggestion);
                if (index != -1) {

	                var whichPage = '';

	                if ( location.href.toLowerCase().indexOf('/curso/') != -1 ) {
		                whichPage = 'Detalhe curso ' + $('.course-heading .course-title h1').text();
	                } else if ( location.href.toLowerCase().indexOf('/cursos') != -1 ) {
		                whichPage = 'Listagem cursos';
	                } else if ( location.href.toLowerCase().indexOf('/roteiro/') != -1 ) {
		                whichPage = 'Detalhe Roteiro ' + $('.course-heading .course-title h1').text();
	                } else if ( location.href.toLowerCase().indexOf('/roteiros') != -1 ) {
		                whichPage = 'Listagem roteiros de estudo';
	                } else {
		                whichPage = 'Pagina inicial';
	                }

	                // analytics
	                ga('send','event','Pagina - ' + whichPage, whichPage + ' – Encontre seu Curso', 'Cursos ' + suggestion);

                    location.href = arrBuscaCursosValue[index];
                }
            });
        }


        // carousel
        $('.hero-carousel .carousel-caption h2 a').click(function(){
	       if ( location.href.toLowerCase().indexOf('/curso/') != -1 ) {
			    // analytics
			    ga('send','event','Pagina - Detalhe curso ' + $('.course-heading .course-title h1').text(),'Banner', $(this).text());

			} else if ( location.href.toLowerCase().indexOf('/cursos') != -1 ) {
			    // analytics
			    ga('send','event','Pagina - Listagem cursos','Banner', $(this).text());

			} else if ( location.href.toLowerCase().indexOf('/roteiro/') != -1 ) {
			    // analytics
			    ga('send','event','Pagina - Detalhe Roteiro ' + $('.course-heading .course-title h1').text(),'Banner', $(this).text());

			} else if ( location.href.toLowerCase().indexOf('/roteiros') != -1 ) {
			    // analytics
			    ga('send','event','Pagina - Listagem roteiros de estudo','Banner', $(this).text());
			} else {
			    // analytics
			    ga('send','event','Pagina - Página inicial','Banner', $(this).text());
			}
        });


    });
</script>
