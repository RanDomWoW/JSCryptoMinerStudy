  <script type="text/template" id="nugs-tmpl-radio">
    <div>
      <@ if (track.get('songTitle')) { @>
        <a href="<@= track.get('soundUrl') @>" target="_blank" class="media-btn">Play</a>
        <div class="label">Now Playing on<br>LivePhish Radio</div>
        <div class="meta">
          <a href="<@= track.get('soundUrl') @>" target="_blank">
            <span class="song-title"><@= track.get('songTitle') @></span>
            <span class="container-info">
              <@= track.get('date') @>
              <@= track.get('venue') @>
            </span>   
          </a>
        </div>    
        <a class="buy button small">VIEW / BUY SHOW</a>
      <@ } else { @>  
        <a href="<@= track.get('soundUrl') @>" target="_blank" class="media-btn">Play</a>
        <a href="<@= track.get('soundUrl') @>" target="_blank">Listen to LivePhish Radio</a>
      <@ } @>
    </div>
  </script>


  <script type="text/template" id="nugs-tmpl-cart-status">CART<@ if (display_count !== false) { @><a style="display:inline" <@ if (cart.getCount() > 0){ @>class="has-items"<@ } @>><span class="count"><@= cart.getCount() @></span></a><@ } @></script>

  <script type="text/template" id="nugs-tmpl-status">
    <@= user.isAuthenticated() ? 'loggedin' : 'a guest' @>
    <!--<a data-event="trial:request">Request a trial</a>-->
  </script>

  <script type="text/template" id="nugs-tmpl-catalog">
  
    <header class="entry-top">
      <nav class="catalog-nav">      
        <@ if (show_sorting && Nugs.Settings.get('facet_search') !== false) { @>
        <form class="custom" id="catalog-sort-by">
     
          <div class="row collapse sorter">
            <div class="large-2 small-2 columns">
              <span class="prefix">Sort</span>
            </div>
            <div class="large-10 small-10 columns">
              <@ var options = {
                'date:desc':'by show date (recent-older)',
                'date:asc':'by show date (older-recent)',
                'release:desc':'by release date (recent-older)',
                'release:asc':'by release date (older-recent)',
                'location:asc':'by location (a-z)',
                'location:desc':'by location (z-a)',
                'title:asc':'by album title (a-z)',
                'title:desc':'by album title (z-a)',
                'preorder:asc':'pre-orders only'
              };
              @>
              <select name="sortby">
                <@ for(var k in options) { var v = options[k]; @>
                <option value="<@= k @>" <@ if (filters && filters.sort == k) { @>selected<@ } @>><@= v @></option>
                <@ } @>
              </select>                  
            </div>
          </div>
        </form>
        <@ } @>
        <span class="list-style-buttons">
            <a id="listview" class="switcher list-view-switch">
              <span></span>
              <span></span>
              <span></span>
            </a>
            <a id="gridview" class="switcher active grid-view-switch">
              <span></span>
              <span class="right"></span>
              <span></span>
              <span class="right"></span>
            </a>
        </span>
      </nav>
      <h5 class="entry-title"><@= title ? title : 'catalog' @></h5>
    </header>
    <div class="entry-content">
        <div class="table-headers row">
          <div class="column large-2 active desc" data-sort="date"><span>Date</span><em></em></div>
          <div class="column large-3" data-sort="artist"><span>Artist</span><em></em></div>
          <div class="column large-2" data-sort="location"><span>Location</span><em></em></div>
          <div class="column large-3" data-sort="title"><span>Album Title</span><em></em></div>          
          <div class="column large-2">&nbsp;</div>
        </div>
        <div id="catalog-no-results" class="catalog no-results">No results found</div>
        <div class="catalog loading searching" style="display:none">Searching and sorting catalog...</div>
        <ul id="products" class="grid container-quickviews large-block-grid-4"></ul>
        <div class="fetch-more center" style="display:none;text-align:center"><button class="button medium radius">Load more shows and albums</button></div>
        <div class="loading fetching-more" style="display:none">Loading more shows and albums...</div>
    </div>
  </script>

  <script type="text/template" id="nugs-tmpl-filters">
    <ol class="widget-list widget-list-single-white search-filters">
        <li>
            <p>
                <label>DATE: <span>&nbsp;</span></label>
                <input type="text" class="text" id="name1" name="date" placeholder="Enter Year or Date">
            </p>
            <div id="date-labels" class="clearfix search-labels-container"></div>
            <p>
                <label>LOCATION: <span>&nbsp;</span></label>
                <input type="text" class="text" id="city" name="city" placeholder="Enter City">
            </p>
            <div id="city-labels" class="clearfix search-labels-container"></div>
            <p>
                <label>VENUE: <span>&nbsp;</span></label>
                <input type="text" class="text" id="venue" name="venue" placeholder="Enter venue">
            </p>
            <div id="venue-labels" class="clearfix search-labels-container"></div>            
            <p>
                <label>SONG: <span>&nbsp;</span></label>
                <input type="text" class="text" id="song" name="song" placeholder="Enter Song">
            </p>
            <div id="song-labels" class="clearfix search-labels-container"></div>
            <p>
                <label>ARTISTS: <span>&nbsp;</span></label>
            </p>
            <div class="artists checkboxes">
              
              <@ for(var i=0; i < bandMembers.length; i++) { var member = bandMembers[i]; @>
                <div class="<@= !filters.band_members || filters.band_members.length == 0 || _.indexOf(filters.band_members, member) > -1 ? 'checked' : '' @>">
                    <span class="checkbox"></span>
                    <label class="checkbox"><@= member @></label>
                </div>              
              <@ } @>
            </div>
            <p>
                <label>RELEASE TYPE: <span>&nbsp;</span></label>
            </p>
            <div class="release-types checkboxes">
                <div class="<@= !filters.release_types || filters.release_types.length == 0 || _.indexOf(filters.release_types, 'audio') > -1 ? 'checked' : '' @>">
                    <span class="checkbox"></span>
                    <label class="checkbox">Audio</label>
                </div>
                <div class="<@= !filters.release_types || filters.release_types.length == 0 || _.indexOf(filters.release_types, 'video') > -1 ? 'checked' : '' @>">
                    <span class="checkbox"></span>
                    <label class="checkbox">Video</label>
                </div>
            </div>
        </li>

    </ol>
  </script>

  <script type="text/template" id="nugs-tmpl-catalog-container">
    <div class="image for-grid-view" <@= onclickForContainer(container) @>>
      <@= container.getImgTag({width: '100%'}) @>
    </div>
    <div class="list-date for-grid-view">
      <@ if (container.isShow()) { @><@= container.getDate('MMM D') @>, <span><@= container.getDate('YYYY') @></span><@ } @>

    </div>
    <div class="list-date for-list-view columns large-2" <@= onclickForContainer(container) @>>
      <@ if (container.isShow()) { @><@= container.getDate('M/D/YYYY') @><@ } @>
    </div>
    <div class="products"></div>
    <div class="list-title for-grid-view"><@= container.get('artistName') @></div>
    <div class="list-title for-list-view columns large-3" <@= onclickForContainer(container) @>><@= container.get('artistName') @></div>    
    <@ if (container.isShow() && container.getVenueName() && container.getVenueCity()) { @>
      <p class="title for-grid-view">
          <@= container.getVenueName() @>
          
      </p>
      <p class="location for-grid-view">
        <b><@= container.get('venueCity') @>, <@= container.get('venueState') @></b>
      </p>
      <div class="large-2 columns for-list-view location"  <@= onclickForContainer(container) @>>
          <b><@= container.getVenueName() @></b>
          <@ if (container.getVenueCity()) { @>
            <br>
            <@= container.get('venueCity') @>, <@= container.get('venueState') @>
          <@ } @>
      </div>
      <div class="for-list-view columns large-3 title"></div>      
    <@ } else {  @>
      <p class="title for-grid-view"><@= container.get('containerInfo') @></p>
      <div class="for-list-view columns large-2 location"></div>      
      <div class="for-list-view columns large-3 title" <@= onclickForContainer(container) @>><@= container.get('containerInfo') @></div>
    <@ } @>
    <div class="drop-down-container for-list-view columns large-2">
      <div class="video-icon" <@= onclickForContainer(container) @>></div>
      <div class="products"></div>
    </div>
    <div class="for-grid-view video-icon" <@= onclickForContainer(container) @>></div> 
    <span class="for-list-view-clearfix"></span>
  </script>

  <script type="text/template" id="nugs-tmpl-container-details">
    <div class="entry-content <@= container.hasLiveVideo() ? 'live-video' : '' @>">
        <div class="row">
            <!-- title area -->
            <div class="large-12 columns">
                <div class="large-7 small-8 columns" style="padding-right: 20px;">
                    <div class="row" style="padding-top: 20px;">                        
                        <div class="large-8 columns container-details-header">

                            <@ if (container.isShow()) { @>
                            <div class="big-date-container">
                              <div class="panel big-date">
                                  <div class="month"><@= container.getDate('MMM') @></div>
                                  <div class="day"><@= container.getDate('DD') @></div>
                                  <div class="year"><@= container.getDate('YYYY') @></div>
                               </div>
                             </div>
                            <@ } @>
                            <div class="panel main-info">
                                <div class="artist"><a href="<@= urlForArtist(container.get('artistName')) @>"><@= container.get('artistName') @></a></div>
                                <@ if (container.isShow()) { @>
                                    <div class="venue"><@= container.get('venueName') @></div>
                                    <div class="city"><@= container.get('venueCity') @>, <@= container.get('venueState') @></div>
                                <@ } else { @>
                                    <div class="venue"><@= container.get('containerInfo') @></div>
                                <@ } @>
                            </div>
                            <div class="prev-next">                              
                              <a <@= onclickForContainerByPageURL(container.get('prevContainerURL')) @>>PREV</a>
                              <a <@= onclickForContainerByPageURL(container.get('nextContainerURL')) @>>NEXT</a>
                            </div>

                        </div>
                        <div class="large-4 columns share-and-products-container">
                            <div class="video-icon"></div>

                            <div class="share-and-products">
                              <div class="share panel">
                                <label>Share</label>
                                <div class="share-buttons">                               
                                  <a class="fb"></a>
                                  <a class="tw"></a>
                                </div>
                              </div>
                              <div class="panel products"></div>
                            </div>                            
                        </div>

                    </div>          
                    
                    <div class="tracks" style="padding-top:10px;"></div>
                    
                    <@
                    var notes = container.getNotes();
                    if (notes.length > 0) { @>
                    <div class="show-notes large-12 columns">
                      <h5>Notes</h5>
                      <@ _.each(notes, function(n) { @>
                        <div class="note"><@= n @></div>
                      <@ }) @>
                    </div>
                    <@ } @>                      
                      
                    
                    <div class="show-reviews large-12 columns"></div>      
                </div>
                <!-- image -->
                <div class="large-5 small-4 columns container-right-column" style="padding-top: 20px;">
                    <@= container.getImgTag({width:'100%'}) @>
                    

                    
                    <!-- packages -->
                    <@ 
                    var packages = container.getPackages();
                    if (packages.size()) {
                    @>
                    <div class="packages large-12">
                      <h6>Packages</h6>
                      <@ 
                      packages.each(function(package) { 
                        var slug = package.getSlug();
                        console.log(slug)
                        var url = urlFor('package', {id: package.get('ID'), slug: slug});
                      @>
                      <div>
                        <a href="<@= url @>"><@= package.get('listingName') @></a>
                      </div>
                      <@ }) @>
                    </div>
