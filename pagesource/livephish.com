  <script type="text/template" id="nugs-tmpl-featured-item">
    <@ 
    var url, static = false;
    if (item.get('SHOW_STATICURL')) {
      static = true;
      url = item.get('SHOW_STATICURL');
    } else if (item.isPackage()) { 
      url = '/packages/2,';
    } else if (item.isShow()) {
      url = '/music/0,';
    }
    if (!static) url+=item.get('SHOW_ID')+'/'+item.get('SHOW_SLUG');
    @>
    <a href="<@= url @>"><img src="<@= item.get('SHOW_IMG') @>" /></a>

    <p class="list-title"><@= item.get('SHOW_TITLE') @></p>
    <div class="dropdown-wrapper">
        <a href="<@= url @>"  class="small button empty-dropdown no-arrow"><span><@= item.get('SHOW_STATUS') @></span></a>
    </div>
    <div class="list-subtitle"><span class="orange"><@= item.get('SHOW_DATE') @></span> <@= item.get('SHOW_LOC') @></div>
    <p class="desc"><@= item.get('SHOW_DESC') @></p>
    
    <div class="share" >
      <label>SHARE</label>
      <a class="fb" data-share-url="<@= item.get('SHOW_SHARE_URL') @>" data-share-fb-caption="<@= item.get('SHOW_SHARE_FB_CAPTION') @>" data-share-fb-description="<@= item.get('SHOW_SHARE_FB_DESCRIPTION') @>"></a>
      <a class="tw" data-share-tweet="<@= item.get('SHOW_SHARE_TWEET') @>" data-share-url="<@= item.get('SHOW_SHARE_URL') @>"></a>      
    </div>
      
  </script>  

  <script type="text/template" id="nugs-tmpl-popular-item">
    <h5 class="track-meta">
    <a>
    <span class="teal"><@= item.get('artistName') @></span>
    <@ if (item.get('songTitle')) { @>
      <span class="orange"><b><@= item.get('songTitle') @></b></span>
      <span class="container-info">
      <@ if (item.get('venueName')) { @>        
          <b><@= item.getDate('M/D/YYYY') @></b>, 
          <@= item.get('venueCity') @>, <@= item.get('venueState') @>
      <@ } else { @>
        <@= item.get('containerTitle') @>
      <@ } @>            
      </span>
    <@ } else { @>
      <span class="container-info">
      <@ if (item.isShow()) { @>
        <b><@= item.getDate('M/D/YYYY') @></b>,
        <@= item.get('venueCity') @>, <@= item.get('venueState') @>
      <@ } else { @>
        <@= item.get('containerTitle') @>
      <@ } @>
      </span>
    <@ } @>
    </a></h5>
  </script>

