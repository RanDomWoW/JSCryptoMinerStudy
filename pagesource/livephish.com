  <script type="text/template" id="nugs-tmpl-container-track">
    <a class="media-btn play">Play</a>
    <h5 class="track-meta"><@= track.get('songTitle') @></h5>
    <div class="action-btns <@= track.container.hasVideo() ? 'with-video' : 'audio-only' @> <@= track.container.hasLiveVideo() ? 'webcast' : '' @>">
        <span class="duration"><@= track.getDuration() @></span>
        <span class="buy-buttons">
        <@
        var mp3Product = track.getProductByFormat('MP3');
        var vidProduct = track.getProductByFormat('VIDEO');
        if (mp3Product) { 
          var added = App.data.cart.containsProduct(mp3Product);
        @>
          <a class="cart <@ if (added) { @>in-cart<@ } @> buytrack-btn" data-format="MP3">
            <span class="buy">MP3 - $<@= mp3Product.getPrice() @></span>
            <span class="in">ADDED</span>
          </a>
        <@ } else { @>
          <span class="empty audio">Album Only</span>
        <@ } @>        
        <@ if (vidProduct) {
          var added = App.data.cart.containsProduct(vidProduct);        
        @>
          <a class="cart <@ if (added) { @>in-cart<@ } @> buytrack-btn" data-format="VIDEO">
            <span class="buy">VIDEO - $<@= vidProduct.getPrice() @></span>
            <span class="in">ADDED</span>            
          </a>    
        <@ } else { @>
            <span class="empty video">VIDEO</span>
        <@ } @>        
        </span>
    </div>
  </script>

  <script type="text/template" id="nugs-tmpl-product">
    <@ 
    var label, cssclass = '', format, isPackage = product.model && product.model.get('packageID');
    if (isPackage) {
      format = product.get('packageFormat');
    } else {
      format = product.get('formatStr');
    }
    
    switch(format) {
      case 'LIVE VIDEO':
        label = 'WEBCAST';
        break;
      case 'LIVE HD VIDEO':
        label = 'HD WEBCAST';
        cssclass = 'hd-webcast';
        break;
      default:
        label = product.get(isPackage ? 'packageFormat' : 'pfTypeStr');
    }        
    @>
    <a class="add <@= cssclass @>" data-sku="<@= product.get('skuID') @>"
        data-cost-plan-id="<@= product.get('costplanID') @>"
        >
        <@ if (product.getPrice() > 0) { @>        
          <@= label @>        
          <@ if (!product.get('gift')) { @><span class="sep"> - </span><span class="price">$<@= product.getPrice() @></span><@ } @>
        <@ } else { @>
          <@ if (!product.get('gift')) { @>FREE<@ } @> <@= label @>
        <@ } @>
      </a>    
  </script>
  

  <script type="text/template" id="nugs-tmpl-model-products">
    <@ var rand = Math.round(Math.random()*100000) @>
    <a  data-dropdown="drop-products-<@= model.id @>-<@= rand @>" class="small button dropdown">
      <span style="padding-right:5px;">
      <@ if (model.isFree()) { @>
        FREE 
      <@ } else if (!isUpgrade) { @>
        <@= model.isAvailable() || model.get('packageID') ? 'BUY' : 'PRE-ORDER' @>
      <@ } else { @>
        MORE FORMATS
      <@ } @>
      </span>
    </a>
    <br>
    <ul id="drop-products-<@= model.id @>-<@= rand @>" class="f-dropdown" data-dropdown-content></ul>
  </script>  

