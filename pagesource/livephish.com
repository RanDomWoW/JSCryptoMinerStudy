  <script type="text/template" id="nugs-tmpl-set-header">
    <@
    var num = number, settype = 'set';
    if (number == 10) {
      settype = 'sound check'
      num = false;
    } else if (number == 13) {
      settype = 'filler';
      num = false
    } else if (number > 3 && number != 8) {
      settype = 'encore';
      num = num - 3;
      if (num == 1) num = false;
    } else if (number == 8) {
      num = 4;
    }

    if (num == 1) suffix = 'st';
    else if (num == 2) suffix = 'nd';
    else if (num == 3) suffix = 'rd';
    else if (num > 3) suffix = 'th';
    @>
    <h6 class="entry-title set-title"><@= num ? num+suffix+' ' : '' @><@= settype @></h6>
  </script>

