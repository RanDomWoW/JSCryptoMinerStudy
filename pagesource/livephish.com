  <script type="text/template" id="nugs-tmpl-stash-item">
    <@
    var button = !item.isExpired(), label = item.isVideo() ? 'Watch' : 'Download';
    if (!item.isLiveVideo()) {
      button = button && !item.isPreorder();
      if (item.isPreorder()) label = 'Pre-ordered';
      var status = item.get('status');
      if (!item.isVideo()) {
        label += ' '+item.get('format');
      }
    } else {
      if (item.get('isEventLive')) {
        label += ' Now';
      } else if (item.get('isEventPassed')) {
      } else {
        button = false;
        label = 'Pre-ordered Live Video<br>Tune in on '+item.get('eventStartDate')+' '+item.get('timeZoneToDisplay');
      }
    }
    if (item.isVideo()) {
      var format = item.get('format').replace('VIDEO', 'WEBCAST');
      if (format == 'LIVE WEBCAST') format = 'LIVE SD WEBCAST';
      label += '<br><span class="video-format">'+format+'</span>';
    }
    var clickForContainer = item.get('availabilityType') != 5 ? onclickForContainer(item) : 'style="cursor:default"';
    @>
    <div class="for-list-view show-date column large-2">
      <@ var isSingle = item.get('trackTitle') @>
      <span class="cb-container"><@ if (button && !item.isVideo()) { @><input type="checkbox" value="<@= item.get('skuID') @>" /><@ } @></span>
      <span <@= clickForContainer @>><@= item.get('performanceDate') @></span>
    </div>    
    <@ if (!isSingle) { @>
    <div class="for-list-view artist column large-2">
      <@= item.get('artistName') @>
    </div>   
    <@ } @>
     
    <div class="for-list-view location column large-<@= isSingle ? 4 : 3 @>">
      <a <@= clickForContainer @>>
        <@ if (item.get('trackTitle')) { @>
          <span class="song"><@= item.get('trackTitle') @></span>
          <br>
        <@ } @>
        <@ if (item.get('showTitle')) { @>
          <b><@= item.get('venueName') @></b>
          <br><@= item.get('venueCity') @>, <@= item.get('venueState') @>
        <@ } else { @>
          <@= item.get('albumTitle') @>        
        <@ } @>
      </a>    
    </div>   
    <@ if (item.get('trackTitle')) { @>
    <div class="for-list-view artist column large-2">
      <@= item.get('artistName') @>
    </div>   
    <@ } @>
          
    <div class="for-list-view buy-date column large-<@= isSingle ? 2 : 1 @>">
      <@= item.getBuyDateOnly() @>
    </div>      
    <div class="for-list-view button-container status column large-<@= isSingle ? 2 : 4 @>">
      <div class="row">
        <div class="columns large-<@= isSingle ? 12 : 6 @>">
          <@
          if (item.isExpired()) {
          @>
          Expired <@= item.get('format') @>
          <@
          } else {
          @>
            <@= button ? '<button class="dl stylish"><span>'+label+'</span></button>' : label @>
            <@ if (item.expires()) { @><p class="expires">Expires: <@= moment().add('minutes', item.get('minutesLeft')).format('M/D/YYYY HH:mm a') @></p><@ } @>
          <@ } @>
        </div>
        <@ if (!isSingle) { @>
        <div class="column large-6"> 
        
            <@ if (item.isUpgradable()) { @>          
            <div class="dropdown-wrapper empty">     
              <a class="small button dropdown">
                <span style="padding-right:5px;">            
                  MORE FORMATS            
                </span>
              </a>
            </div>  
            <@ } @>
        </div>
        <@ } @>      
      </div>
    </div> 


  </script>

