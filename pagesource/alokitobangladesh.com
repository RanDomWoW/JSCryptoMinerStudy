<script>



window.Url = "http://www.alokitobangladesh.com/";

function LoadCalendar()

{

	month = jQuery("#dmonth").val();

	year = jQuery("#dyear").val();

	jQuery.ajax({

		url: window.Url + 'welcome/calendar_display/' + year + '/' + month,

		type: 'GET',

		success: function(data) 

		{

			jQuery('#calendar').empty();   

			jQuery('#calendar').append(data);     

		},

	});

}



</script>







