<script language="JavaScript" type="text/javascript">
    function RefreshMenu()
    {
        var outUrl = 'product_count-ajax2.php?s=7&',
            style1 = document.getElementById("style1"),
            grade1 = document.getElementById("grade1"),
            watts1 = document.getElementById("watts1"),
            brand1 = document.getElementById("brand1");

        if (style1.value != 0) {outUrl += 'style=' + style1.value + '&';}
        if (grade1.value != 0) {outUrl += 'grade=' + grade1.value + '&';}
        if (watts1.value != 0) {outUrl += 'watts=' + watts1.value + '&';}
        if (brand1.value != 0) {outUrl += 'brand=' + brand1.value + '&';}
        outUrl += 't=' + new Date().getTime();

        $.ajax({url: outUrl, type: 'GET', success: recreateMenu});
        function recreateMenu(data)
        {
            var menu1 = data.documentElement.getElementsByTagName("menu1"),
                menu2 = data.documentElement.getElementsByTagName("menu2"),
                menu3 = data.documentElement.getElementsByTagName("menu3"),
                menu4 = data.documentElement.getElementsByTagName("menu4"),
                menus = [menu1, menu2, menu3, menu4],
                ids = ['style1', 'grade1', 'watts1', 'brand1'],
                resetStrings = ['Reset Style', 'Reset Fuel', 'Reset Wattage', 'Reset Brand'];

            // Sets each Menu to empty
            style1.length = 0; grade1.length = 0; watts1.length = 0; brand1.length = 0;

            // Populates the Menus
            for (var i = 0; i < menus.length; i++) {
                for (var j = 0; j < menus[i].length; j++) {
                    var valueElement = menus[i][j].getElementsByTagName("value"),
                        valueElementValue = valueElement[0].firstChild.nodeValue,
                        nameElement = menus[i][j].getElementsByTagName("name"),
                        nameElementValue = nameElement[0].firstChild.nodeValue,
                        selectedElement = menus[i][j].getElementsByTagName("selected"),
                        selectedElementValue = selectedElement[0].firstChild.nodeValue;

                    addMenuOption(ids[i], decode(nameElementValue), valueElementValue, selectedElementValue);
                    if (selectedElementValue == 1 && valueElementValue != 0) {
                        document.getElementById(ids[i]).options[0].text = resetStrings[i];
                    }
                }
            }
        }
    }
    function addMenuOption(id, name, value, selected)
    {
        var optionElement = document.createElement("option");
        document.getElementById(id).options.add(optionElement);
        optionElement.text = name;
        optionElement.value = value;
        if (selected == 1) {
            optionElement.selected = 'selected';
        }
        if (id == 'style1') {
            if (value == '1' || value == '5' || value == '14' || value == '8' || value == '9' || value == '13') {
                optionElement.style.fontWeight='700';
            } else {
                optionElement.style.textIndent='10px';
            }
        }
    }
    function decode(string)
    {
        return unescape(string.replace(/\+/g, " "));
    }
</script>


