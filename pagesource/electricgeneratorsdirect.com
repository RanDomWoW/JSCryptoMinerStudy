     function ratingStars(rating){
            if (rating=='' || rating=='0'){rating='0.0';}
            var star_output = '';
            var rating_num = rating.split('.')[0];
            if(rating_num > 5){rating_num = 5;}
            if(rating_num <= 0){rating_num = 0;}

            var rating_dec = rating.split('.')[1];
                rating_dec = rating_dec.substring(0,1);

            var full_stars = rating_num;

            var var_star = '';

            var blank_stars = 4 - rating_num;
            if (full_stars <= 0){full_stars=0;}
            if (blank_stars <= 0){blank_stars=0;}

            //Write total full stars
            for(var s = rating_num; s > 0; s--){
                star_output += '<img src="images/full-stars-15x15.gif" width="15" height="15" style="border-style:none vertical-align:text-top;" alt="' + rating + ' out of 5">';
            }

            //if rating_num < 5 calculate and write var_star
            if(rating_num < 5){

                var star_src= '';

                if(rating_dec == 0){
                    star_src = 'empty-stars-15x15.gif';
                }

                else if((rating_dec > 0) && (rating_dec < 5)){
                    star_src = '25-percent-stars-15x15.gif';
                }

                else if((rating_dec >= 5) && (rating_dec < 7)){
                    star_src = 'half-stars-15x15.gif';
                }

                else if((rating_dec >= 7) && (rating_dec <= 9)){
                    star_src = '34-percent-stars-15x15.gif';
                }

                star_output += '<img src="images/' + star_src + '" width="15" height="15" style="border-style:none vertical-align:text-top;" alt="' + rating + ' out of 5">';
            }

            //Calculate and write blank stars
            for(var t = blank_stars; t > 0; t--){
                star_output += '<img src="images/empty-stars-15x15.gif" width="15" height="15" style="border-style:none vertical-align:text-top;" alt="' + rating + ' out of 5">';
            }

            return star_output;
        }



