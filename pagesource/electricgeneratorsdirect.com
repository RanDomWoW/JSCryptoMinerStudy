<script type="text/javascript">
// JQuery Cart Update
var has_items = false;
MiniCartAjax();
function create_cart_html(products_id,products_id_full,name,url,host,model,price,image,qty,i){
    var d = '';
    d += '<div class="item item_'+products_id+'" id="item' + i + '" style="box-sizing:border-box; border:0px; padding:20px; border-bottom:1px solid #ddd;">';
        d += '<div style="display:inline-block; vertical-align:top; margin-right:15px;"><a title="'+name+'" href="'+url+'"><img width="64" height="64" src="//'+location.host+'/products-image/64/' + image + '"></a></div>';
        d += '<div style="display:inline-block; max-width:260px; width:260px;">';
            d += '<div style="display:block; font-family:arial; font-size:14px; line-height:21px; white-space:nowrap; overflow-x:hidden; text-overflow:ellipsis; width:275px;"><a title="'+name+'" class="bluelink" href="'+url+'">' + name + '</a></div>';
            d += '<div style="display:block; font-family:arial; font-size:14px; line-height:21px; margin-right:20px;">Model: ' + model + '</div>';
            d += '<div style="display:block; font-family:arial; font-size:14px; line-height:21px; font-weight:bold; color:#009f3f;">$' + price + '</div>';
            d += '<div style="display:block; font-family:arial; font-size:14px; line-height:21px;">Quantity: <div style="display:inline-block; width:74px; box-sizing:border-box; border:1px solid #ddd; text-align:center; margin-left:5px;">';
            d += '<div style="display:inline-block; width:22px; height:22px; background:white; font-weight:bold; color:#9f9f9f; text-align:center; cursor:pointer;" onclick="MiniCartAjax(\'sub\', \''+products_id_full+'\');" id="rem_'+products_id+'">-</div>';
            d += '<div style="display:inline-block; color:#6b6b6b; width:26px; height:22px; box-sizing:border-box; border-left:1px solid #ddd; border-right:1px solid #ddd; text-align:center;" id="quantity_'+products_id+'">' + qty + '</div>';
            d += '<div style="display:inline-block; width:22px; height:22px; background:white; font-weight:bold; color:#9f9f9f; cursor:pointer;" onclick="MiniCartAjax(\'add\', \''+products_id_full+'\');" id="add_'+products_id+'">+</div></div></div>';
            d += '<div style="display:block; font-family:arial; font-size:14px; line-height:21px; padding-top:2px;"><a style="cursor:pointer;" class="bluelink" id="' + products_id + '" onclick="MiniCartAjax(\'remove\',\''+products_id_full+'\');">Remove From Cart</a></div>';
        d += '</div>';
    d += '</div>';
    return d;
}
function MiniCartAjax(action, id) {
    if(!!action){
        id = id.toString();
        var id_clean = id.replace(/[{}]/g, '\\$&');
        var id_pos = id.indexOf('{');
        if ( id_pos > -1 ) { var id_clean2 = id.substring(0, id_pos); } else { var id_clean2 = id; }
        var counter = $('#quantity_'+id_clean).html();
        if ( action == "add" ) { // Add Quantity
            $('#quantity_'+id_clean).html(++counter);
        } else if( action == "sub" ) { // Remove Quantity
            if ( counter == 1 ) { return false; } else if ( counter > 1 ) { $('#quantity_'+id_clean).html(--counter); }
        } else if ( action == "remove" ) {
            $('#pid'+id_clean2).attr('href', 'https://' + location.host + '/shopping_cart.php?action=buy_now&products_id='+id_clean2);
            $('#pid'+id_clean2+' span.button').text('Add To Cart');
            $('#pid'+id_clean2).each( function(){ addToCart( $(this).attr('id') );} );
        }
    }
    $.ajax({
        type: "GET",
        url: "https://www.electricgeneratorsdirect.com/cart-ajax.php?mini_scart=yes&update=yes&action="+action+"&products_id="+id+"&quantity="+counter,
        dataType: "json",
        success: function(data) {
            $(".cart-link").html(data[0].display);
            if ( data[3].myaccount == "logged_in" ) {
                $('.myaccount_logged_in').show();
                $('.myaccount_logged_out').hide();
            } else if ( data[3].myaccount == "logged_off" ) {
                $('.myaccount_logged_in').hide();
                $('.myaccount_logged_out').show();
            }
            if ( data[3].firstname.length > 0) {
                $('#accfirstname').text(data[3].firstname);
            }
            if (data[5].zip != null) {
                if ($('#c_zip').length > 0) {
                    $('#c_zip').val(data[5].zip);
                }
            }
            if (data[6].region_code != null) {
                if ($('#00Nj00000085V0p').length > 0){
                    $('#00Nj00000085V0p option[data-zone="' + data[6].region_code + '"]').attr("selected", "selected");
                }
                if ($('#MERGE3').length > 0){
                    $('#MERGE3 option[data-zone="' + data[6].region_code + '"]').attr("selected", "selected");
                }
                if ($('#shipping_state').length > 0){
                    $('#shipping_state option[data-zone="' + data[6].region_code + '"]').attr("selected", "selected");
                }
                /*if ($('#ship_tax_calc_select').length > 0){
                    $('select#ship_tax_calc_select option:contains("' + data[6].region_code + ' - ")').prop('selected', true).select(this.form.submit());
                }*/
            }
            myaccountlogin();
            if ( data[2].length == 0 ) {
                has_items = false;
                $('#item-container').css('display', 'none');
                return false;
            } else {
                if ( data[2].products.length > 0 ) { has_items = true; }
                var d = ''; var d2 = '';
                for ( var i = 0; i < data[2].products.length; i++ ) {
                    var products_id = data[2].products[i].id;
                    var products_id_full = data[2].products[i].full_id;
                    var url = '//'+location.host+'/product_info.php?products_id='+products_id;
                    var price = data[2].products[i].price; price = price.toString().substring(0, price.length - 2); price = price.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    var name = data[2].products[i].name;
                    var cart = '//'+location.host+'/shopping_cart.php';
                    d += create_cart_html(products_id,products_id_full,name,url,location.host,data[2].products[i].model,price,data[2].products[i].image,data[2].products[i].quantity,i);
                    $("#pid"+parseInt(products_id)).attr("href",'https://'+ location.host + '/shopping_cart.php'); $("#pid"+parseInt(products_id)).unbind("click");
                    $("#pid"+parseInt(products_id)+" span.button").text("In Cart");                    
                }
                if(!!action){
                    $('#items').html(d);
                    var subtotal = data[1].price; subtotal = subtotal.replace(' - ', '');
                    $('#mini_cart_subtotal').html(subtotal);
                    $('#item-container').css('height', 'auto');
                    $('#item-container').css('background', 'white');
                }else{
                    $('#items').html(d);
                    $('#fixed_section').remove();
                    var subtotal = data[1].price; subtotal = subtotal.replace(' - ', '');
                    d2 += '<div id="fixed_section" style="border-top:1px solid #ccc; border-left:1px solid #ccc;">';
                        d2 += '<div style="text-align:center; height:35px; padding-top:20px; font-family:arial; font-size:14px; font-weight:bold;">Sub-Total: <span id="mini_cart_subtotal" style="color:#009f3f;">' + subtotal + '</span></div>';
                        d2 += '<div style="text-align:center;"><a href="//'+location.host+'/shopping_cart.php" id="cart-url" class="button" style="width:200px; height:25px; font-family:arial; font-size:16px; color:white; padding-top:15px;">Continue To Cart</span></div>';
                        d2 += '<div style="height:25px;"></div>';
                    d2 += '</div>';
                    $('#item-container').append(d2);
                    $('#item-container').css('height', 'auto');
                    $('#item-container').css('background', 'white');
                }

                                if (data[4].shipping.threshold == true) { $('.shippingInfoDisplay').show(); }

            }
        },
        xhrFields: { withCredentials: true }
    });
    $('#container, #item-container').mouseenter(function() { if ( has_items == true ) { $('#item-container').stop().slideDown(200, function() { $('#item-container').css('height', 'auto'); }); } });
    $('#container, #item-container').mouseleave(function() { if ( has_items == true ) { $('#spacer').stop().toggle(200); $('#item-container').stop().slideUp(200); } });
}
$('#container').mouseover(function() {
    if ( $('.cart-link').html().substring(0, 1) == 0 || location.pathname == '/shopping_cart.php' || location.pathname == '/login.php' || location.pathname == '/shopping_cart.php' || location.pathname == '/guest_checkout.php' || location.pathname == '/checkout_account.php' ) { $('#item-container').css('display', 'none'); }
    $('#item-container').css('height', 'auto !important');
});
</script>

