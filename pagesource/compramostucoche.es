<script type="html/template" id="on-site-price">
  <% dataLayer.push({'event': 'event.customerBanner.open', 'customer-banner': 'on-site-price'}) %>
<div class="customer-banner es" id="on-site-price">
  <div class="container">
    <div class="row">
      <div class="customer-banner__body customer-banner__body--min100">
        <div class="hidden-xs col-sm-3 customer-banner__col customer-banner__col--no-border text-center">
          <% if (diffDays(evaluation.onsiteDateTime) <= 5) { %>
          <div class="customer-banner__ribbon customer-banner__ribbon--desktop">
            <div>
              Sólo durante <%= 6 - diffDays(evaluation.onsiteDateTime) %> DÍAS VÁLIDO!
            </div>
          </div>
          <div class="customer-banner__ribbon customer-banner__ribbon--tablet">
            <div>
              Sólo durante <%= 6 - diffDays(evaluation.onsiteDateTime) %> DÍAS VÁLIDO!
            </div>
          </div>
          <% } else { %>
          <div class="customer-banner__illustration"></div>
          <% } %>
        </div>
          <% if (diffDays(evaluation.onsiteDateTime) <= 5) { %>
        <div class="col-xs-12 col-sm-6 text-center customer-banner__col customer-banner__col--no-border">
            <div class="customer-banner__header customer-banner__header--2">Precio para tu  <%= manufacturer %></div>
            <div class="hidden-xs customer-banner__car-price customer-banner__header--margin-bottom"><%=number(evaluation.onsitePrice)%>€</div>
            <table class="hidden-sm hidden-md hidden-lg customer-banner__table-box">
              <tr>
                <td>
                  <div class="customer-banner__car-price customer-banner__header--margin-bottom"><%=number(evaluation.onsitePrice)%>€</div>
                </td>
                <td>
                  <div class="customer-banner__ribbon customer-banner__ribbon--mobile">
                    <% if (diffDays(evaluation.onsiteDateTime) <= 5) { %>
                    Sólo durante <%= 6 - diffDays(evaluation.onsiteDateTime) %> DÍAS VÁLIDO!
                    <% } else { %>
                    Precio expirado
                    <% } %>
                  </div>
                </td>
              </tr>
            </table>
            <div>
              <a href="/booking/<%= carlead.hash %>/" class="btn btn-warning btn-block btn--arrow hidden-xs">
                Crea un calendario de venta ahora
              </a>
              <a href="/booking/<%= carlead.hash %>/" class="btn btn-warning btn-block btn--arrow visible-xs">
                Pide una cita ahora
              </a>
            </div>
          </div>
          <% } else { %>
        <div class="col-xs-12 col-sm-6 text-center customer-banner__col customer-banner__col--no-border not-valid">
            <div class="customer-banner__header customer-banner__header--2 not-valid">
              Seguimos interesados <br/> en tu <%=manufacturer%>
            </div>
            <div class="customer-banner__header customer-banner__header--3 not-valid">
              Oferta expirada
            </div>
            <div class="customer-banner__car-price customer-banner__header--margin-bottom not-valid"><%=number(evaluation.onsitePrice)%>€</div>
            <div>
              <a href="/booking/<%= carlead.hash %>/" class="btn btn-warning btn-block btn--arrow not-valid">
                Solicitar una nueva oferta
              </a>
            </div>
          </div>
          <% } %>
        <% if (diffDays(evaluation.onsiteDateTime) <= 5) { %>
        <div class="hidden-xs col-sm-3 text-left customer-banner__col customer-banner__col--no-border">
          <div class="customer-banner__header customer-banner__header--3">
            Esto es lo que dicen nuestros clientes
          </div>
          <div class="customer-banner__quote">
            &quot;Muy buen equipo, totalmente satisfecho con la amabilidad y el servicio. ¡El mejor precio!&quot
          </div>
          <strong>Cristian F.</strong> - <span>Audi A3</span>
        </div>
        <% } else { %>
        <div class="hidden-xs col-sm-3 text-left customer-banner__col customer-banner__col--no-border not-valid">
          <div class="customer-banner__header customer-banner__header--3">
            Hemos comprado más de 550.000 coches
          </div>
          <div class="customer-banner__quote not-valid">
            &quot;Muy buen equipo, totalmente satisfecho con la amabilidad y el servicio. ¡El mejor precio!&quot
          </div>
          <strong>Cristian F.</strong> - <span>Audi A3</span>
        </div>
        <% } %>
      </div>
    </div>
  </div>
</div>
</script>


