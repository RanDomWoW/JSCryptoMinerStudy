    <script>
        $('#loginForm').submit(function (e) {
            //отменяем стандартное действие при отправке формы
            e.preventDefault();
            var form = $(this);
            //берем из формы метод передачи данных
            var m_method = form.attr('method');
            //получаем адрес скрипта на сервере, куда нужно отправить форму
            var m_action = form.attr('action');
            //получаем данные, введенные пользователем в формате input1=value1&input2=value2...,
            //то есть в стандартном формате передачи данных формы
            var m_data = form.serialize();

            form.find('button[type="submit"]').button("loading");

            $.ajax({
                type: m_method,
                url: m_action,
                data: m_data,
                success: function (data) {
                    var json = JSON.parse(data);
                    var idHtml = json.idHtml;

                    var text = "";

                    if (idHtml["state"] == 1) {
                        $("#dialogLogin").modal("hide");
                        $("#loginTop").html("<a id='cart' class='atoplk' href='/my-orders'><i class='glyphicon glyphicon-user'></i> <span>  Личный кабинет </span></a>");
                        form.find("#responseLogin").text(text);
                    } else {
                        form.find('button[type="submit"]').button("reset");
                        form.find('#responseLogin').text(text);
                    }
                    if ($("#orderContentSpan").length > 0) {
                        $(".loadContent").addClass("active");
                        $.ajax({
                            type: 'get',
                            url: "/OrderContentSpan",
                            data: {},
                            success: function (data2) {

                                $("#orderContentSpan").html(data2);
                            }
                        });
                    }
                    OnSuccess(data);

                },
                error: function (text) {
                    buttons.button('reset');
                    form.find('#responseLogin').text("Что то пошло не так!!! Попробуйте еще раз");
                }
            });

        });
        $("#activatorWM").click(function () {
            //$("#boxWM").dialog("open");
            $("#dialogLogin").modal("show");
        });
