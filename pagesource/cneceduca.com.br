<script>
jQuery(function($){
	var embeds = $(".youtube");
	var trigger = $('.show-cnec-text-message');
	var message = $('.cnec-educacao-text');
	function goToMessage(){
		if(message.css('display') == 'none'){
			message.fadeIn("normal");
		}
		$("html, body").animate({scrollTop: 150}, "normal");
	}
	trigger.click(goToMessage);
	
	function adjust(){
		var newHeight = Math.round(embeds.width() * .5625);
		embeds.height(newHeight);
	}
	$(window).bind('resize', adjust);
	adjust();
});

jQuery(function($){
	function pageHeight(){
		return Math.max(document.documentElement.scrollHeight, document.documentElement.offsetHeight - document.documentElement.scrollTop);
	}
	
	var isMobile = navigator.userAgent.match(/ipad|iphone|ipod|android|windowsphone/i) != null;
	var repeatingBg = $('body').css('background-image').match(/repeating/)  ? true : false;
	
	function getClass(x, y){
		if(!repeatingBg && y < 3000) return 'white';
		if(!repeatingBg && y > 3000) return 'none';
		y = y % 7000;
		if(y > 1200 && y < 2000) return "blue";
		if(y > 2000 && y < 3900) return "yellow";
		if(y >= 3900 && y < 6000) return "green";
		if(y >= 6000 && y < 6500) return "blue";
		return "white";
	}
	
	var bgCount = 8;
	var isBg = true;
	var cubes = $('#cubes');
	for(var i = 0; i < 110; i++){
		var y = i * 136;
		var sin = Math.sin((y - 250) / 300);
		var x = Math.round(2 * Math.PI * 80 * sin);
		
		var box = $("<div class='box' />");
		cubes.append(box);
		box.css("top", y + "px");
		
		if(i % bgCount == 0){
			isBg = !isBg;
		} 
		
		box.addClass(getClass(x, y));
		box.data("posY", y);
		
		box.css("margin-left", x - 205 + "px");
		if(isMobile)		
			box.css("position", "absolute");
	}
	if(isMobile)
		cubes.css('height', pageHeight() + "px");
	
	//tablets and phones viewport stuff
	var viewportmeta = document.querySelector && document.querySelector('meta[name="viewport"]');
	var ua = navigator.userAgent;
	function gestureStart() {
		viewportmeta.content = "width=device-width, minimum-scale=1, maximum-scale=1";
        cubes.css('height', pageHeight() + "px");
    }
	
	function scaleFix() {
	    if (viewportmeta && /iPhone|iPad/i.test(ua) && !/Opera Mini/.test(ua)) {
		    viewportmeta.content = "width=device-width, minimum-scale=1.0, maximum-scale=1.0";
		    $(window).bind("resize", gestureStart);
	    }
	}
	scaleFix();
	
	var viewportmeta = document.querySelector && document.querySelector('meta[name="viewport"]');
	var isAndroid = (navigator.userAgent + "a").match(/android|zbov/i);
	if(isAndroid) {
		$(window).bind("resize", function(){
			viewportmeta.content = "width=device-width, maximum-scale=1.0, initial-scale=1";
		});
	}
	
	//$('.educacao-logo').click(function(){document.title = $(window).width();});
	
	if(ua.match(/opera mobi/i)){
		var topbar = $("#top-bar");
		topbar.css("position", "absolute");
		$(window).bind("scroll", function(){
			var top = $(window).scrollTop();
			topbar.css("top", (top - topbar.height()) + "px");
 			topbar.animate({"top": top}, "normal");
		});
	}
	
	//animation related to scroll stuff
	if(isMobile) return;
	var boxes = $(".box");
	var sections = $("section");
	
	function onScroll(){
		var top = -$(window).scrollTop();
		function animate(){
			var box = $(this);
			var pos = box.data("posY");
			box.stop().animate({top: pos + top}, 1000, "easeOutCubic");
		}
		boxes.each(animate);
		top *= -1;
		markSections(top, top + $(window).height());
	}
	
	function markSections(topStart, topEnd){
		function checkSection(a, b){
			var section = $(this);
			var sectionTop = section.offset().top;
			topEnd -= 50;
			
			if(sectionTop + 400 > topStart && sectionTop < topEnd){
				section.addClass("inView");
			}
			else{
				section.removeClass("inView");
			}
		}
		
		sections.each(checkSection);
	}
	
	$(window).bind('scroll', onScroll);
	onScroll();
});


//section title
jQuery(function($){
	var sections = $(".section-mark");
	var visible = false;
	var doc = $(document);
	
	var title = $('#section-title'); 
	
	function update(){
		var top = doc.scrollTop();
		var label = "";
		
		for(var i = 0; i < sections.size(); i++){
			var section = sections.eq(i);
			if(top > section.offset().top - 100)
				label = section.data("section-title");
		}
		
		if(label == ""){
			title.fadeOut("normal");
			visible = false;
			return;
		}
		
		title.html(label);
		
		if(!visible && $(window).width() > 1300){
			visible = true;
			title.fadeIn("normal");
		}
	}
	
	function resize(){
		if($(window).width() < 1300){
			title.fadeOut("normal");
			visible = false;
		}
		else{
			if(!title.html()) return;
			title.fadeIn("normal");
			visible = true;
		}
		
			
	}
	
	doc.on('scroll', update);
	$(window).on('resize', resize);
});
</script>

