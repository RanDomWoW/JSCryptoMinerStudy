<script>
jQuery(function($){
	var menu = $("#main-menu");
	function show(e){
		if(menu.css('display') == 'block'){
			hide();
			return false;
		}
		
		if($(e.target).is(".contact-nav-button")){
			return;
		}
		
		if($(e.target).is("#logo-home-link")){
			$("html, body").animate({scrollTop: 0}, "normal");
			return false;
		}
		
		var isMobile =  $(window).width() < 800  // $('#logo-home-link').css('display') == 'block';
		if(!isMobile && !($(e.target).is("a") || $(e.target).is("span"))) return false;
		
		var top = $(window).scrollTop() + (isMobile ? 67 : 100);
		menu.show();
		var padding = parseInt(menu.css("paddingLeft"));
		
		var width = menu.width() + padding;
		var left = isMobile ? 0 : ($(window).width() / 2 - width / 2);
		
		menu.css("left", left + "px");
		menu.css("top", (top - menu.height()) + "px");
		menu.css("opacity", 0);
		menu.stop().animate({opacity:1, top : top}, "normal");
		return false;
	}
	
	function hide(){
		menu.fadeOut("normal");	
	}
	
	function hideOnClickOutside(e){
		var e = $(e.target);
		if(e.is("#main-menu")) return;
		var parent = e.parentsUntil("#main-menu");
		if(parent.last().is("html")) hide();
	}
	
	function gotoSection(e){
		if(!this.href.match(/#$/)) return;
		var a = $(this);
		if(a.parent().hasClass("show-cnec-text-message")) {
			if(!$("#sections").length) location.href = "/";
			hide();
			return;
		}
		var sectionClass = "." + a.parent().attr('class');
		var section = $("section" + sectionClass);
		
		if(section.length == 0){
			location.href = "/" + sectionClass.replace(/^\./, "#");
			hide();
			return false;
		}
	
		var top = section.offset().top;
		var isMobile = $('#logo-home-link').css('display') == 'block';
		if(isMobile) top -= 40;
		else
			top -= 100;
		
		menu.fadeOut("normal", function(){
			$("html, body").animate({scrollTop: top}, "normal");
		});
		return false;
	}
	
	$(".solutions-button a").click(show);
	$("a", menu).click(gotoSection);
	$(document).click(hideOnClickOutside);
});

jQuery(function($){
	var form = $(".contact-form");
	form.find("h3 .close").click(hide);
	form.submit(submit);
			
	function submit(){
		$.ajax({
			  "url": "/contato",
			  "dataType": 'json',
			  "type": 'POST',
			  "data": form.serialize(),
			  "contentType": "application/x-www-form-urlencoded; charset=UTF-8",
			  success: onResult,
			  error: function(e){form.find(".hint").html("Erro ao acessar o servidor.");}
			});
		return false;
	}
	
	function onResult(result){
		if(result.error){
			form.find(".hint").html(result.error);
			form.find(".hint").show();
			return;
		}
		resultOk();
	}
	
	function resultOk(){
		form.find(".submit, .form-item, .reCaptcha").hide();
		form.append("<p class=\"success-message\">Mensagem enviada com sucesso!</p>");
	}
	
	function show(){
		var isMobile = $(window).width() < 800; //$('#logo-home-link').css('display') == 'block';
		var top = $(window).scrollTop() + (isMobile ? 67 : 100);
		
		form.show();
		
		var padding = parseInt(form.css("paddingLeft"));
		var width = form.width() + padding;
		var left = isMobile ? 0 : ($(window).width() / 2 - width / 2);
		
		form.css("left", left + "px");
		form.css("top", (top - form.height()) + "px");
		form.css("opacity", 0);
		form.stop().animate({opacity:1, top : top}, "normal");
		return false;
	}
	
	function hide(){
		form.fadeOut("normal");
	}
	
	function hideOnClickOutside(e){
		if (form.css('position') == 'relative') return;
		var e = $(e.target);
		if(e.is(".contact-form")) return;
		var parent = e.parentsUntil(".contact-form");
		if(parent.last().is("html")) hide();
	}
	$(".contact-nav-button, .contact-footer-button").click(show);
	$(document).click(hideOnClickOutside);
});
</script>

