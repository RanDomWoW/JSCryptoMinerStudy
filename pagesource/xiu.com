jQuery('.vCarousel,.sm_ercode').hover(function() {
    clearInterval(timer);
  }, function() {
    doTimer();
  });


  function doTimer() {
    if (vCarouselItem.length > 1) { //当有大于一个item时才设置定时器
      timer = setInterval(function() {
        updateItem();
      }, 2000);
    }
  }

  function updateItem() {
    vCarouselList.animate({
      top: '-=' + vCarouselHeight
    }, 600, function() {
      animateCount++;
      if (animateCount == total) {
        vCarouselList.css({
          top: 0
        });
        animateCount = 0;
      }
    });
  }

  // init process
  var itemClone = jQuery('.vCarousel-item').first().clone(true);
  itemClone.addClass('vCarousel-item-clone').appendTo(vCarouselList);
  doTimer();
});

