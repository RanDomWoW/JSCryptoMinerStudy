$('#emailSub').click(function(){
	var email = $('#emailAddress').val();
	if(!checkEmail(email)){
		showEmailSubTips('请输入正确的邮箱', 2); 
		return false;
	}
	submitEmailSub();
});
var submitEmailSub=function(){
   var options = {
	  beforeSubmit:function(){showEmailSubTips('数据提交中...', 2); },
	  success:function(s_json, statusTxt){
		  if(typeof s_json.result === 'undefined'){
			showEmailSubTips('系统繁忙，请稍后再试', 3);
		  }else{
			showEmailSubTips(s_json.result, 3);
		  }
	  },
	  url: url['portal'] + "xiusubscription/checktion?format=json&jsoncallback=?&timeStamp=" + (new Date().getTime()),
	  type:'POST',
	  dataType:"jsonp",
	  timeout:120000
   };
   $("#emailSubForm").ajaxSubmit(options);  
}

function checkEmail(a){
   re = /^[\w-]+(\.[\w-]+)*@[\w-]+(\.[\w-]+)+$/;
   if (!re.test(a)){
     return 0;
   }
   return 1;
}

