	xiuTipBoxDiv.html("");
	xiuTipBoxDiv.hide();
	if($('#emailSubTips').data('remove'))
		clearTimeout($('#emailSubTips').data('remove'));
	if($('body').data('showTip'))
		clearTimeout($('body').data('showTip'));
	if(document.getElementById('emailSubTips'))
		$('#emailSubTips').text("");
}

function showEmailSubTips(content,time){
	hideEmailSubTips();
	content=content?content:'';
	time=time?time*1000:3000;
	
	var xiuTipBoxDiv = $('#emailSubTips')
	xiuTipBoxDiv.html(content);
	$('body').data('showTip',setTimeout(function(){
		xiuTipBoxDiv.show().data('remove',setTimeout(function(){hideEmailSubTips()},time))
	},100))
};

</script>

