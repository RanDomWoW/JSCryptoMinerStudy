</script><script type='text/javascript'>jQuery(document).ready(function($) {

  function determineDevice() {
    if ($(window).width() > 1050) { // Desktop

      padp['currentObj'] = padp['desktop'];
      padp['currentObj']['device'] = 'desktop';

    } else if ($(window).width() > 767) { // Tablet

      padp['currentObj'] = padp['tablet'];
      padp['currentObj']['device'] = 'tablet';

    } else { // Mobile

      padp['currentObj'] = padp['mobile'];
      padp['currentObj']['device'] = 'mobile';

    }
  }

  determineDevice();

  function createActiveObject() {
    padp['currentKeysArr'] = Object.keys(padp['currentObj']);

    padp['activeKeysObj'] = {};

    for (var i = 0; i < padp['currentKeysArr'].length; i++) {
      if (padp['currentKeysArr'][i].indexOf(padp['page_type']) >= 0) {
        padp['activeKeysObj'][padp['currentKeysArr'][i]] = padp['currentObj'][padp['currentKeysArr'][i]];
      } else if (padp['currentObj']['device'] === 'mobile' && padp['page_type'] === 'scripta' && padp['currentKeysArr'][i].indexOf('article') >= 0) { // INELEGANT SOLUTION DUE TO CHANGE OF SPECS
        padp['activeKeysObj'][padp['currentKeysArr'][i]] = padp['currentObj'][padp['currentKeysArr'][i]];
      }
    }
  }

  createActiveObject();

  // Needed variables
  var adName, adDimensions, adSelector, adClass, adMarkup;
  var indexes1, indexes2, indexes3;
  var subAds = [];
  var rangeStart, rangeEnd, baseAdName;
  var wordNo, paragraphOffset, wordNoIndex, adIndex, paragraphStart;

  generateAds();
  callDFP('all');

  padp.recalculateAds = function() {
    generateAsyncAds();
  };

  function removeAds() {
    $('.adunit').each(function() {
      $(this).remove();
    });
  }

  function generateAsyncAds() {
    adName = padp['last_async_ads'].shift();
    if (padp['last_async_ads'].length > 0) {
      adClass = adNameToClass(adName);
      adDimensions = padp['last_async_dimensions'];
      adSelector = padp['last_async_selector'];
      adMarkup = adBuildMarkup(adClass, adName, adDimensions);
      $(adMarkup).insertAfter($(adSelector));
      callDFP('.' + adClass);
    }
  }

  function generateAds() {
    // Iterate through the external object
    for (var key in padp['activeKeysObj']) {
      if (!padp['activeKeysObj'].hasOwnProperty(key)) {
        continue;
      }
      if (padp['activeKeysObj'][key] !== null && typeof padp['activeKeysObj'][key] === 'object') {

        // Now we need to iterate through the internal objects that contain the juicy stuff
        for (var keyInner in padp['activeKeysObj'][key]) {
          if (!padp['activeKeysObj'][key].hasOwnProperty(keyInner)) {
            continue;
          }
          if (padp['activeKeysObj'][key][keyInner] !== null && typeof padp['activeKeysObj'][key][keyInner] === 'object') {

            adName = padp['activeKeysObj'][key][keyInner]['name'];
            adDimensions = padp['activeKeysObj'][key][keyInner]['dimensions'];
            adSelector = padp['activeKeysObj'][key][keyInner]['selector'];
            adClass = adNameToClass(adName);

            if (adName.indexOf('-') > -1) { // Sequential Ads
              // Go through the different sequential ad cases
              if (padp['currentObj']['device'] === 'desktop') { // Desktop
                // Home page does not have any special rules
                if ('home' in padp['activeKeysObj']) {
                  indexes1 = indexes2 = indexes3 = -1;
                }
                // Category page special rules
                if ('category' in padp['activeKeysObj']) {
                  indexes1 = parseInt(padp['activeKeysObj']['category_article_interval']);
                  indexes2 = parseInt(padp['activeKeysObj']['category_first_article']);
                  indexes3 = -1;
                }
                // Scripta post special rules
                if ('scripta' in padp['activeKeysObj']) {
                  indexes1 = parseInt(padp['activeKeysObj']['scripta_image_space']);
                  indexes2 = parseInt(padp['activeKeysObj']['scripta_word_space_first']);
                  indexes3 = parseInt(padp['activeKeysObj']['scripta_word_space_next']);
                }
                // Articles' special rules
                if ('articles' in padp['activeKeysObj']) {
                  indexes1 = parseInt(padp['activeKeysObj']['articles_image_space']);
                  indexes2 = parseInt(padp['activeKeysObj']['articles_word_space_first']);
                  indexes3 = parseInt(padp['activeKeysObj']['articles_word_space_next']);
                }
              } else if (padp['currentObj']['device'] === 'tablet') { // Tablet
                // Home
                if ('home' in padp['activeKeysObj']) {
                  indexes1 = indexes2 = indexes3 = -1;
                }
                // Category
                if ('category' in padp['activeKeysObj']) {
                  indexes1 = parseInt(padp['activeKeysObj']['category_article_interval']);
                  indexes2 = parseInt(padp['activeKeysObj']['category_first_article']);
                  indexes3 = -1;
                }
                // Scripta post special rules
                if ('scripta' in padp['activeKeysObj']) {
                  indexes1 = parseInt(padp['activeKeysObj']['scripta_image_space']);
                  indexes2 = parseInt(padp['activeKeysObj']['scripta_word_space_first']);
                  indexes3 = parseInt(padp['activeKeysObj']['scripta_word_space_next']);
                }
                // Articles' special rules
                if ('articles' in padp['activeKeysObj']) {
                  indexes1 = parseInt(padp['activeKeysObj']['articles_image_space']);
                  indexes2 = parseInt(padp['activeKeysObj']['articles_word_space_first']);
                  indexes3 = parseInt(padp['activeKeysObj']['articles_word_space_next']);
                }
              } else { // Mobile
                // Home
                if ('home' in padp['activeKeysObj']) {
                  indexes1 = indexes2 = indexes3 = -1;
                }
                // Category
                if ('category' in padp['activeKeysObj']) {
                  indexes1 = parseInt(padp['activeKeysObj']['category_article_interval']);
                  indexes2 = parseInt(padp['activeKeysObj']['category_first_article']);
                  indexes3 = -1;
                }
                // Scripta post special rules
                if (padp['page_type'] === 'scripta') { // Special inelegant solution because it wasn't included in specs
                  indexes1 = parseInt(padp['mobile']['articles_image_space']);
                  indexes2 = parseInt(padp['mobile']['articles_word_space_first']);
                  indexes3 = parseInt(padp['mobile']['articles_word_space_next']);
                }
                // Articles' special rules
                if ('articles' in padp['activeKeysObj']) {
                  indexes1 = parseInt(padp['activeKeysObj']['articles_image_space']);
                  indexes2 = parseInt(padp['activeKeysObj']['articles_word_space_first']);
                  indexes3 = parseInt(padp['activeKeysObj']['articles_word_space_next']);
                }
              }

              // Create the subAds array
              rangeStart = adName.match(/\d+(?=-)/);
              rangeStart = parseInt(rangeStart[0]);
              rangeEnd = adName.match(/\d+$/);
              rangeEnd = parseInt(rangeEnd[0]);
              baseAdName = adName.match(/.*(?=\d+-\d+$)/);
              baseAdName = baseAdName[0];
              subAds = [];
              for (var nameIndex = rangeStart; nameIndex <= rangeEnd; nameIndex++) {
                subAds.push(baseAdName + nameIndex);
              }
              if ($(adSelector).length == 1) { // If the selector has only one result it's an async-loaded ad
                if (typeof padp['last_async_ads'] == "undefined") {
                  padp['last_async_ads'] = subAds;
                  padp['last_async_selector'] = adSelector;
                  padp['last_async_dimensions'] = adDimensions;
                }
                if (padp['last_async_ads'].length > 0) {
                  adName = padp['last_async_ads'].shift();
                  adClass = adNameToClass(adName);
                  adMarkup = adBuildMarkup(adClass, adName, adDimensions);
                  $(adMarkup).insertAfter($(adSelector));
                }
              } else if (indexes1 === -1) { // Determine whether the first index is a valid option and act accordingly
                for (adIndex = 0; adIndex < subAds.length; adIndex++) {
                  adName = subAds[adIndex];
                  adClass = adNameToClass(adName);
                  adMarkup = adBuildMarkup(adClass, adName, adDimensions);
                  $(adMarkup).insertAfter($(adSelector).eq(adIndex));
                }
              } else if (indexes3 === -1) {
                for (adIndex = 0; adIndex < subAds.length; adIndex++) {
                  adName = subAds[adIndex];
                  adClass = adNameToClass(adName);
                  adMarkup = adBuildMarkup(adClass, adName, adDimensions);
                  $(adMarkup).insertAfter($(adSelector).eq((adIndex * indexes1) + indexes2 - 1));
                }
              } else {
                // First we determine paragraph 0 based on the "space from start" option
                wordNo = 0;
                paragraphOffset = 0;
                do {
                  wordNo += numOfWords(paragraphOffset, adSelector);
                  paragraphOffset += 1;
                } while (wordNo < indexes2); // indexes2 is the "word space first" option
                // Since we're inserting the ad AFTER the paragraph, the first paragraph that we check
                // is the one before the last paragraphOffset. For a better understanding on this, do a
                // mental run of the loop above for the case that the first paragraph has more words than
                // the "word space" parameter (note: requires 3-digit IQ).
                paragraphStart = paragraphOffset - 1;
                adElementProximityCheck(paragraphStart, indexes1, indexes3, adSelector, adDimensions, subAds);
              }

            } else { // Normal Ads
              adMarkup = adBuildMarkup(adClass, adName, adDimensions);
              $(adMarkup).insertAfter($(adSelector));
            }

          }

        } // End inner iteration

      } // End object check
    } // End outer iteration
  }

  function adElementProximityCheck(paragraphStart, imageSpace, adSpace, adSelector, adDimensions, subAds) {

    var elementsToCheck = ['figure', 'blockquote', 'div', 'img', 'iframe'];
    var elementIndex, elementStatus;
    var paragraphs, paragraphCount, paragraphIndex, lastAdIndex, wordCount, interAdIndex;
    var imageParagraphIndexMin, imageParagraphIndexMax;

    lastAdIndex = -1;
    paragraphs = $(adSelector);
    paragraphCount = paragraphs.length;

    if (paragraphs.length < 2) {
      return;
    }

    // Add a class to paragraphs that contain skippable elements
    for (paragraphIndex = paragraphStart; paragraphIndex < paragraphs.length-2; paragraphIndex++) {
      var tempParagraphElement = $(adSelector).eq(paragraphIndex);
      for (elementIndex = 0; elementIndex < elementsToCheck.length; elementIndex++) {
        if (tempParagraphElement.has(elementsToCheck[elementIndex]).length) {
          tempParagraphElement.addClass('hasSkippableElement');
          break;
        }
      }

    }

    // ... now add the skippable selector in the elements to skip
    elementsToCheck.push('.hasSkippableElement');

    // The index will be used to place the ads so we'll need to start counting from paragraphStart, not 0
    for (paragraphIndex = paragraphStart; paragraphIndex < paragraphs.length-2; paragraphIndex++) {

      // Instantiate the status variable
      elementStatus = "active";
      // Everything we do, we do it (... for you, ha ha) if there are ads left in our array
      if (subAds.length > 0) {
        // Ad space check comes before image space check because it runs faster.
        // AD SPACE CHECK
        if (lastAdIndex !== -1) { // the check is meaningful if there is a previous ad to check
          wordCount = 0;
          for (interAdIndex = lastAdIndex + 1; interAdIndex < paragraphIndex + 1; interAdIndex++) {
            wordCount += numOfWords(interAdIndex, adSelector);
          }
          if (wordCount < adSpace) {
            continue; // One condition is not met, no condition is met. Get out now!
          }
        }
        // IMAGE SPACE CHECK
        // (it checks for all the elements in the elementsToCheck array but "image" is shorter)
        // First we need to determine our boundaries
        // -= Lower Boundary =-
        imageParagraphIndexMin = paragraphIndex;
        wordCount = 0;
        do {
          if (imageParagraphIndexMin < 0) {
            imageParagraphIndexMin = 1;
            break; // Negative indexes need to be excluded.
          }
          wordCount += numOfWords(imageParagraphIndexMin, adSelector);
          imageParagraphIndexMin -= 1;
        } while (wordCount < imageSpace);
        imageParagraphIndexMin += 1;
        // -= Upper Boundary =-
        imageParagraphIndexMax = paragraphIndex + 1;
        wordCount = 0;
        do {
          if (imageParagraphIndexMax >= paragraphCount) {
            break; // Length zero means that the p does not exist
          }
          wordCount += numOfWords(imageParagraphIndexMax, adSelector);
          imageParagraphIndexMax += 1;
        } while (wordCount < imageSpace);
        imageParagraphIndexMax -= 1;

        // ... and now the MAGIC happens!
        for (elementIndex = 0; elementIndex < elementsToCheck.length; elementIndex++) {
          if ($(adSelector).eq(imageParagraphIndexMin).nextUntil($(adSelector).eq(imageParagraphIndexMax)).filter(elementsToCheck[elementIndex]).length > 0){
            elementStatus = 'inactive';
          }
        }
        if (elementStatus === 'inactive') {
          continue;
        }
        // CREATE AD
        // Since we reached this point, it means that the current paragraph index is eligible for an ad placement,
        // so let's create the ad after updating the adIndex
        lastAdIndex = paragraphIndex;
        // Let's build our ad
        adName = subAds[0];
        adClass = adNameToClass(adName);
        adMarkup = adBuildMarkup(adClass, adName, adDimensions);
        // ... and don't forget to remove the item that we have just created from the array
        subAds.shift();
        // Finally, we're inserting the ad
        $(adMarkup).insertAfter($(adSelector).eq(paragraphIndex));
      } // subAds Array Size Check
    } // paragraph Loop

  }

  function numOfWords(pNo, adSelector) {

    return $(adSelector).eq(pNo).text().split(" ").length;

  }

  function adNameToClass(adName) {

    return adName.replace(/_/g, "-");

  }

  function adBuildMarkup(adClass, adName, adDimensions) {

    var currentDevice = padp['currentObj']['device'];

    var $output = '';

    $output += '<div class="adunit ' + adClass;
    $output += ' ad-device-' + currentDevice;
    $output += '" data-adunit="' + adName;
    $output += '" data-dimensions="' + adDimensions;
    $output += '"></div>';

    return $output;
  }

  function adResizer(state) {

    if (padp['currentObj']['device'] !== 'mobile') {

      var containerElement, iframeElement;

      if (state === 'consequent') {
        $('.adunit').each(function () {
          containerElement = $(this);
          iframeElement = $(this).children(":first").children("iframe");
          adResizeCalculations(containerElement, iframeElement);
        });
      } else {
        state.children(":first").children(":first").load(function() {
          containerElement = $(this).parent().parent();
          iframeElement = $(this);
          adResizeCalculations(containerElement, iframeElement);
        });
      }
    }

  }

  function adResizeCalculations (adElement, iframeElement) {
    var adContainerWidth = adElement.width();
    var adIframeWidth = parseInt(iframeElement.attr('width'));
    var adContainerHeight = adElement.height();
    var adIframeHeight = parseInt(iframeElement.attr('height'));
    if (adContainerWidth <= adIframeWidth) {
      var adScale = adContainerWidth / adIframeWidth;
      var calculatedHeight = iframeElement.height() * adScale;
      var heightDifference = (adIframeHeight > adContainerHeight) ? adIframeHeight - adContainerHeight : adContainerHeight - adIframeHeight;
      heightDifference += 20;
      adElement.css('transform', adScaleMarkupBuilder(adScale));
      adElement.css('transform-origin', adScaleMarkupBuilder(0));
      adElement.children(":first").width(adIframeWidth);
      adElement.height(calculatedHeight);
      adElement.css('margin-bottom', heightDifference + 'px');
    }
  }

  function adScaleMarkupBuilder(scale) {
    if (scale !== 0) {
      return 'scale(' + scale + ') translate3d(0px, 0px, 0px)';
    } else {
      return '0px 0px 0px';
    }
  }

  function callDFP(type) {
