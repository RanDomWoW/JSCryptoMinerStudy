  function stickyResize(adUnit, metricsObj) {

    // No need to do anything funky here. Just calculate the metrics object new values based on
    // the new offsets and heights of elements
    metricsObj['topLimit'] = adUnit.parent().offset().top;
    metricsObj['entryContent'] = adUnit.parent().parent();
    metricsObj['adHeight'] = adUnit.outerHeight(true);
    metricsObj['bottomLimit'] = metricsObj['entryContent'].offset().top + metricsObj['entryContent'].outerHeight(true) - metricsObj['adHeight'] - 40;
    metricsObj['adAbsoluteTop'] = metricsObj['bottomLimit'] - adUnit.parent().offset().top;
    metricsObj['adFixedLeft'] = adUnit.parent().offset().left;

    stickyScroll(adUnit, metricsObj);

    // No need to return anything either since the objects are passed by reference (remember?)

  }

