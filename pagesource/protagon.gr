  function stickyHandling(adUnit) {

    // We are using an object instead of plain variables so that we can pass it by reference
    // in the resize function and give it the option to alter the values.
    // The alternative would be to pollute the global namespace which is bad. Really bad. Apple Maps bad.
    var metricsObj = {};

    // Why use the parent element of the adUnit instead of the adUnit itself, you say?
    // Very good question!
    // Since we're recalculating everything on each resize of the window, we need to base our
    // calculations on something that is always sitting at the same spot, not a wild-card that is moving
    // around all the time. Otherwise it would be like building castles on the sand. Literally.
    // Well... not literally. But you get my point.
    metricsObj['topLimit'] = adUnit.parent().offset().top;

    // @todo: or not to do the fix you say, the parent needs to move away oh yay
    metricsObj['entryContent'] = adUnit.parent().parent();
    metricsObj['adHeight'] = adUnit.outerHeight(true);
    metricsObj['bottomLimit'] = metricsObj['entryContent'].offset().top + metricsObj['entryContent'].outerHeight(true) - metricsObj['adHeight'] - 40;
    metricsObj['adAbsoluteTop'] = metricsObj['bottomLimit'] - adUnit.parent().offset().top;
    metricsObj['adFixedLeft'] = adUnit.parent().offset().left;

    // Even when we start at the middle of the page (i.e. new Google Chrome "pick up where
    // you left off" policy) we need to make sure that the sticky item is where it is supposed to be
    stickyScroll(adUnit, metricsObj);

