                hash = hash.substr(1).replace('show-post-window', '');
                var params = hash.split('~');
                listener.trigger('com.neihanshequ', 'showPost', params);
            }

            setTimeout(loadLazyImage, 400);
            $(document.body).find('.timeago').timeago();

            var onClosePopup = function(event) {
                $(event.currentTarget).parents('.popup-window-wrapper').hide();
            };
            $('.popup-window-wrapper .close').click(onClosePopup);
            $('.popup-window-wrapper .mask').click(onClosePopup);
            formatDurationItems();

            var $backTopBtn = $('#backTopBtn');
            $backTopBtn.click(function() {
                window.scrollTo(0, 0);
            });

            $(window).bind('scrollEnd', function(event, data) {
                var t = (data || event._args || {scrollTop: 0}).scrollTop;
                if(t > 600) {
                    $backTopBtn.show();
                }else if(t < 100) {
                    $backTopBtn.hide();
                }
            });

            $('.logout').click(function(event) {
                $.ajax({
                    url: '/sso/auth/logout/'
                }).done(function() {
                    // 调用sso的logout
                    location.href = 'https://sso.toutiao.com/logout/?service=' + location.href;
//                    $.ajax({
//                        url: 'https://sso.toutiao.com/logout/?service=' + location.href,
//                    }).done(function (res) {
//                        // 调用结束
//                        console.log(res)
//                    });
//                    listener.trigger('com.neihanshequ.user', 'change', null);
                })
                event.stopPropagation();
                event.preventDefault();
                return false;
            });

        });

        initScrollEvents();
    </script>


