            <script type="text/javascript">
                (function($) {
                    // Back to top functionality
                    $(function() {
                        $('.backToTopBtn').on('click', function() {
                            $("html, body").animate({scrollTop: 0}, 500);
                            return false;
                        });

                        var $backToTopContainer = $('#backToTop');
                        var $backToTop = $('.backToTopBtn');
                        var $homeBanner = $('.promo-banner');

                        var $body = document.body,
                          $html = document.documentElement;
                        var $pageHeight = Math.max( $body.scrollHeight, $body.offsetHeight,
                                             $html.clientHeight, $html.scrollHeight, $html.offsetHeight );
                        var $windowHeight = $(window).height();
                        var $featuredLinksListItems = $('.featured-links__list-item a');

                        // console.log($backToTop);
                        // console.log('$windowHeight: ' + $windowHeight);
                        // console.log('$pageHeight: ' + $pageHeight);

                        if ($pageHeight > $windowHeight * 2.45) {
                            // console.log('show back to top');
                            $backToTop.fadeIn(200);
                            $backToTopContainer.addClass('show');
                            // need to check if it's a home page and there are 6 items
                            if ($homeBanner.length && $featuredLinksListItems.length > 5) {
                                $backToTopContainer.addClass('extra-pad');
                            } else if ($featuredLinksListItems.length > 5) {
                                $backToTopContainer.addClass('extra-pad');
                            }
                        } else {
                            // console.log('page not that tall - do not show back to top');
                        }
                    });
                })(jQuery);
            </script>

