<script>
(function() {
  function track(selector, category, action, opt_label, opt_value, opt_noninteraction) {
    var links = document.querySelectorAll(selector);
    var origin = window.location.protocol + '//' + window.location.hostname;
    function handleLinkClick(e) {
        if (window.ga) {
        e.preventDefault();
        var href = e.currentTarget.href.replace(origin, '');

        ga('send', 'event', {
            eventCategory: category, eventAction: action, eventLabel: href, eventValue: opt_value, nonInteraction: opt_noninteraction, 'hitCallback': function () {
                document.location = href;
            }
        });

      }
    }
    for (var i = 0, l = links.length ; i < l ; i++) {
      links[i].addEventListener('click', handleLinkClick);
    }
  }
  window.wbcLinkTracker = track;
})();
</script>

