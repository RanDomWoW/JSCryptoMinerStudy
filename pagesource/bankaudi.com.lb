<script type="text/javascript">
    function validEmail(v) {
        var r = new RegExp("[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?");
        return (v.match(r) == null) ? false : true;
    }

    $("#mailTextBox").on('keyup', function (e) {
        if (e.keyCode == 13) {
            subscribeMail();
        }
    });

    function showSubscribeAudiLoader() {
        //$('#mailSubmitionSection').css("display", "none");
        //$('#mailSubmitionSection').find('input:text').css("display", "none");
        $('#mailSubmitionSection').children().hide();
        $('#loaderSubsC').css("display", "block");
    }
    function hideSubscribeAudiLoader() {
        //$('#mailSubmitionSection').css("display", "block");
        $('#mailSubmitionSection').children().show();
        $('#loaderSubsC').css("display", "none");

    }
    function subscribeMail() {
        if ($('#mailTextBox').val() != "" && validEmail($('#mailTextBox').val())) {
            showSubscribeAudiLoader();
            var email = $('#mailTextBox').val();
            var link = '/SubDomain/Site/subscribeAudiMail';
            $.ajax({
                url: link,
                type: "post",
                data: { Email: email },
                success: function (response) {
                    if (response == "0") {
                        $('#subscribeMessage').text("Thank you. You are now registered to our mailing list");
                        //$('#subscribeMessage').toggleClass('errorMessage');
                        //if ($('#subscribeMessage').is('.errorMessage')) {
                        //    $('#subscribeMessage').removeClass('errorMessage');
                        //}
                        $('#subscribeMessage').addClass('successMessage');
                    } else {
                        $('#subscribeMessage').text("The email address you provided is already registered in our mailing list");
                        if ($('#subscribeMessage').is('.successMessage')) {
                            $('#subscribeMessage').removeClass('errorMessage').removeClass('successMessage');
                        }
                        //$('#subscribeMessage').toggleClass('successMessage');
                        $('#subscribeMessage').removeClass('successMessage').addClass('errorMessage');
                    }
                    hideSubscribeAudiLoader();
                    $('#mailTextBox').val("");
                    //$('#pageContents').empty();

                    //$('#pageContents').html(response)
                    //alert(response);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $('#subscribeMessage').text("An error has occurred, please try again later");
                    $('#subscribeMessage').removeClass('successMessage').addClass('errorMessage');
                    hideSubscribeAudiLoader();
                    console.log(textStatus, errorThrown);
                }
            });
        }
        else {
            $('#subscribeMessage').text("Please fill in a valid email address");
            $('#subscribeMessage').removeClass('successMessage').addClass('errorMessage');
            hideSubscribeAudiLoader();
        }
    }

