    });
    function getSelectors(parentId, selectedId) {
        var sectionId = '15';
        var dataModel = {
            parentId: parentId,
            sectionId: sectionId,
            selectedId: selectedId
        }
        dataModel.__RequestVerificationToken = $(':input[name="__RequestVerificationToken"]').val();
        var link = '/SubDomain/Site/GetHomeSelector';
        $.ajax({
            url: link,
            type: "post",
            data: dataModel,
            success: function (response) {
                $('#servicesContents ul').empty().hide().append(response.Data).fadeIn(1500);
                $("#selectorBack").show();
