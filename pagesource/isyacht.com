<script>
$(function(){
	$(".tabs_main .tab li").mouseover(function(){
		$(this).addClass('on').siblings().removeClass('on');
		var index = $(this).index();
		number = index;
		$('.tabs_main .content .tabs_con').hide();
		$('.tabs_main .content .tabs_con:eq('+index+')').show();
	});
	
	var auto = 1;  //等于1则自动切换，其他任意数字则不自动切换
	if(auto ==1){
		var number = 0;
		var maxNumber = $('.tabs_main .tab li').length;
		function autotab(){
			number++;
			number == maxNumber? number = 0 : number;
			$('.tabs_main .tab li:eq('+number+')').addClass('on').siblings().removeClass('on');
			$('.tabs_main .content .tabs_con:eq('+number+')').show().siblings().hide();
		}
		var tabChange = setInterval(autotab,5000);
		//鼠标悬停暂停切换
		$('.tabs_main').mouseover(function(){
			clearInterval(tabChange);
		});
		$('.tabs_main').mouseout(function(){
			tabChange = setInterval(autotab,5000);
		});
	  }
});
</script> 
