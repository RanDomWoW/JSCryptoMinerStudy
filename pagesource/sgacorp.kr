<script>
initCGetCook();

function getCookie(key) {
   var cookie = document.cookie;
   var first = cookie.indexOf(key+"=");
   if (first>=0) {
       var str = cookie.substring(first,cookie.length);
       var last = str.indexOf(";");
       if (last<0) {
           last = str.length;
       }
       str = str.substring(0,last).split("=");
       return unescape(str[1]);
   } else {
       return null;
   }
}

function initCGetCook(){
	var cookTemp = getCookie("sgaweb")
	if(cookTemp==null){
		document.getElementById("webc_frame").contentWindow.location="/inc/webc.jsp";
	}
}

function setCCookie()
{
	var todayDate = new Date();
	todayDate.setDate( todayDate.getDate() + 1 );
	document.cookie = "sgaweb=kosga; path=/; expires=" + todayDate.toGMTString() + ";"
}
</script>
