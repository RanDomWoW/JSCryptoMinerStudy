                $("#verresultadosenc2").attr('href', '/preguntas/view/' + arrey[0].Pregunta + '/oculto/' + arrey[0].Mensaje + '/?iframe=true&width=700&height=100%');
                $("#verresultadosenc2").trigger("click");
            }
        });

        return false; // Evitar ejecutar el submit del formulario.
    }
