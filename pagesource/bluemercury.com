  <script id="cartTemplate" type="text/template">
  
    <div class="modal-arrow"></div>

    {{#if mixedOrderWarning }}
      {{{mixedOrderWarning}}}
    {{/if}}

    {{#if items.[0] }}
    <form action="/cart" method="post" novalidate>
      <div class="ajaxifyCart__header">
        <h3>ADDED <span class="italic">to bag<span></h3>
        <button class="close-modal-btn">Close <span class="icon icon-close"></span></button>
      </div>
      <div class="ajaxifyCart__products">
        {{#items}}
        <div class="ajaxifyCart__product">
          <div class="ajaxifyCart__row" data-id="{{id}}">
            <div class="grid">
              <div class="grid__item one-whole">
                <div class="grid--full">
                  <div class="grid__item one-quarter">
                    <a href="{{url}}" class="ajaxCart__product-image"><img src="{{img}}" alt=""></a>
                  </div>
                  <div class="grid__item one-half">
                    <p class="brand">{{brand}}</p>
                    <a href="{{url}}" class="product-title">{{name}}</a>
                    <p> {{price}} </p>
                    <p class="sku">SKU: {{sku}}</p>
                    <p>{{variation}}</p>
                    <div class="ajaxifyCart__qty">
                      <span class="ajaxifyCart__num" data-id="{{id}}">qty: {{itemQty}}</span>
                    </div>
                  </div>
                  <div class="grid__item one-quarter text-right">
                    <p class="price">{{linePrice}}</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        {{/items}}
      </div>
      <div class="ajaxifyCart__row text-right ajaxifyCart__footer">

        <div class="grid--full">
          <div class="grid__item one-half small--one-quarter">
          </div>
          <div class="grid__item one-half small--three-quarters text-right">
            <div class="grid--full cart-total-line">
              <div class="grid__item one-half cart-total-label">
                subtotal:
              </div>
              <div class="grid__item one-half cart-total-value">
                {{totalPrice}}
              </div>
            </div>
          </div>
        </div>
        <p><em>{{promotionMessage}}</em></p>
        <div class="grid--full">
          <div class="grid__item one-half text-left">
            <a href="#" class="close-modal-btn">Continue Shopping</a>
          </div>
          <div class="grid__item one-half">
            <a href="/cart" class="{{btnClass}}">View Bag</a>
          </div>
        </div>
      </div>
    </form>

    {{else}}
      <h2 class="empty-mini-cart">Your cart is empty</h2>
    {{/if}}

    <div class='mini-cart-shipping-message'>
      {{{shippingMessage}}}
    </div>
  
  </script>

  <script id="drawerTemplate" type="text/template">
  
