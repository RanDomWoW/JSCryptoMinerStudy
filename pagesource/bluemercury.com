<script id="HeaderNavTemplate" type="text/template">
    
    <div class="nav-dropdown flyout {{this.styles}}">
      {{#if this.touch}}
      <div>
        <div class="close-drop">
            <a title="Close navigation" class="close-nav-button"><span>Close<span class="x">&times;</span></span></a>
        </div>
      </div>
      {{/if}}
      <div class="flyout-container">
        <div class="grid--full">
          <div class="grid__item medium--two-thirds large--two-thirds nav-items">

            <div class="grid">
              {{#each this.links}}
              {{#if @index }}
                {{#is-divisible @index 3}}
                  </div>
                  <div class="grid">
                {{/is-divisible}}
              {{/if}}
              <div class="grid__item medium--one-third large--one-third nav-item">
                <a href="{{this.href}}" title="{{this.title}}">
                  <img src="{{this.image}}"><br/>
                  {{this.title}}
                </a>
              </div>
              {{/each}}
            </div>
            <div class="grid">
              <div class="grid__item one--whole view-all">
                <a href="/collections/{{this.for}}">View All {{this.category}} Categories</a>
              </div>
            </div>
          </div>
          {{#if this.featuredItem}}
          <div class="grid__item medium--one-third large--one-third featured-product">
            {{#with this.featuredItem }}
              {{> productItemTemplate }}
            {{/with}}
          </div>
          {{/if}}
          {{#if this.brands}}
          <div class="grid__item medium--one-whole large--one-whole brands">
            <h2 class="sub-header"><span>Featured<i>brands</i></span><a href="/pages/all-brands" title="View all Brands" class="view-all">View All</a></h2>
            <div class="grid">
              {{#each this.brands}}
              <div class="grid__item medium--one-fifth large--one-fifth nav-brand">
                <a href="{{this.href}}" title="{{this.title}}">
                  <img src="{{this.image}}"><br/>
                </a>
              </div>
              {{/each}}
            </div>
          </div>
          {{/if}}
        </div>
      </div>
    </div>
    
</script>

