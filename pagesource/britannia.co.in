    <script type="text/javascript" src="http://britannia.co.in/js/slick.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            var siteUrl = 'http://britannia.co.in/';
            var winWidth = $(window).width();
            var slideHeight = $('.home_div1').height();
            var steps = $('.home_div1').length - 1;
            $("#slider").css("height", slideHeight + "px");
            var currentSlide = 0;
            var clicked = false;
            var slideTime;
            var current_scroll_top;
            current_scroll_top = $(document).scrollTop();
            if (current_scroll_top > 390) {
                $('#scroll_change').attr('src', siteUrl + 'images/scroll-up.png');
            }
            else {
                $('#scroll_change').attr('src', siteUrl + 'images/scroll.png');
            }

            $(".scroll").click(function (e) {
                e.preventDefault();
                current_scroll_top = $(document).scrollTop();
                if (current_scroll_top > 390) {
                    $('#scroll_change').attr('src', siteUrl + 'images/scroll-up.png');
                    $('html, body').animate({ scrollTop: 0 }, "slow");
                }
                else {
                    $('#scroll_change').attr('src', siteUrl + 'images/scroll.png');
                    $('html, body').animate({ scrollTop: 400 }, "slow");
                }
            });

            $(document).scroll(function () {
                current_scroll_top = $(document).scrollTop();
                if (current_scroll_top > 390) {
                    $('#scroll_change').attr('src', siteUrl + 'images/scroll-up.png');
                }
                else {
                    $('#scroll_change').attr('src', siteUrl + 'images/scroll.png');
                }
            });

            $(window).resize(function () {
                winWidth = $(window).width();

                if ($(window).width() != winWidth) {
                    loadBannerImgs();

                    var currentSlideHeight = slideHeight;
                    var currentRunningSlider = currentSlide;
                    var currentSize = currentRunningSlider * currentSlideHeight;
                    $('.home_div1').removeAttr('style');
                    slideHeight = $('.home_div1').height();
                    //console.log(slideHeight);
                    if (winWidth < 600) {
                        //slideHeight = 540;
                        clearTimeout(slideTime);
                        $(".home_banner").css("margin-top", "0px");
                        $('.home_banner').slick({
                            autoplay: true,
                            slidesToShow: 1,
                            slidesToScroll: 1,
                            autoplaySpeed: 3000,
                            prevArrow: '<img src="images/left_swipe.png" class="left_swipe" />',
                            nextArrow: '<img src="images/left_swipe.png" class="right_swipe" />',
                            appendArrows: $('#slidesNav'),
                            mobileFirst: true
                        });
                    }
                    else {
                        if ($('.home_div1').hasClass('slick-slide')) {
                            $('.home_banner').slick('unslick');
                        }
                        clearTimeout(slideTime);
                        slideTime = setTimeout(AutoSlide, 5000);
                    }
                    if (slideHeight > currentSlideHeight) {
                        var newsize = slideHeight * (currentRunningSlider);
                    } else {
                        var newsize = slideHeight * (currentRunningSlider - 1);
                    }
                    steps = $('.home_div1').length;
                    $("#slider").css("height", slideHeight + "px");
                    $(".home_div1").css("height", slideHeight + "px");
                    $(".home_banner").css("margin-top", "-" + newsize + "px");
                }
            });

            loadBannerImgs();

            $(window).load(function () {
                slideHeight = $('.home_div1').height();
                //console.log(slideHeight);
                if (winWidth <= 600) {
                    clearTimeout(slideTime);
                    //  $(".home_banner").css("margin-top", "0px");
                    $('.home_banner').slick({
                        autoplay: true,
                        slidesToShow: 1,
                        slidesToScroll: 1,
                        autoplaySpeed: 3000,
                        prevArrow: '<img src="images/left_swipe.png" class="left_swipe" />',
                        nextArrow: '<img src="images/left_swipe.png" class="right_swipe" />',
                        appendArrows: $('#slidesNav'),
                        mobileFirst: true

                    });
                }
                else {
                    if ($('.home_div1').hasClass('slick-slide')) {
                        $('.home_banner').slick('unslick');
                    }
                    clearTimeout(slideTime);
                    slideTime = setTimeout(AutoSlide, 5000);
                }
                steps = $('.home_div1').length - 1;
                $("#slider").css("height", slideHeight + "px");
                $(".home_div1").css("height", slideHeight + "px");



            });

            function loadBannerImgs() {
                //var i = 0;
                //var el=$('.home_div1 > img').length;
                winWidth = $(window).width();
                $('.home_div1 > img,.home_div1 > a > img').each(function () {
                    var imageSrc = $(this).attr('src');

                    if (winWidth <= 600) {
                        if (imageSrc.indexOf("-m.jpg") > -1) {
                            imageSrc = imageSrc.replace('-m.jpg', '.jpg');
                        }
                        else {
                            imageSrc = imageSrc.replace('.jpg', '-m.jpg');
                        }
                    }
                    else {
                        imageSrc = imageSrc.replace('-m.jpg', '.jpg');
                    }
                    $(this).attr('src', imageSrc);
                    //i++;
                    //console.log(i + ', ' + el);
                    //if (i == el) {
                    //    $('.home_div1').removeAttr('style');
                    //    slideHeight = $('.home_div1').height();
                    //    console.log(slideHeight);
                    //}
                });
            }

            function AutoSlide() {
                if (currentSlide < steps) {
                    currentSlide++;
                    $(".wround:eq(" + currentSlide + ")").trigger("click");

                } else {
                    currentSlide = 0;
                    $(".wround:eq(" + currentSlide + ")").trigger("click");
                }
                slideTime = setTimeout(AutoSlide, 5000);
            }

            //side nav click slide starts here
            $('.wround').click(function (e) {
                e.preventDefault();
                if (!$(this).hasClass("active4")) {
                    $(".wround").removeClass("active4");
                    $(this).addClass("active4");
                    currentSlide = $(this).index();
                    slideOnClick();
                }
            });
            //side nav click slide ends here

            function slideOnClick() {
                if (!clicked) {
                    var multiPly = 0;
                    clicked = true;
                    if (currentSlide == 0) {
                        multiPly = 0;
                    } else {
                        multiPly = slideHeight * (currentSlide);
                    }
                }
                var tslide = currentSlide;
                $(".home_banner").stop(true, true).animate({ marginTop: "-" + multiPly + "px" }, "slow", function () {
                    $(".move").fadeOut();
                    $(".banner" + (tslide)).find(".move").fadeIn(2000);
                });
                clicked = false;
            }
        });
    </script>
  
       
