function scrollToTop(o){const l=window.scrollY,n=Math.PI/(o/15),c=l/2;var r,t=0,s=setInterval(function(){0!=window.scrollY?(t+=1,r=c-c*Math.cos(t*n),window.scrollTo(0,l-r)):clearInterval(s)},15)}
