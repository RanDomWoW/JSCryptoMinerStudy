<script>
;(function($, window, document, undefined) {
    function PosScroll() {
        
        this.btnScroll = $('.gnav > li');
        this.poscontents = $('.js-scroll-pos');
    }
    PosScroll.prototype = {
        _init: function() {
            var _this = this;
            // click for android: because To prevent reaction of the other link
            _this.btnScroll.on('click', function(e) {
                var i = $(this).index(),
                    pos = _this.poscontents.eq(i).offset().top;

                $('html, body').animate({
                    scrollTop : pos
                }, {
                    duration: 'slow',
                    queue: false
                });
            });
        }
    };
    window.posScroll = new PosScroll;
    window.posScroll._init();

})(jQuery, this, this.document);
</script>

