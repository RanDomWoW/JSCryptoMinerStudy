<div class="lineSpace"></div>
<script>
    $(document).ready(function() {
        $.ajax({
            dataType: "json",
            url: "http://sports-weather.com/api/requestPoi.php?token=GX8d2t4BPg/QwK9Dnmyvjbzh&type=WAVES",
            xhrFields: {
                withCredentials: true
            }
        }).done(function(data) {
            var tab = data.apiDatas;
            var nbSpots = 0;
            for(var i = 0; i < tab.length; i++)
            {
                var tableau = tab[i];
                for(var j = 0; j < tableau.pois.length; j++)
                {
                    nbSpots += 1;
                }
            }
            
            $('#spots').append($('<option>', {
                    value: "",
                    text: "Choisir mon spot (" + nbSpots + " disponibles)"
                }));

            for(var i = 0; i < tab.length; i++)
            {
                var tableau = tab[i];
                $('#spots').append($('<optGroup>', {
                    label: tableau.region
                }));
                
                for(var j = 0; j < tableau.pois.length; j++)
                {
                    var spot = tableau.pois[j];
                    $('#spots').append($('<option>', {
                        value: "/meteo/?latitude=" + spot.latitude + "&longitude=" + spot.longitude,
                        text: spot.name + " (" + spot.city + ")"
                    }));
                }
            }
        })
    });
</script>

