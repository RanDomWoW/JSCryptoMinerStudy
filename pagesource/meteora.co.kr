	});

		var pops = $(".mainpopup");
	$.each(pops, function(i,e){
		var idx = $(e).find(".popup_idx").val();
		var key = "mopopup_"+idx;

		var cookie = getCookie(key);

		
		if (cookie == 'done') {
		} else {
			$(e).show();
		}
		
	});
		
		
	
	
});

