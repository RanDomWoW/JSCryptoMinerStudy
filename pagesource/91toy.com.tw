	var search = $('input[type="search"]').val();
	$('input[type="search"]').val(encodeURI(search));
}

function FBlogin(nurl){
	var uri;
	if(nurl == '' || typeof(nurl) == "undefined"){
		uri = window.location.href;
	}else{
		uri = nurl;
	}
	var Islogin = $('#Islogin').val();
	if(Islogin == 1){
		parent.location.href = uri;
	}else{
		parent.layer.open({
			skin: 'left-class',
