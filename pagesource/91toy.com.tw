function serial_mycard(){

	var Islogin = $('#Islogin').val();
	if(Islogin == 1){
		parent.layer.prompt({
			formType: 0,
			title: '請輸入序號',
			icon:1,
			closeBtn: false,
			btn: ['確認','取消'],
			shadeClose: false
		},function(val){
			parent.layer.load(2,{shade: [0.8,'#000']});
			$.ajax({	
				type: 'POST',
				data: 'serial_no='+val,
				url:'http://www.91toy.com.tw/activity/serial_action',
				success:function(data){
					parent.layer.closeAll('loading');
					var str = new Array();
					str = data.split('|');
					if(str[0] == '0'){
						parent.layer.open({
							title: '訊息',
							content: str[1],
							closeBtn: false,
							icon:1,
							btn: ['確認','取消'],
							shadeClose: false,
							yes: function(){
								parent.layer.closeAll();
								parent.location.href = str[2];
							},
							cancel: function(){
								parent.layer.closeAll();
							}
						});
					}else{
						parent.layer.closeAll();
						parent.layer.alert(str[1], {title: '訊息',icon:1,shadeClose: false,closeBtn: false,btn: ['確認']}, function(index){
							window.location.reload();
						});
					}
				}
			});
		}); 
	}else{
		var uri;
		uri = window.location.href;
		parent.layer.open({
			skin: 'left-class',
			title: "會員登入",
				content: "您好!歡迎來到蛋鋪～<br>請點使用FB帳號登入<br>體驗更豐富的蛋舖世界!",
			closeBtn: false,
			icon: 1,
			area: ['400px'],
			btn: ['使用facebook登入','取消'],
			shadeClose: false,
			yes: function(){
				//parent.location.href = "http://www.91toy.com.tw/facebook/examples/example.php?nurl="+uri;
				FB.login(function(response){
				  checkLoginState(uri);
				});
			},
			cancel:  function(){
				parent.layer.closeAll();
			}
		});
	}
}
