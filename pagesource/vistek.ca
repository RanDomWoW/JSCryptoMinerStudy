    $(function () {
        $('.cartlinks_signin').magnificPopup({
            type: 'inline',
            prependTo: document.forms[0],
            removalDelay: 300,
            mainClass: 'mfp-fade'
        });
        $('.cartlinks_signin_close').on('click', function () {
            $.magnificPopup.close();
        });
        $('.menu-login-switch').on('click', function () {
            if ($('#menu_login_register').is(':visible')) {
                // Switch labels
                $('#menu_login_switch_button span.label-alt').fadeOut(function () { $('#menu_login_switch_button span.label-main').fadeIn(); });
                // Copy values over if exist.
                if ($('.login-register-email').val() != "" && $('.login-signin-email').val() == "") { $('.login-signin-email').val($('.login-register-email').val()); }
            } else {
                $('#menu_login_switch_button span.label-main').fadeOut(function () { $('#menu_login_switch_button span.label-alt').fadeIn(); });
                // Copy values over if exist.
                if ($('.login-signin-email').val() != "" && $('.login-register-email').val() == "") { $('.login-register-email').val($('.login-signin-email').val()); }
            }
            $('#menu_login_form').slideToggle();
            $('#menu_login_register').slideToggle({ 'complete': function () { $('#menu_login input').placeholder().blur(); } });
        });
        $('#menu_login input').placeholder();
        $('.login-register-enews-label').on('click', function () {
            $(this).toggleClass('checkbox-selected');
            $('.login-register-enews input[type=checkbox]').prop('checked',  $(this).hasClass('checkbox-selected'));
        });

    });
</script>
