<script type="text/javascript">
	var results = [];
	var responses = 0;
	var numberOfResponses = 2;
	$("#recent-articles-homepage").empty();

	$.ajax({
      url: '/blogs/blog/tagged/homepage?view=json',
		type: 'GET',
		dataType: 'json',
		success: handleResponses
	}).done(function() {}).fail(function() {}).always(function() {});

	$.ajax({
		url: '/blogs/features/tagged/homepage?view=json',
		type: 'GET',
		dataType: 'json',
		success: handleResponses
	}).done(function() {}).fail(function() {}).always(function() {});


	function handleResponses(data) {
		results.push(data);
		responses++;
		if (responses >= numberOfResponses) {
			printResults();
		}
	}

	function printResults() {
		var latestArticles = [];
		for (i = 0; i < results[0].results.length; i++) {
			latestArticles.push(results[0].results[i]);
		}
		for (i = 0; i < results[1].results.length; i++) {
			latestArticles.push(results[1].results[i]);
		}

        // Don't reimplement sort functions, use Array.sort and a comparator.
		latestArticles.sort(function(a, b) {
            return b['date_sort'] - a['date_sort'];
        });


		for (z = 0; z < 6; z++) {
			// latestArticles[z]['title']
			var postInfo = "";
			if (z == 0 || z == 3) {
				postInfo += '<h1><div class="blog-row-clear" style="clear:both;"></div></h1>';
			}
			postInfo += '<li class="col-md-4 article-item">';
			postInfo += '<div class="home-article-featured-image blog-img-container"><a class="blog-img" href="' + latestArticles[z]['url'] + '" style="background-image: url(\'' + latestArticles[z]['image'] + '\');"></a></div>';
			postInfo += '<h4><a>' + latestArticles[z]['date'] + '</a></h4>';
			postInfo += '<div class="article-caption">';
			postInfo += '<h3><a href="' + latestArticles[z]['url'] + '">' + latestArticles[z]['title'] + '</a></h3>';
			postInfo += '<p class="article-summary">' + latestArticles[z]['excerpt'] + '</p>';
			postInfo += '</div>';
			postInfo += '<p><a href="' + latestArticles[z]['url'] + '">Read more</a></p>';
			postInfo += "</li>";
            $("#recent-articles-homepage").append(postInfo);
        }
	}
</script>

