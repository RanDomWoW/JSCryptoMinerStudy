		 var searchTerm2 = "";

		 doProductsSmallSearch = function(term) {
			var searchURL = '/search?type=product&view=json&q=' + searchTerm2;
		    return searchURL;
		  } 

		 $("input[name='q']").on("input", debounce(function(e){
		 		searchTerm2 = $(this).val().trim();
		 		$(".search-extra").hide();
		 		doAjaxSearch();
		 }, 250));

		 var doAllFiltersSmallSearch = function(output){
			var numberProds = output.results_count;
			var productHTML = "";
			$(".results-count-search").text(numberProds);

			var plural = numberProds == 1 ? "result" : "results";
			$(".search-extra").show();
			$(".search-extra .plural").text(plural);
			$('.search-form').removeClass("searching").addClass("searched");

			output = output.results;
			for(var i = 0; i < output.length; i++){
				productHTML += "<li><a href='" + output[i].url + "'>" + output[i].title + "</a></li>";
			}

			if (output.length != numberProds) {
				productHTML += "<li class='view-more-link'><a href='/search?type=product&q=" + encodeURIComponent(searchTerm2) + "'>" + "View all results</a></li>";
			}

			if(output.length == 0){
				$(".search-extra").hide();
				$('.search-results').html("<li><a>No products found</a></li>");
			} else {
				$(".search-extra").fadeIn();
				$('.search-results').html(productHTML);
			} 		           
			$('.search-results').slideDown();
		 }

		 var doAjaxSearch = function() {		  
		   var url = doProductsSmallSearch().replace(/^https:?/,'').replace(/^http:?/,'');
		   $('.search-results').hide().find("li").remove();
		   $('.search-form').addClass("searching");
		   $.getJSON(url, function(data) {doAllFiltersSmallSearch(data);});
		  }
	})
</script>

