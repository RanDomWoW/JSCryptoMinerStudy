			$(document).ready(function(){
				$('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
		        // save the latest tab; use cookies if you like 'em better:
		        localStorage.setItem('lastTab', $(this).attr('href'));
		    });

		    // go to the latest tab, if it exists:
		    var lastTab = localStorage.getItem('lastTab');
		    if (lastTab) {
		        $('[href="' + lastTab + '"]').tab('show');
		    }
				if(window.location.href.indexOf("/news/")!=-1){
				var url = window.location.href;
				var ID = url.substring(url.lastIndexOf('/')+1);
					$.ajax({
				type:'POST',
				url:'/controllers/view.php',
				data:'id='+ID,
				success:function(html){
					 $('.content_area').empty().prepend(html);
					 $("html, body").animate({ scrollTop: 0 }, 600);
					   $("#wrapper").addClass("toggled-2");
				}
			});
			}

                $('.news_list').on('scroll', function() {
						 if($(this).scrollTop() + $(this).innerHeight() >= $(this)[0].scrollHeight) {
                          var ID = $('.news_list div').last().attr("data-id");
						  $.ajax({
						  type: 'post',
						  asynch: false,
						  url: '/controllers/arrToJson.php',
						  cache: false,
						  data:'id='+ID,
						  success: function (response) {
						  $('.news_list').append(response);
						  }
						  });
                    }
                });
				});
    // Display content script
      $(document).on('click','.list_item',function(ev){
    ev.preventDefault();
		$(this).find('div[align=right]').remove();
        var ID = $(this).data('id');
		var title = $(this).text();
		history.pushState(null, null, '/news/'+ID);
		$(document).prop('title',title);
        $.ajax({
            type:'POST',
            url:'/controllers/view.php',
            data:'id='+ID,
            success:function(html){
                 $('.content_area').empty().prepend(html);
                 $("html, body").animate({ scrollTop: 0 }, 600);
                 $("#wrapper").addClass("toggled-2");
			}
        });
    });
	$(document).on('click','.list_item',function(ev){
    ev.preventDefault();
        var ID = $(this).data('id');
	$.ajax({ type:'POST', url:'/controllers/comments.php', data:'id='+ID, cache: false,
					success:function(cmnt){
						$('#comments').empty().prepend(cmnt);
					}
				 });
	});
	setInterval(function() {
		var postId = $('.news_list div').first().attr('data-id');
		$.ajax({
            type:'POST',
            url:'/controllers/ajaxnew.php',
            data:'id='+postId,
            success:function(html){
                $('.news_list').prepend(html);
		}
        });
	}, 1000 * 60 * 1); // where X is your every X minutes


	function openComments() {
		document.getElementById("mcomments").style.display="block";
		document.getElementById("closeBtn").style.display="block";
    document.getElementById("content_no_scroll").style.display="none";
	}

	/* Close when someone clicks on the "x" symbol inside the overlay */
	function closeComments() {
			document.getElementById("closeBtn").style.display="none";
	    document.getElementById("mcomments").style.display="none";
			document.getElementById("content_no_scroll").style.display="block";
	}
	function openNews() {
		document.getElementById("mnews").style.display="block";
		document.getElementById("closeBtn1").style.display="block";
    document.getElementById("content_no_scroll").style.display="none";
	}
	/* Close when someone clicks on the "x" symbol inside the overlay */
	function closeNews() {
			document.getElementById("closeBtn1").style.display="none";
	    document.getElementById("mnews").style.display="none";
			document.getElementById("content_no_scroll").style.display="block";
	}
    </script>


</body>

</html>