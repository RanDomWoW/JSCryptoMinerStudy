                <script defer type="text/javascript">
                    function showMobilHeaderCompact()
                    {
                        changeChosenKfz=0;
                        replaceClassNameById('header_blueLineMobil_complete','block', 'min480');
                        replaceClassNameById('header_blueLineMobil_compact','noneDisplay', 'max480');
                        
                            var meineFahrzeuge=readCookie('meineFahrzeuge');
                            if(meineFahrzeuge)
                        
                            {
                                hideChooseNewKfz();
                            }
                    }

                    function showChangeKfz()
                    {

                        showChooseNewKfz();
                        changeChosenKfz=1;
                        showMobilHeaderComplete();
                    }

                    function showBlueLineWithoutGarage()
                    {
                        var element=document.getElementById('header_blueLine1_mobil');
                        if(element.className.match(/height_garage/g))
                        {
                            replaceClassNameById('header_blueLine1_mobil', 'header_blueLine1_height_garage', 'header_blueLine1_height_chooseType');
                        }
                        
                    }

                    function updateDynamicFields()
                    {
                        var kba2=readCookie('kba2');
                        var kba3=readCookie('kba3');

                        if(kba2 && kba3) $('.currentKfzDetails, .searchTree .carInfo .kbaDisplay')
                                            .toggleClass('kbaIsSet', true)
                                            .find('.kba2display').text(kba2).end()
                                            .find('.kba3display').text(kba3);

                        
                        
                        var shoppingCartCookie=readCookie('shoppingCart');
                        
                        if(shoppingCartCookie)
                        {
                            var parts=shoppingCartCookie.split("_");                    
                            var articles=parts[0];
                            var total=parts[1];
                            
                            if(articles>0 || total>0)
                            {
                                document.getElementById('shoppingCart').innerHTML=articles;
                                document.getElementById('shoppingCartQty').innerHTML=articles + '&nbsp;Artikel&nbsp;&nbsp;';
                                document.getElementById('shoppingCartSumme').innerHTML=total.replace(/\./g, ",") + '&nbsp;&euro;';
                            }
                            else
                            {
                                document.getElementById('shoppingCart').innerHTML='0';
                                document.getElementById('shoppingCartQty').innerHTML=0 + '&nbsp;Artikel&nbsp;&nbsp;';
                                document.getElementById('shoppingCartSumme').innerHTML='0,00&nbsp;&euro;';
                            }
                        }
                        else
                        {
                            document.getElementById('shoppingCart').innerHTML='0';
                            document.getElementById('shoppingCartQty').innerHTML=0 + '&nbsp;Artikel&nbsp;&nbsp;';
                            document.getElementById('shoppingCartSumme').innerHTML='0,00&nbsp;&euro;';
                        }
                        
                        var login = readCookie('loginType');
                        var endDeviceType=readCookie('endDeviceType');
                        
                        if(endDeviceType=='salesTerminal' || endDeviceType=='fairTerminal')
                        {
                            // kein Login anzeigen
                            if (document.getElementById('terminalToolbar')) document.getElementById('terminalToolbar').style.display='block';
                        }
                        else
                        {
                            var aboutUsVideo=document.getElementById('aboutUs_video');                        
                            if(aboutUsVideo)
                            {
                                aboutUsVideo.style.display='block';
                            }
                            
                            if (login)
                            {
                                document.getElementById('account_loggedIn').style.display='block';
                                //document.getElementById('wishList_loggedIn').style.display='block';
                            }
                            else
                            {
                                document.getElementById('account_loggedOut').style.display='block';
                            }
                        }
                        
                        var meineFahrzeuge=getRawCookie('meineFahrzeuge');
                        var kfzData = getMyGarageCars(meineFahrzeuge);

                        var myCompactGarageList = [];
                        var myCompleteGarageList = [];
                        
                        if (login)
                        {
                            $('.loyaltyLogo').toggleClass('hide', login !== 'loyalty');
                            $('.upsElements').toggleClass('loyalty min768', login === 'loyalty');
                            $('.upsElements .rightToReturn').toggleClass('min860', login === 'loyalty');


                            var url = window.location.href;
                            var expression=/showChooseNewKfz|forceChooseType/g;
                            var showChooseTypeHeader=false;
                            
                            
                            
                            if(meineFahrzeuge && !showChooseTypeHeader)
                            {
                                    if (url.match(expression))
                                    {
                                        document.getElementById('chooseKfzByKbaNr').style='display: inline-block;';
                                    }
                                    else 
                                    {
                                        var oUL=document.getElementById('meineFahrzeugeAuflistung');
        
                                        //var fahrzeuge=meineFahrzeuge.split('%3A')
                                        
                                        for(var i=0; i<=kfzData.length; i++)
                                        {
                                            if(kfzData[i])
                                            {
                                                var oLI = fullMyGarageListElement(kfzData[i]);

                                                if(i<=2)
                                                {
                                                    myCompactGarageList.push(oLI);
                                                }
                                                
                                                myCompleteGarageList.push(oLI);
                                            }
                                        }
                                        
                                        for(var i=0; i<myCompactGarageList.length; i++)
                                        {
                                            oUL.appendChild(myCompactGarageList[i]);
                                        }
                                        
                                        oUL.style.display='block';
                                        
                                        if(myCompleteGarageList.length > 3)
                                        {
                                            document.getElementById('myGarage_showMoreButton').style.display='block';
                                            var oLI = fullMyGarageListElement('+');
                                            oUL.appendChild(oLI);
                                            replaceClassNameById('meineFahrzeugeAuflistung', 'complete', 'compact');
                                        }
                                        else
                                        {
                                            replaceClassNameById('meineFahrzeugeAuflistung', 'compact', 'complete');
                                        }
                                        
                                        $('#currentKfz').show();
                                        document.getElementById('displayMeineFahrzeugeList').style.display='block';
                                        document.getElementById('displayMeineLeereFahrzeuge').style.display='none';
                                        
                                        replaceClassNameById('myGarage_newKfzButton', 'myGarage_newKfzButton_complete', 'myGarage_newKfzButton_compact');
                                        replaceClassNameById('header_blueLine1_mobil', 'header_blueLine1_height_noGarage', 'header_blueLine1_height_garage');
                                        // showHotlineHeaderCompact();
                                    }
                            }
                            else
                            {
                                document.getElementById('meineFahrzeugeAuflistung_empty').style.display='inline-block';
                                document.getElementById('meineFahrzeugeAuflistung').style.display='none';
                                document.getElementById('displayMeineFahrzeugeList').style.display='inline-block';
                                $('.shadowHeader').toggleClass('myGarageIsEmpty');
                            }
                            
                            if (!url.match(expression) && !showChooseTypeHeader)
                            {
                                $('#chooseKfzByKbaNr').hide();
                                $('#chooseTypeContainer').hide();
                                $('#displayMeineFahrzeuge').show();
                                $('#currentKfz').show();
                            }
                        }
                        else
                        {
                            
                        }
                    }

                    function fullMyGarageListElement(kfzFromCookie)
                    {
                        var oLI=document.createElement('li');
                        
                        if(kfzFromCookie=='+')
                        {
                            var a = document.createElement('a');
                            a.href='/index.cgi?rm=myGarage';
                            a.setAttribute('class','white');
                            a.style='float: left; width: 20px;';
                            var txt=document.createTextNode('...');
                        }
                        else
                        {
                            var a = document.createElement('a');
                            a.href='/index.cgi?rm=confirmType&action=selectFromGarage&ktypnr_new='+kfzFromCookie.ktypnr+'&kba2_new='+kfzFromCookie.kba2+'&kba3_new='+kfzFromCookie.kba3+'&description='+encodeURIComponent(kfzFromCookie.label)+'';
                            a.setAttribute('class','white');
                            a.style.float='left';
                            var kfz_name=kfzFromCookie.label;
                            if(kfz_name.length > 50){
                                oLI.setAttribute('class','ellipsis');
                            }
                            var txt=document.createTextNode(kfz_name + '');
                        }
                        
                        a.appendChild(txt);
                        oLI.appendChild(a);
                        
                        return oLI;
                    }

                    function updateChooseType(field, value)
                    {
                        var hernr = (field=='p')? value : $("#typeSelectionField_1").val();
                        var kmodnr = (field=='m')? value : (field=='t')? $("#typeSelectionField_2").val() : undefined;
                        var ktypnr_new;

                        if (field == 't')
                        {
                            ktypnr_new = value;
                            var url = "/index.cgi?rm=confirmType&hernr="+hernr+"&kmodnr="+kmodnr+"&ktypnr_new="+ktypnr_new;
                            self.location.href = url;
                            return;
                        }

                        $.getJSON( "/",
                                   {
                                       'rm':     'headerChooseType',
                                       'hernr':  hernr,
                                       'kmodnr': kmodnr,
                                   },
                                   function( result ) {
                                       $("#typeSelectionField_2")
                                           .empty()
                                           .append( function() {
                                               return $("<option>").val("").append("--- Modell w&auml;hlen ---");
                                           });
                                       $.each(result.models, function() {
                                           var element = this;
                                           $("#typeSelectionField_2").append( function() {
                                               return $("<option>")
                                                          .val(element.value)
                                                          .append(element.name)
                                                          .attr("selected", element.selected);
                                           });
                                       });
                                       $("#typeSelectionField_3")
                                           .empty()
                                           .append( function() {
                                               return $("<option>").val("").append("--- Typ w&auml;hlen ---");
                                           });
                                       $.each(result.types, function() {
                                           var element = this;
                                           $("#typeSelectionField_3").append( function() {
                                               return $("<option>")
                                                          .val(element.value)
                                                          .append(element.name);
                                           });
                                       });
                                       var disableField = hernr? false : true;
                                       $("#selectControlTypeSelectionField_2").toggleClass("disabled", disableField);
                                       $("#typeSelectionField_2").toggleClass("disabled", disableField);
                                       $("#typeSelectionField_2").prop("disabled", disableField);

                                       disableField = kmodnr? false : true;
                                       $("#selectControlTypeSelectionField_3").toggleClass("disabled", disableField);
                                       $("#typeSelectionField_3").toggleClass("disabled", disableField);
                                       $("#typeSelectionField_3").prop("disabled", disableField);
                        }).fail( function( request, status, error ) {
                            console.log("getJSON failed! Status: " + status + "; Error: " + error);
                        });
                    }


                    $(document).ready(function(){
                        updateDynamicFields();

                    });
                </script>
            


            

            
