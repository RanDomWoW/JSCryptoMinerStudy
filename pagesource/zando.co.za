<script id="searchProductTemplate" type="text/template">
    {{#products}}
        <a
            href="{{url}}"
            data-trk-category="category finding methods"
            data-trk-action="enhanced search performed"
            data-trk-label="{{url}}|{{term}}|{{count}}|placeholder:{{placeholder}}"
        >
            <div class="enhanced-search__product clearfix">
                <div class="pull-left enhanced-search__image">
                    <img src="{{image}}" alt="{{name}}" width="60" height="75" />
                </div>
                <div class="pull-left enhanced-search__detail">

                    <div class="enhanced-search__name">{{name}}</div>
                    <div class="enhanced-search__rating">
                        {{#rating}}
                            <span class="{{.}}"></span>
                        {{/rating}}
                    </div>

                    <div class="enhanced-search__sizes">
                        <ul>
                            {{#sizes}}
                                <li>
                                    <b>
                                        <span
                                            data-type="link"
                                            data-href='{{url}}?size={{.}}'
                                            data-trk-category="category finding methods"
                                            data-trk-action="enhanced search performed"
                                            data-trk-label="{{url}}?size={{.}}|{{term}}|{{count}}|placeholder:{{placeholder}}"
                                        > {{.}}</span>
                                    </b>
                                </li>
                            {{/sizes}}
                        </ul>
                    </div>

                    <div class="enhanced-search__price">
                        {{#is_special}}<span class="on-sale">{{/is_special}}
                            R{{normal_price}}{{#is_special}}</span>{{/is_special}}
                        &nbsp;
                        {{#is_special}}
                            <span class="discount">
                                R{{price}}
                            </span>
                        {{/is_special}}
                    </div>
                </div>
            </div>
        </a>
    {{/products}}
</script>
<script id="searchBrandTemplate" type="text/template">
    {{#brands}}
        <a
            href="{{url}}"
            data-trk-category="category finding methods"
            data-trk-action="enhanced search performed"
            data-trk-label="{{url}}|{{term}}|{{count}}|brand|{{name}}|placeholder:{{placeholder}}"
        >
            <div class="enhanced-search__brand--container">
                <img
                    class="lazy"
                    src="{{image}}"
                    alt="{{name}}"
                />
            </div>
        </a>
    {{/brands}}
</script>
<script id="searchPaginationTemplate" type="text/template">
    <ul class="pagination pagination-default">
    {{#pages}}
        <li {{#active}} class="active" {{/active}}>
            <a 
                data-page="{{page}}" 
                data-action="enhanced-search-pagination" 
                href="#page{{page}}"
            >{{page}}</a>
        </li>
    {{/pages}}
    </ul>
</script>

