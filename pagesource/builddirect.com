<!-- scripts --><script>
function bindSendBDEventToLink() {
    var links = document.getElementsByClassName("bd-header-promo-strip-sale-link");

    function publishBDEvent(eventName, href) {
        if (typeof BD !== 'undefined' && BD.events && BD.events.publish) {
            BD.events.publish(eventName).then(function () {
                window.location = href;
            }, function () {
                window.location = href;
            });
        }
    }

    var sendClickEvent = function(mouseEvent) {
        mouseEvent.preventDefault();
        publishBDEvent("header-promo-strip-link-clicked", mouseEvent.target.href);
    }

    if (links) {
        Array.prototype.slice.call(links).forEach(function(link) {
            link.addEventListener("click", sendClickEvent, false);
        });
    }
}

// Set the date we're counting down to
var countDownDate = new Date("Mar 23, 2018 23:00:00").getTime();

function setInnerText(value) {
    return function(domElement) {
        if (domElement) {
            domElement.innerText = value;
        }
    }
}

function updateDigit(className, value) {
    var digitContainers = document.getElementsByClassName(className);

    if (digitContainers) {
        Array.prototype.slice.call(digitContainers).forEach(setInnerText(value));
    }
}

function updateCountDownText() {
    // Get todays date and time
    var now = new Date().getTime();

    // Find the distance between now an the count down date
    var distance = countDownDate - now;

    // Time calculations for days, hours, minutes and seconds
    var days = Math.floor(distance / (1000 * 60 * 60 * 24));
    var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
    var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
    var seconds = Math.floor((distance % (1000 * 60)) / 1000);

    updateDigit("count-down__clock__days", days + 'd');
    updateDigit("count-down__clock__hours", hours + 'h');
    updateDigit("count-down__clock__minutes", minutes + 'm');
    updateDigit("count-down__clock__seconds", seconds + 's');
}

updateCountDownText();

// Update the count down every 1 second
setInterval(function() {
    updateCountDownText();
}, 1000 );

bindSendBDEventToLink();
</script></div>
