		$(document).on('click', '.bookmark', function(e){
			var index = $('.bookmark').index(this);
			var book_id = $(".council_history").eq(index).attr('id');

			if($('input[name=user_no]').val()==''|| $('input[name=user_no]').val()==-1){
				location.href='/std_register.php';
			}else{
				if($(this).hasClass('bookmark-full')){
					$(this).fadeOut(100, function() {$(this).attr('src','/assets/resource/images/bookmark_empty.svg').fadeIn(250);});
					$.post('/assets/lib/mypage/proc_addBookmark.php',{no:book_id,feed_type:'case',flag:'0'}, function(data) {
					});
					$(this).addClass('bookmark-empty');
					$(this).removeClass('bookmark-full');
					$(this).attr('flag', '1');
				}else{
					$(this).fadeOut(100, function() {$(this).attr('src','/assets/resource/images/bookmark_full.svg').fadeIn(250);});
					$.post('/assets/lib/mypage/proc_addBookmark.php',{no:book_id,feed_type:'case',flag:'1'}, function(data) {
					});
					$(this).addClass('bookmark-full');
					$(this).removeClass('bookmark-empty');
					$(this).attr('flag','0');
				}
			}

			return false;

		});
	</script>

