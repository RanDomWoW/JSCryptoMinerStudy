			var keyword = $('.keyword').val();
			search_council(keyword);

		});

		$('.keyword').keypress( function(e) {
			var keyword = $('.keyword').val();
			if (e.which == 13) search_council(keyword);
		});

		$(document).on('click','.recomm-keyword', function(e){
			var keyword = $(this).html();
			search_council(keyword);
		});
	</script>
