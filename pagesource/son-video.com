                $('body').on('click', '.widget-rappel-immediat', function (event) {
    event.preventDefault()

    $.ajax({
        type: 'GET',
        url: '\x2Frappel\x2Dimmediat\x2Fpopin',
        dataType: 'json',
        success: function (response) {
            loadImmediateRecallPopin(response)
        }
    });
})

function loadImmediateRecallPopin(response) {
    window.swal({
        title: window.Translator.trans('immediate_recall', 'immediate_recall'),
        html: response.content,
        width: 540,
        timer: response.timer || false,
        showCloseButton: true,
        reverseButtons: true, // Important for styling as well as ui logic
        showCancelButton: !response.hide_buttons,
        showConfirmButton: !response.hide_buttons,
        confirmButtonColor: '#466EDC',
        cancelButtonColor: '#FFF',
        confirmButtonText: window.Translator.trans('validate', 'immediate_recall'),
        cancelButtonText: window.Translator.trans('cancel', 'immediate_recall'),
        preConfirm: function () {
            if(response.hide_buttons === false) {
                var form = $('#formRappelImmediat')

                return new Promise(function (resolve, reject) {
                    $.ajax({
                        type: 'POST',
                        url: form.attr('action'),
                        data: form.serialize(),
                        dataType: 'json',
                        success: function (res) {
                            if(res.hide_buttons) {
                                loadImmediateRecallPopin(res)
                            } else {
                                $('#modalContentId').html(res.content)

                                                                 reject()
                            }
                        }
                    })
                })
            }
        }
    }).catch(function () {
        swal.noop()
    })
}
            });
        </script>

            

