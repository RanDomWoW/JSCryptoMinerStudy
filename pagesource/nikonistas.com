<script type="text/javascript">
	/* <![CDATA[ */
	var _cii169jq = jQuery.noConflict();
	_cii169jq(function(jq){
		var modal = jq('[data-role="modal-login"]');

		if (modal.length > 0) {
			jq('[data-role="login"]').click(function(event){
				event.preventDefault();

				var item = jq('[data-role="remember"]', modal);
				if (item.length > 0) {
					jq('#'+item.data('field'), modal).val(0);
					jq(
						'[data-role="password"] input', modal
					).attr('required', 'required');
					if (modal.is(':visible')) {
						jq('[data-role="password"]', modal).slideDown();
						item.parent().slideDown('fast');
					} else {
						jq('[data-role="password"]', modal).show();
						item.parent().show('fast');
					}
					jq('[type="submit"]', modal).val('Acceder');
				}
				modal.fadeIn(400).css('display','flex');
			});
			jq('[data-role="close"]', modal).click(function(){
				modal.fadeOut(400);
			});
			jq('[data-role="pt"]', modal).click(function(){
				jq('[data-role="close"]', modal).click();
				jq('body').trigger('login-pt');
			});
			jq('[data-role="remember"]').click(function(event){
				event.preventDefault();

				var item = jq('[data-role="remember"]', modal);
				if (item.length > 0) {
					jq(
						'form [data-role="message"]',
						modal
					).slideUp().empty();
					jq('#'+item.data('field'), modal).val(1);
					jq(
						'[data-role="password"] input', modal
					).removeAttr('required');
					if (modal.is(':visible')) {
						jq('[data-role="password"]', modal).slideUp();
						item.parent().slideUp();
					} else {
						jq('[data-role="password"]', modal).hide();
						item.parent().hide();
					}
					jq('[type="submit"]', modal).val('Recordar acceso');
				}
				modal.fadeIn(400).css('display','flex');
			});
			jq('form', modal).submit(function(){
				var $this = jq(this);
				var ok = true;
				
				jq('[required="required"]', $this).each(function(){
					if (jq(this).val() == '') {
						jq(this).parent().addClass('required');
						if (ok) {
							jq(this).focus();
							ok = false;
						}
					} else {
						jq(this).parent().removeClass('required');
					}
				});
				if (ok && !$this.data('sending')) {
					$this.data('sending', true);
					jq(
						'[data-role="message"]',
						$this
					).slideUp().empty();

					jq.ajax({
						'type': 'post',
						'url': $this.attr('action'),
						'data': $this.serialize(),
						'complete': function(response){
							if (response.status == 200) {
								if (response.responseText) {
									jq(
										'[data-role="message"]',
										$this
									).html(
										'<span class="ok">'
										+response.responseText
										+'</span>'
									);
									jq('[data-role="login"]:first').click();
									jq('[required="required"]', $this).each(function(){
										jq(this).val('');
									});
								} else {
									if ($this.data('redirect') != '') {
										document.location.href = $this.data('redirect');
									} else {
										document.location.reload();
									}
								}
							} else {
								jq(
									'[data-role="message"]',
									$this
								).html(
									'<span class="ko">'
									+response.responseText
									+'</span>'
								);
							}
							$this.data('sending', false);
							jq(
								'[data-role="message"]',
								$this
							).slideDown();
						}
					});
				}
				
				return false;
			});
			if (window.location.search.substr(1).indexOf('IDS=') != -1) {
				jq('[data-role="login"]:first').click();
			}
		}
	});
	/* ]]> */
</script>

