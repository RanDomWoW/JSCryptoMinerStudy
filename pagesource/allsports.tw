// 検索結果のpage_idを格納する変数
events['display'] = [];
events['display']['open']  = [];
events['display']['ready'] = [];

// 検索条件: searchItems とフィルター: events['filter'] とをマッチングさせ、
// 検索結果を events['display'] に格納する(page_idの配列)
function searchEvents() {
  events['display']['open']  = [];
  events['display']['ready'] = [];

  var kekka;
  var page_id;
  for (var status in events['display']) { // status in ('open' or 'ready')
    for (var i = 0; i < events['filter'][status].length; i++) {
      page_id = events['filter'][status][i]['page_id'];
      kekka = true;

      for (var item in searchItems) {
        if (kekka) {
          if (searchItems[item] != 'ALL') {
            //合致しなければ false。
            if (events['filter'][status][i][item].indexOf(searchItems[item]) === -1) {
              kekka = false;
            }
          }
        }
      }
      if (kekka) {
        events['display'][status].push(page_id);
      }
    }
  }
}

function makeEventLists() {
  var statuses = ['open', 'ready'];
  for (var i=0; i<statuses.length; i++) {
    makeEventList(statuses[i], 0);
  }
}

function makeEventList(status, page_num) {

  var htmlstr;
  var page_id;
  var range_per_page = 15; // 表示大会数/ページ
  var start_index = range_per_page * page_num;
  var max_index   = start_index + range_per_page;
  if (max_index > events['display'][status].length) {
    max_index = events['display'][status].length;
  }

  // 初期化
  $('#event_list_' + status).html('');
  $('#pagination_' + status).html('');

  // 検索結果が0件の場合の表示。「イベントが見つかりませんでした。検索条件を変更して再検索してください。」
  if (events['display'][status].length == 0) {
    htmlstr  = '<tr><td>找不到符合的活動。<br />請重新輸入關鍵字。</td></tr>';
    $('#event_list_' + status).append(htmlstr);
  }
  // 検索結果が1件以上の場合の表示
  else {

    // 大会情報の表示
    for (var j = start_index; j < max_index; j++) {
      page_id = events['display'][status][j];

      htmlstr  = '<tr><td>';
      htmlstr += '<a href="/event/' + ("00000000" + page_id).substr(-8) + '.html">';
      htmlstr += events['master'][page_id]['date'] + '　' + events['master'][page_id]['name'];
      htmlstr += '</a>';
      htmlstr += '</td></tr>';
      $('#event_list_' + status).append(htmlstr);
    }

    // ページネーションの表示
    var max_page = Math.ceil(events['display'][status].length / range_per_page);
    var pagination_status = '';
    if (max_page > 1) {
      htmlstr  = '<ul class="pagination pagination-sm">';
      for (var i = 0; i < max_page; i++) {
        pagination_status = '';
        if (i == page_num) pagination_status = ' class="active"';
        htmlstr += '<li' + pagination_status + '><a href="#" onClick="makeEventList(\'' + status + '\', ' + i + ');return false;">' + (i + 1) + '</a></li>';
      }
      htmlstr += '</ul>';
      $('#pagination_' + status).append(htmlstr);
    }

  }

}

function setSearchItem(type, val) {
  $('#main_graphic').slideUp("slow");

  searchItems[type] = val;
  searchEvents();
  makeEventLists();
}
</script>

