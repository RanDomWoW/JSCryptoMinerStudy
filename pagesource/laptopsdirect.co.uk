                                </div>
                            </div>
                            <div style="display: none">
                                <div id="confirm">
                                    <p class="title">Are you sure you want to delete your wishlist?</p>
                                    <br />
                                    <input type="button" class="confirm yes" value="Yes" onclick="ga('send', 'event', 'Wishlist', 'Wishlist Bar', 'Delete Wishlist');" />
                                    <input type="button" class="confirm no" value="No" />
                                </div>
                            </div>
                        </div>

                        <div class="retreieveWlDiv">
                            <a href="#content-div" id="getwishlist" class="fancybox" style="display:none" onclick=" ga('send', 'event', 'Wishlist', 'Retrieve Wishlist', 'Reassurance Bar');">Retrieve Saved Items</a>
                            <div style="display: none">
                                <div id="content-div" style="padding-top:30px">
                                    Email Address: <input id="wlEmailID" name="wlEmailID" style="width: 250px" type="text" value="" />
                                    <br />
                                    <br />
                                    <br />
                                    <input id="retrievewl" type="button" title="Get My Wishlist" value="Get My Wishlist" onclick="ga('send', 'event', 'Wishlist', 'Retrieve Wishlist', 'Get Wishlist'); RetrieveWishList();"/>
