                values['beforeSectionDistance'] = menu.height() + rightSection.position().top + header.height();
                values['screenHeight'] = screen.availHeight;
                values['HHeight'] = header.height();
                values['LSHeight'] = leftSection.height();

                if (windowq.scrollTop() > myOffset){
                    leftSection.hide();
                    rightSection.removeClass('col-xs-6').addClass('col-xs-11 col-xs-offset-1').css({'padding-top':(myOffset>>1)+'px'});
                    return false;
                } else {
