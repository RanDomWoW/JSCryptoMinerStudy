        $(document).scroll(function(e){
            var windowq         = $(window),
                header          = $('header'),
                leftSection     = $('#leftSection'),
                rightSection    = $('#rightSection'),
                menu            = $('#menu'),
                values          = {},
                myOffset;




            if (leftSection && rightSection){

                myOffset = leftSection.height() + header.height();

                values['bodyHeight'] = $('body').height();
                values['windowScrollTop'] = windowq.scrollTop();
                values['RSHeight'] = rightSection.height();
                values['myOffset'] = myOffset;
