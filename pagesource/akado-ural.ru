<script type="text/javascript">
    jQuery(document).ready(function(e){

        /* jQuery('#back-call-wrapper .center-block').click(function(e){
         e.stopPropagation();
         }); */


        jQuery('#back-call-popup .phone').inputmask('+7 ( 999 ) 999 - 99 - 99', {placeholder: '+7 ( ___ ) ___ - __ - __'});

        jQuery(document).on('click' , '.back-call-phone', function(e){
            e.preventDefault();
            jQuery('.popup-background').show();
            jQuery('.back-call-popup').show();
            // console.log(1);
            /*jQuery('#back-call-wrapper').toggle();
            jQuery(document).scrollTop(0);
            jQuery('body').toggleClass('open-online-call-body');*/
            if(jQuery('#back-call-popup .select-row #back-call-popup-time').length == 0){
                jQuery('#back-call-popup .select-row').html(jQuery('#back-call-popup .select-container').html());
                jQuery('#back-call-popup .select-container').html('');
                jQuery('#back-call-popup-time').show();
                jQuery('#back-call-popup-time').next().remove();
                jQuery('#back-call-popup-time').chosen();
            }
        });

        jQuery(document).on('change', '#back-call-popup-time', function(){
            jQuery('#back-call-popup .time-description .item').removeClass('active');
            jQuery('#back-call-popup .time-description .item').eq(jQuery('#back-call-popup option:selected').index()).addClass('active');
        });

        $('body').on('click', function(event){
            var target = $(event.target);
            if(!target.closest('.back-call-popup').is('.back-call-popup')) {
                $('.back-call-popup').hide();
                $('.popup-background').hide();
                $('.back-call-popup__answer').hide();
            }
        });
        $('#back-call-popup .popup-close').click(function(){
            $('.back-call-popup').hide();
            $('.popup-background').hide();
            $('.back-call-popup__answer').hide();
        });

        jQuery('#page').on('click', '#back-call-popup-submit', function(e){
            e.preventDefault();
            if (jQuery(this).hasClass('disabled')){
                return;
            }
            jQuery('.back-call-phone, .back-call__name').removeClass('error');
            var callName = jQuery('.back-call__name');
            var callPhone = jQuery('.back-call-phone');
            /* if(callName.val() == ''){
             callName.addClass('error');
             } */
            if(callPhone.val() == ''){
                callPhone.addClass('error');
            }
            if(!callName.hasClass('error') && !callPhone.hasClass('error')){
                jQuery(this).addClass('disabled');
                var callTime = jQuery('#back-call-time').val();
                yaCounter17450176.reachGoal('oredercallsended');
                ga('send', 'event', 'ordercall', 'ordersended', 'call_from_site');
                jQuery.post(
                    '/modules/mod_back_phone/mod_back_phone.php',
                    {name: callName.val(), phone: callPhone.val(), time: callTime, result: 1},
                    function(data){
                        jQuery('.back-call-popup').hide()
                        jQuery('.back-call-popup__answer').show();
                    }
                );
            }
        });
    });
</script>

