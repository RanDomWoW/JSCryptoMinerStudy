<script type="text/javascript">
    $(function () {        
        var activeProfile = getActiveProfile();        
        if (activeProfile.name != "") {
            $("#profile-bar").show();
            manageProfileBar(true);
            $("#profileNameBox").bind("click", function () { showDialog(getActiveProfile()); });
