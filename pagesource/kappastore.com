<script type="text/javascript" src="https://cdn.kappastore.com/script/jquery.ui.rcarousel.min.js"></script>
<script type="text/javascript">
    $().ready(function () {
        //Carousel Jumbo
        var widthJumbo = 771;
        
        var heightJumbo = 370;
        
        

        function generatePages() {
            var _total, i, _link;

            _total = $("#carousel").rcarousel("getTotalPages");

            for (i = 0; i < _total; i++) {
                _link = $("<a href='#'></a>");

                $(_link)
					.bind("click", { page: i },
						function (event) {
						    $("#carousel").rcarousel("goToPage", event.data.page);
						    event.preventDefault();
						}
					)
					.addClass("bullet off").appendTo("#pages");
            }
            generateVideo();
            // mark first page as active
            $("a:eq(0)", "#pages").removeClass("off").addClass("on");

        }
        
        function pageLoaded(event, data) {
            $("div.on", "#pages").removeClass("on");
            $("a", "#pages").eq(data.page).addClass("on");
            generateVideo();
        }

        $("#carousel").rcarousel({
            visible: 1,
            step: 1,
            width: widthJumbo,
            height: heightJumbo,
            auto: { enabled: true },
            start: generatePages,
            pageLoaded: pageLoaded,
            speed: 500
        });

        $(".bullet").hover(function () { $(this).css("opacity", 0.7); }, function () { $(this).css("opacity", 1.0); });

        function generateVideo() {
            //bind click over video to stop carousel
            $("[data-type*='video']").each(function () {
                //Setup Jwplayer on video
                //get title
                var title = $(this).find("div").attr("titlevideo");
                var myplayer = jwplayer($(this).attr("data-type")).setup({
                    file: $(this).find("div").attr("urlvideo"),
                    image: $(this).find("div").attr("urlposter"),
                    height: "100%",
                    width: "100%",
                    stretching: "uniform"
                });
                myplayer.onPlay(function () {
                    $("#carousel").rcarousel("option", { auto: { enabled: false } });
                    //monto l'immagine per 16:9
                    $('.jwplayer').css('background-color', 'transparent');
                    if ($('.clsVid169').length == 0)
                        $('<img src="/images/vidLayer.png" class="clsVid169"/>').appendTo('.jwdisplay');
                    if ($('.clsVidTitle').length > 0)
                        $('.clsVidTitle').remove();
                });
                myplayer.onReady(function () {
                    //Monto l'immagine per i 4:3
                    $('<img src="/images/vidPreLayer.png"/>').appendTo($('.jwpreview'));
                    //Title Video
                    if (title != "")
                        $('<div class="clsVidTitle"><span class=\"clsVidText\">' + title + '</span></div>').insertAfter($('.jwdisplayIcon'));
                });
                myplayer.onComplete(function () {
                    //Rimuovo la l'img 16:9
                    $('.clsVid169').remove();
                    //Title Video
                    if (title != "")
                        $('<div class="clsVidTitle"><span class=\"clsVidText\">' + title + '</span></div>').insertAfter($('.jwdisplayIcon'));
                    $("#carousel").rcarousel("option", { auto: { enabled: true } });
                });
            });
        }
    });
</script>
