    function showDialog() {        
        $("#modal-profile").dialog({
            modal: true, resizable: false, draggable: true, width: 900, height: 620,
            title: ($("#div-language").prop("lang") == "it") ? "Naviga con il tuo profilo" : "Browse with your profile",
            open: function (event, ui) {
                $("#modal-profile").load('/controls/profilo/profilo.aspx?lang=' + $("#div-language").prop("lang") + '&showlist=1', function (response, status, xhr) {
                    if (status == "success") {
                        $('#BtnCloseModalProfile')
                            .bind("click", function () { closeThisModalProfile(); });
                        $("#BtnSaveProfile")
                            .bind("click", function () {
                                if ($("#TxtProfile").val() == "") {
                                    GetSingleText("kstr_profile", 13, $("#div-language").attr("lang"), "profile-messages-confirm");
                                    $("#TxtProfile").focus();
                                    return false;
                                }
                                else if (tempProfile.genders[0].shoes.length == 0 && tempProfile.genders[0].apparel.length == 0
                                                && tempProfile.genders[1].shoes.length == 0 && tempProfile.genders[1].apparel.length == 0
                                                && tempProfile.genders[2].shoes.length == 0 && tempProfile.genders[2].apparel.length == 0) {
                                    //debugger;
                                    GetSingleText("kstr_profile", 20, $("#div-language").attr("lang"), "profile-messages-confirm");
                                    $("#TxtProfile").focus();
                                    return false;
                                }
                                else {
                                    $("#profile-messages").html("&nbsp;");
                                    saveProfile();
                                    showProfileList();
                                }
                                GetSingleText("kstr_profile", 14, $("#div-language").attr("lang"), "profile-messages-confirm");
                                setTimeout('$("#profile-messages-confirm").html("");', 2500);
                                loadProfileList();
                            });
                        $("#BtnSaveProfileToUser")
                            .bind("click", function () {

                                if ($("#TxtProfile").val() == "") {
                                    GetSingleText("kstr_profile", 13, $("#div-language").attr("lang"), "profile-messages-confirm");
                                    $("#TxtProfile").focus();
                                    return false;
                                }
                                else if (tempProfile.genders[0].shoes.length == 0 && tempProfile.genders[0].apparel.length == 0
                                                && tempProfile.genders[1].shoes.length == 0 && tempProfile.genders[1].apparel.length == 0
                                                && tempProfile.genders[2].shoes.length == 0 && tempProfile.genders[2].apparel.length == 0) {
                                    //debugger;
                                    GetSingleText("kstr_profile", 20, $("#div-language").attr("lang"), "profile-messages-confirm");
                                    $("#TxtProfile").focus();
                                    return false;
                                }
                                else {
                                    $("#profile-messages").html("&nbsp;");
                                    tempProfile.account = true;
                                    saveProfile();
                                    //Salvataggio profile DB Controllo se l'utente è loggato
                                    if (!$('#div-userid').data("userid") > 0) {
                                        showLoading();
                                        //Devo mandarlo nel login per farlo loggare...
                                        //https://kappastore.development/IT/sign.asp
                                        window.location.href = "/" + $("#div-language").prop("lang") + "/login?profile=1";
                                    }
                                    else {
                                        //Chiamo il Servizio per il salvataggio su DB del profilo
                                        $.ajax({
                                            type: "POST",
                                            url: kstrConfig.WSPROFILE + "/SetProfileByUserId",
                                            data: "{\"userId\":" + $('#div-userid').data("userid") + ", \"profile\":'" + JSON.stringify(getRenderedProfile()) + "'}",
                                            contentType: "application/json; charset=utf-8"
                                        }).done(function (p) {
                                            //salvataggio eseguito con successo
                                        });
                                    }
                                    showProfileList();
                                }
                                GetSingleText("kstr_profile", 14, $("#div-language").attr("lang"), "profile-messages-confirm");
                                setTimeout('$("#profile-messages-confirm").html("");', 2500);
                                loadProfileList();
                            });
                        $("#BtnDeleteProfile")
                            .bind("click", function () {
                                deleteProfile(getRenderedProfile());
                                showProfileList();

                                //Chiamo il Servizio per il salvataggio su DB del profilo
                                $.ajax({
                                    type: "POST",
                                    url: kstrConfig.WSPROFILE + "/SetProfileByUserId",
                                    data: "{\"userId\":" + $('#div-userid').data("userid") + ", \"profile\":'" + JSON.stringify(getRenderedProfile()) + "'}",
                                    contentType: "application/json; charset=utf-8"
                                }).done(function (p) {
                                    //salvataggio eseguito con successo
                                });

                                closeProfileBar();
                                initTempProfile(true);
                            });
                        $("#BtnUpdateProfile")
                              .bind("click", function () {
                                  //debugger;
                                  if ($("#TxtProfile").val() == "") {
                                      GetSingleText("kstr_profile", 13, $("#div-language").attr("lang"), "profile-messages-confirm");
                                      $("#TxtProfile").focus();
                                      return false;
                                  }
                                  else if (tempProfile.genders[0].shoes.length == 0 && tempProfile.genders[0].apparel.length == 0
                                                  && tempProfile.genders[1].shoes.length == 0 && tempProfile.genders[1].apparel.length == 0
                                                  && tempProfile.genders[2].shoes.length == 0 && tempProfile.genders[2].apparel.length == 0) {
                                      //debugger;
                                      GetSingleText("kstr_profile", 20, $("#div-language").attr("lang"), "profile-messages-confirm");
                                      $("#TxtProfile").focus();
                                      return false;
                                  }
                                  else {
                                      $("#profile-messages").html("&nbsp;");
                                      //Salvataggio profile DB
                                      saveProfile();
                                      if (tempProfile.account == true) {
                                          //Chiamo il Servizio per il salvataggio su DB del profilo
                                          $.ajax({
                                              type: "POST",
                                              url: kstrConfig.WSPROFILE + "/SetProfileByUserId",
                                              data: "{\"userId\":" + $('#div-userid').data("userid") + ", \"profile\":'" + JSON.stringify(getRenderedProfile()) + "'}",
                                              contentType: "application/json; charset=utf-8"
                                          }).done(function (p) {
                                              //salvataggio eseguito con successo
                                          });
                                      }
                                      showProfileList();
                                  }
                                  GetSingleText("kstr_profile", 14, $("#div-language").attr("lang"), "profile-messages-confirm");
                                  setTimeout('$("#profile-messages-confirm").html("");', 2500);
                                  loadProfileList();
                              });

                        var activeProfile = getActiveProfile();
                        renderProfile(activeProfile);

                        if (activeProfile.name == "") {
                            $("#BtnDeleteProfile").hide();
                            $("#BtnUpdateProfile").hide();
                        }
                        else {
                            //Se sono in modifica nascondo i pulsanti di salvataggio
                            $("#BtnSaveProfileToUser").hide();
                            $("#BtnSaveProfile").hide();
                        }
                    }
                });
            }
        });
    }
    function closeProfileBar() {
        disableAllProfiles();
        $("#profile-bar").hide();
        manageProfileBar(false);
    }

    //
    function closeThisModalProfile() {
        $("#modal-profile").dialog('close');
        return false;
    }

    //
    function saveProfile() {
        var thisProfile = getRenderedProfile();
        thisProfile.name = $("#TxtProfile").val();
        thisProfile.genders = tempProfile.genders;
        thisProfile.account = tempProfile.account;
        //Test se vuole utilizzare subito il prfilo creato
        if ($("#chkActivateProfile").prop("checked"))
            thisProfile.active = true;
        setProfileCookie(thisProfile);
        if ($("#chkActivateProfile").prop("checked"))
            activeProfile(thisProfile);
    }

    function loadProfileList() {
        var divProfilesList = $(".profile-list");
        divProfilesList.html("");
        var jsProfileCookie = getProfileCookie();
        //if (jsProfileCookie.profiles.length == 1)
        //kstrUtility.applyLabelToDOMObj("kstr_profile", 11, $("#BtnYourProfile"));

        //Se non ci son profili salavati nel cookie prendo quelli da DB se l'utente è loggato
        if (jsProfileCookie.profiles.length == 1 && $('#div-userid').data("userid") > 0) {
            //Chiamo il Servizio per leggere i profili da DB
            $.ajax({
                type: "POST",
                url: kstrConfig.WSPROFILE + "/GetProfileByUserId",
                data: "{\"userId\":" + $('#div-userid').data("userid") + "}",
                contentType: "application/json; charset=utf-8",
                async: false
            }).done(function (p) {
                //Leggo i profili salvati su DB					    	
                if (p.JsonProfiles != undefined)
                    jsProfileCookie = p;
            });
        }

        for (var i = 1; i < jsProfileCookie.profiles.length; i++) {
            //Regola di caricamento dei Profili:
            //Se utente non è loggato (cookie)
            //Se utente è loggato 		(cookie+DB)
            var p;
            if (!$('#div-userid').data("userid") > 0) {
                if (!jsProfileCookie.profiles[i].account) {
                    p = jsProfileCookie.profiles[i];

                    if (p.active)
                        divProfilesList.append("<div style='margin:10px 0;'><input type='radio' name='RadSelectedProfile' class='rad-profile' value='" + i + "' checked='checked'/><label class='rad-profile-label'>" + p.name + "</label> <a href='#' id='" + i + "'></a><div style='clear:both'></div></div>");
                    else
                        divProfilesList.append("<div style='margin:10px 0;'><input type='radio' name='RadSelectedProfile' class='rad-profile' value='" + i + "' /><label class='rad-profile-label'>" + p.name + "</label> <a href='#' id='" + i + "'></a><div style='clear:both'></div></div>");
                }
            }
            else {
                p = jsProfileCookie.profiles[i];
                if (p.active)
                    divProfilesList.append("<div style='margin:10px 0;'><input type='radio' name='RadSelectedProfile' class='rad-profile' value='" + i + "' checked='checked'/><label class='rad-profile-label'>" + p.name + "</label> <a href='#' id='" + i + "'></a><div style='clear:both'></div></div>");
                else
                    divProfilesList.append("<div style='margin:10px 0;'><input type='radio' name='RadSelectedProfile' class='rad-profile' value='" + i + "' /><label class='rad-profile-label'>" + p.name + "</label> <a href='#' id='" + i + "'></a><div style='clear:both'></div></div>");
            }

            GetSingleText("kstr_profile", 28, $("#div-language").attr("lang"), i);
        }
        //Se c'è un profilo selezionato devo fare scomparire la combo di selezione delle taglie
        if ($(".rad-profile:checked").length > 0 && $(".rad-profile:checked").val() != "0") {
            $("#CmbSizeFilter").val("-1").hide(); //Nascondo la combo delle taglie
            $("#CmbSeasonFilter").val("-1");
            manageProfileBar(true);
            $("#profileNameBox").bind("click", function () { showDialog(getActiveProfile()); });
        }
        if ($(".rad-profile:checked").length == 0) {
            disableAllProfiles();
            $("#RadNoProfile").prop("checked", "checked");
            $("#CmbSizeFilter").show();
            manageProfileBar(false);
        }
        applyProfileFilter();
    }
</script>
