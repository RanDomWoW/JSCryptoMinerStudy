<script type="text/javascript">
    $(document).mouseup(function (e) {
        var logincont = $("#FloatingLoginContent");
        var profilecont = $("#FloatingProfileContent");

        if (!logincont.is(e.target) && logincont.has(e.target).length === 0) {
            if ($(logincont).css('display') == "block") {
                logincont.slideUp(400);
            }
        }
        if (!profilecont.is(e.target) && profilecont.has(e.target).length === 0) {
            if ($(profilecont).css('display') == "block") {
                profilecont.slideUp(400);
            }
        }
    });
    $().ready(function () {
        /* Manage div over login */
        $('.menu-logout').bind('click', function () { document.location = "/it/?op=logout"; });
        //$('#pnlLoginButton').show();

        //if (!Page.AppRelativeVirtualPath.Contains("login")) {
        $("#btnFastLogin").click(function () {
            showLoading();
            if ($("#actFast").val() === "")
                $("#actFast").val("login");
            return checkFormLogin();
        });

        function checkFormLogin() {
            var formValid = true;
            if ($("#txtFastPwd").val().trim() === "") {
                //$("div[refctr='txtFastPwd']").show();
                $("#txtFastPwd").addClass("TextBox-Highlighted");
                $("#txtFastPwd").focus();
                formValid = false;
            }

            if ($("#txtFastUser").val().trim() === "") {
                //$("div[refctr='txtLoginUserName']").show();
                $("#txtFastUser").addClass("TextBox-Highlighted");
                $("#txtFastUser").focus();
                formValid = false;
            }
            if (!formValid)
                hideLoading();

            return formValid;
        }

        $('.menu-login')
        .bind('click', function () {
            if ($("#FloatingLoginContent").css('display') == "none") {
                $("#FloatingLoginContent").slideDown(200);
                $('#FloatingLoginContent').show();
                $('#txtFastUser').focus();
            } else {
                $("#FloatingLoginContent").slideUp(400);
            }
        });

        $('.menu-profile')
        .bind('click', function () {
            if ($("#FloatingProfileContent").css('display') == "none") {
                $("#FloatingProfileContent").slideDown(200);
                $('#FloatingProfileContent').show();
            } else {
                $("#FloatingProfileContent").slideUp(400);
            }

        });
        //}
        /* Manage div over login */
    });
</script>
    
