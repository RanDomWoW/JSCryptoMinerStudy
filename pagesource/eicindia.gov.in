<script type="text/javascript">
$(function(){
var moveLeft = 10;var moveUp = -110;
$('a#trigger').hover(function(e) {
$('div#pop-up').show();
}, function() {
$('div#pop-up').hide();
});
$('a#trigger').mousemove(function(e) {
$("div#pop-up").css('top', e.pageY + moveUp).css('left', e.pageX + moveLeft);
});
});
</script>
