        setTopicsHeight();
        setTopicsTxt();
      });
    });
  function setTopicsTxt(){
    //アクティブになった一番目のクラス名を取得
    var select_tab = $('.topicTab li.active')
    var select_ctg = select_tab[0].className.split(" ")[0];
    $('.topicsTxt ul').each(function () {
      var target_ctg = $(this).attr('class')
      if($(this).hasClass(select_ctg)){
        $(this).fadeIn("slow");
      }else{
        $(this).hide();
      }
    });
  }

  function setTopicsHeight(){
    console.log('setTopicsHeight処理開始');
    var max_h=0;
    var current_h=0;
    $(".topicsTxt ul").each(function () {
      console.log('最大高さ取得処理開始');
      current_h=$(this).height();
      if(max_h < current_h){
        max_h=current_h
      }
    });
    $('.topicsTxt').css('height',max_h+"px")
    $('.topicsTxt ul').css('height',max_h+"px")
  }
  </script>
