function CreateImageUrl(symbol) {
    return "https://cdn-files.prsmedia.fr/files/ALS/fr/images/appli/meteo/MeteoNewsIcons/" + symbol + ".png";
}
    
function setUpDataDisplay(town, todayForecast, nextDaysForecasts) {
	$(".localitemeteo > img").attr('src', CreateImageUrl(todayForecast.WeatherSymbol));
	$(".localitemeteo > img").attr('alt', todayForecast.WeatherTitle);
	$(".townWeather").empty();
	$(".townWeather").append(town);
	$(".temp_maxi").empty();
	$(".temp_maxi").append(todayForecast.AfternoonTemperature);
	$(".temp_mini").empty();
	$(".temp_mini").append(todayForecast.MorningTemperature);
}

window.setUpDataDisplay = setUpDataDisplay;

