<script type="text/javascript">
	$(document).ready(function(){
		$('#login-form').submit(function(){
			w = $('#login-form [name=submit]').outerWidth();
			$('#login-form [name=submit]').hide().parent()
				.find('.formloader').show().css({display:'block',width:w+'px'});
			var options = {
			  url: "/user/forms/loginform",
			  //dataType: "json",
			  success: function(xdata) {
					if(typeof(xdata) != 'object')
						data= eval('(' + xdata + ')');
					else
						data = xdata;
					clearErrors($('#login-form'));
					if (typeof(data.errors) == 'undefined') {
						if (typeof(data.user) != 'undefined')
							//1+1;
							loginSuccess(data.user);
						else {
							// в cлучае, если произошло что-то непонятное
							ajax_log("Login очень странно::"+xdata);
							reloadwin();
						}
					} 
					else {
						//showFormErrors($('#login-form'), data.errors);
						$('.info-message.error', '#login-form').remove();
						$.each(data.errors, function(index, value){
							t_data = {errors: value}
							$('#errorMessage').tmpl(t_data).prependTo($('[name='+index+']', '#login-form').parents('.rowElem'));
						})
					}
					$('#login-form [name=submit]').show().parent().find('.formloader').hide();
			  },
			  complete: function(jqXHR, textStatus){
				if (textStatus!='success'){
					ajax_log("Login complete::"+jqXHR.statusText+":"+jqXHR.responseText); 
					reloadwin();
				}
			  }
			};
			// передаем опции в  ajaxSubmit
			$("#login-form").ajaxSubmit(options);			
			return false;
		});
		$('.showregform').click(function(){
			showregform();
		});
		$('.showgetmailform').click(function(){
			showgetmailform();
		});
	});
</script>
