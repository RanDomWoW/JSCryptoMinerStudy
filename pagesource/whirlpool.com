<script type="text/javascript">
	var scrollChecker = function(){
		var scrollPosition = $(document).scrollTop();
		var scrollPositionBottom = $(document).scrollTop() + ($(window).height()/2);
		var heroTop = $('#homepage-slider').offset().top;
		var heroBottom = heroTop + $('#homepage-slider').outerHeight();
		var referTop = $('#refrigeration').offset().top;
		var referBottom = referTop + $('#refrigeration').outerHeight();
		var cookingTop = $('#cooking').offset().top;
		var cookingBottom = cookingTop + $('#cooking').outerHeight();
		var dishwasherTop = $('#dishwasher').offset().top;
		var dishwasherBottom = dishwasherTop + $('#dishwasher').outerHeight();
		var laundryTop = $('#laundry').offset().top;
		var laundryBottom = laundryTop + $('#laundry').outerHeight();
		var ownerCenterTop = $('#owner-center').offset().top;
		var ownerCenterBottom = ownerCenterTop + $('#owner-center').outerHeight();
		var footerTop = $(".subtext > *, footer").offset().top - $(window).height() / 2;


		//HIDE SHOW NAV
		if (scrollPosition > heroBottom && scrollPositionBottom < footerTop){
			$('#sticky-nav').addClass('active');
		}
		else if (scrollPosition < heroBottom){
			$('#sticky-nav').removeClass('active');
		}
		else if (scrollPositionBottom > footerTop){
			$("#sticky-nav").removeClass('active');
		}

		//NAV CHECKER
		if (scrollPositionBottom > heroTop && scrollPositionBottom < heroBottom){
			$('#sticky-nav ul li').removeClass('active');
		}

		else if (scrollPositionBottom >= referTop && scrollPositionBottom < referBottom){
			$('#sticky-nav ul li').removeClass('active');
			$('#refrigeration-sticky').addClass('active');
		}
		else if(scrollPositionBottom >= cookingTop && scrollPositionBottom < cookingBottom){
			$('#sticky-nav ul li').removeClass('active');
			$('#cooking-sticky').addClass('active');
		}
		else if(scrollPositionBottom >= dishwasherTop && scrollPositionBottom < dishwasherBottom){
			$('#sticky-nav ul li').removeClass('active');
			$('#dishwashers-sticky').addClass('active');
		}
		else if(scrollPositionBottom >= laundryTop && scrollPositionBottom < laundryBottom){
			$('#sticky-nav ul li').removeClass('active');
			$('#laundry-sticky').addClass('active');
		}
		else if(scrollPositionBottom >= ownerCenterTop && scrollPositionBottom < ownerCenterBottom){
			$('#sticky-nav ul li').removeClass('active');
			$('#owner-center-sticky').addClass('active');
		}
	}
	// scrollChecker();

	$(window).scroll(function(){
		scrollChecker();
	})

	$(document).on('click','a[href^="#"]', function(event){
		event.preventDefault();
		if ($($(this).attr('href')).offset()) {
			$('html, body').stop().animate({ scrollTop: $($(this).attr('href')).offset().top}, '100', 'swing');
			window.location.hash = $(this).attr('href');
		}
	})
</script>


