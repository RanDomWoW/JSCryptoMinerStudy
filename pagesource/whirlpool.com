        <script type="text/javascript">
            var digitalData = {}; //Data Layer Object Created
            facetNames = {};
            $("").each(function () {
                facetNames[""] = "";
            });

            digitalData.page = {
                pageInfo: {
                    "pageID": "/content/whirlpool/en_us.html",
                    "destinationURL": "http://www.whirlpool.com/content/whirlpool/en_us.html",
                    "referringURL": document.referrer,
                    "pageType": "whirlpool/digitalplatform/components/templates/homepage",
