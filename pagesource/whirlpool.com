                    "errorPage": !isNaN(document.title)
                }
            };

            digitalData.user = {
                profile: {
                    profileInfo: {
                        "profileID": getCookie('customerName'),
                        "profileEmail": getCookie('email'),
                        "productsOwned": {},
                        "lastInteraction": getCookie('lastLogin')
                    },
                    address: {
                        "city": getCookie('city'),
                        "state_province": getCookie('state'),
                        "postalcode": splitZip(getCookie('zipcode')),
                        "country": getCookie('country')
                    },
                    attributes: {
                        "loginStatus": getLoginStatus()
                    }
                }
            };

            function getCookie(cname) {
                var name = cname + "=";
                var ca = document.cookie.split(';');
                for (var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == ' ') {
                        c = c.substring(1);
                    }
                    if (c.indexOf(name) == 0) {
                        return c.substring(name.length, c.length);
                    }
                }
                return "";
            }

            function splitZip(zipCode) {
                var result = "";
                if (zipCode) {
                    result = zipCode.split(new RegExp('[-+()*/:? ]', 'g'))[0];
                }
                return result;
            }


            function getLoginStatus() {
                var status = "";
                var user = getCookie("customerName");
                if (!user || user.length == 0) {
                	status = "guest";        	
                } else {            	
                	status = "signed in";
                }

                return status;
            }

        </script>
    

