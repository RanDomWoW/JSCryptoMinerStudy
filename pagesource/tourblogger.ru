				<script>
				function Slider()
				{					
					var active = $('#slid_main').attr('active');
				
					if(active==2)
					var next = 1;
					else
					var next = 	parseInt(active)+1;					
					
					$('span[id*=slider_nav_]').attr('class','slider_page_nav');	
					$('#slider_nav_'+next).attr('class','slider_page_nav_active');													
					
					$('div[id*=slider]').fadeOut(500);					
					setTimeout(function () { $('#slider_'+next).fadeIn(200)}, 200);
					
					$('#slid_main').attr('active',next);
					
				}
				function Slide(id)
				{
					var interval=$('#slid_main').attr('interval');
					clearInterval(interval);
					
					$('span[id*=slider_nav_]').attr('class','slider_page_nav');	
					$('#slider_nav_'+id).attr('class','slider_page_nav_active');	
					
					$('div[id*=slider]').fadeOut(500);			
					setTimeout(function () { $('#slider_'+id).fadeIn(200)}, 200);
					$('#slid_main').attr('active',id);
					
					var interval=setInterval('Slider()', 8000);
					$('#slid_main').attr('interval',interval);					
				}
				
				 $(document).ready(function() 
				 {
					Slider();
					var interval=setInterval('Slider()', 8000);
					$('#slid_main').attr('interval',interval);
				});
				
			
				</script>	
	
