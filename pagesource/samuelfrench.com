<script type="text/javascript">
    $.widget("custom.catcomplete", $.ui.autocomplete, {
        _renderMenu: function (ul, items) {
            var that = this,
            currentCategory = "";
            $.each(items, function (index, item) {
                if (item.value.Category != currentCategory) {
                    if(item.value.Category != '')
                    {
                        if(ul.length > 1)
                        {
                            ul.append("<li class='ui-autocomplete-category cat-sep ui-menu-item'>" + item.value.Category);
                        }
                        else                                               
                        {
                            ul.append("<li class='ui-autocomplete-category ui-menu-item'>" + item.value.Category);
                        }
                        currentCategory = item.value.Category;
                    }
                }
                that._renderItem(ul, item);
            });
            ul.css('top','27px');
         },
    });
</script>

<script type="text/javascript">

    $("#txtHeaderSearchBox").catcomplete({
        appendTo: '#search-cont-HeaderSearchBox',
        source: function (request, response) {
            var term = JSON.stringify({ text: request.term, resultType : 2, seeMore : 1 });
            $.ajax({
                url: "/Catalog/PredictiveSearch",
                data: term,
                dataType: "json",
                type: "POST",
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                    var matcher = new RegExp( $.ui.autocomplete.escapeRegex(request.term), "i" );
                    response($.map(data, function (item) {
                        var labelText = item.Name;
                        if(!request.term || matcher.test(labelText)){
                            labelText = labelText.replace(new RegExp("\\b(?![^&;]+;)(?!<[^<>]*)(" + $.ui.autocomplete.escapeRegex(request.term) + ")(?![^<>]*>)(?![^&;]+;)", "gi"), "<strong>$1</strong>");
                        }

                        return {
                            value: item,
                            label: labelText
                        }
                    }))
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    //alert(textStatus);
                }
            });
        },
        focus: function (event, ui) {
            //this.value = ui.item.label.Name;
            event.preventDefault(); // Prevent the default focus behavior.
        },

        create: function (event, ui) {

        },
        open: function (event, ui) {
            var main = $('.main');
            var footer = $('.footer');
            if (footer.offset().top - $('#search-cont-HeaderSearchBox').offset().top < 100) {
                var totalHeight = main.height();
                var footerHeight = footer.outerHeight();

                main.height(totalHeight + $('#search-cont-HeaderSearchBox ul').outerHeight() - footerHeight);
            }
        },
        close: function (event, ui) {
            $('.main').removeAttr('style');
        },
        select: function (event, ui) {
            SaveQuery();

            window.location = ui.item.value.Url;
            return false;
        },
        minLength: 1
    })
    .data("catcomplete")._renderItem = function (ul, item) {
        if (item.value.Category != '') {
            return $("<li class='ui-autocomplete-item'></li>")
	    		.data("item.autocomplete", item)
		        .append("<a href='" + item.value.Url + "'>" + item.label + "</a>")
			    .appendTo(ul);
        }
        else {
            // set search link url
            // $('#btn-small-search-txtHeaderSearchBox').attr("href", item.value.Url);

            // add "see more" item
            return $("<li class='ui-autocomplete-item'></li>")
	    		.data("item.autocomplete", item)
		        .append("<a class='seeMore' href='" + item.value.Url + "'>" + item.value.Name + "</a>")
			    .appendTo(ul);
        }
    };

    $("#txtHeaderSearchBox").keydown(function(e) {
        if(e.which || e.keyCode){
            if ((e.which == 13) || (e.keyCode == 13)) { 
                var searchUrl = "/catalog/search?keyword=" + encodeURIComponent($("#txtHeaderSearchBox").val());
                                
                SaveQuery();

                window.location = searchUrl;
                return false;
            }
        }

        return true;
    });

    $("#btn-small-search-txtHeaderSearchBox").click(function(e) {
        var keyword = "";
        if( isValidSearchTerm( "txtHeaderSearchBox", "Search Titles, Authors, Keywords")){
            keyword = encodeURIComponent($("#txtHeaderSearchBox").val());
        }

        SaveQuery();
        
        var searchUrl = "/catalog/search?keyword=" + keyword;
        window.location = searchUrl;
    });

    function SaveQuery(){
        var string = $("#txtHeaderSearchBox").val();

        $.ajax({
            data: {query : string},
            url: '/Catalog/SaveQuery',
            type: "POST",
            async: false
        });
        
    };

</script>
