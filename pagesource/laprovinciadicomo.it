                                            'skipoffset':5
                                        }
                                    });

                                    var video_played_big_box = false;
                                    playerInstance_big_box.onPlay(function() {
                                        if (video_played_big_box==false){
                                            $.ajax("/counters/video/play?object_id=" + video_box_home_video_id +"&content_type_id="+video_box_home_content_type_id, {
                                                cache: false,
                                                crossDomain: false,
                                                xhrFields: {
                                                    'withCredentials': false
                                                },
                                                success: function(data, textStatus, jqXHR) {
                                                    video_played_big_box = true;
                                                },
                                                dataType:"json"
                                            });
                                        }
                                    });
                                }

                            });
                        }
                    }
                }

                if (done_rubriche == 0){
                    if ( $("#rubrichemobile").length ) {
                        if ((videoBoxFromTop - scrollTop < 1080)){
                            done_rubriche = 1;
                            $('#rubrichemobile_scroll_waiting').show();
                            $.get("/stories/get_news_rubriche/ajax/", function( data ) {
                                $( "#rubrichemobile" ).append(data);
                                $('#rubrichemobile_scroll_waiting').hide();
                            });
                        }
                    }
                }
            }
        }, 400);


        //video bigbar
        if ( $("#my-video-bigbar").length ) {
            var myVideo = videojs('my-video-bigbar', {
