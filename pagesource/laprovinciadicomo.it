                    }
                    if ($("#banner_kauppa_col2_2").length) {
                        if (scrollTop > 750){
                            done_kauppa = 1;
                            $.ajax("https://live.mediaon.it/live/ajax/box/col2/?no_object=1&city_id=&province_id=Como&site_id=1&content_type_id=715&target=_blank&section_pr=True", {
                                cache: false,
                                crossDomain: true,
                                xhrFields: {
                                    'withCredentials': false
                                },
                                success: function(data, textStatus, jqXHR) {
                                    $("#banner_kauppa_col2_2").html(data);
                                },
                                dataType:"html"
                            });
                        }
                    }
                }
                if (done_announces == 0){
                    if ( $("#announces_col2").length ) {
                        if (scrollTop > 750){
                            done_announces = 1;
                            $.ajax("https://live.mediaon.it/live/ajax/box/73/?no_object=11&city_id=&province_id=Como&site_id=1&content_type_id=73&target=_blank&col2=1&random=1", {
