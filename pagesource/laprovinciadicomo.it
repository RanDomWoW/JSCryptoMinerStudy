                            });
                        }
                    }
                }



                //video box
                if (done_video_box == 0){
                    if ( $("#day_video_big_box").length ) {
                        if ((videoBoxFromTop- scrollTop < 1080)){
                            done_video_box = 1;
                            $('#day_video_big_box_scroll_waiting').show();
