          window.open(text+"+"+x[0].getAttribute('href').replace("https://twitter.com/intent/tweet?url=",""), 'TwitterShare', 'height=450, width=550, top=' + ($(window).height() / 2 - 275) + ', left=' + ($(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
