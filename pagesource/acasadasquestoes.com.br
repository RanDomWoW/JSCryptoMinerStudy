<script>
    $(document).ready(function()
    {
        $(".visualizePrint").on('click',function(){
            var id = $(this).prop("id");
            $("#modalPrint .modal").html()
        });

        var Contador = setInterval(function(){
            $("#video").remove();
            var img = $(".demonstrativo").find("img").prop("src");
            var patt1 = /[1-9]/g;
            var result = parseInt(img.match(patt1))+1;
            if(result>4){
                result=1;
            }

            $(".demonstrativo").find("img").fadeOut('fast');
            $(".demonstrativo").find("img").prop("src","/front/assets/img/questoes"+result+".jpg");
            $(".demonstrativo").find("img").fadeIn();
        }, 5000);

        function Temporizador(acao){
            if(acao==="stop")
            {
                clearInterval(Contador);
            }
            else{
                Contador;
            }
        }

        Temporizador();

        $(".Anterior").on('click',function(event){
            event.stopPropagation();

            $("#video").remove();
            Temporizador("stop");
            var img = $(this).parent().parent().find("img").prop("src");
            var patt1 = /[1-9]/g;
            var result = parseInt(img.match(patt1))-1;
            if(result<1){
                result=4;
            }

            $(this).parent().parent().find("img").fadeOut('fast');
            $(this).parent().parent().find("img").prop("src","/front/assets/img/questoes"+result+".jpg");
            $(this).parent().parent().find("img").fadeIn();

            event.preventDefault();
        });

        $(".Proximo").on('click',function(event){
            
            event.stopPropagation();
            $("#video").remove();
            Temporizador("stop");

            var img = $(this).parent().parent().find("img").prop("src");
            var patt1 = /[1-9]/g;
            var result = parseInt(img.match(patt1))+1;
            if(result>4){
                result=1;
            }

            $(this).parent().parent().find("img").fadeOut('fast');
            $(this).parent().parent().find("img").prop("src","/front/assets/img/questoes"+result+".jpg");
            $(this).parent().parent().find("img").fadeIn();

            event.preventDefault();
        });

        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id))
                return;
            js = d.createElement(s);
            js.id = id;
            js.src = "//connect.facebook.net/pt_BR/all.js#xfbml=1&appId=398587363492398";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));


        $('#enviarContato').on('click',function(){

            var form = $("#formContato").serialize();

            $(this).text("Enviando sua mensagem...");

            Ajax("/mensagemSuporte",form, function(data){
                $('#enviarContato').text("Enviar minha mensagem =D");
                $("#modalEmail").modal('show');
            }, "", function(){
                $('#enviarContato').text("Enviar minha mensagem =D");
                alert("Houve um problema ao enviar sua mensagem, tente novamente =/");
            });

            return true;
        });


        $(".videoCorrecao").on('click',function(){
            Temporizador("stop");
            $(".demonstrativo img").hide();

            if($(".demonstrativo").find("iframe").length>0)
                $("#video").html('<iframe id="video" src="//www.youtube.com/embed/QxwDxyosDMk" width="100%" height="" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen ></iframe>');
            else
                $(".demonstrativo").prepend('<iframe id="video" src="//www.youtube.com/embed/QxwDxyosDMk" width="100%" height="" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen ></iframe>');

            $("#video").fadeIn();
            $("#video").css("display","table");
        });

        $(".assinar").on('click', function(){
           location.href="https://acasadasquestoes.com.br/planos";
        });

    });
</script>


