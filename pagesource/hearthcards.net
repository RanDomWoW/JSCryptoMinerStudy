            url: "putingallery.php",
            data: {
                action: 'putingallery',
                cid: pigcardid
            },
            cache: false,
            context: this,
            success: function(ret) {
             
                $(this).attr("src", "images/button_putingallerychecked.png");

            },
            error: function() {}
        })
    });    
  

</script>

