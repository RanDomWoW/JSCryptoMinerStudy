		function fnOpenPopup(popSeq, popW, popH, popL, popT){
			var target = "popup"+popSeq;
			var width = popW;
			var height = popH;
			var popLeftPos = (screen.width - popL) / 2;
			var popTopPos = (screen.height - popT) / 2;
			var popScroll = "Yes";
			var popOpt = "width="+ width +", height="+ height +", top="+ popTopPos +", left="+ popLeftPos +", scrollbars="+popScroll+", menubar=No, location=No, resizable=Yes, status=No, toolbar=No";
			pop = window.open("/seapt/main/popupView.do?popSeq="+popSeq , target, popOpt + "");
		}
		
