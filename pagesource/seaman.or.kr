		// 팝업 오픈 함수
		   function popupOpen(obj) {
		   	var cookieName = obj.cookieName;
		   	var url = obj.url;
		   	var target = obj.target;
		   	var width = obj.width;
		   	if (width == undefined) {
		   		width = 595;
		   	}
		   	var height = obj.height;
		   	if (height == undefined) {
		   		height = 445;
		   	}
		   	var limitDate = obj.limitDate;
		   	if (limitDate == undefined) {
		   		limitDate = new Date('9999/12/31 23:59');
		   	}
		   	var inTime = false;
		       if (new Date().getTime() <= limitDate.getTime()) {
		       	inTime = true;
		       }
		       if(inTime && getCookie(cookieName) != "done"){
		           window.open(url, target, "width=" + width + ", height=" + height + ", menubar=no,status=no,scrollbars=no");
		       }
		   }
		

		   function getCookie( name )
		   {
		       var nameOfCookie = name + "=";
		       var x = 0;
		       while ( x <= document.cookie.length )
		       {
		           var y = (x+nameOfCookie.length);
		           if ( document.cookie.substring( x, y ) == nameOfCookie )
		           {
		               if ( (endOfCookie=document.cookie.indexOf( ";", y )) == -1 )
		                   endOfCookie = document.cookie.length;
		               return unescape( document.cookie.substring( y, endOfCookie ) );
		           }
		           x = document.cookie.indexOf( " ", x ) + 1;
		           if ( x == 0 )
		               break;
		       }
		       return "";
		   }
	   
		function fnPopupCompulsion(){			
			$.ajax
			( { type:"post"
			, dataType:"xml"	
			, url:"/seapt/main/detailPopupOnLoadView.do"
			, async:false
			, success:function(xml){
				var popSeq = "";
				var popW = "";
				var popH = "";
				var popL = "";
				var popT = "";
				var popCookie = "";
				if ( $(xml).find("newaddr").length > 0 ) {
					$(xml).find("newaddr").each(function() {
						newAddrYn = $(this).find("newAddrYn").text();
						userSeq = $(this).find("userSeq").text();
						if(newAddrYn != "Y" ){  	
							fnNewAddr(userSeq);
						}
						email = $(this).find("email").text();
						handphone = $(this).find("handphone").text();
						if(email =='N' || handphone == 'N'){
							if(confirm("회원정보 중  필수정보가 없습니다. 정보수정을 하시겠습니까?") == true){
								location.href="/seapt/user/upuserinfo/selectUserInfoView.do";
							}
						}
					});
				}
				
				if ( $(xml).find("result").length > 0 ) {
					$(xml).find("result").each(function() {
						popSeq = $(this).find("popupzoneSeq").text();
						popW = $(this).find("widthSize").text();
						popH = $(this).find("heightSize").text();
						popL = $(this).find("widthPosition").text();
						popT = $(this).find("heightPosition").text();
						popCookie = "CK" + $(this).find("popupzoneSeq").text();
						if(fnGetCookie(popCookie) != "done" ){  	
							fnOpenPopup(popSeq, popW, popH, popL, popT);
						}
					});
				} else {
					return;
				}

				
			}
			, error: function(xhr, status, error) {
			}
		} 
		);}

		function fnGetCookie(name){
			var nameOfCookie = name + "=";
			var x = 0;
			while ( x <= document.cookie.length )
			{
				var y = (x+nameOfCookie.length);
				if ( document.cookie.substring( x, y ) == nameOfCookie ) {
					if ( (endOfCookie=document.cookie.indexOf( ";", y )) == -1 )
						endOfCookie = document.cookie.length;
					return unescape( document.cookie.substring( y, endOfCookie ) );
				}
				x = document.cookie.indexOf( " ", x ) + 1;
				if ( x == 0 )
					break;
			}
			return "";
		}
	
