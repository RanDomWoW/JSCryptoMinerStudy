			var section1 = $('#section-wrap > div:first-child').attr('id');
			var fn1 = getFn(section1);
			loadSection(fn1, $("#"+section1));
		}
		setTimeout(function() { 
			if($('#section-wrap > div:nth-child(2)').length)
			{
				var section2 = $('#section-wrap > div:nth-child(2)').attr('id')
				var fn2 = getFn(section2);
				loadSection(fn2, $("#"+section2));						
			}
			if($('#section-wrap > div:nth-child(3)').length)
			{
				var section3 = $('#section-wrap > div:nth-child(3)').attr('id');
				var fn3 = getFn(section3);
				loadSection(fn3, $("#"+section3));
			}
			if($('#section-wrap > div:nth-child(4)').length)
			{
				var section4 = $('#section-wrap > div:nth-child(4)').attr('id');
				var fn4 = getFn(section4);
				loadSection(fn4, $("#"+section4));
			}
			if($('#section-wrap > div:nth-child(5)').length)
			{
				var section5 = $('#section-wrap > div:nth-child(5)').attr('id');
				var fn5 = getFn(section5);
				loadSection(fn5, $("#"+section5));
			}
			if($('#section-wrap > div:nth-child(6)').length)
			{
				var section6 = $('#section-wrap > div:nth-child(6)').attr('id');
				var fn6 = getFn(section6);
				loadSection(fn6, $("#"+section6));
			}
		}, 2000);
		
		function getFn(sectionName)
		{
			var arrSectionName = ['latest-section','popular-section','livestream-section','world-section','sponsored-section','kapamilya-section'];
			var arrFn = ['get_recently_added','get_most_watched','get_livestreams','get_all_worlds','get_sponsored_worlds','get_all_shows'];
			return arrFn[arrSectionName.indexOf(sectionName)];
		}
		
		function loadSection(fn, $section){
			$section.show();

			$.ajax({
				url: 'tv/'+fn+'/'+winWidth,
				method: 'GET',
				success: function(response)
				{
					$section.html(response);
					loadedSectionCount++;
					makeMobileReponsive();
					if(loadedSectionCount === max_section)
				    {
						$('#section-wrap > div > section').each(function(i){
							if(i%2)
							{
							   $(this).addClass('section-bg-color');
							}
						});
				    }

				}
			})
		}
	// })
</script>