        <script defer type="text/javascript">require(['dojo/query', 'dojo/_base/fx', 'dojo/dom-class', 'dojo/domReady!'], function(query, fx, domClass){	setTimeout(function() {	var content          =  query('section#main, div#mainNav, header, footer, div#tools');	content.forEach(function(node)	{		fx.animateProperty(			{				node : node,				properties :				{ opacity: 1 },				onEnd: function()				{					domClass.remove( query(node)[0], 'hidden' );				}			}).play();	});	}, 100);});</script></body></html>