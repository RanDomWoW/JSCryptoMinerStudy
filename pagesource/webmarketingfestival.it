			$('#immagine-home-video img').click(function(c) {
		        c.preventDefault();
		        $("#immagine-home-video").hide();
				$("#contenuto-home-video").show();
				e.target.playVideo();
		    });
		}

		
		function videoLoad() {
		    if ((window.innerWidth > 599) &&(!$( ".videoCoverCont" ).hasClass('loaded'))) {
		        $( ".videoCoverCont" ).load( "/2017/fascia-video.php", function() {
		            $( ".videoCoverCont" ).addClass('loaded');

		        });
		    }
		}
		videoLoad();

		
		</script>

