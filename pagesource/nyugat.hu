function showMenu(event){
	$('#mobil_menu').toggle();
	window.scrollTo(0,0);
	if($('.showmenu').hasClass('on')){
		$('.showmenu').removeClass('on')}
	else{
		if($('.showsoc').hasClass('on')){
			$('.showsoc').removeClass('on');
			$('#social_menu').toggle();}
		$('.showmenu').addClass('on')}}
function showSoc(event){
	$('#social_menu').toggle();
	window.scrollTo(0,0);
	if($('.showsoc').hasClass('on')){
		$('.showsoc').removeClass('on')}
	else{
		if($('.showmenu').hasClass('on')){
			$('.showmenu').removeClass('on');
			$('#mobil_menu').toggle();}
		$('.showsoc').addClass('on')}}
</script>

