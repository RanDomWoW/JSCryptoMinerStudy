            jQuery.ajax({
            method: 'GET',
            url: ajaxurl,
            headers: {'Content-Type': 'application/x-www-form-urlencoded'},
            data: {
                action: "get_video_gallery",
            },
            success: function (data) {
                var feeds = data;

                if (feeds.videos.length < 1) return;

                var videos = "";
                var video_template = jQuery('#video_template').html();


                for (var i = 0; i < feeds.videos.length; i++) {
                    videos += Mustache.to_html(video_template, feeds.videos[i]);
                }


                delete feeds.videos;

                jQuery("#videogallery-container").html(Mustache.to_html(jQuery('#videogallery-container-template').html(), feeds));
                jQuery("#videogallery").html(videos);

            }
        });

        </script>  </div><!--container-->
