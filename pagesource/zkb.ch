<script>
$(function () {
    $('.header-login-dropdown').each(function(index, item) {
        var timer = null;
        var loginDropdown = new Cl.Accordion(item, {
            'event': 'click',
            'forceClose': true,
            'duration': 0,
            'cls': {
                'trigger': '.header-login-dropdown-trigger',
                'container': '.header-login-dropdown-menu'
            }
        });

        $(item).on('mouseenter mouseleave', function (e) {
            clearTimeout(timer);
            if (e.type === 'mouseleave') {
                timer = setTimeout(function () {
                    loginDropdown.hide();
                }, Cl.Base.delayConfig.delayLeave);
            }
        });
    }).loginService({
        onLoggedIn: function (elem, username) {
            elem.find("h2.header-login-dropdown-title").html(username);
            elem.find("div.hl-logged-out").remove();
            elem.find("div.hl-logged-in").removeClass("hidden").addClass("block");
            elem.addClass("header-login-dropdown-logged-in");
        },
        onLoggedOut: function (elem) {
            elem.find("div.hl-logged-in").remove();
            elem.find("div.hl-logged-out").removeClass("hidden").addClass("block");
        }
    });
})
