			var showServicePackAlert = function() {
				if(servicepackAlertTimeout!=-1) {
					window.clearTimeout(servicepackAlertTimeout);
					servicepackAlertTimeout = -1;
				}

				if(!servicepackAlertShown && parseInt($('servicepackAlertContainer').getStyle('right')) < 0) {
					servicepackAlertShown = true;
					$('servicepackAlertContainer').setStyle('visibility', 'visible');
					//console.log(parseInt($('servicepackAlertContainer').getStyle('bottom')));
					$('servicepackAlertContainer').set('tween', {
					    duration: 'normal',
					    transition: Fx.Transitions.Sine.easeInOut,
					    unit: 'px'
					});
					$('servicepackAlertContainer').tween('right', 0);
				}
			};
			
			window.addEvent('domready', function() {
								if($('servicepackAlertContainer')!=null) {
					var servicepackAlertDelay = 5000;

					$('servicepackAlertContainer').setStyle('right', (-1*$('servicepackAlertContainer').getSize().x) + 'px');
					$$('#servicepackAlert').addEvent('click', function() {
						window.location.href = $$('#servicepackAlert .actionButton a').get('href');
					});
					$$('#servicepackAlert #servicepackClose').addEvent('click', function(e) {
						e.preventDefault();
						$('servicepackAlert').fade('out');
						return false;
					});
					window.addEvent('scroll', function() {
						if(servicepackAlertTimeout!=-1) {
							window.clearTimeout(servicepackAlertTimeout);
						}
						servicepackAlertTimeout = window.setTimeout(showServicePackAlert, servicepackAlertDelay);

						if(window.getScrollTop() > ($('topContent').getSize().y+$('whyUs').getSize().y)) {
							showServicePackAlert();
						}
					});

					servicepackAlertTimeout = window.setTimeout(showServicePackAlert, servicepackAlertDelay);
				}
				
				var login = $('loginWelcome');
				var accediItem = $$('.accediItem');
				var navigationTop = $$('.navigationTop');

				login.grab($('loginFormContainer'), 'top');
				login.set('tween', { onComplete:function(e) {
