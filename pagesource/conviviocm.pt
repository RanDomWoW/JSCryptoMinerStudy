        var contentTypeName = "ClassificadosConvivio";
        $('#barra-cookies .btn-permitir').click(function () {
            $.when(CreateCookiePolicyCookie(true, contentTypeName)).done(function () {
                $('#barra-cookies').slideUp();
            });
        });

        ///Criar a cookie automagicamente.
        CreateCookiePolicyCookie(true, contentTypeName);
    });

    ///Método para criar a cookie
    function CreateCookiePolicyCookie(allowsCookies, contentTypeName) {
        return $.ajax({
            url: '/HTTPHandlers/GenericHandler.ashx',
            type: 'GET',
            cache: false,
            data: {
                op: 'SetCookiePolicyCookie',
                AllowCookies: allowsCookies,
                ContentTypeName: contentTypeName
            },
            beforeSend: function () { },
            complete: function () { },
            success: function (responseData) { }
        });
    }

</script>

    
