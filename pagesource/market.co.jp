    document.addEventListener('DOMContentLoaded', function () {
        particleground(document.getElementById('particle01'), {
            dotColor: 'rgba(255,255,255,0.4)',
            lineColor: 'rgba(255,255,255,0.3)'
        });
    }, false);
    document.addEventListener('DOMContentLoaded', function () {
        particleground(document.getElementById('particle02'), {
            dotColor: 'rgba(255,255,255,0.4)',
            lineColor: 'rgba(255,255,255,0.3)'
        });
    }, false);
    document.addEventListener('DOMContentLoaded', function () {
        particleground(document.getElementById('particle03'), {
            dotColor: 'rgba(255,255,255,0.4)',
            lineColor: 'rgba(255,255,255,0.3)'
        });
    }, false);

    $(document).ready(function() {
        //IEの場合オープニングアニメーション無効
        if (!$.support.noCloneChecked) {
            location.hash = 'skip';
        }

        //リロード時のハッシュタグを無効にする
        if(location.hash=='#feature01' || location.hash=='#feature02' || location.hash=='#feature03'){
            location.hash = 'top';
        }

        $('.drawer').drawer();

        //テキストアニメーション終了時
        $('.textArea h3 span,.textArea p span').on('webkitAnimationEnd', function(){
            $(this).css('background','transparent');
        });

        //「About Us」、「Our Business」クリック時の遷移の動き
        $('#wrapper:not(body#index #wrapper)').css({display:'block',marginLeft:$(window).width(),opacity:'0'});
        $('#wrapper:not(body#index #wrapper)').animate({marginLeft:'0px',opacity:'1'},500);
        $('body#index #wrapper').css({display:'block',opacity:'0'});
        $('body#index #wrapper').animate({opacity:'1'},500);
        $('.about_link').click(function(){
            var pass = $(this).attr("href");
            $('#wrapper').animate({marginLeft:'+=' + $(window).width() + 'px'},550,function(){
                location.href = pass;
                setTimeout(function(){
                    $('#wrapper').css({marginLeft:'0',opacity:'1'})
                },100);
            });
            return false;
        });
        $('.business_link').click(function(){
            var pass = $(this).attr("href");
            $('#wrapper').animate({marginLeft:'-=' + $(window).width() + 'px'},550,function(){
                location.href = pass;
                setTimeout(function(){
                    $('#wrapper').css({marginLeft:'0',opacity:'1'})
                },100);
            });
            return false;
        });


        //トップページのアニメーション
        var now = (new Date()).getTime();
        $('#container_top').find('.top_links,.opening,.main').hide();
        $('#container_top').find('.feature').css('visibility,hidden');
        $(window).on('load',function(){
            if(location.hash=='#skip'){
                //URLに#skipがつく場合（オープニングアニメーションなし）
                $('#container_top').find('.main').fadeIn();
                setTimeout(function () {
                    $('#container_top').find('.top_links,.feature').fadeIn(700);
                }, 500);
            }else{
                //URLに#skipがつかない場合（オープニングアニメーションあり）
                $('.top_links,.feature').hide();
                $('#container_top').find('.opening').fadeIn();
                $('.opening').css({
                    backgroundImage: 'url("img/opening.gif' + '?' + now +'")'
                });
                setTimeout(function () {
                    $('#container_top').find('.opening').fadeOut();
                    $('#container_top').find('.main').fadeIn();
                    setTimeout(function () {
                        $('#container_top').find('.top_links,.feature').fadeIn(700);
                    }, 1500);
                }, 4200);
            }
        });

        $(window).on('load resize',function(){
            $('#menu,.top_links,.feature .textArea, canvas, #logo').height($(window).height());
            $('.particle').css('marginTop','-'+$(window).height()+'px');
            if ($(window).width() < 680) {
                $('.drawer-hamburger').addClass('forSP');
            } else {
                $('.drawer-hamburger').addClass('forPC');
            }
        });
        $('#container_top').fullpage({
            onLeave: fullpageEvent,
            autoScrolling: true,
            anchors: ['top', 'feature01', 'feature02', 'feature03'],
            menu: '#menu'
        });
        $('#menu').hide();
        $('.drawer-hamburger').css({
            'top': '0',
            'marginTop': '0'
        });
    });
    var fullpageEvent = function(index, nextIndex, direction){
        if(nextIndex == 1){
            $('#menu').fadeOut();
            $('.top_links').delay(500).fadeIn();
            $('.drawer-hamburger').css({
                'top': '0',
                'marginTop': '0'
            });
        }else{
            $('#menu').fadeIn();
            $('.top_links').fadeOut();
            $('.drawer-hamburger.forPC').css({
                'top': '50%',
                'marginTop': '-24px'
            });
        }
        if(nextIndex == 2){
            $('#textArea01').addClass("slideInDelay");
            $('.top_links').hide();
        }else if(nextIndex == 3){
            $('#textArea02').addClass("slideInDelay");
            $('.top_links').hide();
        }else if(nextIndex == 4){
            $('#textArea03').addClass("slideInDelay");
            $('.top_links').hide();
        }else{
            //それ以外の時の処理
        }
    }
