 <script>
    (function($){
        var Directives = Telescope.Directives, Directive = Telescope.classes.Directive ;
        Directives['togglable-carousel'] = Directive.extend({
            templates: {
                nav:'<div class="t-carousel-nav"></div>',
                bullet:'<span class="t-carousel-bullet"></span>',
                next:'<span class="t-carousel-next"><i class="sbicon sbicon-dropright"></i></span>',
                prev:'<span class="t-carousel-prev"><i class="sbicon sbicon-dropleft"></i></span>'
            },
            initialize: function(){
                Directive.prototype.initialize.apply(this, arguments);
                this._options = $.parseJSON(this.expression) || {} ;

                this.setUp();

            },
            load: function(){
                var $el = this.$el ;
                $el.addClass('t-carousel-core');
                this.$el.find('.t-carousel-next, .t-carousel-prev').show();
            },
            unload: function(){

                this.$el.removeClass('t-carousel-core');
                this.$el.find('.t-carousel-next, .t-carousel-prev').hide();
            },
            zSort: function(){

            },
            onScopeAttach: function(){
                var that = this ;
                this.scope.set('current', 0);
                var bullets =  this.$el.find('.t-carousel-bullet') ;
                var count = bullets.length;
                bullets.click(function(){
                    that.scope.set('current',$(this).data('t-index') );
                });
                var arrows =  this.$el.find('.t-carousel-next, .t-carousel-prev') ;


                arrows.click(function(){

                    var t = $(this);
                    var c = that.scope.get().current;

                    if (t.hasClass('t-carousel-next')) {
                        if (c < count-1){
                            that.scope.set('current', c + 1 );
                        }else {
                            that.scope.set('current', 0 );
                        }
                    }else{
                        if (c > 0){
                            that.scope.set('current', c - 1 );
                        }else {
                            that.scope.set('current', count-1 );
                        }
                    }

                });
            },
            checker: function(){
                if ($(window).width() < this._options.max) this.load();
                else this.unload();
            },
            onScopeChange: function(){

                var current = this.scope.get().current ;
                var elements =  this.$el.find('.t-carousel-slide') ;

                elements.removeClass('active');
                $(elements[current]).addClass('active');

                var bullets =  this.$el.find('.t-carousel-bullet') ;

                bullets.removeClass('active');
                $(bullets[current]).addClass('active');

            },
            setUp: function(){
                var that = this ;
                var $el = this.$el;
                var elements =  this.$el.find(this._options['slideSelector']) ;
                /*CORE*/

                /* NAVIGATION */
                var $nav = $(that.templates.nav);
                $el.append($nav);

                /* PREV/NEXT */
                $el.append($(that.templates.next));
                $el.append($(that.templates.prev));

                /*SLIDES AND BULLETS*/
                $.each(elements, function(idx, elem){

                    var $elem = $(elem);
                    $elem.addClass('t-carousel-slide');
                    $elem.attr('data-carousel-index', idx);

                    var $bullet = $(that.templates.bullet);
                    $bullet.attr('data-carousel-index', idx);

                    $bullet.data('t-index',idx);

                    $nav.append($bullet);

                });

                /* RESIZE MANAGER */
                $(window).resize($.proxy(this.checker, this));
                this.checker();

            }
        });
        $('.paperboard, .products').telescope({directiveSelector:'[data]',
            data:{
                cslide: null
            }
        });
    })($sfr);
    
    //$sfr('header').data('scope').set('current',1);
    //$sfr('header').data('scope').set('subMenu',1);
    //$sfr('header').data('scope').set('subHeader',3);

</script>

