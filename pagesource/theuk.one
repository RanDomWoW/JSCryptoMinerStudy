  jQuery.simpleWeather({
    location: 'London, England, GB',
    woeid: '',
    unit: 'c',
    success: function(weather) {
      var timestamp = moment(weather.updated);
//      html = '<span class="w-location">'+weather.city+', '+weather.region+'</span>';
      html = '<span class="w-weather"><img style="position:absolute; left:0px; top:9px; height: 32px!important;" src="'+weather.image+'"> '+weather.high+'&deg;'+weather.units.temp+' &uarr;</span>';
      html += '<span class="w-weather"> '+weather.low+'&deg;'+weather.units.temp+' &darr;</span>';
      //html += '<span class="w-wind">'+weather.wind.direction+' '+weather.wind.speed+' '+weather.units.speed+'</span>';
  
      jQuery("#weather").html(html);
        
        $.post
        (
            "/save_weather.php",
            {
                field : "weather",
                content : html
            }
        );
    },
    error: function(error) {
        $.post
        (
            "/load_weather.php",
            {
                field : "weather"
            },
            function(ret)
            {
                ret=$.trim(ret);
                
                if (ret!="")
                {
                    $("#weather").html(ret);
                }
                else
                {
                    $("#weather").html('<p style="display: none;">'+error+'</p>');
                }
            }
        );
    }
  });

  jQuery.simpleWeather({
    location: 'London, England, GB',
    woeid: '',
    unit: 'c',
    success: function(weather) {
      var timestamp = moment(weather.updated);
      html = '<span class="w-location-m">Лондон</span>';
      html += '<span class="w-weather-m"><img style="position:absolute; left:0px; top:0px; height: 32px!important;" src="'+weather.image+'"> '+weather.high+'&deg;'+weather.units.temp+' &uarr;</span>';
      html += '<span class="w-weather-m"> '+weather.low+'&deg;'+weather.units.temp+' &darr;</span>';
  
      jQuery("#weather-m").html(html);
        
      $.post
      (
        "/save_weather.php",
        {
            field : "weather_m",
            content : html
        }
      );
    },
    error: function(error) {
        $.post
        (
            "/load_weather.php",
            {
                field : "weather_m"
            },
            function(ret)
            {
                ret=$.trim(ret);
                
                if (ret!="")
                {
                    $("#weather_m").html(ret);
                }
                else
                {
                    $("#weather_m").html('<p style="display: none;">'+error+'</p>');
                }
            }
        );
    }
  });  
});
</script>    

                   


