function submitSearch(searchLink){
var query = document.instituteSearchForm.query.value;
if (query == '') {
query = "";
}
var state = document.instituteSearchForm.state ? document.instituteSearchForm.state.value : null;
var cityState = document.instituteSearchForm.city ? document.instituteSearchForm.city.value : null;
var city;
if (cityState != null) {
var cityStateArr = cityState.split(':');
if(state == '' || state==null)
state = cityStateArr[0];
city = cityStateArr[1];
}
var category = document.instituteSearchForm.category ? document.instituteSearchForm.category.value : null;
var uLink = '';
if (city != null) {
uLink = '/mba/colleges/state/' + state + "/city/" + city;
} else if (state) {
uLink = '/mba/colleges/state/' + state;
} else {
uLink = '/mba/colleges';
}
if (category) {
uLink += '/category/' + category;
}
if (query) {
uLink += '?query=' + query;
}
searchLink.href = uLink;
return true;
//document.instituteSearchForm.submit();
}
</script>
