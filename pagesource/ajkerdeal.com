    function ObjectMapper(item) {
        var obj = {}, elem = [];
        var catItem = item.CategorySuggestion;
        var productItem = item.ProductSuggestion;
        var merchantItem = item.MerchantSuggestion;

        if (catItem != null) {
            for (var i = 0; i < catItem.length; i++) {

                elem.push({
                    CategoryName: catItem[i].CategoryName,
                    CategoryId: catItem[i].CategoryId,
                    SubCategoryId: catItem[i].SubCategoryId,
                    SubSubCategoryId: catItem[i].SubSubCategoryId,
                    DealCount: catItem[i].ProductCount
                });

            }
        }
        if (merchantItem != null) {
            for (var i = 0; i < merchantItem.length; i++) {
                elem.push({
                    CompanyName: merchantItem[i].CompanyName,
                    ProfileId: merchantItem[i].ProfileId,
                    RoutingName: merchantItem[i].RoutingName
                });
            }
        }
        if (productItem != null) {
            for (var i = 0; i < productItem.length; i++) {
                elem.push({
                    DealTitle: productItem[i].DealTitle,
                    DealPrice: productItem[i].DealPrice,
                    DealId: productItem[i].DealId,
                    FolderName: productItem[i].FolderName,
                    AccountsTitle: productItem[i].DealTitleEng
                });
            }
        }


        return elem;


    }

