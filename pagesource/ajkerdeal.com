<script type="text/javascript">
    function SetLangToCookie(cookieName, lang, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();

        var path = "path=/";
        //var domain = "domain=localhost";
        var domain = "domain=ajkerdeal.com";
        document.cookie = cookieName + "=" + lang + "; " + expires + "; " + domain + "; " + path;
        LoadPageByLang(lang);
    }

    function LoadPageByLang(lang) {
        var path = "";
        if (lang == 'eng') {
            path = window.location.host;
            path += '/en';
            path = path + window.location.pathname;
            window.location = 'https://' + path.toLowerCase();
        }
    }
</script>


