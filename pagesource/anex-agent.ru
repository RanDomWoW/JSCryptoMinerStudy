<script type="text/javascript">     
    $('select#tscountry').change(function(){        
        $.get( 
            "/ajax/GetCities/"+$(this).val()+"/",
            function( json_data ) {
                $( "select#tsplace" ).selectBox('destroy');
                $( "select#tsplace" ).find("option").remove();
                $( "select#tsplace" ).append('<option value="0" selected="selected">Курорт - любой -</option>');
                $.each(json_data, function(i, obj){
                    var option = document.createElement("option");
                    option.setAttribute("value", obj.id);
                    option.innerText = obj.name;
                    $( "select#tsplace" ).append(option); 
                });
                $( "select#tsplace" ).selectBox();                
            }
        );
    });
    $('#main_slider_form a.button').click(function(){
        var form     = $('#main_slider_form');
        var formData = form.serialize();
        formData += '&date='+form.find(".datepicker").datepicker({dateFormat:'dd.MM.yyyy'}).val();
        if(form.find('.irating__in span._cur').length) {
            formData += '&hotelCat='+form.find('.irating__in span._cur').data('id');
        } else {
            formData += '&hotelCat=0';
        }
        form.find('.qntChild__item').each(function( index ) {        
            var age = $(this).find('.qntChild__item__txt').text().replace('до 2 лет','1').replace(/[^0-9]+/,'');
            formData += '&kid[]='+age;            
        });  
        window.location.href = '/search/?'+formData;
        return false;
    })    
</script>


