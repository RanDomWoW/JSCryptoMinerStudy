<script type="text/javascript"> 
    var h_hght = 100; // высота шапки
    var h_mrg = 0;
    $(function () {
        $(window).scroll(function () {
            var top = $(this).scrollTop();
            var left = $(this).scrollLeft();
            var elem = $('.menu-cont');
            elem.css('left',-left);
            if (top + h_mrg < h_hght) {
                elem.css('top', (h_hght - top));
            } else {
                elem.css('top', h_mrg);
            }
        });
    });
</script>



