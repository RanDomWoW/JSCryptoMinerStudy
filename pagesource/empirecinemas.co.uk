    <script type="text/javascript">
        var gaTracker = new function()
        {
            this.enabled = false;
            this.queue   = [];

            this.trackingID = null;

            this.utm_source = 'empirecinemas';

            // Setup the required campaign details
            // eg. http://www.empirecinemas.co.uk/?utm_source=http%3A%2F%2Fwww.empirecinemas.co.uk%2Fselect_cinema_first%2Fci%2F&utm_medium=HPTO&utm_campaign=2015-M01-Sniper
            this.HPTOTarget   = '/synopsis/tomb_raider/f5968';
            this.HPTOCampaign = '2018-M03-Tomb-Raider-NOW-HPTO';

            this.init = function(trackingID)
            {
                this.trackingID = trackingID;
                this.enabled = true;

                ga('create', this.trackingID, 'auto');
                ga('send', 'pageview');

                this.sendQueue();
            }

            this.sendQueue = function()
            {
                var self      = this;
                var _tmpQueue = this.queue;
                this.queue    = [];

                _tmpQueue.forEach(function(trackObj) {
                    self.track(trackObj);
                });

            }

            this.track = function( trackObj )
            {
                if (this.enabled) {
                    ga('send', trackObj);
                } else {
                    this.queue.push(trackObj);
                }
            }

            this.trackImpression = function(category, label)
            {
                this.track({
                    'hitType'       : 'event',      // Required.
                    'eventCategory' : category,     // Required.
                    'eventAction'   : 'Impression', // Required.
                    'eventLabel'    : category + "-" + label
                });
            }

            // Track Clicks -> Click Through Rate
            this.trackCTR = function(category, label)
            {
                this.track({
                    'hitType'       : 'event',    // Required.
                    'eventCategory' : category,   // Required.
                    'eventAction'   : 'Click',    // Required.
                    'eventLabel'    : category + "-" + label
                });
            }

            this.trackCampaign = function(type, campaign, campaignTarget, linkTarget)
            {


                this.trackCTR(type, campaign);
                // var link = campaignTarget + this.setupCampaignLink(type, campaign);
                var link = campaignTarget;

                window.open(link, linkTarget);
                return false;
            }

            this.trackOutboundLink = function(url, action, category, isFlash)
            {
                try {
                    this.trackCTR(category, action);
                    if (isFlash) {
                        setTimeout('window.location = "' + url + '"', 100);
                    }
                } catch(err) {
                    //
                }
            }


            this.handleBodyClick = function(e)
            {
                // Is it the body?
                if ($( e.target ).is('body')) {
                    e.preventDefault();
                    e.stopPropagation();

                    this.trackCampaign('HPTO', this.HPTOCampaign, this.HPTOTarget, "_self");
                }
            }

            // this.setupCampaignLink = function(category, label)
            // {
            //     // Setup Campaign Details
            //     var utm_medium   = category;
            //     var utm_campaign = label;

            //     return '?utm_source=' + this.utm_source + '&utm_medium=' + utm_medium + '&utm_campaign=' + utm_campaign;
            // }

            this.trackTransaction = function(data)
            {
                if (this.enabled) {

                    ga('require', 'ecommerce');

                    ga('ecommerce:addTransaction', {
                        'id'          : data.bookingProcessId,  // Transaction ID. Required.
                        'affiliation' : data.siteName,          // Affiliation or store name.
                        'revenue'     : data.amount,                 // Grand Total.
                        'currency'    : 'GBP'
                    });

                    ga('ecommerce:addItem', {
                        'id'       : data.bookingProcessId,              // Transaction ID. Required.
                        'name'     : data.filmTitle,    // Product name. Required.
                        'price'    : data.amount,
                        'quantity' : 1,
                        'sku'      : String(data.sku),
                        'category' : data.category
                    });

                    //submits transaction to the Analytics servers
                    ga('ecommerce:send');
                }
            }
        }
    </script>

    
