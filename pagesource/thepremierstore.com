  <script type="text/javascript">
    $(document).ready(function(){
      $("body").click(function(e){
          var flyout = $("#menuCart");
          if (flyout.css("display") == "block") {
            var target = e.target;
            if ($(target).attr("id") != "dropDown-cart" && $(target).parent().attr("id") != "dropDown-cart") {
              var foW = flyout.width();
              var foH = flyout.height();
              var foL = flyout.offset().left;
              var foT = flyout.offset().top;
              var x = e.clientX;
              var y = e.clientY;
              
              // is the mouse outside of the cart menu region?
              if ((x < foL) || (x > (foL + foW)) || (y < foT) || (y > (foT + foH))) {
                $("#dropDown-cart").click();
              }
            }
          }
      });
    });
  </script>
  
