	.slide{ display: none; width: 1050px; border:1px solid #e7e7e7;}
	.cnt{ position: absolute; bottom: 0; left:0; width: 100%; display: table; table-layout: fixed; border-top:1px solid #e7e7e7; border-bottom: 1px solid #e7e7e7;}
	.cnt a{ display: table-cell; margin:0; padding: 0; text-align: center; height: 32px; line-height: 32px; background: #FFFFFF; color: #333333; opacity:0.7; filter:alpha(opacity=70);}
	.cnt a.active{ background: #2a2a2a; text-decoration:none; color: #FFF; opacity:1; filter:alpha(opacity=100); }
