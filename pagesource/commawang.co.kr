<script type="text/javascript">
	$(function(){
		var img = $(".slide");
		var $this = $(".cnt a");
		$this.mouseover(function(){
			var index = $(this).index();
			$this.removeClass("active");
			$(this).addClass("active");	
			img.hide();
			img.eq(index).show();
			return false;
		})
		setInterval(function(){
			if(!$(".active").next().is("a")){
				$this.eq(0).trigger("mouseover");
			}else{
				$(".active").next().trigger("mouseover");
			}
		},7000)
	})
</script>
											
