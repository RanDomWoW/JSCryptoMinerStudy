			var countImg = $("#btmCounter_bnrHDR td");
			countImg.eq(0).addClass("activeHDR");
			
			/* 다음버튼 */
			$("#next_bnrHDR").click(function(){
				var thisImg = rollingId.find("a[show=\"Y\"]");
				RollingImg.hide();
				rollingId.find("[show]").attr("show","N");
				countImg.removeClass("activeHDR");
				
				if(thisImg.next().is("a")){
					thisImg.next().attr("show","Y");
					thisImg.next().fadeIn("slow");
					countImg.eq(thisImg.prev().index()).addClass("activeHDR");
					countImg.eq(thisImg.prev().prev().index()).removeClass("activeHDR");
				}else{
					RollingImg.eq(0).attr("show","Y");
					rollingId.find("a[show=\"Y\"]").fadeIn("slow");
					countImg.eq(0).addClass("activeHDR");
				}
			});

			/* 이전버튼 */
			$("#prev_bnrHDR").click(function(){
				var thisImg = rollingId.find("a[show=\"Y\"]");
				RollingImg.hide();
				rollingId.find("[show]").attr("show","N");
				countImg.removeClass("activeHDR");
										
				if(thisImg.prev().is("a")){
					thisImg.prev().attr("show","Y");
					thisImg.prev().fadeIn("slow");
					//console.log(thisImg.prev().index());
					var prIndex = thisImg.prev().index();
					countImg.eq(thisImg.prev().index()-2).addClass("activeHDR");
				}else{
					RollingImg.eq(RollingImg.length-1).attr("show","Y");
					rollingId.find("a[show=\"Y\"]").fadeIn("slow");
					countImg.eq(RollingImg.length-1).addClass("activeHDR")
				}
			});

			/* 하단 슬라이드 */
			$("#btmCounter_bnrHDR td").mouseover(function(){
				var thisEq = $(this).index();
				RollingImg.attr("show","N");
				RollingImg.eq(thisEq).attr("show","Y");
				RollingImg.hide();
				rollingId.find("a[show=\"Y\"]").show();
				countImg.removeClass("activeHDR");
				$(this).addClass("activeHDR");
			});

			setInterval(function(){
				 $("#next_bnrHDR").trigger("click");
			},5000)
		})
</script>
											
			
