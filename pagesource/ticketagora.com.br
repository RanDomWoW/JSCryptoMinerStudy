    <!-- DATE RANGE PICKER -->
    <link href="../../css/daterangepicker/daterangepicker.css" rel="stylesheet" type="text/css" />
    <script src="../../js/plugins/daterangepicker/daterangepicker.js" type="text/javascript" ></script>

    <script type="text/javascript">
        $(function () {
            
            $('.slider-busca .dropdown-menu a').on('click', function (e) {
                e.preventDefault();
                var type = $(this).attr('data-type');
                var value = $(this).attr('data-value');
                switch (type) {
                    case 'estado': $('#hdf_estado').val(value); break;
                }
                $(this).closest('.btn-group').find('button').html($(this).html() + '<span class="caret pull-right"></span>');
            });

            $('.filtro-evento').on('click', 'a[rel="add"]', function (e) {
                e.preventDefault();
                $('ul.selected').append('<li><a href="#" data-value="' + $(this).attr('data-value') + '" rel="del"><i class="fa fa-times-circle"></i> ' + $(this).attr('data-text') + '</a></li>');
                UpdateAdvanced();
            });

            $('.filtro-evento').on('click', 'a[rel="del"]', function (e) {
                e.preventDefault();
                $(this).closest('li').remove();
                UpdateAdvanced();
            });

            $('#filtro-data-de').daterangepicker({
                autoApply: true,
                singleDatePicker: true,
                autoUpdateInput: false,
                locale: {
                    format: "DD/MM/YYYY",
                    daysOfWeek: ['Dom', 'Seg', 'Ter', 'Qua', 'Qui', 'Sex', 'Sáb'],
                    monthNames: ["Jan", "Fev", "Mar", "Abr", "Mai", "Jun", "Jul", "Ago", "Set", "Out", "Nov", "Dez"]
                }
            }, function (start, end, label) {
                $('#hdf_data_de').val(start.format('DD/MM/YYYY'));
                $('#filtro-data-de').val(start.format('DD/MM/YYYY'));
            });

            $('#filtro-data-ate').daterangepicker({
                autoApply: true,
                singleDatePicker: true,
                autoUpdateInput: false,
                locale: {
                    format: "DD/MM/YYYY",
                    daysOfWeek: ['Dom', 'Seg', 'Ter', 'Qua', 'Qui', 'Sex', 'Sáb'],
                    monthNames: ["Jan", "Fev", "Mar", "Abr", "Mai", "Jun", "Jul", "Ago", "Set", "Out", "Nov", "Dez"]
                }
            }, function (start, end, label) {
                $('#hdf_data_ate').val(start.format('DD/MM/YYYY'));
                $('#filtro-data-ate').val(start.format('DD/MM/YYYY'));
            });

            /*DATA DE - INPUT DIRETO NO CAMPO*/
            $('#filtro-data-de').on('change', function () {
                $('#hdf_data_de').val($(this).val());
            });

            /*DATA ATE - INPUT DIRETO NO CAMPO*/
            $('#filtro-data-ate').on('change', function () {
                $('#hdf_data_ate').val($(this).val());
            });

            $('input[name="filtro_periodo"]').on('change', function () {
                $('#hdf_periodo').val($(this).val());
            });

            $('#btn_filtrar').on('click', function () {
                GoToCalendario(1);
            });

            $('a[rel="avancado"]').on('click', function (e) {
                e.preventDefault();
                $('#evento').slideToggle();
            });

            $('.pag-anterior').on('click', function (e) {
                e.preventDefault();
                GoToUrl($(this).attr('data-value'));
            });

            $('.pag-proxima').on('click', function (e) {
                e.preventDefault();
                GoToUrl($(this).attr('data-value'));
            });

            var callback = function (e) {
                var text = e.type;
                var code = e.which ? e.which : e.keyCode;
                if (13 === code) {
                    $("#btn_filtrar").trigger('click');
                }
            };

            $('#filtro-busca').keypress(callback);
         
        });


        function GetParameters(page) {
            var busca = 'q=' + $('#filtro-busca').val();
            var tag = ($('#hdf_tag').val() != '' ? '&tag=' + $('#hdf_tag').val() : '');
            var tagfiltro = ($('#hdf_tag_filtro').val() != '' ? '&tagfiltro=' + $('#hdf_tag_filtro').val() : '');

            var estado = $("#hdf_estado").val() != '' ? '&local='+ $("#hdf_estado").val() : '';

            //var local = ($('#filtro-localizacao').val() != ''? '&local=' + $('#filtro-localizacao').val() : '');

            //if (estado != '' && local != '') {
            //    local = '';
            //}

            var periodo = ($('#hdf_periodo').val() != '' ? '&periodo=' + $('#hdf_periodo').val() : '');
            var data_de = ($('#hdf_data_de').val() != '' ? '&datade=' + $('#hdf_data_de').val() : '');
            var data_ate = ($('#hdf_data_ate').val() != '' ? '&dataate=' + $('#hdf_data_ate').val() : '');

            return busca + '&page=' + page + periodo + estado + data_de + data_ate + tag + tagfiltro;
        }

        function Search(page) {
            $.get('home.aspx?ajax=true&' + GetParameters(page),
                function (o) {
                    $('#resultado').fadeOut(function () {
                        $('#registroTotal').html(o.registroTotal);
                        $('#tagRelacionada').html(o.tagRelacionada);
                        $('#resultado').html(o.data);
                        $('#resultado').fadeIn();
                    });
                    Pagination(o);
                }
            );
        }

        function Pagination(obj) {
            if (obj.registroTotal > 0) {
                if (obj.page == 1)
                    $('.pag-anterior').hide();
                else
                    $('.pag-anterior').attr('data-value', obj.page - 1).show();

                if (obj.registroTotal > obj.registroFim)
                    $('.pag-proxima').attr('data-value', obj.page + 1).show();
                else
                    $('.pag-proxima').hide();
            }
            else {
                $('.pag-anterior').hide();
                $('.pag-proxima').hide();
            }
        }

        function GoToUrl(page) {
            window.location.href = 'home.aspx?page=' + page;
        };

        function GoToCalendario(page) {
            window.location.href = 'calendario.aspx?' + GetParameters(page);
        };

        function UpdateAdvanced() {
            var ids = $('ul.selected a').map(function (index, obj) {
                return $(obj).attr('data-value');
            });
            $('#hdf_tag_filtro').val(ids.get().join());
        }
    </script>

