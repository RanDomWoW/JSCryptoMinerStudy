<script language="javascript">
    $(document).ready(function () {

        $("#a-login").click(function () {
            $('#modal-auth').modal('show');
        });
        $("#a-cadastro").click(function () {
            $('#modal-cadastro').modal('show');
        });

        $('.reenvio-senha').click(function () {
            //$('#modal-reenvio-retorno').modal('hide');
            $('#modal-reenvio').modal('show');
            $('#modal-auth').modal('hide');

        });

        $("#body-modal-orcamento").html()

        $('.alert .close').on('click', function (e) {
            $(this).parent().hide();
        });

        try {
            var callback = function (e) {
                var text = e.type;
                var code = e.which ? e.which : e.keyCode;
                if (13 === code) {
                    $("#btn-auth").trigger('click');
                }
            };

            $('#senha').keypress(callback);

        } catch (e) {

        }

        $('#btn-newsletter').on('click', function (e) {
            e.preventDefault();
            if ($('.newsletter').is(':hidden')) {
                $('.news-register').show();
                $('.news-done').hide();
            }
            $('.newsletter').slideToggle('slow');
        });

        $('.news-close').on('click', function () {
            $('.newsletter').slideToggle('slow');
        });

        $("#newsletter-form").validate(validate_news_opts);
        $("#newsletter-mobile-form").validate(validate_news_opts);

        $('.news-assinar').on('click', function (e) {
            e.preventDefault();

            var form = $(this).parents('form');
            if ($(form).valid()) {
                $.post('home.aspx', $(form).serialize());
                $('.news-register').hide();
                $('.news-done').show();
                $(form)[0].reset();
            }
        });


        $("#modal-auth").on('shown.bs.modal', function () {
            $("#email").focus();
        });

        $('#modal-orcamento').on('shown.bs.modal', function () {
            $('#modal-orcamento').find('iframe').attr("src", "about-blank");
            var windowWidth = window.screen.width < window.outerWidth ?
                  window.screen.width : window.outerWidth;
            var mobile = windowWidth < 500;

            if (!mobile) {
                $("#modal-orcamento-dialog").animate({
                    width: "900px"
                }, 180);
                $('#modal-orcamento').find('iframe').attr('src', 'arquivos/atend/formCadastrar.aspx');
            } else {
                $("#modal-orcamento-dialog").animate({
                    width: "95%"
                }, 180);
                $('#modal-orcamento').find('iframe').attr('src', 'arquivos/atend/formCadastrar.aspx'); 
            }
        });

        $('#modal-denuncie').on('shown.bs.modal', function () {
            $('#modal-denuncie').find('iframe').attr('src', '/arquivos/atend/formDenuncie.aspx');
        });

        $('#modal-contato').on('shown.bs.modal', function () {
            $('#modal-contato').find('iframe').attr('src', '/arquivos/atend/formContato.aspx');
        });

        $("#btn-auth").click(function () {
            if ($('#email').val().length > 3 && $("#senha").val().length > 4) {
                $("#asuccess").fadeIn('fast');
                $("#asuccess").html('<center>Aguarde...</center>');
                $.ajax({
                    type: "POST",
                    url: '/auth.aspx',
                    data: "email=" + $('#email').val() + "&senha=" + $("#senha").val(),
                    success: function (respText) {
                        if (respText != "0") {
                            //alert(respText);
                            $("#btn-auth").hide();
                            $("#asuccess").html('<center>Aguarde o carregamento da página...</center>');
                            $("#asuccess").fadeIn('fast');
                            $("#adanger").fadeOut('fast');
                            $("#awarning").fadeOut('fast');
                            $('.reenvio-senha').hide();
                            window.location = respText;
                        } else {
                            $("#adanger").fadeIn('fast');
                            $("#asuccess").fadeOut('fast');
                            $("#awarning").fadeOut('fast');
                        }
                    }
                });
            } else {
                $("#awarning").fadeIn('fast');
                $("#adanger").fadeOut('fast'); $("#asuccess").fadeOut('fast');
            }
        });

        $("#btn-reenvio").click(function () {
            if ($('#emailauth').val().length > 3) {
                $("#asuccess2").fadeIn('fast');
                $("#asuccess2").html('<center>Aguarde...</center>');
                $.ajax({
                    type: "POST",
                    url: '/auth.aspx',
                    data: "emailauth=" + $('#emailauth').val(),
                    success: function (respText) {
                        if (respText.indexOf("sucesso") > -1) {
                            $("#btn-reenvio").hide();
                            $("#asuccess2").fadeIn('fast');
                            $("#asuccess2").html('<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>' + respText);
                            $('#asuccess2 .close').on('click', function (e) {
                                $(this).parent().hide();
                            });
                            $("#adanger2").fadeOut('fast');
                            $("#awarning2").fadeOut('fast');
                        } else {
                            $("#adanger2").fadeIn('fast');
                            $("#asuccess2").fadeOut('fast');
                            $("#awarning2").fadeOut('fast');
                            $("#adanger2").html('<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>' + respText);
                            $('#adanger2 .close').on('click', function (e) {
                                $(this).parent().hide();
                            });
                        }
                    }
                });
            } else {
                $("#awarning2").fadeIn('fast');
                $("#adanger2").fadeOut('fast'); $("#asuccess2").fadeOut('fast');
            }
        });
    });

    var validate_news_opts = {
        rules: {
            email: { required: true, email: true }
        },
        messages: {
            email: {
                email: "Digite um e-mail válido",
                required: "O campo E-mail deve ser preenchido"
            }
        }
    };

    $(function(){
        $('.btn-busca').click(function () {
            if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '') && location.hostname == this.hostname) {
                var target = $(this.hash);
                target = target.length ? target : $('[id=' + this.hash.slice(1) + ']');
                if (target.length) {
                    $('html, body').animate({
                        scrollTop: target.offset().top - ($(window).width() >= 970 ? 145 : 0)
                    }, 1000);
                    return false;
                }
            }
        });
    });
