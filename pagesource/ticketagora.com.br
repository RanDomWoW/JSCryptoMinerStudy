</form>


<style type="text/css">
    .dvcodigo {
        display:none;
    }
</style>

<script type="text/javascript">
    $(function () {
        $('#codigo').mask("99999");
        $('#sel_tipdoc_c').change(function () {
            var docSEL = $('#sel_tipdoc_c').val();
            $('#documento_c').unmask();
            $('#documento_c').attr("disabled", false);
            switch (docSEL) {
                case "404": $('#documento_c').val(''); $('#documento_c').attr('data-content', 'Selecione o tipo de documento'); $('#documento_c').attr("disabled", true); break;
                case "0": $('#documento_c').mask("999.999.999-99"); $('#documento_c').attr('data-content', 'Digite seu CPF apenas números'); break;
                case "1": $('#documento_c').mask("999.999.999-99"); $('#documento_c').attr('data-content', 'Digite seu CPF apenas números'); break;
                case "5":
                    $('#documento_c').mask("99.999.999/9999-99");
                    $('#documento_c').attr('data-content', 'Digite o CNPJ apenas números');
                    break;
                case "2": $('#documento_c').attr('data-content', 'Digite seu documento Estrangeiro (RNE)');  break;
                case "3": $('#documento_c').attr('data-content', 'Digite seu documento Estrangeiro (Passaporte)'); break;
            }
            $('#documento_c').focus();
        });

        $.validator.addMethod("senhaTesteC", function (value, element) {
            var mediumRegex = new RegExp("^(?=.{5,})(((?=.*[A-Z])(?=.*[a-z]))|((?=.*[A-Z])(?=.*[0-9]))|((?=.*[a-z])(?=.*[0-9]))).*$", "g");
            var input = value;
            if (mediumRegex.test(input)) {
                return true;
            }
            else {
                return false;
            }
        }, "Digite uma senha mais forte que contenha (letras  e números)");


        $("#form-recuperar-celular").validate({
            rules: {
                documento_c: { required: true },
                codigo: { minlength: 5},
                passwordc: {
                    required: true,
                    minlength: 5,
                    senhaTesteC: true
                },
                passwordConfirmc: { equalTo: "#passwordc" }
               
            },
            highlight: function (element) {
                $(element).parent().removeClass('has-success').addClass('has-error');
            },
            unhighlight: function (element) {
                $(element).parent().removeClass('has-error').addClass('has-success');
            },
            messages: {
                documento_c: {
                    required: "O campo documento deve ser preenchido"
                },
                codigo: { minlength: "Informe o código com 5 digitos." },
                passwordc: {
                    required: "O campo senha deve ser preenchido",
                    minlength: "Informe mais de 5 caracteres que contenham (letras e números)."
                },
                passwordConfirmc: { equalTo: "A senha de confirmação esta incorreta" }
            },
            submitHandler: function (form) {
                $(".dvcodigo").hide();
                $("#msg_recuperar_celular").html('');
                $("#msg_recuperar_celular").hide();
                $('#loadPage').css('display', 'block');
             
                $.post("https://ticketagora.com.br/home.aspx", $.param($("#form-recuperar-celular").serializeArray()) + '&btnRecuperarCelular=codigo', function (data) {
                    $("#msg_recuperar_celular").html(data);
                    $("#msg_recuperar_celular").show();
                    $('#loadPage').css('display', 'none');
                    if (data.indexOf("sucesso") > -1) {
                        $(".dvcodigo").show();
                        $("#form-recuperar-celular").remove("#hf_msg");
                        $("#form-recuperar-celular").append($("#hf_msg"));
                        $("#codigo").focus();
                    }
                });
              
            }
        });


        $("#btnRecuperarCelular").click(function () {
            $("#msg_recuperar_celular").html('');
            $("#msg_recuperar_celular").hide();
            $('#loadPage').css('display', 'block');
            $.post("https://ticketagora.com.br/home.aspx", $.param($("#form-recuperar-celular").serializeArray()) + '&btnRecuperarCelular=enviar', function (data) {
                $("#msg_recuperar_celular").html(data);
                $("#msg_recuperar_celular").show();
                if (data.indexOf('sucesso') > -1) {
                    $(".dvcodigo").hide();
                    window.location = "https://ticketagora.com.br/home.aspx";
                } else {
                    $('#loadPage').css('display', 'none');
                }
             });
        });
      
    });

</script>
                </div>
                
            </div>
        </div>
    </div>
</div>


