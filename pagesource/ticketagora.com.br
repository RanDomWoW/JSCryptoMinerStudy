<script type="text/javascript">
    $(function () {

        try {
        $('#data_r').mask("99/99/9999");
        $('#sel_tipdoc_r').change(function () {
            var docSEL = $('#sel_tipdoc_r').val();
            $('#documento_r').unmask();
            $('#documento_r').attr("disabled", false);
            switch (docSEL) {
                case "404": $('#documento_r').val(''); $('#documento_r').attr('data-content', 'Selecione o tipo de documento'); $('#documento_r').attr("disabled", true); break;
                case "0": $('#documento_r').mask("999.999.999-99"); $('#documento_r').attr('data-content', 'Digite seu CPF apenas números'); break;
                case "1": $('#documento_r').mask("999.999.999-99"); $('#documento_r').attr('data-content', 'Digite seu CPF apenas números'); break;
                case "5":
                    $('#documento_r').mask("99.999.999/9999-99");
                    $('#documento_r').attr('data-content', 'Digite o CNPJ apenas números');
                    break;
                case "2": $('#documento_r').attr('data-content', 'Digite seu documento Estrangeiro (RNE)');  break;
                case "3": $('#documento_r').attr('data-content', 'Digite seu documento Estrangeiro (Passaporte)'); break;
            }
            $('#documento_r').focus();
        });
           
        $.validator.addMethod(
            "aDateR",
            function (value, element) {
                // put your own logic here, this is just a (crappy) example 
                return value.match(/^\d\d?\/\d\d?\/\d\d\d\d$/);
            },
            "Por favor informe uma data no formato dd/mm/yyyy"
        );

            $("#form-recuperar").validate({
                rules: {
                    fname_r: { required: true, minlength: 2 },
                    data_r: {
                        required: true,
                        "aDateR": true
                    },
                    documento_r: { required: true },
               
                    email_r: { required: true, email: true },
                    emailConfirm_r: { equalTo: "#email_r" }
                },
                highlight: function (element) {
                    $(element).parent().removeClass('has-success').addClass('has-error');
                },
                unhighlight: function (element) {
                    $(element).parent().removeClass('has-error').addClass('has-success');
                },
                messages: {
                    fname_r: {
                        required: "Informe seu primeiro nome",
                        minlength: "O campo nome deve ser preenchido"
                    },
                    documento_r: {
                        required: "O campo documento deve ser preenchido"
                    },
                    data_r: {
                        required: "O campo Data de Nascimento deve ser preenchido",
                        "aDate": "Digite uma data válida"
                    },
                    email_r: {
                        email: "Digite um e-mail válido",
                        required: "O campo E-mail deve ser preenchido"
                    },
                    emailConfirm_r: { equalTo: "O e-mail de confirmação está incorreto" }
                },
                submitHandler: function (form) {
                    try {
                        $("#msg_recuperar").hide();
                        
                        $('#loadPage').css('display', 'block');
                       
                        $.post("https://ticketagora.com.br/home.aspx", $.param($("#form-recuperar").serializeArray()), function (data) {
                            $("#msg_recuperar").html(data);
                            $("#msg_recuperar").show();
                            $('#loadPage').css('display', 'none');
                        });
                        
                    } catch (e) { alert(e.message);}
                }
            });
        } catch (e) { alert(e.message);}

      
    });

</script>
                </div>
                
            </div>
        </div>
    </div>
</div>

