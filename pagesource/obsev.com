			{
				var http = new XMLHttpRequest();
				http.open('HEAD', json_url, false);
				http.send();
				if(http.status!= 404){
					return json_url;
				}
				else{
					return ajax_url;
				}
			}
			function loadvideolist(action,post_type,category,page,callback,slider,async,baseRequest){
				var new_post_type = (post_type=='post' ? 'posts' : post_type);
							//var json_url = '/_UX/'+new_post_type+category+page+'.'+baseRequest+'.json';
							var ajax_url = "/wp-content/plugins/obsev_common/ajax.php?action="+action+"&category="+category+"&page="+page+'&post_type='+post_type+"&base_request="+baseRequest;
							//var url = UrlExists(json_url, ajax_url);
				//	var currentRequest = null;
				var settings = {
					"async": async,
					"crossDomain": true,
					"url": ajax_url,
					"method": "GET",
					"processData": false,
					"contentType": false,

					"mimeType": "multipart/form-data"	,

				}

				jQuery.ajax(settings).done(function (response) {
					var obj = JSON.parse(response);
					var div_data = '';

					jQuery.each(obj.articles, function(i,val){
                                    // var classes = val.item_class.join(' ');
                                    var newClass = (val.counter == 0) ? " hover-processed-look " : " ";
                                    // var sectionStyle = (val.post_type == 'bws-gallery') ? " display:none" : "";
                                    

                                    if(val.post_type == 'post'){

                                    	div_data += '<li class="load_ajax_contnt"><article data-uuid="'+val.post_ID+'" class="article menu-grid-item item first '+val.post_type+'">';

                                    	div_data += '<figure><a title="'+val.post_title+'" href="'+val.post_link+'"><img itemprop="image" data-src="'+val.post_image_post+'" alt="'+val.post_title+'" nopin="nopin" /></a><figcaption>';
                                    	div_data += '<div class="caption-wrapper"><h2><a title="'+val.post_title+'" href="'+val.post_link+'">'+val.post_title+'</a></h2><div class="summary hidden"></div>';

                                    	div_data += '</div></figcaption></figure></article></li>';

                                    }
                                    if(val.post_type == 'bws-gallery'){
                                    	div_data += '<li class="load_ajax_contnt"><article data-uuid="'+val.post_ID+'" class="gallery menu-grid-item item '+newClass+' '+val.post_type+'">';

                                    	div_data += '<figure><a title="'+val.post_title+'" href="'+val.post_link+'"><img itemprop="image" data-src="'+val.post_image_bws+'" alt="'+val.post_title+'" nopin="nopin" /></a><figcaption>';
                                    	div_data += '<div class="caption-wrapper"><h2><a title="'+val.post_title+'" href="'+val.post_link+'">'+val.post_title+'</a></h2><div class="summary hidden"></div>';
                                    	div_data += '</div></figcaption></figure></article></li>';

                                    }
                                    if(val.post_type == 'video'){
                                    	var source = "/wp-content/themes/obsev_desktop";

                                    	div_data += '<li class="load_ajax_contnt"><article data-uuid="'+val.post_ID+'" class="video item video-ajax-item hover-processed"><div class="content-type video"><img src="'+source+'/images/play-btn.png" alt="" nopin="nopin"></div>';
                                    	div_data += '<figure><a class="video-list-ajax" data-video-id="'+val.postMeta+'" data-delta="0" title="'+val.post_title+'" href="'+val.post_link+'"><img typeof="foaf:Image" data-src="'+val.thumbnailUrls+'" alt="'+val.post_title+'" title="'+val.post_title+'" nopin="nopin"></a><figcaption><div class="caption-wrapper">';

                                    	div_data += '<h2><a class="video-list-ajax" data-video-id="'+val.postMeta+'" data-delta="0" title="'+val.post_title+'" href="'+val.post_link+'">'+val.post_title+'</a></h2>';

                                    	div_data += '</div></figcaption></figure></article></li>';

                                    }
                                    
                                    
                                }); 

					if(callback !== undefined){
						callback(div_data);
					}else{
						var length = jQuery(div_data).filter(".load_ajax_contnt").length;

						if(div_data!=''){
							slider.pageCount = slider.pageCount+1;
							var _count = slider.count;
							slider.addSlide( div_data );
							slider.count = _count + length;
							slider.last = slider.count - 1;

						}
					}							
				});


			}

