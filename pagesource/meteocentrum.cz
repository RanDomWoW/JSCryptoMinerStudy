        var prompt = $('div.search-box input.text');

        prompt.focus(function () {
            if ($(this).val() == "Název města") {
                $(this).val('');
            }
        });

        prompt.blur(function () {
            if ($(this).val() == '') {
                $(this).val("Název města");
            }
        });
    });
</script>

