        function matchingResults(inputArray, searchingTerm) {
            var newResults = [];
            for (var key in inputArray) {
                var city = inputArray[key];
                var cityNameLowerCase = city.name.toLowerCase();
                if (cityNameLowerCase.indexOf(searchingTerm) >= 0) {
                    newResults.push(inputArray[key]);
                    continue;
                }

                if(city.alternateNames){
                    var splitArray = city.alternateNames.split(",")
                    for (var splitKey in splitArray){
                        if (splitArray[splitKey].toLowerCase().indexOf(searchingTerm) == 0) {
                            inputArray[key].name = splitArray[splitKey].concat(' - ',inputArray[key].name);
                            newResults.push(inputArray[key]);
                            break;
                        }
                    }
                }
            }
            return newResults;
        }

        function addFilteredResults(resultsArray, inputArray, maxCountToInsert, country) {
            var count = 0;
            for (var key in inputArray) {
                var singleCity = inputArray[key];
                resultsArray.push({"label": singleCity.name + " (" + singleCity.region + ")", "city": singleCity.name, "id": singleCity.id, "country": country});

                count++;
                if (count >= maxCountToInsert) {
                    break;
                }
            }
        }

