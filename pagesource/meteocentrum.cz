    <script type="text/javascript">
        $(document).ready(function () {
            var matchedCZCities = [];
            var matchedEUCities = [];
            function matchingResults(inputArray, searchingTerm) {
                var newResults = [];
                for (var key in inputArray) {
                    var city = inputArray[key];

                    if (city.alternateNames) {
                        var splitArray = city.alternateNames.split(",")
                        for (var splitKey in splitArray) {
                            inputArray[key].name = inputArray[key].name.concat(' - ', splitArray[splitKey]);
                            newResults.push(inputArray[key]);
                            break;
                        }
                    }
                    else{
                        newResults.push(inputArray[key]);
                    }
                }
                return newResults;
            }

            function addFilteredResultsForAddFavouriteCity(resultsArray, inputArray, maxCountToInsert) {
                var count = 0;
                for (var key in inputArray) {
                    var singleCity = inputArray[key];
                    resultsArray.push({"label": singleCity.name + " (" + singleCity.region + ")", "city": singleCity.name, "id": singleCity.id, "country": singleCity.country});

                    count++;
                    if (count >= maxCountToInsert) {
                        break;
                    }
                }
            }

            $('input.add-favourite-city-search').autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: "\/search\/json",
                        dataType: "json",
                        cache: true,
                        data: {
                            czCitySearch: false,
                            term: request.term//,
                            //category: searchCategory
                        },
                        success: function (data) {
                            var filteredResponse = [];
                            if (typeof (data) === 'object') {
                                var MAX_ALL = 10;

                                var matchedCities = matchingResults(data['cities']);

                                addFilteredResultsForAddFavouriteCity(filteredResponse, matchedCities, MAX_ALL);
                            }
                            if(filteredResponse.length === 1){
                                $("#addFavouriteCitySearchPlace").val(filteredResponse[0].city);
                                $("#cityId").val(filteredResponse[0].id);
                                $("#cityCountry").val(filteredResponse[0].country);
                            }else{
                                //DELETE values
                                $("#cityId").val("");
                                $("#cityCountry").val("");
                            }
                            response(filteredResponse);
                        }
                    })
                },
                minLength: 2,
                focus: function (event, ui) {
                    event.preventDefault();
                    $(this).val(ui.item.city);
                },
                open: function (event, ui) {
                    $(this).autocomplete('widget').show();
                    return false;
                },
                select: function (event, ui) {
                    event.preventDefault();
                    $("#cityId").val(ui.item.id);
                    $("#cityCountry").val(ui.item.country);
                    $(this).val(ui.item.city);
                }
            });

            $('#submit-add-favourite-city-search').click(function(){
                window.setTimeout(showCitiesIfError,10);
            });

            function afterClick(singleCity){
                $.ajax({
                    type: "POST",
                    url: "\/?do=setupFavouriteForecast",
                    data: {
                        id: singleCity.id,
                        name: singleCity.name,
                        country: singleCity.cityCountry,
                        removeIfExist: false
                    }
                });
            }

            function showCitiesIfError(){
                var message = $("#cityId_message");
                if(message != null && message.html() != null){
                    $("#city-items").empty();
                    for (var key in matchedCZCities) {
                        var singleCity = matchedCZCities[key];
                        addElement(singleCity, "cz");
                    }

                    $("#city-items").append("<hr>");

                    for (var key in matchedEUCities) {
                        var singleCity = matchedEUCities[key];
                        addElement(singleCity, "eu");
                    }
                }
            }

            function addElement(singleCity, country){
                var object = {'city': singleCity.name, 'id': singleCity.id, 'cityCountry' : country};
                var element = $('<div class="spacer20 single-item"><a class="highlight" style="font-size: initial">' + singleCity.name + " (" + singleCity.region + ")" + '</a></div>');
                $("#city-items").append(element);
                element.click(function(){ afterClick(object)});
            }
        });
    </script>

