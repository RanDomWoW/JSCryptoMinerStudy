            }

            function error(error) {
                console.log(error);
                savePosition({ coords: {}});
            }

            function savePosition(position) {
                $.ajax({
                    type: "POST",
                    url: "\/?do=setupGeolocation",
                    data: { longitude: position.coords.longitude, latitude: position.coords.latitude },
                    dataType: "json"
                });      
            }
        });

    </script>
