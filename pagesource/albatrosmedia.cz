    <section class="bg--white">
        <div class="container-fluid">
            <div class="row pb-6">
                <div class="col-md-7 col-xl-6 offset-xl-1">
                    <h1 class="red">Aktuality</h1>
                </div>
                        <div class="col-md-7 col-xl-6 offset-xl-1 border border-md--right o-hidden">
                            <article>
