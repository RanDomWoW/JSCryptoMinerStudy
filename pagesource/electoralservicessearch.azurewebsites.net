    $(function () {
        $("[id$=txt_Locality]").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: "Searchbyname.aspx/Getlocation",
                    data: "{ 'prefix': '" + request.term + "'}",
                    dataType: "json",
                    type: "POST",
                    contentType: "applicationjson; charset=utf-8",
                    success: function (data) {
                        response($.map(data.d, function (item) {
                            return {
                                label: item,
                                val: item
                            }
                        }))
                    },
                });
            },
            select: function (e, i) {
                $("[id$=hfvillage]").val(i.item.val);
            },
            minLength: 1
        });
    });



