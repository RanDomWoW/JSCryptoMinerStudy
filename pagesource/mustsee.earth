			<h4>Mustsee is only available on your desktop with Google Chrome.</h4>
			<span>Send a reminder email with a download link:</span>
			<div class="input">

				<form class="mc-form-example" method="POST" action="mailchimp.php">
					<input type="email" id="mc-email" name="mc-email" placeholder="Your Email" required/>
					<input type="text" value="pending" id="status" name="status" hidden/>
					<input type="submit" value="">
					<span id="alertMessage"></span>
				</form>

				<script type='text/javascript'>
					$('document').ready(function(){
					  $('.mc-form-example').submit(function(e){
					        
					    //prevent the form from submitting via the browser redirect
					    e.preventDefault();
					        
					    //grab attributes and values out of the form
					    var data = {email: $('#mc-email').val()};
					    var endpoint = $(this).attr('action');
					        
					    //make the ajax request
					    $.ajax({
					      method: 'POST',
					      dataType: "json",
					      url: endpoint,
					      data: data
					    }).success(function(data){
					      if(data.id){
					        //successful adds will have an id attribute on the object
					        document.getElementById("alertMessage").innerHTML = "Thanks, you should receive an email in few minutes.";
					      } else if (data.title == 'Member Exists') {
					        //MC wil send back an error object with "Member Exists" as the title
					        document.getElementById("alertMessage").innerHTML = "Thanks, but this email is already used.";
					      } else {
					        //something went wrong with the API call
					        document.getElementById("alertMessage").innerHTML = "Oh no, there has been a problem.";
					      }
					    }).error(function(){
					      //the AJAX function returned a non-200, probably a server problem
					        document.getElementById("alertMessage").innerHTML = "Oh no, there has been a problem.";
					    });
					  });
					});
				</script>

			</div>
		</div>
