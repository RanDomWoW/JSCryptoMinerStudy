	<script>
		(function() {
  var $preview = d3.select('.preview');
  var $video = $preview.select('video');
  var $collection = d3.select('.collection');
  var $loader = $collection.select('.loader');
  var $authors = d3.select('.sidebar .authors select');
  var VIDEO_WIDTH = 300;
  var VIDEO_HEIGHT = 150;
  var PAD = 10;
  // const DEFAULT_COLLECTION = 'media_diversity';

  // device sniffing for mobile

  var isMobile = {
    android: function () { return navigator.userAgent.match(/Android/i); },

    blackberry: function () { return navigator.userAgent.match(/BlackBerry/i); },

    ios: function () { return navigator.userAgent.match(/iPhone|iPad|iPod/i); },

    opera: function () { return navigator.userAgent.match(/Opera Mini/i); },

    windows: function () { return navigator.userAgent.match(/IEMobile/i); },

    any: function () { return isMobile.android() ||
      isMobile.blackberry() ||
      isMobile.ios() ||
      isMobile.opera() ||
      isMobile.windows(); }
  };

  function loadImage(url, cb) {
    var img = new Image();
    img.onload = function () { return cb(null, img); };
    img.onerror = function () { return cb(("error loading image: " + url)); };
    img.src = url;
  }

  function loadVideo(url, cb) {
    var video = document.createElement('video');
    video.oncanplay = function () { return cb(null, video); };
    video.onerror = function () { return cb(("error loading video: " + url)); };
    video.setAttribute('preload', true);
    video.src = url;
  }

  function slugify(str) {
    return str
      .trim()
      .toLowerCase()
      .replace(/\s/g, '_')
      .replace(/\W/g, '')
      .replace(/\_/g, '-');
  }

  function getUrlParameter(key) {
    var name = key.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
    var regex = new RegExp(("[\\?&]" + name + "=([^&#]*)"));
    var results = regex.exec(window.location.search);
    return results === null
      ? ''
      : decodeURIComponent(results[1].replace(/\+/g, ' '));
  }

  function getAuthor(story) {
    return story.author
      .map(function (d) {
        var ref = window.authorData.find(function (a) { return a.id === d; });
        var name = ref.name;
        var slug = slugify(name);
        // return `<a href='author/${slug}'>${name}</a>`;
        return ("<a>" + name + "</a>");
      })
      .join(', ');
  }

  function generateFeatureHTML(ref) {
    var collection = ref.collection;
    var story = ref.story;

    var img = "common/assets/collections/" + (collection.id) + ".jpg";
    var author = getAuthor(story);
    return ("\n\t\t\t<div class='collection__feature'>\n\t\t\t\t<div class='feature__tease'>\n\t\t\t\t\t<div class='tease__bg' style='background-image: url(" + img + ")'></div>\n\t\t\t\t\t<div class='tease__title'>\n\t\t\t\t\t\t<p class='title__overline show-desktop tk-atlas xxx-small'>Essays about</p>\n\t\t\t\t\t\t<p class='title__overline show-mobile tk-atlas xxx-small'>Featured collection</p>\n\t\t\t\t\t\t<h3 class='title__name tk-canela'>" + (collection.name) + "</h3>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div class='feature__story'>\n\t\t\t\t\t<a class='story__hed tk-canela large' data-preview='" + (story.img) + "' href='" + (story.url) + "'>" + (story.hed) + " <span>" + (story.dek) + "</span></a>\n\t\t\t\t\t<p class='story__author tk-atlas xx-small'>By " + author + "</p>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t");
  }

  function generateInfoHTML(ref) {
    var collection = ref.collection;
    var count = ref.count;
    var stories = ref.stories;

    var list = stories
      .map(
        function (story) { return ("\n\t\t\t<li class='list__item'>\n        <a class='list__link' data-preview='" + (story.img) + "' href='" + (story.url) + "'>\n          <time class='list__date tk-atlas xxx-small' datetime='" + (story.date) + "'>\n            " + (story.time) + "\n          </time>\n          <h3 class='list__hed tk-canela medium'>" + (story.hed) + "</h3>\n          <p class='list__dek tk-atlas xx-small'>" + (story.dek) + "</p>\n        </a>\n      </li>\n\t\t"); }
      )
      .join('');

    var isMore = count > 4 ? 'is-visible' : '';
    var more = "<a href='#collection-" + (collection.id) + "' class='all__view'>View all <span>" + count + "</span> essays</a>";
    var other = "<a href='#collections' class='all__view'>Check out other <span>collections</span></a>";

    return ("\n\t\t<div class='collection__info'>\n\t\t\t<p class='info__description x-small'>" + (collection.description) + "</p>\n\t\t\t<ul class='info__list'>" + list + "</ul>\n\t\t\t<p class='info__all xx-small tk-atlas is-visible'>" + (isMore ? more : other) + "</p>\n\t\t</div>\n\n\t\t");
  }

  function generateHTML(ref) {
    var collection = ref.collection;
    var stories = ref.stories;

    var featureHTML = generateFeatureHTML({ collection: collection, story: stories[0] });
    var infoHTML = generateInfoHTML({
      collection: collection,
      count: stories.length,
      stories: stories.slice(1, 4)
    });
    $collection.html(("" + featureHTML + infoHTML));
  }

  function preloadImages(stories) {
    var base = 'common/assets/preview-thumbnails';
    var promises = stories.slice(0, 4).map(function (d) {
      return new Promise(function (resolve, reject) {
        var url = base + "/" + (d.img) + ".jpg";
        loadImage(url, function (err) {
          if (err) { reject(err); }
          else { resolve(); }
        });
      });
    });
    return Promise.all(promises);
  }

  function preloadVideos(stories, cb) {
    var base = 'common/assets/previews';
    var promises = stories.slice(0, 4).map(function (d) {
      return new Promise(function (resolve, reject) {
        var url = base + "/" + (d.img) + ".mp4";
        loadVideo(url, function (err) {
          if (err) { reject(err); }
          else { resolve(); }
        });
      });
    });

    return Promise.all(promises);
  }

  function getCollection() {
    var param = getUrlParameter('collection');
    var collection = null;
    if (param) {
      // first try url paramter
      var c = window.collectionData.find(function (d) { return d.id === param; });
      if (c) { return c; }
    } else if (document.referrer.includes('pudding.cool')) {
      // then try document.referrer
      var split = document.referrer.split('pudding.cool/')[1];
      var s = window.storyData.find(function (d) { return split.includes(d.url); });
      if (s && s.collection) {
        var c$1 = window.collectionData.find(function (d) { return d.id === s.collection; });
        if (c$1) { return c$1; }
      }
    }
    // use default
    var ran = Math.floor(Math.random() * window.collectionData.length);
    return window.collectionData[ran];
  }

  function getStories(id) {
    var stories = window.storyData.filter(function (d) { return d.collection === id; });
    // if first story is same as referrer, move to bottom of arr
    // const { referrer } = document
    var referrer = 'pudding.cool/2017/07/comics';
    if (referrer.includes('pudding.cool')) {
      var split = referrer.split('pudding.cool/')[1];
      var index = stories.findIndex(function (d) { return split.includes(d.url); });
      if (index > -1) {
        var from = stories.splice(index, 1)[0];
        stories.push(from);
      }
    }
    return stories;
  }

  function enablePreview() {
    $preview.classed('is-enabled', true);
  }

  function setupCollection() {
    // get collection and story data
    var collection = getCollection();
    var stories = getStories(collection.id);

    // generate html
    generateHTML({ collection: collection, stories: stories });

    // preload images and videos
    var sliced = stories.slice(0, 4);

    preloadImages(sliced)
      .then(function () { return console.log('images loaded'); })
      .catch(console.error);
    preloadVideos(sliced)
      .then(function () {
        console.log('videos loaded');
        enablePreview();
      })
      .catch(console.error);

    $loader.remove();
  }

  function handlePreviewEnter() {
    var $sel = d3.select(this);
    var val = $sel.attr('data-preview');
    var videoSrc = "common/assets/previews/" + val + ".mp4";
    var thumbSrc = "common/assets/preview-thumbnails/" + val + ".jpg";
    $video.attr('src', videoSrc);
    $video.attr('poster', thumbSrc);
    $preview.classed('is-visible', true);
  }

  function handlePreviewMove() {
    var $sel = d3.select(this);
    var ref = d3.mouse(this);
    var mX = ref[0];
    var mY = ref[1];
    var bb = this.getBoundingClientRect();

    var x = bb.left + mX;
    var y = bb.top + mY - VIDEO_HEIGHT;

    var dirX = x + VIDEO_WIDTH > window.innerWidth ? -1 : 0;
    var dirY = y < 0 ? 1 : 0;

    var padX = dirX ? -PAD : PAD;
    var padY = dirY ? PAD : -PAD;
    var addX = dirX * VIDEO_WIDTH + padX;
    var addY = dirY * VIDEO_HEIGHT + padY;

    var pX = x + addX;
    var pY = y + addY;

    $preview.style('left', (pX + "px")).style('top', (pY + "px"));
  }

  function handlePreviewExit() {
    $preview.classed('is-visible', false);
  }

  function setupPreview() {
    d3
      .selectAll('[data-preview]')
      .on('mouseenter', handlePreviewEnter)
      .on('mousemove', handlePreviewMove)
      .on('mouseleave', handlePreviewExit);
  }

  function setupToolbar() {
    var controller = new ScrollMagic.Controller();

    var scene = new ScrollMagic.Scene({
      triggerElement: '.sidebar',
      triggerHook: 0,
      offset: 0
    })
      .setPin('.sidebar')
      .addTo(controller);

    function scrollTween(item) {
      var offset = -40;
      var distanceScrolled =
        window.pageYOffset || document.documentElement.scrollTop;
      var distanceDiv = d3
        .select(item)
        .node()
        .getBoundingClientRect().top;
      var scrollAdjust = 0;
      return function() {
        var i = d3.interpolateNumber(
          window.pageYOffset || document.documentElement.scrollTop,
          distanceScrolled + distanceDiv + offset - scrollAdjust
        );
        return function(t) {
          scrollTo(0, i(t));
        };
      };
    }

    d3.selectAll('.toolbar__item').on('click', function(d) {
      var id = d3
        .select(this)
        .text()
        .toLowerCase()
        .replace(/\s/g, '-');

      d3
        .transition()
        .delay(200)
        .duration(500)
        .tween('scroll', scrollTween('#' + id));
    });
  }

  function handleAuthorInput() {
    window.location.href = "author/" + (this.value);
  }

  function setupAuthors() {
    $authors.on('input', handleAuthorInput);
  }

  function init() {
    setupCollection();
    if (!isMobile.any()) { setupPreview(); }
    setupToolbar();
    setupAuthors();
  }

  init();
})();

	</script>

