<script>
	function respositingFooterAndContentContainer() {
		vh=$(window).height();
		nh=$('.navbar').height() + $('.naviwrapper').height() + $('.footerhead').height() +90;
		//console.log("windowHeight: "+vh);
		//console.log("naviHeight: "+nh);		
		if($('#content').height() < vh-nh && $(window).width()>768) {
			tmpContentHeight = $('#content').height();
			tmpHeightToUse = vh-nh-tmpContentHeight;
			tmpPadding = tmpHeightToUse / 2;
			//console.log("initial - content height: " + tmpContentHeight);
			//console.log("tmpHeightToUse: " + tmpHeightToUse);
			//console.log("tmpPadding: "+tmpPadding);
			
			$('#content').css({'paddingTop':tmpPadding+'px', paddingBottom:tmpPadding+'px'});
			//console.log("after CSS adjustment - content height: " + tmpContentHeight);
			
		}
	}
		
	$(window).load(function() {
		respositingFooterAndContentContainer();
	});

	$(window).on("resize", function() {
		respositingFooterAndContentContainer();
	});
	
</script>
