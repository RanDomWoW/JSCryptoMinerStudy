	<script>
		$("li").eq(0).find("span").addClass("line").parent().siblings().find("span").removeClass("line");
			$("li").eq(0).find("a").addClass("a_current").parent().siblings().find("a").removeClass("a_current");
		$(".nav_con").on("mouseleave", function() {
			$("li").eq(0).find("span").addClass("line").parent().siblings().find("span").removeClass("line");
			$("li").eq(0).find("a").addClass("a_current").parent().siblings().find("a").removeClass("a_current");
		});
		$("#erweima").on("mouseover", function() {
			$("#show").stop().fadeIn();
		});
		$("#erweima").on("mouseout", function() {
			$("#show").stop().fadeOut();
		});
		$(window).scroll(function() {
			var a = $(window).scrollTop();
			console.log(a);
			if(a >= 200) {
				$('.item1 .text div').css({
					'transform': "translate(0)",
				});
			}
			if(a >= 750) {
				$('.item2 .text div').css({
					'transform': "translate(0)",
				});
			}
			if(a >= 1350) {
				$('.item3 .text div').css({
					'transform': "translate(0)",
				});
			}
			if(a >= 1900) {
				$('.item4 .text div').css({
					'transform': "translate(0)",
				});
			}
		});
