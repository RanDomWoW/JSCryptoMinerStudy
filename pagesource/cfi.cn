 <script>
//自动翻页类
//tableid:数据table的id
//pageid:页码div的id
//pagerows：每页显示条数
//rollInterval:滚动间隔毫秒，0代表不自动滚动，大于0代表自动滚动的时间间隔
function PageAction(tableid, pageid, pagerows,rollInterval) {
    //是否自动滚动
    this.AutoRoll = true;
    if (rollInterval == 0) {
        this.AutoRoll = false;
    }

    var delay = rollInterval;//翻页间隔毫秒
 
    var stepdelay = 40;
    var startopacity = 0.05; //透明度
    var stepopacity = 0.05;//透明度递增

    var ie4 = document.all && !document.getElementById;
    var DOM2 = document.getElementById;
  
    var index = 1;
    var pageSum = -1; //总页数
    var pageTimer = null;

    var that = this;

    function changecontent() {
        if (DOM2) {
            document.getElementById(tableid).style.opacity = startopacity;
            //翻到下一页
            that.turnpage(index);
            colorfade(startopacity + stepopacity)
        }
        index++;
        if (index > pageSum) { index = 1 }
    } 
    var fadecounter;
    function colorfade(A) {
        if (A < 1) {
            document.getElementById(tableid).style.opacity = A;

            A = A + stepopacity;
            fadecounter = setTimeout(function(){colorfade(A);}, stepdelay)
        } else {
            clearTimeout(fadecounter);
            document.getElementById(tableid).style.opacity = 1;           
            if (pageTimer != null) {
                clearTimeout(pageTimer);              
                pageTimer = setTimeout(function () { changecontent(); }, delay);
            }
        }
    }

    if (that.AutoRoll) {
        if (window.addEventListener) { window.addEventListener('load', changecontent, false) } else { if (window.attachEvent) { window.attachEvent('onload', changecontent) } else { if (document.getElementById) { window.onload = changecontent } } };
    } 
    //tableid:数据table的id
    //pageid:页码div的id
    //pagerows：每页显示条数
    //pagen：显示第几页
    this.turnpage = function (pagen) {

        //找到数据table
        var pageTab = document.getElementById(tableid);
        //获取table的所有行
        var trList = pageTab.getElementsByTagName("tr");

        //判断是否是当前页，如果是当前页，就不再重复翻页     
        var startShowIndex = -1;
        var aList = document.getElementById(pageid).getElementsByTagName("a");
        for (var i = 0; i < trList.length; i++) {
            if (trList[i].style.display == "block") {
                startShowIndex = i;
                break;
            }
        }
        if (startShowIndex > -1 && (startShowIndex + pagerows) / pagerows == pagen) {
            return;
        }

        pageSum = trList.length / pagerows;
        if (trList.length % pagerows != 0) {
            pageSum = pageSum + 1;
        }
        if (pagen < 1) {
            pagen = 1;
        }
        if (pagen > pageSum) {
            pagen = pageSum;
        }

        var year = new Date().getFullYear().toString();
        var month = (new Date().getMonth() + 1).toString();
        var day = new Date().getDate().toString();
        var today = year + (month.length == 1 ? "0" + month : month) + (day.length == 1 ? "0" + day : day);

        //循环行，控制显示和隐藏
        for (var i = 0; i < trList.length; i++) {
            if (i >= (pagen - 1) * pagerows && i < pagen * pagerows) {
                trList[i].style.display = "block";
            }
            else {
                trList[i].style.display = "none";
            }
            if (pagen > 1) {

                //检测td里的a标签
                var al = trList[i].getElementsByTagName("a");
               
                if (al.length > 0) {                 
                    var ahref = al[0].href;
                    var patt = new RegExp(".*?" + today + ".*?");
                    if (!patt.test(ahref)) {
                        al[0].style.color = "Gray";
                    }
                }
            }
        }
        //修改页码的超链接样式
        for (var i = 0; i < aList.length; i++) {
            //判断是否是当前页码
            if (i + 1 == pagen.toString()) {
                //设置当前页码超链接背景为灰色
                aList[i].style.backgroundColor = "#CC9999";
            }
            else {
                //设置其他页码超链接背景为白色
                aList[i].style.backgroundColor = "White";
            }
            if (aList[i].target != "_blank") {
                aList[i].target = "_self";
            }
        }
    }
    
    function mouseFunc(index) {
        this.mouseOverFunc = function () {
            if (index >= 0) {
                that.turnpage(index + 1);
            }
            clearTimeout(pageTimer);
            pageTimer = null;
        }
        this.mouseOutFunc = function () {
            pageTimer = setTimeout(function () { changecontent(); }, delay);
        }
        this.clickFunc = function () {
            that.turnpage(index+1);
        }
    }


    //初始化翻页事件
    this.initialFunc = function () {
        if (that.AutoRoll) {
            //找到数据table,设置table鼠标移上移出事件
            var pageTab = document.getElementById(tableid);
            pageTab.onmouseover = function () {
                clearTimeout(pageTimer);
                pageTimer = null;
            };
            pageTab.onmouseout = function () { pageTimer = setTimeout(function () { changecontent(); }, delay); };
        }

        //初始化翻页a的鼠标移上移出事件
        var pagelinks = document.getElementById(pageid).getElementsByTagName("a");
        for (var i = 0; i < pagelinks.length; i++) {
            if (pagelinks[i].target != "_blank") {
                var col = new mouseFunc(i);
                if (that.AutoRoll) {
                    pagelinks[i].onmouseover = col.mouseOverFunc;
                    pagelinks[i].onmouseout = col.mouseOutFunc;
                }
                else {
                    pagelinks[i].onclick = col.clickFunc;
                }
            }
        }
        //初始化显示第一页
        that.turnpage(1);
        if (that.AutoRoll) {
            pageTimer = setTimeout(function () { changecontent(); }, delay);
        }
    }
}


 //设置自动翻页
    
            var rp_pagejs = new PageAction('rp_table', 'rp_page', 7, 0);
            rp_pagejs.initialFunc();
            var mj_pagejs = new PageAction('mj_table', 'mj_page', 7, 0);
            mj_pagejs.initialFunc();
            var yw_pagejs = new PageAction('yw_table', 'yw_page', 7, 0);
