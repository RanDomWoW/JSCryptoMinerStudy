/* お問い合せフォーム */
#form-box{
	display: none;
	background: url(http://seabirdscafe.com/wp/wp-content/uploads/2017/02/point-bird.png) no-repeat center 77px;
	width: 700px;
	margin: 0 auto;
	padding: 170px 150px 80px;}

#form-box .screen-reader-response{
	display: none;}
#form-box .invalid .wpcf7-not-valid-tip{
	display: block;
	color: #fff;
	font-size: 14px;
	font-weight: bold;
	background: #f00;
	border-radius: 10px;
	padding: 5px 10px;}
#form-box .invalid .wpcf7-validation-errors,
#form-box .sent .wpcf7-mail-sent-ok{
	display: block;
	color: #f00;
	font-size: 14px;
	font-weight: bold;
	text-align: center;
	background: #fff;
	border: 1px solid #f00;
	border-radius: 10px;
	margin: 20px 0;
	padding: 5px 10px;}
#form-box .sent #cf7-box{
	display: none;}

#cf7-box input[type=text],
#cf7-box input[type=tel]{
	font-size: 18px;
	width: 100%;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	margin: 15px 0 5px;
	padding: 5px 10px;}

#cf7-box #cf7-use{
	position: relative;
	margin: 15px 0 30px;}
