#cf7-box #cf7-use-other input{
	width: 600px;}

#cf7-box #cf7-submit{
	text-align: center;}
#cf7-box #cf7-submit input{
	color: #fff;
	font-size: 20px;
	font-weight: bold;
	text-align: center;
