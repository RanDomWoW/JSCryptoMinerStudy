#aist .text-box{
	clear: both;
	color: #fff;
	text-align: center;}
#aist #aist-text-01{
	font-size: 20px;
	font-weight: bold;
	margin-bottom: 20px;}

/* 入り口 */
#ent{
	background: url(http://seabirdscafe.com/wp/wp-content/uploads/2017/04/home_ent_bg3.jpg) no-repeat center center;
	height: 620px;
	padding: 0 50px;}
#ent .inner{
	max-width: 900px;
	margin: 0 auto;}
#ent #lunch{
	float: left;
	text-align: center;
    width: 50%;
	padding: 165px 0 0;}
#ent #lunch .text-box{ 
	text-shadow: 0 0 6px #000;
    color: #fff;}
#ent #lunch .ent-btn{
	line-height: 0;
	text-indent: -9999px;}
#ent #lunch .ent-btn a{
	display: block;
	background: url(http://seabirdscafe.com/wp/wp-content/uploads/2017/04/lunchtime.png) no-repeat;
	width: 310px;
	height: 50px;
	margin: 0 auto;
	-webkit-transition: 0.5s;
	-moz-transition: 0.5s;
	-o-transition: 0.5s;
	-ms-transition: 0.5s;
	transition: 0.5s;}
#ent #lunch .ent-btn a:hover{
	background: url(http://seabirdscafe.com/wp/wp-content/uploads/2017/04/lunchtime_over.png) no-repeat;}
#ent #dinner{
	float: right;
	text-align: center;
	width: 50%;
	padding: 165px 0 0;}
#ent #dinner .text-box{
    text-shadow: 0 0 6px #000;
	color: #fff;}
#ent #dinner .ent-btn{
	line-height: 0;
	text-indent: -9999px;}
#ent #dinner .ent-btn a{
	display: block;
	background: url(http://seabirdscafe.com/wp/wp-content/uploads/2017/02/dinnertime.png) no-repeat;
	width: 310px;
	height: 50px;
	margin: 0 auto;
	-webkit-transition: 0.5s;
	-moz-transition: 0.5s;
	-o-transition: 0.5s;
	-ms-transition: 0.5s;
	transition: 0.5s;}
#ent #dinner .ent-btn a:hover{
	background: url(http://seabirdscafe.com/wp/wp-content/uploads/2017/02/dinnertime_over.png) no-repeat;}
#ent section h3{
	line-height: 0;
	margin-bottom: 10px;}
#ent section .text-box{
	font-size: 14px;
	font-weight: bold;
	text-align: left;
	min-height: 150px;
	padding: 0 30px;}

/* 予約 */

#yyk{
	padding: 170px 50px 170px;}
