#aist .inner{
	overflow: hidden;
	max-width: 900px;
	margin: 0 auto;}
#aist h2{
    width: 50%;
	float: left;
	line-height: 0;
	margin-top: 53px;}
#aist .img-box{
    width: 50%;
	float: right;
	line-height: 0;
