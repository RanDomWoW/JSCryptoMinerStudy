#yyk #yyk-btn{
	line-height: 0;}
#yyk #yyk-btn a{
	-webkit-transition: 0.5s;
	-moz-transition: 0.5s;
	-o-transition: 0.5s;
	-ms-transition: 0.5s;
	transition: 0.5s;}
#yyk #yyk-btn a:hover{
	opacity: 0.4;}

