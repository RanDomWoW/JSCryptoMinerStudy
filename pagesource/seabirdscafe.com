/* シングル */
.single article{
	overflow: hidden;
    max-width: 700px;
	width: 100%;
	margin: 0 auto 80px;
	padding: 0 100px;}
.single article h2{
	color: #19289b;
	font-size: 24px;
	border-bottom: 2px solid #000;
	padding-bottom: 5px;}
.single article .post-date{
	color: #666;
	font-size: 14px;
	text-align: right;
	margin-bottom: 20px;}
.single article #sc_event_sub_ttl{
	font-size: 16px;
	font-weight: bold;
	line-height: 2;}
.single article .sc_event_details_inner{
	line-height: 2;
	background: #f6f6f6;
	border: 1px dashed #ccc;
	padding: 10px;}
.single article #return-home{
	text-align: center;
	margin-top: 80px;}
.single article #return-home a{
	color: #19289b;}

/* フッター */
footer{
	background: #19289b;
}
footer .inner{
	overflow: hidden;
	margin: 0 auto;
	padding: 0px;}

footer #foot-left{
	float: left;
	text-align: center;
	width: 285px;}
footer #foot-left #foot-logo{
	line-height: 0;
	margin-bottom: 20px;}
footer #foot-left #foot-logo a{
	-webkit-transition: 0.5s;
	-moz-transition: 0.5s;
	-o-transition: 0.5s;
	-ms-transition: 0.5s;
	transition: 0.5s;}
footer #foot-left #foot-logo a:hover{
	opacity: 0.6;}
footer #foot-left #foot-tel{
	line-height: 0;}

footer #foot-right{
	float: right;
	margin-top: 38px;}
footer #foot-right #foot-info{
	color: #fff;
	font-style: normal;}
footer #foot-right #foot-info dl{
	overflow: hidden;
	width: 380px;}
footer #foot-right #foot-info dl dt{
	float: left;
	width: 100px;}
footer #foot-right #foot-info dl dd a{
	color: #fff;
	-webkit-transition: 0.5s;
	-moz-transition: 0.5s;
	-o-transition: 0.5s;
	-ms-transition: 0.5s;
	transition: 0.5s;}
footer #foot-right #foot-info dl dd a:hover{
	color: #fff000;
	text-decoration: none;}

/* フッターナビ */
footer #foot-navi{
    margin: -10px 0 ;
    heigth: auto;
	background: #fff;
	padding: 15px 50px;}
footer #foot-navi ul{
	overflow: hidden;
	border-left: 1px solid #000;
	width: 600px;
	margin: 0 auto;
    padding-left:0px;}
footer #foot-navi ul li{
	float: left;
	font-size: 14px;
	line-height: 1;
	list-style-type: none;
	border-right: 1px solid #000;
	padding: 2px 15px;}
footer #foot-navi ul li a{
	color: #000;
	text-decoration: none;
	-webkit-transition: 0.5s;
	-moz-transition: 0.5s;
	-o-transition: 0.5s;
	-ms-transition: 0.5s;
	transition: 0.5s;}
footer #foot-navi ul li a:hover{
	color: #036eb8;
	text-decoration: underline;}

/* コピーライト */
footer #copyright{
	color: #fff;
	font-size: 12px;
	margin-top: 15px;}
footer #copyright a{
	color: #fff;}

