<script type="text/javascript">
	$(function() {
    	/*Inicio orden de paises mobile*/
		var countryList = $("#dv-country option");
		countryList.sort(function(a, b) {
		    return $(a).attr("data-prioridade") - $(b).attr("data-prioridade")
		});
		$("#dv-country").html(countryList);
		/*Fin orden de paises mobile
		Inicio orden de paises desktop*/
		var countryList = $(".bloco-country div");
		countryList.sort(function(a, b) {
		    return $(a).attr("data-prioridade") - $(b).attr("data-prioridade")
		});
		$(".bloco-country").html(countryList);
		/*Fin orden de paises desktop*/

		$("input[name=country]").first().attr("checked", "checked");

		$.each($("input[name=language]"), function() {
		    if ($(this).val() == Liferay.ThemeDisplay.getLanguageId()) {
		        $(this).attr("checked", "checked");
		    }
		});
		$.each($("input[name=country]"), function() {
		    if ($(this).val().trim() == store.get("country")) {
		        $(this).attr("checked", "checked");
		    }
		});
		setTimeout(function() {
		    if (window.location.href.indexOf("/faq") > -1 && store.get("Aba")) {
		        var Aba = store.get("Aba");
		        $('#faq ul li a').eq(Aba).click();
		        store.remove("Aba");
		    }
		}, 1000);
		$(".modal-red-btn").on('click', function() {
		    var country = $("input[name=country]:checked").val().trim();
		    var newLang = "/" + $("input[name=language]:checked").val().split("_")[0].trim() + "/";
		    var lang = "/" + Liferay.ThemeDisplay.getLanguageId().split("_")[0] + "/";
		    var curURL = window.location.href;
		    var relURL = window.location.pathname;
		    var aba_faq = $('#faq ul li a.selected').attr("href");

		    if (curURL.indexOf("/faq") > -1) {
		        var aba_faq = $("#faq li a.selected").attr("href").substring(1);
		        store.set("Aba", aba_faq);
		    }

		    $(".menu-hover-backdrop").css("display", "none");
		    $("#modal-pais").css("display", "none");
		    $("#boton-cerrar").css("display", "block");

		    store.set("country", country);
		    if(relURL == "/") {
		    	relURL = "";
		    }

		    if (newLang != lang) {
				updateLanguage($("input[name=language]:checked").val());
		        if (curURL.indexOf("/en/") > -1 || curURL.indexOf("/pt/") > -1 || curURL.indexOf("/es/") > -1) {
		            var newURL = curURL.replace(lang, newLang);
		        } else {
		            var newURL = Liferay.ThemeDisplay.getPortalURL() + newLang + relURL + window.location.hash;
		        }
		        if (curURL.indexOf("/busca") == -1) {
		            setTimeout(function() {
		                window.location.href = newURL;
		            }, 500);
		        }
		    }
		});

		if (store.get("country")) {
		    $("#modal-pais").css("display", "none");
		    $("#boton-cerrar").css("display", "block");
		} else {
		    if ($(".bloco-country .radio").length == 1) {
		        $("#dv-country option[data-prioridade]").attr('selected', 'selected');
		        $('.modal-red-btn').trigger('click');
		        $('.modal-red-btn-mobile').trigger('click');
		    } else {
		        $("#modal-pais").css("display", "block");
		    }
		}
		$(".modal-red-btn").attr("disabled", true);
		$(".modal-red-btn-mobile").attr("disabled", true);
		$("#boton-cerrar").on("click", function() {
		    $("#modal-pais").css("display", "none");
		    $(".menu-hover-backdrop").css("display", "none");
		});
		/*INICIO Posiciones elementos*/
		function positionElement() {
		    var pos_radio = $(".bloco-country .radio:first").position();
		    var pos_p = $(".country-modal p:first").position();
		    var pos_p2 = $(".country-modal p:last").position();
		    $(".bloco-language").css("top", pos_radio.top);
		    $("#modal-pais .modal-pais-content .modal-pais-body .languages-modal p:first").css("top", pos_p.top);
		    $("#modal-pais .modal-pais-content .modal-pais-body .languages-modal p:last").css("top", pos_p2.top);
		}
		positionElement();
		/* FIN Posiciones elementos */
		$(".modal-red-btn").attr("disabled", false);
		$(".modal-red-btn-mobile").attr("disabled", true);



		$("#dv-language, #dv-country").on('change', function() {
		    if (($("#dv-language").val().trim().length) && ($("#dv-country").val().trim().length)) {
		        $(".modal-red-btn-mobile").attr("disabled", false);
		    } else {
		        $(".modal-red-btn-mobile").attr("disabled", true);
		    }
		});

		/* Inicio MOBILE */
		$(".modal-red-btn-mobile").on('click', function() {
		    var country = $("#dv-country").val().trim();
		    var newLang = "/" + $("#dv-language").val().split("_")[0].trim() + "/";
		    var lang = "/" + Liferay.ThemeDisplay.getLanguageId().split("_")[0] + "/";
		    var curURL = window.location.href;
		    var relURL = window.location.pathname;

		    $(".menu-hover-backdrop").css("display", "none");
		    $("#modal-pais").css("display", "none");
		    $("#boton-cerrar").css("display", "block");

		    store.set("country", country);

		    if(relURL == "/") {
		    	relURL = "";
		    }
		    if (newLang != lang) {
		        if (curURL.indexOf("/en/") > -1 || curURL.indexOf("/pt/") > -1 || curURL.indexOf("/es/") > -1) {
		            var newURL = curURL.replace(lang, newLang);
		        } else {
		            var newURL = Liferay.ThemeDisplay.getPortalURL() + newLang + relURL;
		        }
				updateLanguage($("#dv-language").val());
		        setTimeout(function() {
		            window.location.href = newURL;
		        }, 500);
		    }
		});

		function attSelectCountry() {
		    var country;
		    if (store.get("country")) {
		        $.each($("#dv-country option"), function() {
		            country = $(this).val().trim();
		            if (country == store.get("country")) {
		                $('#dv-country').val(country).change();
		            }
		        });
		    } else {
		        $('#dv-country').val('').change();
		    }

		}

		function attSelectLanguage() {
		    var language;
		    $.each($("#dv-language option"), function() {
		        language = $(this).val().trim();
		        if (language == Liferay.ThemeDisplay.getLanguageId()) {
		            $('#dv-language').val(language).change();
		        }
		    });
		}

		attSelectLanguage();
		attSelectCountry();
		/* Fin MOBILE */
		/*CR-473809 ...*/

		function checkLocalStorage() {
		    var country;
		    var countries = [];
		    if (store.get("country")) {
		        $.each($(".bloco-country .radio-success-per"), function() {
		            country = $(this).find('input').val().trim();
		            countries.push(country);
		        });

		        if (countries.indexOf(store.get("country")) < 0) {
		            $("#modal-pais").css("display", "block");
		            $("input[name=country]").first().attr("checked", "checked");
		            $("#boton-cerrar").css("display", "none");
		            positionElement();
		        }
		    }
		}
		checkLocalStorage();
		/*CR-473809 */
	});
	
	function updateLanguage(newLang){
		try{
			var clientId = store.get("clientId");
			if(clientId != "" && clientId != null & clientId != undefined){
				var customerToken = services.retrieveCustomerToken(clientId);
				if(customerToken.accessToken != ""){
					services.saveCustomerToken( customerToken.accessToken, customerToken.refreshToken, customerToken.expiresIn, clientId, newLang);
				}
			}
		}catch(err){
			console.log("erro servico - modal");
		}
	}
        setTimeout(function(){
            if (store.get('expire_in')) {
			 var interval = setInterval(functions.checkExpiredSession, 60000);
			 functions.checkExpiredSession();
			}	
        },800);	
</script>


