<script type="text/javascript">
	function lookup() {
		if(document.vform.word.length <4) {
			// Hide the suggestion box.
			$('#suggestions').hide();} else {
			$.post("http://www.encyclo.co.uk/autoComplete/rpc.php", {word: ""+document.vform.word.value+""}, function(data){
				if(data.length >0) {
					$('#suggestions').show();			$('#autoSuggestionsList').html(data);		}
			});}
	} // lookup
	
	function fill(thisValue) {
		$('#word').val(thisValue);setTimeout("$('#suggestions').hide();", 200);
	}
</script>

