<section class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="page-header">
                <h1>Encontre Seu imóvel <small>Temos vários imóveis excelentes, um deles é perfeito para você</small></h1>
            </div>
        </div>
    </div>


    <section class="home_results realestates-vue">

        <div class="row">
            <div class="col-md-12">
                <form class="form-inline search__home" action="/encontre-seu-imovel" method="GET">
                    <div class="form-group form-group-lg">
                        <label for="search" class="sr-only">Busque o bairro que você você gostaria de morar</label>
                        <input type="text" class="form-control" id="search" name="q" placeholder="Em qual bairro ou cidade você gostaria de morar?" v-model="keyword">
                    </div>
                    <button type="submit" class="btn btn-round btn-danger-light "><span class="sr-only">Buscar</span> <span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
                </form>
            </div>
        </div>
      <section>
  <div class="container loading" v-show="!loaded">
    <i class="fa fa-spinner fa-pulse fa-3x fa-fw" aria-hidden="true"></i>
  </div>
  <div v-show="loaded" style="display:none">
    <div class="row results_list django-load">
      
    </div>
    <div class="row results_list js-load">
      <div class="col-md-4 col-sm-6 animated"  transition="fade" v-for="cards in loadedData | filterBy filterByKeyword | count | limitBy pages">
        
<div class="card [[cards.status.slug]]">

  <a href="[[cards.url]]">
    <header>
        <span class="hidden-sm hidden-xs"><p class="truncated hidden-sm">[[cards.region]] <small>| [[cards.neighborhood.name]]</small></p></span>
        <p class="truncated hidden-md hidden-lg">[[cards.region]] <br> <small>[[cards.neighborhood.name]]</small></p>
    </header>
    <div class="image" aria-hidden="true">
      <img v-if="cards.covers[0]" :src="cards.covers[0].versions.card.url" alt="" class="img-responsive"  />
      <img v-else src="/static/placehold/card_360x260.png" alt=""  class="img-responsive" />
        <ul class="list-inline badges-list">
            <li v-for="badges in cards.badges"><img v-if="badges.name === 'Minha Casa Minha Vida'" :src="badges.image" class="img-responsive" alt="[[ badges.name ]]"></li>
        </ul>
    </div>

    <div class="name">
      <div>
        <p>
          [[cards.name]]
        </p>
      </div>
      <div class="text-right">
        <p class="status">
          <span class="icons" :class="'status-'+cards.status.slug" aria-hidden="true"></span> [[cards.status.name]]
        </p>
      </div>
    </div>

    <div class="address">
      <span class="icons icon-place" aria-hidden="true"></span>
      <p>
        [[cards.address]]
      </p>
    </div>
  </a>


  <div class="content">
    <p>[[cards.teaser]]</p>
  </div>
  <footer class="text-right">
    

  <a class="btn btn-danger chat-service" data-media="Site_Tenda" onclick="window.open(this.href, 'mywin',
'left=20,top=20,width=500,height=600,toolbar=1,resizable=0')" role="button" target="_blank" name="button[[cards.code]]" href="http://tenda.hypnobox.com.br/?acao=index&controle=Atendimento&produto=[[cards.code]]&description=[[cards.name]]">Tire suas dúvidas no chat</a>

      
  </footer>
</div>

      </div>
    </div>
    <div class="row">

      <ul class="pagination">
          <li class="disabled">
             <a href="#" aria-label="Anterior">
               <span aria-hidden="true">&laquo;</span>
             </a>
          </li>

          

