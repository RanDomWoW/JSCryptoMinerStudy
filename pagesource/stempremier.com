<script>
  function Testimonial(data) {
    this.name = data[0];
    this.title = data[1];
    this.spaceBefore = data[2]; // number of squareSpace columns
    this.width = data[3]; // number of squareSpace columns
    this.slideNum = data[4];
    this.rowNum = data[5];
    this.justification = data[6];
    this.statement = data[7];
    this.profilePic = data[8];
  }

  // called by various pages to create and populate their Testimonial Slider
  function initTestimonialSlider(slider, testimonials) {
    function appendTestimonialToRow(tstObj, row) {
      row.append('<div class="sqs-col sqs-col-' + tstObj.spaceBefore + '">&nbsp;</div>');

      var tstDiv = document.createElement('div');
      $(tstDiv).addClass('testimonial tst-' + tstObj.justification + ' sqs-col sqs-col-' + tstObj.width);

      $(tstDiv).append('<p class="tst-statement">' + tstObj.statement + '</p>');
      $(tstDiv).append('<div class="tst-profile"><img src="http://labs.slantmedia.net/STEM/'+ tstObj.profilePic + '"><p class="tst-name">' + tstObj.name + '</p><p class="tst-title">' + tstObj.title + '</p></div></div></div>');

      row.append(tstDiv);
    }

    var currentSlideNum = 1;
    var currentRowNum = 1;

    var slide = document.createElement('div');
    $(slide).addClass("testimonial-slide");
    var row = document.createElement('div');
    $(row).addClass("sqs-row");

    // loop through all the testimonials, starting and ending the slide and
    // row divs as appropriate
    for (i=0; i<testimonials.length; i++) {
      if (testimonials[i].slideNum !== currentSlideNum) {
        $(slide).append(row);
        slider.append(slide);

        slide = document.createElement('div');
        $(slide).addClass("testimonial-slide");
        currentSlideNum++;

        row = document.createElement('div');
        $(row).addClass("sqs-row");
        currentRowNum = 1;
      }
      else if (testimonials[i].rowNum !== currentRowNum) {
        $(slide).append(row);

        row = document.createElement('div');
        $(row).addClass("sqs-row");
        currentRowNum++;
      }

      appendTestimonialToRow(testimonials[i], $(row));
    }

    $(slide).append(row);
    slider.append(slide);
    slider.cycle();
  }
</script>

