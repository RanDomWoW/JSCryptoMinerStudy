<script type="text/javascript">
    window.onresize = function () {
        setDog();
        setXGN();
        resetBannerMsg();
    }
    setDog();
    setXGN();
    function setDog() {
        var w = $(".dogBox").width();
        var h = jsps(w, 900);
        $(".dogBox").height(h);
        var fzs1 = 60;
        if (w > 992) {
            var fzs = jsps(w, fzs1);
            $(".dogMsg").css({ "font-size": fzs });
        } else {
            $(".dogMsg").css({ "font-size": "20px" });
        }
        var mts = jsps(w, 60);
        $(".dogBtnL,.dogBtnR").css({ "margin-top": mts });
    }

    function setXGN() {
        var w = $(".xiaoguniang").width();
        var h = jsps(w, 880);
        $(".xiaoguniang").height(h);
        var mts = jsps(w, 200);
        var pts = jsps(w, 30);
        var fzs = jsps(w, 40);
        $(".xgnMsg").css({ "margin-top": mts, "padding-top": pts });
        $(".xgnFont").css({ "font-size": fzs });
        var xbmts = jsps(w, 60);
        $(".xgnBtn").css({ "margin-top": xbmts });
    }
    function jsps(w, ws) {
        var nw = 1724 / ws;
        var w = w / nw;
        var w = parseInt(w);
        return (w + 1) + "px";
    }

    $(function () {
        var cw = $(".contentDiv").width();
        if (cw != null && cw < 200) {
            $(".shbtn").hide();
        }
    });
</script>

