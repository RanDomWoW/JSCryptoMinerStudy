resetBannerMsg();
function resetBannerMsg() {
  var w = $(".banner_itemsDiv").width();
  setmsg0(w);
  setmsg2(w);
  setmsg3(w);
  setmsg4(w);
}
function setmsg0(w) {
  var s = 115;
  var s1 = 50;
  var s2 = 30;
  if (w < 769) {
    $(".banner_item0 .tle2").css({ "margin-top": "-6px" });
    s1 = 70;
    s2 = 50;
    s = 130;
  }
  setBottom(".banner_item0", w, s);
  setFontSize(".banner_item0 .tle1", w, s1);
  setFontSize(".banner_item0 .tle2", w, s2);
}
function setmsg2(w) {
  var bt = 100;
  var s1 = 40;
  var s2 = 25;
  if (w < 769) {
    var pt = 30;
    $(".banner_item2").css({ "top": "10px", "bottom": "auto" });
    s1 = 60;
    s2 = 40;
    var s2mt = w / (768 / 250);
    var s2mt = parseInt(s2mt);
    var s2mtStr = s2mt + "px";
    $(".banner_item2 .tle2").css({ "margin-top": s2mtStr });
  } else {
    setBottom(".banner_item2", w, bt);
    $(".banner_item2").css({ "top": "auto" });
    $(".banner_item2 .tle2").css({ "margin-top": "auto" });
  }

  setFontSize(".banner_item2 .tle1", w, s1);
  setFontSize(".banner_item2 .tle2", w, s2);
}
function setmsg3(w) {
  var s = 95;
  setBottom(".banner_item3", w, s);
  setFontSize(".banner_item3 .tle1", w, 50);
}
function setmsg4(w) {

  setFontSize(".banner_item4 .tle1", w, 40);
  setLineH(".banner_item4 .tle1", w, 100);
  if (w < 769) {
    $(".banner_item4").css({ "top": "30%" });
    setLineH(".banner_item4 .tle1", w, 120);
    setFontSize(".banner_item4 .tle1", w, 60);
  }
}

// 根据参数设置底部距离，c：需要控制的class，w：当前banner宽度，b：需要设置的底部默认底部距离（css最初的设置）
function setBottom(c, w, b) {
  var bs = jspx(w, b);
  $(c).css({ "bottom": bs });
}
// 根据参数设置字体大小，c：需要控制的class，w：当前banner宽度，b：需要设置的底部默认底部距离（css最初的设置）
function setFontSize(c, w, s) {
  var ss = jspx(w, s);
  $(c).css({ "font-size": ss });
}
// 根据参数设置行高，c：需要控制的class，w：当前banner宽度，b：需要设置的底部默认底部距离（css最初的设置）
function setLineH(c, w, s) {
  var ss = jspx(w, s);
  $(c).css({ "line-height": ss });
}
//根据参数计算实际需要缩放的值，w：当前banner宽度，ws：实际参数默认值（css最初的设置）
function jspx(s, ws) {
  var nw = 1724 / ws;
  var w = s / nw;
  var w = parseInt(w);
  var str = (w + 1) + "px";
  return str;
}
</script>
