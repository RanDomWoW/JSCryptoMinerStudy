        setTimeout(function() {
            layer.style.display = 'block';
                            layer.style.left = parseInt(document.body.clientWidth / 2) + parseInt(_x) + 'px';
                layer.style.top = _y + 'px';
                    }, 100);
    } else {
        layer.style.display = 'block';
        layer.style.left = _x;  
