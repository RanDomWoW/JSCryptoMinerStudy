		function configurar_banner_flutuante() {

			$(".colunaads").height( $(".container-principal").height() );

        	$(document).on('scroll', function () {

	            var tamanho_banner 			= new Number( 600 );
	            var distancia_para_o_topo 	= new Number( $(this).scrollTop() );
	            var distancia_para_o_rodape = new Number( $(document).height() ) - distancia_para_o_topo + tamanho_banner;
	            var altura_do_rodape 		= new Number( $("footer").height() );

				console.log( distancia_para_o_topo );
				console.log( "---" );

				$(".banner-flutuante").css("width", "200px");
				$(".banner-flutuante").css("height", "600px");

	            if ( distancia_para_o_topo > 2295 ) {

					if ( $(document).height() <= distancia_para_o_topo + altura_do_rodape + tamanho_banner + 55 ) {

		            	$(".banner-flutuante").css("position", "absolute");
		            	$(".banner-flutuante").css("top", "auto");
		            	$(".banner-flutuante").css("bottom", "-100px");

	            	} else {

		            	$(".banner-flutuante").css("position", "fixed");
		            	$(".banner-flutuante").css("top", "10px");
	            	}
	            } else {

	            	$(".banner-flutuante").css("position", "relative");
	            	$(".banner-flutuante").css("top", "0px");
	            }
	        });
		}
