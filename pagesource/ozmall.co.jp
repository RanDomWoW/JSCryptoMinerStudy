<script type="text/javascript">
    
    var RUN_MODE_ONLOAD = "RUN_MODE_ONLOAD";
    
    var RUN_MODE_CLICK = "RUN_MODE_CLICK";
    
    var guid = "";
    
    var hashUid = "";
    
    var requestUrl = "";

    
    jQuery('.point_ico').bind('click',function(){
        callApplyPoint_ajax(RUN_MODE_CLICK);
    });

    
    applyPoint_OnLoad();

    
    jQuery('.closeBtn, .closeBtn02').click(function () {
        jQuery("div.DPWrapper").remove();
        jQuery('div.dpBox').css('display', 'none');
    });

    
    function applyPoint_OnLoad() {
        
        var url = location.href;
        var urlParams = url.split("?");
        if (2 <= urlParams.length) {
            
            var params = urlParams[1].split("&");
            for (var i = 0; i < params.length; i++) {
                var keyValue = params[i].split("=");
                var key = keyValue[0];
            	if ("guid" == key) {
            		guid = keyValue[1];
            	} else if ("uid" == key) {
					hashUid = keyValue[1];
            	}
            }
            
            if (guid != "" && hashUid != "") {
                requestUrl = urlParams[0];
                callApplyPoint_ajax(RUN_MODE_ONLOAD);
            }
        }
	}

    
    function callApplyPoint_ajax(runMode) {
        jQuery.ajax({
            type: "GET",
            url: "/common_inc/inc/ApplyPointPresent.aspx",
            cache: false,
            data:
			{
			    guid: guid,
			    hashUid: hashUid,
			    requestUrl: requestUrl,
                runMode:runMode
			},
            dataType: "text",
            success: function (response) {
                var json = jQuery.parseJSON(response);
                dispResult(json.status, json.point);
            },
            error: function (err) {
                dispResult("ng", 0);
            }
        });
    }

    
	function dispResult(status, point) {
		if (status == "ok") {
			jQuery("#placeStart").css("display", "none");
			jQuery("#placeWinning").css("display", "block");
			jQuery("#winningImg").attr({
				"src": "/common_inc/dailypresent/images/tit_point_" + point + ".png",
				"alt": "おめでとうございます" + point + "ポイントGET",
				"width": "405",
				"height": "76"
			});
		}
		else if (status == "ng") {
			jQuery("#placeStart").css("display", "none");
			jQuery("#placeError").css("display", "block");
			jQuery("#errorMsg").html("エラーが発生しました。<br>再度お試しください。");
		}
		else if (status == "expire") {
			jQuery("#placeStart").css("display", "none");
			jQuery("#placeExpire").css("display", "block");
		}
		else if (status == "repeated") {
			jQuery("#placeStart").css("display", "none");
			jQuery("#placeDone").css("display", "block");
		}
		else if (status == "start") {
			jQuery("#placeStart").css("display", "block");
		}
	}
</script>
