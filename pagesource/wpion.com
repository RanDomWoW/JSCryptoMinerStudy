<script>
    /**
     * Parse cookies and return specific cookie content
     * @param cookieName
     * @returns {*} cookie content or null when cookie si not set
     */
    function tdGetCookieByName(cookieName) {
        var cookies = document.cookie.split(";"),
            cookie = null, //return null when cookie is not set
            cookieSplit;

        for (var i=0; i < cookies.length; i++) {
            cookieSplit = cookies[i].split("=");
            if (cookieSplit[0].trim() === cookieName) {
                cookie = cookieSplit.slice(1).join("=");
                break;
            }
        }

        return cookie;
    }

    /**
     * menu match between wpion.com and wpion.com/members
     */
    function tdMembersMenu() {
        var tdUserMenuData = tdGetCookieByName('tdUserMenuData'),
            mainMenuLogin = jQuery(".td-main-menu-login");
        //check if cookie is set
        if (tdUserMenuData !== null) {
            try
            {
                //parse cookie JSON
                var tdUserMenuDataObj = JSON.parse(decodeURIComponent(tdUserMenuData));
                if (typeof tdUserMenuDataObj.display_name === 'string' && tdUserMenuDataObj.display_name.length) {
                    //cookie content is not empty
                    var menuElements = mainMenuLogin.children(),
                        affiliateLink = '';

                    if (typeof tdUserMenuDataObj.affiliate !== 'undefined' && tdUserMenuDataObj.affiliate.length > 0) {
                        affiliateLink = '<li class="tdm-sub-menu-list"><a href="' + tdUserMenuDataObj.affiliate + '">Affiliate</a></li>';
                    }

                    //remove "Sign in" and "Create free acount"
                    menuElements.remove();

                    //add new menu elements
                    var userMenu = '<li class="tdm-menu-avatar menu-item">' + tdUserMenuDataObj.user_avatar + '<span class="td_user_logd_in">' + tdUserMenuDataObj.display_name + '</span>' +
                                       '<ul class="tdm-sub-menu" style="display: none;">' +
                                            '<li class="tdm-sub-menu-list"><a href="' + tdUserMenuDataObj.view_profile +'">View profile</a></li>' +
                                            '<li class="tdm-sub-menu-list"><a href="' + tdUserMenuDataObj.edit_profile + '">Edit profile</a></li>' +
                                            '<li class="tdm-sub-menu-list"><a href="' + tdUserMenuDataObj.my_downloads + '">My downloads</a></li>' +
                                            '<li class="tdm-sub-menu-list"><a href="' + tdUserMenuDataObj.my_invoices + '">My invoices</a></li>' +
                                             affiliateLink +
                                            '<li class="tdm-sub-menu-list"><a href="' + tdUserMenuDataObj.signout + '">Sign out</a></li>' +
                                       '</ul>' +
                                   '</li>';
                    mainMenuLogin.append(userMenu);
                }
            }
            catch(e)
            {
                //error - JSON parse failed
                console.log('invalid tdUserMenuData json');
                console.log(e);
            }
        }
        //restore visibility for main menu and user menu
        mainMenuLogin.css('visibility', 'visible');
        jQuery('#td-header-menu .sf-menu').css('visibility', 'visible');

    }

    jQuery().ready(function() {
        tdMembersMenu();
    });
</script>

