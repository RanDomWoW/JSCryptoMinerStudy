      function changeContent(pestanaSeleccionada)
      {
          $('#ptw-container.ptw-menu-vertical #ptw-menu>ul>li#'+pestanaSeleccionada).removeClass('background_li_white');
          $('#'+pestanaSeleccionada+' a').removeClass('background_li_white');
		  
		  $('#ptw-container.ptw-menu-vertical #ptw-menu>ul>li#'+pestanaSeleccionada).addClass('background_li_azul');
          $('#'+pestanaSeleccionada+' a').addClass('background_li_azul');

          var pestanaAnterior=$('#tabselected').val();
          $('#ptw-container.ptw-menu-vertical #ptw-menu>ul>li#'+pestanaAnterior).addClass('background_li_white'); 
          $('#'+pestanaAnterior+' a').addClass('background_li_white');          
		  
          if(pestanaAnterior=='pestanavuelos')
            $('#ap_booker_Flight').hide();
          if(pestanaAnterior=='pestanahoteles')
            $('#ap_booker_Hotel').hide();
          if(pestanaAnterior=='pestanapaquetes')
            $('#ap_booker_FlightPackage').hide();
          if(pestanaAnterior=='pestanaautos')
            $('#ap_booker_Car').hide();
          if(pestanaAnterior=='pestanatransfers')
            $('#ap_booker_Transfer').hide();
          if(pestanaAnterior=='pestanabuses')
            $('#ap_booker_Bus').hide();
          if(pestanaAnterior=='pestanacruises')
            $('#ap_booker_Cruise').hide();
          if(pestanaAnterior=='pestanatours')
            $('#ap_booker_Tour').hide();

          if(pestanaSeleccionada=='pestanavuelos')
            $('#ap_booker_Flight').show();
          if(pestanaSeleccionada=='pestanahoteles')
            $('#ap_booker_Hotel').show();
          if(pestanaSeleccionada=='pestanapaquetes')
            $('#ap_booker_FlightPackage').show();
          if(pestanaSeleccionada=='pestanaautos')
            $('#ap_booker_Car').show();
          if(pestanaSeleccionada=='pestanatransfers')
            $('#ap_booker_Transfer').show();
          if(pestanaSeleccionada=='pestanabuses')
            $('#ap_booker_Bus').show();
          if(pestanaSeleccionada=='pestanacruises')
            $('#ap_booker_Cruise').show();
          if(pestanaSeleccionada=='pestanatours')
            $('#ap_booker_Tour').show();

          $('#tabselected').val(pestanaSeleccionada);
      }
       (function ($) {
      $('#ptw-container.ptw-menu-vertical #ptw-menu>ul>li#pestanavuelos').css('background','#699DCD');
