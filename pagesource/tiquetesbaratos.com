      $('#tabselected').val('pestanavuelos');
      
	  $('.ptw-menu-vertical#ptw-container #ptw-menu>ul>li#pestanatours>a').css('padding-bottom','14px');
	  
      $('#pestanavuelos a').click(function(){         
         if($('#tabselected').val()!='pestanavuelos')
		   changeContent('pestanavuelos');
      });
      $('#pestanahoteles a').click(function(){
		 widgetMain.switchEngine('Hotel', false);       
         if($('#tabselected').val()!='pestanahoteles')
		   changeContent('pestanahoteles');
      });
      $('#pestanapaquetes a').click(function(){
