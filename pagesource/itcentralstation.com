  //$('[data-item-type*="download-report"], [data-item-type*="download_report"]').each(function () {
  //  this.href = $.add_search_params(this.href, {iframe: 1});
  //  $(this).click(lity);
  //});
  // see https://docs.google.com/document/d/1_OVS8rOKDpedUBTVh2FXof2TsvVto9jluHFdg9SkDJk/edit regarding lightbox closing behavior
  var show_alert = false,
      redirect_to = false,
      first_visit_path = '/pages2/first_visit';
  $(document).on('lity:ready', function (e, instance) { // Executed when link is clicked and iframe is created.
    var iframe = instance.element().find('iframe'),
        iframe_window = iframe.get(0).contentWindow; 
    iframe.one('load', function () { // On iframe load. Use one() because we have multiple pages loading in this iframe, but we only need execute this code once.
      iframe_window.$('#leadform').submit(function() {
        show_alert = true;
      });
      iframe.contents().find('.change-email').click(function() {
        redirect_to = location.pathname;
      });
      iframe.on('load', function() { // Anytime the iframe loads within itself, we want to close it
        if (iframe_window.location.pathname === first_visit_path) { // They are currently on the first_visit page.
          redirect_to = first_visit_path;
        }
        instance.close();
      })
    })
  }).on('lity:remove', function() { // lightbox close event
    show_alert && alert(' We are sending you the report by email.');
    if (redirect_to) {
      location.pathname = redirect_to;
    }
  });
});</script></div></body></html>