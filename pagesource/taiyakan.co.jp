			<script>
			$(function() {
				var req;
			
				$("#prefecture").change(function(){
					if (req) {
						req.abort();
					}
			
					$("#salemore").show();
					$(".sale-block ul").empty();
					$("#pref_id").val( $("#prefecture").val() );
			
					req = $.ajax({
						url: '/api/sales.json',
						type: 'GET',
						dataType: 'json',
						data: {
							'pref_id': $("#pref_id").val()
						},
						xhrFields: {
							withCredentials: true
						},
						beforeSend: function(xhr){
							xhr.overrideMimeType('application/json;charset=UTF-8');
						}
			
					}).done(function(res, status, xhr) {
						req = null;
			
						if (res.result !== 'success') {
							// TODO: エラー処理
							console.log('error', res.error);
							return;
						}
			
						$("#cnt").val( res.items.length );
						$("#total").val( res.total );
						$(".sale-block ul").append( $("#sale_tmpl").tmpl(res.items) );
			
						if (res.total == 0) {
							$(".sale-none").show();
						} else {
							$(".sale-none").hide();
						}
			
						if ( $(".sale-block ul li").length == parseInt($("#total").val()) ) {
							$("#salemore").hide();
						}
			
					}).fail(function(error) {
						req = null;
			
						// TODO: エラー処理
						console.log('error', error);
					});
				});
			
				$("#salemore").click(function(){
					if (parseInt($("#cnt").val()) >= parseInt($("#total").val())) {
						return;
					}
			
					if (req) {
						req.abort();
					}
			
					req = $.ajax({
						url: '/api/sales.json',
						type: 'get',
						dataType: 'json',
						data: {
							'pref_id': $("#pref_id").val(),
							'offset': $("#cnt").val()
						},
						xhrFields: {
							withCredentials: true
						},
						beforeSend: function(xhr){
							xhr.overrideMimeType('application/json;charset=UTF-8');
						}
			
					}).done(function(res, status, xhr) {
						req = null;
			
						if (res.result !== 'success') {
							// TODO: エラー処理
							console.log('error', res.error);
							return;
						}
			
						$("#cnt").val( parseInt( $("#cnt").val() ) + res.items.length );
						$(".sale-block ul").append( $("#sale_tmpl").tmpl(res.items) );
			
						if ( $(".sale-block ul li").length == parseInt($("#total").val()) ) {
							$("#salemore").hide();
						}
			
					}).fail(function(error) {
						req = null;
			
						// TODO: エラー処理
						console.log('error', error);
					});
				});
			});
			</script>
