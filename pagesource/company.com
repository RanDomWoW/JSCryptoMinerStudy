    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script> 
<script>
  var availableDates;
  $('#button--1800-loggedin').click(function() {
    $('#button--1800-loggedin span').text('Working on it..');
    $.ajax({
        method: "GET",
        url: "/dashboard/site/getAvailableSlots",
        cache: false,
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        data: {
            "website": 1
        },
        cache:false,
        success:function(data){
            availableDates = data;
            showTalkToExpertForm();
            
        }
    })
    return false;
  });

  function showTalkToExpertForm(){
    $("html, body").animate({
        scrollTop: 0
    }, "slow");
    setTimeout(function() {
        $('#talk-with-expert-loggedin').show();
        $('.pricing-email').css('top', '3%');
        $('#talk-with-expert-loggedin').addClass('loaded');
    }, 700);
    showDatePicker();
  }


  function showDatePicker(){
    var datesArray = Object.keys(availableDates);
    var minDate = datesArray[0];
    var maxDate = datesArray[(datesArray.length-1)];


     $( "#datepicker1" ).datepicker({ minDate: new Date(minDate),maxDate: new Date(maxDate),beforeShowDay: $.datepicker.noWeekends});
     // $('#timepicker1').timepicker({ disableTextInput:true });
  }

  $( "#datepicker1").change(function() {
    if($(this).val()!== ""){
      var select_date = $(this).val().split('/');
      var format_date = select_date[2]+"-"+select_date[0]+"-"+select_date[1];
      var timeTranslate = {
"09:00":"9:00 AM",
"09:30":"9:30 AM",
"10:00":"10:00 AM",
"10:30":"10:30 AM",
"11:00":"11:00 AM",
"11:30":"11:30 AM",
"12:00":"12:00 PM",
"12:30":"12:30 PM",
"13:00":"1:00 PM",
"13:30":"1:30 PM",
"14:00":"2:00 PM",
"14:30":"2:30 PM",
"15:00":"3:00 PM",
"15:30":"3:30 PM",
"16:00":"4:00 PM",
"16:30":"4:30 PM",
"17:00":"5:00 PM",
"17:30":"5:30 PM",
"18:00":"6:00 PM",
"18:30":"6:30 PM"
};

      $.each( availableDates[format_date], function( key, value ) {
        if(value == 0){
           $('#timepicker2').append($("<option></option>").attr("value",key).text(timeTranslate[key])); 
        }
      });
       $("#timepicker2").prop('disabled', false);
    }
  });


</script>


