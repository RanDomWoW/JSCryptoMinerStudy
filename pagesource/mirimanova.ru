<script>
    function dump(obj) {
        var out = '';
        for (var i in obj) {
            out += i + ": " + obj[i] + "\n";
        }

        console.log(out);
    }

    function get_book(id, prev) {
        $('.spinner').show();

        if(typeof(id) === 'undefined') {
            $.ajax({
                type: "POST",
                url: 'http://mirimanova.ru/get_book',
                success: function(data){
                    var link = '/books/show/';
                    $('.book-title>span').text(data.caption);
                    $('.ozon_link').attr('href', data.ozon_link);
                    $('.litres_link').attr('href', data.litres_link);
                    $('.labirint_link').attr('href', data.labirint_link);
                    $('#index_book_pdf>a').attr('href', link + data.id);
                    $('.book-image>img').attr('src', data.picture);
                    if(data.color){
                        $('.books-preview').css('background-color', data.color);
                    }else{
                        $('.books-preview').css('background-color', 'green');
                    }

                    $(".next_book").attr('onClick', "get_book("+data.id+")");
                    $(".prev_book").attr('onClick', "get_book("+data.id+", true)");
                    $('.spinner').hide();
                },
                error: function(){
                    console.log('error');
                }
            });

        }else{
            if(typeof(prev) === 'undefined'){
                $.ajax({
                    type: "POST",
                    url: 'http://mirimanova.ru/get_next_book',
                    data: 'id='+id,
                    success: function(data){
                        var link = '/books/show/';

                        $('.book-title>span').text(data.caption);
                        $('.ozon_link').attr('href', data.ozon_link);
                        $('.litres_link').attr('href', data.litres_link);
                        $('.labirint_link').attr('href', data.labirint_link);
                        $('#index_book_pdf>a').attr('href', link + data.id);
                        $('.book-image>img').attr('src', data.picture);
                        if(data.color){
                            $('.books-preview').css('background-color', data.color);
                        }else{
                            $('.books-preview').css('background-color', 'green');
                        }

                        $(".next_book").attr('onClick', "get_book("+data.id+")");
                        $(".prev_book").attr('onClick', "get_book("+data.id+", true)");

                        $('.spinner').hide();
                    },
                    error: function(){
                        console.log('error');
                    }
                });
            }else{
                $.ajax({
                    type: "POST",
                    url: 'http://mirimanova.ru/get_prev_book',
                    data: 'id='+id,
                    success: function(data){
                        var link = '/books/show/';

                        $('.book-title>span').text(data.caption);
                        $('.ozon_link').attr('href', data.ozon_link);
                        $('.litres_link').attr('href', data.litres_link);
                        $('.labirint_link').attr('href', data.labirint_link);
                        $('#index_book_pdf>a').attr('href', link + data.id);
                        $('.book-image>img').attr('src', data.picture);
                        if(data.color){
                            $('.books-preview').css('background-color', data.color);
                        }else{
                            $('.books-preview').css('background-color', 'green');
                        }

                        $(".next_book").attr('onClick', "get_book("+data.id+")");
                        $(".prev_book").attr('onClick', "get_book("+data.id+", true)");
                        $('.spinner').hide();
                    },
                    error: function(){

                        console.log('error');
                    }
                });
            }
        }
    }

