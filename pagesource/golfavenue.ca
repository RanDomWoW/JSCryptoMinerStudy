    var registerFormAjax = new VarienForm('register-form-ajax', true);

	jQuery('#loginRegisterAjaxModalPanels').find('.modal-flip-login').click(function(event) {
		event.preventDefault();
		registerFormAjax.validator.reset();
		jQuery('#loginRegisterAjaxModalPanels').flip(false);
	});

	jQuery('#loginRegisterAjaxModal').on('hidden.bs.modal', function (e) {
		registerFormAjax.validator.reset();
		jQuery('#register-form-ajax-alert').slideUp(500, function() {
        	jQuery('#register-form-ajax-alert').html('');
        });
	});

	jQuery('#register-form-ajax').submit(function(event) {
		event.preventDefault();
		
		 if(!registerFormAjax.validator.validate()) {
            return false;
        }

        jQuery('#register-form-ajax-alert').slideUp(500, function() {
        	jQuery('#register-form-ajax-alert').html('');
        });

		var $form = jQuery(this);
		jQuery('#registerAjaxModal-loadingOverlay').fadeIn();
		
		jQuery.ajax({
			url: $form.attr('action'),
			type: 'POST',
			dataType: 'JSON',
			data: $form.serialize(),
		})
		.done(function(response) {
			if (response.ack == 'success') {
				window.location.href = response.redirect_url;
			} else {
				jQuery('#register-form-ajax-alert').slideDown(500, function() {
		        	jQuery('#register-form-ajax-alert').html(response.message);
		        });
			}
		})
		.fail(function(response) {
			jQuery('#register-form-ajax-alert').slideDown(500, function() {
		        jQuery('#register-form-ajax-alert').html('An error occured. Please try again.');
		    });
		})
		.always(function(response) {
			jQuery('#registerAjaxModal-loadingOverlay').fadeOut();
		});
	});
