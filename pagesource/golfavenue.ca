<script type="text/javascript">
    var loginFormAjax = new VarienForm('login-form-ajax', true);

    jQuery('#loginRegisterAjaxModalPanels').find('.modal-flip-register').click(function(event) {
		event.preventDefault();
		loginFormAjax.validator.reset();
		jQuery('#loginRegisterAjaxModalPanels').flip(true);
	});

	jQuery('#loginRegisterAjaxModal').on('hidden.bs.modal', function (e) {
		loginFormAjax.validator.reset();
		jQuery('#login-form-ajax-alert').slideUp(500, function() {
        	jQuery('#login-form-ajax-alert').html('');
        });
	});

	jQuery('#login-form-ajax').submit(function(event) {
		event.preventDefault();
		
		 if(!loginFormAjax.validator.validate()) {
            return false;
        }

        jQuery('#login-form-ajax-alert').slideUp(500, function() {
        	jQuery('#login-form-ajax-alert').html('');
        });

		var $form = jQuery(this);
		jQuery('#loginAjaxModal-loadingOverlay').fadeIn();
		
		jQuery.ajax({
			url: $form.attr('action'),
			type: 'POST',
			dataType: 'JSON',
			data: $form.serialize(),
		})
		.done(function(response) {
			if (response.ack == 'success') {
				window.location.href = response.redirect_url;
			} else {
				jQuery('#login-form-ajax-alert').slideDown(500, function() {
		        	jQuery('#login-form-ajax-alert').html(response.message);
		        });
			}
		})
		.fail(function(response) {
			jQuery('#login-form-ajax-alert').slideDown(500, function() {
		        jQuery('#login-form-ajax-alert').html('An error occured. Please try again.');
		    });
		})
		.always(function(response) {
			jQuery('#loginAjaxModal-loadingOverlay').fadeOut();
		});
	});
