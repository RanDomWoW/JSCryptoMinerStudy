<script type="text/javascript">
/* DRAG AND DROP BOX */
$(function(){
    if (jQuery.browser.msie && parseInt(jQuery.browser.version) <= 6) {
        $('.page-edit, .edithandle, #col_ghost, .bg-edit-content').remove();
