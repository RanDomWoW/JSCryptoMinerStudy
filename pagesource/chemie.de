<script>
    $('#nl-subscribe-form').submit(function() {
        var _serialized = $(this).serializeArray();
        var _erg = emailModuleCheck($("input[name='email2']").val());

        if (_erg == true) {
            var _serializeStr = '';
            var _nlids = '';
            var _count = 0;
            var _countnlids = 0;
            jQuery.each(_serialized, function(index, element){
                if (element.name=='newsletterid') {
                    if (_countnlids == 0) {
                        _nlids = element.value;
                    } else {
                        _nlids += '|'+element.value;
                    }
                    _countnlids++;
                } else {
                    if (_count > 0) {
                        _serializeStr += '&'+element.name+'=' + encodeURIComponent(element.value);
                    } else {
                        _serializeStr += element.name+'=' + encodeURIComponent(element.value);
                    }
                    _count++;
                }
            });
            _serializeStr += '&newsletterid='+_nlids;

            $.ajax({
                url     : 'http://www.chemie.de/visitor/send/',
                data    : _serializeStr,
                dataType : "json",
                success : function(data) {
                    if (data.message == "OK") {
                        $('#nl-subscribe-form input').parent().css('border','0px');
                        $('#nlcontent').html('<p>'+data.required+'</p>').css('background', '#B1D2B3');
                    } else {
                        var _required = data.required.split(',');
                        jQuery.each(_required, function(index, element) {
                            if (element=='newsletterid') {
                                $("input[name='"+element+"']").parent().css({
                                    'border':'3px solid #DF1710',
                                    'width':'270px',
                                    'padding':'5px'
                                });
                            } else {
                                $("input[name='"+element+"']").parent().css({
                                    'border':'3px solid #DF1710',
                                    'width':'245px'
                                });
                            }
                        });
                    }
                }
            });
        }
        return false;
    });


    function emailModuleCheck(_mailadress) {
        var _addrObj = new Object({
            0 : 'info',
            1 : 'contact',
            2 : 'kontakt',
            3 : 'support',
            4 : 'sales'
        });
        var _check = false;

        var _emailReg = new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,8}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);

        /* disabled (PORTAL-8742)
        jQuery.each(_addrObj, function(index, _addr) {
            var i = (_mailadress).indexOf(_addr+'@', 0);
            if (i > -1) _check = true;
        });
        */
        if (_check == false) {
            if(_emailReg.test(_mailadress)) {
                $("input[name='email2']").parent().css('border','0');
                var _emailCheck = true;
            } else {
                $("input[name='email2']").parent().css({
                    'border':'3px solid #DF1710',
                    'width':'245px'
                });
                var _emailCheck = false;
            }
        } else {
            _emailCheck = false;
        }

        if ($("input[name='newsletterid']:checked").val()) {
            $("input[name='newsletterid']").parent().css('border','0');
            var _nlidCheck = true;
        } else {
            $("input[name='newsletterid']").parent().css({
                'border':'3px solid #DF1710',
                'width':'270px',
                'padding':'5px'
            });
            var _nlidCheck = false;
        }

        if ((_emailCheck && _nlidCheck) == true) {
            return true;
        } else {
            return false;
        }
    }
</script>
