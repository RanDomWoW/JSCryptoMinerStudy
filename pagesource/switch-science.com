      function drawMostViewedItems(json){
        if(json.recommendeditems == null){
          $('#easyrec-recommend').hide();
          return;
        }
        // shuffle!
        for(i = json.recommendeditems.item.length; i > 0;) {
          var j = Math.floor(Math.random()*i);
          var t = json.recommendeditems.item[--i];
          json.recommendeditems.item[i] = json.recommendeditems.item[j];
          json.recommendeditems.item[j] = t;
        }
        drawMostViewedItemsToDiv(json, "mostviewed_items");

        //animation
        $("div.holder").jPages({
          perPage      : 2, // 4 items *2 rows = 8 items
          containerID : "items",
          //links       : false,
          //first       : false,
          //last        : false,
          callback    : function( pages, items ){
            items.showing.find("img").trigger("turnPage");
            items.oncoming.find("img").trigger("turnPage");
          }
        });
      }
      function drawMostViewedItemsToDiv(json, divname){
        if("undefined" == typeof(json.error)){
          try{
            var items = json.recommendeditems.item;
          }catch(e){
            return;
          }
          if("undefined" == typeof(items.length)){
            items = new Array(items);
          }
          if(items.length>0){
            listString ="<ul class='items' id='items'>";
            for (x=0; x <items.length; x++){
              if(x%4 === 0) {
                listString += "<div class='itemrow'>";
              }
              listString += "<li class='item'><a href='" + items[x].url + "'>" + "<img class='lazy' src='" + items[x].imageUrl + "'><div class='info'><div class='name'>" + items[x].description + "</div></div></a>" + "</li>";
              if(x%4 === 3) {
                listString += "</div>";
              }
            }
            $('#mostviewed_items > .holder').after(listString+"</ul>");
          }
        }
      }
