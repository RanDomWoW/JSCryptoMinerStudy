/**
 * 广告跟踪注册方法，写入cookie跟踪,以后其他地方都用这个方法，各自页面或js里面定义的废掉
 * 
 * utm_cookie 新增的，标示广告主的唯一标示，回调通知或是订单查询的时候需要
 */
var adsFollow = function(utm_source, utm_medium, utm_term, utm_content, utm_campaign, utm_cookie) {
	var args = {
		"method" : "adsFollowRegester",
		"utmSource" : utm_source,
		"utmMedium" : utm_medium,
		"utmTerm" : utm_term,
		"utmContent" : utm_content,
		"utmCampaign" : utm_campaign,
		"utmCookie" : utm_cookie
	};
	Webapp.post(Service.auth, args, function(data) {}, function(data) {});
}
 $(function(){      
    //广告跟踪
    if(xl.P("utm_source")){
      adsFollow(xl.P("utm_source"),xl.P("utm_medium"),xl.P("utm_term"),xl.P("utm_content"),xl.P("utm_campaign"),xl.P("utm_cookie"));
    }
});  
</script> 
