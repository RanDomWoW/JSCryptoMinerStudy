$(document).ready(function() {

  $("#close-sidebar-cross").on("click", function(){
    $("#shopping-cart").sidebar('hide');
  });
  var mobileMenu = new Hammer(document.getElementById('shopping-cart'));
    // listen to events...
    mobileMenu.on("swiperight", function(ev) {
        $("#shopping-cart").sidebar('hide');
    });


	var $div = $("#shopping-cart");
	var observer = new MutationObserver(function(mutations) {
		mutations.forEach(function(mutation) {
			if (mutation.attributeName === "class") {
				var attributeValue = $(mutation.target).prop(mutation.attributeName);
				var rowCount = $('#product-table tbody tr').length;
				if(rowCount == '0') {
					$("#cart-empty").removeAttr( 'style' );
					$("#cart-products").css('display','none');
					$("#checkout-coupon-wrap").css('display','none');
					$("#cart-action-box").css('display','none');

				}else{
					$("#cart-empty").css('display','none');
					$("#cart-products").removeAttr( 'style' );
					$("#checkout-coupon-wrap").removeAttr( 'style' );
					$("#cart-action-box").removeAttr( 'style' );
				}
			}
		});
	});
	observer.observe($div[0],  {
		attributes: true
	});
});
</script>

