<script type="text/template" id="tWishUIItems">
<% if (items.length > 0) { %>
<% _.each(items, function(item) { %>
<div class="WishUI_item sWishUIItem">
<a href="/item/<%= item.id %>">
<div class="WishUI_item_ec"><img src="<%= imageUrl+getItemThumb(item.id, '110x110') %>"></div>
<div class="WishUI_item_info">
<%= item.name %>
</div>
</a>
</div>
<% }); %>
<% } else { %>
<div class="WishUI_empty"><%= emptyText %></div>
<% } %>
</script>
<script type="text/template" id="tCartUIItems">
<% if (items.length > 0) { %>
<% _.each(items, function(item) { %>
<div class="CartUI_item sCartUIItem">
<a href="/item/<%= item.id %>">
<div class="CartUI_item_ec"><img src="<%= imageUrl+getItemThumb(item.id, '45x45') %>"></div>
<div class="CartUI_item_info">
<p class="itemName"><%= item.name %></p>
<p class="skuName"><%= item.skuName %></p>
<p class="quantity">数量 <%= item.quantity %></p>
</div>
</a>
</div>
<% }); %>
<% } else { %>
<div class="CartUI_empty"><%= emptyText %></div>
<% } %>
</script>
