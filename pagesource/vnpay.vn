			$('#frm_lang').submit();
		})
		$(".main-menu li.dropdown").click(function() {
			$(this).find("ul.dropdown-menu").toggle();
		})
		$(".multi_language ul").hover(function() {
			$(this).find(".lang-hidden").toggle();
		})
		$(".multi_language a").click(function(){
			var id = parseInt($(this).attr("dataid")); 
			$("#txtlang").val(id);
			$("#frm_lang").submit();
		})

		$('#form_search_help').submit(function(){
			var val = $(this).find($('#ip_search_help')).val();
			$(this).find($('input[name="q"]')).val(val);
		})

		$('#frm_search_home').submit(function(){
			var val = $(this).find($('#txt_keywork')).val();
			$(this).find($('input[name="q"]')).val(val);
		})

		$('.icon_search_tablet').click(function(){
			$('#frm_search_home').slideToggle();
		})
	})


