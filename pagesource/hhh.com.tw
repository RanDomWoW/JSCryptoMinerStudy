<script type="text/javascript">
$(document).ready(function(){
       
    // fade in scrollToTop
    $(function () {
        $(window).scroll(function () {
            if ($(this).scrollTop() > 100) {
                $('.scrollToTop').fadeIn();
            } else {
                $('.scrollToTop').fadeIn();
            }
        });

        // scroll body to 0px on click
        $('.scrollToTop').click(function () {
            $('body,html').animate({
                scrollTop: 0
            }, 800);
            return false;
        });
    });

    var calculateFee = function(){
        var v1 = $("#toolForm2 input[name='tff21']").val() | 0;
        var v2 = $("#toolForm2 input[name='tff22']").val() | 0;
        var v3 = (v2 > v1) ? v1 : v2;
        $("#tff23").html( Math.round(v3 * $("#tff24").html() *0.01) );
    }
    var isNumber = function(value){
        if( /^\d+$/.test(value) ){
            $("#tff25").html('');            
        }else{
            $("#tff25").html('請輸入半型數字');
        }
    }
    $("#toolForm2 input[name='tff21']").bind("change paste keyup",function(){
        isNumber($(this).val());
        var v = 5;
        if($(this).val()>509999){ v+=1}
        if($(this).val()>1509999){ v+=1}
        if($(this).val()>3009999){ v+=1}
        if($(this).val()>5009999){ v+=1}
        if($(this).val()>10009999){ v+=1}
        $("#tff24").html(v);
        calculateFee();
    });
    
    $("#toolForm2 input[name='tff22']").bind("change paste keyup",function(){
        isNumber($(this).val());
        calculateFee();
    });
});

function toolForm1(){
    var v1 = $("#toolForm1 input[name='name']").val();
    var v2 = $("#toolForm1 input[name='phone']").val();
    if( v1!=='' && v2 !=='' ){ 
        $("#toolForm1").html('<h4><br>已將您的需求送出<br>將於<span>1個工作日</span>內<br>由幸福經紀人與您連繫<br></h4>');
        $.post('http://www.hhh.com.tw/modules/gs/ajax.php',
            {'name':v1, 'phone':v2, 'ajax_decoRequest':1},
            function(data){ console.log(data)
        });
    }else{
        $('#toolForm1 .note').html('皆為必填');
    }
}
</script>

