<script>
  function load_script(url, callback) {
    var script = document.createElement( "script" )
    script.setAttribute('async', '');
    script.type = "text/javascript";
    if(script.readyState) {  //IE
      script.onreadystatechange = function() {
        if (script.readyState === "loaded" || script.readyState === "complete") {
          script.onreadystatechange = null;
          callback();
        }
      };
    } else {  //Others
      script.onload = function() {
        callback();
      };
    }
    script.src = url;
    document.getElementsByTagName( "head" )[0].appendChild( script );
  }
  if ((window.location.href.indexOf("media_source=clubedosdados") > -1)) {
    if ((window.location.href.indexOf("https") > -1)) {
      load_script('//cdn.pushwoosh.com/webpush/pushwoosh-web-notifications.js', function() {});
      Pushwoosh.push(['init', {
        logLevel: 'info', // or debug
        applicationCode: 'E813B-B1DED',
        safariWebsitePushID: 'clubedosdescontos.com',
        defaultNotificationTitle: 'Clube dos Descontos',
        defaultNotificationImage: 'https://cp.pushwoosh.com//icons//E/8/E813B-B1DED.png'
      }]);
      if (null === localStorage.getItem('pwAllowPushNotifications')) {
        this.showSubscriptionWindow();
      }
    } else {
      load_script('//www.boostbox.com.br/pushwoosh-web-pushes-http-sdk.js?pw_application_code=E813B-B1DED', function() {
        pushwoosh.subscribeAtStart();
      });
    }
  }
</script>
