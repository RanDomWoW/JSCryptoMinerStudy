<script>
  function toggle_forgot() {
    if ($('#forgot_password').css('display') == 'none') {
      $('#forgot_password').css('display', 'block');
      $('#new_admin_user').css('display', 'none');
    } else {
      $('#forgot_password').css('display', 'none');
      $('#new_admin_user').css('display', 'block');
    }
  }
  function forgot() {
    window.jQuery.ajax({
      data: {
        "email": $('#forgot_admin_user_email').val()
      },
      async: true,
      url: '/send_password_email',
      success: function(data) {
        if (data != false) {
          $('#forgot_status').html('Enviado.');
        } else {
          $('#forgot_status').html('Algum erro ocorreu. Tente mais tarde.');
        }
      }
    });
  }
  
  function login() {
    window.jQuery('#login_button').text('Carregando...')
    window.jQuery.ajax({
      type: 'POST',
      data: { 'admin_user' : {
        "email": $('#admin_user_email').val(),
        "password": $('#admin_user_password').val(),
        "remember_me": 1
      }},
      async: true,
      url: '/admin_users/sign_in.json',
      success: function() {
        window.location.href = window.location.href + 'administration'
      },
      error: function() {
        $('#try_again').css('display', 'block');
        window.jQuery('#login_button').text('LOGIN');
      }
    });
  }
</script>