<script type="text/javascript">

(function( $ ) {
	'use strict';

	var ThumbSlider = function( elem, options, callback ) {
		var settings = $.extend({
			anz_thumnails: 4,
			class_item: 'media-box__gallery__item',
			class_navigation: 'thumbslider-navigation',
			class_btn_prev: 'thumbslider-previous',
			class_btn_next: 'thumbslider-next',
			selector_image_holder: 'figure.slider-box__item__img-holder',
			selector_title: '.ribbon__content',
			selector_link: '.item-link',
			callback: function() {},
			debug: false,
		}, options || {});

		var isLocked;

		// public functions via this.fn
		this.init = function() {

			debug('---- init');

			var item_selector = '.' + settings.class_item + '[data-item-index]'; 

			$( elem ).find( item_selector ).hide();

			this.isLocked = false;
			this.i_min = $( elem ).find( item_selector ); // tmp
			this.i_current = 0;
			// this.i_current = this.i_min;

			this.count_img = $( elem ).find( item_selector ).length; 

			debug('init -> this');
			debug(this);

			$( elem ).append( '<div class="box-modul"><div class="slider-box" /></div>' );

			var k_max = this.count_img; // alle bilder hier rein, quasi preloading
			for (var k=0; k<k_max; k++) {
				var $new_item = $( '<div class="slider-box__item thumb-' + k +'" data-thumb-index="'+k+'" />' );

				$new_item.appendTo( $( elem ).find(' .slider-box' ) );
				if ( k > (settings.anz_thumnails - 1) ) {
					$new_item.hide();
				}
			}

			// this fuer's each merken ...
			var that = this;
			$( elem ).find( '.slider-box .slider-box__item').each( function(){
				debug('init -> that.i_current');
				debug(that.i_current);
				debug('init -> $(this)');
				debug($(this));
				updateThumbnail.call( that, $(this), that.i_current, false );
			});

			// init buttons
			$( elem ).find('.' + settings.class_navigation).click(function( e ){

				e.preventDefault();

				var direction;
				// welcher isses?
				if ( $(this).hasClass(settings.class_btn_prev) ) {
					direction = 'prev';
				} else if ( $(this).hasClass(settings.class_btn_next) ) {
					direction = 'next';
				} else {
					debug('direction: wedernoch!');
					return false;
				}

				initSlide.call(that,direction);
			});

			// done?
			options.callback.call(that);

		} // init

		var initSlide = function( direction ) {
			// 1. check 'n' lock
			if (isLocked) {
				debug('THUMBSLIDER LOCKED!');
				return false;
			} else {
				lock();
			}

			// maximaler index ... davon ausgehend dass immer stupide hochgezaehlt wurde ...
			var i_max = this.count_img-1;

			var i_target;
			if ( direction == 'next' ) {
				i_target = this.i_current + 1;
			} else {
				i_target = this.i_current - 1;
			}

			if (i_target > i_max) {
				i_target = 0;
			} else if (i_target < 0) {
				i_target = i_max;
			}


			debug( 'current ' + this.i_current );
			debug( 'target ' + i_target );
			debug( 'max ' + i_max ); 
			debug( 'direction (pre) ' + direction );

			// wenn anhand von aktuellem i, maximalem i und ziel-i checken obs nach vorne oder zurueck geht ...
			if ( !( this.i_current == 0 && i_target == i_max ) && (this.i_current < (i_target) || ( this.i_current == i_max && i_target == 0 ) ) ) {
				direction = 'next';
			} else {
				direction = 'prev';
			}
			debug( 'direction (post) ' + direction );

			// die liste der thumb-boxen durchlaufen und befuellen ...
			for (var i=0; i<settings.anz_thumnails; i++) {
				debug(i);
				var $thumb = $( elem ).find('.slider-box .thumb-'+i);
				debug($thumb);
				updateThumbnail.call( this, $thumb, i_target, direction );
			}
		}

		var doSlide = function ( i_target, direction, box, data ) {
			var that = this;
			var img = box.find( settings.selector_image_holder );
			var img_w = img.eq(0).width();
			var i_this = box.data('thumb-index');

			var options, style;
			if (direction == 'next') {
				options = { left: "-="+img_w };
				style = 'left: +'+img_w+'px;';
			} else {
				options = { left: "+="+img_w };
				style = 'left: -'+img_w+'px;';
			}

			// solang die nutzung von velocity statt animate noch unklar ist hier switchen - qnd
			if (typeof $.fn.velocity == 'function') {
				// nur letztes thumb animiert entfernen - die anderen werden eh von den neuen ueberdeckt!
				if (direction == 'prev' && i_this < (settings.anz_thumnails-1) ) {
					img.remove();
				} else {
					img.velocity( options, function() { 
						$(this).remove(); 
					});
				}
				$( getThumbnailHtml.call( that, data.src,data.text,data.link,style) )
				.appendTo(box)
				.velocity(options,function() {
					endSlide.call( that, i_target );
				});
			}
		}

		var endSlide = function( i_target ) {
			unlock(); // auf verdacht unlocken wenn einer sich fertig bewegt hat
			this.i_current = i_target;
		}

		// private functions via var fn

		// thumbnailbox nehmen, in bildliste nach passendem thumb suchen und das reinplatzieren bzw -animieren
		var  updateThumbnail = function( box_thumbnail, i_target, direction ) {

			var that = this;

			if (i_target !== 0 && !i_target ) {
				return false;
			}

			var i_this = box_thumbnail.data('thumb-index');


			var i_img = i_target + i_this ; // + statt -
			if (i_img >= this.count_img ) {
				debug(this.count_img + ' / ' + i_target + ' / ' + i_this + ' / ' + i_img);

				i_img = i_img - this.count_img;

				debug(this.count_img + ' / ' + i_target + ' / ' + i_this + ' / ' + i_img);
			}


			var $img_thumb = $( elem ).find('[data-image-index=' + i_img + ']' );
			var src_thumb = $img_thumb.data( 'image-thumb' );
			if (src_thumb == undefined) { // wenn kein thumb als data angegeben src nehmen 
				var src_thumb = $img_thumb.attr( 'src' );
				// debug(src_thumb);
			} else {
				// debug(src_thumb);
				// return false; // master interessiert uns net, ist im big
			}

			var $item = $( elem ).find('[data-item-index=' + i_img + ']' );
			var $titel = $item.find( settings.selector_title );
			var $link = $item.find( settings.selector_link );

			var text = false;
			if ( $titel.length > 0 ) {
				text = $titel.html();
			}
			var link = false;
			if ( $link.length > 0 ) {
				link = $link.attr('href');
			}

			// wenn direction mitgegeben: animieren - sonst einfach anzeigen ...
			if ( direction) {

				var data_thumbnail = {
					src: src_thumb,
					text: text,
					link: link
				};
				doSlide.call(that, i_target, direction, box_thumbnail, data_thumbnail);
			} else {
				// wie oben angedroht: keine animation ...
				$( getThumbnailHtml.call( that, src_thumb, text, link) ).appendTo(box_thumbnail);
				// unlock(); // auf verdacht unlocken wenn einer sich fertig bewegt hat
			}
		} // ubdade thumbnail

		// html-block fuer's slidin' thumbnail
		var getThumbnailHtml = function( src_thumb, text, link, style ) {
			if (style && style.length > 0) {
				style = 'style="'+style+'"';
			} else {
				style = '';
			}
			if (text && text.length > 0) {
				text = '<div class="img-overlay"><div class="img-overlay__content">' + text + '</div></div>';
			}
			var link_0, link_1 = false;
			if (link && link.length > 0) {
				link_0 = '<a href="'+link+'">';
				link_1 = '</a>';
			}
			var html = '<figure class="slider-box__item__img-holder" '+style+'>'
						+ text
						+ link_0
						+ '<img src="' + src_thumb + '" alt="" />'
						+ link_1
						+ '</figure>';
			return html;
		}

		// lock'n'unlock
		var lock = function() {
			debug('LOCK');
			isLocked = true;
		}

		var unlock = function() {
			debug('UNLOCK');
			isLocked = false;
		}

		// debugger
		var debug = function( bug ) {
			if ( settings.debug && typeof console == 'object' && typeof console.debug == 'function' ) {
				console.debug( bug );
			}
		}
	};

	// Jquery Wrapper For Init Handling
    $.fn.thumbSlider = function(options, callback) {

    	if (typeof options == 'function') {
    		callback = options;
    		options = {};
    	}

    	if (typeof callback == 'function') {
    		options.callback = callback;
    	}

		// pass options to plugin constructor
		var thumbSlider = new ThumbSlider(this, options);

		// prepare dom, bind listeners
		thumbSlider.init();

		// iterate through given dom nodes and append click handling
    	return this.each(function() {
    		var $elem = $(this);

    		// Return early if this element already has a plugin instance
			if ( $elem.data('thumbSlider') ) return;

			// Store plugin object in this element's data
			$elem.data('thumbSlider', thumbSlider);
		});

	}
})( jQuery );

