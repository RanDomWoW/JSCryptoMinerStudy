            var openMenu = function () {
                var qal = document.querySelectorAll("#mobMenuToggle,.navbar-toggle,.overlay-mobilemenu");
                for (var i = 0; i < qal.length; i++) {
                    qal[i].classList.add("in");
                }
                disableScroll();// area expanded = false
            };

            var closeMenu = function () {
                var qal = document.querySelectorAll("#mobMenuToggle,.navbar-toggle,.overlay-mobilemenu");
                enableScroll(); // area expanded = true
                for (var i = 0; i < qal.length; i++) {
                    qal[i].classList.remove("in");
                }
            };

            var toggleMenu = function (el) {
                //menu,menu-button,overlay
                if (el.className.indexOf('in') < 0) {
                    openMenu();
                    document.getElementById("mobMenuToggle").addEventListener('touchmove', function (e) {
                        if (e.target.scrollHeight > (e.target.clientHeight)) {
                            e.stopPropagation();
                        }
                    }, false);
                } else {
                    closeMenu();
                }
            };

            // submenu collapse and expand
            var subMenuToggle = function (el) {
                var thisMenuItem = el.parentNode.getElementsByClassName('SubMenuITEM');
                if (el.className.indexOf('active') >= 0) {
                    el.classList.remove('active');
                    thisMenuItem[0].classList.remove('vs');
                } else {
                    var sbi = document.querySelectorAll('.SubMenuITEM');
                    var hdg = document.querySelectorAll('.HeadIng');
                    for (var i = 0; i < sbi.length; i++) {
                        sbi[i].classList.remove('vs');
                    }
                    for (var i = 0; i < hdg.length; i++) {
                        hdg[i].classList.remove('active');
                    }
                    el.classList.add('active');
                    thisMenuItem[0].classList.add('vs');
                }
            };

            window.addEventListener('load', function () {
                // on opening popup hide mobile menu
                var mbm = document.getElementById('mobMenuToggle');
                var mbmLinks = mbm.querySelectorAll(".list-unstyled,.list-unstyled a,[gtmdir-url='gba-prelogin']");
                for (var i = 0; i < mbmLinks.length; i++) {
                    mbmLinks[i].addEventListener('click', function (e) {
                        closeMenu();
                    }, false);
                }
                // on logo
                document.getElementsByClassName('navbar-brand')[0].addEventListener('click', function () {
                    closeMenu();
                });

                // toggle fixed header on scroll
                window.addEventListener('scroll', function (e) {
                    var scrollArea = document.querySelectorAll('.headerCntr nav.navbar, .topmenuArea');
                    for (var i = 0; i < scrollArea.length; i++) {
                        if (document.documentElement.scrollTop > 50 || document.body.scrollTop > 50) {
                            scrollArea[i].classList.add("fixed-down");
                        } else {
                            scrollArea[i].classList.remove("fixed-down");
                        }
                    }
                }, false);
            });
        </script>

        
        
