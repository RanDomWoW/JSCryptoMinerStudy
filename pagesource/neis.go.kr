<title>나이스 대국민서비스</title>
<script type="text/javascript" src="/jquery/jquery-1.4.2.min.js"></script>
<script type="text/javascript" src="/netfunnel/js/netfunnel.js" charset=UTF-8 ></script>

<script type="text/javascript">
//<![CDATA[
var isSido = true;//현재 17개 시도에서 접속중인지 여부
$(document).ready(function() {
		var currentUrl = document.location+"";//현재 브라우저 url
		currentUrl = currentUrl.toUpperCase();

		//브라우저의 url과 비교할 주소. neis.go.kr(총괄)에 대해서만 netfunnel을 적용함
		var netFunnelApplyUrl = "NEIS.GO.KR";

		var splitUrl = "";
		if (currentUrl.indexOf("HTTP") != -1)
		{
			var tempArr = currentUrl.split("//");
			tempArr = tempArr[1].split("/");
			splitUrl = tempArr[0];
		}
		else if (currentUrl.indexOf("WWW") != -1)
		{
			var tempArr = currentUrl.split("/");
			splitUrl = tempArr[0];
		}
		else
		{
			splitUrl = currentUrl;
		}

		splitUrl = splitUrl+"";

		if (splitUrl.indexOf(netFunnelApplyUrl) != -1)
		{
			isSido = false;
		}



//    if(navigator.appName.indexOf("Microsoft") > -1)
	if(false)
	{
		var activeXObject = new Array();

		activeXObject[0] = "EPKIWCtl.EPKIWCtlCtrl.1";				// #1. 표준 보안 모듈
		activeXObject[1] = "MAWEBSAFERKERIS.MaWebSAFERKERISCtrl.1";	// #2. 웹DRM 보안 모듈
		activeXObject[2] = "mkd25aos2.mkd25aosCtrl.1";				// #3. 키보드 보안 모듈
		activeXObject[3] = "aosmgr.aosmgrCtrl.1";					// #3. 개인방화벽 모듈

//		activeXObject[0] = "MAWEBSAFERKERIS.MaWebSAFERKERISCtrl.1";	// #1. MARKANY
//		activeXObject[1] = "NPKFXX.NPKFXXCtrl.1";						// #2. nProtect 6.0
//		activeXObject[1] = "NPKCX.NPKCXCtrl.1";						// #2. nProtect 5.0
//		activeXObject[2] = "npenkIEInstall5.npenkInstall5.1";		// #3. 개인방화벽
//		activeXObject[3] = "EPKIWCtl.EPKIWCtlCtrl.1";				// #4. BCQRE

		var notIstallObjectCount = 0;

		for ( i = 0 ; i < activeXObject.length ; i++ )
		{
			try {
				// 윈도우 registry에서 ActiveX의 PROGID 를 찾아서 넣어줘야함
				var activeX = new ActiveXObject( activeXObject[i] );

				if ( !activeX )
				{
					notIstallObjectCount++;
				}
			}
			catch(ex)
			{
				notIstallObjectCount++;
			}
		}
		if ( notIstallObjectCount > 0 )
		{
			window.location="/pas_ObjectInstall.html";
		}
		else
		{
			if (!isSido)
			{
				NetFunnel_goUrl({}, "/pas_mms_nv99_001.do");
			}
			else
			{
				window.location.href="/pas_mms_nv99_001.do";
			}
		}
	}
	else
	{
		if (!isSido)
		{
			NetFunnel_goUrl({}, "/pas_mms_nv99_001.do");
		}
		else
		{
			window.location.href="/pas_mms_nv99_001.do";
		}
	}
});
//]]>
</script>
