<script type="text/javascript">
	$(function(){
			var rollingId = $("#slide_bnrA");
			var RollingImg = rollingId.find("a");
			RollingImg.attr("show","N");
			RollingImg.eq(0).attr("show","Y");
			RollingImg.hide();
			rollingId.find("a[show=\"Y\"]").show();

			var countImg = $("#counter_bnrA td:even");
			countImg.eq(0).addClass("active");
			
			/* 다음버튼 */
			$("#next_bnrA").click(function(){
				var thisImg = rollingId.find("a[show=\"Y\"]");
				RollingImg.hide();
				rollingId.find("[show]").attr("show","N");
				countImg.removeClass("active");
				
				if(thisImg.next().is("a")){
					thisImg.next().attr("show","Y");
					thisImg.next().fadeIn("slow");
					countImg.eq(thisImg.prev().index()).addClass("active");
					countImg.eq(thisImg.prev().prev().index()).removeClass("active");
				}else{
					RollingImg.eq(0).attr("show","Y");
					rollingId.find("a[show=\"Y\"]").fadeIn("slow");
					countImg.eq(0).addClass("active");
				}
			});

			/* 이전버튼 */
			$("#prev_bnrA").click(function(){
				var thisImg = rollingId.find("a[show=\"Y\"]");
				RollingImg.hide();
				rollingId.find("[show]").attr("show","N");
				countImg.removeClass("active");
										
				if(thisImg.prev().is("a")){
					thisImg.prev().attr("show","Y");
					thisImg.prev().fadeIn("slow");
					//console.log(thisImg.prev().index());
					var prIndex = thisImg.prev().index();
					countImg.eq(thisImg.prev().index()-2).addClass("active");
				}else{
					RollingImg.eq(RollingImg.length-1).attr("show","Y");
					rollingId.find("a[show=\"Y\"]").fadeIn("slow");
					countImg.eq(RollingImg.length-1).addClass("active")
				}
			});

			/* 하단 슬라이드 */
			$("#counter_bnrA td:even").mouseover(function(){
				var thisEq = $(this).attr("num");
				RollingImg.attr("show","N");
				RollingImg.eq(thisEq).attr("show","Y");
				RollingImg.hide();
				rollingId.find("a[show=\"Y\"]").show();
				countImg.removeClass("active");
				$(this).addClass("active");
			});

			setInterval(function(){
				 $("#next_bnrA").trigger("click");
			},5000)
		})
</script>
											
			
