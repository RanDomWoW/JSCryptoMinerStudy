    function wishListDeleteProduct(customerProductCode,customerWishListID,customerWishListProductID,customerWishListName, wishListProductsCountDelete,apiCall){
        if(customerWishListID > 0 && customerWishListName != '' && customerWishListProductID > 0){
            if (customerWishListName == 'Wish List') {
                $('.productBoxTopSellerSp-mbl').find('a.whishListIcon').attr('data-wishListProductsCount',(wishListProductsCountDelete - 1));
                $('.productBoxTopSellerSp-mbl').find('a#' + customerProductCode).removeClass('active').attr('data-customerWishListID', 0).attr('data-customerWishListProductID', 0).attr('data-customerWishListName', '');
                if ($('li#listRow-' + customerProductCode + '-' + customerWishListID).parents('ul').children('li').length == 1) {
                    $('li.sec-wishlist').removeClass('fillActive');
                    $('.merklisteDelSec').addClass('blankList');
                    $('.merklisteSec-hd').parents('.wishlistTabSec').find('ul > li > i#wish-list').attr('data-customerWishListID', 0).attr('data-customerWishListName', '').attr('onclick', 'javascript:void(0);');
                }
            } else if ($('li#listRow-' + customerProductCode + '-' + customerWishListID).parents('ul').children('li').length == 1) {
                $('.gesehenDelSec').addClass('blankList');
                $('.merklisteSec-hd').parents('.wishlistTabSec').find('ul > li > i#last-visited').attr('data-customerWishListID', 0).attr('data-customerWishListName', '').attr('onclick', 'javascript:void(0);');
            }

            $('li#listRow-' + customerProductCode + '-' + customerWishListID).remove();

            $.ajax({
                type: "POST",
                url: "/api/public/CSRFToken",
                dataType: "json",
                async:true,
                success : function(json) {
                    var csrfToken = json.jsonResult;
                    $.ajax({
                        type: "POST",
                        url: "/api/public/WishListDeleteProduct",
                        data : {
                            "customerWishListID" : customerWishListID,
                            "customerWishListName" : customerWishListName,
                            "customerWishListProductID" : customerWishListProductID,
                            "csrfToken" : csrfToken
                        },
                        async:true,
                        contentType: "application/x-www-form-urlencoded",
                        dataType: "json",
                        success: function(resp){
                            if(resp.success == true) {
                                dataLayer.push({
                                    'event': 'wishlist',
                                    'action_type': 'remove'
                                });
                            }
                        },
                        error : function(json){
                            $('.merklisteDelSec, .gesehenDelSec').removeClass('loading-state');
                            showLoading(false);
                            showAlert(dynamicVariables['apiResponseError'], 'danger');
                            console.log(json);
                        }
                    });
                },
                error : function(json){
                    if(!apiCall) wishListDeleteProduct(customerProductCode,customerWishListID,customerWishListProductID,customerWishListName,wishListProductsCountDelete,apiCall--);
                    else{
                        $('.merklisteDelSec, .gesehenDelSec').removeClass('loading-state');
                        showLoading(false);
                    }
                }
            });
        }
    }

