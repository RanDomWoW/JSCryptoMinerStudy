                    showLoading(false);
                    showAlert(dynamicVariables['apiResponseError'], 'danger');
                    console.log(resp);
                }
            }
        });
    }

    function logout(isExpire,apiCall) {
        if (typeof isExpire === 'undefined') isExpire = false;
        $('li.sec-wishlist').removeClass('fillActive');
        $.ajax({
            type: "POST",
            url: "/api/public/Logout",
            contentType: "application/x-www-form-urlencoded",
            dataType: "json",
            async:true,
            success : function(resp) {

                if(resp.success == true) {
                    eraseCookie("isLoggedIn");
                    eraseCookie("isBeforeLoggedIn");
                    $('.productBoxTopSellerSp-mbl').find('a.whishListIcon').removeClass('active').attr('data-customerWishListID',0).attr('data-customerWishListProductID',0).attr('data-customerWishListName','');
                    swapForm("");
                    $('.dropdownBox').removeClass('dropdownAnmeldenBox dropdownMerklisteBox');
                    $('.overlayDropdown').hide();
                    $('body').removeClass('dropdown-hidden');
                    $('.header-menus').removeClass('open');
                    $(window).resize();
                    isLoggedIn();
                    if(isExpire === false){
                        showAlert(dynamicVariables['wishList.logout'], 'success');
                    }
                }
                else {
                    if(isExpire === false){
                        showAlert(dynamicVariables['wishList.invalidUserNamePassword'], 'danger');
                    }
                    $("#forgotPasswordBtn").removeClass("hide");
                }
            },
            error : function (resp)
            {
                if(!apiCall) logout(isExpire,apiCall--);
                $('.merklisteDelSec, .gesehenDelSec').removeClass('loading-state');
                showLoading(false);
            }
        });
    }

    function isLoggedIn() {
        $('.overlayDropdown').hide();
        $('.dropdownBox').removeClass('dropdownAnmeldenBox dropdownMerklisteBox');
        if (readCookie("isLoggedIn") == 1) {
            swapLogo("loggedIn");
            retrieveProfile(1);
        }
        else {
            $(".profilePhoto").text("&nbsp;");
            $(".profileName").text("Anmelden");
            swapLogo("loggedOut");
        }
    }

    function retrieveProfile(apiCall) {
        if(readCookie("isLoggedIn") != 1) {
            swapForm("login");
            swapLogo("loggedOut");
            return;
        }

        var result = "";

        $.ajax({
            type : "GET",
            url : "/api/public/AccountAddresses",
            dataType : "json",
            async:true,
            success : function (resp)
            {
                if (resp.success == true) {

                    result = resp.jsonResult;
                    var profilePhoto =  result.city;
                    var firstName   =  result.firstName;
                    var lastName    =  result.lastName;
                    var initails    =  result.userName.substring(0, 2);

                    //$('.profilePhoto').text(initails).removeClass('profile-pic');
                    $('.profilePhoto').html('<img src="'+dynamicVariables['images_root_path']+'/userIcon.svg" alt="User Login">').removeClass('profile-pic').addClass('profile-user-icon');
                    if(profilePhoto != "" && typeof profilePhoto != 'undefined' && profilePhoto != null && profilePhoto != "(NULL)")
                    {
                        $('.profilePhoto').removeClass('profile-user-icon').addClass('profile-pic').html('<img src="'+profilePhoto+'" title="Profile Photo" alt="'+firstName+'"/>');
                    }
                    else if(firstName != "" && firstName != "(FirstName)" && lastName != "" && lastName != "(LastName)")
                    {
                        initails = (firstName.substring(0, 1)+''+lastName.substring(0, 1));
                        $('.profilePhoto').text(initails).removeClass('profile-pic profile-user-icon');
                    }

                    $(".profileName").text("");$("#profileFirstName").val('');$("#profileLastName").val('');
                    if(firstName != "" && firstName != "(FirstName)" && lastName != "" && lastName != "(LastName)"){
                        $(".profileName").text(firstName + " " + lastName);
                        $("#profileFirstName").val(firstName);
                        $("#profileLastName").val(lastName);
                    }
                    $("#profileUserName").val(result.userName);

                    if(result.address1 == 1) {
                        $("#profileNewsLetter").prop("checked", true)
                    }

                    if(result.address2 == 1) {
                        $("#gender-male").prop("checked", true);
                    } else {
                        $("#gender-female").prop("checked", true);
                    }
                    wishListsMark(1);
                }else{
                    logout(true,1);
                }
            },
            error : function (resp)
            {
                if(!apiCall) retrieveProfile(apiCall--);
                else{
                    $('.merklisteDelSec, .gesehenDelSec').removeClass('loading-state');
                    showLoading(false);
                    showAlert(dynamicVariables['apiResponseError'], 'danger');
                    eraseCookie("isLoggedIn");
                }
            }
        });

        return result;
    }

