    function checkLoginState() {
        FB.login(function(response) {
            var authResponse = response.authResponse;
            if (authResponse && typeof authResponse != 'undefined') {
                FB.api('/me?fields=id,email,first_name,last_name', function(response) {
                    signup(response,1);
                });
            }
