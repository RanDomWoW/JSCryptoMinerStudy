    function afterLoginOrSingUp(resp,logged_in_type){
        if (resp == true) {
            dataLayer.push({
                'event': 'logged_in',
                'action_type': logged_in_type
            });
        }
        createCookie("isLoggedIn", "1", cookie_lifetime_validity);
        swapForm("");
        $("#forgotPasswordBtn").addClass("hide");
        $('.dropdownBox').removeClass('dropdownAnmeldenBox dropdownMerklisteBox');
        $('.overlayDropdown').hide();
        $('body').removeClass('dropdown-hidden');
        $('.header-menus').removeClass('open');
        isBeforeLoginSingUpAddWishList();
        isLoggedIn();
    }

    function isBeforeLoginSingUpAddWishList(){
        var isBeforeLoggedInAddWishList = readCookie("isBeforeLoggedIn");
        if(isBeforeLoggedInAddWishList != "" && isBeforeLoggedInAddWishList != null && typeof isBeforeLoggedInAddWishList != 'undefined'){
            isBeforeLoggedInAddWishList = (isBeforeLoggedInAddWishList).split(',');
            $.each(isBeforeLoggedInAddWishList,function(key,isBeforeProductCode){
                wishListAddProduct(0, isBeforeProductCode,'Wish List',0,1);
            });
            eraseCookie("isBeforeLoggedIn");
        }
    }

    function addRemoveWishList(productCode){
        swapForm("");
        if(readCookie("isLoggedIn") == 1) {
            $('.dropdownBox').removeClass('dropdownMerklisteBox dropdownAnmeldenBox');
            $('.header-menus').removeClass('open');
            $('.overlayDropdown').hide();
            $('body').removeClass('dropdown-hidden');
            var activeMark                  = $('.productBoxTopSellerSp-mbl').find('a#'+productCode).hasClass('active');
            var customerWishListID          = $('.productBoxTopSellerSp-mbl').find('a#'+productCode).attr('data-customerWishListID');
            var customerWishListProductID   = $('.productBoxTopSellerSp-mbl').find('a#'+productCode).attr('data-customerWishListProductID');
            var customerWishListName        = $('.productBoxTopSellerSp-mbl').find('a#'+productCode).attr('data-customerWishListName');
            var wishListProductsCount       = parseInt($('.productBoxTopSellerSp-mbl').find('a#'+productCode).attr('data-wishListProductsCount'));
            var maxProductsPerWishLists     = parseInt($('.productBoxTopSellerSp-mbl').find('a#'+productCode).attr('data-maxProductsPerWishLists'));
            if (activeMark === false) {
                if(wishListProductsCount < maxProductsPerWishLists){
                    wishListAddProduct(customerWishListID, productCode,customerWishListName,wishListProductsCount,1);
                }else{
                    $('.productBoxTopSellerSp-mbl').find('a#'+productCode).removeClass('active');
                    showAlert(dynamicVariables['wishList.maxProductsPerWishLists'], 'danger');
                }
            }
        }
        else {
            swapForm('login');
            $('.header-menus').removeClass('open');
            $('.overlayDropdown').show();
            $('body').addClass('dropdown-hidden');
            if(readCookie("isBeforeLoggedIn") != null && typeof readCookie("isBeforeLoggedIn") != 'undefined'){
                productCode += ','+readCookie("isBeforeLoggedIn");
            }
            createCookie("isBeforeLoggedIn", productCode, cookie_lifetime_validity);
            $('.productBoxTopSellerSp-mbl').find('a#'+productCode).addClass('before-login');
        }
    }

    function wishListAddProduct(customerWishListID,productCode,customerWishListName,wishListProductsCount,apiCall){
        if (typeof wishListProductsCount === 'undefined') wishListProductsCount = 0;
        $('.productBoxTopSellerSp-mbl').find('a.whishListIcon').attr('data-wishListProductsCount',(wishListProductsCount + 1));
        $('.productBoxTopSellerSp-mbl').find('a#'+productCode).addClass('active');
        $('li.sec-wishlist').addClass('fillActive');
        $('li.sec-wishlist').find('a > p > i').prepend( "<em class='dotAlert'></em>");
        $.ajax({
            type: "POST",
            url: "/api/public/CSRFToken",
            dataType: "json",
            async:true,
            success : function(json) {
                var csrfToken = json.jsonResult;
                $.ajax({
                    type: "POST",
                    url: "/api/public/WishListAddProduct",
                    data : {
                        "customerWishListID" : customerWishListID,
                        "customerWishListName" : customerWishListName,
                        "productCode" : productCode,
                        "csrfToken" : csrfToken
                    },
                    async:true,
                    contentType: "application/x-www-form-urlencoded",
                    dataType: "json",
                    success: function(resp){
                        if(resp.success == true) {
                            dataLayer.push({
                                'event': 'wishlist',
                                'action_type': 'add'
                            });
                        }
                    },
                    error : function(json){
                        $('.merklisteDelSec, .gesehenDelSec').removeClass('loading-state');
                        showLoading(false);
                        showAlert(dynamicVariables['apiResponseError'], 'danger');
                        console.log(json);
                    }
                });
            },
            error : function(json){
                $('.productBoxTopSellerSp-mbl').find('a.whishListIcon').attr('data-wishListProductsCount',(wishListProductsCount - 1));
                if(!apiCall) wishListAddProduct(customerWishListID,productCode,customerWishListName,wishListProductsCount,apiCall--);
                else{
                    $('.merklisteDelSec, .gesehenDelSec').removeClass('loading-state');
                    showLoading(false);
                    showAlert(dynamicVariables['apiResponseError'], 'danger');
                    console.log(json);
                }
            }
        });
        setTimeout(function () {
            $('li.sec-wishlist').find('a > p > i > em.dotAlert').remove();
        }, 2000);
    }

    function retrieveWishLists(apiCall) {
        $('#wish-list-data, #last-visited-data').html('');
        $('.merklisteDelSec, .gesehenDelSec').removeClass('blankList').addClass('loading-state');
        $.ajax({
            type: "GET",
            url: "/api/public/AccountWishLists",
            dataType: "json",
            async:true,
            success : function(json) {
                if(json.success == true) {
                    if (json.jsonResult.customerWishLists) {
                        $('.merklisteDelSec, .gesehenDelSec').removeClass('loading-state');
                        var wishListsHtml = "";
                        var last_visited = "";
                        var wish_list = "";
                        if(json.jsonResult.customerWishLists.length > 0){
                            for (var i = 0; i < json.jsonResult.customerWishLists.length; i++) {
                                var wishList = json.jsonResult.customerWishLists[i];
                                var customerWishListID      = wishList.customerWishListID;
                                var customerWishListName    = wishList.name;
                                if (wishList.customerWishListProducts) {
                                    var wishListProductsCountDelete = 0;
                                    if(customerWishListName == 'Wish List'){
                                        wishListProductsCountDelete =  parseInt(wishList.customerWishListProducts.length);
                                        $('.productBoxTopSellerSp-mbl').find('a.whishListIcon').attr('data-wishListProductsCount',wishList.customerWishListProducts.length);
                                    }
                                    for (var j = 0; j < wishList.customerWishListProducts.length; j++) {
                                        var wishListProduct             = wishList.customerWishListProducts[j];
                                        var customerWishListProductID   = wishListProduct.customerWishListProductID;

                                        var productCode                 = wishListProduct.product.code;
                                        var productName                 = wishListProduct.product.name;
                                        var productUnitPrice            = wishListProduct.product.unitPrice;
                                        var productAltPrice             = wishListProduct.product.altPrice;
                                        var productUnitPriceFinal       = wishListProduct.product.productUnitPriceFinal;

                                        //Delivery Time
                                        var productDeliveryTruckColor   = 'green';
                                        var productDeliveryTime         = wishListProduct.product.deliveryTime;
                                        if(productDeliveryTime > 'unter 1 Woche'){
                                            productDeliveryTruckColor   = 'yellow';
                                        }

                                        //Shipping Cost and Shipment Text
                                        var shipmentText = '';
                                        var productShippingCost         = wishListProduct.product.shippingCost;
                                        var shippingCostShipmentText    = wishListProduct.product.shippingCostText;

                                        //Product Rating
                                        var productNumberRating     = wishListProduct.product.numberRating;
                                        var productAverageRating    = wishListProduct.product.averageRating;
                                        var productRatingSection    = '';
                                        if(productAverageRating > 3){
                                            var ratingReviewStars   = (productAverageRating/5*100);
                                            productRatingSection = '<div class="ratingSecPreisvergleich-pdp"><p><span><i style="width:'+ratingReviewStars+'% !important"></i></span>('+productNumberRating+dynamicVariables['pdp.ratingText']+')</p></div>';
                                        }

                                        var productUpdated      = wishListProduct.product.updated;

                                        //Advertiser
                                        var productAdvertiserCode       = wishListProduct.product.advertiser.code;
                                        var productAdvertiserName       = wishListProduct.product.advertiser.name;
                                        var productAdvertiserLogo       = 'https://assets.moebel24.de/images/shoplogos/88/'+productAdvertiserCode+'.png';
                                        var advertiserRatingSection     = '';
                                        var advertiserShopRating        = wishListProduct.product.advertiser.shopRating;
                                        if(typeof advertiserShopRating != 'undefined' && advertiserShopRating != ''){
                                            advertiserShopRating        = advertiserShopRating.split('/');
                                            var advertiserObtainReviews     = (advertiserShopRating[0]).replace(',', '.');
