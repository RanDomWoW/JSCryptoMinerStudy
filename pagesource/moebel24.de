        else{
            closeAlert();
            $( ".allert" ).remove();

            var userName = $("#profileUserName").val();
            var password = $("#profilePassword").val();

            var DATA = {};

            DATA['userName'] = userName;
            DATA['decryptedPassword'] = password;
            DATA['confirmPassword'] = password;

            var csrfToken = getCsrfToken();

            if(csrfToken == '') {
                return false;
            }

            DATA['csrfToken'] = csrfToken;

            $.ajax({
                type : "POST",
                url : "/api/public/UpdatePassword",
                data : DATA,
                contentType : "application/x-www-form-urlencoded",
                dataType : "json",
                async:true,
                success : function (response)
                {
                    if (response.success == true) {
                        showAlert(dynamicVariables['wishList.userNamePasswordUpdated'], 'success');
                    }
                    else{
                        showAlert(dynamicVariables['wishList.emailExists'],'danger');
                        return false;
                    }
                },
                error : function (resp)
                {
                    showAlert(dynamicVariables['wishList.emailExists'],'danger');
                    return false;
                }
            });
        }
    }

