    function setFieldActive(param) {

        var check = $("#" + param + "Hidden").val();
        if (check == 1) {
            $("#" + param).prop('disabled', true);
            $("#" + param + "Hidden").val(0);
        }
        else {
            $("#" + param).prop('disabled', false);
            $("#" + param + "Hidden").val(1);
        }
    }

