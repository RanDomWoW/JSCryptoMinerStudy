    function showAlert(msgText, type) {

        $("#alert-notification").html(msgText).parent().removeClass("alert-success alert-warning alert-info alert-danger").addClass("alert-"+type).show();

        setTimeout(function(){
            closeAlert();
        },2000);

    }

    function closeAlert() {
        $('#alert-msg').fadeOut();
    }

    function getCsrfToken() {

        var csrfToken = '';
        $.ajax({
            type : "POST",
            url : "/api/public/CSRFToken",
            dataType : "json",
            async: false,
            success : function (resp) {
                if(resp.success) {
                    csrfToken = resp.jsonResult
                } else {
                    eraseCookie("isLoggedIn");
                    swapForm("login");
                }
            },
            error: function (resp) {
                $('.merklisteDelSec, .gesehenDelSec').removeClass('loading-state');
                showLoading(false);
                showAlert(dynamicVariables['apiResponseError'], 'danger');
                console.log(resp);
            }
        });

        return csrfToken;
    }

    function enableProfileButton(e){
        $('.profile-submit').prop("disabled", false);
    }

    function disableProfileButton(e){
        $('.profile-submit').prop("disabled", true);
    }

    function showLoading(flag) {
        if (flag)
            $('.loginPopupSec-hd .startup-loading').show();
        else
            $('.loginPopupSec-hd .startup-loading').hide();
    }
