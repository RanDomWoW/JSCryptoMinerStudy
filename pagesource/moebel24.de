                    $('body').removeClass('dropdown-hidden');
                    $('.header-menus').removeClass('open');
                });

                $(document).on('click',"body,html" ,function(event){
                    if (!$(event.target).closest('.wishListSec-hd').length && !$(event.target).closest('.wishlistTrigger').length && !$(event.target).closest('.wishlistDropdown').length && !$(event.target).closest('.regFormSec-inner').length && !$(event.target).closest('.alert').length) {
                        $('.dropdownBox').removeClass('dropdownMerklisteBox dropdownAnmeldenBox');
                        $('.overlayDropdown').hide();
                        $('body').removeClass('dropdown-hidden');
                        $('.header-menus').removeClass('open');
                    }
                });
            }

            if($(window).width() > 767) {
                $(document).on('click',"body,html" ,function(event){
                    if(!$(event.target).closest('.dropdownBoxSupporter').length){
                        $('body').removeClass('dropdown-hidden');
                    }

                    if($(document).find('li.secRemove').hasClass('active') === true && !$(event.target).closest('.dropdownSpSec').length && !$(event.target).closest('.dropdownAnmeldenTrigger').length && !$(event.target).closest('.dropdownMerklisteTrigger ').length  && !$(event.target).closest('.alert').length){
                        swapForm('');
                        $('.dropdownBox').removeClass('dropdownAnmeldenBox dropdownMerklisteBox');
                        $('.overlayDropdown').hide();
                        $('.header-menus').removeClass('open');
                    }else if(!$(event.target).closest('.dropdownSpSec').length){
                        $('.dropdownBox').removeClass('dropdownAnmeldenBox dropdownMerklisteBox');
                        $('.overlayDropdown').hide();
                        $('.header-menus').removeClass('open');
                    }
                });
            }

            resizeFunction();

            $(window).resize(function(){
                if($(window).width() < 768) {
                    $(document).on('click','.concludeBtn',function(){
                        var this_ele = $(this);
                        $('.dropdownBox').removeClass('dropdownAnmeldenBox dropdownMerklisteBox');
                        $('.overlayDropdown').hide();
                        $('body').removeClass('dropdown-hidden');
                        $('.header-menus').removeClass('open');
                    });

                    $(document).on('click',"body,html" ,function(event){
                        if (!$(event.target).closest('.wishListSec-hd').length && !$(event.target).closest('.wishlistTrigger').length  && !$(event.target).closest('.wishlistDropdown').length && !$(event.target).closest('.regFormSec-inner').length && !$(event.target).closest('.alert').length) {
                            $('.dropdownBox').removeClass('dropdownMerklisteBox dropdownAnmeldenBox');
                            $('.overlayDropdown').hide();
                            $('body').removeClass('dropdown-hidden');
                            $('.header-menus').removeClass('open');
                        }
                    });
                }
            });

            $(document).on('click','ul li.listRow',function(e){
                if(!$(event.target).closest('.deleteRowTrigger ').length){
                    e.preventDefault();
                    var this_id = $(this).attr('id'); this_href = $('#'+this_id).find('a').attr('href');
                    window.open(
                        this_href,
                        '_blank'
                    );
                }
            });

            if($(window).width() > 767) {
                $(document).on('click',"body,html" ,function(event){
                    if (!$(event.target).closest('.searchBoxCollapse').length) {
                        $('.searchBoxCollapse').removeClass('searchBoxToggle');
                        $('#searchString').removeClass('focusInput').val('');
                        $('#searchSubmit').removeClass('input-focus');
                        $('.header-menus').removeClass('open');
                        if ($(window).width() > 767){
                            if($(window).width() > 767 && $(window).width() < 994) {
                                $('.headerMiddleContentArea-mbl').hide();
                            }else{
                                $('.headerMiddleContentArea-mbl').show();
                            }
                        }
                    }
                });
            }

            $(document).on('click', 'button.searchSubmit', function () {
                var searchString = $('#searchString').val();
                $('.overlayDropdown').hide();
                $('body').removeClass('dropdown-hidden');
                if(searchString != ''){
                    $(this).parents('form').submit();
                    return false;
                }
                if($(this).hasClass( "input-focus" )){
                    $(this).parents('li').removeClass('searchBoxToggle');
                    $(this).removeClass( "input-focus" );
                    $('#searchString').removeClass('focusInput');
                    if($(window).width() > 767){
                        if($(window).width() > 767 && $(window).width() < 994) {
                            $('.headerMiddleContentArea-mbl').hide();
                        }else{
                            $('.headerMiddleContentArea-mbl').show();
                        }
                    }
                } else {
                    $('.headerMiddleContentArea-mbl').hide();
                    $('#searchString').addClass('focusInput');
                    $(this).addClass( "input-focus" );
                }
            });

            $(document).on('click', '#searchString', function () {
                $('.overlayDropdown').hide();
                $('body').removeClass('dropdown-hidden');
                $('.headerMiddleContentArea-mbl').hide();
                $('.searchSubmit').addClass("input-focus");
                $(this).addClass( "focusInput input-focus" );
            });

            $(document).on('keypress', '#searchString', function (e) {
                var searchString = $(this).val();
                if(e.which == 13 && searchString != ''){
                    $('.searchSubmit').click();
                }
            });

            $(document).on('click', '.js-analytics-register', function(e) {
                dataLayer.push({
                    'event': 'registration',
                    'action_type': 'jetzt_registrieren_clicked'
                })
            });

            $(document).on('click', '.js-analytics-fb', function(e) {
                dataLayer.push({
                    'event': 'registration',
                    'action_type': 'FB_anmelden_clicked'
                })
            });

            $(document).on('click', '.js-analytics-login', function(e) {
                dataLayer.push({
                    'event': 'registration',
                    'action_type': 'anmelden_clicked'
                })
            });
        });
    </script>
<section class="overlayDropdown"></section>
</body>
</html>