    function login(userName, password,apiCall) {
        if (typeof userName === 'undefined') userName = "";
        if (typeof password === 'undefined') password = "";

        closeAlert();
        $( ".allert" ).remove();

        var fb = true, logged_in_type = '';
        if(userName == "" && password == "")
        {
            fb = false;
            var userName = $("#loginUserName").val();
            var password = $("#loginPassword").val();
            logged_in_type = 'email';
        }

        if(!validateEmpty(userName)) {
            $("#loginUserName").focus().parent('.input-holder').append('<div class="allert"><span>'+dynamicVariables['wishList.emptyUserName']+'</span><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 286.1 286.1"><path d="M143 0C64 0 0 64 0 143c0 79 64 143 143 143 79 0 143-64 143-143C286.1 64 222 0 143 0zM143 259.2c-64.2 0-116.2-52-116.2-116.2S78.8 26.8 143 26.8s116.2 52 116.2 116.2S207.2 259.2 143 259.2zM143 62.7c-10.2 0-18 5.3-18 14v79.2c0 8.6 7.8 14 18 14 10 0 18-5.6 18-14V76.7C161 68.3 153 62.7 143 62.7zM143 187.7c-9.8 0-17.9 8-17.9 17.9 0 9.8 8 17.8 17.9 17.8s17.8-8 17.8-17.8C160.9 195.7 152.9 187.7 143 187.7z" fill="#E2574C"/></svg></div>');
            return;
        }
        else if(!validateEmail(userName)) {
            $("#loginUserName").focus().parent('.input-holder').append('<div class="allert"><span>'+dynamicVariables['wishList.invalidUserName']+'</span><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 286.1 286.1"><path d="M143 0C64 0 0 64 0 143c0 79 64 143 143 143 79 0 143-64 143-143C286.1 64 222 0 143 0zM143 259.2c-64.2 0-116.2-52-116.2-116.2S78.8 26.8 143 26.8s116.2 52 116.2 116.2S207.2 259.2 143 259.2zM143 62.7c-10.2 0-18 5.3-18 14v79.2c0 8.6 7.8 14 18 14 10 0 18-5.6 18-14V76.7C161 68.3 153 62.7 143 62.7zM143 187.7c-9.8 0-17.9 8-17.9 17.9 0 9.8 8 17.8 17.9 17.8s17.8-8 17.8-17.8C160.9 195.7 152.9 187.7 143 187.7z" fill="#E2574C"/></svg></div>');
            return;
        }
        else if(!validateEmpty(password)) {
            $("#loginPassword").focus().parent('.input-holder').append('<div class="allert"><span>'+dynamicVariables['wishList.emptyPassword']+'</span><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 286.1 286.1"><path d="M143 0C64 0 0 64 0 143c0 79 64 143 143 143 79 0 143-64 143-143C286.1 64 222 0 143 0zM143 259.2c-64.2 0-116.2-52-116.2-116.2S78.8 26.8 143 26.8s116.2 52 116.2 116.2S207.2 259.2 143 259.2zM143 62.7c-10.2 0-18 5.3-18 14v79.2c0 8.6 7.8 14 18 14 10 0 18-5.6 18-14V76.7C161 68.3 153 62.7 143 62.7zM143 187.7c-9.8 0-17.9 8-17.9 17.9 0 9.8 8 17.8 17.9 17.8s17.8-8 17.8-17.8C160.9 195.7 152.9 187.7 143 187.7z" fill="#E2574C"/></svg></div>');
            return;
        }

        showLoading(true);

        var csrfToken = getCsrfToken();

        if(csrfToken == '') {
            return;
        }

        $.ajax({
            type : "POST",
            url : "/api/public/Login",
            data : {
                "userName" : userName,
                "decryptedPassword" : password,
                "csrfToken" : csrfToken
            },
            async:true,
            contentType : "application/x-www-form-urlencoded",
            dataType : "json",
            success : function (resp)
            {
                showLoading(false);
                if (resp.success == true) {
                    if (fb) {
                        logged_in_type = 'fb';
                        showAlert(dynamicVariables['wishList.changePassword'], 'success');
                    }
                    else {
                        showAlert(dynamicVariables['wishList.loginSuccessfully'], 'success');
                    }
                    afterLoginOrSingUp(resp.success,logged_in_type);
                }
                else {
                    showAlert(dynamicVariables['wishList.invalidUserNamePassword'], 'danger');
                    $("#forgotPasswordBtn").removeClass("hide");
                }
            },
            error : function (resp)
            {
