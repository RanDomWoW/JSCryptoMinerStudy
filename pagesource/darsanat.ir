                    if(data.custom_field_status === true) {
                        $("#search-custom-fields").html(data.custom_field_result).slideDown();
                        $('.selectpicker').selectpicker();
                    }
                }
            }
        })
    })

    $("#search-select-subcategory").change(function(e)
    {
        var catId = $("#search-select-category").val();
        var subcatId = $(this).val();

        blockScreen();

        $.ajax({
            url  : "process/pr_ajax_calls.php",
            type : "POST",
            dataType : "JSON",
            data : {
                catId : catId,
                subcatId : subcatId,
                action : "find-subsidiary-for-search"
            },
            success : function(data)
            {
                unblockScreen();

                removeInput('#subsidiary-placeholder', '#search-select-subsidiary');
                removeInput('#search-custom-fields');

                if(data.status === true)
                {
                    showInput('#subsidiary-placeholder', '#search-select-subsidiary', data.result);

                } else {
                    if(data.custom_field_status === true) {
                        $("#search-custom-fields").html(data.custom_field_result).slideDown();
                        $('.selectpicker').selectpicker();
                    }
                }
            }
        })
    })

    $("#search-select-subsidiary").change(function(e)
    {
        var catId = $("#search-select-category").val();
        var subcatId = $("#search-select-subcategory").val();
        var subsidiaryId = $(this).val();

        blockScreen();

        $.ajax({
            url  : "process/pr_ajax_calls.php",
            type : "POST",
            dataType : "JSON",
            data : {
                catId : catId,
                subcatId : subcatId,
                subsidiaryId : subsidiaryId,
                action : "find-final-result-for-search"
            },
            success : function(data)
            {
                unblockScreen();
                removeInput('#search-custom-fields');

                if(data.custom_field_status === true) {
                    $("#search-custom-fields").html(data.custom_field_result).slideDown();
                    $('.selectpicker').selectpicker();
                }
            }
        })
    });

    price_field_remover('price');
    price_field_remover('mortgage');
    price_field_remover('rent');

</script>

</body>
</html>