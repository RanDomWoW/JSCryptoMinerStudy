    <script type="text/javascript">
        
        function NewslettersignupFooter() {
            try {
                var email = $.trim($("#signUpEmailAddress").val());
                var firstName = $.trim($("#signUpFirstName").val());
                var lastName = $.trim($("#signUpLastName").val());
                $('#imgSubscribeLoadingFooter').show();
                signUpForNewsLetter(firstName, lastName, email);
            }
            catch(Exception)
            {
                $('#imgSubscribeLoadingFooter').hide();
                bootbox.alert('Sorry, Newsletter subscription is not successful.');
            }
            return false;
        }

        function signUpForNewsLetter(firstName, lastName, email) {

            var errorMessage = '';
            if (email == "") {
                errorMessage += '\nPlease enter email address.';
            }
            else {
                var regularExpression = /[\w-]+@([\w-]+\.)+[\w-]+/
                if (email.search(regularExpression) == -1) {
                    errorMessage += '\nPlease enter email address in correct format.';
                }
            }
            if (firstName == "") {
                errorMessage += '\nPlease enter first name.';
            }
            if (lastName == "") {
                errorMessage += '\nPlease enter last name.';
            }

            if (errorMessage.length == 0) {
                var dataArray = {
                    emailAddress: email,
                    firstName: firstName,
                    lastName: lastName
                };
               
                $.ajax({
                    type: "POST",
                    url: '/home/NewsletterSignUp',
                    data: dataArray,
                    success: function (response) {
                        if (response == true)
                            bootbox.alert('Newsletter subscription successful.');
                        else
                            bootbox.alert('Sorry, Newsletter subscription is not successful.');
                        $('.imgSubscribeLoading').hide();
                    },
                    error: function () {
                        bootbox.alert('Sorry, Newsletter subscription is not successful.');
                        $('.imgSubscribeLoading').hide();
                    }
                });
            }
            else {
                $('.imgSubscribeLoading').hide();
                alert(errorMessage);                
            }
        }

</script>
    



 
