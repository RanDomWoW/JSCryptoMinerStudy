<script>

    function closeFacebookOverlay() {

        $(".sidemenu").attr("style", "margin-top:0 !important;");
        $("#centercontent").attr("style", "margin-top:0 !important;");
        $('.facebookLinkAccountInfoContainer').hide();
        fixHeaderHeight();
    }

    function unbindUserInfoBar() {
        $('#loginForm').unbind('submit');
        $('#loginLink').unbind('click');
        $('#myNotifications10').unbind('click');
        $('#myAccess').unbind('click');
        $(document).unbind('click');
    }
    var searchTimeout = null;
    var searchSuggestionRequest = null;
    var searchQuery = '';
    var selectedIndex = -1;

    $(document).ready(function () {

        var originalSearchCategory = $('#searchCategories').html();
        var originalSearchCategoryId = $('#searchByCategoryId').val();

        $('#inputSearch10, #btnSearch10').bind('keyup', function (evt) {
            if (evt.keyCode != 13) {
                if (evt.keyCode == 40) {

                    var results = $('.result');
                    if (results.length > 0) {
                        $li = $(results[selectedIndex]);
                        if (selectedIndex >= 0) {
                            $li.removeClass('resultSel');
                        }
                        if (results.length > selectedIndex + 1) {
                            selectedIndex++;
                            $li = $(results[selectedIndex]);
                            $li.addClass('resultSel');


                            if ($li.data('categoryid') !== undefined) {
                                $('#searchByCategoryId').val($li.data('categoryid'));
                            } else {
                                $('#searchByCategoryId').val(0);
                            }
                        }
                    }
                } else if (evt.keyCode == 38) {
                    var results = $('.result');
                    if (results.length > 0) {
                        var $li = $(results[selectedIndex]);
                        if (selectedIndex >= 0) {
                            $li.removeClass('resultSel');
                        }
                        if (selectedIndex > 0) {
                            selectedIndex--;
                            $li = $(results[selectedIndex]);
                            $li.addClass('resultSel');

                            if ($li.data('categoryid') !== undefined) {
                                $('#searchByCategoryId').val($li.data('categoryid'));
                            } else {
                                $('#searchByCategoryId').val(0);
                            }

                        }
                    }
                } else {

                    var q = $.trim($('#inputSearch10').val());
                    if (q.length > 2) {
                        if (searchTimeout != null) clearTimeout(searchTimeout);

                        searchTimeout = setTimeout(doSearch, 350);
                    } else {
                        $('#searchSuggestions10').hide();
                        if ($('#searchPanel').length != 0 && q.length == 0) {
                            searchTimeout = setTimeout(function () {
                                var url = '/classified/default.aspx';
                                if (typeof getUrl === 'function') url = getUrl();

                                $.get(url, function (data) {
                                    if (history.pushState && (typeof disablePush == 'undefined' || !disablePush)) history.pushState({ load: true }, '', '/classified/default.aspx');
                                    $('#searchPanel').replaceWith(data);
                                });
                            }, 250);
                        }

                    }
                }
            } else if (selectedIndex >= 0 && $('.result').length > selectedIndex) {
                evt.preventDefault();


                $('.result').unbind('hover');

                var results = $('.result');
                var $li = $(results[selectedIndex]);
                var url = $li.data('url');

                selectedIndex = -1;
                if (url.indexOf('http') != -1) {
                    location.href = url;
                } else {

                    if ($li.data('categoryid') !== undefined) {
                        originalSearchCategory = $li.data('name');
                        originalSearchCategoryId = $li.data('categoryid');
                        $('#searchCategories').html($li.data('name'));
                        $('#searchByCategoryId').val($li.data('categoryid'));
                        recalcCategories();

                        url += '&categoryId=' + originalSearchCategoryId;
                    }

                    if (history.pushState && (typeof disablePush == 'undefined' || !disablePush)) {
                        $('#inputSearch10').val($li.data('value'));
                        loadContent(null, url, true);
                    } else {
                        location.href = url;
                    }
                }
            }


        });

        function doSearch() {
            var searchInput = $('#inputSearch10');
            var q = $('#inputSearch10').val();

            var searchIn = searchInput.data('searchIn');
            if (typeof searchIn === 'undefined') {
                searchIn = 1;
            }
            if (searchIn == 3) {
                searchConversation();
            }

            if (searchIn == 1 || searchIn == 4) {
                //
                if (searchSuggestionRequest != null && typeof searchSuggestionRequest.abort !== 'undefined') searchSuggestionRequest.abort();
                if (searchQuery != q && q.length > 2) {
                    searchQuery = q;
                    if (searchTimeout != null) clearTimeout(searchTimeout);


                    $.get('/search/suggestions.aspx', { q: q, searchIn: searchIn }, function (data) {
                        $('#searchSuggestions10').html(data);
                        $('#searchSuggestions10').show();
                        selectedIndex = -1;
                        $(document).bind('click.searchSuggestion', function () {
                            $('#searchSuggestions10').hide();
                            $(document).unbind('click.searchSuggestion');
                            searchQuery = null;
                        });

                        var originalSearchCategory = $('#searchCategories').html();
                        var originalSearchCategoryId = $('#searchByCategoryId').val();


                        $('.result').hover(function (evt) {
                            if (selectedIndex != -1) {
                                $($('.result')[selectedIndex]).removeClass('resultSel');
                            }
                            selectedIndex = $('.result').index($(this));

                        }, function (evt) {
                            $('#searchCategories').html(originalSearchCategory);
                            $('#searchByCategoryId').val(originalSearchCategoryId);
                            recalcCategories();
                        });

                        $('.result').click(function (evt) {
                            var url = $(this).data('url');
                            if (url.indexOf('http') != -1) {
                                location.href = url;
                            } else {
                                $('.result').unbind('hover');
                                if ($(this).data('categoryid') !== undefined) {
                                    originalSearchCategory = $(this).data('name');
                                    originalSearchCategoryId = $(this).data('categoryid');
                                    $('#searchCategories').html($(this).data('name'));
                                    $('#searchByCategoryId').val($(this).data('categoryid'));
                                    recalcCategories();
                                    url += '&categoryId=' + originalSearchCategoryId;
                                }

                                if (history.pushState && (typeof disablePush == 'undefined' || !disablePush)) {
                                    $('#inputSearch10').val($(this).data('value'));
                                    loadContent(null, url, true);
                                } else {
                                    location.href = url;
                                }
                            }
                        });

                    });
                }
            }
        }

        $('#loginForm').bind('submit', function (evt) {
            evt.preventDefault();
            $('#loginButton').prop('disabled', true);
            $('.loadingLogin').show();
            $('#loginForm > .error').hide();
            $.post('/website/api/graphapi/me/login.aspx', { username: $('#loginUsername').val(), password: $('#loginPassword').val(), remember: ($('#loginMuna:checked').length > 0) }, function (data) {

                $('.loadingLogin').hide();
                if (!data.success) {

                    $('#loginButton').removeAttr('disabled');
                    $('#loginButton').val('Innskrá');
                    $('#loginForm > .error').html(data.message).show();

                    if (data.errorCount == 6 || data.errorCount == 8 || data.errorCount > 10) {
                        $('.loadingLoginDisabled').addClass('loadingLogin').removeClass('loadingLoginDisabled');
                        $('#popup').popup({ url: '/pers/loginwarning.aspx', modal: true });
                    }
                } else {
                    $('#loginForm > .error').hide();
                    var url = location.href;
                    if (url.indexOf('mobilesuggestion.aspx') != -1) url = '/';

                    location.href = url;
                }
            }).fail(function (data) {
                $('#loginButton').removeAttr('disabled');
            });
        });

        $('#loginLink').click(function (e) {
            e.preventDefault();
            e.stopPropagation();

            if ($('.loginForm').css('display') == 'none') {
                $('#loginLinkLi').addClass('loginLinkLiSelected');

                $('.loginForm').fadeIn();
                $('#loginUsername').focus();

            } else {
                $('#loginLinkLi').removeClass('loginLinkLiSelected');
                $('.loginForm').hide();
                $(document).unbind('click.hideLogin');
                //addOverflow();
            }
        });

        $('#myNotifications10').click(function (e) {
            if (hasValue(e.target.nodeName) && e.target.nodeName != null && e.target.nodeName.toLowerCase() != 'a' && e.target.nodeName.toLowerCase() != 'strong') {
                e.preventDefault();
                e.stopPropagation();

                $('#myNotifications10 a i').css('color', '#333');
                $('#notifications10').fadeIn('fast');
                $('.overlay').show();

                $('.overlay').bind('click', function (event) {
                    if (!$(event.target).hasClass('form_wrapper2')) {
                        $("#notifications10").hide();
                        $('#myNotifications10 a i').css('color', '#555');
                        $('ul.form_wrapper2 .unread').removeClass('unread');
                        $('.overlay').unbind('click', this);
                        $('.overlay').hide();
                    }
                });

                var lastDateTime = $('#notifications10 ul li')[0].getAttribute('datetime');
                $.post('/pers/markNotificationRead.aspx', { lastDateTime: lastDateTime }, function (obj) {
                    if (obj.success) {
                        $('#unreadNotificationCount').html('0').hide();

                        setWebsiteTitle(0, 2);
                    }
                });
            }
        });

        $('#myAccess').click(function (e) {
            var tagName = '';
            if (e.target.nodeName != null) {
                tagName = e.target.nodeName.toLowerCase();
            }
            if (tagName != 'a' && tagName != 'span' && tagName != 'p') {
                e.preventDefault();
                e.stopPropagation();
                $('#arrowDownList').css({ 'color': '#717171', backgroundColor: '#FFF', border: '1px solid #D4D4D4', borderBottom: 'none', position: 'relative' });

                $('.overlay').bind('click', function (event) {
                    if (!$(event.target).hasClass('form_wrapper') && event.target.id != 'myAccess' && event.target.id != 'arrowDownList') {
                        $("#myAccessOptions10").hide();
                        $('#arrowDownList').css({ backgroundColor: '', color: '#717171', 'border-color': 'transparent' })
                        $('#myAccess').removeClass('myAccessLiSelected');
                        $('.overlay').unbind('click', this);
                        $('.overlay').hide();
                    }
                });
                $('#myAccessOptions10').show();
                $('.overlay').show();
            }
        });

        $('#myAccessOptions10 li').click(function () {
            $("#myAccessOptions10").hide();
            $('#arrowDownList').css({ backgroundColor: '', color: '#717171', 'border-color': 'transparent' })
            $('#myAccess').removeClass('myAccessLiSelected');
            $('.overlay').unbind('click', this);
            $('.overlay').hide();
        });

        $('#searchCategories').click(function (e) {

            if ($('.searchCategories').css('display') != 'block') {

                $('.searchCategories').show();
                $('.overlay').show();
                $('.overlay').bind('click', function (event) {
                    if (!$(event.target).hasClass('searchCategories')) {
                        $('.searchCategories').hide();
                        $('.overlay').unbind('click', this).hide();
                    }
                });
            } else {
                $('.searchCategories').hide();
                $('.overlay').unbind('click', this).hide();
            }

        });
        $('.select_category_search').click(function () {
            $('input[name="categoryId"]').val($(this).attr('data-id'));
            $('#searchCategories').html($(this).attr('data-text'));
            recalcCategories();
            $('.searchCategories').hide();
        });

        $('#search10').bind('submit', function (evt) {
            evt.preventDefault();

            var searchInput = $('#inputSearch10');
            var searchIn = searchInput.data('searchIn');
            if (typeof searchIn === 'undefined') {
                searchIn = 1;
            }
            var q = encodeURIComponent($.trim(searchInput.val()));
            var url = '/classified/default.aspx'

            if (searchIn == 2) {
                url = '';
                location.href = 'https://www.google.com/search?q=' + q + '+site:bland.is%2Fmessageboard';
            } else if (searchIn == 3) {
                url = '';
            } else if (searchIn == 4) {
                url = '/barnaland/websites.aspx'
            } else {
                if (searchTimeout != null) clearTimeout(searchTimeout);
                $('#searchSuggestions10').hide();

                var sep = '?';
                if (q.length > 0) {
                    url += sep + 'q=' + q;
                    sep = '&';
                }
                if (parseInt($('input[name="categoryId"]').val()) > 0) {
                    url += sep + 'categoryId=' + $('input[name="categoryId"]').val();
                }
                goTo(url);
            }

        });

    });

    function recalcCategories() {
        var totalWidth = $('.searchBar').css('width').replace('px', '');
        var categoryWidth = ($('#searchCategories').css('display') == 'none') ? 0 : $('#searchCategories').css('width').replace('px', '');
        var inputWidth = $('#inputSearch10').css('width').replace('px', '');

        var btnWidth = $('#btnSearch10').css('width').replace('px', '');

        var newInputWidth = totalWidth - categoryWidth - btnWidth - 59;

        $('#inputSearch10').css('width', newInputWidth);
    }

    $(window).resize(function () {
        recalcCategories();
    });
    var resetSearchBar = null;
    var prevScrollPosition = 0;
    var scrollStep = 0;
    var headerTimer = null;

    

    var headerHeight = $('.header').height();

    $(window).scroll(function (evt) {
        var currentScroll = $(document).scrollTop();

        if (currentScroll < headerHeight && currentScroll < prevScrollPosition) {
            $('.header').removeClass('fixedHeader').removeClass('backColorHeader');
            $('.searchBarNewButton').removeClass('zeroMarginTop');
            if (typeof dfp !== "undefined") {
                dfp.showOrHideBigBanner();
            }
            $('.boughtsmasgroup').css('top', '');
        }

        if (currentScroll >= headerHeight && currentScroll < prevScrollPosition && $('.header').css('position') != 'fixed') {
            setSearchBarAtTop();
            setResetSearchBarTimer();
        } else if (currentScroll <= 10) {
            resetSearchBarFunc();
        } else if (currentScroll > prevScrollPosition) {
            resetSearchBarFunc();
        }

        if (currentScroll > 432) {
            $("#advImg6Warp").removeClass("advImg6WarpTop").addClass("advImg6WarpSmallTop");
        } else {
            $("#advImg6Warp").removeClass("advImg6WarpSmallTop");
        }

        prevScrollPosition = currentScroll;
    });

    

    function setSearchBarAtTop() {
        $('.header').removeClass('resetFixedHeader');
        $('.header').addClass('backColorHeader').addClass('fixedHeader');
        $('.searchBarNewButton').addClass('zeroMarginTop');
        $('#advImg18').hide();
        $('.boughtsmasgroup').css('top', '132px');
    }

    function setResetSearchBarTimer() {
        resetSearchBar = setTimeout(function () {
            resetSearchBarFunc();
        }, 2000);
    }
    var hasEvent = false;

    function doAnimateEnd(evt) {
        if (evt.originalEvent.animationName == 'slideup') {
            var header = $('.header');
            header.removeClass('fixedHeader');
            header.removeClass('backColorHeader');
            header.removeClass('resetFixedHeader');
            $('.searchBarNewButton').removeClass('zeroMarginTop');
            if (typeof dfp !== "undefined") {
                dfp.showOrHideBigBanner();
            }
            hasEvent = false;
            header.unbind('animationend webkitAnimationEnd MSAnimationEnd oAnimationEnd');
            $('.boughtsmasgroup').css('top', '');
        }

    }

    function resetSearchBarFunc() {
        if (animationSupported) {
            var header = $(".header");
            if (header.length > 0 && !hasEvent && header.css('position') == 'fixed') {
                hasEvent = true;
                header.bind("animationend webkitAnimationEnd MSAnimationEnd oAnimationEnd", function (evt) {
                    doAnimateEnd(evt);
                });
                header.addClass('resetFixedHeader');
            }
        } else {
            $('.header').removeClass('fixedHeader').removeClass('backColorHeader');
            $('.searchBarNewButton').removeClass('zeroMarginTop');
            if (typeof dfp !== "undefined") {
                dfp.showOrHideBigBanner();
            }
            $('.boughtsmasgroup').css('top', '');
        }
        clearTimeout(resetSearchBar);
    }

    var hasClickOnSearchbar = false;
    $('.header').click(function () {
        clearTimeout(resetSearchBar);
        hasClickOnSearchbar = true;
    });
    $('.header').blur(function () {
        hasClickOnSearchbar = false;
        setResetSearchBarTimer();
    });
    $('.header').mouseover(function () {
        clearTimeout(resetSearchBar);
    }).mouseleave(function () {
        if (!hasClickOnSearchbar) {
            setResetSearchBarTimer();
        }
    });

    
</script>


    
