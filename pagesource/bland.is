        } catch (e) {
            pageLoding = false;
        }
        function scrollRight(id) {
            $('#' + id ).scrollLeft($('#' + id).scrollLeft() + 700);
            if ($('#' + id).scrollLeft() != 0) {
                $('#scrollLeft' + id).css('display', 'inline-block');
            }
        }

        var tmpTimeout = null;
        function scrolling(div) {
            if (tmpTimeout == null && div != null) {
                var lis = document.getElementById('scroll' + div.id).children;
                tmpTimeout = setTimeout(function () {
                    tmpTimeout = null
                }, 200);
                var liLeft = null;
                var ulLeft = document.getElementById(div.id).getBoundingClientRect().left;
                for (var i = 0; i < lis.length; i++) {
                    if (!lis[i].isDone) {
                        liLeft = lis[i].getBoundingClientRect().left;

                        var calc = ulLeft - liLeft;

                        if ((calc > -1000 && calc < -800) || (!lis[i].isDone && calc > -800)) {

                            var item = lis[i].children[0].children[0].children[0];
                            var attr = item.getAttribute('data-img');
                            if (attr != null) {
                                item.style.backgroundImage = 'url("' + attr + '")';
                            }
                            lis[i].isDone = true;
                        }
                    }
                }

            } else {
               // console.log('NOTHONG');
            }
        }


        function scrollLeft(id) {
            $('#' + id).scrollLeft($('#' + id).scrollLeft() - 700);
            if ($('#' + id).scrollLeft() == 0) {
                $('#scrollLeft' + id).hide();
            }

        }

        function removeSearch(searchWord) {
            if (confirm('Ertu viss um að þú viljir eyða þessu leitarorði út?')) {
                $('span[data-word="' + searchWord + '"]').remove();
                $.post('/classified/removesearch.aspx', { word: searchWord })
            }

        }

        $(document).ready(function () {
            if (!pageLoding) {
                loadFrontpageData();
            }
        })

        if (typeof dfp !== "undefined") {
            dfp.setTargeting(dfp.homepageConstant);
        }

</script>


