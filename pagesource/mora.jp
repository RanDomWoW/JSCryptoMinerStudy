<section id="rect_bunner1" class="rect_bunner">
<script type="text/javascript">
//スマホ以外
if(isMobile){
}else{
  if(isMediaGo){
    document.write('<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js">\x3c/script><!-- PC：Rectangle C 300x250 - rect_bunner --><ins class="adsbygoogle" style="display:inline-block;width:300px;height:250px" data-ad-client="ca-pub-7446447263045997" data-ad-slot="7938257064"></ins>');
    (adsbygoogle = window.adsbygoogle || []).push({});
  }else{
    if(isXapl || isMusicCenter){
      //$("#rect_bunner1").css("display", "none");
      for_noAd();
    }else{
    document.write('<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js">\x3c/script><!-- PC：Rectangle C 300x250 - rect_bunner --><ins class="adsbygoogle" style="display:inline-block;width:300px;height:250px" data-ad-client="ca-pub-7446447263045997" data-ad-slot="7938257064"></ins>');
    (adsbygoogle = window.adsbygoogle || []).push({});
    }
  }
}

//外部広告非表示環境向け
function for_noAd(){
	// ランダムに画像を表示する
	url = new Array();
	img_src = new Array();
	alt_text = new Array();
	target = new Array();
	hires_flg = new Array();
	coverart_flg = new Array();

	//ホスト名取得
	host = location.host;

	var disp_flg = xmlLoad();

	//xml取得成功時
	if (disp_flg.length > 0) {
		//alert(disp_flg.length);
		disp();
		
	//xml取得失敗時
	} else {
		disp_error();
	}
}
function xmlLoad(){
	// XML読み込み
	var xml_url = "http://" + host + "/pc/rect_banner_data.xml";
	var result = $.ajax({
		url:xml_url,
		type:'GET',
		dataType:'xml',
		timeout:1000,
		async: false,
		error:function() {
			//alert("ロード失敗");
			disp_error();
		},
		success:function(xml){
            i = 0;
			disp_flg = 1;
            $(xml).find("item").each(function() {
				url[i] = $(this).find('url').text();
				img_src[i] = $(this).find('img_src').text();
				alt_text[i] = $(this).find('alt_text').text();
				target[i] = $(this).find('target').text();
				hires_flg[i] = $(this).find('hires_flg').text();
				coverart_flg[i] = $(this).find('coverart_flg').text();
				
				i += 1;
            });
		}
	}).responseText;
	return result;
}
// HTML生成関数
function disp(){
	n = Math.floor(Math.random()*url.length);
	htmls = "";

	//ジャケ写の場合はベタ塗りする
	//if ((coverart_flg[n] == "") || (coverart_flg[n] == 0)) {
	//	document.write('<section class="rect_bunner">');
	//} else {
	//	document.write('<section class="rect_bunner" style="background-color: #000000;">');
	//}

	// target
	if ( url[n].match(/\?/)) {
		param_kigou = "&";
	} else {
		param_kigou = "?";
	}
	
	if ((target[n] == "") || (target[n] == 0)) {
		htmls = htmls + '<a href="' + url[n] + param_kigou + 'cpid=moratop_rct" title="' + alt_text[n] + '">';
	} else {
		htmls = htmls + '<a href="' + url[n] + param_kigou + 'cpid=moratop_rct" title="' + alt_text[n] + '" target="_blank">';
	}

	document.write('<div>');

	// ハイレゾロゴマーク
	if ((hires_flg[n] == "") || (hires_flg[n] == 0)) {
	} else {
		htmls = htmls + '<img src="http://cf.mora.jp/cfdocs/pc/img/banner/mora_Hi-res_selection.png" width="50" height="250" alt="mora Hi-res selection" class="hires_selection">';
	}

	if ((coverart_flg[n] == "") || (coverart_flg[n] == 0)) {
		htmls = htmls + '<img src="' + img_src[n] + '" border="0" alt="' + alt_text[n] + '" width="300" height="250" style="-ms-interpolation-mode: bicubic;">';
	} else {
		if ((hires_flg[n] == "") || (hires_flg[n] == 0)) {
			htmls = htmls + '<img src="' + img_src[n] + '" border="0" alt="' + alt_text[n] + '" width="250" height="250" class="rect_randjacket" style="margin-left: 50px; -ms-interpolation-mode: bicubic;">';
		} else {
			htmls = htmls + '<img src="' + img_src[n] + '" border="0" alt="' + alt_text[n] + '" width="250" height="250" class="rect_randjacket_hi-res" style="-ms-interpolation-mode: bicubic;">';
		}
	}

	htmls = htmls + '</div>';
	htmls = htmls + '</a>';
	
	$("#rect_bunner1").html(htmls);
}
// HTML生成関数（エラー時）
function disp_error(){
	$("#rect_bunner1").html('<a href="/ranking?term=lrank24&genre=hires&cpid=moratop_rct"><img src="http://cf.mora.jp/cfdocs/pc/img/banner/top_ad_hiresrank.jpg" alt="TOPレクタングルバナー" width="300" height="250"></a>');
}
</script>
