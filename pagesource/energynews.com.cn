<script type="text/javascript">

	var n_c_ser = 0;//设置初始值为0；

	function t_player(cur){

		$('ul.nav-list > li').eq(n_c_ser).removeClass("nav-cus-"+n_c_ser);

		if(cur==null){

			n_c_ser = ++n_c_ser % 3;   //递加函数

		}else{

			n_c_ser = cur;

		}

		if(n_c_ser !=0 ){

		}



		$('.inner').animate({marginLeft:(-1 * n_c_ser * 800) + 'px'});//下面内容的动画效果左移动682px；



		$('ul.nav-list > li').eq(n_c_ser).addClass("nav-cus-"+n_c_ser);

	}



	var n_t = setInterval('t_player()', 5000);//每隔5秒 执行一次



	$('ul.nav-list > li span').hover(function(){//当我鼠标悬浮上去的时候

		var self = this;

		clearInterval(n_t);   //清除动画

		$('.inner').stop();

		var mid = $(this).parent().index()

		$('ul.nav-list > li').eq(n_c_ser).addClass("nav-cus-"+n_c_ser);

		t_player(mid);

	},function(e){

		n_t = setInterval('t_player()', 5000);//当鼠标离开的时候  继续执行上面循环的函数

	});



	/*会展无缝滚动*/

	var ocontent = document.getElementById('wfgd');

	var olist = document.getElementById('list');

	var clone_list = list.cloneNode(true);

	//将克隆后的div放入到#list中

	list.parentNode.appendChild(clone_list);

	var height = ocontent.offsetHeight;

	var list_height = olist.offsetHeight;

	var timer = null;

	timer = setInterval(Scroll,30);

	function Scroll(){

		var top = ocontent.scrollTop;

		if(top>=list_height){

			ocontent.scrollTop-=list_height;

		}else{

			ocontent.scrollTop++;

		}

	}

	function over($this){

		clearInterval(timer);

		timer=null;

	}

	function out($this){

		timer = setInterval(Scroll,30);

	}

</script>