<body ng-app="app">

<div class="wrapper">
    <div class="column3">
        <div class="table--wrapper">
            <div class="table--wrapper__center text-center">

                <a href="/site/register">
                    <h4 style="color: #333; font-size: 18px;">Create an account</h4>

                </a>
                <div class="row-table">
                    <div class="cell2">

                        <div class="reg" ng-controller="RegCtrl" ng-init='init({"captcha":"6LekggoUAAAAAKrArq6yXkYEoz2axWTFNU81SX4x"})'>
                            <div style="display: none;">
                                <form action="/site/register" method="post" id="register-form">
                                    <input type="text" id="registerform2-email" class="form-control"
                                           name="RegisterForm2[email]" value="{{user.email}}">
                                    <input type="text" id="registerform2-username" class="form-control"
                                           name="RegisterForm2[username]" value="{{user.email}}">
                                    <input type="text" id="registerform2-firstname" class="form-control"
                                           name="RegisterForm2[firstname]" value="{{user.firstname}}">
                                    <input type="text" id="registerform2-gender" name="RegisterForm2[gender]"
                                           value="{{user.gender}}">
                                    <input type="text" id="RegisterForm2_birthday" name="RegisterForm2[birthday]"
                                           value="{{user.birthday.day}}">
                                    <input type="text" id="RegisterForm2_birthmonth" name="RegisterForm2[birthmonth]"
                                           value="{{user.birthday.month + 1}}">
                                    <input type="text" id="RegisterForm2_birthyear" name="RegisterForm2[birthyear]"
                                           value="{{user.birthday.year}}">
                                    <input type="password" id="registerform2-password" class="form-control"
                                           name="RegisterForm2[password]" value="{{user.password}}">

                                    <input type="hidden" id="registerform2-verifycode" name="RegisterForm2[verifyCode]"
                                           value="{{verifyCode}}">

                                </form>
                            </div>
                            <multi-step-container steps="steps" on-finish="finish()" class="ng-cloak">
                                <header>
                                    <span ng-bind-template="{{$getActiveStep().title}}"></span>
                                </header>
                                <br>

                                <main step-container></main>
                                <br>
                                <footer>
                                    <button ng-show="!$isFirst()" class="btn btn-default" ng-click="$previousStep()">
                                        Back
                                    </button>
                                    <button ng-show="!$isLast()" ng-disabled="!$getActiveStep().valid"
                                            class="btn btn-default" ng-click="validateForm($getActiveIndex, $nextStep)">
                                        Continue
                                    </button>
                                    <button ng-show="$isLast()" ng-disabled="!$getActiveStep().valid"
                                            class="btn btn-success" ng-click="$finish()">
                                        Complete registration
                                    </button>
                                </footer>
                            </multi-step-container>
                        </div>
                    </div>
                </div>
                <style>
                    .wrapper .column3 a, .nav-pills li a:hover {
                        color: #86929A;
                        font-size: 11px;
                    }
                </style>
