<script src="/core/assets/vendor/jquery/jquery.min.js?v=2.1.4"></script><script>
// Version 1
jQuery.fn.outerHTML = function() {
  return jQuery('<div />').append(this.eq(0).clone()).html();
};
var months = {
    januar: 'January',
    februar: 'February',
    marts: 'March',
    april: 'April',
    maj: 'May',
    juni: 'June',
    juli: 'July',
    august: 'August',
    september: 'September',
    oktober: 'October',
    november: 'November',
    december: 'December',
};
$(document).ready(function() {
    
    // If cs-id is there check to see if customer service is open
    if($('#cs').length > 0) {
        isCustomerServiceOpen();
    }
    function isCustomerServiceOpen() {
        var icon = $('#cs .icon');
        var CSopen = $('#cs .cs-open');
        var CSclosed = $('#cs .cs-closed');
        var serviceOpen = 1;
        var openingUrl = '/en/contact';
        var today = new Date(); // You can write a date here to simulate another time like new Date('October 9, 2016, 09:25:00');
        var timeDiff = today.getTimezoneOffset()/60;
        var timeDiffLocal   = (timeDiff > 0) ? -Math.abs(timeDiff) : Math.abs(timeDiff);

        var normalOpening   = [9, 9, 9, 9, 9, 9, 9];
        var normalClosing   = [14, 15, 15, 15, 15, 14, 14];
        getNormalOpeningHours();

        var diffOpening     = []; // 'October 10, 2016, 10:25:00'
        var diffClosing     = [];
        var allDayClosed    = getOtherOpening(); //'October 9, 2016'
    
        var normalOpeningTable, otherOpening;

        Date.prototype.addHours= function(h){
            this.setHours(this.getHours()+h);
            return this;
        }
        today = today.addHours(timeDiff);

        var dkTime  = getDkTime(today);
        // Get todays date without hours etc
        var d1      = new Date(dkTime).setHours(0, 0, 0, 0, 0);
        function getDkTime(d) {
            d.addHours(timeDiffLocal);
            return d;
        }

        if(closedAllDay()) {
            closingTime();
        } else {
            getOpeningHours();
        }
        function getNormalOpeningHours() {
            var arr = new Array();
            $.ajax({
                url : openingUrl,
                dataype: 'html',
                cache: false,
                async: true,
                success: function(k){
                    $(k).find('#normalOpeningHours tr').each(function(index) {
                        var $this = $(this);
                        $this.find('td').each(function(i) {
                            var $this_td = $(this);
                            if(i == 1) {
                                if(index == 0) {
                                    for (x = 1; x < 5; x++) { 
                                        //alert(x);
                                        normalOpening[x] = parseInt($this_td.html().split(':')[0].replace(/^0/, ''));
                                        normalClosing[x] = parseInt($this_td.html().split(' - ').pop().split(':')[0].replace(/^0/, ''));
                                    }
                                }
                            }
                            //console.log($this_td.html());
                        });
                    });
                }
            });
        }

        // Checks for if there is closed all day. Also sets if there's a difference in opening hours or closing hours
        function getOtherOpening() {
            var arr = new Array();
            $.ajax({
                url : openingUrl,
                dataype: 'html',
                cache: false,
                async: false,
                success: function(k){
                       if($(k).find('#otherOpeningHours tr').length > 1) {
                        // Start looping through the rows
                        // otherOpening.find('tr').each(function(index) {
                        $(k).find('#otherOpeningHours tr').each(function(index) {
                            var $this = $(this);
                            // First row is a header - skip that
                            //if(index > 0) {
                                // Declare a temp var to hold date if 'closed' or 'lukket' is found
                                var temp;
                                $this.find('td').each(function(i) {
                                    var $this_td = $(this);
                                    // If the first cell which always should hold the date
                                    if(i == 0) {
                                        // Take the date and turn it into english if in danish
                                        var closedMonthDate = checkMonthLanguage($this_td.html());
                                        // Add the current year to have a complete date
                                        temp = closedMonthDate+', '+today.getFullYear();
                                    }
                                    //console.log(temp);
                                    // If closed push the date to the date to closed all day array
                                    if($this_td.html().toLowerCase() == 'lukket' || $this_td.html().toLowerCase() == 'closed') {
                                        arr.push(temp);
                                    } else {
                                        if(i == 1) {
                                            diffOpening.push(temp+', '+$this_td.html().split(' - ')[0].replace(/^0/, '')+':00');
                                            diffClosing.push(temp+', '+$this_td.html().split(' - ').pop().replace(/^0/, '')+':00')
                                        }
                                    }
                                    //console.log(diffOpening);
                                    //console.log(diffClosing);
                                });
                            //}
                        });
                    }
                }
            });
            return arr;
        }

        function checkMonthLanguage(l) {
            var month = l.slice(0, l.indexOf(' ')).toLowerCase();
            var date = l.substring(l.indexOf(' ') +1);
            if(months[month] != undefined) {
                month = months[month];
            }
            // Make sure months first letter is capitalized
            return month.substr(0,1).toUpperCase()+month.substr(1)+' '+date;
        }

        function getOpeningHours() {
            var dayIndex    = dkTime.getDay();
            var hourIndex   = dkTime.getHours();
            var minuteIndex = dkTime.getMinutes();

            var opening = differentOpeningHours('open');
            var closing = differentOpeningHours('closed');

            // No different opening or closing set
            if((!opening.hour) && (!closing.hour)) {
                if(dayIndex == 0 || dayIndex == 6) {
                    closingTime();
                }
                if(dayIndex >= 1 && dayIndex <= 5) {
                    if(hourIndex < normalOpening[dayIndex] || hourIndex >= normalClosing[dayIndex]) {
                        closingTime();
                    }
                }
            } else {
                opening.hour = (opening.hour > 0) ? opening.hour : normalOpening[dayIndex];
                
                closing.hour = (closing.hour > 0) ? closing.hour : normalClosing[dayIndex];

                if((hourIndex == opening.hour) && (minuteIndex < opening.minute)) {
                    closingTime();
                }
                if(hourIndex <= opening.hour || hourIndex >= closing.hour) {
                    if(
                        ((hourIndex == opening.hour) && (minuteIndex < opening.minute)) || 
                        ((hourIndex == closing.hour) && (minuteIndex >= closing.minute)) || 
                        ((hourIndex < opening.hour) || (hourIndex >= closing.hour) && (minuteIndex >= closing.minute))) {
                            closingTime();
                    } 
                }
            }
        }

        function differentOpeningHours(ur) {
            var obj = new Object;
            var clocklist;
            if(ur == 'open') {
                clocklist = diffOpening;
            } else {
                clocklist = diffClosing;
            }
            // Number of different opening hours
            obj.length = clocklist.length; // Return diffOpenLength
            if(obj.length > 0) {
                // Loop through the list of dates
                for (var i = 0; i < obj.length; i++) {
                    var d2 = getJustDate(clocklist[i]);
                    // If there is a different opening hours for today
                    if(d1 - d2 == 0) {
                        obj.hour        = new Date(clocklist[i]).getHours();  // Return diffOpenHour
                        // Rund op til at håndtere hvert kvarter
                        obj.minute      = new Date(clocklist[i]).getMinutes(); // Return diffOpenMin
                    }
                }
                // obj.minute = getQuarter(obj.minute);
                return obj;
            } else {
                return false;
            }
        }

        function closingTime() {
            icon.removeClass('icon-cs-green').addClass('icon-cs-red');
            CSopen.hide();
            CSclosed.show();
            serviceOpen = 0;
            return;
        }
    
        function closedAllDay() {
            if(allDayClosed.length == 0) {
                return false;
            } else {
                for (var i = 0; i < allDayClosed.length; i++) {
                    var d2 = getJustDate(allDayClosed[i]);
                    // If there is a different opening hours for today
                    if(d1 - d2 == 0) {
                        return true;
                    }
                }
                return false;
            }
        }

        function getJustDate(d) {
            var justDate = new Date(d).setHours(0, 0, 0, 0, 0);
            return justDate;
        }
    }
});
</script></div>
