<script type="text/javascript">
	$(function() {
		$('.cms-block').each(function() {
			var $elem = $(this);
			var $ul = $elem.find('ul.cms-block-images-slider');
			var $period = parseInt($elem.attr('data-period'));
			var $items = $ul.find('li');

			if ($period > 0 && $ul.length && $items.length > 1) {
				setInterval(function() {
					var $active = $elem.find('li.active');
					var $next = $active.next('li');
					if (!$next.length) {
						$next = $elem.find('li:first');
					}

					var $slide = parseInt($next.attr('data-item')) * 620;
					$active.removeClass('active');
					$next.addClass('active');
					$ul.animate({ 'left': -$slide }, 600, 'easeInOutCubic');
				}, $period);
			}

		});
	});
</script>
