    var isAuthenticated = 0;

    $(document).ready(function () {

        var errors = {
            // Not authenticated error messages
            cart: '<strong>To add a track in your shopping cart</strong> you need to be logged in',
            favourite: '<strong>To add a track to your favourites</strong> list you need to be logged in',
            audition: '<strong>To use Drumdrops Audition <br />you need to be logged in</strong>',

            // Authorization error messages
            credentials: '<strong>Sorry the password doesn\'t match</strong>Please try again or retrieve using the link below',
            formInvalid: '<strong>Sorry but the form is not complete</strong>Please fill in all fields and try again',
            emailInvalid: '<strong>We don\'t recognize this email address.</strong>Please check it and try again',
            emailBlacklist: '<strong>Sorry we don\'t allow anonymous email addresses</strong><p style="font-size: 7pt">If you want free product you can at least put up with the few newsletters we send out</p>',
            accountInactive: '<strong>Sorry but this account has not been activated yet.</strong>Please activate it with the email we sent you and then try&nbsp;again.',
            accountDeleted: '<strong>Sorry but this account was deleted.</strong>You will need to re-register again.',

            // Common error messages
            serverError: '<strong>Sorry, unexpected server error</strong>Please try again or contact administration if error recurs'
        };

        function displayLoginWarning(message) {
            $('#login-form-error').html(message);
        }

        function removeLoginWarning(message) {
            $('#login-form-error').html(message);
        }

        function clearLoginErrorBackground()
        {
        	$thisform = $('form.login');
        	$thisform.parents('div.overlay-small').removeClass('errors');
        }
        
        function clearLoginErrors()
        {
            // TODO
        }

        var callback = null;

        $('#do-login-form').submit(function (event) {
            event.preventDefault();
            var form = $(this);

			clearLoginErrorBackground();
            
            form.ajaxSubmit({
                dataType: 'json',
                success: function(response) {
                    if (response.result == 'error') {
                        displayErrorBackground();
                        displayLoginWarning(errors[response.type]);
                        if (response.type == 'accountInactive')
                            $('#send_activation_email_line').show('fast');
                    } else {
                        if (callback) {
                            callback();
                        } else if (location.href.indexOf('auth/activatePassword') > -1) {
                            var host = location.host;
                            location.href = 'http://' + host;
                        } else {
                            location.reload();
                        }

                    }
                },
                error: function() {
                    displayLoginWarning(errors.serverError);
                }
            });
        });


        // Add to favourites action
        $('body').on('click', 'a.savefav', function (event) {
            if (!isAuthenticated) {
                event.preventDefault();
                var el = this;

                callback = function () {
                    $favid = el.id;
                    $arr = $favid.split("_");
                    addToFavourites($arr[2]);
                    location.reload();
                };

                displayErrorBackground();
                displayLoginWarning(errors.favourite);
                $('div.lightbox.login').fadeIn(200);
                return false;
            }
        });


        // Audition action
        $('body').on('click', 'a.audition-link', function (event) {
            if (!isAuthenticated) {
                event.preventDefault();
                displayErrorBackground();
                displayLoginWarning(errors.audition);
                $('div.lightbox.login').fadeIn(200);
                return false;
            }
        });

        // Audition More Info action
        $('body').on('click', 'a.audition-this', function (event) {
            if (!isAuthenticated) {
                displayErrorBackground();
                displayLoginWarning(errors.audition);
                $('div.lightbox.login').fadeIn(200);
                return false;
            }
        });


        // Cart items

        // Define it as function, because it will be called after ajax requests (for popup purchase forms)
        function handleCartForms() {
            $('form.cart-form').submit(function (e) {

                var self = this;
                if (isAuthenticated)
                    return true;

                e.preventDefault();

                callback = function () {
                    isAuthenticated = true;
                    $(self).submit();
                };

                displayErrorBackground();
                displayLoginWarning(errors.cart);
                $('div.lightbox.login').fadeIn(200);

            });
        }

        $('body').ajaxComplete(function () {
            handleCartForms();
        });


    });
</script>
