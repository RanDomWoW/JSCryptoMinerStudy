    <script>
        function gCs(elem, value)
        {
            res =  getComputedStyle(elem)[value];
            if(res.match(/px/)) {
                return parseFloat(res);
            }
            return res;
        }
    </script>
    <script>
        a = $("expand");
        text = $("text");
        mainWrapper = $("center_static_block");
        aHeight = gCs(a, "height");
        aWidth = gCs(a, "width");
        aPaddingTop = gCs(a, "padding-top");
        textHeight = parseFloat(getComputedStyle($("center_static_block"))["height"]);
        textTop = gCs(mainWrapper, "margin-top");
        textWidth = gCs(mainWrapper, "width");
        fullCache = text.innerHTML;
        a.style.top = textHeight - aHeight  + "px";
        a.style.left = textWidth - aWidth + "px"; // позиционировать почти с правого краю!
        function minimize()
        {
            text.id = "text";
            a.style.display = "block";
            text.innerHTML = fullCache;
            $("homepageBottom").style.marginTop = "0px";
        }
        expandBottomBlock = text.scrollHeight - gCs(text, "height");
        function expand()
        {
            text.id = "text-expand";
            $("homepageBottom").style.marginTop =  expandBottomBlock + "px";
            a.style.display = "none";
            text.innerHTML+="&nbsp; &nbsp;  <a href='javascript: void(null)' onclick='minimize()'>Close</a>";
        }

    </script>
