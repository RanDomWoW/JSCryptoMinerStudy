<script type="text/javascript">
  function calcTotalQty(){
    var totalcount = 0;

try {
    deliverystoreCarMgr.getCartInfo('d', function(cart){
      if(cart.totalQty !=null){
        totalcount += cart.totalQty;
        $("span#dcart").text( cart.totalQty );
        $("span#cart").text(totalcount);
      }
    });

    deliverystoreCarMgr.getCartInfo('s', function(cart){
      if(cart.totalQty!=null){
        totalcount += cart.totalQty;
        $("span#scart").text( cart.totalQty );
        $("span#cart").text(totalcount);
      }
    });
    deliverystoreCarMgr.getCartInfo('h', function(cart){
      if(cart.totalQty!=null){
        totalcount += cart.totalQty;
        $("span#hcart").text( cart.totalQty );
        $("span#cart").text(totalcount);
      }
    });
}
catch(err) {
console.log(err);
}


  }

  function showLogin() {
    $.ajax({
      type: "POST",
      url:  "/TreeMall/sso/auth/checkLogin.jsp?timeStamp=" + new Date().getTime()
    }).done(function(data) {
      alertShowLoginContents(data);
    });
  }

  function alertShowLoginContents(data) {
    if (data.length <= 300) { //已登入
      $('#is-login-info > .need-login').show();
      setCookie('isLogin', '1', 1);

      var $response = jQuery.parseJSON( data.replace(/\'/g, '\"').replace(';','') );
      var $cmemNum = parseInt($response[0]);

      if ($cmemNum != null) {
        $.ajax({
          type: "POST",
          url: "/SymphoxAPI/point/query",
          data: {cmemNum: $cmemNum}

        }).done(function(data) {
          var $jsonData = $.parseJSON(data), $link = '';
          var $cmemName = $jsonData.cmemName;
          var $coupon = number_format($jsonData.ecouponCount ? $jsonData.ecouponCount : '0');
          var $epoint = number_format($jsonData.totalEpoint ? $jsonData.totalEpoint : '0');
          var $point = number_format($jsonData.totalPoint ? $jsonData.totalPoint : '0');
          var $total = number_format( ($jsonData.totalPoint ? $jsonData.totalPoint : 0 ) + ($jsonData.totalEpoint ? $jsonData.totalEpoint : 0));

          if($cmemName.indexOf("先生") != -1){
            $("span#appellation").text("先生");
          }else if(($cmemName.indexOf("小姐") != -1)){
            $("span#appellation").text("小姐");
          }

          $("span#cmemName").text( $cmemName.replace("先生", "").replace("小姐", "")), $("span#epoint").text( $epoint ), $("span#point").text( $point ), $("span#coupon").text( $coupon );

          $("body").addClass("member-sign-in");

          $("input#mInfo").data( "mInfo", { cmemName: $cmemName, epoint: $epoint, point: $point, coupon: $coupon } );

          if ($jsonData.cmemBank && $jsonData.cmemBank == '100') {
            $link = '<li><a href="/member/pointlist">查詢</a></li>';
          } else {
            $link = '<li><a href="/PM/regist/auth" style="text-decoration: underline;">卡友認證合併舞動紅利點GO→</a></li>';
          }

          var $promotions = '<li><a href="/member/memberSale">優惠活動</a></li>';
          var $content = '<li>親愛的<span class="user-name">' + $cmemName + '</span>，您共有<span class="points">' + $total + '</span>點 ' + '及折價券<span class="points">' + $coupon + '</span>張</li>';

          $("ul#topmenu_ICON").show().append($content + $link + $promotions);
        });
      }

    } else {
      $('#is-login-info > li:not(.need-login)').show();

      setCookie('isLogin', '0', 1);
      setCookie('bfy_auth', '0', 1);
    }
  }

  function number_format(n) {
    n += "";
    var arr = n.split("."), re = /(\d{1,3})(?=(\d{3})+$)/g;
    return arr[0].replace(re,"$1,") + (arr.length == 2 ? "." + arr[1] : "");
  }

  function setCookie(name, value, days) {
    var expires = "";

    if (days) {
      var date = new Date();
      date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
      expires = "" + date.toGMTString();
    }

    document.cookie = name + "=" + value + "; expires=" + expires + "; path=/;";
  }

  function goShopping(type){
	var dcart = $("#dcart").text();
	var scart = $("#scart").text();
	var hcart = $("#hcart").text();
  	   
  	if(type === 'dcart') {
		dcart == 0 ? alert("購物車內無商品！") : window.location.href = "/buy/deliveryThreshold";
	} else if(type === 'scart') {
		scart == 0 ? alert("購物車內無商品！") : window.location.href = "/buy/storeThreshold";
	} else if(type === 'hcart') {
		hcart == 0 ? alert("購物車內無商品！") : window.location.href = "/buy/multiShopping";
	} else {
		( dcart == 0 && scart == 0 && hcart == 0) ? alert("購物車內無商品！") : dcart != 0 ? window.location.href = "/buy/deliveryThreshold" : scart != 0 ? window.location.href = "/buy/storeThreshold" : window.location.href = "/buy/multiShopping";
	}
  }

  $(function(){
    $("#shopping-cart").appendTo('body');

    showLogin();
    calcTotalQty();
  })
</script>
