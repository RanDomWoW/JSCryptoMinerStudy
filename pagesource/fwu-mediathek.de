		$( document ).ready(function() {
			$('#search').click(function() {
				$('#mobilesearch').fadeIn('slow');
			});
			$('#destroymobilesearch').click(function() {
				$('#mobilesearch').fadeOut('slow');
			});
 			$('[data-toggle="tooltip"]').tooltip();
				//Enable swiping...
					$(".carousel-inner").swipe( {
						//Generic swipe handler for all directions
						swipeLeft:function(event, direction, distance, duration, fingerCount) {
							$(this).parent().carousel('prev'); 
						},
						swipeRight: function() {
							$(this).parent().carousel('next'); 
						},
						//Default is 75px, set to 0 for demo so any distance triggers swipe
						threshold:0
					});
			
			//$( "#addtoedit" ).click(function() {
			//	window.location.href = "/eigenemedien?pid=pqkgd0ropsonenmijpsn4aqgd7";
			//});
			
		});
		
		function merken(id, store){
			$.ajax({
			  url: "/store?pid=pqkgd0ropsonenmijpsn4aqgd7&store=" + id,
			  beforeSend: function( e ) {

					if(store) {
						$('a.mstore[data-id="' + id + '"]').hide();
						$('a.mremove[data-id="' + id + '"]').show();
						$('#modal-merkliste p.mstore').show();
						$('#modal-merkliste p.mremove').hide();

					} else {
						$('a.mstore[data-id="' + id + '"]').show();
						$('a.mremove[data-id="' + id + '"]').hide();
						$('#modal-merkliste p.mstore').hide();
						$('#modal-merkliste p.mremove').show();

					}			 
					$('#modal-merkliste').modal('show');

				}
			}).done(function( data ) {
				ret=$.parseJSON(data);
				$(".a_navcount").html('(' + ret.count + ')');
				// Update Stored and!! Button here.
			});
		}		


		function showfrombottom(id) {
			$('#' + id).css('display', 'block');
			$('.affix').hide();
			$('body').css('overflow','hidden');
			//$('.antares-bottom-show').removeClass('antares-bottom-show');
			$('#' + id).removeClass('antares-bottom-hide');
			$('#' + id).addClass('antares-bottom-show');
		}
		
		function hidetobottom(id) {
			$('body').css('overflow','auto');
			$('#' + id).removeClass('antares-bottom-show');
			$('#' + id).addClass('antares-bottom-hide');
			
			setTimeout(function() {
				$('#' + id).css('display', 'none');
			}, 1000);
			//if(sinited)$('.affix').show();
		}

		function close_player() {
			hidetobottom("bottomplayer");
			$('#playerframe').attr('src', '');
		}

		function open_player(src) {
			console.log(src);
			src=src.replace('http://', '//');
			if(!src||src=="") {
				flexmodal("Diese Funktion steht nur eingeloggt zur Verf&uuml;gung.");
				return;	
			}
			
				showfrombottom('bottomplayer');
				document.getElementById('playerframe').setAttribute('src', src);
			

		}		
		
		function vollbild() {
			var src=$('#playerframe').attr('src');
			close_player();
			window.open(src);
		}
	</script>

		<div id="bottomplayer" class="antares-bottom-hide antares-bottom-base" style="z-index:550">
	
			<button type="button" title="Player schlie&szlig;en" class="close" onclick="close_player()"><span class="close_text">Player schliessen</span> <i class="fa fa-times fa-lg"></i></button>

			<h4 class="modal-title" id="playertitle">
				<button style="margin-right:20px" type="button" title="Vollbildwiedergabe" xclass="close" onclick="vollbild()"><i class="fa fa-arrows-alt"></i> Vollbild</button>
				Player
			</h4>
			<hr />
			<div id="playercontent">
				<center>
					<iframe width="1024" height="768" src="" id="playerframe" style="border:0" allowfullscreen="true" webkitallowfullscreen="true" mozallowfullscreen="true"></iframe>
				</center>
			</div>
		</div>
	

