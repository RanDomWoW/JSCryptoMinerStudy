            $("#nome").closest('.box-input').append("<div class='form-validator-stack help-block erro-email' id='emailHelper'><div role='alert' class='email'>Houve um erro no envio dos seus dados. Por favor, tente novamente.</div></div>");
           $("#nome").addClass('error-field').removeClass('success-field').closest('.form-group').addClass('has-error').removeClass('has-success');
        }          
        
    }

