    var formIsValid = function(idForm) {
    
        var selectorForm = $(idForm);
        var retorno = false;

        var inputs = selectorForm.find(".error-field");

        if (inputs.length === 0 ) {
            retorno = true;
        } else {
            retorno = false;
        }
        return retorno;
