<script type="text/javascript">
	var $j190 = jQuery.noConflict(true);
		jQuery(function(){
		var $ = $j190;
		$("#kvCarousel").owlCarousel({
			singleItem:true,
			autoPlay:8000,
			stopOnHover : true,
			pagination:false,
			navigation:true
		});

		$("#eventCarousel").owlCarousel({
			singleItem:true,
			autoPlay:8000,
			stopOnHover : true,
			pagination:false,
			navigation:true,
			afterInit: function(elem){
				this.options["updateDateParam"] = function(currentItem){
					var item = $("#eventCarousel ul").eq(currentItem).children("li:first");
					var fromDataVal = item.attr("data-from-date");
					var toDateVal = item.attr("data-to-date");
					
					var trendCalObj = $("div.trendArea div.trendCalendar");
					if ((fromDataVal) && (toDateVal)) {
						trendCalObj.show();
					} else {
						trendCalObj.hide();
						return;
					}
					
					var fromDate = fromDataVal.split("/");
					var toDate = toDateVal.split("/");
					
					var yearObjs = $("div.trendArea div.trendCalendar span.titYear");
					var dayObjs = $("div.trendArea div.trendCalendar span.titDate");
					var weekObjs = $("div.trendArea div.trendCalendar span.titDay");
					
					yearObjs.eq(0).text(fromDate[0]);
					yearObjs.eq(1).text(toDate[0]);
					dayObjs.eq(0).text(fromDate[1] + "/" + fromDate[2]);
					dayObjs.eq(1).text(toDate[1] + "/" + toDate[2]);
					
					var dayList = new Array("Sun.", "Mon.", "Tue.", "Wed.", "Thu.", "Fri.", "Sat.");
					var holidayList = new Array(true, false, false, false, false, false, false);
					
// 2015/08/20 mdify start <<
//					var fromDateObj = new Date(parseInt(fromDate[0]), parseInt(fromDate[1]) - 1, parseInt(fromDate[2]));
//					var toDateObj = new Date(parseInt(toDate[0]), parseInt(toDate[1]) - 1, parseInt(toDate[2]));
					var fromDateObj = new Date(parseInt(fromDate[0], 10), parseInt(fromDate[1], 10) - 1, parseInt(fromDate[2], 10));
					var toDateObj = new Date(parseInt(toDate[0], 10), parseInt(toDate[1], 10) - 1, parseInt(toDate[2], 10));
// 2015/08/20 mdify end >>
					
					var fromDateWeek = fromDateObj.getDay();
					var toDateWeek = toDateObj.getDay();
					
					weekObjs.eq(0).removeClass("holyday").text(dayList[fromDateWeek]);
					weekObjs.eq(1).removeClass("holyday").text(dayList[toDateWeek]);
					if (holidayList[fromDateWeek]) weekObjs.eq(0).addClass("holyday");
					if (holidayList[toDateWeek]) weekObjs.eq(1).addClass("holyday");
				};
				this.options.updateDateParam(0);
			},
			afterMove: function(elem){
				this.options.updateDateParam(this.currentItem);
			}
		});
	});
</script>
