').val()+'&Email='+jQuery('#create-carte-form #email_crd').val()+'&Tel='+jQuery('#create-carte-form #tel_crd').val()+'&Code='+jQuery('#create-carte-form #code_secret_crd').val()+'&Offres='+jQuery('#create-carte-form #offres_crd').val()+'&time='+jQuery('#create-carte-form #time_crd').val()+'&result='+jQuery('#create-carte-form #result_crd').val(),success:function(ret_data){var tab_data=ret_data.split("-");loyaltyConfirmData=new Array();if(tab_data[0]=="OK"){jQuery('#loading-popup-picto').fadeOut(100);loyaltyConfirmData['email']=jQuery('#create-carte-form #email_crd').val();loyaltyConfirmData['birthday']=jQuery('#create-carte-form #yeard_crd').val()+jQuery('#create-carte-form #monthd_crd').val()+jQuery('#create-carte-form #dayd_crd').val();loyaltyConfirmData['firstname']=jQuery('#create-carte-form #prenom_crd').val();loyaltyConfirmData['name']=jQuery('#create-carte-form #nom_crd').val();loyaltyConfirmData['civility']=(jQuery('#create-carte-form #nom_crd').val()=='Mr')?'monsieur':'madame';jQuery("#popup_createcarte #content_carte").html("Nous venons de vous envoyer un email à l'adresse "+loyaltyConfirmData['email']+" avec votre numéro de carte E.Leclerc.");jQuery('#popup_createcarte h2:first-child').html("Votre carte a bien été créée.");jQuery("#popup_createcarte").show();}else{jQuery('#loading-popup-picto').fadeOut(100);jQuery("#popup_createcarte #content_carte").html(ret_data);jQuery('#popup_createcarte h2:first-child').html("Une erreur est survenue.");jQuery("#popup_createcarte").show();}}});}function showCreateCarte(){jQuery('#loading-popup-picto').fadeOut(100);jQuery("#popup_createcarte").insertAfter('#fondpopup');jQuery("#fondpopup").fadeIn(1000);jQuery("#popup_createcarte").css({position:'absolute',background:''});jQuery('#popup_createcarte').fadeIn(1000);jQuery('#popup_createcarte').css('top',jQuery(window).scrollTop()-40+((jQuery(window).height()-jQuery('#popup_createcarte').height())/2)+'px');}</script>							</div>
