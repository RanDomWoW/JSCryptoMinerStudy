	$("#closeSticky").click(function() {
		$(this).parent().hide();
		$("#btnOpenSticky").show();
		setCookie('stickyBox',0,7);
	});
	$("#btnOpenSticky").click(function() {
		$(".stickyJobbar").show();
		$(this).hide();
		setCookie('stickyBox',1,7);
	});
	$(window).load(function(){
		var runStickBox = function(){
			var autoTab = setInterval(autoTabFun,15000);
			var stickyBox = getCookie('stickyBox');
			if(stickyBox == 0)
				$("#closeSticky").trigger('click');
			else{
				$("#btnOpenSticky").trigger('click');
			}
			$(".tab-content").mouseleave(function(){autoTab = setInterval(autoTabFun,15000);}).mouseenter(function(){clearInterval(autoTab);});
		}
		window.setTimeout(runStickBox,5000);
	});
	function actionSticky(show,hide){
		var show_list = show+'_list';
		var unfocus = hide+' a';
		var hide_list = hide+'_list';
		$(hide).removeClass('active');
		$(hide_list).hide();
		$(show).addClass('active');
		$(show_list).show();
		$(unfocus).blur();
	}
</script>

