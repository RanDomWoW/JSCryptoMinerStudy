					slideDistance:0,//スライド同士の距離
					visibleSize: '100%',//前後のスライドを表示
					sliderResize: function() {
						$(".sp-slide a img").css({
							"margin-top": "0px",
							"margin-left": "0px"
						});
					}
				});
			}
		}).attr('src', $(allImage[i]).attr('src'));
	}
	$('#topSlide_02 .slideScroll ul').each(function(k, ele){
		var dir = ($(ele).hasClass("slideRight")) ? "backwards" : "forwards";
		$(ele).simplyScroll({
			autoMode: 'loop',
			speed: 1,
			frameRate: 24,
			horizontal: true,
			pauseOnHover: false,
			pauseOnTouch: false,
			direction: dir
		});
	});
});
</script>
