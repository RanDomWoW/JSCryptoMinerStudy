<script>
    require([ 'jquery', 'jquery/ui'], function($) {
        var minicart = $('.minicart .minicart__details');
        var timer;

        // Simple way to check if a user is currently on the Cart/Bag page
        var userOnCart = $('body').hasClass('checkout-cart-index');

        // Hover of bag icon
        $('.minicart__summary').hover(function () {
            if (userOnCart) {
                return;
            }

            //Clear timeout just in case you hover-in again within the time specified in hover-out
            clearTimeout(timer);
            minicart.addClass('updatedCart');
        }, function () {
            //Clear the timeout set in hover-in
            clearTimeout(timer);
            timer = setTimeout(function () {
                minicart.removeClass('updatedCart');
            }, 4000);
        });
        // Hover of actual minicart
        $('.minicart__details').hover(function () {
            if (userOnCart) {
                return;
            }

            //Clear timeout just in case you hover-in again within the time specified in hover-out
            clearTimeout(timer);
        }, function () {
            //Clear the timeout set in hover-in
            clearTimeout(timer);
            timer = setTimeout(function () {
                minicart.removeClass('updatedCart');
            }, 4000);
        });
    });
</script>
