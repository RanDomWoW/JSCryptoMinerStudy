    $('.search-icon-toggle-button').click(function(e){
        e.preventDefault();
        setTimeout(function(){
            $('#search-main').click().focus();
        }, 300);
    });
    

    $(document).click(function (event) {

        if(!$(event.target).closest('.navbar-collapse').length){
            var clickover = $(event.target);
            var $navbar = $(".navbar-collapse");               
            var _opened = $navbar.hasClass("in");
            if (_opened === true && !clickover.hasClass("navbar-toggle")) {      
                $navbar.collapse('hide');
            }
            
            if($(event.target).closest('.header-avatar').length || $(event.target).closest('.menu-mobile-nav-button').length){
                $('.affix-top, .affix').toggleClass('affix-force'); 
            } else {
                $('.affix-top, .affix').removeClass('affix-force');
            }
            

        }
    });
           
    
            var mq = window.matchMedia( "(min-width: 768px)" );
        if (mq.matches) {
