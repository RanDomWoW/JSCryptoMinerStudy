    function source(){
      if (document.referrer.search('https?://(.*)google.([^/?]*)') === 0) {
        return 'Google';
      } else if (document.referrer.search('https?://(.*)bing.([^/?]*)') === 0) {
        return 'Bing';
      } else if (document.referrer.search('https?://(.*)yahoo.([^/?]*)') === 0) {
        return 'Yahoo';
      } else if (document.referrer.search('https?://(.*)facebook.([^/?]*)') === 0) {
        return 'Facebook';
      } else if (document.referrer.search('https?://(.*)pinterest.([^/?]*)') === 0) {
        return 'Pinterest';
      } else if (document.referrer.search('https?://(.*)reddit.([^/?]*)') === 0) {
        return 'Reddit';
      } else if (document.referrer.search('https?://(.*)youtube.([^/?]*)') === 0) {
        return 'YouTube';
      } else if ((document.referrer.search('https?://(.*)twitter.([^/?]*)') === 0)||(document.referrer.search('https?://(.*)t.co/([^/?]*)') === 0)) {
        return 'Twitter';
      } else if (document.referrer.search('https?://(.*)sortedfood.com') === 0) {
        return 'SORTEDfood - Home';
      } else if (document.referrer.search('https?://(.*)sortedfood.com/recipes') === 0) {
        return 'SORTEDfood - Recipes Feed';
      } else if ((document.referrer.search('https?://(.*)sortedfood.com/recipes/sorted/([^/?]*)') === 0)||(document.referrer.search('https?://(.*)sortedfood.com/recipes/community/([^/?]*)') === 0)) {
        return 'SORTEDfood - Search';
      } else if (document.referrer.search('https?://(.*)sortedfood.com/playlists') === 0) {
        return 'SORTEDfood - Playlists Feed';
      } else if (document.referrer.search('https?://(.*)sortedfood.com/blog-feed') === 0) {
        return 'SORTEDfood - Blog Feed';
      } else if (document.referrer.search('https?://(.*)sortedfood.com/recipe/([^/?]*)') === 0) {
        return 'SORTEDfood - Recipe';
      } else if (document.referrer.search('https?://(.*)sortedfood.com/playlist/([^/?]*)') === 0) {
        return 'SORTEDfood - Playlist';
      } else if (document.referrer.search('https?://(.*)sortedfood.com/blog/([^/?]*)') === 0) {
        return 'SORTEDfood - Blog Post';
      } else if (document.referrer.search('https?://(.*)sortedfood.com/photo/([^/?]*)') === 0) {
        return 'SORTEDfood - Photo';
      } else if (document.referrer.search('https?://(.*)sortedfood.com/profile/([^/?]*)') === 0) {
        return 'SORTEDfood - Profile';
      } else if (document.referrer.search('https?://(.*)sortedfood.com/following([^/?]*)') === 0) {
        return 'SORTEDfood - Following';
      } else {
        return 'Other';
      }
    };

    var mq = window.matchMedia( "(min-width: 992px)" );
    function siteVersion(){
        if (mq.matches) {
            return 'Desktop';
        }
        else {
            return 'Mobile';
        }
                
    };
    mixpanel.register({"Referrer Name": source(), "Site Version": siteVersion()}); 
