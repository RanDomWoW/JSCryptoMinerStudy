        function updateFeed(page, showLoader) {
            $('#nextFeedPage').show();
            $('#endOfFeed').hide();
            
            var type = 'recipe';
            
            if(typeof($('#currentFeedState').data('categoryId')) != "undefined" && $('#currentFeedState').data('categoryId') !== null) {
                var categoryId = $('#currentFeedState').data('categoryId');
                
            } else {
                 var categoryId = '';
            };

            var url = '/feed/' + type + '/' + categoryId;

            Sorted.connection.internal.get(url,
                    {
                        page: page,
                        categoryId: categoryId
                    }, function (apiResponse) {

                if (apiResponse == $('#currentFeedState').data('prevPage')) {
                    $('#nextFeedPage').hide();
                    $('#endOfFeed').show();
                } else {
                    $('#feedContents').append(apiResponse);
                    $('#currentFeedState').data('prevPage', apiResponse);
                    roundDownFeed(3);
                }
            }
            , function (apiResponse) {
            }, showLoader);
        };

        $('#nextFeedPage a').click(function () {

            if ($('#currentFeedState').data('page') >= 1) {
                var page = $('#currentFeedState').data('page') + 1;
            } else {
                var page = 1;
            }

            $('#currentFeedState').data('page', page);

            updateFeed(page, false);

        });
      
