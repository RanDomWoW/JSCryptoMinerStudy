                $('#form-newser').submit(function (e) {
                    e.preventDefault();
                    $('.loading').fadeIn(300);
                    var email = $('#newsemail').val();
                    $.ajax({
                        url : 'https://www.daneshjooyar.com/wp-admin/admin-ajax.php',
