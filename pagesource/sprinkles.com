<script type="text/javascript">
  // mailing list
  $(document).ready(function(){
    $("#mailing-list-submit").click(function() {
      emailSubmit();
    });

    // also submit on Enter
    $('#email').keypress(function(e){
      if(e.which == 13) {
        e.preventDefault();
        emailSubmit();
      }
    });
    
    function emailSubmit() {
      // send to post
      $.post("/v3_scripts/email_register.php", $("#mailing-list").serialize(), function(resp) {
        $("#mailing-list-status").html(resp).show();
        if (resp == "thanks for registering") $("#email").val("");
      });
    }
  });
</script>

