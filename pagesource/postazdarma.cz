    <script>
        function saveSoutez(evt){
            console.log('save');    
            var email = document.forms["newsletter-validate-detail2"]["newsletterEmail"].value;
           /* var gender = document.forms["newsletter-validate-detail2"]["gender"].value;*/
            var xmlhttp = new XMLHttpRequest();
            xmlhttp.open("POST", "ajax/saveNewsletterEmail.php", true);
            xmlhttp.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
            xmlhttp.send("email="+email);
            xmlhttp.onreadystatechange = function() {
                if (this.readyState == 4 && this.status == 200) {
                    console.log('response '+this.responseText); 
                    if(this.responseText == '{"result":"ok"}'){
                        document.getElementById("modal-body").innerHTML = 'Děkujeme za přihlášení do soutěže.';
                    }
                    else if(this.responseText == '{"result":"allreadyexist"}'){
                        document.getElementById("modal-body").innerHTML = 'Váš email byl již přihlášen do soutěže.';
                    }
                    else{
                        document.getElementById("modal-body").innerHTML = document.getElementById("modal-body").innerHTML + '<br> Omlouváme se, nastala chyba při uložení. Zkuste to prosím za chvíli.'
                    }    
                        
                } 
            };  
            $.cookie("modalShow", 1, { expires: 3650 });  
            evt.preventDefault();
            return false;
        }
        
        function closeModalMozBug(){
            $('#myModal').modal("hide");
        }
    </script>
