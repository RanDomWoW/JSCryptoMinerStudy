// Le bouton Top avec le scrolling
$(function(){
	$('.top-link').click(function(){
		$('html, body').animate({
			scrollTop:$('body').offset().top
		}, 'slow');
		return false;
	});

	displayOrNotTopLink();
	$(window).scroll(function(){
		displayOrNotTopLink();
	});
	function displayOrNotTopLink() {
		posScroll = $(document).scrollTop();  
		if(posScroll >=150)  {
			$('#top').fadeIn();
		} else {
			$('#top').fadeOut();
		} 
			
	} 
});	



