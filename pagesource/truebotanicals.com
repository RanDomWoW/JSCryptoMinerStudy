  function setShowModal(){
    console.log('setShowModal');
    /* Show modal just the first time */
    if (localStorageSupported){
      if(typeof customer != "undefined"){
        /* User logged, don't show modal */
        window.localStorage.setItem('firstTime', false);
        window.localStorage.setItem('secondTime', false);
      } else {
        if(window.localStorage.getItem('firstTime') == "true" || window.localStorage.getItem('firstTime') == undefined){
          firstModal = new Foundation.Reveal($('#register-first-time'));
          firstModal.open();
          // trackModalOpen($('#register-first-time').data('title'));

          $('html').addClass('modal-opened');
          window.localStorage.setItem('firstTime', false);
          window.localStorage.setItem('secondTime', true);

        } else if(window.localStorage.getItem('secondTime') == "true" || window.localStorage.getItem('secondTime') == undefined){
          firstModal = new Foundation.Reveal($('#register-first-time'));
          firstModal.open();
          // trackModalOpen($('#register-first-time').data('title'));

          $('html').addClass('modal-opened');
          window.localStorage.setItem('firstTime', false);
          window.localStorage.setItem('secondTime', false);

        } 
      } 
    }

  }
  function setRegisterModal(){
    $('.no-thanks').on('click', function(e) {
      e.preventDefault();
      //$('#register').foundation('reveal', 'close');
      $('#register-first-time').foundation('close');
    })

    $('.close-reveal-modal').on('click', function(e){
      setTimeout(function(){
        $('.first-step-mobile').removeClass('hide-important');
        $('.second-step-mobile').removeClass('block-important');
        $('#register').removeClass('second-step-active');
        $('#register-first-time').removeClass('second-step-active');
        $('#register-promo').removeClass('second-step-active');
      }, 500);

    })
  }

  function setFormMobile(){
    $('.sign-up-btn').on('click', function(e){
      e.preventDefault();
      console.log('click');
      $('.first-step-mobile').addClass('hide-important');
      $('.second-step-mobile').addClass('block-important');
      $('#register').addClass('second-step-active');
      $('#register-first-time').addClass('second-step-active');
      $('#register-promo').addClass('second-step-active');
    })
  }
</script>
