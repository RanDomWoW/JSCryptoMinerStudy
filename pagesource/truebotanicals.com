		// Close after 2 seconds
		$('#alertModal').on('open.zf.reveal', function(){
			
		    setTimeout(function() {
	        $('#alertModal').foundation('close');
		    }, 3000);
	    

		});

		$('#alertModal').on('closed.zf.reveal', function(){
			// Reset text
			$(this).find('#alertContent').children().remove();
		});

	})

	function openAlert(text, delay_for_close){
		var modal = $('#alertModal');
		modal.find('.modal-content').empty().append(text);
		var popup = new Foundation.Reveal(modal);
		popup.open();

		if (delay_for_close) {
			setTimeout(function() {
				if ($('body').hasClass('is-reveal-open'))
					modal.foundation('close');
			}, delay_for_close);
		}

	}
</script>

  


