<script type="text/javascript">
  var _rsq = _rsq || [];
  _rsq.push(['_setSiteId', '460']); // replace site_id with your static Site ID
  _rsq.push(['_enableOnsite']);

  

  /*** EVENT SPECIFIC CODE ***/
  

  

  

  _rsq.push(['_track']);
  /*** END EVENT SPECIFIC CODE ***/

  /*** FUNCTION FOR AJAX TRACKING ***/
  var _rs_ajax = {};
  _rs_ajax.updateCart = function() {
    jQuery.getJSON('/cart.js', function (cart, textStatus) {
      for(var i = 0; i < cart.items.length; i++) {
        var cartItem = cart.items[i];
        var price = cartItem.price / 100;

        _rsq.push(['_addItem', { 'id':cartItem.id.toString(), 'name':cartItem.title.toString(),'price':price.toFixed(2).toString() }]);
      }
      _rsq.push(['_setAction', 'shopping_cart']);
      _rsq.push(['_track']);
    });
  };
  /*** END FUNCTION FOR AJAX TRACKING ***/

  (function() {
     var rScix = document.createElement('script');
     rScix.type = 'text/javascript';
     rScix.async = true;
     rScix.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'd1stxfv94hrhia.cloudfront.net/waves/v2/w.js';
     (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(rScix);
  })();
</script>
<script>
  //Forms Tracking (Search and Email forms )
  (function(){
    var searchTimeout;
    $('.search-input').on('keyup',function(evt){
     
      if(searchTimeout){
      	clearTimeout(searchTimeout);
        searchTimeout = null;
        
      }
      searchTimeout = setTimeout(function(){
      	  var searchElm = $(evt.target);
        if(searchElm.val() != ''){
        _rsq.push (['_setParams', { 'term': searchElm.val()}]);
        _rsq.push(['_setAction', 'search']) ;
        _rsq.push(['_track']);
        }
      },1500);
    });

    $('input[type=email]').on('change',function(evt){
      var emailElm = $(evt.target);
      var source = emailElm.siblings('input[name="source"]').val();
      if(emailElm.val() != '' && source != 'footer-newsletter' && source != 'popup-newsletter' ){
        if( 'index' == 'page.quiz') {
           md5Email = CryptoJS.MD5(emailElm.val()).toString();
           var rsObj = {'email':emailElm.val(), 'registration_source':'quiz_complete', 'record_id': md5Email};
          _rsq.push(['_setUserProperties', rsObj]);
          _rsq.push(['_setAction', 'email_entered']);
          _rsq.push(['_track']);
        }
      }
    });
      
    $('.email-signup-form').submit(function(e){
      e.preventDefault();
      rs_form = $(this);
      grecaptcha.execute();
    });  
  })();
  
  var rs_form ;
  var onSubmitfooter = function(gtoken){ 
    grecaptcha.reset();
    var form = rs_form;
    var email = rs_form.children('input[data-field="email"]').val();
    var name = rs_form.children('input[data-field="name"]') ? form.children('input[data-field="name"]').val() : '';
    var source =  rs_form.children('input[name="source"]').val();

    if(source == 'footer-newsletter' || source == 'popup-newsletter'){
      var settings = {
        "async": true,
        "crossDomain": true,
        "url": dom+'customers/subscribe_to_rs_email',
        "method": "POST",
        "headers": {
          "content-type": "application/x-www-form-urlencoded",
        },
        "data": {
          "email": email,
          "name": name,
          "source": source,
          "gcaptcha": gtoken
        }
      }
      $.ajax(settings).done(function (response) {
        if(response.success){
          _rsq.push(['_setUserEmail', response.email]);
          _rsq.push(['_setAction', 'email_entered']);
          _rsq.push(['_track']);
          if(rs_form.attr('id') == 'mc-embedded-subscribe-form'){
            openAlert('<p>Thank you for Subscribing.</p>');
          }
          rs_form[0].reset();
        }
      });
    }
  }
</script>
