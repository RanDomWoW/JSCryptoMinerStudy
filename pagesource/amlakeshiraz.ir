<script>
    var premin = "<select id=\"minprice\"type=\"text\"class=\"form-control\">";
    var premax = "<select id=\"maxprice\" type=\"text\"class=\"form-control\">";
    var postmin = "</select>";
    var postmax = "</select>";
    var priceText = "";

    function getPriceCollection(paramType) {
        $.ajax({
            type: 'GET',
            url: '/Cms/PriceText',
            data: { paramType: paramType },
            //contentType: "application/json; charset=utf-8",
            //dataType: "json",
            success: function(result) {
                switch (paramType) {
                case "1":
                {
                    document
                        .getElementById("mindiv")
                        .innerHTML = premin + "<option value=''>قیمت از</option>" + result.data + postmin;
                    document
                        .getElementById("maxdiv")
                        .innerHTML = premax + "<option value=''>قیمت تا</option>" + result.data + postmax;
                    break;
                }
                case "2":
                {
                    document
                        .getElementById("mindiv")
                        .innerHTML = premin + "<option value=''>رهن از</option>" + result.data + postmin;
                    document
                        .getElementById("maxdiv")
                        .innerHTML = premax + "<option value=''>رهن تا</option>" + result.data + postmax;
                    break;

                }
                case "3":
                {
                    document
                        .getElementById("mindiv")
                        .innerHTML = premin + "<option value=''>اجاره از</option>" + result.data + postmin;
                    document
                        .getElementById("maxdiv")
                        .innerHTML = premax + "<option value=''>اجاره تا</option>" + result.data + postmax;
                    break;
                }
                default:
                }

            }
        });
    }

    function searchchanged3(el) {
        document.getElementById("minprice").disabled = "";
        document.getElementById("maxprice").disabled = "";
        switch (el.value) {
        case "0":
        {
            $("#minprice").val("");
            $("#maxprice").val("");
            document.getElementById("minprice").disabled = "disabled";
            document.getElementById("maxprice").disabled = "disabled";
            $("#minprice_lbl").text("قیمت از");
            $("#maxprice_lbl").text("قیمت تا");
            break;
        }
        case "2":
        {
            getPriceCollection("2");

            $("#minprice_lbl").text("رهن از");
            $("#maxprice_lbl").text("رهن تا");

            break;
        }
        case "3":
        {
            getPriceCollection("3");

            $("#minprice_lbl").text("اجاره از");
            $("#maxprice_lbl").text("اجاره تا");
            break;
        }

        default:
        {
            getPriceCollection("1");

            $("#minprice_lbl").text("قیمت از");
            $("#maxprice_lbl").text("قیمت تا");

            break;

        }
        }

        BindChangeEvent();
    }

    function BindChangeEvent() {
        $('select, input')
            .change(function() {
                var val = $(this).val();
                if (val !== "" && val !== "0") {
                    $(this).addClass('sel');
                } else {
                    $(this).removeClass('sel');
                }
            });
    }
</script>
<script>
    function searchMvc() {
        //baseUrl = baseUrl + "?ag"
        var baseUrl = "";
        var province;
        var city = $("#City").val();
        var region;
        var estateType = $("#CmsSearchViewModel_EstateType").val();
        var sleepCount = $("#CmsSearchViewModel_SleepCount option:selected").val();
        var metraj = $("#CmsSearchViewModel_Metraj option:selected").index();
        var building = $("#CmsSearchViewModel_BuildingAge option:selected").val();
        var num = $("#Number").val();
        var date = "31";


        //جهت تایین منطقه یا محدوده
        if ($('#Region').val().replace(/^\s+|\s+$/g, "") !== "محدوده")
            region = $('#Region').val();

        //جهت مشخص کردن استان
        if ($("#Province").val() != null)
            province = $("#Province").val();


        //جهت مشخص کردن تاریخ
        if ($("#datefrom").val() != null)
            date = $("#datefrom").val();

        //جهت تایین قیمت ملک
        var searchType = $("#SearchType").val();
        var sumprice1 = "";
        var sumprice2 = "";
        var rahnprice1 = "";
        var rahnprice2 = "";
        var leaseprice1 = "";
        var leaseprice2 = "";
        var price1 = "";
        var price2 = "";

        //جهت تایین قیمت ملک ها
        if ($("#minprice").val())
            price1 = $("#minprice").val();
        if ($('#maxprice').val())
            price2 = $('#maxprice').val();
        if (searchType === "2") {
            rahnprice1 = price1;
            rahnprice2 = price2;
        } else if (searchType === "3") {
            leaseprice1 = price1;
            leaseprice2 = price2;
        } else {
            sumprice1 = price1;
            sumprice2 = price2;
        }


        //ایجاد Url
        var ag = querySt("ag");
        if (ag != null)
            baseUrl = baseUrl + "?ag=" + ag;

        if (province !== undefined)
            baseUrl += '&est=' + province;

        if (city !== '')
            baseUrl += '&ct=' + city;

        if (region !== '')
            //baseUrl += '&rgn1=' + region + 'منطقه';
            baseUrl += '&rgn1=' + region ;


        if (estateType !== '')
            baseUrl += "&mtp=" + estateType;

        if (searchType !== '' && $("#SearchType option:selected").index() > 0) {
            baseUrl += '&slvl=' + searchType;

        }

        if (rahnprice1 !== '' || rahnprice2 !== '')
            baseUrl += "&rhn1=" + rahnprice1 + "&rhn2=" + rahnprice2;
        if (leaseprice1 !== '' || leaseprice2 !== '')
            baseUrl += "&les1=" + leaseprice1 + "&les2=" + leaseprice2;
        if (sumprice1 !== '' || sumprice2 !== '')
            baseUrl += "&sprs1=" + sumprice1 + "&sprs2=" + sumprice2;

        if (sleepCount !== '')
            baseUrl += "&slp=" + sleepCount;

        if (building !== '')
            baseUrl += "&bld=" + building;

        if (metraj !== '' && $("#CmsSearchViewModel_Metraj option:selected").index() > 0)
            baseUrl += "&mt=" + metraj;

        if (num !== '')
            baseUrl += "&num=" + num;

        var s = "/cms/searchresult";
        if (baseUrl != "") {
            s = "/cms/searchresult?" + baseUrl;
        }
        window.location.href = encodeURI(s);

    }

</script>
