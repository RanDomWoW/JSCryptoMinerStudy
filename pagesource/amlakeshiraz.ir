    function CheckModelToLoad(listCount, id) {
        var model = parseInt(listCount);
        if (model === 0) {
            $("#" + id).hide();
        }
    }

    $(document)
        .ready(function() {
            checkModelCount();
            BindChangeEvent();
            $('.mani-page').addClass('active');
            $('#Number')
                .keyup(function() {
                    if ($("#Number").val().trim().length > 0) {
                        $('#Province').prop('disabled', true);
                        $('#Number').prop('disabled', false);
                        $("#City").prop('disabled', true);
                        $("#Region").prop('disabled', true);
                        $("#SearchType").prop('disabled', true);
                        $("#minprice").prop('disabled', true);
                        $("#maxprice").prop('disabled', true);
                        $("#CmsSearchViewModel_EstateType").prop('disabled', true);
                        $("#CmsSearchViewModel_SleepCount").prop('disabled', true);
                        $("#CmsSearchViewModel_Metraj").prop('disabled', true);
                        $("#CmsSearchViewModel_BuildingAge").prop('disabled', true);

                        $("#City").val("");
                        $("#Region").val("");
                        $("#SearchType").val($("#SearchType option:first").val());
                        $("#minprice").val($("#minprice option:first").val());
                        $("#maxprice").val($("#maxprice option:first").val());
                        $("#CmsSearchViewModel_EstateType")
                            .val($("#CmsSearchViewModel_EstateType option:first").val());
                        $("#CmsSearchViewModel_SleepCount")
                            .val($("#CmsSearchViewModel_SleepCount option:first").val());
                        $("#CmsSearchViewModel_Metraj")
                            .val($("#CmsSearchViewModel_Metraj option:first").val());
                        $("#CmsSearchViewModel_BuildingAge")
                            .val($("#CmsSearchViewModel_BuildingAge option:first").val());
                    } else {
                        $('#Number').prop('disabled', false);
                        $('#Province').prop('disabled', false);

                        $("#City").prop('disabled', false);
                        $("#Region").prop('disabled', false);
                        $("#SearchType").prop('disabled', false);
                        $("#minprice").prop('disabled', false);
                        $("#maxprice").prop('disabled', false);
                        $("#CmsSearchViewModel_EstateType").prop('disabled', false);
                        $("#CmsSearchViewModel_SleepCount").prop('disabled', false);
                        $("#CmsSearchViewModel_Metraj").prop('disabled', false);
                        $("#CmsSearchViewModel_BuildingAge").prop('disabled', false);
                    }
                });

            var h1 = document.getElementById("agency-name");
            var text = h1.textContent.split('');
            h1.textContent = '';

            for (var i = 0; i < text.length; i++) {
                if (text[i] != ' ') {
                    setTimeout(function(i) {
                            return function() {
                                return h1.textContent = text.slice(0, i + 1).join('');
                            };
                        }(i),
                        i * 0.2 * 600);
                }
            }
        });
