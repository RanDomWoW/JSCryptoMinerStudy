<script language="javascript">
	function popupQuotazioneRasBank(url, params ) {
  		domain      = "http://www.allianzbank.it"
		popupId = "popup";
  		localApriPopup(url, params, popupId, domain);
	}	
	
	function popupQuotazioneFondi(url, params ) {
  		domain  = "";
		popupId = "fondiAssicurativi";
  		localApriPopup(url, params, popupId, domain);
	}	
	
	function popup(url, params ) {
  		domain  = "";
		popupId = "Popup";
  		localApriPopup(url, params, popupId, domain);
	}		

	function popupGenerica(url, params ) {
  		domain   = "";
		popupId  = "PopupGenerica";
		urlPopup = "/popups?info=" + url;
  		
  		//localApriPopup(urlPopup, "\'" + params + "\'", popupId, domain);
  		localApriPopup(urlPopup, params, popupId, domain);
	}		

	function popupHtml(url, id, h, w ) {
		domain  = "";
  		params = "directories=0,status=0,left=0,top=0,width=" + w + ",height=" + h + ",menubar=0,scrollbars=1,toolbar=0,location=0,resizable=1";

		urlPopup = "/popups?info=" + url;
		localApriPopup(urlPopup, params, "popup", domain);
  		
	}

	function localApriPopup(url, params, nome, domain ) {
  	
  		browserVer  = parseInt(navigator.appVersion);
  		browserName = navigator.appName;
  		popupId     = nome;
  		if (browserName == "Netscape") {
  			if (browserVer >= 3) version = "n3";
    		else version = "n2";
  		}
  		else {
  			if (browserVer > 4) version = "n3";
    		else version = "n2";
  		}
  		newUrl = domain + url;
  		finestra=window.open(newUrl, popupId, params).focus();
  		
  		if (version == "n3") finestra.focus();  			
	}		
</script>




