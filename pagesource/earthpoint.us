    <script type="text/javascript">

        function OnTextFocus(x, TextValue) {

            if (x.value.toLowerCase() == TextValue.toLowerCase()) {
                x.value = "";
            }
        }

        //colors are also set by server validation
        function OnTextBlur(x, TextValue, Validation) {

            if (x.value == "") {
                x.value = TextValue;
                x.style.backgroundColor = "transparent";
            }
            else if (x.value.toLowerCase() == TextValue.toLowerCase()) {
                x.value = TextValue;
                x.style.backgroundColor = "transparent";
            }
            else if (Validation == "TestNumeric") {
                if (IsNumeric(x.value)) {
                    x.style.backgroundColor = "transparent";
                }
                else {
                    x.style.backgroundColor = "PaleTurquoise";
                }
            }
            else {
                x.style.backgroundColor = "transparent";
            }
        }

        //from http://www.codetoad.com/javascript/isnumeric.asp 
        function IsNumeric(sText) {

            var ValidChars = "0123456789.";
            var IsNumber = true;
            var Char;

            sText = sText.replace(/,/g, "");

            for (i = 0; i < sText.length && IsNumber == true; i++) {

                Char = sText.charAt(i);

                if (ValidChars.indexOf(Char) == -1) {
                    IsNumber = false;
                }
            }

            return IsNumber;

        }

    </script>
    
    
