            <img src="https://www.creatrip.net/static/images/logo.png"/>
            <meta itemprop="url" content="https://www.creatrip.net/static/images/logo.png">
            <meta itemprop="width" content="600">
            <meta itemprop="height" content="60">
        </div>
        <meta itemprop="name" content="creatrip">
    </div>
    <meta itemprop="datePublished" content="{{ngMeta.write_date}}"/>
    <meta itemprop="dateModified" content="{{ngMeta.write_date}}"/>
</div>

