<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html lang="ko">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
   <title>행정안전부 홈페이지</title>
</head>
<body>
	<style type="text/css">
		*{margin:0px; padding:0px; text-decoration:none; list-style:none; font-family:'Malgun Gothic';}
		html, body{height:100%;}
		.dg_bg_top{position:fixed;top:0;left:0;z-index:998;width:100%;height:395px; background:#1c71ad;}
		.dg_ct{position:relative;z-index:999;margin-top:-180px; padding-bottom:214px;padding-top:395px;}
		.dg_ct_bd{width:745px; margin:0 auto; padding-top:95px; padding-bottom:18px; background:#16669f url('./images/netfunnel/dg_icon1.png') no-repeat center 26px; border:1px solid #93b4ca;}
		.dg_ct_bd .p1{font-size:43px; color:#ffffff; font-weight:bold; margin-bottom:16px; padding-left:198px;}
		.dg_ct_bd .p1 span{font-size:43px; color:#fc8f02; font-weight:bold;}
		.dg_ct_bd .p2{padding-left:198px; font-size:17px; color:#ffffff; margin-bottom:5px;}
		.dg_ct_bd .p3{padding-left:198px; font-size:23px; color:#f3b702; font-weight:bold; margin-bottom:8px;}
		.dg_ct_bd .p4{padding-left:198px; font-size:17px; color:#ffffff; margin-bottom:28px;}
		.p_menu{width:696px; margin:0 auto;}
		.clear{clear:both; height:0px;}
		.p_menuc{float:left; width:25%; height:66px; display:table;}
		.p_menuc1{background:#459fd3;}
		.p_menuc2{background:#50bfba;}
		.p_menuc3{background:#f0a53a;}
		.p_menuc4{background:#4f996c;}
		.p_menuc p{display:table-cell; vertical-align:middle; text-align:center;}
		.p_menuc p a{font-size:19px; color:#fff; font-weight:bold;}
		@media screen and (max-width:770px){
			.dg_ct_bd{margin:0px 10px; width:calc(100% - 22px);}
			.dg_ct_bd .p1, .dg_ct_bd .p2, .dg_ct_bd .p3, .dg_ct_bd .p4{padding-left:10px; padding-right:10px; text-align:center;}
			.p_menu{width:calc(100% - 20px); margin:0px 10px;}
			.dg_bg_top{height:250px;}
			.dg_ct{padding-bottom:70px;padding-top:250px;}
		}
		@media screen and (max-width:678px){
			.p_menuc{width:50%;}
		}
		@media screen and (max-width:530px){
			.dg_ct_bd .p1{font-size:30px;}
			.dg_ct_bd .p2, .dg_ct_bd .p4{font-size:15px;}
			.dg_ct_bd .p3{font-size:20px;}
		}
		@media screen and (max-width:370px){
			.mo_br{display:none;}
		}
		@media screen and (max-width:340px){
			.p_menuc{width:100%;}
		}
	</style>
	<script type="text/javascript" src="/js/netfunnel/netfunnel.js" charset="UTF-8"></script>
	<script type="text/javascript" src="/js/netfunnel/test_skin.js" charset="UTF-8"></script>
	<script type="text/javascript">
	    window.onload = function(){
			NetFunnel_Action({action_id:"load"},"index.jsp");
		}
        //location.href="http://www.mois.go.kr/frt/a01/frtMain.do";
	</script>
