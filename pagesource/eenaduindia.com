	function getColor() {
        var color = '#004db3';
        var url = location.href.toLowerCase();
        var urlsection = url.split('/')[3];
		//alert(urlsection);
        if (urlsection != '' && urlsection != '#') {
            switch (urlsection) {
                case 'states':
                    color = '#007c72';
                    break;
				case 'state':
                    color = '#007c72';
                    break;
                case 'news':
                    if (url.split('/')[4] == 'national')
                        color = '#5a940c';
                    else if (url.split('/')[4] == 'international')
                        color = '#CE568F';
                    else if (url.split('/')[4] == 'business')
                        color = '#d28404';
                    break;
                case 'crimenews':
                    color = '#E8003E';
                    break;
				case 'crime':
                    color = '#E8003E';
                    break;
                case 'sports':
                    color = '#29B000';
                    break;
                case 'entertainment':
                    color = '#FF5C00';
                    break;
                case 'rainbow':
                    color = '#CE568F';
                    break;
                case 'foodnews':
                    color = '#007c72';
                    break;
                case 'evezonely':
                    color = '#ff3c5e';
                    break;
                case 'photos':
                    color = '#CE568F';
                    break;
                case 'indiasporanews':
                    color = '#2E459C';
                    break;
                case 'india':
                    color = '#FF5C00';
                    break;
				case 'iccworldcup':
					color = '#D14627';
					break;
                default:
                    color = '#2E459C';
                    break;
            }
        }
        else {
            color = '#2E459C';
        }
        return color;        
    }
