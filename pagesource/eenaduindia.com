                $('#footerclosebutton').css('display', 'block');
                //$('#scroll_top').fadeIn();
                if ($("#ShowAd").val() == "Footer") {
                    $('.bottomadd').fadeIn();
                }
                else if ($("#ShowAd").val() == "NoFooter") {
                    $('.bottomadd').hide();
                }
                if (document.body.scrollHeight - $(this).scrollTop() <= $(this).height()) {
                    $('.bottomadd').show();
                    $('#footerclosebutton').css('display', 'none');
                }
            }
            if ($(this).scrollTop() > 180) {
                $('#scroll_top').fadeIn();
            }
            else {
                $('#scroll_top').fadeOut();
            }            
        });
