<script type="text/javascript">			
	/*
		function to remove the compare footer image boxes and remove corresponding cookies
	*/	
	function remove_box(c_name,value,exdays) {
		setCookie(c_name,value,exdays);
		$('#cb'+value).attr('checked', false);
		$('#'+value).remove();

		if ( $('.cmp_footer').find('.cp_box').length == 0 ) {
			hideBar();
		}
	}


	/*
		function to add the items to the cmp_footer box
		also performs doubleChecks() to make sure that un-selected items are not shown
	*/
	function addToBox(id) {

		if ( id.length > 0 ) {
			if ( $('#'+id).length == 0 ) { // it doesn't exist

				if ($('.cp_box').length >= 10) {
					alert('Sorry, maximum of 10 products to compare at once. Please remove items to add more.');
					remove_box('compare',id,7);
					return false;
				}

				$.ajax({
					url: '/compare-dock-ajax.php',
					method: 'post',
					data: "id="+id,
					dataType: "json",
					success: function(data) {
						if ($('.cmp_footer').position().top > 0) {
							$('.cmp_footer').append("<span class='cp_box' id='"+id+"'><a class='boxclose' id='boxclose' onclick='remove_box(\"compare\"," +id+ ",7)'></a><a rel=\"nofollow\" href='"+data.products_link+"'><img src='/products-image/50/" +data.products_image+ "' alt='" +data.products_model+ "' title='" +data.products_name+ "' /></a></span>");	
						} else {
							$('.cmp_footer').slideDown(400);
							$('.under').css({'margin-bottom' : '25px'});
							$('.cmp_footer').append("<span class='cp_box' id='"+id+"'><a class='boxclose' id='boxclose' onclick='remove_box(\"compare\"," +id+ ",7)'></a><a rel=\"nofollow\" href='"+data.products_link+"'><img src='/products-image/50/" +data.products_image+ "' alt='" +data.products_model+ "' title='" +data.products_name+ "' /></a></span>");
						}
					}}).done(function(){
					doubleChecks();	
				});
			} else {
				doubleChecks();
			} 
		} 

		doubleChecks();	
				
	}


	/*
		CountChecks() function modified for the footer_compare form (cmp_footer section)
	*/
	function CountChecks2() {
		var maxchecked = 10;
		var count = 0;
		var ret = true;
		var ar_value = getCookie('compare').split("|");
		var count = ar_value.length;

		if(count > maxchecked) {
			alert('Sorry, you may only compare up to ' + maxchecked + ' items.');
			ret = false;
		}
		if(count < 2) {
			alert('You must select at least 2 items to compare.');
			ret = false;
		}
		if (ret) {
			document.footer_compare.submit(); 	
			return true;
		}			
				
	}


	/*
		click function for "clear all"
		also hides bar once cleared out
	*/	
	$('#clear_all').click(function(){
		var ar_value = getCookie('compare').split("|");
		$.each(ar_value, function(key, val) {
			if (val !== '') {
				remove_box('compare', val, 7);					
			} else {
				return true;
			}					
		});			
			
		hideBar();
	});



	/*
		click function for the show/hide tab
		also updates the text on the tab
	*/
	$('.tab').click(function() {
		var footerTop = $('.cmp_footer').position().top;
		if (footerTop > 0) {	// footer is not hidden

			if (footerTop < $(window).height()) {	// footer is on screen
				$('.cmp_footer').animate({
					bottom: "-65"
				}, 600, function() {
					$('#tab_text').text('Show');	
				});					
			} else if (footerTop > $(window).height()) {	// footer is below screen with only tab visible
				$('.cmp_footer').animate({
					bottom: "0"
				}, 600, function() {
					$('#tab_text').text('Hide');
				});						
			}
		}
	});

	/* 
		simple function to hide the compare footer bar
	*/
	function hideBar() {				
		$('.cmp_footer').slideUp(600); 	
		$('.under').css({'margin-bottom' : '0px'});	
	}


	/*
		double check that the items shown in the cmp_footer are still selected,
		if not remove them
	*/
	function doubleChecks() {				
		var ar_value = getCookie('compare').split("|");						// get all 'compare' cookies' values
		var in_box = $.map($('.cp_box'), function(a,b) { return a.id; }); 	// get all products in compare footer box

		$.each(in_box, function(key, val) {
			if ( $.inArray(val, ar_value) == -1) {
				// if it is in the box but not in cookie / selected
				$('#'+val).remove();
				if ( $('.cmp_footer').find('.cp_box').length == 0 ) {
					hideBar();
				}
			}

			// trying to prevent the duplicates that appear occassionally
			if ( $('span[id='+val+']').length > 1 ) {
				$('#'+val).remove();
				if ( $('.cmp_footer').find('.cp_box').length == 0 ) {
					hideBar();
				}	
			}
		});

	}


	/* 
		initiation function for subsequent pages / page loads 
	*/
	function cmp_init() {
		reset_right();														// set the left offset
		var ar_value = getCookie('compare').split("|");						// get all 'compare' cookies' values

		$.each(ar_value, function(key, val) {
			if (val !== '') {						// if the cookie isn't blank
				if ( $('#'+val).length == 0 ) {		// if the product is not already in the box
					addToBox(val);
				}
			}
		});
	}

	
	/* 
		function to keep the box aligned with the right edge of the content 
	*/
	function reset_right(){
		if ($(".contentbody")[0]){
		var new_right = ($(window).width() - ($('.contentbody').offset().left + $('.contentbody').outerWidth()));
		} else {
		var new_right = ($(window).width() - ($('#r_container').offset().left + $('#r_container').outerWidth()));
		}
		if (new_right > 0 ) {
			$('.cmp_footer').css({ 'right' : new_right+"px" });
		} else {
			$('.cmp_footer').css({ 'right' : "0px" });
		}
	};

	
	/* 
		update the right side position of the compare footer box 
	*/
	$(window).resize(function() {
		reset_right();
	});


</script>

