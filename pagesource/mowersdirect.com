    function find_recommends_list(the_category,id,stores_id) {
        $.ajax({
            type: "GET",
            url: "recommend_fetch.php?action=fetch_list&stores_id=" + stores_id + "&category_id=" + the_category,
            dataType: "json",
            success: function(data){
                $("#recModule" + id + " #rec_options").empty();
