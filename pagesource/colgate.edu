            function slideWordMark() {
                $(".logo").show();
                $("#mobileLogoImg").hide();
                $('#TopNavSeal').css("width", 45);
                if ((window.pageYOffset || document.documentElement.scrollTop) <= 60) {
                    window.scrollTo(0,60);
                }
            }
            $("#hamburgerMenu, #audience, #search").click(function () {
                slideWordMark();
            });
            if ($(window).width() < 749) {
                $(".logo").hide();
                $('#TopNavSeal').css("width", 67);
                $("#mobileLogo").show();
                $("#mobileLogoImg").show();
            }
            function checkBreak() {
                return (window.pageYOffset || document.documentElement.scrollTop) >= 15
            }
            function scrollImg() {
                if (($(window).width() < 749)) {
                    if (checkBreak()) {
                        $(".logo").show();
                        $("#mobileLogoImg").hide();
                        $('#TopNavSeal').css("width", 45);
                    } else {
                        $(".logo").hide();
                        $("#mobileLogoImg").show();

                        $('#TopNavSeal').css("width",65);

                    }

                }
            }
