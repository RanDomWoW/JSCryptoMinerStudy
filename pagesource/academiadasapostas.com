<script>
	$(document).ready(function(){
        if (!IS_MOBILE){
            $(".tipsy-active").tipsy({gravity: 's'}); 
        }

		$(".selectAllGames").on("change", function(){
			var self = $(this);
			var selfChecked = self.prop("checked");
			$(".livescores.large .my_games input").each(function(){
				if (selfChecked == $(this).prop("checked")){
					return;
				}
				$(this).prop("checked",selfChecked);
				$(this).click();
				$(this).prop("checked",selfChecked);
			});
		});
       
    });
    $(function(){
	    
	    if($('#i_am_a_stats_popup').length == 1){
        	LIVESCORES.is_popup = true;
        }
        
		if(LIVESCORES.is_popup == true){
			$('.competition-today tbody a').click(function(event){
				if(window.opener != null){
	        		event.preventDefault();
					window.parent.open($(this).attr('href')).focus();
				}
				else{
					$(this).attr('target', '_blank');
				}	
        	});
        	
			
			if(LIVESCORES.auto_window_height == true){		
				height = $('div.livescores').height() >= 220 ? $('div.livescores').height() + 80 : 300;
				window.resizeTo(680,height);
				
				LIVESCORES.window_heights[LIVESCORES.tab] = height;
				setTimeout(function(){
					LIVESCORES.auto_window_height = true;
				}, 400);
			}	
		}
		
		$('a.popup').click(function(){
			LIVESCORES.opened_from_parent = true;
			var cs = LIVESCORES.competitions_has_changed !== false && typeof(LIVESCORES.day_competitions_selected[LIVESCORES.ts]) !== 'undefined' ? LIVESCORES.day_competitions_selected[LIVESCORES.ts].join() : "";
			var parent_ts = LIVESCORES.ts != null ? LIVESCORES.ts : "0";
			var url = parent_ts == "0" && cs == "" ? "" : "/" + parent_ts + "/" + cs;
				 
		    window.open($(this).attr('href') + url,'','width=680, height=300, scrollbars=yes, resizable=yes');
		    return false;
    	});
	    
    	$(".livescores .tipsy-active").tipsy();
    	
    	//var count_my = 0;
    	var counter = $('a#show_tab2 h2 span');
    	$(counter).text('0');
    	
    	LIVESCORES.my_matches_changed = false;
		$('#fh_main_tab td.my_games input[type="checkbox"]').click(function(){
			
			var match_id = $(this).val();
			
			if($(this).prop('checked')){
				action = 'add';
			}
			else{
				action = 'remove';
			}

			$(this).toggleClass('changed');
			changed_matches = $(this).parents('table').find('td.my_games input[type="checkbox"].changed');
			if(changed_matches.length > 0)
				LIVESCORES.my_matches_changed = true;
			else
				LIVESCORES.my_matches_changed = false;
				
			LIVESCORES.my_matches_ids = [];
				
			$(this).parents('table').find('td.my_games input:checked').each(function(){
				LIVESCORES.my_matches_ids.push($(this).val());
			});
			
			$(counter).text(LIVESCORES.my_matches_ids.length);
			
			var tr = LIVESCORES.tab == '#fh_my_tab' ? $(this).parents('tr') : '';
			
			$.ajax({
				type: "GET",
				dataType: "json",
				url: "bookmarks/bookmark_match/" + $(this).val() + "/" + $(this).data('day_timestamp') + "/" + action,
				data: { }
