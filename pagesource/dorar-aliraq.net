<a id="showmore" style="display:none;text-align:center;text-decoration:underline;font-weight:bold;padding:10px;" href="#">إظهار الأقدم</a>
</div>
<script type="text/javascript">
var firstpost=false;
var newpostsarray=[];
var ajax_trys =0;
var window_focus=true;
var npt=0;
var org_title=document.title;
var TIMENOW=1521533286;
var TIMEAFTERNOW=TIMENOW;
var TIEMSINCE=0;



function insert_newposts(){
if(newpostsarray.length !=0){
jQuery(newpostsarray[newpostsarray.length-1]).hide().prependTo('#latest_posts').slideDown(600,function(){
jQuery(this).addClass('highlightyellow');
init_time();
window.setTimeout(function(){jQuery('.highlightyellow').removeClass('highlightyellow')},1000);
if(!window_focus){npt++;document.title='('+npt+') '+org_title};
}).find('.tiptip').tipTip({maxWidth: "auto" , defaultPosition: "top"});
$('#showmore').show();
if($('#showmore').data('more'))
{
$('.lpost:not(.showm)').last().slideUp(600).addClass('showm');
if($('.lpost').length>30)$('.lpost').last().remove();
}
else{
$('.lpost:not(.showm)').last().addClass('showm');
if($('.lpost').length>30) $('.lpost').last().slideUp(600,function(){$(this).remove();});
}



newpostsarray.splice(-1,1);
window.setTimeout(insert_newposts,10000);
}
else
window.setTimeout(getpostajax, 1000);
}

function getpostajax(){
firstpost=jQuery("#latest_posts>:first-child").attr('id').replace(/post_/, '');

$.get(BBURL+"/", { getnewposts : '1', newestpost : firstpost},
   function(data){   
     ajax_trys =0;
 if(data == null || data=='' || jQuery(data).filter('.lpost').length==0) window.setTimeout(getpostajax, 30000);
 else{
 newpostsarray=jQuery(data).filter('.lpost');
 insert_newposts();
 }
 
   },'html').error(function() {
ajax_trys++;
if(ajax_trys<6)
window.setTimeout(getpostajax,10000);	
});
}

function init_time(){
$('.ptime:not(.timedata)').each(function(){
var timeval=$(this).find('.timeval').html();
var timeunt=$(this).find('.timeunt').html();

if(timeval=='عدة' && timeunt=='ثواني') timeval=10;
if(timeval=='دقيقة' && timeunt=='واحدة') timeval=60;
if(timeunt=='دقيقة') timeval=60*timeval;
if(timeunt=='ساعة') timeval=60*60*timeval;
$(this).data('timeval',timeval);
$(this).addClass('timedata');
});
}

function init_display(){
$('.lpost.displaynone').last().slideDown(600,function(){
jQuery(this).addClass('highlightyellow');
window.setTimeout(function(){jQuery('.highlightyellow').removeClass('highlightyellow')},1000);
if(!window_focus){npt++;document.title='('+npt+') '+org_title};
}).removeClass('displaynone');
if($('.lpost.displaynone').length==0) window.setTimeout(getpostajax,10000);
else window.setTimeout(init_display,10000);
}

jQuery(function(){
window.setTimeout(init_display, 10000);

$('#showmore').data('more',true).click(function(){
if($(this).data('more')){$('.showm').slideDown();$(this).html('تقليص').data('more',false);}
else {$('.showm').slideUp();$(this).html('إظهار الأقدم').data('more',true);}
return false;
});

setInterval(function(){
$('.ptime.timedata').each(function(){
var timeval=$(this).data('timeval');
timeval=parseInt(timeval) +10;
$(this).data('timeval',timeval); //saveing for future

if(timeval <15) $(this).html('منذ عدة ثواني');
else if(timeval < 45) $(this).html('منذ 30 ثانية');
else if(timeval < 90) $(this).html('منذ دقيقة واحدة');
else if(timeval < 60*60) $(this).html( 'منذ '+Math.round(timeval/60)+' دقيقة');
else $(this).html('منذ '+Math.round(timeval/(60*60))+' ساعة');
});
}, 10000);

$(window).focus(function() {
    window_focus = true;
window.setTimeout(function(){document.title=org_title;npt=0;},400);
}).blur(function() {
