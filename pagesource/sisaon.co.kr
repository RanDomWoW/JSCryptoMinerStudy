<script language='javascript'>

	function selSection(firstSec, secondSec, serialCode, table,fold, this_page){
		topMenuObj = document.getElementById('topMenu').getElementsByTagName('img');
		subMenuObj = document.getElementById('subMenu').getElementsByTagName('table');

		topCnt = subMenuObj.length;
		for(i = 0; i < topCnt; i++){
			imgNo = i + 1;

			topMenuObj.item(i).setAttribute('src', '/image2017/sec' + imgNo + '.gif');
			subMenuObj.item(i).style.display = 'none';
		}

 		if( firstSec == 'S1N8'){
			topMenuObj.item(0).setAttribute('src', '/image2017/sec1_ov.gif');
			subMenuObj.item(0).style.display = 'inline';
		}else if(firstSec == 'S1N9' ){
			topMenuObj.item(1).setAttribute('src', '/image2017/sec2_ov.gif');
			subMenuObj.item(1).style.display = 'block';			
		}else if(firstSec == 'S1N12' || secondSec == "S2N29" || secondSec == "S2N30" || secondSec == "S2N31" || secondSec == "S2N32" || secondSec == "S2N33" || secondSec == "S2N34" || secondSec == "S2N35" || secondSec == "S2N36"){
			topMenuObj.item(2).setAttribute('src', '/image2017/sec3_ov.gif');
			subMenuObj.item(2).style.display = 'block';
		}else if(firstSec == 'S1N13' || secondSec == "S2N37" || secondSec == "S2N38" || secondSec == "S2N39" || secondSec == "S2N40" || secondSec == "S2N41"){
			topMenuObj.item(3).setAttribute('src', '/image2017/sec4_ov.gif');
			subMenuObj.item(3).style.display = 'block';
		}else if(firstSec == 'S1N14' || secondSec == "S2N42" || secondSec == "S2N43" || secondSec == "S2N44" || secondSec == "S2N45"){
			topMenuObj.item(4).setAttribute('src', '/image2017/sec5_ov.gif');
			subMenuObj.item(4).style.display = 'block';
		}else if(firstSec == 'S1N6'){
			topMenuObj.item(5).setAttribute('src', '/image2017/sec6_ov.gif');
			subMenuObj.item(5).style.display = 'block';
		}else if(firstSec == 'S1N7' || secondSec == "S2N18" || secondSec == "S2N19" || secondSec == "S2N20" || secondSec == "S2N21" || secondSec == "S2N23"){
			topMenuObj.item(6).setAttribute('src', '/image2017/sec7_ov.gif');
