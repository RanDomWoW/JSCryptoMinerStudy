<script>
	$(function(){
		if(pageData.pageType == "service" && pageData.subPageType == "contactus"){
			var $insertionPoint = $(".customer-service form ul"),
				$content = $(".kr-compliance-cs");
				
			$insertionPoint.after($content);
			
