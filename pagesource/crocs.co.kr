<script type="text/javascript">
//Newsletter script
$(document).ready(function() {
	initErrMsg();
	
	$(".home_nl_cont .home_nl_btn").click( function() {
		app.ajax.loader.start();
		var clickedBtn = $(this).parent().parent().attr("class");
		console&&console.log&&console.log("clickedBtn is : " + clickedBtn);
		if ( clickedBtn.indexOf("nl_AU") >=0 ) { nlLang = "en"; nlCtry = "AU"; } 
		else if ( clickedBtn.indexOf("nl_SG") >=0 ) { nlLang = "en"; nlCtry = "SG"; } 
		else if ( clickedBtn.indexOf("nl_CN") >=0 ) { nlLang = "zh"; nlCtry = "CN"; } 
		else if ( clickedBtn.indexOf("nl_KR") >=0 ) { 
			nlLang = "ko"; nlCtry = "KR"; 
			checkboxCheck = $(".nl_KR .home_nl_box div div.superCuteFormElemWrapper").attr("class");
			removeRedClear();
		} 
		else { console&&console.log&&console.log("language not specified"); }	
		console&&console.log&&console.log("language is : " + nlLang);
		console&&console.log&&console.log("country is : " + nlCtry);

		// if it is KR and it is unchecked, alert something
		if ( clickedBtn.indexOf("nl_KR") >=0 && checkboxCheck.indexOf("unchecked") >=0 ) {
			$(".home_nl_box div:first-child div").addClass("borderItRed");
			$(".nl_terms_hover").addClass("borderItRedBottom");
			$(".home_nl_box").addClass("colorItRed");
			$(".nl_" + nlCtry + " .home_nl_msg").text(window["nlErrUncheck_"+nlLang]); 
			console&&console.log&&console.log("KR && checkbox not checked");
			app.ajax.loader.end();
		} else {
			var email = $(".nl_" + nlCtry + " .home_nl_cont .nl_email").val();
			var eResult = validateEmail(email);
			if(email == "") {
				$(".nl_" + nlCtry + " .nl_email").addClass("borderItRed");
				$(".nl_" + nlCtry + " .home_nl_msg").text(window["nlErrNoEmail_"+nlLang]);
				console&&console.log&&console.log("empty e-mail address input");
				app.ajax.loader.end();
			} else {

				if( eResult == false ) {
					$(".nl_" + nlCtry + " .home_nl_cont .nl_email").addClass("borderItRed");
					$(".nl_" + nlCtry + " .home_nl_msg").text(window["nlErrInvEmail_"+nlLang]);
					console&&console.log&&console.log("invalid e-mail address");
					app.ajax.loader.end();
				} else {
					console&&console.log&&console.log("valid e-mail address. checking if registered e-mail address");
					var dwCtry = nlCtry.toLowerCase();
					omnitureEmailSignUp();
					var pipelineUrl = "/on/demandware.store/Sites-crocs_"+dwCtry+"-Site/"+nlLang+"_"+nlCtry+"/Newsletter-AJAXSubscribe" + "?subscribe=true&signupSource=homepageBlock&email=" + email;

					$.ajax({
						"type": "POST",
						"url": pipelineUrl,
						"success": function(content)
						{
							var json_obj = $.parseJSON(content);
							var checkEmailExist = (json_obj['existing']);
							if(checkEmailExist == true) {
									console&&console.log&&console.log("valid e-mail address but already registered.");
									$(".nl_" + nlCtry + " .nl_email").addClass("borderItRed");
									$(".nl_" + nlCtry + " .home_nl_msg").text(window["nlErrDupEmail_"+nlLang]);
									app.ajax.loader.end();
							} else {
								$(".nl_" + nlCtry + " .home_nl_cont .nl_email, " + ".nl_" + nlCtry + " .home_nl_btn, " + ".nl_" + nlCtry + " .home_nl_box").css("visibility","hidden");
								$(".nl_" + nlCtry + " .home_nl_msg").css("color","#fff");
								$(".nl_" + nlCtry + " .home_nl_msg").html(window["nlSuccesslMsg_"+nlLang]);
								console&&console.log&&console.log("no errors. successful registration.");
								app.ajax.loader.end();
							}
						}
					});						
					
				}
			}
		}

	});

	$(".home_nl_box, .nl_terms_hover").on('click', function(e) {
	  if (e.target !== this) {
		removeRedClear();
		return;
	  }
	  $(".home_nl_box .superCuteFormElem").click();
		checkboxCheck = $(".nl_KR .home_nl_box div div.superCuteFormElemWrapper").attr("class");
		if( checkboxCheck.indexOf("uncheck") >=0 ) {
			//do nothing
		} else {
			removeRedClear();
		}
	});


	$(".home_nl_cont .nl_email").keyup(function() {
		removeRedClear();
	});

	$(".home_nl_cont .nl_email").keypress(function(e) {
		var key = e.which;
		if(key == 13){ $(".home_nl_cont .home_nl_btn").click(); return false; }
	});

    $(".nl_popup").colorbox({ maxWidth: "90%", width: "800px", height: "800px", maxHeight: "90%", scrolling: true, iframe: false });
	
});

function validateEmail(email) { var re = /\S+@\S+\.\S+/; return re.test(email); }

function initErrMsg() {
	nlErrNoEmail_en = "Please enter your e-mail address";
	nlErrInvEmail_en = "Please enter a valid e-mail address";
	nlErrDupEmail_en = "e-mail address already registered";
	nlSuccesslMsg_en = "<u>You're successfully signed up!</u><br><br>You'll receive your coupon within 1 hr.<br>Please check your spam/junk folder<br> if you don’t receive anything.";
	nlErrNoEmail_ko = "이메일을 적어주세요";
	nlErrInvEmail_ko = "올바른 이메일을 적어주세요";
	nlErrUncheck_ko = "개인정보수집 및 이용방안에 동의 해주세요";
	nlErrDupEmail_ko = "이미 등록된 이메일주소";
	nlSuccesslMsg_ko = "<u>가입해주셔서 감사합니다</u>!<br>고객님 이메일로 쿠폰을 보내드리겠습니다.";
	nlErrNoEmail_zh = "请输入您的电子邮箱";
	nlErrInvEmail_zh = "电子邮箱不正确";
	nlErrDupEmail_zh = "注: 您的电子邮箱已注册";
	nlSuccesslMsg_zh = "<u>恭喜! 注册成功!</u>";
}
function removeRedClear() {
		$(".home_nl_box div:first-child div, .home_nl_cont .nl_email").removeClass("borderItRed").css("box-shadow","none");
		$(".nl_terms_hover").removeClass("borderItRedBottom");
		$(".home_nl_box").removeClass("colorItRed");
		$(".home_nl_msg").text("");
		$(".home_nl_box div:first-child div").removeClass("borderItRed");
}
</script>

