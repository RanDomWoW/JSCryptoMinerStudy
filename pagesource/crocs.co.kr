$(".apac-couponpopup-close").click(function() { $(".apac-couponpopup").addClass("closed"); localStorage.setItem("closedByUser","yes"); });

function storageCheck() {
	//console.log("running function storageCheck()");
	//localStorage.clear();
	srcStored = localStorage.getItem("srcVal");
	couponStored = localStorage.getItem("couponVal");
	if(srcStored) {
		//console.log("srcStored found : " + srcStored);
		//console.log("couponStored found : " + couponStored);
		
		// if stored data found, check expiry
		today = new Date();
		currentTime = today.getTime();
		startTimeStored = localStorage.getItem("startTimeStored");
		timeDifference = currentTime - startTimeStored;
		secondsUsed = (timeDifference / 1000 );
		//console.log("popup will expire if beyond 120 seconds");
		//console.log("running seconds difference. seconds used is : " + secondsUsed);
		
		if( secondsUsed < 3600 ) { // 3600 seconds is 1 hour
			//console.log("not yet expired...");
			var closedByUser = localStorage.getItem("closedByUser");
			if(closedByUser !== "yes") {
				//console.log("closedByUser was not triggered so display popup");
				apacpopup(srcStored,couponStored);
			} else if (window.location.href.indexOf("Cart-Show") > -1) {
				//console.log("you're in the Cart Page, so always display popup");
				apacpopup(srcStored,couponStored);
			} else {
				//console.log("closedByUser was triggered so DO NOT display popup");
			}
		} else {
			//console.log("already expired so DO NOT display popup even in Cart Page");
		}
	} else {
		//console.log("srcStored NOT found!");
	}

}

// function getUrlParameter() is stored in the sitewide CSS content-slot


</script>
