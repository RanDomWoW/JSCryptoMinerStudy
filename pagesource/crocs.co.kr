<script>
	app.accessibility.init = function() {
		// Adds an alt attribute to all images that don't already have one
		$("img").not("[alt]").attr("alt","");
		
		/**
		 * Remove empty heading tags (mostly found in content slots) to avoid validation errors.
		 * Note: Validation errors still occur when hiding the element or adding the aria-hidden attribute (tested with WAVE evaluation tool).
		 */
		$("h1,h2,h3,h4,h5,h6").not("header h1").each(function() {
			var $el = $(this);
			if ($.trim($el.text()).length == 0) {
				$el.remove();
			}
		});
		
		// Hide storefront toolkit iframe from screen readers
		$("#__DW__SFToolkit, #DW-SFToolkit").attr({
			"title": "empty",
			"role": "presentation",
			"tabindex": "-1",
			"aria-hidden": "true"
		});
		
		// accessibility-related qtip plugin default overrides 
		$.fn.qtip.defaults.show.event = 'mouseenter focusin';
		$.fn.qtip.defaults.hide.event = 'mouseleave focusout';
	}
</script>

 
	


	
