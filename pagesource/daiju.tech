	background: #12105e;
}
/*===============================
  13.見出し色(H2,H3)
===============================*/
.post h2{background: #008200;}
h2 {background: #008200;}
.post .fukidasi h2{background: #008200;}
.post .fukidasi h2:after{border-top: 15px solid #008200;}
.post .shadow h2{background: #008200;}
.post .ribbon h2{background: #008200;}
.post .ribbon h2::before,
.post .ribbon h2::after{border-top: 5px solid #008200;}
.post .ribbon h2::before{border-left: 5px solid #008200;}
.post .ribbon h2::after{border-right: 5px solid #008200;}
.post .under-gradline h2{color:#008200;}
.post .under-gradline h2:after{
	background: -moz-linear-gradient(to right, #008200, transparent);
	background: -webkit-linear-gradient(to right, #008200, transparent);
	background: linear-gradient(to right, #008200, transparent);
}
.post .first-big h2{color:#1a1a1a;}
.post .first-big h2:first-letter {
	color:#008200;
	border-bottom:solid 3px #008200;
}
.post .hamiwaku h2{
	border-top: solid 1px #008200;
	border-bottom: solid 1px #008200;
	color:#008200;
}
.post .hamiwaku h2:before,.post .hamiwaku h2:after{
	background-color: #008200;
}
.post .chapter h2{
	border:1px solid #008200;
	color:#008200;
}
.post .chapter h2::after{color: #008200;}
.post .icon h2 {background:#008200;}
.post h3 {
	border-color: #008200;
	color:#008200;
}
.post .h3underline h3 {
	border-color: #008200;
	color:#008200;
}
.post .thin-underline h3 {
	border-color: #008200;
	color:#008200;
}
.post .lh3 h3{
	border-color: #008200;
	color:#008200;
}
.post .stripes h3:after {
background: -webkit-repeating-linear-gradient(-45deg, #008200, #008200 0px, #fff 2px, #fff 2px);
background: repeating-linear-gradient(-45deg, #008200, #008200 0px, #fff 2px, #fff 2px);
}
.post .box2 h3{color: #008200;}
.post .box2 h3::before{color: #008200;}
.post .box2 h3::after{color: #A1B8D1;}
.post h4{	color:#008200;}
.post .trans-color h4{	color:#008200;}
.post .cube-icon h4{	color:#008200;}
.post .left-border h4{border-color: #008200;}
.post .left-thin-border h4{border-color: #008200;}
.textwidget h3{
	border-color: #008200;
	color:#008200;
}
#newposts h4.widgettitle {
background: #008200;
}
.sankaku { border-top: 30px solid #008200;}


