    <script>
        if (document.cookie.indexOf("MagiMetricsLandingPage") == -1)
        {
            create30DayCookie("MagiMetricsLandingPage", window.location.href);
        }

        function create30DayCookie(name,value) {
            var date = new Date();
            date.setTime(date.getTime()+2592000000);
            var expires = "; expires="+date.toGMTString();
            document.cookie = name+"="+value+expires+";domain=.magimetrics.com;path=/";
        }
    </script>


