                                                                <script type="text/javascript">
                    function rollsubordinate(obj,i){
                        var mth = 0;
                        var key = obj.height()/44 -1 -i;
                        if (key > 0) {
                            mth = -(i+1)*44;
                        }

                        obj.animate({
                            marginTop: mth,
                        }, 1000, function() {
                            if (key > 0) {i=i+1;}else{i=0;}
                            rollsubordinate(obj,i);
                        }).delay(3000);
                    }

                    $(function(){
                        $('.subordinate').each(function(i){
                            if ($(this).height() > 44) {
                                rollsubordinate($(this),0);
                            }
                        });
                    });
                </script>
