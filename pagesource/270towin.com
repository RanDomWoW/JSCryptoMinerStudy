    function set_candidate(party, name, id)
    {
        var old_name = $('#'+party+'_can').html();
        $('#'+party+'_can3').html(name);
        $('#'+party+'_name2').html(name);
        if (party == 'ind') $('#ind_name3').html(name);
        var bPopup = $('#'+party+'_candidates').bPopup();
        bPopup.close();
        
        if (old_name != name) {
            m[party+'_can'] = name;
            m[party+'_can_id'] = id;
            m.hashid = false;
            
            if (m.init_mapid == "") {
