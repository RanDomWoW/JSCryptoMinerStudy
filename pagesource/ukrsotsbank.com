		function cBookmarks (bmid,active) {
			this.active = active;
			this.bmID = bmid;
			this.root = document.getElementById(bmid);
			this.childs = [];
			this.init = function () {
				var j,i;
				if (this.root) {
					var elements = this.root.getElementsByTagName('div');
					if (elements) {
						j = 0;
						for (i in elements) {
							if (elements[i].style) {
								if (j == this.active) {
									elements[i].className = 'bmsel';
								} else {
									elements[i].className = '';
								}
								this.childs[j] = elements[i];
								j++;
							}
						}
					}
				}
			}

			this.activate = function (bmIndex) {

				if (this.childs[this.active]) {
					this.childs[this.active].className = '';
				}
				if (this.childs[bmIndex]) {
					this.childs[bmIndex].className = 'bmsel';
					this.active = bmIndex;
				}
			}
		}
		function cMmenu(id,active){
			this.bid = id;
			this.pid;
			this.active = active;
			this.bookmarks = new cBookmarks(this.bid,this.active);
			this.ppanels = [];
			this.root;
			this.pclass;

			this.init = function (id,pclass){
				this.bookmarks.init();
				this.pid = id;
				this.pclass = pclass;				
				this.root = $(id);
				var elements = this.root.getElementsByTagName('div');
				this.childs = [];
				if (elements) {
					j = 0;
					for (i in elements) {
						if (elements[i].className && (elements[i].className == this.pclass)) {
							this.childs[j] = new cPanels(elements[i],this.active);
							this.childs[j].initRoot(elements[i]);
							j++;
						}
					}

				}
			}
			this.activate = function (pIndex) {
				for (var i in this.childs) {
					this.childs[i].activate(pIndex);
				}
				this.bookmarks.activate(pIndex);
				if(window.pageLoaded && window.cutthroughbanners[mainmenuitems[pIndex]]){ //banners display none problem hardcode
					window.cutthroughbanners[mainmenuitems[pIndex]]();
					window.cutthroughbanners[mainmenuitems[pIndex]] = null;
				}
				setCookie("mbmsel",pIndex,false,"/");
			}
		}
		
		
				var mbmsel = getCookie("mbmsel");
		if (!mbmsel){
