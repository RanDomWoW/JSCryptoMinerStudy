$(function(){
  var $this = $(".scrollNews");
  var scrollTimer;
  $this.hover(function(){
     clearInterval(scrollTimer);
   },function(){
     scrollTimer = setInterval(function(){
       scrollNews( $this );
     }, 3000 );
  }).trigger("mouseleave");
});
function scrollNews(obj){
   var $self = obj.find("ul:first"); 
   var lineHeight = $self.find("li:first").width(); //获取宽度，向上滚动则需要获取高度.height()
   $self.animate({ "marginLeft" : -lineHeight +"px" }, 600 , function(){ //向左滚动，向上滚动则需要改为marginTop,其他方向类似，下同
         $self.css({marginLeft:0}).find("li:first").appendTo($self); //appendTo能直接移动元素
   })
};
    // 计算content的宽度
    var $con=$(".cell_3893_ .content");
    var item_w=$(".cell_3893_ .item").width();
	var len=$(".cell_3893_ .item").length;
	$con.css({width:item_w*len});
	// 初始化页数
	var page=1;
	// 下一页
	var $next=$(".carousel-next");
	$next.click(function(){
		if(page==len){
			$con.animate({left:0},500);
			page=1;
		}else{
			$con.animate({left:-item_w*page},500);
			page++;
		};
	});
	// 上一页
	var $prev=$(".carousel-prev");
	$prev.click(function(){
		if(page==1){
			$con.animate({left:'-='+item_w*(len-1)},500);
			page=len;
		}else{
			$con.animate({left:'+='+item_w},500);
			page--;
		};
	});
	// 封装函数
	function next(){
		if(page==len){
			$con.animate({left:0},500);
			page=1;
		}else{
			$con.animate({left:-item_w*page},500);
			page++;
		};
	};
	// 自动播放
	time=setInterval(next,5000);
	// 鼠标放到盒子上时停止
	$(".cell_3893_ .inner").hover(function(){
		clearInterval(time);
	},function(){
		time=setInterval(next,5000);
	});
	$(".item").each(function(){
		$(this).find("li").last().css('margin-right','0');
	});
