	height: 63px !important;
	position: absolute;
	top: 50% !important;
	margin-top:-31px !important;
	opacity:0.8;
	position:absolute !important;
	color:#fff;
}

.rev_slider_wrapper .tp-leftarrow:before{
	content:"\f104";
}

.rev_slider_wrapper .tp-rightarrow:before{
