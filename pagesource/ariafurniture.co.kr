		$(".visual_num em").eq(v_num).addClass("on");
		$(".visual div").fadeOut();
		$(".visual div").eq(v_num).fadeIn();
	});
	$(".visual_num em").click(function(){
		$(".visual_num em").removeClass("on");
		$(this).addClass("on");
		$(".visual div").fadeOut();
		$(".visual div").eq($(this).index()).fadeIn();
		v_num = $(this).index();
	});
});
