  <script>
    var head = document.getElementsByTagName('head')[0];

    /**
     *
     * sets up the script HTML object and returns it so we can add event listeners etc. to it
     *
     * @param string url the URL for the script src
     * @returns {HTMLElement}
     */
    function setupScript(url) {
        // Adding the script tag to the head
        var script = document.createElement('script');
        script.type = 'text/javascript';
        script.src = url;

        return script;
    }

    /**
     *
     * recursively load URLs in the order they are supplied
     *
     * @param array urls an array of URLs to load
     * @param int index where to start loading items from in the array
     */
    function loadUrls(urls, index) {
        if (urls[index]) {

            var script = setupScript(urls[index]);

            if(urls[index + 1]) {
                // bind the event to the callback function.
                // There are several events for cross browser compatibility.
                script.addEventListener('readystatechange', function(event) {
                    // yay for recursion!
                    loadUrls(urls, (index + 1));
                });
                script.addEventListener('load', function(event) {
                    // yay for recursion!
                    loadUrls(urls, (index + 1));
                });
            }

            // Fire the loading
            head.appendChild(script);
        }

    }

    var urls = [];
    // only load jquery if we need to
    if (!window.jQuery) {
        urls.push('/booking-form/jquery-1.11.3.min.js');
        urls.push('/booking-form/jquery-ui-1.10.4.min.js');
    } else {
        // otherwise load scripts excluding jquery
        if (!window.jQuery.ui) {
            urls.push('/booking-form/jquery-ui-1.10.4.min.js');
        }
    }
    loadUrls(urls, 0);

</script>
