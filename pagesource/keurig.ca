.ir__button {
    color: #FFF;
    background-color: #FFAF11;
    border: 1px solid #db9623;
    white-space: nowrap;
    text-align: center;
    text-transform: uppercase;
    text-decoration: none;
    font-family: 'Reader Bold', Helvetica;
    font-size: 14px;
    font-weight: 800;
    display: inline-block;
    padding: 10px 20px;
    -webkit-transition: 300ms;
    -moz-transition: 300ms;
    -ms-transition: 300ms;
    -o-transition: 300ms;
    transition: 300ms
}

.ir__button:hover,
.ir__button:focus,
.ir__button:active {
    cursor: pointer;
    background-color: #db9623;
    color: #FFF;
    text-decoration: none
}

.ir .ir__bluelink {
    color: #336699
}

.ir .ir__bluelink:hover {
    color: RGB(54, 119, 183)
}

