<script type="text/javascript">
	  $( document ).ready(function() {
	      $('.main-menu > li').each(function(e) {
	        var self = $(this);
	        var subnavSelector = $(this).data('subnav');
	      
	        var contentSubNav = "";
	
	        if((typeof(subnavSelector) != "undefined") && (subnavSelector != "")) {
	          contentSubNav = $('.content-' + subnavSelector);
	          
	          if((typeof(contentSubNav) != "undefined") && (contentSubNav != "")) {
	            $(self).append($(contentSubNav));
	          }
	        }
	      });
	
	      $('.main-menu > li').hover(function(e) {
	        $('body').addClass("body-shadow");
	      }, function() {
	        $('body').removeClass("body-shadow");
	      });
	
	     $(document).bind('touchstart', function(e) {
	       $('.main-nav > ul > li > a').click(function(e) {
	         e.preventDefault;
	         return false;
	       });
	
	        var isMenuClick = false;
	        var parentsEls  = $(e.target).parents();
	        
	        parentsEls.each(function(e) {
	          if($(this).hasClass('main-nav')) {
	            isMenuClick = true;
	            return;
	          }
	        });
	        
	        if(!isMenuClick) {
	          $('.main-nav > ul > li').removeClass('hover');
	        }
	    });
	    
	    $('.main-nav > ul > li').bind('touchstart', function(e) {
	      if($(this).hasClass('hover')) {
	        $(this).removeClass('hover');
	        return true;
	      } else {
	        e.preventDefault();
	        $('.main-nav > ul > li').removeClass('hover');
	        $(this).addClass('hover');
	      }
	    });
	  });
	</script>
