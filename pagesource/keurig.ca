    .ir__banner_desk {
        background: url("https://s7d4.scene7.com/is/image/keurig/4a-vh-desktop-en?id=eY9qt0&wid=1465&hei=712&fmt=jpg");
        padding:0;
        font-family: "Reader", "sans-serif";
    }
    
    .ir__banner_desk div{

        color:#fff;
        text-align: left;
            padding-left: 40px;
    padding-top: 40px;
    }

     .ir__banner_desk div h2{
        font-size: 18px;
     }

      .ir__banner_desk div h3{
        text-transform: capitalize;
        font-weight: 600;
        font-size: 40px;
        padding-right: 400px;
        margin-bottom: 5px;
        letter-spacing: 1px;
      }

      .ir__banner_desk div p{
        padding-right: 350px;
        line-height: 24px;
        font-size:14px;
              }

      .ir__banner_desk div a{
        -webkit-transition: 300ms;
    -moz-transition: 300ms;
    -ms-transition: 300ms;
    -o-transition: 300ms;
    transition: 300ms;
    background: #ffaf11;
    color: #fff;
    display: inline-block;
    font-family: "Reader Bold", "sans-serif";
    font-size: 14px;
    font-weight: bold;
    text-align: center;
    text-decoration: none;
    text-transform: uppercase;
    font-style: normal;
    padding: 12px;
    width:170px;
    margin-top: 10px;
      }

      .ir__banner_desk div a:hover{
        background: #db9623;
    color: white;
    }

    /*end*/
    .ir .ir__banners__row1 div.ir__banners-blockContainer:nth-child(2) {
        display: none
    }
    /*
    .ir .ir__banners {
        background-image: url("https://s7d4.scene7.com/is/image/keurig/k84h_white_wood_bk?scl=1");
        background-position: 0;
        background-size: cover
    }*/
    .ir .ir__banners .ir__banners__row1 div:nth-child(2) {
        display: none !important
    }
    .ir .ir__banners .ir__banners__row2 {
        margin-top: 30px
    }
    .ir .ir__banners .ir__banners__text div h2 {
        font-size: 1.11111em
    }
    .ir .ir__banners .ir__banners__blocks--2345 {
        margin-top: 0
    }
    .ir .ir__banners__block1 {
        height: 372px;
        margin-bottom: 30px
    }
    .ir .ir__banners__block1 div {
        height: 100%;
        margin-bottom: 30px;
        background: url(https://s7d4.scene7.com/is/image/keurig/4a-vh-desktop-en?id=eY9qt0&wid=1465&hei=712&fmt=jpg);
        background-size: cover;
        background-position-x: -16px
    }
}

@media only screen and (min-width: 992px) {
    
    .ir__banner_desk div h3{
        font-size:40px;
    }

    .ir__banner_desk div p{
        padding-right: 250px;
      }

    .ir .ir__banners {
        background-position: 0 -485px
    }
    .ir .ir__banners .ir__banners__row1 div:nth-child(2) {
        display: block !important
    }


    .ir .ir__banners__block1 {
        height: 1px;
        min-height: auto
    }
    .ir .ir__banners__block1 div {
        margin-bottom: 0px;
        background-position-x: 0px
    }
}
@media only screen and (min-width: 1024px) {
.ir__banner_desk div h3{
    font-size: 35px;
}
}



@media only screen and (min-width: 1200px) {
        
    .ir__banner_desk div{
        /*padding-top: 55px;*/
    }

     .ir__banner_desk div h3{
        font-size:45px;
    }

    .ir__banner_desk div p{
        padding-right: 340px;
    
      }

    .ir .ir__banners__block1 div {
        background-position-x: 0px
    }
}

