<script type="text/javascript">
var product_list;
	var tealiumTagCall = function(){

		<!-- Grab product data from carousels -->
		if (typeof ids !== 'undefined' && ids) utag_data.gaImpPID = ids;
		if (typeof names !== 'undefined' && names) utag_data.gaImpPName = names;
		if (typeof product_base !== 'undefined' && product_base) utag_data.gaImpBase = product_base;
		if (typeof brands !== 'undefined' && brands) utag_data.gaImpPBrand = brands;
		if (typeof categories !== 'undefined' && categories) utag_data.gaImpPCat = categories;
		if (typeof variants !== 'undefined' && variants) utag_data.gaImpPVariant = variants;
		if (typeof positions !== 'undefined' && positions) utag_data.gaImpPPos = positions;
		if (typeof prices !== 'undefined' && prices) utag_data.gaImpPPrice = prices;
		if (typeof impressionList !== 'undefined' && impressionList) {utag_data.gaImpPList = impressionList;	product_list=impressionList;}
		if (typeof impressionList === 'undefined' && typeof ids !=='undefined') {
			var impressionPage = (typeof pageType !== 'undefined' && pageType) ? pageType : uri;
			var impressionList_n = [ids.length];
			for (var i = 0 ; i < ids.length; i++) {
				
				impressionList_n[i] = impressionPage;
			}
			utag_data.gaImpPList = impressionList_n;
			product_list=impressionList_n;
		}

		var pageUrl = window.location.search.substring(1);
		var urlParameters = pageUrl.split('&');

		for (var i = 0; i <urlParameters.length; i++) {
			var param = urlParameters[i].split('=');
			if (param[0] == 'utm_medium') {
				utag_data.gaTransAff = param[1];
			}
		}	
	};
	tealiumTagCall();
	</script>


