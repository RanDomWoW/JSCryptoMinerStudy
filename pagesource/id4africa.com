					return this.canonicalLinkMap[i][this.ENGLISH_INDEX];
				}
			}
		},
		isEnglish : function(canonicalLink) {
			for(var i=0; i<this.canonicalLinkMap.length; i++) {
				if(this.canonicalLinkMap[i][this.ENGLISH_INDEX] === canonicalLink) {
					return true;
				}
			}
			return false;
		}
