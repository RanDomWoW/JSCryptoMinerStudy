		assignIdsToLanguageLinks : function() {
			var anchorElements = document.getElementsByTagName('a');
			
			for(var i=0; i<anchorElements.length; i++) {
				var thisAnchor = anchorElements[i];

				if(!thisAnchor.id) {
					var childrenNodes = thisAnchor.childNodes;
					for(var j=0; j<childrenNodes.length; j++) {
						var childNode = childrenNodes[j];
						
						if(childNode.nodeName === "IMG") {
							var imgSrc = childNode.src;
							
							if(imgSrc.indexOf("/wp-content/uploads/2016/04/french.png") != -1) {
								thisAnchor.id = "french_lanugage_id";
							}
							else if(imgSrc.indexOf("/wp-content/uploads/2016/04/english.png") != -1) {
								thisAnchor.id = "english_lanugage_id";
							}
						}
					}
				}
			}			
		},
		alterLanguageLinks : function() {
			var curCanonicalUrl = window.location.pathname;			
			
			if(!!curCanonicalUrl) {
				while(curCanonicalUrl.charAt(0) === '/') {
				    curCanonicalUrl = curCanonicalUrl.substr(1);
				}

				var canonicalArray = curCanonicalUrl.split("/");

				if(canonicalArray.length == 0) {
					return;
				}

				curCanonicalUrl=canonicalArray[0];
				
				var translatedCanonicalUrl = this.translateCanonicalLink(curCanonicalUrl);
				var frenchLink = document.getElementById("french_lanugage_id");
				var englishLink = document.getElementById("english_lanugage_id");
				var hrefBase = "/index.php/";
				
				if(!!translatedCanonicalUrl) {
				
					if(this.isEnglish(curCanonicalUrl)) {
						if(!!frenchLink) {
							frenchLink.href=hrefBase+translatedCanonicalUrl+"/";
						}

						if(!!englishLink) {
							if(curCanonicalUrl == "") {
								englishLink.href=hrefBase;
							}
							else {
								englishLink.href=hrefBase+curCanonicalUrl+"/";
							}
						}
					}
					else {
						if(!!englishLink) {
							if(translatedCanonicalUrl == "") {
								englishLink.href=hrefBase;
							}
							else {
								englishLink.href=hrefBase+translatedCanonicalUrl+"/";
							}
						}

						if(!!frenchLink) {
							frenchLink.href=hrefBase+curCanonicalUrl+"/";
						}
					}
				}
			}
		},
		translateCanonicalLink : function(canonicalLink) {
			for(var i=0; i<this.canonicalLinkMap.length; i++) {
				if(this.canonicalLinkMap[i][this.ENGLISH_INDEX] === canonicalLink) {
					return this.canonicalLinkMap[i][this.FRENCH_INDEX];
				}

