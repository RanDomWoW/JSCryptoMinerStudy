var IDVideoOpen = 0;
var booNext		= -1;
var booPrev		= -1;

function fnShowPlan(pIDVideo) {
	clearInterval(timer);
	booReload = 0;
	if(pIDVideo != IDVideoOpen){	
		$("#VI" + IDVideoOpen).show();
		$("#VP" + IDVideoOpen).css("color", "#000000");
		
		$("#VI" + pIDVideo).hide();
		$("#VP" + pIDVideo).css("color", "#00be61");

		IDVideoOpen = pIDVideo;
		
		$.ajax({
			type: "POST",
			url: "ajax-get-video-hp.asp",
			data: "IDVideo=" + pIDVideo,
			success: function(returnText) {
				$("#videoContainer").html(returnText);
				$("#PlayerVON").show();
				
				if($("#slidePrev").css("display") == "block"){
					$("#slidePrev").hide();
					if(booPrev == -1){
						booPrev = 1;
					}
				}else{
					if(booPrev == -1){
						booPrev = 0;
					}
				}
				
				if($("#slideNext").css("display") == "block"){
					$("#slideNext").hide();
					if(booNext == -1){
						booNext = 1;
					}
				}else{
					if(booNext == -1){
						booNext = 0;
					}
				}
			},
			error: function(XMLHttpRequest, textStatus, errorThrown) {
				alert('ajax-get-video-hp.asp - Errore in fase di visualizzazione');
			}
		});
	}
}

