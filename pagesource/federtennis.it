		$('#TribunaPwd').focus();
		alert("Attenzione !\n inserire la password scelta in fase di registrazione");
		return false;
	}

	return true;
}

function fnTrapKeyLogin(evt) {
	var evt = (evt) ? evt : ((event) ? event : null);
	if (evt.keyCode == 13) {
		fnLogin();
	}
	/*
	if (window.event)
		key = window.event.keyCode;     //IE
	else
		key = e.which; 				//firefox

	if (key == 13) {
		fnLogin();
		//return false;
	}
	*/
}

function fnLogin() {
	if (fnCheckTribunaLogin()) {
		//AjaxLoginmyFITScore("TribunaEmail=" + escape(strEmail) + "&TribunaPwd=" + escape(strPwd) + "&Provenienza=FITAgenda");
		$.ajax({
			type: "POST",
			url: "AjaxGetDataLogin.asp?" + new Date().getTime(),
			data: "TribunaEmail=" + escape($('#TribunaEmail').val()) + "&TribunaPwd=" + escape($('#TribunaPwd').val()),
			success: function(returnText) {
				if (returnText == "OK") {
