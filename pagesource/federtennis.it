var currentPosition = 0;
var slideWidth = 640;
var slides = "";
var numberOfSlides = "";

var booReload	= 1;
var timer = null;

function slideNext(){
	currentPosition = currentPosition+1;
	
	if(currentPosition > numberOfSlides-1){
		currentPosition = 0;
	}
	
	manageControls(currentPosition);

	$('#slideInner').animate({
	  'marginLeft' : slideWidth*(-currentPosition)
	});
}

function manageControls(position){
	if(position==0){ 
		$('#leftControl').hide();
		$('#slidePrev').hide();
	}else{
		$('#leftControl').show();
		$('#slidePrev').show();
	}
	
    if(position==numberOfSlides-1){
		$('#rightControl').hide();
		$('#slideNext').hide();
	}else{
		$('#rightControl').show();
		$('#slideNext').show();
	}
	
	if(position == 0){
		$("#imgNext").attr("src", arrNF[position+1]["Immagine"]);
	}else  if(position==numberOfSlides-1){
		$("#imgPrev").attr("src", arrNF[position-1]["Immagine"]);
	}else{
		$("#imgPrev").attr("src", arrNF[position-1]["Immagine"]);
		$("#imgNext").attr("src", arrNF[position+1]["Immagine"]);
	}
	
    if(arrNF[position]["IDVideo"] > 0){
		$("#PPVideo").show();
    }else{
		$("#PPVideo").hide();
    }
    
    if(arrNF[position]["FileAudio"] != "KO"){
		$("#PPAudio").show();
    }else{
		$("#PPAudio").hide();
    }
    
    if(arrNF[position]["IDCategoriaGalleria"] >  0){
		$("#PPImg").show();
    }else{
		$("#PPImg").hide();
    }
    
    if(arrNF[position]["Commenti"] >  0){
		$("#PPComm").html(arrNF[position]["Commenti"]);
		$("#PPComm").show();
	}else{
		$("#PPComm").hide();
	}
}

