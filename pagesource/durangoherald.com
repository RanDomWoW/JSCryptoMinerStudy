<script>
  function addCookieInterest(interest_type, interest_name){
    switch(interest_type){
      case "Tag":
        tag_interests[interest_name] = -~tag_interests[interest_name]
        break;
      case "Column":
        column_interests[interest_name] = -~column_interests[interest_name]
        break;
      case "Profile":
        profile_interests[interest_name] = -~profile_interests[interest_name]
        break;
      case "Category":
        category_interests[interest_name] = -~category_interests[interest_name]
        break;
      default:
        console.log("Missing interest for: "+interest_type);
        break;
    }
  }
</script>


    <!-- Start CircSmart Paywall -->
  <script type="text/javascript">
    var valid_user = false;
    var viewsLeft;
    var d      = new Date();
    var days   = ['Sun','Mon','Tue','Wed','Thu','Fri','Sat'];
    var day    = days[d.getDay()];
    var months = [ "January", "February", "March", "April", "May", "June",
               "July", "August", "September", "October", "November", "December" ];
    var month  = months[d.getMonth()]
    function clearUser(){
      _bwtp.clearID(function (f) {
        log_gm_event(["CircStore", "Cookies", "Remove"]);
        Cookies.remove('DH_subscriber', {path: ".durangoherald.com"})
        updateLoginLinks(false);
      });
    }
    function loginUser(){
      _bwtp = new TheWall('https://subscriptions.durangoherald.com/BWTW/', 'DH', 'DH', null, 'Login');
      _bwtp.init(true);
      _bwtp.status(function (f) {
        statusValid('Login');
      });
    }
    function statusValid(api_call){
      if (Cookies.get('DH_subscriber') !== undefined){
        log_gm_event(["CircStore", "Cookies", "Check"]);
        var subscriber = JSON.parse(Cookies.get('DH_subscriber'));
        var product = $.grep(subscriber.GrantProducts, function(e){ return e.Product === "DH"; })[0];
        if(product[day]){
          updateLoginLinks(true);
          if(api_call != "Any"){
            log_gm_event(["CircStore", "Cookies", "Valid"]);
            log_gm_event(["CircStore", "Status"+api_call, "Valid"]);
          }
          return true;
        }
      }
      if(_bwtp.updated != null && _bwtp.updated.userInfo != null){
        var product = $.grep(_bwtp.userStatus().GrantProducts, function(e){ return e.Product === "DH"; })[0];
        if(product[day]){
          Cookies.set('DH_subscriber', JSON.stringify(_bwtp.userStatus()), {path: ".durangoherald.com", expires: 30});
          updateLoginLinks(true);
          if(api_call != "Any"){
            log_gm_event(["CircStore", "Cookies", "Set"]);
            log_gm_event(["CircStore", "Status"+api_call, "Valid"]);
          }
          return true;
        }
      }
      if(api_call != "Any"){
        log_gm_event(["CircStore", "Status"+api_call, "Fail"]);
      }
      return false;
    }
    function updateLoginLinks(valid){
      if(valid){
        valid_user = true;
        log_gm_event(["CircStore", "Login", "Success"]);
        $("#paywall-header-link").off('click');
        $("#paywall-header-link").html('<i class="fa fa-user"></i>');
        $('#paywall-header-link').attr('data-open','paywall-modal')
        $('#paywall-header-link').attr('data-ga-event','["Navigation","Account","Fly-out-nav"]')
        $('#paywall-header-link').on('click', function(){
          log_gm_event($(this).data("ga-event"));
        });

        $("#paywall-side-menu-link").off('click');
        $("#paywall-side-menu-link").html('<i class="fa fa-user"></i>Account');
        $('#paywall-side-menu-link').attr('data-open','paywall-modal')
        $('#paywall-side-menu-link').attr('data-ga-event','["Navigation","Account","Fly-out-nav"]')
        $('#paywall-side-menu-link').on('click', function(){
          log_gm_event($(this).data("ga-event"));
        });
        notice_success = '<p class="alert alert-success">';
        notice_success += "You are now logged in.";
        notice_success += "</p>";
        if($('#paywall-notice').html() !== undefined && $('#paywall-notice').html().indexOf("article page") !== -1){
          $('#paywall-notice').html(notice_success);
        }
        setUserModalContent();
        _bwtp.clean();
      }else{
        log_gm_event(["CircStore", "Logout", "Success"]);
        $("#paywall-header-link").on('click', loginUser);
        $("#paywall-header-link").html('<i class="fa fa-sign-in"></i>');
        $('#paywall-header-link').removeAttr('data-open')
        $('#paywall-header-link').data('data-ga-event','["Navigation","Login","Fly-out-nav"]')
        $('#paywall-header-link').on('click', function(){
          log_gm_event($(this).data("ga-event"));
        });

        $("#paywall-side-menu-link").on('click', loginUser);
        $("#paywall-side-menu-link").html('<i class="fa fa-sign-in"></i>Login');
        $('#paywall-side-menu-link').removeAttr('data-open')
        $('#paywall-side-menu-link').data('data-ga-event','["Navigation","Login","Fly-out-nav"]')
        $('#paywall-side-menu-link').on('click', function(){
          log_gm_event($(this).data("ga-event"));
        });
      }
    }

    $(document).ready(function() {
      $("#paywall-side-menu-link").on('click', loginUser);
      $("#paywall-header-link").on('click', loginUser);
      _bwtp = new TheWall('https://subscriptions.durangoherald.com/BWTW/', 'DH', 'DH', null, "Any");
      _bwtp.init(true);
      _bwtp.status(function (f) {
        statusValid('Any');
      });
    });

  window.addEventListener('message', function(event) {
    if (~event.origin.indexOf('https://subscriptions.durangoherald.com')) {
      // The data has been sent from your site
      if(event.data == "42"){
        _bwtp.clean();
        loginUser();
      }
    }
  });
  </script>
  <!-- End CirsSmart Paywall -->


