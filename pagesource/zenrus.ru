<div id="container" data-vide-options="muted: true, volume: 0.02">
    <div class="fullscreen-layer backgrounds-layer"></div>
    <div class="fullscreen-layer main-data-layer">
        <div class="layout">
            <div class="stacks">
                <div class="stack reserves-stack">
                    <div class="item">
                        <div class="note additional zvr"><i></i><abbr title="Золотовалютные резервы">ЗВР</abbr>: <span
                                class="zvr-val"></span> млрд долларов
                        </div>
                    </div>
                    <div class="item">
                        <div class="note additional reserve"><i></i><abbr title="Резервный фонд">Резервный фонд</abbr>: <span
                                class="reserve-val"></span> млрд долларов
                        </div>
                    </div>
                    <div class="item">
                        <div class="note additional national"><i></i><abbr title="Фонд национального благосостояния">ФНБ</abbr>:
                            <span class="national-val"></span> млрд долларов
                        </div>
                    </div>
                </div>
                <div class="stack main-quotes-stack">
                    <div class="item usd" data-item-name="usd">
                        <div class="value"></div>
                        <div class="note comment usd"><span>рублей</span> за доллар</div>
                    </div>
                    <div class="item eur" data-item-name="eur">
                        <div class="value"></div>
                        <div class="note comment eur"><span>рублей</span> за евро</div>
                        <div class="note additional eurusd"></div>
                    </div>
                    <div class="item brent" data-item-name="brent">
                        <div class="value"></div>
                        <div class="note comment brent"><span>долларов</span> за баррель</div>
