                        <div class="note currency">BTC/USD</div>
                    </div>
                    <div class="item bch" data-item-name="bch">
                        <div class="value"></div>
                        <div class="note currency">BCH/USD</div>
                    </div>
                    <div class="item eth" data-item-name="eth">
                        <div class="value"></div>
                        <div class="note currency">ETH/USD</div>
                    </div>
                </div>
            </div>
        </div>
