	var like = function (like, with_text = false)
	{

		var $like = $(like);

		var mp3_id = $like.data('mp3_id')

		var name_storage = 'like_'+mp3_id;

		var url = '/api/mp3/like/mp3_id';

		url = url.replace('mp3_id', mp3_id);

		if (localStorage.getItem(name_storage))
		{
			alertify.log('Вы уже оценивали', 'info');

		} else
		{
			$.get(url, function ()
			{
				var count_like = parseInt
				(
					$like.text()
						.replace('лайл', '')
						.replace(' ', '')
						.replace('+', '')
				);

				count_like ++;

				$like.text(
					(count_like >= 0 ? '+ ' : '- ')+count_like+( with_text ? ' лайк' : '' )
				);

				localStorage.setItem(name_storage, true);

				alertify.log('Ваша оценка учтена', 'success');
			});
		}
		return false;
	};
</script>

	
		
