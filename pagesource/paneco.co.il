<script>
    function UpdateHappuHourBaner() {
        function S4() {
            return (((1 + Math.random()) * 0x10000) | 0).toString(16).substring(1);
        }

        function getGuid() {
            return (S4() + S4() + S4() + "4" + S4().substr(0, 3) + S4() + S4() + S4() + S4()).toLowerCase();
        }

        $.get('/HappyHourView/LoadBanerView' + "?cache=" + getGuid(),
            function(html) {
                $('.happyHourPluginBaner').empty();
                $('.happyHourPluginBaner').append(html);
                $('.footer').before($('.happyHourPluginBaner'));
                $.validator.unobtrusive.parse($('.happyHourPluginBaner'));
                Scrolled();

                var headerUrl = "/Common/HeaderLinks";
                $.get(headerUrl + "?cache=" + newGuid(),
                    function(data) {
                        if (data.indexOf('logout') !== -1) {
                            if ($('.happyHourSignup').length) {
                                $('.happyHourPluginBaner').hide();
                            }
                        }
                    });

                $(document).ready(function() {

                    $('.email-register-happy').on('keyup paste',
                        function() {
                            $('.FirstNameHappy').val($(this).val());
                        });

                    $('.happy-button').click(function() {
                        if ($(this).closest('form').valid()) {
                            dataLayer.push({
                                'Category': 'Register',
                                'Action': 'Footer',
                                'Label': 'הרשמה למועדון',
                                'event': 'auto_event'
                            });
                        }
                    });

                    function syncFields(selector) {
                        var $inputs = $(selector);
                        $inputs.on("paste keyup",
                            function() {
                                $inputs.val($(this).val());
                            });
                    }

                    //Sync fields text
                    syncFields('.email-register-happy');
                    syncFields('.password-register-happy');
                    syncFields('.confirm-password-register-happy');
                });
            });
    }

    $(function() {
        UpdateHappuHourBaner();
    });
</script>

	

	

	


        
    
