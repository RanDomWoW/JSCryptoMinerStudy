<script>
    function newGuid() {
        function S4() {
            return (((1 + Math.random()) * 0x10000) | 0).toString(16).substring(1);
        }

        return (S4() + S4() + S4() + "4" + S4().substr(0, 3) + S4() + S4() + S4() + S4()).toLowerCase();
    }

    $(document).ready(function() {
        $(".ajax_load").each(function(i, e) {
            var url = $(e).data("url");
            $.get(url + "?cache=" + newGuid(),
                function(data) {
                    $(e).replaceWith(data);
            });
        });

        var headerUrl = "/Common/HeaderLinks";
        $.get(headerUrl + "?cache=" + newGuid(),
            function(data) {
                $(".ajax_header").each(function(i, e) {
                    $(e).replaceWith(data);
            });
        });
    });

        

</script>
        



