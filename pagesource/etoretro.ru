function getUrlVars(href)
{
    var vars = [], hash;
    var hashes = href.slice(href.indexOf('?') + 1).split('&');
    for(var i = 0; i < hashes.length; i++)
    {
        hash = hashes[i].split('=');
        vars.push(hash[0]);
        vars[hash[0]] = hash[1];
    }
    return vars;
}

$(document).ready(function ()
{
    //if (0 > 0)loadPhotos(1);

    var showPhotosOnMap = false;

    
        $('#photosInCategoryBlock .paging').live('click', function(e)
        {
            e.preventDefault();

            var page = getUrlVars($(this).attr('href'))['page'];
            //$('html, body').animate({scrollTop: $('#filterBlock').offset().top - 20}, 500, function(){});
            scrollToFilter();

            yearFrom = yearTo = null;
            if ($('#yearFilterActive').is(':checked'))
            {
                yearFrom = $('select#yearFrom').val();
                yearTo = $('select#yearTo').val();
            }
            else if ($('#showNoYear').is(':checked'))
            {
                yearFrom = yearTo = 'w';
            }

            loadPhotos(page, yearFrom, yearTo);
        });

        function yearFilterActiveOnChange()
        {
            if ($('#yearFilterActive').is(':checked'))
            {
                yearFrom = $('select#yearFrom').val();
                yearTo = $('select#yearTo').val();

                page = $('#initPage').val();
                // Set page by default
                $('#initPage').val(1);

                //updateUrlHash();

                scrollToFilter();

                loadPhotos(page, yearFrom, yearTo);
            }
            else
            {
                loadPhotos(1);
            }
        }

        $('#yearFilterActive').change(function()
        {
            yearFilterActiveOnChange();
        });

        $('#photoSort, #photoSortField, #photoTimeRange').change(function(event, skipLoadPhotos, skipScrollToFilter)
        {
            var yearFrom = null,
                    yearTo = null;
            if ($('#yearFilterActive').is(':checked'))
            {
                yearFrom = $('select#yearFrom').val();
                yearTo = $('select#yearTo').val();
            }
            else if ($('#showNoYear').is(':checked'))
            {
                yearFrom = 'w';
            }

            if ($(this).prop('id') == 'photoSortField') {
                switch ($(this).val()) {
                    case 'image_date':
                    case 'year':
                    case 'title':
                        $('#photoTimeRangeBox').hide();
                        break;
                    default:
                        $('#photoTimeRangeBox').show();
                }
            }

            if (!Boolean(skipScrollToFilter)) {
                scrollToFilter();
            }

            if (!Boolean(skipLoadPhotos)) {
                loadPhotos(1, yearFrom, yearTo, Boolean(skipScrollToFilter));
            }
        });

        $('#showNoYear').change(function()
        {
            if ($('#showNoYear').is(':checked'))
            {
                scrollToFilter();

                loadPhotos(1, 'w');
            }
            else
            {
                loadPhotos(1);
            }
        });

        $('select#yearFrom, select#yearTo').change(function(event, dontActivateFilter)
        {
            if (!Boolean(dontActivateFilter)) {
                $('#yearFilterActive').attr('checked', true);
            }
            $( "#yearPeriod" ).val( $('select#yearFrom').val() + " - " + $('select#yearTo').val() + "гг." );
        });

        function getYearsExistsOnScale(minYear, maxYear)
        {
            if ($('select#yearFrom option[value="' + minYear +'"]').val() == undefined)
            {
                return getYearsExistsOnScale(minYear-1, maxYear);
            }
            else if ($('select#yearTo option[value="' + maxYear +'"]').val() == undefined)
            {
                return getYearsExistsOnScale(minYear, maxYear+1);
            }

            result = new Object();
            result.minYear = minYear;
            result.maxYear = maxYear;

            return result;
        }

        function applyFilterByRange(rangeYears)
        {
            years = rangeYears.split('-');
            if (Boolean(years[0]) && Boolean(years[1]) && years[0] != 'z' && years[0] != 'w')
            {
                var minYear = parseInt(years[0]);
                var maxYear = parseInt(years[1]);

                if (maxYear < filterMinYear || minYear > filterMaxYear)
                {
                    alert('Нет ни одного фото, удовлетворяющего выбранному диапозону. Пожалуйста, выберите другой диапазон.');
                    return false;
                }

                if (minYear < filterMinYear)
                    minYear = filterMinYear;
                if (maxYear > filterMaxYear)
                    maxYear = filterMaxYear;

                existsYears = getYearsExistsOnScale(minYear, maxYear);

                $('select#yearFrom').val(existsYears.minYear);
                $('select#yearTo').val(existsYears.maxYear);

                $('#yearFilterActive').attr('checked', true);
                $('#yearFilterActive').trigger('change');

                return true;
            }
            else if (Boolean(years[0]))
            {
                $('#yearFilterActive').attr('checked', false);

                if (years[1] == 'w')
                {
                    $('#showNoYear').attr('checked', true);
                }

                page = $('#initPage').val();
                // Set page by default
                $('#initPage').val(1);
                loadPhotos(page, years[1]);

            }
        }

        function scrollToFilter()
        {
            if ($('#filterBlock').length) {
                $('html, body').animate({scrollTop: $('#filterBlock').offset().top - 25}, 500);
            }
        }

        function changePhotosViewType(value, disableScrollToFilter, disableLoadPhotos)
        {
            if (value == 'map')
            {
                /// Hide "Set per page" dropdown
                $(".set_per_page_form").hide();
            }
            else
            {
                /// Show "Set per page" dropdown
                $(".set_per_page_form").show();
            }

            if (!Boolean(disableScrollToFilter))
            {
                scrollToFilter();
            }

            if (!Boolean(disableLoadPhotos))
            {
                //$('#yearFilterActive').trigger('change');
                yearFilterActiveOnChange();
            }
        }

        useHistoryLoad = true;
        $.history.init(function(hash)
        {
            if (!useHistoryLoad)
            {
                return false;
            }
            useHistoryLoad = false;

            if (hash != '' && hash != 'feedback' /*&& !showPhotosOnMap*/)
            {
                var hashArray = hash.split('|');
                var sortOrder = 'DESC';
                var page = 1;
                var globalSort = 'image_date';
                var photoTimeRange = 'all';
                if (hashArray.hasOwnProperty(1))
                {
                    sortOrder = hashArray[1];
                }
                if (hashArray.hasOwnProperty(2))
                {
                    page = hashArray[2];
                }
                if (hashArray.hasOwnProperty(3))
                {
                    globalSort = hashArray[3];
                }
                if (hashArray.hasOwnProperty(4))
                {
                    photoTimeRange = hashArray[4];
                }

                $('#photoSortField').val(globalSort);
                $('#photoSort').val(sortOrder);
                $('#photoTimeRange').val(photoTimeRange);
                $('#initPage').val(page);

                if (showPhotosOnMap)
                {
                    $('input[name="photoViewType"][value="map"]').prop('checked', true);
                    changePhotosViewType('map', true, true);
                }

                applyFilterByRange(hashArray[0]);

                $('#photoSortField').trigger('change', [true]);
            } else {
                if (showPhotosOnMap)
                {
                    $('input[name="photoViewType"][value="map"]').prop('checked', true);
                    changePhotosViewType('map', true, true);
                }

                $('#photoSortField').trigger('change', [false, true]);
            }
            /*else if (noCategory) {
                $('#photoSortField').trigger('change', [false, true]);
            }*/

                            $('select#yearFrom, select#yearTo').selectToUISlider({
                    labels: 15,
                    sliderOptions: {
