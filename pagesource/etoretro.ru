                        },
                        start: function(event, ui) {
                            $('#yearFilterActive').attr('checked', true);
                        }
                    }
                });

                $('select#yearFrom').trigger('change', true);
                            });

            $('#hotMarker').change(function()
            {
                var rangeYears = $(this).val();
                if (rangeYears != '')
                {
                    if (applyFilterByRange(rangeYears))
                    {
                        $('select#yearFrom, select#yearTo').trigger('change', true);
                        //updateUrlHash();
                    }
                }
            });

            $('input[name="photoViewType"]').change(function(event, disableScrollToFilter)
            {
                changePhotosViewType($(this).val(), false);
