        //params = params + "&y1="+yearFrom+"&y2="+yearTo+"&sort="+$('#photoSort').val();
        params = params + "&y1="+yearFrom+"&y2="+yearTo;
        $('#showNoYear').attr('checked', false);
        resetUrlHash = false;
    }

    if (mapView)
    {
        params = params + "&type=map";
        if (mapLoaded)
        {
            params = params + "&markers=true";
        }
    }

    /*if (resetUrlHash)
     {
     $.history.load('_');
     }*/

    var dataType = "html";
    if (mapView && mapLoaded)
    {
        dataType = "json";
    }

    $.ajax({
        url: 'spic.php',
        type: "POST",
        data: params,
        dataType: dataType,
        success: function(data)
        {
            if (mapView)
            {
                if (!mapLoaded)
                {
                    $('#photosInCategoryBlock').html(data);

                    /*var script = document.createElement("script");
                    script.type = "text/javascript";
                    script.id = "google_clusterer_js";
                    script.src = "/js/maps/markerclusterer_packed.js";
                    document.body.appendChild(script);*/

                    /// Google map
                    var script = document.createElement("script");
                    script.type = "text/javascript";
                    script.id = "google_map_js";
                    //script.src = "http://maps.googleapis.com/maps/api/js?key=AIzaSyAL5zQ7SMm3m_MAjd2BeVQdv2i3BCIU4_E&sensor=true&callback=init_map";
                    script.src = "http://maps.googleapis.com/maps/api/js?key=AIzaSyAL5zQ7SMm3m_MAjd2BeVQdv2i3BCIU4_E&callback=init_map";
                    document.body.appendChild(script);

                    mapLoaded = true;

                    if (enableGeolocation) {
                        enableGeolocation = false;

                        setTimeout(function() { navigator.geolocation.getCurrentPosition(gMap.geoLocationDetectSuccess, gMap.geoLocationDetectFailed); }, 1000);
                    }
                }
                else
                {
                    if (data.show_all_photos != undefined)
                    {
                        if(typeof(locations_data) != 'undefined' && locations_data != null){
                            data = locations_data;
                            gMap.setPhotosData(data);
                            gMap.showMarkers();

                            $('.blockUI').remove(); // Manually remove overlay, because of strange bug
                            //gMap.showPreloader(false);
                        } else{
                            $.getJSON(data.all_locations_file_url, function( locations ) {
                                data = locations;    
                                gMap.setPhotosData(data);
                                gMap.showMarkers();

                                $('.blockUI').remove(); // Manually remove overlay, because of strange bug
                                //gMap.showPreloader(false);
                            });
                        }
                    } else {
                        gMap.setPhotosData(data);
                        gMap.showMarkers();

                        $('.blockUI').remove(); // Manually remove overlay, because of strange bug
                        //gMap.showPreloader(false);
                    }
                }
            }
            else
            {
                $('#photosInCategoryBlock').html(data);
                mapLoaded = false;
            }

            if (!Boolean(skipUpdateUrlHash)) {
                updateUrlHash();
            }
        }
    });
