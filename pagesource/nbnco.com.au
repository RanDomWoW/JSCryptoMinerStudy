<script>
    (function () {
        var mobileSearchBtn = $('.navigation-mobile-header-search-button');
        var mobileSearch = $('#mobileSearch');

        var searchBtn = $('#nav-search-btn');
        var searchQuery = $("#searchTerm");
        $(document).ready(function () {

            var initSearchInput = function(searchQueryInput,searchButton) {
                if (nbnco.utils.getUrlParam('query')) {
                    searchQueryInput.val(nbnco.utils.getUrlParam('query'));
                };
                searchQueryInput.on('keyup', {searchButton: searchButton },function (event) {
                    if (event.keyCode == 13) {
                        event.data.searchButton.click();
                    }
                });
            };

            var clickSearch = function (event) {
                var searchParams = {};
                if (event.data.searchInput.val().length > 1) {
                    searchParams.query = event.data.searchInput.val();
                };
                window.location.href = nbnco.utils.constructUrlWithParameters("/search.html", searchParams);
            };

            searchBtn.on('click', { searchInput: searchQuery }, clickSearch);
            initSearchInput(searchQuery,searchBtn);
            mobileSearchBtn.on('click', { searchInput: mobileSearch }, clickSearch);
            initSearchInput(mobileSearch,mobileSearchBtn);

        });
    })();

</script>

