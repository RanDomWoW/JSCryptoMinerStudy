<script>
    (function () {

        var $checkAddressSearch = $(".check-address-search:last");

        var checkAddressCookie = new nbnco.checkAddressCookie();
        if (checkAddressCookie.getLocId()) {
            $checkAddressSearch.find('.input-group').addClass('has-cookie');
            $checkAddressSearch.find('.btn-results').attr('href', checkAddressCookie.getResultPageUrl());
            $checkAddressSearch.find('input.form-control').val(checkAddressCookie.getOriginalAddress());
            $checkAddressSearch.find('.btn-results').attr('data-locid', checkAddressCookie.getLocId());
            $checkAddressSearch.find('.btn-results').attr('data-original-address', checkAddressCookie.getOriginalAddress());
        }


        $(document).ready(function () {
            var checkAddress = null;
            if ("" === "true") {
                checkAddress = new CheckAddressSearch($checkAddressSearch, null, "NBN\u002DCYA:Home", "/iframe/check-address-redirect.html", "");
            } else {
                checkAddress = new CheckAddressSearch($checkAddressSearch, $("#exceptionMapContainer"), "NBN\u002DCYA:Home", null, "");
                if(checkAddressCookie.getLocId() && !checkAddressCookie.getResultPageUrl()) {
                    checkAddress.setResultPageUrl(checkAddressCookie);
                }
            }
            if("" === "true") {
                var address = nbnco.utils.getUrlParam("address");
                var type = nbnco.utils.getUrlParam("type");
                if(address != null && type != null) {
                    checkAddress.resolveAddress(address, type);
                }
            }
        });
    })();
</script>


