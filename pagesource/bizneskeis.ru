jQuery(document).ready(function(){
function myhover(){
    var active_menu_item = jQuery("[role=active_menu]"),
        hover_el = jQuery("#activeMenu");
    if(active_menu_item.length>0){ // если есть активный пункт меню, то позиционируем двигающуюся плашку на нем
        var menuWidth = active_menu_item.outerWidth(); // определяем ширину активного пункта меню
        var menuLeft = active_menu_item.position().left; // определяем смещение активного пункта меню слева
        hover_el.stop().animate({ // анимируем движущуюся плашку
            left: menuLeft+'px',
            width: menuWidth+'px'
        }, 500, 'linear');
    };
    jQuery("#topMenu a.mainlevel").mouseover(function(){ // поведение движущейся плашки п ии на любой пункт меню. Все тоже самое, что и при наличии активного пункта, только позиция плашки определяется относительно пункта, на который произошло наведение курсора мыши
            var menuWidth = jQuery(this).outerWidth();
            var menuLeft = jQuery(this).position().left;
            hover_el.stop().animate({
                left: menuLeft+'px',
                width: menuWidth+'px'
            }, 300, 'linear');      
    });
    jQuery("#topMenu").mouseleave(function(){ // поведение плашки при окончании события наведения мыши на пункт меню (выход курсора мыши на пределы блока, в котором содержится меню)
        if(active_menu_item.length<=0){ // если активного пункта нет, то перемещаем плашку за границу экрана
            active_menu_item.stop().animate({
                left: '-999px',
                width: '0px'
            }, 500, 'linear');
        }
        else{ // иначе, если есть активный пункт меню – возвращаем плашку на него
            var menuWidth = active_menu_item.outerWidth();
            var menuLeft = active_menu_item.position().left;
            hover_el.stop().animate({
                left: menuLeft+'px',
                width: menuWidth+'px'
            }, 500, 'linear');
        }
    });
    $("#topMenu a").click(function() {
	$("#topMenu a").attr('role','');
	$(this).attr('role','active_menu');
        active_menu_item = jQuery("[role=active_menu]");
      var elementClick = $(this).attr("href")
      var destination = $(elementClick).offset().top;
      jQuery("html:not(:animated),body:not(:animated)").animate({
        scrollTop: destination
      }, 800);
      return false;
    });
    var menuItems = $("#topMenu").find("a"),
        scrollItems = menuItems.map(function() {
	var href = $(this).attr("href");
	var re = new RegExp('^#.*','i');
	if (re.test(href)){
		var item = $($(this).attr("href"));
		if (item.length) { return item; }
	}
    }),
    lastId='';
    $(window).scroll(function() {
	var fromTop = $(this).scrollTop() + 10;
	var cur = scrollItems.map(function() {
		if ($(this).offset().top < fromTop) {
			return this;
		}
	});
	cur = cur[cur.length-1];
	var id = cur ? cur[0].id : "";
	if (lastId !== id) {
		lastId = id;
		$("#topMenu a").attr('role','');
		$("#topMenu").find("[data-id="+id+"]").attr('role','active_menu');
	        active_menu_item = jQuery("[role=active_menu]");
		$("#topMenu").trigger('mouseleave');
	};

    });


}
myhover();
});

                 
$(function(){
    var h_hght = $('header').outerHeight(); // высота шапки
    var h_mrg = 0;    // отступ когда шапка уже не видна
 
    var elem = $('#topMenu');
    var top = $(this).scrollTop();
     
    if(top > h_hght){
        elem.css('top', h_mrg);
    }           
     
    $(window).scroll(function(){
        top = $(this).scrollTop();
         
        if (top+h_mrg < h_hght) {
            elem.css('top', (h_hght-top));
        } else {
            elem.css('top', h_mrg);
        }
    });
});
/*********************
  //Контакты
**********************/
$(document).ready(function(){
	var cnt_phone = $('.sponsor_info .contacts .phone'),
	    cnt_skype = $('.sponsor_info .contacts .skype'),
	    cnt = $('.sponsor_info .contacts'),
	    info = $('.sponsor_info .info'),
	    info_max_width = info.outerWidth();
	var cnt_re_width = function(){
		info_max_width = Math.max(info_max_width, cnt.outerWidth()); 
		info.css({width: info_max_width+"px"});
	}
	cnt_re_width();
	$('.sponsor_info .contacts .phone i').click(function(){
	
            cnt_skype.animate({
                maxWidth: '22px'
            }, 220);
            cnt_phone.animate({
                maxWidth: '300px'
            }, 500, 'linear',function(){ var self = $(this); var w = self.attr('data-width') || self.outerWidth(); self.attr('data-width', w); self.css({maxWidth: w+"px"}); cnt_re_width();});

	});
	$('.sponsor_info .contacts .skype i').click(function(){
            cnt_skype.animate({
                maxWidth: '300px'
            }, 500, 'linear',function(){ var self = $(this); var w = self.attr('data-width') || self.outerWidth(); self.attr('data-width', w); self.css({maxWidth: w+"px"});cnt_re_width();});
            cnt_phone.animate({
                maxWidth: '22px'
            }, 220);
	});

});

</script>

