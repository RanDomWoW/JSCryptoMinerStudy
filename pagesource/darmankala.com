                                               value="tSVHdKPxH4qM6Pn4"/>
                                        <input type="hidden" name="qty" value="1">
                                        <button type="submit">افزودن به سبد</button>
                                    </form>
                                {{/in_stock}}
                            {{/isAddToCartEnabled}}
                        </div>
                    </div>
                </a>
            </div>
        </div>
    {{/hits}}
</script>
