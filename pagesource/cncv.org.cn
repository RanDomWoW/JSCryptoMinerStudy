    //社区排行切换
    function sub_switch(n,m){
        for(var id = 1;id<=m;id++){
            var content="content_"+id;
            var link="link_"+id;
            if(id==n)
            {
                document.getElementById(content).style.display="block";
                document.getElementById(link).style.fontWeight="bold";
                document.getElementById(link).style.backgroundColor="#F7E6B2";
            }
            else
            {
                document.getElementById(content).style.display="none";
                document.getElementById(link).style.fontWeight="normal";
                document.getElementById(link).style.backgroundColor="#FFF";
            }
        }
    }
    function close_tip(){
