    $(function(){
        var x = -12;
        var y = 18;
        $(".live2").hover(function (e) {
            var pid=this.title;
            if($(".popbox").length>0){$(".popbox").remove();}
            $.ajax({
                type: "GET",
                url: "/contact/ajax/index.php",
                data: {ac:"load",pid:pid,rnd:new Date().getTime()},
                success: function(data){
                    console.log(data);
                    var poptipHtml = "<div id='poptip' class='popbox'><span class='poptip-arrow'></span><span class='poptip-emp'></span><span class='poptip-close'><a href='javascript:void(0)' onclick='close_tip();'><img src='/images14/close.gif' /></a></span>" + data + "</div>";
                    if($(".popbox").length>0){
                        $(".popbox").remove();
                    }else{
                        $("body").append(poptipHtml);
                        $("#poptip").css({"top":(e.pageY+y)+"px","left":(e.pageX+x)+"px"}).show("fast");
