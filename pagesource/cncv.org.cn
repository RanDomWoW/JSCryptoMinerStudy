    float_view();

    function float_view() {
        if (parseInt($(document).width()) > 1250) {
            $('.float_right').css('right', (parseInt($(document).width()) - parseInt($('#container').width())) / 2 - $(".float_right").width() - 20);
        } else {
            $('.float_right').css('right', '0');
        }
        if (parseInt($(window).height()) < 768) {
            $('.float_right').css('top', '210px');
        }
    }

    $('.float_right_close').click(function(){
        $('.float_right').hide();
    })


</script>
