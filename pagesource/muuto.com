<script>
	var HeroTopContentList = JSON.parse('[{"type":"landscape","bgcolor":"#000000","image":null,"designer":"Anderssen & Voll","link":"\/accessories\/framed-mirror","title":{"type":"arrow","top":"FRAMED","bottom":"MIRROR"},"video":{"type":"unbranded","link":"\/pub\/media\/pwblocks\/1504511847389872muuto_mirror_grd_01_mix02_20170831-HD_1280x720p (1).MP4","image":"https:\/\/muuto-static.imgix.net\/pub\/media\/pwblocks\/1505472741860104Framed_1.jpg?w=1920"}},{"type":"landscape","bgcolor":"#ffffff","image":null,"designer":"Cecilie Manz","link":"\/stories\/in-the-workshop","title":{"type":"arrow","top":"WORKSHOP","bottom":"CHAIR"},"video":{"type":"unbranded","link":"\/pub\/media\/pwblocks\/1506778502839479muuto_chair_grd_01_mix03_20170904-HD_1280x720p.MP4","image":"https:\/\/muuto-static.imgix.net\/pub\/media\/pwblocks\/1505472828856401Workshop_3.jpg?w=1920"}},{"type":"landscape","bgcolor":"#ffffff","image":null,"designer":"Lars Torn\u00f8e","link":"\/accessories\/the-dots","title":{"type":"arrow","top":"THE","bottom":"DOTS"},"video":{"type":"unbranded","link":"\/pub\/media\/pwblocks\/1510156339195747muuto_hanger_grd_01_mix03_20170904-HD_1280x720p.MP4","image":"https:\/\/muuto-static.imgix.net\/pub\/media\/pwblocks\/1510156722957881Dots_1_Easy-Resize.com.jpg?w=1920"}}]');

	function shuffleTopContent()
	{

		var content = HeroTopContentList[Math.floor(Math.random() * HeroTopContentList.length)];
		console.log(content);
		// Wrapper
		document.querySelector('#hero-top-content').className += ' '+content.type;
		if (content.image != null) {
			document.querySelector('.image').style.backgroundImage = 'url('+content.image+')';
		}

		if(content.designer == ""){
			document.querySelector('.designer span').innerHTML = "";
		}

		// Title
		document.querySelector('#hero-top-content .custom-title a').href = content.link;
		document.querySelector('#hero-top-content .title.h1').innerHTML = content.title.top;
		document.querySelector('#hero-top-content .title.h2').innerHTML = content.title.bottom;
		document.querySelector('#hero-top-content .designer span').innerHTML += content.designer;
		document.querySelector('#hero-top-content .custom-title').className += ' '+content.title.type;

		if (content.title.type == 'btn') {
			document.querySelector('#hero-top-content .w12.row.btn').style.display = 'block';
		}

		// Video
		if (content.video.link != "") {
			document.querySelector('#hero-top-content .content-wrap').className += ' video';
			document.querySelector('#hero-top-content .video-element').style.display = 'block';
			document.querySelector('#hero-top-content .click-area').style.display = 'block';
			document.querySelector('#hero-top-content .eq-wrap').style.display = 'flex';
			document.querySelector('#hero-top-content .click-area').href = content.link;

			if (content.video.image != null) {
				document.querySelector('#hero-top-content .video-placeholder').style.backgroundImage = 'url('+content.video.image+')';
				document.querySelector('#hero-top-content .video-placeholder').style.display = 'block';
			}

			if (content.video.type == 'unbranded') {
				document.querySelector('#hero-top-content video.video source').src = content.video.link;
				document.querySelector('#hero-top-content video.video').style.display = 'block';
				document.querySelector('#hero-top-content #video').load();
				document.querySelector('#hero-top-content #video').play();
				// Press play
			} else if (content.video.type == 'youtube') {
				document.querySelector('#hero-top-content #player.youtube').style.display = 'block';
				document.querySelector('#hero-top-content #player.youtube').setAttribute('data-id', content.video.link);
			} else if (content.video.type == 'vimeo') {
				document.querySelector('#hero-top-content #player.vimeo').style.display = 'block';
				document.querySelector('#hero-top-content #player.vimeo').src = "https://player.vimeo.com/video/{id}?api=1&player_id=player".replace('{id}', content.video.link);
			}
		}
	}

	shuffleTopContent();
</script>                                                                                            
