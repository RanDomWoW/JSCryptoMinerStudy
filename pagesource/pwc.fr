<script id="language-selector-template" type="text/x-handlebars-template">
	<div class="dropdown language-selector" id="languageSelector{{#if isMobile}}Mobile{{/if}}">
    		<button class="dropdown-toggle" type="button"  id="languageSelectorDropdown{{#if isMobile}}Mobile{{/if}}">
        		{{currentLanguageTitle}}
             <span class="chevron"></span>
        </button>
        <ul class="dropdown-menu language-selector__dropdown-options" aria-labelledby="languageSelectorDropdown{{#if isMobile}}Mobile{{/if}}">
        		<li><span>Choose a language:</span></li>
            {{#each alternateLanguages}}
        <li {{#if this.isCurrentLanguage}} class="selected" {{/if}}><a href="{{this.linkURL}}">{{this.linkText}}</a></li>
            {{/each}}
        </ul>
    </div>
</script>

<script id="territory-selector-template" type="text/x-handlebars-template">
	<div class="language-field-container">
    		<input class="languageField" type="text" list="PwC_countries">
    </div>

    <div class="PwC_countries">
    		<span> Indiquez une localisation</span>
        {{#each this}}
			{{#if territoryFinder}}
        			<a href="{{domain}}" title="{{name}}">{{name}}</a>
				{{#each alias}}
					<a href="{{../domain}}" title="{{this}}">{{this}}</a>
        			{{/each}}
			{{/if}}
        {{/each}}
    </div>
    <button type="button" class="territorySubmit"></button>
    <span class="languageField_valid">No match found</span>
</script>

<script id="breadcrumb-template" type="text/x-handlebars-template">
    <div class="breadcrumb-component__scroll-pane" id="breadcrumbScrollPane{{#if isMobile}}Mobile{{/if}}">
    		<ul class="{{#unless isMobile}}col-sm-12{{/unless}} breadcrumb full-closed">
        		{{#unless isMobile}}<li class="breadcrumbMenu" style="display: none;"></li>{{/unless}}
            {{#each breadcrumbs}}
            <li class="breadcrumbVisible {{#if @first}}firstOnLine{{/if}}">
            		<a href="{{this.linkURL}}">{{this.linkText}}</a>
            </li>
            {{/each}}
        </ul>
    </div>
    <div class="breadcrumb-component__scroll-fade breadcrumb-component__scroll-fade--left"></div>
    <div class="breadcrumb-component__scroll-fade breadcrumb-component__scroll-fade--right"></div>
</script>

<script id="hamburger-template" type="text/x-handlebars-template">
    {{#ifEquals navigationStrategy "sibling-children"}}
        {{#if hasSectionTitle}}
        <h5 class="dynamic-site__heading"><a href="{{sectionPagePath}}.html">{{sectionTitle}}</a></h5>
        {{/if}}
        <div class="row hamburger-submenu">
            <ul class="nav nav-tabs level-two-navs col-md-6">
                {{#each hamburgerLinks.primary}}
                <li><a data-tab-pane="secondLevelItem{{math @index '+' '1'}}"
                       href="{{this.url}}{{#stringDoesntContain this.url '.html'}}.html{{/stringDoesntContain}}">
                    <span class="level-two-nav__title">{{this.text}}</span>
                    {{#if this.children.length}}
                    <span class="level-two-nav__arrow"></span>
                    {{/if}}
                </a></li>
                {{/each}}
            </ul>
            {{#if hamburgerLinks.primary.length}}
            <div class="tab-content level-three-navs col-md-6">
                {{#each hamburgerLinks.primary}}
                <div id="secondLevelItem{{math @index '+' '1'}}" class="tab-pane">
                    <ul>
                        {{#each this.children}}
                        <li>
                            <a href="{{this.url}}{{#stringDoesntContain this.url '.html'}}.html{{/stringDoesntContain}}">{{this.text}}</a>
                        </li>
                        {{/each}}
                    </ul>
                </div>
                {{/each}}
            </div>
        </div>
        {{/if}}
        {{/ifEquals}}
        {{#ifEquals navigationStrategy "manual"}}
        {{#if hasSectionTitle}}
        <span class="sectiontitle"><a href="{{sectionPagePath}}.html">{{sectionTitle}}</a></span>
        {{/if}}
        <div class="row">
            {{#if hamburgerLinks.primary.length}}
            <div class="col-sm-6" id="inPageNavigation">
                <ul>
                    {{#each hamburgerLinks.primary}}
                    <li>
                        <a href="{{this.url}}{{#stringDoesntContain this.url '.html'}}.html{{/stringDoesntContain}}" class="inPageLink"
                           target="{{this.targetType}}"><span>{{this.text}}</span></a>
                    </li>
                    {{/each}}
                </ul>
            </div>
            {{/if}}
            {{#if hamburgerLinks.secondary.length}}
            <div class="col-sm-6" id="furtherArticles">
                <ul>
                    {{#each hamburgerLinks.secondary}}
                    <li>
                        <a href="{{this.url}}{{#stringDoesntContain this.url '.html'}}.html{{/stringDoesntContain}}"
                           target="{{this.targetType}}"><span>{{this.text}}</span></a>
                    </li>
                    {{/each}}
                </ul>
            </div>
            {{/if}}
        </div>
    {{/ifEquals}}
</script>

<script id="hamburger-levelOne-template" type="text/x-handlebars-template">
    {{#if levelOneLinks.length}}
        <h3>Pour aller plus loin
        </h3>
        <ul class="row">
            {{#each levelOneLinks}}
            <li>
                <a href="{{this.link}}" target="{{#if this.openNewWindow}}_blank{{else}}_self{{/if}}" class="clearfix">
                    <span>{{this.title}}</span>
                </a>
            </li>
            {{/each}}
        </ul>
    {{/if}}
</script>

<script id="hamburger-mobile-template" type="text/x-handlebars-template">
    {{#ifEquals navigationStrategy "sibling-children"}}
    		{{#each hamburgerLinks.primary}}
        		<div class="panel panel-default">
            		<div class="panel-heading" role="tab" id="levelTwoLinks_{{math @index '+' '1'}}">
                		<h4 class="panel-title">
                    		<a role="button" href="{{this.url}}{{#stringDoesntContain this.url '.html'}}.html{{/stringDoesntContain}}" 
                       		aria-expanded="false" aria-controls="levelThreeLinks_{{math @index '+' '1'}}">
                        		{{this.text}}
                    		</a>
                    		<span></span>
                		</h4>
            		</div>
            		<div id="levelThreeLinks_{{math @index '+' '1'}}" class="panel-collapse collapse" role="tabpanel"
                 		aria-labelledby="levelTwoLinks_{{math @index '+' '1'}}">
                		<div class="panel-body">
                    		<ul>
                        		{{#each this.children}}
                        			<li>
                        				<a href="{{this.url}}{{#stringDoesntContain this.url '.html'}}.html{{/stringDoesntContain}}">{{this.text}}</a>
                        			</li>
                        		{{/each}}
                    		</ul>
                		</div>
            		</div>
        		</div>
    		{{/each}}
    {{/ifEquals}}
	{{#ifEquals navigationStrategy "manual"}}
        {{#if hasSectionTitle}}
        <span class="sectiontitle"><a href="{{sectionPagePath}}.html">{{sectionTitle}}</a></span>
        {{/if}}
        <div class="row">
            {{#if hamburgerLinks.primary.length}}
            <div class="col-sm-6" id="inPageNavigation">
                <ul>
                    {{#each hamburgerLinks.primary}}
                    <li>
                        <a href="{{this.url}}{{#stringDoesntContain this.url '.html'}}.html{{/stringDoesntContain}}" class="inPageLink"
                           target="{{this.targetType}}"><span>{{this.text}}</span></a>
                    </li>
                    {{/each}}
                </ul>
            </div>
            {{/if}}
            {{#if hamburgerLinks.secondary.length}}
            <div class="col-sm-6" id="furtherArticles">
                <ul>
                    {{#each hamburgerLinks.secondary}}
                    <li>
                        <a href="{{this.url}}{{#stringDoesntContain this.url '.html'}}.html{{/stringDoesntContain}}"
                           target="{{this.targetType}}"><span>{{this.text}}</span></a>
                    </li>
                    {{/each}}
                </ul>
            </div>
            {{/if}}
        </div>
    {{/ifEquals}}
</script>

<script id="social-share-template" type="text/x-handlebars-template">
    {{#each socialShareConfiguration}}
        {{#ifEquals this "Facebook"}}
        <a href="https://www.facebook.com/sharer/sharer.php?u={{socialURL}}">
            <span class="sti-btn sti-facebook-btn circle-icon">FB</span></a>
        {{/ifEquals}}
        {{#ifEquals this "Twitter"}}
        <a href="https://twitter.com/intent/tweet/?url={{socialURL}}">
            <span class="sti-btn sti-twitter-btn circle-icon">twitter</span></a>
        {{/ifEquals}}
        {{#ifEquals this "Linkedin"}}
        <a href="https://www.linkedin.com/shareArticle?mini=true&url={{socialURL}}">
            <span class="sti-btn sti-linkedin-btn circle-icon">Linkedin</span></a>
        {{/ifEquals}}
        {{#ifEquals this "Googleplus"}}
        <a href="https://plus.google.com/share?url={{socialURL}}"><span
                class="sti-btn sti-google-btn circle-icon">GooglePlus</span></a>
        {{/ifEquals}}
        {{#ifEquals this "Weibo"}}
        <a href="http://v.t.sina.com.cn/share/share.php?url={{socialURL}}">
            <span class="sti-btn sti-weibo-btn circle-icon">Weibo</span></a>
        {{/ifEquals}}
    {{/each}}
</script>


