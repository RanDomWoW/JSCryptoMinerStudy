    <script src="https://www.youtube.com/player_api"></script>
    <script>
        function onYouTubePlayerAPIReady() {

            player = new YT.Player('Youtube', {
                events: {'onReady': onPlayerReady}
            });

            players = new YT.Player('open', {
                events: {'onReady': onPlayerReadyOpen}
            });
        }
        function onPlayerReady() {
            document.querySelector('.video-back').addEventListener("click", function() {player.pauseVideo();});
        }
        function onPlayerReadyOpen() {
            document.querySelector('#play').addEventListener("click", function() {
                document.querySelector('.play').parentElement.classList.remove("open");
                players.playVideo();

            });

            document.querySelector('.pause').addEventListener("click", function() {
                document.querySelector('#play').parentElement.classList.add("open");
                players.pauseVideo();

            });


        }

    </script>


