    <script id="ReleatedLinksTemplate" type="text/x-jsrender">
    <li>
        {{if link_type==="internal"}}
            <a class="link-internal" href="{{:url}}"><img src="/assets-ui/img/img-sprite.png" alt="" />{{:title}}
        {{else}}
            {{if file_type !== ""}}
            {{if file_type === "photo"}}
            <a href="{{:img_o}}" class="link-{{:file_type}}" target="_blank">
            {{else}}
                {{if file_path != ""}}
                    <a href="{{:file_path}}" class="link-{{:file_type}}" target="_blank">
                {{else}}
                    <a href="{{:url}}" class="link-{{:file_type}}" target="_blank">
                {{/if}}
            {{/if}}
            {{else}}
            <a href="{{:url}}" class="link-internal">
            {{/if}}
            <img alt="" src="/assets-ui/img/img-sprite.png" />
            {{:title}}
            {{if file_size}}
                {{if friendlyFilesize}}
                    <span class="filetype">{{:file_type}} {{:friendlyFilesize}}</span>
                {{else}}
                    <span class="filetype">{{:file_type}} {{:file_size}}</span>
                {{/if}}
            {{/if}}
            <span class="visuallyhidden">opens in new window</span>
        {{/if}}
        </a>
    </li>
    </script>
