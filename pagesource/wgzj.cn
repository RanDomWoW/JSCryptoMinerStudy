            $.cookie('lrtk_down', 'true', {path: "/", expires: 1, domain: _domain});
        }

        //五秒钟后自动收起
        var t = setTimeout(adsUp, 5000);
        //点击收起
        $('.lrtk a.up').live('click', function () {
            clearTimeout(t);
