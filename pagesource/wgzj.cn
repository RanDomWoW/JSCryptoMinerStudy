                filter: 'alpha(opacity=0)'
            });
            $('.lrtk').animate({
                height: '250px'
            }, function () {
                $(this).find('.down').addClass('up').removeClass('down').css({
                    opacity: '1',
                    filter: 'alpha(opacity=100)'
                });
            });
        });
    });
