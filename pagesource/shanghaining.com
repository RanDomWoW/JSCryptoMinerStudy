        function getHot(page1) {
            App.ajax("", {
                type:'POST',
                dataType:'json',
                data:'page=' + page1,
                url:'/util/indexhot',
                success:function (res) {
                    if (res.success) {
                        var threads = res.data;
                        var html = "";
                        $(threads).each(function (i) {
                            var thread = this;
                            html += '<li>';
                            html += '<a href="' + thread.turl + '" target="_blank" title="' + thread.subject + '"><img src="' + thread.uavatar + '" title="' + thread.subject?html + '" width="48" height="48"/></a>';
                            html += '<div>';
                            html += '<p class="say-title"><a href="' + thread.turl + '" target="_blank" rel="nofollow" title="' + thread.subject?html + '">' + thread.subject?html + '</a></p>';
                            html += '<p class="say-info color9">' + thread.author + ' ' + thread.createdAt + '天前发表 于 ' + thread.fname + '</p>';
                            html += '<span class="color9">回复:' + thread.replies + '<br/>查看:' + thread.views + '</span>';
                            html += '<b>◆</b>';
                            html += '<em>◆</em>';
                            html += '</div>';
                            html += '</li>';
                        })
                        $("#share-list").html(html);
                        App.hoverToggle({"#share-list li":"hover"});
                        if (res.code == 100) {
                            page = 1;
                        } else {
                            page = page1 + 1;
                        }
                    }
                },
                error:function () {
                }
            })//$.ajax

