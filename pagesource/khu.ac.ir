<script type="text/javascript">
$(document).ready(function() {
	$('#search_box .container .tabs .tab').click(function() {
		$('#search_box .container .tabs .tab').attr('data-show',"false");
		$(this).attr('data-show',"true");
		var index =$(this).attr('data-index');
		$("#search_box .container form").attr('data-show',"false");
		$("#search_box .container form[data-index="+ index +"]").attr('data-show',"true");
	});
	$('.select_style .opts').click(function() {
		if($(this).attr('data-show')=="true"){
			$(this).attr('data-show',"false");
		}else{
			$(this).attr('data-show',"true");
		}
	});

	$('.select_style .opts .opt').click(function() {
		var action = $(this).attr('data-action');
		var href = $(this).attr('data-href');
		var title = $(this).attr('data-title');
		var selectVal = $(this).attr('data-val');
		var text = $(this).text();
		$(this).parent().find(".selected").text(text);
		$(this).parent().parent().find(".link_all").attr("href",href);
		$(this).parent().parent().find(".link_all").text(title);
		$(this).parent().parent().parent().attr("action",action);
		$(this).parent().find('input[type=hidden]').val(selectVal);
	});
});
</script>
