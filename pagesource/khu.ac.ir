		}
		$('#section5  .items').css({'background-image': 'none'});
		//////////////// bottom menu
		$("#bottom_box_menu .link_box").click(function()
		{
			var show=$("#bottom_box_menu").attr("data-show")=="open"?"close":"open";
			$("#bottom_box_menu").attr("data-show",show);
		});
		//////////////// slider
		var sliderChange=function()
		{
			$('#slider_box .content .items').css({'display': 'none'});
			var src=$("#slider_box .content .items").first().find("img").attr("src")
			$(".bg_images_full").css({'background-image': 'url('+src+')'});
			$("#slider_box .content .items").first().slideDown();
		};
		sliderChange();
		$("#slider_box .next").click(function()
		{
			$("#slider_box .content .items").first().insertAfter($("#slider_box .content .items").last());
			sliderChange();
		});
		$("#slider_box .previous").click(function()
		{
			$("#slider_box .content .items").last().insertBefore($("#slider_box .content .items").first());
			sliderChange();
		});
		////////////// side bar
		$("#side_bar .tools .up,#side_bar .tools .down").click(function()
		{
			var len=Math.ceil(($(this).parent().parent().find($(".item")).length)/3);
			var top=parseInt($(this).parent().attr('data-top'));
			if($(this).attr('class')=="up"){
				if(len-1>top){
					top=top+1;
					$(this).parent().attr('data-top',top);

					$(this).parent().parent().find($(".items")).css( "top",- top*100 +"%" );
				}
			}else{
				if(top>0){
					top=top-1;
					$(this).parent().attr('data-top',top);
					$(this).parent().parent().find($(".items")).css( "top", -top*100 +"%" );
				}
			}

		});
		$("#side_bar .close").click(function() {
			$("#side_bar").attr("data-show","false");
			$("#side_bar .btns .btn").attr("data-show","false");
		});
		$("#side_bar .btns .btn").click(function() {
			if($(this).attr("data-show")=="true"){
				$("#side_bar").attr("data-show","false");
				$("#side_bar .btns .btn").attr("data-show","false");
			}else{
				$("#side_bar").attr("data-show","true");
				$("#side_bar .btns .btn").attr("data-show","false");
				$(this).attr("data-show","true");
				var x=$(this).index()+1;
				$("#side_bar .container").attr("data-show","false");
				$("#side_bar .container:nth-child("+x+")").attr("data-show","true");
			}
		});
		////////////////////circle box
		$("#section1 .circle_box .circle").click(function() {
			if($(this).attr("data-lock")!="lock"){
				var href=$(this).find("a").attr("data-href");
				$(this).find("a").attr("href",href);
				$("#section1 .circle_box .circle").css({"z-index": "2"});
				$(this).attr("data-show","true").css({"z-index": 100}).attr("data-lock","lock");
				$("#section1").removeClass("active");
			}
		});
		$(".circle_box .circle .close").click(function() {
			var self = $(this).parent();
			self.find("a").removeAttr("href");
			self.attr("data-show","false").css({"z-index": "3"});
			$("#section1 .circle_box .circle").css({"z-index": "2"});
			$("#section1").addClass("active");
			setTimeout(function() {
				self.attr("data-lock","open")
			}, 1000)
		});
		////////////////// event box
		$("#carousel figure:first-child").attr("data-center", "true");
		$("#section4 .items .icons .down, #section4 .items .icons .up").on("click", function() {
			var carousel = $(this).parent().parent().find($(".carousel"));
			var method = $(this).attr('class');
			var centerBox = carousel.find("figure[data-center=true]");
			if ((navigator.userAgent.indexOf("MSIE") != -1) || (!!document.documentMode == true)) //IF IE > 10
			{
				$("#carousel figure").css("transition", "none");
				if (method == "up") {
					carousel.find('figure').first().insertAfter(carousel.find('figure').last());
					carousel.find('figure').attr("data-center", "false")
					carousel.find('figure').first().attr("data-center", "true");
				} else {
					carousel.find('figure').last().insertBefore(carousel.find('figure').first());
					carousel.find('figure').attr("data-center", "false")
					carousel.find('figure').first().attr("data-center", "true");
				}
			} else {
				var x = parseInt(carousel.attr("data-index"));
				if (method == "up") {
					x = x + 30;
					if (centerBox.next().length > 0) {
						centerBox.attr("data-center", "false").next().attr("data-center", "true");
					} else {
						carousel.find("figure:last-child").attr("data-center", "false");
						carousel.find("figure:first-child").attr("data-center", "true");
					}
				} else {
					x = x - 30;
					if (centerBox.prev().length > 0) {
						centerBox.attr("data-center", "false").prev().attr("data-center", "true");
					} else {
						carousel.find("figure:last-child").attr("data-center", "true");
						carousel.find("figure:first-child").attr("data-center", "false");
					}

				}
				carousel.attr("data-index", x);
				carousel.css("transform", "translateY(-50%) translateZ(0) rotateX(" + x + "deg)");



			}
		});
		////////////////// galery
		$('#section5 .right').on('mouseenter', function() {
				var windowX=$(window).width();
				var maxX=$("#section5 .container").width();
				var maxRight=-1*((maxX-windowX));
			this.iid = setInterval(function() {
				var leftNow=parseInt($("#section5 .container").css("left").replace("px", ""));
				console.log(windowX+"--"+maxX+"--"+leftNow);
				if (leftNow > maxRight) {
					leftNow=leftNow-2;
					$('#section5 .container').css('left',leftNow+'px' );
				}else{
					this.iid && clearInterval(this.iid);
				}
			}, 1);
		}).on('mouseleave', function(){
			this.iid && clearInterval(this.iid);
		});

		$('#section5 .left').on('mouseenter', function() {
			this.iid = setInterval(function() {
				var leftNow=parseInt($("#section5 .container").css("left").replace("px", ""));

				if (leftNow < 20){
					leftNow=leftNow+2;
					$('#section5 .container').css('left',leftNow+'px' );
				}else{
					this.iid && clearInterval(this.iid);
				}
			}, 1);
		}).on('mouseleave', function(){
			this.iid && clearInterval(this.iid);
		});
		////
		$( "#section5 .items").on( "click", function() {
			$( "#section5 .items" ).attr("data-show","false");
			$(this).attr("data-show","true");
		});
		//////////////////
	});
</script>
