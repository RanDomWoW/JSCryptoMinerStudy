<script type="text/javascript">
  jQuery(document).ready(function () {
    var printContent = jQuery(".InnerContent");
	if(printContent.length > 0)
	{
      jQuery(".global-print").click(function(){
        var direction = "";
        if(jQuery("body").hasClass("RTL"))
        {
          direction = "RTL";
        }
        else
        {
          direction = "LTR";
        }
        var mode = "popup";
        var close = "false";
        var extraCss = "/CMSPages/GetCSS.aspx?stylesheetname=Niroo-Portal-Responsive";
        var keepAttr = "class,id,style,on";
        var headElements = '<meta charset="utf-8" />,<meta http-equiv="X-UA-Compatible" content="IE=edge"/>'
			var options = { mode : mode, popClose : close, extraCss : extraCss, retainAttr : keepAttr, extraHead : headElements, direction : direction };
          jQuery(printContent).printArea(options);
		});
	}
  else
  {
    jQuery(".GlobalPrint").remove();
  }
  
  // Tov menu 
  jQuery(".TopBox").not(jQuery(".TopBox:has(.mid-nav)")).remove(); 
  // Slider
  jQuery('.slider').owlCarousel({
    items:1,
    rtl:true,
    nav:false,
    loop: true,
    dots: false,
    thumbs:false,
    lazyLoad:true,
    navSpeed:1000,
    autoplay:true,
    autoplaySpeed:2000,
    autoplayTimeout:5000,
    animateOut: 'fadeOut'
  }); 
  
  // News Carousel
  jQuery('.NewsSliderContainer').owlCarousel({
    items: 1,
    rtl:true,
    nav: false,
    loop: true,
    dots: false,
    autoplay: true,
    autoplayHoverPause: true,
    navSpeed: 800,
    autoplaySpeed: 1000,
    autoplayTimeout: 6500,
    animateOut: 'fadeOut',
    navText: ["<i class='fa fa-angle-right'></i>", "<i class='fa fa-angle-left'></i>"],
    theme: "Carousel"
  });
  
  // Classes
  if(jQuery('.InnerSideBar > .SimpleBox').length > 0)
  {
    jQuery('.InnerContent').addClass('TwoColumn');
  }
  jQuery('.main-menu .MenuItemLI').last().addClass('LeftMenu');
  jQuery('.main-menu .MenuItemLI').last().prev().addClass('LeftMenu');
  jQuery('.main-menu .MenuItemLI').last().prev().prev().addClass('LeftMenu');
  
  
  
  
  jQuery('.Row1').wrapAll('<div class="RowContainer ClearBoth" />');
  jQuery('.Row2').wrapAll('<div class="RowContainer ClearBoth" />');
  
  // Menu
  var width = jQuery(window).width();
  if(width>=768)
  {
    jQuery('.main-menu .MenuItemLI').hover(
      function(){
        jQuery(this).children('.Sub1MenuItemUL').stop().slideDown(400);
      },
      function(){
        jQuery(this).children('.Sub1MenuItemUL').stop().slideUp(400);
      }
    );
  }
  
  else
  {
    jQuery('.main-menu .MenuItemLI a,.main-menu .MenuItemLI span').click( function(e) {
      if (jQuery(this).next().length > 0)
      {
        e.preventDefault();
        jQuery(this).next().collapse('toggle');
        jQuery(this).toggleClass('collapsed');
      }
    });
  }
  
  
  
  // Match height
  jQuery('.FooterMenu > ul > li').matchHeight();
  
  jQuery('.MidNavLink').matchHeight();
  
  // Numbering
  var si = 0;
  var ssi = 0;
  
  jQuery('.Table tr').each(function() {
    if(jQuery(this).hasClass("SiteRow"))
    {
      ssi = 0;
      si = si + 1;
      jQuery("td:first", this).html(si);
    }
    
    if(jQuery(this).hasClass("SubSiteRow"))
    {
      ssi = ssi + 1;
      jQuery("td:first", this).html(si + "-" + ssi);
    }
  });
});
  
  
  
  jQuery.fn.equalHeights = function(){
    return this.css("min-height", ( Math.max.apply(this, jQuery.map( this , function(e){ return jQuery(e).height() }) ) ));
  }
</script>
    
    

