<script>
  
  $( ".filterGroupTitle" ).click( function()
  {
    if( $( this ).find( '.filterGroupExpand' ).hasClass( 'contracted' ) )
    {
    	$( this ).find( '.filterGroupExpand' ).html( "&nbsp;&nbsp;-" ).removeClass( 'contracted' ).addClass( 'expanded' );
    	$( this ).find( '.filterGroupExpand' ).parent().parent().find( '.collection-container' ).slideDown();
    }
   	else if( $( this ).find( '.filterGroupExpand' ).hasClass( 'expanded' ) )
    {
    	$( this ).find( '.filterGroupExpand' ).html( "&nbsp;&nbsp;+" ).removeClass( 'expanded' ).addClass( 'contracted' );
    	$( this ).find( '.filterGroupExpand' ).parent().parent().find( '.collection-container' ).slideUp();
    }
  });
  
  $( ".filter-clear" ).siblings( '.scroll-content' ).find( '.collection-container' ).show();
  
  $( ".filter-clear" ).each( function()
  {
    if( $( this ).parent().hasClass( 'filter-group-product-type' ) && !( $( this ).siblings( '.scroll-content' ).find( '.selected' ).hasClass( 'filter-active-ejuice' ) ) )
    {
      $( '.filter-group-brand' ).find( '.filterGroupExpand' ).click();
    }
  });
  
  $( ".filterGroupExpand" ).each( function()
  {
  	if( $( this ).parent().parent().hasClass( 'filter-group-product-type' ) )
    {
      $( this ).parent().parent().find( '.collection-container' ).show();
      $( this ).remove();
    }
  });
  
  if( window.location.href == "https://ejuicedirect.com/pages/hardware" )
  {
    $( ".filter-menu" ).remove();
  }
</script> 
