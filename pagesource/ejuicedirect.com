<script type="text/javascript">
  function deferFilterjQuery() {
    if (window.jQuery) {
      $('body').on('click', '.filter-menu .mobile-header a', function(e){
          if(e.handled !== true) {
            $('#nav-toggle').toggleClass('active');
            $('.filter-menu .filter-group').not('.has_group_selected').find('h4, ul,.filter-clear').toggle('fast');
            e.preventDefault();
            e.handled = true;
          }
      });

      var windowWidth = $(window).width();
      $(window).resize(function() {
        if ($(window).width() != windowWidth) {
        windowWidth = $(window).width();

            if ($( window ).width() > 767) {
              $('.filter-menu .filter-group').find('h4, ul, .filter-clear').show();
            } /* else {
              $('.filter-menu .filter-group').not('.has_group_selected').find('h4, ul, .filter-clear').hide();
            } */
        }
      });
  }
  else {
    setTimeout(function() { deferFilterjQuery() }, 50);
  }
}

deferFilterjQuery();
</script>


<script>
  //Floating Login
  $( ".FLSwitchToLogin" ).click( function()
  {
    $( ".FLForgotPassword" ).slideUp();
    $( ".FLCreateAccount" ).slideUp();
    $( ".FLSignIn" ).slideDown();
  });
  $( ".FLSwitchToCreate" ).click( function()
  {
    $( ".FLSignIn" ).slideUp();
    $( ".FLCreateAccount" ).slideDown();
  });
  $( ".FLSwitchToForgotPassword" ).click( function()
  {
    $( ".FLSignIn" ).slideUp();
    $( ".FLForgotPassword" ).slideDown();
  });
  $( ".floatingLoginTrigger" ).click( function()
  {
    var windowWidth = Math.min( 400, $( window ).width() * 0.8 );
    var windowHeight = Math.min( 600, $( window ).height() * 0.8 );
    console.log( windowWidth, windowHeight );
    $( ".FLLoginWindowContainer" )
    	.css( 'width', windowWidth )
    	/*.css( 'height', windowHeight )*/
    	.css( 'left', ( $( window ).width() / 2 ) - ( windowWidth / 2 ) )
    	.css( 'top', 100 );
    //$( ".FLModalScreen" ).css( 'top', $( window ).scrollTop() ).fadeIn();
    $( ".FLModalScreen" ).css( 'top', 0 ).fadeIn();
  });
  
  $( ".FLModalScreen" ).click( function()
  {
    if( $( ".FLModalScreen" ).css( 'display' ) == 'block' )
    {
      if( $( ".FLLoginWindowContainer:hover" ).length <= 0 )
      {
        $( ".FLModalScreen" ).fadeOut();
      }
    }
  });
  
  //About Us Blurb
  $( ".aboutUsReadMoreButton" ).click( function()
  {
    $( ".homePageAboutUsHidden" ).slideDown();
    $( this ).remove();
  });
</script>

