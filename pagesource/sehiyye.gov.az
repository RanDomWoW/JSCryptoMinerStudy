$().ready(
function()
{
    $('#slides li').each(function(index, elm)
    {
        $(elm).hover(function()
                {
                     if (cur_slide != index)
                     {
                         $('#slides span').fadeOut('normal');
                         $('#slide' + index).fadeIn('normal');
                         $('#slides li').removeClass('here');
                         $(this).addClass('here');
                         cur_slide = index;
                     }
                     }
                
                );
    });
    $('#slide' + cur_slide).fadeIn('normal');
    $('#slides li').eq(cur_slide).addClass('here');
}
        );
