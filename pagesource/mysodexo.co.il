<script>
    function fixGal() { $("#image img").css("margin-left", (($("body").width() - $("#image img").width()) / 2)); }
    function validate() { if (isValid($('.usr')) && isValid($('.cmpy'))) $('.forgot').click(); return false }
    function isValid(f) { if (!f.is(":visible") || f.val() != "") return true; else return invalid(f, 'נא להזין שם חברה'); }
    $(function () { $('.usr').focus();  });
    $(document).ready(function () { init(); });
    function is_email(str) { return /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/.test(str); }
    function is_valid_id(id) { return !isErrTZ(id) }
    function showComp(){ $("#txtCmp").show(); $("#txtPas").removeClass("usr"); }
    function hideComp(){$("#txtCmp").val("").hide();}
    function init() { $("#txtCmp").hide(); toggle_company();}
    function toggle_company() {
        var obj = $("#txtUsr");
        if (obj != document.activeElement) {
            var username = obj.val().trim();
            if (username != "") {
                if (is_email(username) || is_valid_id(username))
                    get_company_name(username);
                else {
                    showComp();
                }
            }                
            else
                hideComp();
        }
    }
    function get_company_name(username) {
        $.ajax({
            type: "POST",
            url: "new_Default.aspx/CheckUserCompany",
            data: '{user_login_name: "' + username + '" }',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) { if(response.d == "True") hideComp(); else showComp(); },
            failure: function (response) { showComp(); }
        });
    }
    function loadAddr() { showLoader(); $('#switch_address .text').load('/mobile/ChangeAddress.aspx?tour=1&ref=/new_rests.aspx form', 0, function () { loadAddressesPopup(); $(this).removeAttr("style"); $('.addr').click = function () { return loadAddressesPopup(); }; fixAddr(); fixHD(); }); return false; }
    var slct = "#radAddress tr";
	if (window !== window.top) setTimeout(init, 5);
</script>
