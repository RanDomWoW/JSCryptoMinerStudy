                                        clearTimeout(req);
                                        req = undefined;
                                    }
                                }
                            }
                        }

                        /* Helper methods */
                        function navigateToResults(e) {
                            if (!e)
                                e = window.event;

                            if (e.stopPropagation) {
                                e.stopPropagation();
                            }
                            else {
                                e.cancelBubble = true;
                            }
                            if (e.preventDefault) {
                                e.preventDefault();
                            }
                            else {
