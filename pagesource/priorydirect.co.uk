                        messageContainer.html(successMessage);
                    }
                    else {
                        messageContainer.html(errorMessage);
                    }
                }
            }
            function OnSortableUpdateError(xhr, ajaxOptions, thrownError) {
                //alert('In sortable update error');
                messageContainer.html(errorMessage);
            }
        });
