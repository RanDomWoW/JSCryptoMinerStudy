<script type="text/javascript"> 
//定时刷新
function onTimer(){
  var lksrc = "http://www.stc.gov.cn/sslkftp/data/lk.js?" + Math.round(1000 + Math.random() * 9000);
  var script = document.createElement('script');
  script.setAttribute('src', lksrc);
  script.setAttribute('charset', "GBK");
  document.body.appendChild(script);
}
//2分钟刷新一次
window.setInterval("onTimer()", 120000);
 
//显示路况信息,参数为编号,类型,信息,循环
function show() {
  var nos = [], types = [], msgs =[];
  for(var i=0; i<arguments.length; i=i+3) {
    nos.push(arguments[i]);
    types.push(arguments[i+1]);
    msgs.push(arguments[i+2]);
  }
  var msgdiv = document.getElementById("MainMSG");
  for(i=0; i<msgs.length; i++) {
    //检查是否存在,存在就不加入
    var exist = false;
    for(no in msgdiv.childNodes) {
      if(typeof msgdiv.childNodes[no].msgno !== "undefined") {
        if(msgdiv.childNodes[no].msgno === nos[i]) {

          exist = true;
        }
      }
    }
    if(exist) continue;
    
    var div = document.createElement("div");
    div.msgno = nos[i];
    if(types[i]===1) div.className = "ziti3";
    if(types[i]===2) div.className = "ziti4";
    if(types[i]===3) div.className = "ziti2";
    div.innerHTML =  "　　"+msgs[i];
    msgdiv.appendChild(div);
  }
  
  //显示不存在的
  for(var i=0; i<msgdiv.childNodes.length; i++) {
    var div = msgdiv.childNodes[i];
    var exist = false;
    if(typeof div.msgno === "undefined") continue;
    for(j=0; j<msgs.length; j++) {
      if(div.msgno === nos[j]) exist=true;
    }
    if(!exist) {
      msgdiv.removeChild(div);
      i--;
    }
  }
}
onTimer();
</script>
