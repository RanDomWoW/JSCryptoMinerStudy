 <script type="text/javascript">
   function save_email() {
      email = $('#input_email_footer').val();

      if (!email  ) {
        $('#email_result').html('<sapn style="color:#EA1B4C;font-size:11px;">ایمیل خود را وارد کنید </span>')
        return;
      }

       var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
       if( !emailReg.test( email )){
       $('#email_result').html('<sapn style="color:#EA1B4C;font-size:11px;">ایمیل خود را به شکل صحیح وارد کنید </span>')
        return;
       }

    $.ajax({
        url: 'index.php?route=common/footer/save',
        type: 'post',
        data: 'email='+email,
        dataType: 'json',
        beforeSend: function() {
          //$('#button-login').button('loading');
    },
        complete: function() {
            $('#button-login').button('reset');
        },
        success: function(json) {
             if (json) {
              $('#email_result').html('<sapn style="color:green;font-size:11px;">ایمیل شما با موفقیت ثبت شد </span>');
              $('#input_email_footer').val('');
             }
       },
        error: function(xhr, ajaxOptions, thrownError) {
            alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
        }
    });
 
   }

