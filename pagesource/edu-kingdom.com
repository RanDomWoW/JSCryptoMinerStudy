<script>
jq(document).ready(function($){

    var d = new Date();
    var curr_month = ("0" + (d.getMonth() + 1)).slice(-2)
    var curr_year = d.getFullYear();
    changeEvent('date',curr_year,curr_month);

    // inline
    $('#calendar').calendar({
        width: 300,
        height: 260,
        date: new Date(),
        onSelected: function (view, date, data) {

            if (view == 'date') {

                $('.t_des').html('正在加載數據中...');
                $('.t_des').show();
                $('.t_event').hide();

                var d = new Date(date);
                var curr_date = ("0" + d.getDate()).slice(-2)
                var curr_month = ("0" + (d.getMonth() + 1)).slice(-2)
                var curr_year = d.getFullYear();
                d = curr_year+'-'+curr_month+'-'+curr_date;

                $.post("https://www.edu-kingdom.com/ek/assets/event_daily_json.php", {date: d}, function(data, status){
                    $('.t_des').hide();
                    $('.t_event').html('');
                    $('.t_event').append(d);
                    var db = JSON.parse(data);
                    if(db){
                        $.each(db, function( index, value ) {
                            $('.t_event').append('<div>\
                                    <div><p class="css_icon_circle"></p></div>\
                                    <div><a href="https://www.edu-kingdom.com/ek/event/'+value.id+'" target="_blank">'+value.name+'</a></div>\
                                </div>');
                        });
                    }
                    $(".t_event").slideDown( "slow", function() { });
                });
            }

        },
        viewChange: function (view, y, m) {
            var mmm = ("0" + m).slice(-2);
            changeEvent(view,y,mmm);
        }
    });

    function changeEvent(view,y,m)
    {
        // get current month
        if (m == "ed") {
            var dt = new Date();
            m = dt.getMonth();
            m = ("0" + (m+1)).slice(-2);
        }

        // get this month data
        if (view == 'date') {
            $.post("https://www.edu-kingdom.com/ek/assets/event_monthly.php", {y: y, m: m}, function(data, status){
                if(data.trim()){
                    var db = JSON.parse(data);
                    setTimeout(function(){ $('#calendar').calendar('setData', db) }, 300);
                }
            });
        }
    }

    // show view month
    $( "#calendar .calendar-display" ).click(function() {
        $( "#calendar .view-month" ).show(200);
    });

    // active gcalendar btn
    $(".t_event").delegate("input","change",function(){
          if ($(".t_event input").is(':checked')){
            $(".gcalendar_btn").addClass("gcalendar_active");
            $('.gcalendar_btn').removeAttr('disabled');
          } else {
            $(".gcalendar_btn").removeClass("gcalendar_active");
            $('.gcalendar_btn').attr('disabled','disabled');
          }
    });

    // google calendar
    $(".gcalendar_icon").click(function() {
        var date = $('#calendar .selected').attr('data-calendar-day');
        if (date == null) {
            date = $('#calendar .now').attr('data-calendar-day');
        }
        var d = new Date(date);
        var curr_date = ("0" + d.getDate()).slice(-2)
        var curr_month = ("0" + (d.getMonth() + 1)).slice(-2)
        var curr_year = d.getFullYear();
        d = curr_year+'-'+curr_month+'-'+curr_date;

        $.post("https://www.edu-kingdom.com/ek/assets/event_daily_json.php", {date: d}, function(data, status){
            $('.t_des').hide();
            $('.t_event').html('');
            $('.t_event').append(d);
            var db = JSON.parse(data);
            if(db){
                $.each(db, function( index, value ) {
                    $('.t_event').append('<div>\
                            <div><input type="checkbox" name="id[]" value="'+value.id+'"></div>\
                            <div><a href="https://www.edu-kingdom.com/ek/event/'+value.id+'" target="_blank">'+value.name+'</a></div>\
                        </div>');
                });
                $('.t_event').append('<button class="gcalendar_btn">確定</button>');
                $('.gcalendar_btn').attr('disabled','disabled');
            }
        });

    });

});
</script>

