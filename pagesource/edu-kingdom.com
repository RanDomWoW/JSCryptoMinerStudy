<script>
$.noConflict();
jQuery(document).ready(function($) {
    // active gcalendar btn
    $(".t_event").delegate(".gcalendar_btn","click",function(){

        // get checked event
        if ($(".t_event input").is(':checked')){
            var checked = [];
            $(".t_event input:checked").each(function ()
            {
                checked.push(parseInt($(this).val()));
            });

            if (checked) {
                if (gapi.auth2.getAuthInstance().isSignedIn.get()) {
                  add_event(checked);
                } else {
                  $.when(gapi.auth2.getAuthInstance().signIn()).done(function(){
                    add_event(checked);
                  });
                }
            }
        }
        $(".t_event input").prop('checked', false);
        $(".gcalendar_btn").removeClass("gcalendar_active");
        $('.gcalendar_btn').attr('disabled','disabled');
    });


function add_event(checked){

    $.post("https://www.edu-kingdom.com/ek/assets/event_detail.php", {checked: checked}, function(data, status){

        var json_data = JSON.parse(data);
        if (json_data) {

            var batch = gapi.client.newBatch();
            $.each( json_data, function( key, value ) {

                var events = {
                    'summary': value.name,
                    // 'location': '800 Howard St., San Francisco, CA 94103',
                    'description': value.description,
                    'start': {
                      'date': value.start_date,
                      'timeZone': 'Asia/Hong_Kong'
                    },
                    'end': {
                      'date': value.start_date,
                      'timeZone': 'Asia/Hong_Kong'
                    },
                      'reminders': {
                        'useDefault': false,
                        'overrides': [
                          {'method': 'popup', 'minutes': 30}
                        ]
                      }
                };

                batch.add(gapi.client.calendar.events.insert({
                    'calendarId': 'primary',
                    'resource': events
                }));

            });

            batch.then(function(){
                alert('已新增至你的Google Calendar');
            });

        }
    });
}

});
</script>
