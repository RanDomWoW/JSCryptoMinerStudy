<script type="text/javascript">
var jq = $.noConflict();
jq(document).ready(function($){

    // full text search database by keyword
    $('#search_keyword').typeahead({
        name: 'search',
        remote: 'https://www.edu-kingdom.com/ek/assets/search_course.php?query=%QUERY'
    });

    // keypress enter submit search form
    $("#search_keyword").keypress(function(event) {
        if (event.which == 13) {
            event.preventDefault();
            $("#form-search").submit();
        }
    });

    // get total course
    $.post("https://www.edu-kingdom.com/ek/assets/course.php", {d: ''}, function(data, status){
        var totalcourse = data.trim();

        totalcourse = number_format(totalcourse);

        $('.t_totalcourse').append('共 '+totalcourse+' 可供搜尋');
    });

    function number_format(x) {
        return isNaN(x)?"":x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
    }


    $( "#form-search" ).submit(function( event ) {

        if(!$('#search_keyword').val() && !$('#district').val() && !$('#age').val() && !$('#fee').val() && !$('#catid').val()){

            $('.search_note').css('margin-top', '10px');
            $('.search_note').html('請輸入關鍵字或選擇其中一項條件搜尋');
            return false;
        }

        var keyword = $('#search_keyword').val();
        keyword = keyword.replace(/<|>|\'|\"|\//g, "");
        $('#search_keyword').val(keyword);

    });



    // search hot keyword
    $.post("https://www.edu-kingdom.com/ek/assets/search_keyword.php", {}, function(data, status){
        if(data.trim()){
           $('#t_hotkeyword').html(data);
        }
    });

});
</script> 
