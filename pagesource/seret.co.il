	$(function(){
    var menu = $('#stickytop'),pos = menu.offset();
      $(window).scroll(function(){
				if(menustuck){
				    //== Do Nothing. Mobile meny already displayed			
						}
        else if(showmobilemenu || ($(this).scrollTop() > pos.top+menu.height() && menu.hasClass('defaultm'))){
          menu.fadeOut(90, function(){
  					if($('#topbannercontainer')!=null){$('#topbannercontainer').hide();}
						if(($('.wideview')!=null) && (window.matchMedia("screen and (max-width: 800px)").matches)){$('.wideview').hide();$('.icmenu').show();$('#menu').hide();}
            $('#mainHeader .maincontent').css('min-height', 'auto');
						$('#mainHeader').css({'min-height':'auto','margin-top':'-8px'});
						$(this).removeClass('defaultm').addClass('fixedm').fadeIn(90);
						if(showmobilemenu) {menustuck = true;$('#topmobilespacer').show();}
          });
        } else if($(this).scrollTop() <= pos.top && menu.hasClass('fixedm')){
          menu.fadeOut(90, function(){
  					if($('#topbannercontainer')!=null){$('#topbannercontainer').show();}
						if(($('.wideview')!=null) && (window.matchMedia("screen and (max-width: 800px)").matches)){$('.wideview').show();$('.icmenu').hide();$('#menu').show();}
            $('#mainHeader .maincontent').css('min-height', '98px');
						$('#mainHeader').css({'min-height':'98px','margin-top':'0px'});
						$(this).removeClass('fixedm').addClass('defaultm').fadeIn(90);
          });
        }
      });
  });
</script>	


