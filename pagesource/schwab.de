    window.general.initSession = function() {

        var xhttp = new XMLHttpRequest();

        xhttp.onreadystatechange = function () {
            if (xhttp.readyState === 4) {
                if (xhttp.status === 200) {
                    var sessionData = JSON.parse(this.responseText.trim());
                    if (window.general.user && sessionData && sessionData.sToken && sessionData.sCSRFToken) {
                        window.general.user.sToken = sessionData.sToken;
                        window.general.user.sCSRFToken = sessionData.sCSRFToken;
                    }

                } else {
                    oTracking.pushEvent('Tech. Info', 'init-session', 'response status code = ' + xhttp.status);
                }
            }
        };

        xhttp.onerror = function () {
            oTracking.pushEvent('Tech. Info', 'init-session', 'response status test = ' + xhttp.statusText);
        };

        xhttp.open("GET",'/request/initsession.php', true);
        xhttp.send();
    };

