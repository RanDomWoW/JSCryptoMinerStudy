    <script>
        $(function () {
            $(".close_list").click(function () {
                $(".bag_list").slideToggle();
            });
            $(".shop_bag").click(function () {
                $(".bag_list").slideToggle();
            });
            
            $("#default >a").attr("class", "selected");
            
        })
        function reload_bag()
        {
            $.post("/Default.aspx", { "act": "master_load_cart" }, function (data) {
                var data = $.parseJSON(data);
                $(".list_info").empty();
                if (data.length > 0) {
                    var total = 0;
                    var items = 0;
                    for (var x = 0; x < data.length; x++) {
                        $(".list_info").append('<div class="item" option_id="' + data[x].item_id + '"><img src="upload/' + data[x].cover_img + '"><div class="detail">' + decodeURI(data[x].p_name) + '</br>NT$' + data[x].now_price + '</br>尺寸:' + decodeURI(data[x].option_name) + '</br>數量:' + data[x].qty + '</div><div class="close" onclick="delete_item($(this))"><a href="javascript:void(0)"><img src="assets/images/close.png"></a></div></div>');
                        total += parseInt(data[x].qty_price);
                        items += parseInt(data[x].qty);
                    }
                    $(".item_total").html(items);
                    $("#qty_total").html(total);
                }
                else
                {
                    $(".item_total").html(0);
                    $("#qty_total").html(0);
                }
            }, "text");
        }
        function delete_item(obj)
        {
            var item_id = obj.parent(".item").attr("option_id");
            $.post("/Default.aspx", { "act": "master_delete_option","option_id":item_id }, null,"script");
        }
    </script>
