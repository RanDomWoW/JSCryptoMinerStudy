  jQuery("ul#sliderhorizontal li").removeClass("mouseOverlayActive", 600, "easeOutBounce");
  jQuery("ul#sliderhorizontal li").removeClass("mouseOverlay", 600, "easeOutBounce");
  }

  function SetDefaultSize()
  {
  eachliWidth = jQuery(".magicslider2").width() / 4;
  jQuery("ul#sliderhorizontal li").css("width", eachliWidth);

  elmwidth = jQuery('ul#sliderhorizontal li img').width() - eachliWidth;
  parentWidth = jQuery('ul#sliderhorizontal li img').offsetParent().width();
  percent = ((100*elmwidth/parentWidth)/2);
  jQuery("ul#sliderhorizontal li img").css("left", "-" + percent + "%");
  }
  jQuery(document).ready(function($){
  initSlider();
