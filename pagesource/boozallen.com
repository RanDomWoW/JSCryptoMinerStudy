    <div class="twitter_feed">

<div class="twitter-feed-wrapper" data-selectors="BoozAllen.false.3">
    <div class="row">
      <div class="columns small-12">
        <h4><i class="fa fa-twitter" aria-hidden="true"></i> Follow <a target="_blank" href="https://twitter.com/intent/follow?screen_name=BoozAllen">@BoozAllen</a></h4>
      </div>
    </div>
    <div class="twitter-target"></div>
    <script id="search-categories-handlebar" type="text/x-handlebars-template">
        <div class="row" data-equalizer data-equalize-on="medium">
            {{#each this.tweets}}
                <div class="columns small-12 medium-4 tweet-wrapper {{#if @last}}end{{/if}}" data-equalizer-watch>
                    {{#if this.id}}
                    <img src="{{#if isRetweet}}{{{retweetAvatar}}}{{else}}{{{avatar}}}{{/if}}" alt="" class="tweet-avatar">
                    <div class="tweet-details" data-equalizer-watch>
                        <div class="tweet-name {{#if isReetweet}}retweet{{/if}}"><i class="fa fa-retweet" aria-hidden="true"></i> {{name}}</div>
                        <div class="tweet-handle">{{#if isRetweet}}Retweeted{{else}}{{/if}}</div>
                        <div class="tweet-text">
                            {{{twemoji text}}}
                        </div>
                        <div class="tweet-extra">
                            <a href="https://twitter.com/intent/like?tweet_id={{id}}" target="_blank"><i class="fa fa-heart" aria-hidden="true"></i></a>
                            <time class="timeago" datetime="{{postDate}}"></time>
                        </div>
                    </div>
                    {{/if}}
                </div>
            {{/each}}
        </div>
    </script>
