                    <span class="new-rect">Yeni Sezon</span>
                {{ } }}
            </div><!-- End .item-image -->
            <div class="item-meta-container">
                <h3 class="item-name"><a href="{{=p.link}}">{{=p.title}}</a></h3>
                <span class="item-price">{{=p.price}} TL</span>
                {{ if(p.msrp>0){ }}
                    <span class="old-price">{{=p.msrp}} TL</span>
                {{ } }}
            </div><!-- End .item-meta-container --> 
        </div><!-- End .item -->
    {{ } }}
]]>
</script>

