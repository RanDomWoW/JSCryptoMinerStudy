<script type="text/javascript">

 (function($) {
	  var cache = [];
	  // Arguments are image paths relative to the current page.
	  $.preLoadImages = function() {
	    var args_len = arguments.length;
	    for (var i = args_len; i--;) {
	      var cacheImage = document.createElement('img');
	      cacheImage.src = arguments[i];
	      cache.push(cacheImage);
	    }
	  }
	})(jQuery);
	

function addHovers() {

	jQuery.each(jQuery('img.swap'), function(index, image) {
		var original = jQuery(image).attr('src');
		var over = original.split('.');
		if(over.length >= 2){
			over[over.length-2] += '_over';
			over = over.join('.');
		} else {
			over = original + '_over';
		}

		jQuery.preLoadImages(over);
		
		if(jQuery(image).hasClass('active')){
			jQuery(image).attr('src', over);
		}else if(original.indexOf('_over') == -1){

			jQuery(image).mouseover(function() { 
				jQuery(this).attr('src', over);
			});
			jQuery(image).mouseout(function() { 
				jQuery(this).attr('src', original);
			});
		}
	});

	
	jQuery.each(jQuery('a.swap'), function(index, image) {
		var original = jQuery(image).css('background-image').replace(/[^(]*\(([^)]*)\).*/, '$1').replace(/\"/g, '');
		var over = original.split('.');
		
		if(over.length >= 2){
			over[over.length-2] += '_over';
			over = over.join('.');
		} else {
			over = original + '_over';
		}
		jQuery.preLoadImages(over);
		
		if(jQuery(image).hasClass('active')){
			jQuery(image).css('background-image', over);
		}else if(original.indexOf('_over') == -1){

			jQuery(image).mouseover(function() { 
				jQuery(this).css('background-image', 'url('+over+')');
			});
			jQuery(image).mouseout(function() { 
				jQuery(this).css('background-image', 'url('+original+')');
			});
		}
	});
	
}



jQuery(document).ready(function() {

