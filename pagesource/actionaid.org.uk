									var appendString = '', x, classes, enteredName, outputName;

									$.get( "/json/latest-donations/3")
								  .done(function( donations ) {
										if (donations.length > 0) {

											heroBanner.append('<div class="latest-donations-widget" id="latest-donations-box"/>');

											var latestDonationsBox = $("#latest-donations-box");


											for (x in donations)
											{
classes = '';
												if (donations[x].first_name)
												{
													enteredName = donations[x].first_name;
												} else {
													enteredName = '';
												}
												enteredName = enteredName.toLowerCase().trim();
												enteredName = enteredName.charAt(0).toUpperCase() + enteredName.slice(1);

												switch (enteredName)
												{

													case 'Null':
													case 'Test':
													case 'test':
													case '':
													case ' ':
														outputName = 'A donor';
														break;
													default :
														outputName = enteredName;
													
												}

												if (x > 2)
												{
													classes = ' hidden-xs hidden-sm hidden-md hidden-letter-box'
												}

												switch (donations[x].method)
												{
													case 'RG':
														appendString += '<li class="regular-donation' + classes + '"><span class="name">' + outputName + '</span> set up a <span class="amount">£' + donations[x].amount + '</span> monthly gift<br/><span class="time-and-date">' + donations[x].dateTime + '</span></li>';
														break;

													default:
													appendString += '<li class="single-donation' + classes + '"><span class="name">' + outputName + '</span> had their donation doubled to <span class="amount">£' + (donations[x].amount * 2) + '</span><br/><span class="time-and-date">' + donations[x].dateTime + '</span></li>';
												}

											}

											latestDonationsBox.append('<h3>Recent donations</h3><ul>' +  appendString + '</ul><p><a class="secondary-link" href="https://www.actionaid.org.uk/donate"><span>Make a donation to our appeal</span></a></p>');

										}
								  });

						    })(jQuery);

						  }

						</script> 
