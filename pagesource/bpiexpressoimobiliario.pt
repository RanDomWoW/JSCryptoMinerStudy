<script type="text/javascript">
	var boxOpen = false; 
	var timeoutFunc = function() {
		if(!boxOpen) {
			jQuery('#caixa-particulares').fadeOut('fast');
			jQuery('#caixa-profissionais').fadeOut('fast');
			clearInterval(timeoutId);
		}
	} 

	var timeoutId = undefined; 

	var showParticulares = function(){
		jQuery('#caixa-particulares').show();
		jQuery('#caixa-profissionais').hide();
	}

	var showProfissionais = function(){
		jQuery('#caixa-particulares').hide();
		jQuery('#caixa-profissionais').show();
	}

	jQuery('#particulares-login-link').click(showParticulares); 
	jQuery('#profissionais-login-link').click(showProfissionais); 

    jQuery('#particulares-login-link').mouseover(function(){
		boxOpen = true; 
	
		showParticulares(); 
		
		clearInterval(timeoutId); 
		timeoutId = setInterval(timeoutFunc, 500);
	})

    jQuery('#profissionais-login-link').mouseover(function(){
		boxOpen = true; 
		
		showProfissionais(); 

		clearInterval(timeoutId); 
		timeoutId = setInterval(timeoutFunc, 500);
	});

	jQuery('#caixa-particulares,#caixa-profissionais').mouseover(function(){
		boxOpen = true; 
	});

    jQuery('#particulares-login-link,#profissionais-login-link,#caixa-particulares,#caixa-profissionais').mouseout(function(){ boxOpen=false; });

</script>
