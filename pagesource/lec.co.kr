//1차섹션 메뉴_오버
function selSection(firstSec, secondSec, serialCode, table, fold, this_page) {
	topMenuObj = document.getElementById("TopMenu").getElementsByTagName("li");

	topCnt = topMenuObj.length;
	for(i = 0; i < topCnt; i++) {
		imgNo = i + 1;
		topMenuObj.item(0).style.backgroundPosition = "0 0";
		topMenuObj.item(1).style.backgroundPosition = "-63px 0";
		topMenuObj.item(2).style.backgroundPosition = "-125px 0";
		topMenuObj.item(3).style.backgroundPosition = "-176px 0";
		topMenuObj.item(4).style.backgroundPosition = "-237px 0";
		topMenuObj.item(5).style.backgroundPosition = "-299px 0";
		topMenuObj.item(6).style.backgroundPosition = "-349px 0";
		topMenuObj.item(7).style.backgroundPosition = "-408px 0";
		topMenuObj.item(8).style.backgroundPosition = "-485px 0";
		topMenuObj.item(9).style.backgroundPosition = "-941px 0";
		topMenuObj.item(10).style.backgroundPosition = "-536px 0";
		topMenuObj.item(11).style.backgroundPosition = "-601px 0";
		topMenuObj.item(12).style.backgroundPosition = "-706px 0";
		topMenuObj.item(13).style.backgroundPosition = "-781px 0";
		topMenuObj.item(14).style.backgroundPosition = "-864px 0";

	}

	if(this_page == "index.html") {
		topMenuObj.item(0).style.backgroundPosition = "0 -30px";

