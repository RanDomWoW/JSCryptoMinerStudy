<script type="text/javascript">
    (function ($, BaseOn, window) {
        $(function () {

            var navStrip = $('.vf-nav-strip'),
                nav = navStrip.find('nav'),
                mainMenu = nav.find('.vf-main-menu'),
                footerMenu = $('.defaultFooterMenu'),
                segmentMenu = nav.find('.vf-segment-menu'),
                serviceMenuItem = navStrip.find('.vf-service-menu__item.-wsc'),
                svgIconUser = serviceMenuItem.find('svg'),
                svgIconLoading = '<svg class="nav-icon nav-icon_loading" aria-hidden="true"><use xlink:href="#nav-icon_loading"></use></svg>',
                userLogged = false,
                showElement,
                initNav;

            showElement = function (selector) {
                var element = document.querySelector(selector);

                if (!element) {
                    return;
                }

                element.style.display = '';
            };

            initNav = function () {
                try {
                    new window.VFNavigation();
                } catch (e) {
                    console.log('Navigation init error.');
                }
            };

            svgIconUser.replaceWith(svgIconLoading);

            $.ajax({
                type: 'POST',
                url: 'https://www.vodafone.cz/_ajax_/VFOneWeb/user/',
                data: {
                    customerSegment: 'osobni'
                },
                success: function (response) {
                    if (typeof response.data === 'undefined') {
                        return;
                    }

                    var responseData = response.data;

                    userLogged = responseData.isLogged;

                    $('#vf-service-menu-item-wsc').html(responseData.htmlUserbox);

                    if (userLogged) {
                        navStrip.addClass('-logged-in -logged-in-wsc');
                    }

                    if (segmentMenu.length > 0) {
                        if (userLogged) {
                            segmentMenu.remove();
                        } else {
                            if (responseData.htmlSegemts.length > 0) {
                                segmentMenu.replaceWith(responseData.htmlSegemts);
                            }
                            showElement('.vf-segment-menu');
                        }
                    }

                    if (mainMenu.length > 0) {
                        if (responseData.htmlMenu.length > 0) {
                            mainMenu.replaceWith(responseData.htmlMenu);
                        }
                        showElement('.vf-main-menu');
                    }

                    if(footerMenu.length > 0) {
                        if(responseData.htmlFooter.length > 0) {
                            footerMenu.replaceWith(responseData.htmlFooter);
                            VFheader.footer.init();
                        }
                        showElement('.defaultFooterMenu');
                    }

                    initNav();
                    if (userLogged) {
                        var mapaStranek = $('.mapaStranek');
                        if (mapaStranek.length > 0) {
                            mapaStranek.remove();
                        }
                    }

                    BaseOn.OneWeb.isLogged = userLogged;
                    BaseOn.OneWeb.userInit();
                },
                dataType: 'json',
                timeout: 3000,
                cache: false,
                error: function (jqXHR, textStatus, errorThrown) {
                    $('#vf-service-menu-item-wsc').html('<a class="vf-service-menu__item -wsc" href="/muj/prihlaseni/" data-s-object-id="top menu:prihlaseni"><svg class="nav-icon nav-icon_nav-user" aria-hidden="true"><use xlink:href="#nav-icon_nav-user" href="#nav-icon_nav-user"></use></svg><span>Můj Vodafone</span></a>');
                    showElement('.vf-main-menu');
                    showElement('.defaultFooterMenu');
                    showElement('.vf-segment-menu');
                    initNav();
                }
            });
        });
    })(jQuery, BaseOn, window);
</script>
