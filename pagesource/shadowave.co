<script>
$(document).ready(function(){
$("#loginform").submit(function(){
   var user = $("#user_name").val();
   $.ajax({
   type:"POST",
   data: $(this).serialize(),
   url:"files/login.php",
   success: function(data){
   if (data == 1){
   $("#result").css({"font-size":"11px","color":"green"}).html("Login is done.Welcome "+user);
   window.location.replace("home");
   }else{
	$("#result").html(data).css("font-size","11px");
	}},
	beforeSend: function(){
	$("#submit").html("Loading...").attr("disabled", true);
	},
	complete: function(){
	$("#submit").html("Login").attr("disabled", false);
	},
	error: function(){
	$("#result").html("404 NOT FOUND");
	}});
	return false;
	});
	});
	</script>
