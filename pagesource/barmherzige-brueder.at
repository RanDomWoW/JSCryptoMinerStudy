<script type="text/javascript" language="javascript">
$(document).ready(function(){

	var mainTab = {
			open: function($elmCont, $elmClose) {
				var elm = $elmCont.attr("rel");
				var mapFrameLnk = $elmCont.find("iframe").attr("link");
				var mapFrameSrc = $elmCont.find("iframe").attr("src");

				$("#mainTabsContentWrapper").addClass("open");
				if (elm == "map") {
					$("#mainTabs .map").animate({height: "40px"}, 500, "easeOutCubic");
					if (typeof(mapFrameSrc) == "undefined" || mapFrameSrc == "") $elmCont.find("iframe").attr("src", mapFrameLnk);
					$("#mainTabsContent").animate({height: $elmCont.height() + $elmClose.height() + "px"}, 500, "easeOutCubic");
				} else {
					$("#mainTabs .dept").addClass("open");
					$("#mainTabs .dept").animate({height: "40px"}, 500, "easeOutCubic");
					$("#mainTabsContent").animate({height: $elmCont.height() + $elmClose.height() + "px"}, 500, "easeOutCubic");
				}
			}
	}


	$("#mainTabs .item").click(function () {$("#mainTabsContent ." + $(this).attr("rel"));
		var elm = $(this).attr("rel");
		var $elmCont = $("#mainTabsContent ." + elm);
		var $elmClose = $("#mainTabsContentWrapper .close");
		
		if ($("#mainTabsContent").height() > 0 && !$elmCont.hasClass("hidden")) {
// ===== close tab =====
			$("#mainTabs .item").animate({height: "20px"}, 500, "easeOutCubic");
			$("#mainTabsContent").animate({height: "0px"}, 500, "easeOutCubic", function(){
					$("#mainTabsContent .content").addClass("hidden");
					$("#mainTabsContentWrapper, #mainTabs .dept").removeClass("open");
				});


		} else if ($("#mainTabsContent").height() > 0 && $elmCont.hasClass("hidden")) {
// ===== open tab: other tab was previously opened =====
			$("#mainTabs .item").animate({height: "20px"}, 500, "easeOutCubic");
			$("#mainTabsContent").animate({height:"0px"}, 500,"easeOutCubic", function(){
					$("#mainTabsContentWrapper, #mainTabs .dept").removeClass("open");
					$("#mainTabsContent .content").addClass("hidden");
					$elmCont.removeClass("hidden");
					mainTab.open($elmCont, $elmClose);
				});


		} else {
// ===== open tab: all tabs were previously closed =====
			$("#mainTabsContent .content").addClass("hidden");
			$elmCont.removeClass("hidden");
			mainTab.open($elmCont, $elmClose);
		}
	});
	$("#mainTabsContentWrapper .close").click(function () {
		$("#mainTabs .item").animate({height: "20px"}, 500, "easeOutCubic");
		$("#mainTabsContent").animate({height: "0px"}, 500, "easeOutCubic", function(){
			$("#mainTabsContentWrapper, #mainTabs .dept").removeClass("open");
		});
	});
	
// ===== accordion =====
// ========================================================
	$("#mainTabsContent .header").click(function () {
		var $elmHeader = $(this);
		var $elmToggler = $(this).next();
		var elmHeight = $elmToggler.height();

		if (elmHeight == 0) {
			$("#mainTabsContent .heightToggler").not($elmToggler).animate({height: "0px"}, 500, "easeOutCubic");
			$elmToggler.animate({height: $elmToggler.find(".heightWrapper").height() + "px"}, 500, "easeOutCubic", function(){
				var $elmCont = $("#mainTabsContent .dept");
				var $elmClose = $("#mainTabsContentWrapper .close");
				$("#mainTabsContent .header").removeClass("opened")
				$elmHeader.addClass("opened");
				
				$("#mainTabsContent").animate({height: $elmCont.height() + $elmClose.height() + "px"}, 500, "easeOutCubic");
			});
		}
	});


});
</script>
