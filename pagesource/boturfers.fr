        var yButton = 314;
        var yContent = 340;
    }
    else {
        var add = (nbR - 5) * 29;
        var yButton = 314 + add;
        var yContent = 340 + add;
    }
    
    if ($("#large_nav").css("display") == "none") {
        $("#thin_nav").css("display","none");
        $("#large_nav").css("display","block");
        $("#showContainer").css("top",yButton+"px");
        $("#content").css("margin-top",yContent+"px");
        $("#showThin").css("display","block");
        $("#showLarge").css("display","none");
    }
    else {
        $("#thin_nav").css("display","block");
        $("#large_nav").css("display","none");
        $("#showContainer").css("top","129px");
        $("#content").css("margin-top","150px");
        $("#showThin").css("display","none");
        $("#showLarge").css("display","block");
    }  
});

var lastScrollTop = 0;
$(window).scroll(function(event){if ($(window).width() > 1200) {var st = $(this).scrollTop();if (st > lastScrollTop){$("#scrollup").click();} else if (st == 0){$("#scrolldown").click();}lastScrollTop = st;}});

var identifier = window.location.hash;
if (identifier === "#arrivee") {$('html, body').animate({scrollTop : $('#arrivee').offset().top	- $("#content").css('margin-top').replace("px","")},'slow');}
else if (identifier === "#rapports") {$('html, body').animate({	scrollTop : $('#rapports').offset().top	- $("#content").css('margin-top').replace("px","")},'slow');}
else if (identifier === "#carriere") {$('html, body').animate({	scrollTop : $('#carriere').offset().top	- $("#content").css('margin-top').replace("px","")},'slow');}
$('a[href^="#"][id$="_scroll"]').click(function() {var the_id = $(this).attr("href");$('html, body').animate({scrollTop : $(the_id).offset().top - $("#content").css('margin-top').replace("px", "")},'slow');return false;});

$(window).scroll(function() {if ($(this).scrollTop() > 300) {$('#top_scroll').fadeIn();} else {$('#top_scroll').fadeOut();}});

</script>



