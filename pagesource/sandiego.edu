	<script type="text/javascript">
        $(document).foundation({
            equalizer: {
            equalize_on_stack: true
            }
        });
        $(document).foundation('equalizer', 'reflow');
        
        var modal = '';
        var modal_id = '';
        var iframe_match = '';
        var iframe_replace = '';
        var iframe_reset = '';
        //on reveal modal opened
        $(document).on('opened.fndtn.reveal', '[data-reveal]', function () {
            modal = $(this);
            modal_id = $(this).attr('id');
            mytest = $('iframe',this).attr('src');
            if (typeof mytest !='undefined') {
                iframe_match = $('iframe',this).attr('src').match("video.sandiego.edu/app/plugin/embed.aspx");
                iframe_replace = $('iframe',this).attr('src').replace("autoPlay=false","autoPlay=true");
                iframe_reset = $('iframe',this).attr('src').replace("autoPlay=true","autoPlay=false");
            }
            $("#"+modal.attr('id')).focus();
            if(iframe_match !== null){
               setTimeout(function(){
                $("#"+modal.attr('id')+" iframe").attr('src',iframe_replace);
               }, 1000);
            }
            
            //temporary fix to get x to close via keyboard tabbing and enter key
            $('.close-reveal-modal',this).html('<button aria-label="Close modal" class="close-button" data-close="" type="button"><span aria-hidden="true">×</span></button><a href="#" style="text-decoration:none;"></a>');
        });
        //on reveal modal closed
        $(document).on('closed.fndtn.reveal', '[data-reveal]', function () {
            $('a[data-reveal-id="'+ modal_id +'"]').focus();
            iframe_replace = '';
            $("#"+modal_id+" iframe").attr('src',iframe_reset);
        });
        //on reveal modal, tabbing past X, close modal 
        $('body').on('blur', '.close-button', function(e){
            $("#"+modal_id).foundation('reveal', 'close');
        });
    </script>
