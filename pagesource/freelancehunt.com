        var videoId = container.dataset.videoId;
        container.onclick = function labnolIframe() {
            var iframe = document.createElement("iframe");
            iframe.setAttribute("src", "//www.youtube.com/embed/" + videoId + "?autoplay=1&autohide=2&border=0&wmode=opaque&enablejsapi=1&controls=1&showinfo=0&modestbranding=0&rel=0&fs=1");
            iframe.setAttribute("frameborder", "0");
            iframe.setAttribute("id", "youtube-iframe");
            container.innerHTML = '';
            container.appendChild(iframe);
            container.style.paddingBottom = "56.25%"; /* assuming that the video has a 16:9 ratio */
        };
    })();

    window.onload = function() {
        var activeLang = document.getElementsByTagName("html")[0].getAttribute("lang");
        var preferredLang = typeof window.navigator.languages !== 'undefined' ? window.navigator.languages[0]: window.navigator.language;
        var modalOptions = {
            focus: false
        };

        if (!window.localStorage.getItem('fh_skip_lang_offer')) {
            if (preferredLang === 'uk' && activeLang !== 'uk') {
                var modal = new RModal(document.getElementById('uk-lang-modal'), modalOptions);
                document.addEventListener('keydown', function (e) {
                    modal.keydown(e);
                }, false);
                modal.open();
                window.fhLangModal = modal;
            } else if (preferredLang.startsWith('en-') && activeLang !== 'en') {
                var modal = new RModal(document.getElementById('en-lang-modal'), modalOptions);
                document.addEventListener('keydown', function (e) {
                    modal.keydown(e);
                }, false);
                modal.open();
                window.fhLangModal = modal;
            }
        }
    }
