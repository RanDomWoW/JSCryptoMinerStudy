            else if(window.location.pathname == "/videos" || window.location.pathname == "/videos/" || window.location.pathname == "/videos/favourites" || window.location.pathname == "/videos/favourites/" || window.location.pathname == "/videos/watch_later" || window.location.pathname == "/videos/watch_later/")
            {
                $('#header-videos').addClass('active');
            }
			else if(window.location.pathname == "/willowhockeyschedules")
			{
				$('#header-UpcomingHockey').addClass('active');
			}
			else if(window.location.pathname == "/tvchannel")
			{
				$('#header-tvchannel').addClass('active');
			}
			else if(window.location.pathname == "/devices")
			{
				$('#header-iptv').addClass('active');
			}
			else if(window.location.pathname == "/contactus")
			{
				$('#header-contactus').addClass('active');
			}
			else if( window.location.href.indexOf("/watch-live/") != -1 )
			{
				// window.scrollTo(0, $("#live_subscription_msg").offset().top-50);
				$('html, body').animate({scrollTop: $("#live").offset().top-50}, 2000);
			}
			else if( window.location.href.indexOf("/videos/") != -1 )
			{
                $('#header-videos').addClass('active');
			}			
		});

		

        

</script>



