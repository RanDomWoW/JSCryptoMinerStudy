    <div class="bg-midnight-swim calm-wave-midnight-swim-before p-large-y">
        <div class="container">
            <div class="row"
                 ng-controller="newsletterController as newsletterCtrl"
                 model-autowire="newsletterSubscribeModel"
                 ng-cloak>
                <div class="col-md-8">
                    <div class="row flex-md y-center">
                        <div class="col-md-7 text-inverse">
                            <h4 class="m-small-bot-xs text-center text-left-md">SIGN UP FOR E-DEALS</h4>
                            <p class="text-center text-left-md">Get our best cruise details and news as soon as they hit the deck.</p>
                            <p class="text-center text-left-md"
                               ng-class="{'text-danger': newsletterCtrl.hasError(), 'text-success': !newsletterCtrl.hasError()}"
                               ng-show="newsletterCtrl.showMessage()">
                                <i class="fa p-small-right-xs"
                                   ng-class="{'fa-close': newsletterCtrl.hasError(), 'fa-check': !newsletterCtrl.hasError()}"></i>
                                <span>{{newsletterCtrl.response.Message}}</span>
                            </p>
                        </div>
                        <div class="col-md-5 text-center text-left-md">
                            <button type="submit"
                                    class="btn btn-white btn-inverse no-animate"
                                    ng-show="!newsletterCtrl.showSubscribeForm"
                                    ng-click="newsletterCtrl.toggleSubscribeForm()">
                                SUBSCRIBE NOW
                            </button>
                        </div>
                    </div>
                    <collapse-block-component open="newsletterCtrl.showSubscribeForm">
                        <form name="newsletterCtrl.form"
                              ng-submit="newsletterCtrl.submit()"
                              novalidate>
                            <div class="row">
                                <div class="col-xs-12 col-md-6">
                                    <input-text-component name="emailAddress"
                                                          label="'Email'"
                                                          required="'true'"
                                                          inverse="'true'"
                                                          pattern="newsletterCtrl.emailPattern"
                                                          model="newsletterCtrl.request.EmailAddress"></input-text-component>
                                </div>
                                <div class="col-xs-12 col-md-4">
                                    <input-text-component name="postCode"
                                                          label="'Post Code'"
                                                          required="'true'"
                                                          inverse="'true'"
                                                          pattern="newsletterCtrl.postCodePattern"
                                                          model="newsletterCtrl.request.PostCode"></input-text-component>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-12 col-md-4">
                                    <input-select-component name="title"
                                                            label="'Title'"
                                                            required="true"
                                                            options="'title.Value as title.Text for title'"
