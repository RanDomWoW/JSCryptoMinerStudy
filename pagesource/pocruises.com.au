    <!-- Detail -->
    <div class="h-100"
         ng-show="!modalCtrl.isMasterVisible">
        <div class="fade h-100"
             pass-element-directive="modalCtrl.$detail">
            <div class="col-md-7 h-100 flex-xs flex-col y-center">
                <div class="row flex-xs flex-center p-base-y-xs">
                    <div class="col-md-9">
                        <!-- Back -->
                        <div class="p-large-bot-xs">
                            <a href="" class="link-inverse clickable" ng-click="modalCtrl.toggleMasterDetail()">
                                <i class="po-chevron fa fa-rotate-180 p-min-left-xs"></i>
                                <span>Back</span>
                            </a>
                        </div>
                        <!-- Title -->
                        <h2 class="m-0 p-large-top-xs">{{modalCtrl.selectedSubSection.Title}}</h2>
                        <p class="m-0 p-large-y-xs">{{modalCtrl.selectedSubSection.Description}}</p>
                        <div ng-show="modalCtrl.selectedSubSection.Link">
                            <a ng-href="{{modalCtrl.selectedSubSection.Link.Url}}"
                               class="btn btn-white btn-color-waterfall">{{modalCtrl.selectedSubSection.Link.Text}}</a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-5 bg-white color-midnight-swim h-100 overflow-hidden theme-nav-modal-detail">
                <!-- Image -->
                
                <div class="row bg-cover-center detail-img"
                     data-media-id="{{modalCtrl.selectedSubSection.Image.MediaId}}"
                     style="background-image: url( {{modalCtrl.selectedSubSection.Image.Src}} );"></div>
                <!-- Index -->
                <div class="row flex-xs x-center overflow-y-scroll detail-list bg-white">
                    <div class="col-xs-12 p-base-y-xs">
                        <div class="relative"
                             ng-repeat="item in modalCtrl.selectedSubSection.SubSections track by $index"
                             ng-class="{'b-1-bot b-sand-darker': !$last}">
                            <a href="{{item.Link.Url}}"
                               class="p-base-y flex-xs flex-col y-center link-unstyled p-base-right-xs"
                               ng-show="item.SubSections.length == 0">
                                <span class="h4 m-0">{{item.Title}}</span>
                                <span class="p-small-top-xs">{{item.ShortDescription}}</span>
                            </a>
                            <div class="p-base-y flex-xs y-center clickable p-base-right-xs"
                                 ng-show="item.SubSections.length > 0"
                                 ng-click="modalCtrl.showSubsections($index)">
                                <span class="h4 m-0">{{item.Title}}</span>
                                <span class="p-small-top-xs">{{item.ShortDescription}}</span>
                            </div>
                            <div class="abs-x-right abs-y-center">
                                <i class="po-chevron font-size-base"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>

<script type="text/ng-template" id="chatModal.html">
    <!-- Master -->
    <div class="container"
         ng-show="modalCtrl.isMasterVisible">
        <div class="row flex-xs flex-center p-large-y-xs">
            <div class="col-md-10">
                <div class="w-100 fade in"
                     pass-element-directive="modalCtrl.$master">
                    <!-- Title -->
                    <div class="flex-xs flex-col x-center">
                        <span class="img-circle bg-white p-large relative">
                            <i class="po-contact fa-2x abs-center color-waterfall"></i>
                        </span>
                        <h2>Contact Options</h2>
                    </div>
                    <!-- Index -->
                    <div class="row flex-xs x-center p-large-top-xs">
                        <div class="col-xs-12 col-md-6">
                            <!-- Chat -->
                            <div class="relative b-1-bot b-white">
                                <div class="p-base-y flex-xs flex-col y-center clickable nowrap"
                                     ng-click="modalCtrl.agentStatus == true && modalCtrl.toggleMasterDetail()">
                                    <div class="block w-100 h5 m-0">
                                        <i class="po-contact fa-lg p-small-right-xs"></i>
                                        <span>Live chat</span>
                                    </div>
                                    <span class="color-shallow-blue p-min-top-xs"
                                          ng-show="modalCtrl.agentStatus == false">
                                        <i class="fa fa-ban p-small-right-xs" aria-hidden="true"></i>
                                        No agents available. Please come back later.
                                    </span>
                                </div>
                                <div class="abs-x-right abs-y-center">
                                    <i class="po-chevron font-size-base"></i>
                                </div>
                            </div>
                            <!-- Contact us -->
                            <div class="relative b-1-bot b-white">
                                <a href="/contact-us"
                                   class="p-base-y flex-xs y-center link-inverse link-unstyled nowrap h5 m-0">
                                    <i class="po-envelope p-small-right-xs"></i>
                                    <span>Contact us</span>
                                </a>
                                <div class="abs-x-right abs-y-center">
                                    <i class="po-chevron font-size-base"></i>
                                </div>
                            </div>
                            <!-- AU phone -->
                            <div class="relative b-1-bot b-white">
                                <div class="p-base-y flex-xs y-center nowrap h5 m-0">
                                    <i class="po-aus fa fa-lg p-small-right-xs" aria-hidden="true"></i>
                                    <span>1300 159 454</span>
                                </div>
                            </div>
                            <!-- NZ phone -->
                            <div class="relative b-1-bot b-white">
                                <div class="p-base-y flex-xs y-center nowrap h5 m-0">
                                    <i class="po-nz fa fa-lg p-small-right-xs" aria-hidden="true"></i>
                                    <span>0800 78 0716</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Detail -->
    <div class="h-100"
         ng-show="!modalCtrl.isMasterVisible">
        <div class="fade h-100"
             pass-element-directive="modalCtrl.$detail">
            <div class="col-md-7 h-100 flex-xs flex-col y-center">
                <div class="row flex-xs flex-center p-base-y-xs">
                    <div class="col-md-9">
                        <!-- Back -->
                        <div class="p-large-bot-xs">
                            <span class="link link-inverse clickable" ng-click="modalCtrl.toggleMasterDetail()">
                                <i class="po-chevron fa fa-rotate-180 p-min-left-xs"></i>Back
                            </span>
                        </div>
                        <!-- Title -->
                        <h2 class="m-0 p-large-top-xs">Live chat</h2>
                        <p class="m-0 p-large-y-xs">Please enter your details and click on Submit, our Customer Service Representative will join the Live Chat Session.</p>
                    </div>
                </div>
            </div>
            <div class="col-md-5 bg-white color-midnight-swim h-100 overflow-y-scroll">
                <!-- Index -->
                <div class="row flex-xs x-center overflow-y-scroll detail-list">
                    <div class="col-xs-12 p-base-y-xs">
                        <form class="ui form w-100 h-100 p-base-y"
                              name="modalCtrl.chatForm"
                              id="fbLiveChatModalBody"
                              novalidate
                              ng-submit="modalCtrl.chatForm.$valid && modalCtrl.startChat()">
                            <!-- user info -->
                            <input-text-component name="fbLiveChatName"
                                                  label="'Chat name*'"
                                                  required="'true'"
                                                  maxlength="126"
                                                  model="modalCtrl.chatNickName"></input-text-component>
                            <input-text-component name="fbLiveChatEmailId"
                                                  label="'Email*'"
                                                  required="'true'"
                                                  pattern="modalCtrl.emailPattern"
                                                  model="modalCtrl.userEmailAddress"></input-text-component>
                            <checkbox-component name="fbEmailSendTranscript"
                                                model="modalCtrl.sendTranscript">Email transcript</checkbox-component>
                            <input id="chatintent"
                                   type="hidden"
                                   name="chatintent_EDU"
                                   ng-value="modalCtrl.updateChatIntent()">
                            <div class="form-group flex-xs flex-wrap y-center m-0-bot-xs p-small-y">
                                <div class="m-0-bot-xs">Are you a travel agent?*</div>
                                <div class="p-small-left-xs">
                                    <input type="radio"
                                           id="travelagentradio1"
                                           name="aicTravelAgentRadioGroup"
                                           value="Yes"
                                           ng-model="modalCtrl.isAgent"
                                           ng-required="true">
                                    <label class="m-0" for="travelagentradio1">Yes</label>
                                </div>
                                <div class="p-small-left-xs">
                                    <input type="radio"
                                           id="travelagentradio2"
                                           name="aicTravelAgentRadioGroup"
                                           value="No"
                                           ng-model="modalCtrl.isAgent"
                                           ng-required="true">
                                    <label class="m-0" for="travelagentradio2">No</label>
                                </div>
                                <div class="w-100"
                                     ng-show="modalCtrl.chatForm.$submitted && modalCtrl.chatForm.aicTravelAgentRadioGroup.$error.required">
                                    <small>Please select yes or no.</small>
                                </div>
                            </div>
                            <div class="form-group p-min-y">
                                <label for="fbLiveChatQuestion">Question*</label>
                                <textarea class="form-control"
                                          id="fbLiveChatQuestion"
                                          placeholder="Enter a detailed question here"
                                          name="fbLiveChatQuestion"
                                          ng-model="modalCtrl.chatQuestion"
                                          ng-required="true"></textarea>
                                <small class="color-wildberry"
                                       ng-show="(modalCtrl.chatForm.$submitted || modalCtrl.chatForm.fbLiveChatQuestion.$touched) && modalCtrl.chatForm.fbLiveChatQuestion.$error.required">
                                    Please enter your question.
                                </small>
                            </div>
                            <!-- for chat api to display errors -->
                            <div id="fbLiveChatAlertError" class="hide">
                                <span></span>
                            </div>
                            <div id="fbLiveChatAlertSuccess" class="hide">
                                <span></span>
                            </div>
                            <!-- start chat -->
                            <div class="form-group p-base-y-xs">
                                <p><i>* indicates required fields.</i></p>
                                <button type="submit" id="fbLiveChatRequestBtn" class="btn btn-waterfall">Submit</button>
                            </div>
                        </form>
                        <!-- chat window -->
                        <div id="aicchat"></div>
                        <!-- chat external resources -->
                        <div chat
                             load-scripts="modalCtrl.loadScripts"
                             chat-load-success="modalCtrl.chatLoadSuccess()"
                             chat-load-error="modalCtrl.chatLoadError()"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>

<script type="text/ng-template" id="signinModal.html">
    <div class="row">
        <div class="p-base-x">
            <h3 class="m-0-top-xs">Sign in</h3>
            <p>Sign in to receive exclusive deals, cruise news and much more.</p>
            <login-form-component model="modalCtrl.model"
                                  await-login="modalCtrl.login()"
                                  class="block text-center">
            </login-form-component>
            <p class="text-center p-small-top-xs p-base-bot-xs m-0"><a href="/en/account/forgotpassword" class="link-underlined">Forgot your Password</a></p>
            <p class="text-center m-0">Not a Cruiseling yet? Sign up <a href="/en/account/register" class="link-underlined">here</a>.</p>
        </div>

    <div class="sidebar-signin__separator bg-shallow-blue relative m-small-top-xs">
        <div class="logo--sidebar abs-x-center abs-y-top bg-shallow-blue">
            <img src="/Content/images/B2C/cc-logo.svg" alt="Cruise Control logo" />
        </div>
    </div>
    <div class="p-small-top-xs p-base-x text-center">
        <img src="/Content/images/B2C/devices.png" class="img-responsive" alt="Cruise Control" />
        <p class="p-small-top-xs">All booked and ready to set sail? Manage your booking using the newly updated Cruise Control and start planning the perfect holiday.</p>
        <a href="https://cruisecontrol.pocruises.com.au" class="btn btn-waterfall btn-block">Visit cruise control</a>
    </div>
</div>

</script>

