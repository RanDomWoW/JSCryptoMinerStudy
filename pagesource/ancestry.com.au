<!--User Type Management-->
<script>
    window.userState = 'NRVisitor';
    if (typeof tao === 'undefined') {
        function baitData() {
            var fBait = document.cookie.match(/BAIT=([^; ]+)/),
                bait = fBait ? fBait[1] : 0;
            if (bait !== 0) {
                bait = unescape(bait);
                if (bait.slice(-1) === ';') {
                    bait = bait.substring(0,bait.length - 1);
                }
                return JSON.parse('{"' + bait.replace(/"/g, '\\"').replace(/;/g, '","').replace(/=/g,'":"') + '"}');
            }
            return 0;
        };
        function omnData() {
            var fOmn = document.cookie.match(/OMNITURE=([^; ]+)/),
                omn = fOmn ? fOmn[1] : 0;
            if (omn === 0) {
                return 'NRVisitor';
            }
            return omn.substring(5);
        }
        window.lohpBAIT = baitData();
        window.lohpOMN = omnData();
    } else {
        window.lohpBAIT = tao.c.BAIT;
        window.lohpOMN = tao.g.TYPE;
    };
    if (lohpBAIT !== 0) {
        if (lohpBAIT.CTrial === '1' || lohpOMN === 'Trialer') {
            window.userState = 'CurrentTrialer';
        } else if (lohpBAIT.CSub === '1' || lohpOMN === 'Subscriber') {
            window.userState = 'Subscriber';
        } else if (lohpBAIT.CSub === '0' && lohpBAIT.ESub === '1' && lohpOMN === 'Registrant') {
            window.userState = 'Winback';
        } else if (lohpBAIT.ETrial === '0' && lohpBAIT.ESub === '0' && lohpOMN === 'Registrant') {
            window.userState = 'RegNevTrialer';
        } else if (lohpBAIT.ETrial === '1' && lohpOMN === 'Registrant') {
            window.userState = 'RegPrevTrialer';
        };
    };
    document.body.classList.add('User_' + window.userState);
</script>
