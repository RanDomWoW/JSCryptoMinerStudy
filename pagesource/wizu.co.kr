		//서비스 뷰 댓글 문자열 체크
		$('.byteLimit').on({
            'keyup':function(){
				var
                    thisObject = $(this),
				    limit = thisObject.attr("limitbyte"), //제한byte를 가져온다.
				    str = thisObject.val(),
				    strLength = 0,
				    strTitle = "",
				    strPiece = "",
				    check = false,
				    strLengthnum = strLength;
				
				for (i = 0; i < str.length; i++){
					var
                        code = str.charCodeAt(i),
					    ch = str.substr(i,1).toUpperCase();
					//체크 하는 문자를 저장
					strPiece = str.substr(i,1);
					code = parseInt(code);
					 
					if ((ch < "0" || ch > "9") && (ch < "A" || ch > "Z") && ((code > 255) || (code < 0))){
						strLength = strLength + 3; //UTF-8 3byte 로 계산
					}else{
						strLength = strLength + 1;
					}
					 
					if(strLength>limit){ //제한 길이 확인
						check = true;
						break;
					}else{
						strTitle = strTitle+strPiece; //제한길이 보다 작으면 자른 문자를 붙여준다.
					}
				}
				thisObject.parent().parent().find('.word_num span').html(strLength);
				
				if(check){
					alert(limit+"byte 초과된 문자는 잘려서 입력 됩니다.");
					thisObject.parent().parent().find('.word_num span').html(limit);
				}
				thisObject.val(strTitle);
			}
		});

