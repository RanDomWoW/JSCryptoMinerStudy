                        + "&redirect_uri="+redirect_uri+"?rtnUrl="+rtnUrl;
                window.open(url,"login","width=500 height=500");
                sns_login.social_api_validate("payco");
            }
        }

        sns_login.logout = function(){
            var regType = "naver";
            if(regType == "kakao"){
                Kakao.Auth.logout(function() {
                    this.logout_process(regType);
                });
            }else{
                this.logout_process(regType);
            }
        }

        sns_login.logout_process = function(regType){
            // 위쥬 세션 데이터 삭제 처리.
            var url = "../social_auth/social_logout.php";
            $.ajax({
                type: "POST",
                url: url,
                data: {regType:regType},
                success: function (rs) {
                    if(rs.indexOf("logout")>-1){
                        location.replace('/');
                    }
                }
            });
        }

        sns_login.social_api_validate = function(regType){
            // 전달하는 타입의 앱키 유효성 체크
            var url = "http://wizu.co.kr/social_auth/social_api_validate.php";
            $.ajax({
                type: "GET",
                url: url,
                data: {"snsType" : regType},
                success: function(data){
                    //console.log(data);
                }
            });
        }

        sns_login.checkDevice = function(){
            // 접속 디바이스 PC,Mobile 구분
            var
                filter = "win16|win32|win64|mac",
                device = "";

            if (navigator.platform) {
                if (0 > filter.indexOf(navigator.platform.toLowerCase())) {
                    device = "mobile";
                } else {
                    device = "pc";
                }
            }
            return device;
        }

        sns_login.checkMember = function(snsData){

//            var ip = "<?//=$_SERVER['$_SERVER["REMOTE_ADDR"]']?>//";
//            if(ip == "61.36.175.165"){
//                console.log(data); return false;
//            }

            //return;
            // 회원 중복체크

            if(snsData) {
                $.ajax({
                    type: "POST",
                    url: "../ajax/ajax_check_data.php",
                    data: {snsData : JSON.stringify(snsData)},
                    success: function (data) {
                        var newsLetterCK = getCookie("letter_status") || "";

                        if(newsLetterCK != ""){
                            if(snsData.regType == "naver"){
                                window.opener.location.href = "../social_auth/social_auth.php";
                                self.close();
                            }else{
                                location.href = "../social_auth/social_auth.php";
                            }
                        }else{
                            if(data.loginData.rsData.memberYn == "y"){
                                if(snsData.regType == "naver"){
                                    window.opener.location.href = "../social_auth/social_auth.php";
                                    self.close();
                                }else{
                                    location.href = "../social_auth/social_auth.php";
                                }
                            }else{
                                if(snsData.regType == "naver"){
                                    window.opener.location.href = "../join.php";
                                    self.close();
                                }else{
                                    location.href = "../join.php";
                                }
                            }
                        }
                    },
                    dataType: "json"
                });
            }
        }

        window.SNS_LOGIN = sns_login;
    })(jQuery);

