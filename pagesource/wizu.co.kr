                    WIZU_LAYER.common.layer_set_template("nickName");
                    WIZU_LAYER.common.layer_pop_open_close();
                },
                // 위쥬레터
                wizuLetter : function(){
                    /**
                     * # letterStatus : 위쥬레터 신청 단계
                     * @ login : 로그인한 상태
                     * @ request : 위쥬레터 메일 발송 상태
                     */
                    var letterStatus = WIZU_COMMON.util.cookies.getCookie('letter_status');
                    if( letterStatus == "request"){
                        $("#letterStep1").hide();
                        $("#letterStep3").show();
                    }else{
                        var isMember = '';
                        if( isMember ){
                            var isNewsLetterGetMail = '';
                            if( isNewsLetterGetMail == "y"){
                                alert('회원님은 현재 위쥬레터 구독중 입니다.');
