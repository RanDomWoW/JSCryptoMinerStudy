                    WIZU_LAYER.common.layer_pop_open_close();
                },
                promotion : {
                    regist: function(){
                        WIZU_LAYER.common.layer_set_template("promotion_regist");
                        WIZU_LAYER.common.layer_pop_open_close();
                    },
                    confirm: function(){
                        WIZU_LAYER.common.layer_set_template("promotion_confirm");
                        WIZU_LAYER.common.layer_pop_open_close();
                    },
                    end: function(){
                        WIZU_LAYER.common.layer_set_template("promotion_end");
                        WIZU_LAYER.common.layer_pop_open_close();
                    }
                }
            },
            openLetter : function(){
                var letterMode = '';

                if(letterMode == "op"){
                    WIZU_COMMON.util.cookies.delCookie("letter_status");
                    WIZU_LAYER.render.wizuLetter();
                }
            },
            checkLetterStatus : function(){
                var
                    letterStatus = WIZU_COMMON.util.cookies.getCookie("letter_status"),
                    path = window.location.pathname;
                if(letterStatus != "" && path !="/global/agreement.php"){
                    WIZU_LAYER.render.wizuLetter();
                }
            },
            addEventLisenter : function(){
                /** 공통 영역 */
                //리사이즈시 해당 함수 호출
                $(window).resize(WIZU_LAYER.common.layer_position);

                //스크롤 이동시 팝업 해제영역이 따라다니도록 이동
                $('.layer_pop .layer_layout_box').scroll(function(){
                    var layer_top = $(this).scrollTop();
                    var layer_content = $('.layer_content').innerHeight();
                    var layer_bg_position =  layer_content - (layer_content - layer_top);
                    $('.layer_layout_box .bg').css('top',layer_bg_position);
                });

                // 클릭 이벤트
                $('.layer_pop .bg, .layer_pop .btn_x, .btn_layer_close').on('click',WIZU_LAYER.common.layer_pop_open_close);

                // 로그인, 회원가입
                $("#layer_login, #layer_regist, .layer_login").on('click',function() {
                    /* 레이어 템플릿 설정 */
                    var mode = $(this).attr("id").split("_")[1];
                    if( mode == "login" ){
                        location.href="/global/login.php";
                    }else if( mode == "regist" ){
                        location.href="/global/login.php?m=regist";
                    }
                });
            }
        }
        WIZU_LAYER.init();
        window.WIZU_LAYER = WIZU_LAYER;

        // 휴면계정 -> 회원 전환처리값 체크.
        var isDormant = "";
