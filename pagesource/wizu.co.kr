                    if(!layer_type){
                        return;
                    }
                    layer_type = layer_type.replace("sellerTopic","sellertopic");
                    $(".layer_template").hide();
                    $("#template_"+layer_type).show();
                }
            },
            render : {
                // 로그인 레이어 오픈
                login : function(){
                    WIZU_LAYER.common.layer_set_template("login");
                    WIZU_LAYER.common.layer_pop_open_close();
