    var WIZU_LETTER = {
        init : function(){
            this.addEventListener();
        },
        status : {
            step1 : function(){
                WIZU_COMMON.util.cookies.setCookie('letter_status', "login");
                var snsType = $(this).attr("class");
                switch (snsType){
                    case "payco" : SNS_LOGIN.payco.login(); break;
                    case "facebook" : SNS_LOGIN.facebook.login(); break;
                    case "naver" : SNS_LOGIN.naver.login(); break;
                    case "kakao" : SNS_LOGIN.kakao.login(); break;
                }
            },
            step2 : function(){
                var
					email = $("#newsLetterEmail").val(),
					isCheck = $("#chk_agree").is(":checked");

                                if(isCheck === false){
                    alert('약관에 동의해주세요.');
                    return false;
				}
				
                if(email){
                    var emailValid = WIZU_COMMON.validate.email(email);
                    if(emailValid === false) return false;
                    WIZU_COMMON.util.cookies.setCookie('letter_status', "request");
                    document.newsLetterForm.submit();
                }else{
                    alert('이메일을 입력해주세요.');
                    $("#newsLetterEmail").focus();
                    return false;
                }
            },
            end : function(){
                WIZU_COMMON.util.cookies.delCookie('letter_status');
            }
        },
        addEventListener : function(){
            // 로그인
            $(".letter_layer_wrap .sns_login_box li").on("click",WIZU_LETTER.status.step1);
            // 레이어 닫기
            $(".letter_close, .letter_complete_box .btn_ok").on("click", WIZU_LETTER.status.end);
            // 구독신청
            $("#subscribe").on("click",WIZU_LETTER.status.step2);
            // 구독신청 완료
            $(".btn_ok").on("click", function () {
                WIZU_LAYER.common.layer_pop_open_close();
            })
        }
    }
    WIZU_LETTER.init();
</script>

