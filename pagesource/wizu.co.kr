    <script>
    // 위쥬 공통 스크립트 정의.
    $(function(){
        "use strict";
        var WIZU_COMMON = {
            init : function(){
                this.addEventListener();
            },
            util : {
                fileExtCheck : function (fileType) {
                    if(fileType) {
                        switch(fileType){
                            case "image/jpg" :
                            case "image/jpeg" :
                            case "image/gif" :
                            case "image/png" :
                            case "image/bmp" :
                                return true;
                                break;
                        }
                    }
                    return false;
                },
                strip_tags : function(input, allowed) {
                    // 태그 제거
                    allowed = (((allowed || "") + "").toLowerCase().match(/<[a-z][a-z0-9]*>/g) || []).join('');
                    var tags = /<\/?([a-z][a-z0-9]*)\b[^>]*>/gi,
                        commentsAndPhpTags = /<!--[\s\S]*?-->|<\?(?:php)?[\s\S]*?\?>/gi;
                    return input.replace(commentsAndPhpTags, '').replace(tags, function ($0, $1) {
                        return allowed.indexOf('<' + $1.toLowerCase() + '>') > -1 ? $0 : '';
                    });
                },
                htmlentities : {
                    encode : function(str) {
                        var buf = [];

                        for (var i=str.length-1;i>=0;i--) {
                            buf.unshift(['&#', str[i].charCodeAt(), ';'].join(''));
                        }
                        return buf.join('');
                    },
                    decode : function(str) {
                        return str.replace(/&#(\d+);/g, function(match, dec) {
                            return String.fromCharCode(dec);
                        });
                    }
                },
                cookies : {
                    setCookie : function(cname, cvalue, exdays) {
                        var d = new Date();
                        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
                        var expires = "expires="+d.toUTCString();
                        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
                    },
                    getCookie : function(cname) {
                        var name = cname + "=";
                        var ca = document.cookie.split(';');
                        for(var i = 0; i < ca.length; i++) {
                            var c = ca[i];
                            while (c.charAt(0) == ' ') {
                                c = c.substring(1);
                            }
                            if (c.indexOf(name) == 0) {
                                return c.substring(name.length, c.length);
                            }
                        }
                        return "";
                    },
                    delCookie : function(name) {
                        document.cookie = name +'=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
                    }
                },
                strValidCheck : function(search_txt){
                    //  특수문자 제거
                    var
                        pattern_tag = /(<([^>]+)>)/ig,
                        pattern_str = /[^ㄱ-ㅎ가-힣A-za-z0-9]/ig;


                    search_txt =  search_txt.replace(pattern_tag, "");
                    search_txt =  search_txt.replace(pattern_str, "");
                    return search_txt;
                }
            },
            validate : {
                email : function(email){
                    var atpos = email.indexOf("@");
                    var dotpos = email.lastIndexOf(".");
                    if (atpos<1 || dotpos<atpos+2 || dotpos+2>=email.length) {
                        alert("이메일 형식이 잘못 입력 되었습니다.");
                        return false;
                    }
                }
            },
            share : {
                // SNS 공유
                tw : function() {
                    var data = this.getMeta();
                    window.open('http://twitter.com/intent/tweet?text=' + encodeURIComponent(data.title) + '&url=' + encodeURIComponent(data.url),'twitter','width=500,height=500');
                },
                fb : function(){
                    var data = this.getMeta();
                    FB.ui(
                        {
                            method: 'share',
                            href: data.url
                        }, function(response){});
                },
                ks : function(){
                    var data = this.getMeta();
                    Kakao.Story.share({
                        url: data.url,
                        text: data.title
                    });
                },
                getMeta : function(){
                    var metaObj = new Object();
                    metaObj.title = $('meta[property="og:title"]').attr('content');
                    metaObj.url = $('meta[property="og:url"]').attr('content');
                    metaObj.desc = $('meta[property="og:description"]').attr('content');
                    metaObj.site = $('meta[property="og:site_name"]').attr('content');
                    metaObj.image = $('meta[property="og:image"]').attr('content');
                    return metaObj;
                }
            },
            loginCheck : function(){
                // 로그인 체크
                                    location.href="/global/login.php";
                    // WIZU_LAYER.render.login();
                    return false;
                                return;
            },
            promotion: {
                cnt : '',
                nowCnt : function(){
                    var data = [],
                        returnFlag = false;
                    data.snsKey      = '';
                    data.regType     = '';

                    $.ajax({
                        type: "POST",
                        url: "/ajax/common/ajax_promotion_cnt.php",
                        data: data,
                        async: false,
                        success: function( cmtData ){
                            if(cmtData != "noData"){
                                var cmtObj = jQuery.parseJSON( cmtData );
                                // 프로모션 추가 참여된 여부 판단
                                if( WIZU_COMMON.promotion.cnt != cmtObj.promotionCnt ){
                                    returnFlag = true;
                                }else{
                                    returnFlag = false;
                                }
                            }
                        }
                    });
                    return returnFlag;
                }
            },
            addEventListener : function(){

                // 페이스북 공유
                $(".sns_fb").on('click',function(){
                    WIZU_COMMON.share.fb();
                });
                // 트위터 공유
                $(".sns_tw").on('click',function(){
                    WIZU_COMMON.share.tw();
                });
                // 카카오스토리 공유
                $(".sns_ks").on('click',function(){
                    WIZU_COMMON.share.ks();
                });
                // 주소복사
                $(".clipboard").on('click',function(){
                    new Clipboard(".clipboard");
                    alert('복사 되었습니다.');
                });

                // 댓글 권한체크
                $("body").on("click","#call_login_layer, .reply_text textarea",WIZU_COMMON.loginCheck);
                // 작가회원
                $("#authorMember").click(function(){
                                            WIZU_COMMON.loginCheck();
                                    });
            }
        }
        WIZU_COMMON.init();
        window.WIZU_COMMON = WIZU_COMMON;
    })
</script>
