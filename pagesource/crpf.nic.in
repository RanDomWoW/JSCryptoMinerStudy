	<script type="text/javascript">

    function getUserDetails(id) {
        $.ajax({
            type: "POST",
            url: "Default.aspx/getUserDetails",
            cache: false,
            contentType: "application/json; charset=utf-8",
            data: "{'id' : " + id + ", 'languageid':'" + hdnLanguageID.value + "'}",
            dataType: "json",
            success: function (data) {
                $('#lblFName').text(data.d.FName);
                $('#lblLName').text(data.d.LName);
                $('#lblDesignation').text(data.d.Designation);

                if (data.d.ContactNumber != '') {
                    $('#trPhone').show();
                    $('#lblPhone').text(data.d.ContactNumber);
                }
                else {
                    $('#trPhone').hide();
                }

                if (data.d.EmailId != '') {
                    $('#trEmail').show();
                    $('#lblEmail').text(data.d.EmailId);
                }
                else {
                    $('#trEmail').hide();
                }

                if (data.d.Zone != null) {
                    $('#trZone').show();
                    $('#lblZone').text(data.d.Zone);
                }
                else {
                    $('#trZone').hide();
                }

                if (data.d.Sector != null) {
                    $('#trSector').show();
                    $('#lblSector').text(data.d.Sector);
                }
                else {
                    $('#trSector').hide();
                }
                if (data.d.Photo != null) {
                    $('#userImg').attr('src', '/writereaddata/Portal/Designation_Photos/' + data.d.Photo);
                }
                else {
                    $('#userImg').attr('src', '/writereaddata/Portal/Designation_Photos/Not_Avalilable.jpg');
                }

                if ('1' == '2') {
                    $('#userImg > img').attr('alt', 'प्रयोक्ता फोटो');
                    tb_show("अधिकारी विवरण", "#TB_inline?height=335&amp;width=300&amp;inlineId=userDetail&amp;modal=false");
                }
                else {
                    $('#userImg > img').attr('alt', 'User Photo');
                    tb_show("Officer Details", "#TB_inline?height=335&amp;width=300&amp;inlineId=userDetail&amp;modal=false");
                }

                //tb_show("Officer Details", "#TB_inline?height=335&amp;width=300&amp;inlineId=userDetail&amp;modal=false");
                // tb_show("Officer Details", "#TB_inline??keepThis=true&amp;TB_iframe=true&amp;height=335&amp;width=300");
            },
            Error: function () {
            }
        });
    }

    
</script>

