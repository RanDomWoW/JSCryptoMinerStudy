function FrontPage_Form1_Validator(theForm)

{



  if (theForm.textfield.value == "")

  {

    alert("Please enter a value for the \"textfield\" field.");

    theForm.textfield.focus();

    return (false);

  }



  if (theForm.textfield.value.length < 3)

  {

    alert("Please enter at least 3 characters in the \"textfield\" field.");

    theForm.textfield.focus();

    return (false);

  }



  if (theForm.textfield.value.length > 16)

  {

    alert("Please enter at most 16 characters in the \"textfield\" field.");

    theForm.textfield.focus();

    return (false);

  }



  var checkOK = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyzƒŠŒŽšœžŸÀÁÂÃÄÅÆÇÈÉÊËÌÍÎÏÐÑÒÓÔÕÖØÙÚÛÜÝÞßàáâãäåæçèéêëìíîïðñòóôõöøùúûüýþÿ0123456789-";

  var checkStr = theForm.textfield.value;

  var allValid = true;

  var validGroups = true;

  for (i = 0;  i < checkStr.length;  i++)

  {

    ch = checkStr.charAt(i);

    for (j = 0;  j < checkOK.length;  j++)

      if (ch == checkOK.charAt(j))

        break;

    if (j == checkOK.length)

    {

      allValid = false;

      break;

    }

  }

  if (!allValid)

  {

    alert("Please enter only letter and digit characters in the \"textfield\" field.");

    theForm.textfield.focus();

    return (false);

  }

  return (true);

}

