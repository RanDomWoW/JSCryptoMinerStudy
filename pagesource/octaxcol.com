 			 var initiateModalMenu = function() {



    				jQuery('a.internal-page-link').on('click', function(e) {

     				 	e.preventDefault();

     				 	var iframe_url = e.currentTarget.href;



      					if(jQuery('#lightbox').length > 0 ) {

        

        					jQuery('#lightbox-content').html('<iframe id="octax-iframe" src="' + iframe_url + '" ></iframe>');

						jQuery("iframe").ready(function (){

							jQuery('#lightbox').fadeIn('slow');

