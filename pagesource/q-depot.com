					}		
					
					if(response[entry].event == 4) { // Scroll from top event
						$(window).scroll(function() {	
							
							//variables to be used				
							
							var startDistance = 0;
							var percentageValue = response[entry].percentage_value;
							var scrollAmount = $(window).scrollTop();
