<script type="text/javascript">
    $(document).ready(function () {

        $.cloudinary.responsive();

        // Call responsive whenever an accordian panel is shown since Cloudinary thinks the dimensions
        // are 0x0 (or negative) on page load when an image is in a hidden wrapper.
        $('.panel-collapse').on('shown.bs.collapse', function (e) {
            $.cloudinary.responsive();
        });

        // Call responsive whenever the application DOM changes just in case new images have been added.
        // In particular, images used in modals.
        $(window).on('ApplicationDOM_Changed', function (e) {
            $.cloudinary.responsive();
        });

    });

    require(['FR'], function (FR) {
        FR.cloudinary.cloudName = 'vail-resorts';
        FR.cloudinary.cloudApiKey = '916949118678158';
        FR.cloudinary.cloudUseCname = 'true';
        FR.cloudinary.cloudCname = 'images.vailresorts.com';
    });

</script>

    

    

