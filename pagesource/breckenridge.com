               title="<%= weather.MountainCamLink.Title %>">
                <%= (weather.MountainCamLink.Text) %>
            </a>
        <% } %>
    </div>

</script>

<script id="header_weather__snow" type="text/template">
    <div class="row">
        <div class="col-xs-12 c126__title--v1 header_weather__title uppercase"><span class="header_weather__title_icon header_weather__title_icon--snowflake icon-weather-snow-report"></span><%= (headerSettings.SnowReportTitle) %></div> 
    </div>
    <div class="row header_weather__snow_report_container">
        <% _.each(snowReport, function(snowfall, index){ %>
        <div class="col-xs-5 col-lg-6 header_weather__us_only header_weather__snowfall_container">
            <div class="c126__total1--v1 header_weather__snowfall_measurement"><%= snowfall.Depth.Inches %>in</div>
            <div class="c126__label1--v1 header_weather__snowfall_measurement uppercase"><%= snowfall.Description %></div>
        </div>
        <div class="col-xs-5 col-lg-6 header_weather__international_only header_weather__snowfall_container">
            <div class="c126__total1--v1 header_weather__snowfall_measurement"><%= snowfall.Depth.Centimeters %>cm</div>
            <div class="c126__label1--v1 header_weather__snowfall_measurement uppercase"><%= snowfall.Description %></div>
        </div>
        <% }) %>
    </div>
    <div class="row header_weather__link">
        <% if (weather.WeatherReportLink && weather.WeatherReportLink.Url) { %>
            <a class="col-xs-12 c126__link--v1 linkCTA" 
               href="<%= weather.WeatherReportLink.Url %>" 
               data-component-element="hdrWeatherHeader_Conditions_Link" 
               data-track="click"
               title="<%= weather.WeatherReportLink.Title %>">
                <%= weather.WeatherReportLink.Text %>
            </a>
        <% } %>
    </div>
</script>

<script id="header_weather__lift_status" type="text/template">
    <div class="row">
        <div class="col-xs-12 c126__title--v1 header_weather__title uppercase"><span class="header_weather__title_icon header_weather__title_icon--terrain icon-brochure-mountain"></span><%= (weather.HeaderSettings.LiftAndTerrainStatusTitle) %></div> 
    </div>

    <div class="header_weather__lift_status_container row">
        <% if (weather.HeaderSettings.ShowLiftsOpenSection === true) { %>
        <% if (weather.HeaderSettings.ShowLiftsGraph === true) { %>
        <div class="header_weather__circle col-lg-3 hidden-xs hidden-sm hidden-md" data-open="<%= (weather.OpenLifts) %>" data-total="<%= (weather.TotalLifts) %>">
            <div class="header_weather__lift_icon" tabindex="0">
                <span class="icon-lift-gondola"></span>
            </div>
        </div>
        <% } %>
        <div class="header_weather__lifts_open_container c126__link--v1 col-xs-4 <%= (weather.HeaderSettings.ShowLiftsGraph === true)? 'col-lg-3' : 'col-lg-6 header_weather__lifts_open_container--nograph' %>">
            <div class="header_weather__lifts_open c126__number1--v1"><%= (weather.OpenLifts) %><span class="c126__number2--v1">/<%= (weather.TotalLifts) %></span></div>
            <div class="c126__label1--v1 uppercase">Lifts Open</div>
        </div>
        <% } %>
        <div class="header_weather__lift_times_container col-xs-8 <%= (weather.HeaderSettings.ShowLiftsOpenSection === true)? 'col-lg-6' : 'col-lg-12' %>">
            <div class="header_weather__todays_hours c126__time--v1"><%= (weather.CurrentWorkDay.Description) %></div>
            <div class="c126__label1--v1 uppercase"><%= (weather.HoursOfOperationTitle) %></div>
        </div>
    </div>
    <div class="row header_weather__link header_weather__link--terrain">
        <% if (weather.TerrainStatusLink && weather.TerrainStatusLink.Url) { %>
            <a class="col-xs-12 c126__link--v1 linkCTA" 
               href="<%= weather.TerrainStatusLink.Url %>" 
               data-component-element="hdrWeatherHeader_Terrain_Link" 
               data-track="click"
               title="<%= weather.TerrainStatusLink.Title %>">
                <%= weather.TerrainStatusLink.Text %>
            </a>
        <% } %>
    </div>
</script>

