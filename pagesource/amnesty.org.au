<script>
		(function() {
			var getCookie = function(name) {
				if (!name) { return null; }
				return decodeURIComponent(document.cookie.replace(new RegExp("(?:(?:^|.*;)\\s*" + encodeURIComponent(name).replace(/[\-\.\+\*]/g, "\\$&") + "\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1")) || null;
			};
			
			var getSupporterLevelOfActivity = function() {
				var total = 0;
				var supporterActivityCookie = getCookie('amnesty_supporter_activities');
				if (supporterActivityCookie) {
					total = JSON.parse(supporterActivityCookie)['total'];
				} else {
					var supporterCookie = getCookie('amnesty_supporter');
					if (supporterCookie) {
						var actions = JSON.parse(supporterCookie)['actions'];
						if (actions instanceof Array && actions.length > 0) {
							for (var i = 0; i < actions.length; i++) {
								total += actions[i]['count'];
							}
						}
					} else {
						if (typeof(Storage) !== "undefined") {
							var supporterActivityLocalStorage = localStorage['amnesty_supporter_activities'];
							if (supporterActivityLocalStorage) {
								total = JSON.parse(supporterActivityLocalStorage)['total'];
							}
						}
					}
				}
				if (total > 1 && total <= 5) {
					return 'lightly';
				} else if (total > 5 && total <= 11) {
					return 'highly';
				} else if (total > 11) {
					return 'committed';
				} else {
					return 'new';
				}
				
			};
			
			window["optimizely"].push({
				  "type": "user",
				  "attributes": {
				  	"amnesty_supporter_level_of_activity": getSupporterLevelOfActivity()
				  }
			});
		})();
	</script>
