    $(document).ready(function () {

        if (typeof r3 === "function" && 'generic_page' != '') {

            function getParameterByName(name, url) {
                if (!url) url = window.location.href;
                name = name.replace(/[\[\]]/g, "\\$&");
                var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                    results = regex.exec(url);
                if (!results) return null;
                if (!results[2]) return '';
                return decodeURIComponent(results[2].replace(/\+/g, " "));
            }

            R3_COMMON = new r3_common();
            R3_COMMON.setApiKey('3ca574ff0a79f0d1');
            R3_COMMON.setBaseUrl(window.location.protocol + '//recs.richrelevance.com/rrserver/'); //"http://[integration] or [recs].richrelevance.com/rrserver/" [Recs] for production
            R3_COMMON.setSessionId('3dafa20cdeeb49a88a5b77dcd152fd61');
            R3_COMMON.setClickthruServer(window.location.protocol + '//' + window.location.host);


                 R3_COMMON.setUserId('');

            //Make sure this doenst refresh already rendered blocks
            if ('generic_page' == "item_page") {
                var categoryid = $($('#addProductButton')[0]).data('categoryid');
                R3_COMMON.addCategoryHintId(categoryid);

                var R3_ITEM = new r3_item();
                var productId = $($('#addProductButton')[0]).data('variantid');
                R3_ITEM.setId(productId);
                var productName = $('.product-details__title')[0].innerText;
                R3_ITEM.setName(productName);
            }

            if ('generic_page' == 'home_page') {
                var R3_HOME = new r3_home();

                var arr = $(".product-list__item");
                var len = arr.length;
                if (len >= 15) {
                    len = 15;
                }

                for (var i = 0; i < len; i++) {
                    var productId = $(arr[i]).find('.button.button--basket').data('variantid');
                    if (productId != null) {
                        R3_COMMON.addItemId(productId);
                    }
                }

            }

            if ('generic_page' == "search_page") {


                if ($("#isSearchPage").text()) {
                    //ProductSearchPage
                    if ($("#isSearchPage").text() === "True") {

                        var R3_SEARCH = new r3_search();
                        var q = $('#searchTextbox').val();
                        R3_SEARCH.setTerms(q);
                    //ProductCategory
                    } else if ($("#isSearchPage").text() === "False") {

                        var R3_CATEGORY = new r3_category();
                        var categoryId = $('#categoryId').text().split(/[# ]+/).pop();
                        R3_CATEGORY.setId(categoryId);
                        var categoryName = $('#categoryName').text().split(/[# ]+/).pop();
                        R3_CATEGORY.setName(categoryName);

                     }
                    var arr = $(".grid-item");
                    var len = arr.length;
                    if (len >= 15) {
                        len = 15;
                    }

                    for (var i = 0; i < len; i++) {
                        var productId = $(arr[i]).find('.button.button--basket').data('variantid');
                        if (productId != null) {
                            if ($("#isSearchPage").text() === "True") {
                                R3_SEARCH.addItemId(productId);
                            } else {
                                R3_COMMON.addItemId(productId);
                            }
                        }
                    }

                } else {
                    //GlobalSearch
                    var R3_SEARCH = new r3_search();
                    var q = getParameterByName('query');

                    var arr = $(".SearchProductVariantId");
                    var len = arr.length;
                    if (len >= 15) {
                        len = 15;
                    }
                    for (var i = 0; i < len; i++) {
                        var productId = $(arr[i]).text();
                        R3_SEARCH.addItemId(productId);
                    }
                    R3_SEARCH.setTerms(q);
                }

            }

            if ('generic_page' == 'cart_page') {
                var R3_CART = new r3_cart();

                var arr = $(".basket-product");
                var len = arr.length;
                if (len >= 15) {
                    len = 15;
                }

                for (var i = 0; i < len; i++) {
                    var productId = $(arr[i]).find('.button.js--remove-product').data('variantid');
                    R3_CART.addItemId(productId);
                }

                $(document).on("basket:removedProduct", function (e, basket) {
                    var basketItems = basket.BasketItems;
                    if (basketItems && basketItems.length) {
                        if (typeof R3_ITEM !== 'undefined') R3_ITEM = undefined;
                        if (typeof R3_SEARCH !== 'undefined') R3_SEARCH = undefined;
                        R3_COMMON.placementTypes = '';
                        var placements = getAllPlacementsOnElement($(document));
                        for (var i = 0; i < placements.length; i++) {
                            R3_COMMON.addPlacementType(placements[i]);
                        }
                        var R3_CART = new r3_cart();
                        R3_COMMON["itemIds"] = '';
                        for (var i = 0; i < basketItems.length; i++) {
                            var basketItem = basketItems[i];
                            R3_CART.addItemId(basketItem.VariantId);
                        }
                        r3();
                    }

                });
            }

            if ('generic_page' == 'error_page') {
                var R3_ERROR = new r3_error();
            }

            if ('generic_page' == 'personal_page') {
                var R3_PERSONAL = new r3_personal();
            }

            if ('generic_page' == 'generic_page') {
                var R3_GENERIC = new r3_generic();
            }

            if ('generic_page' == 'purchase_complete_page') {

                if($('#receipt-page').length > 0) {
                    var R3_PURCHASED = new r3_purchased();
                    var ordernumber = $("#receipt-page").attr('data-orderid');
                    R3_PURCHASED.setOrderNumber(ordernumber);

                    var arr = $(".basket-product.js--basket-product");
                    var len = arr.length;
                    if (len >= 15) {
                        len = 15;
                    }

                    for (var i = 0; i < len; i++) {
                        var el = $(arr[i]).find('.basket-product__price__unit')[0];
                        var itemId = $(el).attr('data-variantid');
                        var quantity = $(el).attr('data-quantity');
                        var price = $(el).attr('data-rrprice');


                        R3_PURCHASED.addItemIdPriceQuantity(itemId, price, quantity);
                    }
                }

            }

            if ($('.rr-block').length > 0) {
                if ($('.rr-block').length == 1) {
                    var rec = $('.rr-block').data('value');
                    var rendered = $('.rr-block').attr('data-rendered') == "true";
                    if (!rendered) {
                        R3_COMMON.addPlacementType(rec);
                    }
                } else {
                    for (var i = 0; i < $('.rr-block').length; i++) {
                        var rec = $($('.rr-block')[i]).data('value');
                        var rendered = $($('.rr-block')[i]).attr('data-rendered') == "true";
                        if (!rendered) {
                            R3_COMMON.addPlacementType(rec);
                        }
                    }
                }
            } else {
                var placement = 'generic_page';
                if ('generic_page' == "search_page") {
                    if ($("#isSearchPage").text() && $("#isSearchPage").text() !== "True") {
                        placement = "category_page";
                    }
                }
                R3_COMMON.addPlacementType(placement);
            }

            rr_flush_onload();
            r3();
        }
    });
</script>   
</body>
</html>