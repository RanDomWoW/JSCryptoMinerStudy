			<footer id="site-footer" ng-controller="Footer as footer">
				<section  class="row">
					<div class="column medium-7 medium-centered">
                        <div class="text-center"><h2>お問い合わせ</h2></div>
                        <form class="form" name="form" ng-submit="footer.sendmail();">
                            <section>
                                <label for="name" class="right">氏名</label>
                                <input type="text" id="name" placeholder="" ng-model="footer.name" required>
                            </section>

                            <section>
                                <label for="mailadress" class="right">メールアドレス</label>
                                <input type="email" id="mailadress" placeholder="" ng-model="footer.email" required>
                            </section>

                            <section>
                                <label for="right-label" class="right">メッセージ</label>
                                <textarea rows="4" placeholder="" ng-model="footer.message" required></textarea>
                            </section>
                            <div class="text-center padding-top-20">
                                <button class="button">問い合わせ送信</button>
                            </div>
                        </form>
					</div>
				</section>
			</footer><!-- .site-footer -->
