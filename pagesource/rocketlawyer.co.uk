    $.getJSON('/ecommerce-site/subscription-info', function (subscriptionJSON) {
        if(subscriptionJSON.hasOwnProperty('plan')) {
    
            userPlan = subscriptionJSON.plan;
            userOrders = subscriptionJSON.orders;
    
            //userPlan.name.indexOf('Single Document') > -1
    
            //check if user had a complete plan but is now not on a plan
            if (userPlan.name != "Complete") {
                var len = userOrders.length;
                for (var i=0;i<len;i++) {
                    var o = userOrders[i];
                    if (o.planId_ == 5) {
                        previousCustomer = true;
                    }
                }            
            }

            planDeferred.resolve();
    
            //overwrite registered user and single doc account plan names if required
            if (
                (subscriptionJSON.plan.name.indexOf('Single Document') != -1) ||
                (subscriptionJSON.plan.name.indexOf('registered user') != -1) ||
                (subscriptionJSON.plan.name.indexOf('Free') != -1)
            ){
                $('#planName').html('Free account');
                if($('.banner').length == 1) $('.banner').show();
            }
            else {
                var planName = (subscriptionJSON.plan.name == "Incorporation Complete" ? "Complete" : subscriptionJSON.plan.name);
