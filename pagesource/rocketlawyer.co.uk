function dateTimeLastAction(val){
    if(val === null){
        localStorage.removeItem('datetimeLastAction');
    }else if(val != undefined){
        localStorage.setItem('datetimeLastAction', val);
    }else{
        return parseFloat(localStorage.getItem('datetimeLastAction') || 0);
    }
}
        
    //timeout after 30 mins
    var timeout_seconds = 30 * 60;
    var timeout_warningLength = 30;

    var now = new Date().getTime();
    var elapsed_ms = now - dateTimeLastAction();
    var elapsed_secs = Math.floor(elapsed_ms / 1000);

    console.log('timeout elapsed_secs = ' + elapsed_secs );
    
    
    //force logout if returning
    if (loggedIn) {
        if (elapsed_secs > (timeout_seconds)) {
            if (document.referrer.indexOf('free-trial-confirmation.rl') == -1) {
                date = new Date();
                date.setDate( date.getDate() - 1 );
                reset_timeout();
                loggedIn = false;
                logout();
                console.log('clear init');
            }
        }
    }
    
    //call timeout loop every second
    setInterval('timeoutloop()', 1000);

function timeoutloop() {

    var now = new Date().getTime();
    var elapsed_ms = now - dateTimeLastAction();
    var elapsed_secs = Math.floor(elapsed_ms / 1000);

    if (loggedIn) {
        if (elapsed_secs > timeout_seconds) {
            var seconds_left = timeout_warningLength - (elapsed_secs - timeout_seconds);
            if (seconds_left > 0) {
                //show timeout warning message
                $('#idletimeout').slideDown();
                //show how many seconds before logout
                $('#idletimeout').find("span").html(seconds_left); 
            }
            //time to logout
            if (seconds_left < 1) {
                $('#idletimeout').slideUp();
                date = new Date();
                date.setDate( date.getDate() - 1 );
                reset_timeout();
                loggedIn = false;
                logout();
                console.log('timeout');
            }
        }
    }
    else {
        reset_timeout();
    }
    
}

    
    //reset timeout datetime to now on mouse move/click
    $('body').on( "mousemove", function() { reset_timeout(); })
    $('body').on( "mousedown", function() { reset_timeout(); })

function reset_timeout() {
    if ($('#idletimeout').is(':visible')) {
        $('#idletimeout').slideUp();
    }
    dateTimeLastAction(new Date().getTime());
}
    




</script>
    
