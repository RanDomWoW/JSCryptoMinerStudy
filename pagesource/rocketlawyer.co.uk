   function checkSimilarNames() {
        var contentType ="application/x-www-form-urlencoded; charset=utf-8";
        var val = $('#companyName').val() + ' Limited';
        $('#scroll').empty();
        $.ajax({
            url:"https://companyincorporations.co.uk/api/SimilarNames",
            data:"client_id=rocketlawyer&name="+val,
            type:"GET",
            dataType:"JSON",   
            contentType:contentType,    
            crossDomain: true
        }).done(function(data){
            console.log(data);
            if(data.length == 0) {
                $('#scroll').append("<li>No similar names</li>");
            }
            else {
                for(var i = 0; i < data.items.length; i++) {
                    var clr;
                    if(data.items[i].status == "Dissolved" || data.items[i].status == "Removed") {
                        clr = 'text-green';
                    }
                    else {
                        clr = 'text-red';
                    }
                    var li = '<li><span class="name">' + data.items[i].name + '</span><span class="pull-right status ' + clr + '">' + data.items[i].status + '</span></li>';
                    $('#scroll').append(li);
                }
            }
        }).fail(function(jqXHR, textStatus, errorThrown){
            console.log('Whoops, something went wrong');
        });
    };
    
    function hideDocModalPreview() {
        $('.similar-names-modal').hide();
    };
    
    function checkName() {
        var contentType ="application/x-www-form-urlencoded; charset=utf-8";
        var val = $('#companyName').val();
        if($.trim(val) == "") {
            $('#loader').hide();
            $('#mainCta').off('click');
            $('#mainCta').attr('disabled', 'true');
            return;
        }
        if(val.indexOf("limited") == -1 && val.indexOf("Limited") == -1) {
            val = $('#companyName').val() + ' Limited';
        }
        $('#companyName').val(val);
        $.ajax({
            url:"https://companyincorporations.co.uk/api/NameCheck",
            data:"client_id=rocketlawyer&name=" + val,
            type:"GET",
            dataType:"JSONP",   
            contentType:contentType,
            crossDomain: true
        }).done(function(data){
            checkSimilarNames();
            if(data.available === true) {
                $('#loader').attr('src', '/assets/bootstrap3/img/solid-checkmark-small.png');
                $('#loader').show();
                $('#cta .warning-message').hide();
                $('#cta .btn').attr('disabled', false).show();
                $('#cta .similar-names-btn').removeClass('visibility-hidden');
    
                $('#mainCta').on('click', function(e) {
                    e.preventDefault();
                    sessionStorage.setItem('companyName', $('#companyName').val());
                    location.href = "/incorporation-form.rl";
                });
            }
            else {
                $('#loader').attr('src', '/assets/img/x_mark.png');
                $('#loader').show();
                $('#cta .btn').attr('disabled', true).off('click');
                $('#cta .warning-message').css('display', 'inline');
                $('#cta .similar-names-btn').removeClass('visibility-hidden');
                
                $('#mainCta').off('click');
                $('#mainCta').attr('disabled', 'true');    
            }
        }).fail(function(jqXHR, textStatus, errorThrown){
            $('#mainCta').off('click');
            $('#mainCta').attr('disabled', 'true');   
            console.log('Whoops! Something went wrong here');
        });
    };
    
    var delay = (function(){
        var timer = 0;
        return function(callback, ms){
            clearTimeout (timer);
            timer = setTimeout(callback, ms);
        };
    })();
    
    
    $(document).ready(function(){
        $('#cookie_opt_in_container').is(':visible') ? console.log('vis') : console.log('not vis - homepage-assets');
        $('#mainCta').on('click', function(event) {
            event.preventDefault ? event.preventDefault() : event.returnValue = false;
            sessionStorage.setItem('companyName', $('#companyName').val());
            location.href = "/incorporation-form.rl";
        });
        $('#companyName').on('keyup', function(e) {
            $('#loader').attr('src', '/assets/img/ajax-loader.gif');
            $('#mainCta').off('click');
            $('#mainCta').attr('disabled', 'true');
            $('#loader').show();
            delay(function(){
                checkName();
            }, 2000 );
        });
    });
    
</script>


