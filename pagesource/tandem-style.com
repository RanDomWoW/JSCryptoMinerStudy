jQuery(function($) {
	$('#footer .go_to_top').click(function () {
		$($.browser.opera ? document.compatMode == 'BackCompat' ? 'body' : 'html' :'html,body').animate({ scrollTop:0 }, 400, 'easeOutQuart');
		return false;
	});
	// イベントカレンダー
	$("a[href='https://www.tandem-style.com/event_calendar/']").each(function (){
		var today = new Date();
		var month = ('00'+(today.getMonth()+1)).slice(-2);
		$(this).attr('href', 'https://www.tandem-style.com/event_calendar/month/'+today.getFullYear()+'/'+month);
	});
	// メニュー内検索アイコンが押された時
	$('#search_toggle').on('click', function() {
		$(this).closest('.menu_search').toggleClass('open');
		return false;
	});
	// モバイル用メニューボタンが押された時
	$('#mobile-menu-button').on('click', function() {
		// ボタンのクラスをトグル
		$(this).find('.menu-animation').toggleClass('close');
		$('#nav-mobile').slideToggle('fast');
		return false;
	});
	// 検索窓の入力値チェック
	$('.search-general').submit(function() {
		if ( !$(this).find('.input').val() ) {
			$(this).addClass('empty-value');
			return false;
		}
	});
	$('.search-general .input').focus(function() {
		$(this).closest('form').removeClass('empty-value');
	});
	// イベントカレンダーウィジェット
	var eventCalWidget = $('#event_calendar_widget');
	eventCalWidget.bxSlider( {
		auto: true,
		autoHover: true,
		pager: false,
		speed: 500,
		pause: 5000,
		nextText: '\uf105',
		prevText: '\uf104',
		onSliderLoad: function() {
			eventCalWidget.removeClass('preload');
		}
	} );
	// DFPのバナー画像の幅調整
	$(window).on('load', function() {
		if ( $(window).width() < 370 ) {
			$('.dfp-container [id^="google_ads_iframe"]').contents().find('.img_ad').css({
				"width" : "100%",
				"height": "auto"
			});
		}
	});
	// 実際のキャプションを元にFancybox3用のdata-caption設定
	$('a[rel^=lightbox]').each(function() {
		var caption = $(this).closest('figure').find('figcaption').html();
		if ( caption ) {
			$(this).attr('data-caption', caption);
		}
	});
	// アーカイブのカードの高さを揃える
	if ( document.getElementsByClassName( 'card-list' ) != null ) {
		$(window).on('load resize', _.debounce(function() {
			var columnSize    = $('.card-list li:eq(0)').outerWidth(true);
			var containerSize = $('.card-list:eq(0)').innerWidth();
			var columnNum     = Math.round(containerSize / columnSize);
			$('.card-list').each(function() {
				$(this).find('.card-body').tile(columnNum);
			});
		}, 200));
	}
	$( '#goods_banners' ).bannerSlider();
	$( '#goods_banners_15th' ).bannerSlider({
		interval: 5000
	});
	$( '#recommend_selection_banners' ).bannerSlider({
		interval: 4000,
		fadeTime: 700
	});
	if ( document.getElementsByClassName( 'photo_gp-slider' ) != null ) {
		var photoGpSlider = $('#photo_gp-slider');
		var photoGpSliderSpeed = parseInt(photoGpSlider.data('count')) * 3500;
		photoGpSlider.bxSlider({
			ticker: true,
			speed: photoGpSliderSpeed,
			maxSlides: 10,
			minSlides: 4,
			slideWidth: 76,
			slideMargin: 3,
			onSliderLoad: function() {
				photoGpSlider.removeClass('preload');
			}
		});
		tippy('.desktop .tooltip', {
			arrow: true,
			sticky: true,
			performance: true,
			arrowTransform: 'scale(0.75) translateY(-1.5px)',
			duration: [250,100]
		} );
	}
});
