    // calculate (and subtract) whole hours
    var totalHours = (delta / 3600) % 24;
    var hours = Math.floor(delta / 3600) % 24;
    delta -= hours * 3600;

    // calculate (and subtract) whole minutes
    var totalMinutes = (delta / 60) % 60;
    var minutes = Math.floor(delta / 60) % 60;
    delta -= minutes * 60;

    var datetimeString = datetimeDefaultString;
    if (date_past.getDate() == (date_now.getDate() - 1)) {
        datetimeString = "Ontem";
    }
    else if (totalDays <= 1) {
        if (totalHours < 1) {
            datetimeString = "H&#225;";
            datetimeString += " " + minutes;
            datetimeString += " " + "Min";
