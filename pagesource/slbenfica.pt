<script>
    $(document).ready(function () {
        $('#openNewsletter, .openNewsletter').on('click', openNewsletter);
        $('#newsletter-submit-button').on('click', submitNewsletter);
    })

    function submitNewsletter(e) {
        e.preventDefault();
        e.stopPropagation();
        var newsletters = getSelectedNewsletters();
        var email = $('#slb-popup-newsletter input').val();
        if (!Utils.validateEmail(email) || newsletters.length == 0) {
            $('#newsletter-input').addClass('slb-error');
            $('#slb-popup-newsletter .slb-error-text').removeClass('slb-hidden');
        }
        else {
            $.ajax({
                type: 'POST',
                dataType: 'HTML',
                url: '/api/AccountsDP/NewsletterSubscriptionForm',
                data: JSON.stringify({ email: email, newsletters: newsletters }),
                contentType: "application/json",
                success: function (response) {
                    $('#slb-popup-newsletter input').val('');
                    $('#newsletter-input').removeClass('slb-error');
                    $('#slb-popup-newsletter .slb-error-text').addClass('slb-hidden');
                    $('#slb-popup-newsletter').removeClass("slb-active");
                    $('#slb-popup-newsletter-success').addClass("slb-active");
                    console.log('success');
                },
                error: function (xhr, textStatus) {
                    console.log('error in subscription')
                }
            })    
        }
    }

    function getSelectedNewsletters() {
        var checkedNewsletters = $('input:checkbox:checked').map(function () {
            return $(this).attr('id');
        }).get();

        return checkedNewsletters;
    }
</script>
