<div id="mediadetail">

</div>

<script>
    $(document).ready(function () {
        var loading = false;

        $(document).on('click', '.item-details a', function (e) {
            if (loading) { return; }
            loading = true;
            var id = $(this).attr('id');
            var fromRecommended = false;
            var language = "pt-pt";
            var url = '/api/sitecore/MediaFeatureDP/GetMediaGallery';
            $.ajax({
                type: 'GET',
                dataType: 'HTML',
                data: { "id": JSON.stringify(id) },
                url: url,
                beforeSend: function () {
                    if ($(".mediadetail").css("opacity") == "1") {
                        mediaDetailSelectReccomended();
                        fromRecommended = true;
                    }
                },
                success: function (response) {
                    var div = $('#mediadetail');
                    div.html('');
                    div.html(response);
                    var mediaUrl = document.getElementById('mediaUrl').textContent.toString();
                    if (fromRecommended) {
                        $(".mediadetail .loader").removeClass("hidden");
                        history.replaceState(id, "Media", mediaUrl);
                        setTimeout(function () {
                            //$(".mediadetail .loader").css("opacity", 0);
                            setTimeout(function () {
                                $(".mediadetail .loader").addClass("hidden");
                                //$(".mediadetail").scrollTop(0);
                                $(".mediadetail .reccomended-area .reccomended-list").slick("slickGoTo", 0);
                                $(".mediadetail .row").css("opacity", 1);
                            }, 250);
                        }, 1000);
                    }
                    else
                    {
                        $(".mediadetail .row").css("opacity", 1);
                        history.pushState(id, "Media", mediaUrl);
                    }


                    $("img").on('load', function () {
                        $(this).addClass('loaded');
                    }).each(function () {
                        if (this.complete)
                            $(this).trigger("load");
                    });
                    loading = false;
                    openMediaDetail(e);
                    loadVideoType();
                },
                error: function (xhr, textStatus) {
                    loading = false;
                }
            })
        })

        function loadVideoType() {
            $(".mediadetail").addClass("photogallery");
            var videoType = document.getElementById('videoType').textContent.toString();
            //console.log(videoType);
            if (videoType !== undefined) {
                //$(".mediadetail").removeClass("html vimeo youtube");
                $(".mediadetail").addClass("video");
                switch (videoType) {
                    case "html":
                        mediaDetailHtmlPlayer = $('#htmlplayer-detail').get(0);
                        $(".mediadetail").addClass("html");
                        mediaDetailHtmlPlayer.play();
                        mediaDetailHtmlPlayer.addEventListener('ended', onHtmlFinish);
                        break;
                    case "vimeo":
                        var iframe = $('#vimeoplayer-detail')[0];
                        mediaDetailVimeoPlayer = $f(iframe);
                        $(".mediadetail").addClass("vimeo");
                        mediaDetailVimeoPlayer.addEvent('ready', function () {
                            mediaDetailVimeoPlayer.api("play");
                            //console.log('vimeo ready');
                            mediaDetailVimeoPlayer.addEvent('finish', onFinish);
                        })
                        break;
                    case "youtube":
                        mediaDetailYoutubePlayer = new YT.Player('ytplayer-detail', {
                            events: {
                                'onReady': detailYoutubePlayerReady,
                                'onStateChange': onPlayerStateChange
                            }
                        });
                        $(".mediadetail").addClass("youtube");
                        break;
                }
            }
        }

        function onPlayerStateChange(event) {
            //console.log('state changed');
            if (event.data === 0) {
                //console.log('done');
                $(".mediadetail .video-detail .thumbnail-area .thumbnail-list .next").trigger("click");
            }
        }

        function onFinish(id) {
            //console.log('vimeo done');
            $(".mediadetail .video-detail .thumbnail-area .thumbnail-list .next").trigger("click");
        }

        function onHtmlFinish(e) {
            //console.log('html player done')
            $(".mediadetail .video-detail .thumbnail-area .thumbnail-list .next").trigger("click");
        }

        function mediaDetailSelectReccomended() {
            $(".mediadetail .row div").css("opacity", 0);
            $(".mediadetail .loader").removeClass("hidden");
            $(".mediadetail").scrollTop(0);
        }

        $(window).on("popstate", function (e) {
            if (e.originalEvent.state === null) {
                mediaDetailPauseVideo();
                $(".mediadetail").css("opacity", 0);
                setTimeout(function () {
                    $(".mediadetail").removeClass("active");
                    $(".mediadetail").removeClass("photogallery");
                    $(".mediadetail").removeClass("video");
                    $(".mediadetail .slick-slider").slick("unslick");
                    $(".mediadetail").removeClass("html vimeo youtube");
                    Utils.unlockBody();
                }, 500);
            }
            else {
                $(".loader.hidden").html('');
                openMediaDetail(e);
                $(".mediadetail").addClass("photogallery");
                var videoType = document.getElementById('videoType').textContent.toString();
                if (videoType !== undefined) {
                    $(".mediadetail").addClass("video");
                    switch (videoType) {
                        case "html":
                            $(".mediadetail").addClass("html");
                            break;
                        case "vimeo":
                            $(".mediadetail").addClass("vimeo");
                            break;
                        case "youtube":
                            $(".mediadetail").addClass("youtube");
                            break;
                    }
                }
                mediaDetailPlayVideo();
                //loadVideoType();
                //var id = event.state.toString();
                //id = id.replace('{', '\\{');
                //id = id.replace('}', '\\}');
                //var video = $('#' + id);
                //$(".mediadetail").css("opacity", 1);
                //video.trigger('click');
            };
        });
    });
</script>

