<script>
var levDist = function(s, t) {
    var d = []; //2d matrix

    // Step 1
    var n = s.length;
    var m = t.length;

    if (n == 0) return m;
    if (m == 0) return n;

    //Create an array of arrays in javascript (a descending loop is quicker)
    for (var i = n; i >= 0; i--) d[i] = [];

    // Step 2
    for (var i = n; i >= 0; i--) d[i][0] = i;
    for (var j = m; j >= 0; j--) d[0][j] = j;

    // Step 3
    for (var i = 1; i <= n; i++) {
        var s_i = s.charAt(i - 1);

        // Step 4
        for (var j = 1; j <= m; j++) {

            //Check the jagged ld total so far
            if (i == j && d[i][j] > 4) return n;

            var t_j = t.charAt(j - 1);
            var cost = (s_i == t_j) ? 0 : 1; // Step 5

            //Calculate the minimum
            var mi = d[i - 1][j] + 1;
            var b = d[i][j - 1] + 1;
            var c = d[i - 1][j - 1] + cost;

            if (b < mi) mi = b;
            if (c < mi) mi = c;

            d[i][j] = mi; // Step 6

            //Damerau transposition
            if (i > 1 && j > 1 && s_i == t.charAt(j - 2) && s.charAt(i - 2) == t_j) {
                d[i][j] = Math.min(d[i][j], d[i - 2][j - 2] + cost);
            }
        }
    }

    // Step 7
    return d[n][m];
}
$(function() {
  // Current Ajax request.
  var currentAjaxRequest = null;
  // Grabbing all search forms on the page, and adding a .search-results list to each.
  var searchForms = $('form[action="/search"]:not(".no-autocomplete-search"),.autocomplete-search').css('position','relative').each(function() {
      // Grabbing text input.
  	var form = $(this);
    var input = $(this).find('input[name="q"]');
      //console.log(input, input.width());
      // Adding a list for showing search results.

    var offSet = input.position().top + input.outerHeight();
    $('<ul class="search-results"></ul>')
      			.css( { 'position': 'absolute', /*'left': '0px',*/ 'top': offSet } )
      			.appendTo($(this)).hide()
      			.keydown(function(e){ var code = e.keyCode || e.which;
                        //down
                        if( code == 40 ){
                          e.preventDefault();
                          var $next = $('li:has(a:focus)', this).next();
                          if( $next.length ){
                            $('a', $next).focus();
                          }
                          return false;
                        }
                        //up
                        else if( code == 38 ){
                          e.preventDefault();
                           var $prev = $('li:has(a:focus)', this).prev();
                          if( $prev.length ){
                            $('a', $prev).focus();
                          }
                          else{
                            input.focus();
                          }
                          return false;
                        }
                       });



           var searchCallback = function(e) {
             var _this = this;
             var input_type = $(this).attr('data-type') ? $(this).attr('data-type'):'search';

             var code = e.keyCode || e.which;
             if( code == 40 ){
               return false;
             }

             // What's the search term?
             var term = $(this).val();
             // What's the search URL?
             var searchURL = '/search?type=product&q=' + term;
             if( input_type != 'set_sku_value' || (input_type == 'set_sku_value' && term.length < 7)){
               //add wildcard character to allow incomplete search term
               searchURL +='*';
             }
             // What's the search results list?
             var resultsList = form.find('.search-results');
             // If that's a new term and it contains at least 3 characters.
             if (term.length > 3 && term.length != $(this).attr('data-old-term') ) {
               // Saving old query.
               $(this).attr('data-old-term', term);
               // Killing any Ajax request that's currently being processed.
               if (currentAjaxRequest != null) currentAjaxRequest.abort();

               currentAjaxRequest = $.getJSON(searchURL + '&view=json', function(data) {

                 // Reset results.
                 resultsList.empty();
                 // If we have no results.
                 if(data.results_count == 0) {
                   // resultsList.html('<li><span class="title">No results.</span></li>');
                   // resultsList.fadeIn(200);
                   resultsList.hide();
                 } else {
                   // If we have results.
                   if( input_type == 'set_sku_value' ){
                     $.each(data.results, function(index, item) {
                       var sorted_variants = $(item.product.variants).sort(function(a, b){
                          return levDist(a.sku,term) > levDist(b.sku,term) ? 1 : -1;
                       });
                       $.each(sorted_variants, function(k, variant){
                         var link = $('<a></a>').attr('href', '#');
                         var title = item.title;
                         var display_title =  title.length > 35 ? title.substring(0,20)+'...'+title.substr(title.length-10, title.length) : title;
                         var thumbnail = (variant.featured_image) ? variant.featured_image.src.replace(/(\.[png|jpg])/g, '_30x30$1') : item.thumbnail;
                         link.append('<span class="thumbnail"><img src="' + thumbnail + '" /></span>');
                         link.append('<span class="title">' + '[' + variant.sku +'] '+ display_title + ' ' + variant.title +'</span>');
                         link.wrap('<li></li>');

                         link.click(function(){
                           $(_this).data('variant', variant);
                           $(_this).val(variant.sku).trigger('change',variant);
                           resultsList.hide();
                         });

                         //exact match sku
                         if( variant.sku == term ){
                           //default select exact match
                           link.addClass('highlight');
                           console.log('exact match click', link)
                           $(_this).data('variant', variant);
                           $(_this).val(variant.sku).trigger('change');
                         }
                         else{
                           $(_this).data('variant', null);
                         }
                         resultsList.append(link.parent());
                       })
                     });
                   }
                   else{
                     $.each(data.results, function(index, item) {
                       var link = $('<a></a>').attr('href', item.url);
                       link.append('<span class="thumbnail"><img src="' + item.thumbnail + '" /></span>');
                       link.append('<span class="title">' + item.title + '</span>');
                       link.wrap('<li></li>');
                       resultsList.append(link.parent());
                     });
                   }
                   var results_width = $(input).attr('data-results-width') ? $(input).attr('data-results-width'): input.outerWidth();

                   resultsList.css({width:results_width});
                   // The Ajax request will return at the most 10 results.
                   // If there are more than 10, let's link to the search results page.
                   if(data.results_count > 10) {
                     resultsList.append('<li><span class="title"><a href="' + searchURL + '">See all results (' + data.results_count + ')</a></span></li>');
                   }
                   $('.search-results li:first a', form).attr('tabindex', 2);
                   resultsList.fadeIn(200);
                 }

               });
             }
             else if( $('li', resultsList).length ){
                resultsList.show();
              }
           }



    // Listening to keyup and change on the text field within these search forms.
    input.attr('autocomplete', 'off')
         .attr('tabindex', 1)
         .bind('keydown', $.debounce(500, searchCallback))
         .bind('keydown',function(e){
           var code = e.keyCode || e.which;
           if( code == 40 ){
            // e.preventDefault();
            // e.stopImmediatePropagation();
             setTimeout(function(){
               $('.search-results li:first a', form).show();
               $('.search-results li:first a', form).focus().select();},10);
             return false;
           }
         })

  });
  // Clicking outside makes the results disappear.
  $('body').bind('click', function(e){
    if( $(e.target).parents('form[action="/search"]').length == 0 ){
      $('.search-results').hide();
    }
  });
});
</script>

