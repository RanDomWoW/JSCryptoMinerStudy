			(function ($) {
				var display_on = "pageviews";
				var sc_page_number = 3;
				var button_text = "SUBSCRIBE";
				var delay = 1000;

				var Cookies = {
					get: function (name) {

						var nameEQ = name + '=';
						var ca = document.cookie.split(';');
						for (var i = 0; i < ca.length; i++) {
							var c = ca[i];
							while (c.charAt(0) == ' ') c = c.substring(1, c.length);
							if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
						}
						return false;
					},
					set: function (name, value, attr) {
						if (attr.expires) {
							hours = attr.expires * 24;
						} else {
							hours = 0;
						}

						var expires = '';
						if (hours) {
							var date = new Date();
							date.setTime(date.getTime() + (hours * 60 * 60 * 1000));
							expires = '; expires=' + date.toGMTString();
						}
						var cookie = name + '=' + value + expires + '; path=/';
						document.cookie = cookie;
					}
				};

				if (Cookies.get('sailthru_popup') || Cookies.get('sailthru_cid') || Cookies.get('sailthru_hid')) {
					return;
				}

				$( '.widget_tvone_social_conversion_widget .btn-reverse' ).text( button_text );
				$( '.form-control.sailthru_email' ).attr( { placeholder: 'Enter email' } );

				var showLightbox = function () {
					setTimeout(function () {
						$('.sc-overlay').fadeIn(function () {
							Cookies.set('sailthru_popup', 'value', {expires: 7, path: '/'});
						});
					}, delay);
				};

				var hideLightbox = function (time) {
					if (!time) {
						time = 30;
					}
					$('.sc-overlay').fadeOut('fast', function () {
						Cookies.set('sailthru_popup', 'value', {expires: time, path: '/'});
					});
				};

				switch (display_on) {
					case 'scroll_up':
						onScroll = function () {
							if ($(document).scrollTop() >= $(document).height() / 2) {
								$(window).off('scroll', onScroll);
								showLightbox();
							}
						};
						$(window).scroll(onScroll);
						break;

					case 'exit':
						$(document).ready(function () {
							onMouseMove = function (e) {
								if ((e.pageY - $(window).scrollTop() ) < 50) {
									$(document).off('mousemove', onMouseMove);
									showLightbox();
								}
							};

							$(document).mousemove(onMouseMove);
						});
						break;

					case 'comment':
						onScroll = function (e) {
							var pos = $(window).scrollTop();
							var comments = $('#comments').offset().top;

							if (pos > comments) {
								$(window).off('scroll', onScroll);
								showLightbox();
							}
						}
						$(window).scroll(onScroll);
						break;

					case 'pageviews':
						var sc_num = 1;
						if (!Cookies.get('sc-page-number')) {
							Cookies.set('sc-page-number', 1, {path: '/'});
						} else {
							sc_num = parseInt(Cookies.get('sc-page-number'), 10);
							sc_num++;

							var in30Minutes = 1 / 48;			// 1800 Seconds
							Cookies.set('sc-page-number', sc_num, {expires: in30Minutes, path: '/'});
						}

						if (sc_num === sc_page_number) {
							showLightbox();
						}
						break;

					default:
						showLightbox();
						break;
				}

				$('.sailthru-add-subscriber-form').submit(function () {
					$('#sc-content').fadeOut('slow');
					$('a.sc-close').show();
					$('.sc-not-now a').html('Click here to go back to the site');
				});
				$('a.sc-close, .sc-not-now a').one('click', function (e) {
					hideLightbox(7);
				});
			})(jQuery);
