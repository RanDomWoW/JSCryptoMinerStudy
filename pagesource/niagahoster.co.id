<script type="text/javascript">
    if (window.addEventListener) {
        window.addEventListener('load', select_tld, false);
    } else if (window.attachEvent) {
        window.attachEvent('onload', select_tld);
    }

    function select_tld() {
        var tldTimer = setInterval(function() {
            if (window.jQuery) {
                $('.domain-tld').selectpicker({
                    'selectedText': '.web.id'
                });
                $("#domain-check-submit-small").click(function() {
                    var form = $(this).closest('.container').find('.form-domain-container-new');
                    var tld = form.find("select[name='tld']").val();
                    productView(tld, 'domain', 'Search Bar'); // GTM Trigger
                    form.submit();
                    return false;
                });
                // for mobile
                $(".btn-mobile").click(function() {
                    // found a space after the tld, so we got orderdomain/searchresults?sld=shadjksadkjsak&tld=.com+
                    var the_tld = $(this).closest('.container').find('#form-mobile').find('input[name="tld"]');
                    var tld_val = the_tld.val().replace(/\s+/g, '');
                    the_tld.val(tld_val);

                    $(this).closest('.container').find('#form-mobile').submit();
                    return false;
                });
                $(".btn-1024").click(function() {
                    // found a space after the tld, so we got orderdomain/searchresults?sld=shadjksadkjsak&tld=.com+
                    var the_tld = $(this).closest('.container').find('#form-1024').find('input[name="tld"]');
                    var tld_val = the_tld.val().replace(/\s+/g, '');
                    the_tld.val(tld_val);

                    $(this).closest('.container').find('#form-1024').submit();
                    return false;
                });
                setTimeout(function() {
                    $('.selectpicker').find('span.text').each(function() {
                        var tld = $(this).text();
                        var $this = $(this);
                        var txt = $('option[value="' + tld + '"]').attr('data-text');
                        if (typeof(txt) != 'undefined') {
                            if (txt.length > 0) $this.html(tld + ' - ' + txt);
                        }
                    });
                }, 3000);
                setTimeout(function() {
                    var ch = $('#dd');
                    li = $('#domain-menu');
                    $(ch).on('click', function(e) {
                        e.preventDefault();
                        li.slideToggle('slidedown');
                    });
                }, 3000);
                $(function() {
                    $('#domain-menu li').bind('click', function(event) {
                        var id = $(this).attr('id');
                        $('#domains').html(this.id);
                        $('#domainext').val(this.id);
                        $('#domainname').focus();
                        $('#domain-menu').hide('slideup');
                        var y = parseInt($('div#domch').offset().top) - parseInt(180);
                        $('html, body').animate({
                            scrollTop: y,
                            scrollLeft: 0
                        }, 1000);
                    });
                });
                clearInterval(tldTimer);
            }
        }, 500);
    }



    function updateViews() {
        if (is_keyboard) {
            $(".container_full").hide();
            if (is_landscape) {
                $(".container_full").hide();
            }
            else {
                $(".container_full").show();
            }
        }
        else {
            $(".container_full").show();
        }
    }

    function keyboardHideShow(initial_screen_size){
        is_keyboard = (window.innerHeight < initial_screen_size);
        is_landscape = (screen.height < screen.width);
        if(is_landscape) {
            $("input").on({
                focus: function(e){
                    $(".container_full").hide();
                },
                blur: function(e){
                    $(".container_full").show();
                }
            });    
        } else {
            $("input").on({
                focus: function(e){
                    $(".container_full").show();
                },
                blur: function(e){
                    $(".container_full").show();
                }
            });
        }
    }

    $(document).ready(function() {
        /* frey added at 14 sept 16 - autocomplete process */
        var pathArray = location.pathname.split("/");
        var n = pathArray.length - 1;
        var newPath = "";
        for(i=1;i<n;i++)
        {
            newPath += "/";
            newPath += pathArray[i];
        }

        var json = location.protocol + "//" + location.host + newPath + "/assets/json/tlds.json";
        
        var options = {
            url: json,
            getValue: "tld",
            list: {
                match: {
                    enabled:true
                }
            }
        };

        $("#tld").easyAutocomplete(options);
        $("#tld2").easyAutocomplete(options);

        // test for keyboard on/off at mobile display
        var isMobile = {
            Android: function() {
                return navigator.userAgent.match(/Android/i);
            },
            BlackBerry: function() {
                return navigator.userAgent.match(/BlackBerry|BB10/i);
            },
            iOS: function() {
                return navigator.userAgent.match(/iPhone|iPad|iPod/i);
            },
            Opera: function() {
                return navigator.userAgent.match(/Opera Mini/i);
            },
            Windows: function() {
                return navigator.userAgent.match(/IEMobile/i);
            },
            any: function() {
                return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
            }
        };

        if (isMobile.any()) {
            /* Android */
            var initial_screen_size = window.innerHeight;
            var is_keyboard = false;
            var is_landscape = false;

            window.addEventListener("resize", function() {
                keyboardHideShow(initial_screen_size);
            }, false);

            keyboardHideShow(initial_screen_size);
        }

        var width = $(window).width();
        switch(width) {
            case 320:
                $(".domain-list").css("margin-left","-42px");
                $(".domain-list").css("padding-left","27px");
            break;
            case 360:
                $(".domain-list").css("margin-left","-52px");
                $(".domain-list").css("padding-left","37px");
            break;
            case 420:
                $(".domain-list").css("margin-left","-67px");
                $(".domain-list").css("padding-left","52px");
            break;
            case 480:
                $(".domain-list").css("margin-left","-82px");
                $(".domain-list").css("padding-left","67px");
            break;
            case 535:
                $(".domain-list").css("margin-left","-96px");
                $(".domain-list").css("padding-left","81px");
            break;
            case 620:
                $(".domain-list").css("margin-left","-117px");
                $(".domain-list").css("padding-left","102px");
            break;
            case 700:
                $(".domain-list").css("margin-left","-137px");
                $(".domain-list").css("padding-left","122px");
            break;
        }

        $(window).resize(function() {
            var width = $(window).width();

            switch(width) {
            case 320:
                $(".domain-list").css("margin-left","-42px");
                $(".domain-list").css("padding-left","27px");
            break;
            case 360:
                $(".domain-list").css("margin-left","-52px");
                $(".domain-list").css("padding-left","37px");
            break;
            case 420:
                $(".domain-list").css("margin-left","-67px");
                $(".domain-list").css("padding-left","52px");
            break;
            case 480:
                $(".domain-list").css("margin-left","-82px");
                $(".domain-list").css("padding-left","67px");
            break;
            case 535:
                $(".domain-list").css("margin-left","-96px");
                $(".domain-list").css("padding-left","81px");
            break;
            case 620:
                $(".domain-list").css("margin-left","-117px");
                $(".domain-list").css("padding-left","102px");
            break;
            case 700:
                $(".domain-list").css("margin-left","-137px");
                $(".domain-list").css("padding-left","122px");
            break;
        }

        });
    });
</script>
