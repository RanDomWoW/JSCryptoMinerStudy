        setTimeout(function() {
            var pull = $('#pull');
            var pull2 = $('#pull2');

            menu = $('#mobile-menu');
            $(pull).on('click', function(e) {
                e.preventDefault();
                menu.slideToggle();
            });

            $(pull2).on('click', function(e) {
                e.preventDefault();
                menu.slideToggle();
            });
        }, 3000);
    });

    $(document).ready(function() {
        $('#close-promo-mobile').on('click', function(event) {
            $('.ebook-promo-section').fadeOut();
            setTimeout(function() {
                $(".home-domain-checker-container").css("top","115px");
                $("nav#mobile-menu").css("top","118px");
            }, 400);
        });

        // Hide Ebook Promo Row when scrolling
        $(window).scroll(function(event){
            var isScroll = false;
            var scroll = $(window).scrollTop();

            if(isScroll == false) {
                $('.ebook-promo-row').show();
                $("nav#mobile-menu").css("top","113px");
                $('.logo-mobile').css("border-bottom","none");
            }

            if(scroll >= 1) {
                var isScroll = true;
                if(isScroll == true) {
                    $('.ebook-promo-row').hide();
                    $("nav#mobile-menu").css("top","65px");
                    $('.logo-mobile').css("border-bottom","1px solid #f0f0f0");
                }
            }
        });

        // hide contact & live chat if orientation change and width > 480
        $(window).bind("orientationchange", function(event) {
            setTimeout(function() {
                var devWidth = $(window).width();
            },200);

            if(devWidth > 480) {
                $('.phone-live').hide();
            } else {
                $('.phone-live').show();
            }
        });

        var i = 0;
        $('.zmdi-menu').on('click', function(event){
            if(i == 0){
                $(this).css({'color':'#fff','background-color':'#4d4d4d'});
                $('.zmdi-hc-border').css({'border':'1px solid #4d4d4d'});
                i++;
            } else {
                $(this).css({'color':'#000','background-color':'#fff'});
                $('.zmdi-hc-border').css({'border':'1px solid #9e9e9e'});
                i=0;
            }
        });
    });
</script> </div>
