<script>
function redraw() {
    var connectIds = [];
    var linkerboxes = document.getElementsByClassName('linkerbox');
    for(var i = 0; i < linkerboxes.length; i++) {
        connectIds.push(linkerboxes[i].dataset.connectId);
    }
    for(var i = 0; i < connectIds.length; i++) {
        draw(connectIds[i]);
    }
}
function draw(connectId) {
    //TODO display none if available
    var elemDiv = false;
    var append = false;
	var linkerboxes = document.getElementsByClassName('linkerbox' + connectId);
	for(var i = 0; i < linkerboxes.length; i++) {
		//linkerboxes[i].parentNode.removeChild(linkerboxes[i]);
        elemDiv = linkerboxes[i];
        elemDiv.style.opacity = 0;
	}
    if(!elemDiv) {
      append = true;
      elemDiv = document.createElement('div');
    }
	var anchors = document.querySelector('#anchor' + connectId);
	var targets = document.querySelector('#target' + connectId);

	var overlapHeight = 0;
	var overlapWidth = 0;
	var anchorElem = anchors;
	var targetElem = targets;
	var anchorStyle = anchorElem.currentStyle || window.getComputedStyle(anchorElem);
	var targetStyle = targetElem.currentStyle || window.getComputedStyle(targetElem);

	var bodyRect = document.body.getBoundingClientRect(),
		anchorElemBox = anchorElem.getBoundingClientRect(),
		targetElemBox = targetElem.getBoundingClientRect(),
		anchorOffset   = anchorElemBox.top + parseInt(anchorStyle.marginTop) - bodyRect.top,
		targetOffset   = targetElemBox.top + parseInt(targetStyle.marginTop) - bodyRect.top;

		// console.error(anchorOffset, anchorElemBox.height, parseInt(anchorStyle.marginTop));
		// console.error(targetOffset, targetElemBox.height, parseInt(targetStyle.marginTop));


	var topStart = (anchorOffset < targetOffset)
			? anchorOffset + anchorElemBox.height - overlapHeight
			: targetOffset + targetElemBox.height - overlapHeight;
	var topEnd = (anchorOffset < targetOffset)
			? targetOffset + overlapHeight + parseInt(targetStyle.marginTop)
			: anchorOffset + overlapHeight + parseInt(anchorStyle.marginTop);

	var leftStart = (anchorElemBox.left < targetElemBox.left)
			? anchorElemBox.left + anchorElemBox.width - overlapWidth
			: targetElemBox.left + targetElemBox.width - overlapWidth;
	var leftEnd = (anchorElemBox.left < targetElemBox.left)
			? targetElemBox.left + overlapWidth
			: anchorElemBox.left + overlapWidth;
	// console.info(topStart, topEnd);
	// console.info(leftStart, leftEnd);

	elemDiv.className  = "linkerbox linkerbox" + connectId;
	elemDiv.style.left = leftStart + "px";
	elemDiv.style.top = topStart + "px";
	elemDiv.style.width = leftEnd - leftStart + "px";
	elemDiv.style.height = (topEnd > topStart) ? topEnd - topStart + "px" : topStart - topEnd + "px";
    elemDiv.style.opacity = 1;
    elemDiv.dataset.connectId = connectId;
    if(append) {
    	document.body.appendChild(elemDiv);
    }
}

// window.addEventListener('resize', draw, true);
var _2load = {};
document.addEventListener("lazyloaded", function(event) {
    for(var i = 0, len = event.target.classList.length;i<len;i++) {
        var className = event.target.classList[i];
        if(className !== 'lazyloaded') {
            _2load[className] = 1;
            var parts = className.split('-');
            if(parts.length === 2) {
                var connectId = parts[1];
                if(_2load['anchor-' + connectId] > 0 && _2load['target-' + connectId] > 0) {
                    draw(connectId);
                }
            }
        }
    }
    redraw();
});

// Safari event if needed
// document.onreadystatechange = function() {
//     if (document.readyState === "complete") {
//     }
// }

window.addEventListener('resize', function() {
    redraw();
});

</script>


