	  }
	});
	
	$('.testimonials .bxslider').bxSlider({
	  auto: true,
	  pause: 10000,
	  autoHover: true,
	  speed: 400,
	  pager: false,
	  slideWidth: 800,
	  responsive: false
	});
	
	var advantages_slider = $('.advantages .bxslider').bxSlider({
	  auto: true,
	  pause: 10000,
	  autoHover: true,
	  speed: 400,
	  pager: false,
	  responsive: true,
	  onSlideAfter: function($slideElement, oldIndex, newIndex) {
		advantages_set_icon(newIndex + 1);
		$('#current-slide-bottom').html(newIndex + 1);
	  }
	});
	
	$('.advantages .selector .icon').click(function() {
	  advantages_set_icon($(this).data('slide'));
	  advantages_slider.goToSlide($(this).data('slide') - 1);
	});
	
	function advantages_set_icon(num) {
	  $('.advantages .selector .icon').removeClass('active');
	  $('.advantages .selector .icon[data-slide='+num+']').addClass('active');
	}




	
  });
</script>


