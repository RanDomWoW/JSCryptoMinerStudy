<script type="text/javascript">
$(document).ready(function(){ 

	var defaultText = "Enter a book title - get resources";

	// Replicate what we usually bind using #main_search. We don't want the associated CSS for that.
	$('#homepage_search_keywords').keyup(function(e) { areYouLookingFor('#homepage_search_keywords'); });

	$('#homepage_search_keywords').keydown(function(e) {
		if ($("#homepage_search_keywords").val() == defaultText) { 
			$("#homepage_search_keywords").val("");
			$('#homepage_search_keywords').unbind('keydown');
		}
	});

	$("#homepage_search_keywords").blur(function() {
		if ($("#homepage_search_keywords").val() == "") {
			$("#homepage_search_keywords").val( defaultText );
			$('#homepage_search_keywords').unbind('keydown');
			$('#homepage_search_keywords').keydown(function(e) {
				if ($("#homepage_search_keywords").val() == defaultText) { 
					$("#homepage_search_keywords").val("");
					$('#homepage_search_keywords').unbind('keydown');
				}
			});
		}
	});

	if ($('#homepage_search_keywords')) { $('#homepage_search_keywords').on('change',function() { loghit('inpage_events'); }); }


});
</script>


