<script type="text/javascript">
var googleTranslateLanguage = '';
$(document).ready(function() { 

	var observer = new MutationObserver( function(mutationsList) { 
		for(var mutation of mutationsList) { 
			if (mutation.attributeName == 'class') { 
				if ( $('html').hasClass('translated-ltr') || $('html').hasClass('translated-rtl') ) {
					pageTranslatedHook();
				} else { 
					pageUntranslatedHook();
				}
			}
		}
	});
	if (document.getElementById('htmlId')) { 
		observer.observe( document.getElementById('htmlId'), { attributes: true, childList: false } );
	}

});
function pageTranslatedHook() { 
	$('.hideWhenTranslated').hide();
	$('.showWhenTranslated').show();

	var language = $('#google_translate_element').find('.goog-te-menu-value').find('span:first').html();
	if (language == 'Select Language') { language = ''; }
	if (language != googleTranslateLanguage) { 
		loghit('translation,inpage_events');
	}
	googleTranslateLanguage = language;
	$.cookie('translated', googleTranslateLanguage);

	// Note seleected search type = $('#searchselector').find("input:radio[name='keyword_type1']:checked").attr('id');
	// Note selected lanague = $('#google_translate_element').find('.goog-te-menu-value').find('span:first').html();
	// Note the latter doesn't get set on a simple page load right away... google's widget has to run
}
function pageUntranslatedHook() { 
	$('.hideWhenTranslated').show();
	$('.showWhenTranslated').hide();

	var language = $('#google_translate_element').find('.goog-te-menu-value').find('span:first').html();
	if (language == 'Select Language') { language = ''; }
	if (language != googleTranslateLanguage) { 
		loghit('translation,inpage_events');
	}
	googleTranslateLanguage = language;
	$.cookie('translated', googleTranslateLanguage);
}
</script>





