<style type="text/css">
#sharingBarData { display: none; }
.sharing_bar_trigger {
  float:left;
  margin-top:5px;
  padding:0;
  font-weight:bold;
}
.sharing_bar_trigger img {
	float: left;
	margin: 0px 5px 5px 5px;
	padding:0;
}
.sharing_bar_trigger div { 
	float: left;
	margin: 0;
	padding: 0;
}
.sharing_bar_trigger:hover { 
	cursor: pointer;
}

.sbm-modal {
	display: none; /* Hidden by default */
	position: fixed; /* Stay in place */
	z-index: 99999; /* Sit on top */
	left: 0;
	top: 0;
	width: 100%; /* Full width */
	height: 100%; /* Full height */
	overflow: auto; /* Enable scroll if needed */
	background-color: rgb(0,0,0); /* Fallback color */
	background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
}

/* Modal Content */
.sbm-content {
	position: relative;
	background-color: #fefefe;
	margin: auto;
	margin-top: -250px;
	top: 50%;
	padding: 10px;
	border: 1px solid #888;
	width: 750px;
	/* height: 500px; */
	box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2),0 6px 20px 0 rgba(0,0,0,0.19);
	overflow: auto;
}
#sharingBarModalSpinner { 
	margin:auto;
}
</style>
<script type="text/javascript">

	$(document).ready(function(){ 
		$('.sharing_bar_trigger').click(function(e) { 
			handleSharingBarTrigger(e);
		});
		$('.sbm-close').click(function() { $('.sbm-modal').hide(); });

 	});
	function framePrint() { 
		window.print();
	}

	function handleSharingBarTrigger(e) { 
		var action = e.currentTarget.dataset.action;
		var p = $('#sharingBarData').data('p');
		var i = $('#sharingBarData').data('i');
		var u = $('#sharingBarData').data('u');
		var t = encodeURIComponent(document.title);
		var tid = $('#sharingBarData').data('tid');
		var lid = $('#sharingBarData').data('lid');
		var ugl_id = $('#sharingBarData').data('uglid');

		if (e.currentTarget.dataset.p) { p = e.currentTarget.dataset.p; console.log("p from dataset: "+p); }
		if (e.currentTarget.dataset.i) { i = e.currentTarget.dataset.i; }
		if (e.currentTarget.dataset.u) { u = e.currentTarget.dataset.u; }

		$('#sharingBarModalContentInner').html('<img id="sharingBarModalSpinner" src="/images/ajax-loader.gif">');
		$('#sharingBarModal').fadeIn(300);

		$.post( 
			"/sharing_bar.cgi",
			{ 'cmd': action, 'p': p, 'i': i, 'u': u, 't': t, 'tid': tid, 'lid': lid, 'ugl_id': ugl_id  },
			function(data) { 
				$('#sharingBarModalContentInner').html(data);
			}
		);
	}
</script>


