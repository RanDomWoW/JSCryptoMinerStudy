<script>
			var storeMonth = 2;
			var storeYear = 2018;
			var monthName = Array('January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December');
			var eventArray = null;
			var eventIndexes = null;
			var eventPos = 0;
			var schedule_score="";

			var today = new Date();
			var dd = today.getDate();
		
			var mm = today.getMonth(); //January is 0!
			var yyyy = today.getFullYear();

			var weekday = new Array(7);
			weekday[0]=  "Sunday";
			weekday[1] = "Monday";
			weekday[2] = "Tuesday";
			weekday[3] = "Wednesday";
			weekday[4] = "Thursday";
			weekday[5] = "Friday";
			weekday[6] = "Saturday";

			var dayName = weekday[today.getDay()]; 



			
			jQuery(document).ready(function() {	

					
				    //console.log(page_SPID +"-"+ page_IS_HOME_PAGE);
					if(page_IS_HOME_PAGE == "YES"){
						document.getElementById('schedule-sport-select').selectedIndex = 0;
						page_SPID=0;
					}else{
						jQuery("#schedule-sport-select").val(page_SPID);
					}
				    


					jQuery("#schedule-sport-select").change(function() {					
						var value = jQuery(this).val();
					 	generateCalendar(storeMonth, storeYear,value);				
					});				
					

					if(storeMonth == (mm+1)){
						generateCalendar(mm, yyyy,page_SPID);

					}else{
						generateCalendar(storeMonth, storeYear,page_SPID);
					}

						
					jQuery("#eventContainer #mainDate").html(dayName+", "+monthName[mm]+" "+ dd);


					
					//console.log(mm+"-"+yyyy);

					createTouchSwipe("#storeCalendar");
			});
			
			function FirstDayOfMonth(Year, Month) {
				return(new Date(new Date(Year, Month,1))).getDay();
			}

			function LastDayOfMonth(Year, Month) {
				return(new Date((new Date(Year, Month+1,1))-1)).getDate();
			}
			
			function calendarChange(calmonth, calyear,page_SPID) {
				jQuery("#storeCalendar .content .dates").fadeOut(500);
				jQuery("#storeCalendar .header .monthyear span").animate({
					left: -jQuery("#storeCalendar .header .monthyear span").width()
				}, 500);



				setTimeout('generateCalendar('+calmonth+', '+calyear+', '+page_SPID+');',500);
				setTimeout('jQuery("#storeCalendar .content .dates").fadeIn(500)',505);
			}
			
			function generateCalendar(calmonth, calyear,page_SPID) {
				startDay = FirstDayOfMonth(calyear, calmonth);
				endDay = LastDayOfMonth(calyear, calmonth);				
				nextMonth = calmonth+1;
				nextYear = calyear;
				lastMonth = calmonth-1;
				lastYear = calyear;
				currentDay = 0;
				currentWeek = 0;
				
				if (nextMonth > 11) {
					nextMonth = 0;
					nextYear++;	
				}
				if (lastMonth < 0) {
					lastMonth = 11;
					lastYear--;	
				}
				
				jQuery("#storeCalendar .header .left").unbind('click');
				jQuery("#storeCalendar .header .left").bind('click', function() {
					calendarChange(lastMonth, lastYear,page_SPID);
				});
				jQuery("#storeCalendar .header .right").unbind('click');
				jQuery("#storeCalendar .header .right").bind('click', function() {
					calendarChange(nextMonth, nextYear,page_SPID);
				});
				
				jQuery("#storeCalendar .header .monthyear").html('<span class="text">'+monthName[calmonth]+' </span>');
				
				





				spanWidth = jQuery("#storeCalendar .header .monthyear span").width();
				spanLeftEnd = (171 / 2) - (spanWidth / 2);
				
				
				
				
				jQuery("#storeCalendar .content .dates").html('');
				
				for (i=0;i<startDay;i++) {
					jQuery('<div class="box"><div class="blank">&nbsp;</div></div>').appendTo("#storeCalendar .content .dates");
					currentDay++;
				}



				

				
				zeromonth = calmonth+1;
				if (zeromonth < 10)
				zeromonth = '0'+zeromonth;
				eventDays = ':';
				//console.log(zeromonth);
				//console.log(calyear);
				jQuery.ajax({
				       type:"post",
			           url: getBaseUrl()+"/ajax/ScheduleItems.dbml",
			           dataType: 'json',
			           data: "DB_OEM_ID=28500&SPID="+page_SPID+"&store_month_dbml="+zeromonth+"&store_year="+calyear+"&event_date_format=MM/DD/YYYY",
			           success: function(data){
			           		eventArray = data;
			           		today = new Date();
			           		
			           		eventFlag = false;
			           		
			               for(var i=0;i<data.length;i++) 
			               		eventDays = eventDays+data[i].start_dd+'.'+i+':';   
			               	 //console.log('String Length: '+eventDays.length);	
			              
			               for (i=1;i<=endDay;i++) {
								boxclass = 'box';
								dateclass = 'date';
								boxonclick='';
								if (currentDay == 6) {
									dateclass = dateclass+' last';
									currentDay = 0;
									currentWeek++;
								}
								else {
									currentDay++;
								}	
							
								j = i;
								if (j < 10) 
									j = '0'+j;
								
								if (eventDays.indexOf(':'+j+'.') > -1) {	
									var loopFlag = true;
									var loopCount = 0;
									var loopStart = 0;
									eventIndexes = '';
									
									while (loopFlag) {
										if (eventDays.indexOf(':'+j+'.', loopStart) > -1 ) {					
											tempStart = eventDays.indexOf('.', eventDays.indexOf(':'+j+'.', loopStart));
											tempEnd = eventDays.indexOf(':', eventDays.indexOf(':'+j+'.', loopStart)+1);
											thisIndexOfEvent = eventDays.substr((tempStart + 1), (tempEnd - (tempStart + 1)));
											
											loopStart = eventDays.indexOf(':'+j+'.', loopStart)+1;
											loopCount++;
											eventIndexes = eventIndexes+thisIndexOfEvent+':';
										}
										else {
											loopFlag = false;
										}	
									}
									
									eventIndexes = eventIndexes.substr(0, (eventIndexes.length-1));					
									boxclass = boxclass+' dot pointer';
									dateclass = dateclass+' pointer';
									boxonclick='onclick="showEvent(\''+eventIndexes+'\');"';
									
									if (!eventFlag) {
										/*if (i <= dd && calmonth == mm) {												
											showEvent(eventIndexes);												
											eventFlag = true;
										}else if (calmonth > mm) {
											showEvent(eventIndexes);												
											eventFlag = true;
										}	*/
										if (i == dd && calmonth == mm ) {
											showEvent(eventIndexes);
											eventFlag = true;
										}

									}
								}

								

								if(i==dd && calmonth == mm)boxclass = boxclass+' highlight pointer';
								
								jQuery('<div class="'+boxclass+'"><div class="'+dateclass+'" id="date'+i+'" '+boxonclick+'>'+i+'</div></div>').appendTo("#storeCalendar .content .dates");
							}
							
							for (j=currentWeek;j<6;j++) {
								for (i=currentDay;i<7;i++) {
									if (currentDay == 6) {
										jQuery('<div class="box"><div class="blank last">&nbsp;</div></div>').appendTo("#storeCalendar .content .dates");
										currentDay = 0;
									}
									else {
										jQuery('<div class="box"><div class="blank">&nbsp;</div></div>').appendTo("#storeCalendar .content .dates");
										currentDay++;
									}
								}
							}							
							jQuery('<div class="clear"></div>').appendTo("#storeCalendar .content .dates");
			           }
			     });
				
			}
			
			function showEvent(eventsString) {				
				jQuery(".setHeightWidth").fadeOut();							
				setTimeout('showEventWork(\''+eventsString+'\');',500);
				setTimeout('jQuery(".setHeightWidth").fadeIn()',550);
			}
			
			function showEventWork(eventsString) {
				eventPos = 0;
				jQuery("#eventInfo").css('left', '0px');
				
				eventId = eventsString.split(":");
				
				if (eventId.length > 1) {
					jQuery("#eventLeft").show();
					jQuery("#eventRight").show();
				}
				else {
					jQuery("#eventLeft").hide();
					jQuery("#eventRight").hide();
				}
				
				i = 0;
				jQuery("#eventInfo").html('');
				jQuery("#eventInfo").css('width', '96%');
				
				while (eventId[i]) {			
				jQuery("#eventInfo").append('<div class="event" id="event_'+i+'"><div class="title"></div><div class="opp"></div><div class="clear"></div><div class="links"><div class="float SchLink stats"></div><div class="float SchLink LVideoExt"></div><div class="float SchLink LAudioExt"></div><div class="float SchLink LVideo"></div><div class="float SchLink LAudio"></div><div class="float SchLink tickets"></div></div><div class="clear"></div></div>');
				if(i==0)
				jQuery("#mainDate").html(eventArray[eventId[i]].mainDate);

				//console.log(eventArray[eventId[i]].mainDate);
				



				
				if(eventArray[eventId[i]].score_info !="" || eventArray[eventId[i]].home_score !=""){

					if(eventArray[eventId[i]].score_info !=""){	
						schedule_score =eventArray[eventId[i]].score_info;
					}else{
						schedule_score =eventArray[eventId[i]].home_score+' - '+eventArray[eventId[i]].opponent_score;
					}
				}


				if(schedule_score != ""){
						jQuery("#eventInfo #event_"+i+" .title ").html(schedule_score+'&nbsp; - &nbsp;<span style="color:orange">'+eventArray[eventId[i]].sport+'</span>&nbsp; - &nbsp;'+ eventArray[eventId[i]].simbol+' '+eventArray[eventId[i]].opponent);

					
				}else{

					jQuery("#eventInfo #event_"+i+" .title ").html(eventArray[eventId[i]].gametime+'&nbsp; - &nbsp;<span style="color:orange">'+eventArray[eventId[i]].sport+'</span>&nbsp; - &nbsp;'+ eventArray[eventId[i]].simbol+' '+eventArray[eventId[i]].opponent);
				

				}


				schedule_score="";

				////////////Stats//////////////
				if(eventArray[eventId[i]].stats_link || eventArray[eventId[i]].stats_id)
				{
					if (!eventArray[eventId[i]].stats_link) {
						if (eventArray[eventId[i]].stats_id) {
					jQuery("#eventInfo #event_"+i+" .links .stats").html('<A TITLE="Live Stats" CLASS="aStats orange" HREF="#" TARGET="_blank">Live Stats</a>');							
							jQuery("#eventInfo #event_"+i+" .links .stats a.aStats").attr('href', '/liveStats/liveStats.dbml?&DB_OEM_ID=28500');					
						}
						
						}
					if (eventArray[eventId[i]].stats_link) {
								jQuery("#eventInfo #event_"+i+" .links .stats").html('<A TITLE="Live link Stats" CLASS="aStats orange" HREF="#" TARGET="_blank">Live Stats</a>');		
								jQuery("#eventInfo #event_"+i+" .links .stats a.aStats").attr('href', eventArray[eventId[i]].stats_link);	
							}
				}



			var type_icon="";
				var type_icon2="";
				var is_external=false;
		
				
			 	 ////////////LIVE VIDEO/AUDIO EXT //////////////
			 	


				if (eventArray[eventId[i]].live_link1 != "") {
					is_external=true;
					if(eventArray[eventId[i]].live_link_type1 == "VIDEO"){
						jQuery("#eventInfo #event_"+i+" .links .LVideoExt").html('<A TITLE="Live Video External" CLASS="aLVideoExt orange" HREF="#" TARGET="_blank">'+eventArray[eventId[i]].live_link_text1+'</a>');
						jQuery("#eventInfo #event_"+i+" .links .LVideoExt a.aLVideoExt").attr('href', eventArray[eventId[i]].live_link1);	
							type_icon2="video";

					}
					if(eventArray[eventId[i]].live_link_type1 == "AUDIO"){
						jQuery("#eventInfo #event_"+i+" .links .LAudioExt").html('<A TITLE="Live Audio External" CLASS="aLAudioExt orange" HREF="#" TARGET="_blank">'+eventArray[eventId[i]].live_link_text1+'</a>');
						jQuery("#eventInfo #event_"+i+" .links .LAudioExt a.aLAudioExt").attr('href', eventArray[eventId[i]].live_link1);
							type_icon="audio";	

					}
					
				}

				if (eventArray[eventId[i]].live_link2  != "") {
					is_external=true;
					if(eventArray[eventId[i]].live_link_type2 == "VIDEO"){
						jQuery("#eventInfo #event_"+i+" .links .LVideoExt").html('<A TITLE="Live Video External" CLASS="aLVideoExt orange" HREF="#" TARGET="_blank">'+eventArray[eventId[i]].live_link_text2+'</a>');
						jQuery("#eventInfo #event_"+i+" .links .LVideoExt a.aLVideoExt").attr('href', eventArray[eventId[i]].live_link2);	
						type_icon2="video";


					}
					if(eventArray[eventId[i]].live_link_type2 == "AUDIO"){
						jQuery("#eventInfo #event_"+i+" .links .LAudioExt").html('<A TITLE="Live Audio External" CLASS="aLAudioExt orange" HREF="#" TARGET="_blank">'+eventArray[eventId[i]].live_link_text2+'</a>');
						jQuery("#eventInfo #event_"+i+" .links .LAudioExt a.aLAudioExt").attr('href', eventArray[eventId[i]].live_link2);
						type_icon="audio";

					}
					
				}





					

						//////////////////////////////LIVE AUDIO/VIDEO INTERNAL////////////////////
						if(!is_external){
							if (eventArray[eventId[i]].live_event_id_1) {
							if(eventArray[eventId[i]].event_type_1 == "VIDEO"){						
									jQuery("#eventInfo #event_"+i+" .links .LVideo").html('<A TITLE="Live Video" CLASS="aLVideo orange" HREF="#" TARGET="_blank">Video</a>');

									jQuery("#eventInfo #event_"+i+" .links .LVideo a.aLVideo").attr('href', '/liveEvents/liveEvents.dbml?db_oem_id=28500');
									type_icon2="video";
							}
							if(eventArray[eventId[i]].event_type_1 == "AUDIO_ONLY"){						
								jQuery("#eventInfo #event_"+i+" .links .LAudio").html('<A TITLE="Live Audio" CLASS="aLAudio orange" HREF="#" TARGET="_blank">Audio</a>');
								jQuery("#eventInfo #event_"+i+" .links .LAudio a.aLAudio").attr('href', '/liveEvents/liveEvents.dbml?db_oem_id=28500');
								type_icon="audio";

								}

							}	
							

						

						if (eventArray[eventId[i]].live_event_id_2) {
							if(eventArray[eventId[i]].event_type_2 == "VIDEO"){						
									jQuery("#eventInfo #event_"+i+" .links .LVideo").html('<A TITLE="Live Video" CLASS="aLVideo orange" HREF="#" TARGET="_blank">Video</a>');

									jQuery("#eventInfo #event_"+i+" .links .LVideo a.aLVideo").attr('href', '/liveEvents/liveEvents.dbml?db_oem_id=28500');
									type_icon2="video";
							}
							if(eventArray[eventId[i]].event_type_2 == "AUDIO_ONLY"){						
								jQuery("#eventInfo #event_"+i+" .links .LAudio").html('<A TITLE="Live Audio" CLASS="aLAudio orange" HREF="#" TARGET="_blank">Audio</a>');
								jQuery("#eventInfo #event_"+i+" .links .LAudio a.aLAudio").attr('href', '/liveEvents/liveEvents.dbml?db_oem_id=28500');
								type_icon="audio";

								}

							}	

						}
						

					
					

			


				
				
				
					if (eventArray[eventId[i]].tickets_event_id || eventArray[eventId[i]].tickets_link) {	
						////////////Tickets/////////////
						if (!eventArray[eventId[i]].tickets_link) {
							if (eventArray[eventId[i]].tickets_event_id) {
								jQuery("#eventInfo #event_"+i+" .links .tickets").html('<A TITLE="Tickets" CLASS="atickets orange" HREF="#" TARGET="_blank">Tickets</a>');		
								jQuery("#eventInfo #event_"+i+" .links .tickets a.atickets").attr('href', '/tickets/TicketHome.dbml?DB_OEM_ID=28500&amp;_MODE_=EVENTSELECT&amp;SALE_TKT_EVENT_ID='+eventArray[eventId[i]].tickets_event_id);
							} 

							}

						if (eventArray[eventId[i]].tickets_link) {	
						jQuery("#eventInfo #event_"+i+" .links .tickets").html('<A TITLE="Tickets" CLASS="atickets orange" HREF="#" TARGET="_blank">Tickets</a>');					
								jQuery("#eventInfo #event_"+i+" .links .tickets a.atickets").attr('href', eventArray[eventId[i]].tickets_link);					
							}
						}
				i++;
				}



			}
			
			function eventSlideRight() {				
				if (-(jQuery("#eventInfo").width()) < (eventPos-422))
					eventPos = eventPos - 422;
				jQuery("#eventInfo").animate({
					left: eventPos
				}, 500);
				
			}
			
			function eventSlideLeft() {
				if (eventPos < 0)
					eventPos = eventPos + 422;
