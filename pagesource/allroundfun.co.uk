					<script>

			
			var im_count = 0;
			var img_array = new Array();
			var link_array = new Array();
			var title_array = new Array();
			
								img_array.push("/images/links/Price_Match_Banner.jpg");
                    link_array.push("");
                    title_array.push("/Price Match");	
								img_array.push("/images/links/plum-trampolines_green.jpg");
                    link_array.push("/trampolines/above-ground?colour=&feature=&size=&price=&brand=plum-products");
                    title_array.push("/Plum Trampolines");	
								img_array.push("/images/links/ARF_Lay-Z-Spa_Generic_Banner.jpg");
                    link_array.push("/hot-tubs-and-accessories/hot-tubs?colour=&feature=&size=&price=&brand=lay-z-spa");
                    title_array.push("/Lay-Z-Spa Range");	
								img_array.push("/images/links/ARF_swing_sets_banner.jpg");
                    link_array.push("/swings-and-climbing-frames/swing-sets?colour=&feature=&size=&price=&brand=plum-products");
                    title_array.push("/Plum Swing Set Range");	
						
			function changeImage(){
				$("#imgholder").attr("src",img_array[im_count]);
				$("#imgholder").attr("title",title_array[im_count]);
				$("#imgholder").attr("alt",title_array[im_count]);
				$("#linkholder").attr("href",link_array[im_count]);
                
                $('.bannerLink').each(function(){
                    $(this).css('background', 'url("/images/common/dot-off.png") no-repeat transparent');
                });
                $('#link'+im_count).css("background", 'url("/images/common/dot-on.png") no-repeat transparent');
                
                //window.setTimeout("changeImage()", 5000);
                
				im_count++;
				
				if (im_count==img_array.length)
					im_count=0;
					
			}

			$(function(){
                changeImage();
                var interval = setInterval("changeImage()",7000);
                
                $(".bannerLink a").click(function(evt){
                    var p = $(this).parent();
                    var id = p.attr('id');
                    
                    im_count = id.replace('link', '');
                    changeImage();
                    
                    clearTimeout(interval);
                    evt.preventDefault();
                });
                
			});

			</script>
            
		
