now.setTime(now.getTime() + 1000 * 60 * 60 * 24 * 31);


document.cookie = name + "=" + escape(value)+  ";Domain=.hrm.ru;Path=/;Expires=" + now.toGMTString();
 //document.cookie = name + "=" + escape(value);
