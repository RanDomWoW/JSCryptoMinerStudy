 // Создаем массив, для хранения названия каждого месяца
 var ar = new Array(12)
if (nameMonth=="rus"||nameMonth=="russ"||nameMonth=="russs")
{


 ar[0] = "Январь"
 ar[1] = "Феврать"
 ar[2] = "Март"
 ar[3] = "Апрель"
 ar[4] = "Май"
 ar[5] = "Июнь"
 ar[6] = "Июль"
 ar[7] = "Август"
 ar[8] = "Сентабрь"
 ar[9] = "Октябрь"
 ar[10] = "Ноябрь"
 ar[11] = "Декабрь"
}else
{
 ar[0] = "January"
 ar[1] = "February"
 ar[2] = "March"
 ar[3] = "April"
 ar[4] = "May"
 ar[5] = "June"
 ar[6] = "July"
 ar[7] = "August"
 ar[8] = "September"
 ar[9] = "October"
 ar[10] = "November"
 ar[11] = "December"
}
 return ar[month]
}


