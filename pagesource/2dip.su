    function valid()
    {
        var flag=true;

        $('#modal-form #tbEmail').parent(".form-group").removeClass("has-error");
        $('#modal-form #tbName').parent(".form-group").removeClass("has-error");
        $('#modal-form #tbTheme').parent(".form-group").removeClass("has-error");
