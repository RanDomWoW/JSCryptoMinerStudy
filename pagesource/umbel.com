				}



		function getJsonFromString(thestring) {
			var result = {};
			var search = thestring;
			if(search){

				var theNewObject= JSON.parse('{"' + decodeURI(search).replace(/"/g, '\\"').replace(/&/g, '","').replace(/=/g,'":"') + '"}') || {};

				return _.mapKeys(theNewObject, function(value, key) {
					//console.log(key);
  					return key.toLowerCase();
				});

			}else{
				return {}
			}
		}

		var tempParamsObject={};

		if(Cookies.get('umbelformtracker')){
			tempParamsObject= getJsonFromString(Cookies.get('umbelformtracker'));
		}else{
			tempParamsObject = getJsonFromString(location.search.substring(1));

		}

		tempParamsObject={};
		// recent_conversion_page = Current URL (without params)
		//This tells us which page the user converted (submitted a form) on, and is useful for debugging and analytics purposes. WITHOUT QUERY PARAMETERS. We use these values for a lookup table that requires an exact match, so there can be no url parameters.
		tempParamsObject.recent_conversion_page = window.location.protocol + '//' + window.location.host + window.location.pathname;

		//This tells us which page the user converted (submitted a form) on, and is useful for debugging and analytics purposes. WITH QUERY PARAMETERS
		tempParamsObject.latest_conversion_url=window.location;

		// first_landing_page = Cookie (first page hit) OR Current URL
		// This is the most recent page the user landed on. It is used for analytics purposes.
		tempParamsObject.first_landing_page = tempParamsObject.first_landing_page || window.location;

		//do we need these?
		//tempParamsObject.elqSiteID = 1037187806
		//tempParamsObject.elqCookieWrite = 0
		//tempParamsObject.elqFormName = ??????? Form name generated in eloqua (e.g. ContactUs-1482172265555)

		var umbelParamString = Object.keys(tempParamsObject).map(function(k) {
			return encodeURIComponent(k) + '=' + encodeURIComponent(tempParamsObject[k])
		}).join('&')

