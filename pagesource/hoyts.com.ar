        if (diaItem.value != "0") {
            $.ajax({
                type: "GET",
                url: "jsonManager.aspx",
                data: "selected=" + diaItem.value + "&type=Day&cinema=" + $("#ctl00_slotContent_buy_express_SelectedCinemaId").val() + "&movie=" + $("#ctl00_slotContent_buy_express_SelectedShowId").val(),
                success: function (datos) {
                    $.each(datos, function (i, item) {
                        var title = item.label;

                        var dbox = "0";

                        var cuatrod = "0";

                        if (title.indexOf("D BOX") != -1) {
                            item.label = item.label.slice(0, -6);

                            dbox = "1";

                            $("#ctl00_slotContent_buy_express_isDbox").val(dbox);
                        }

                        if (title.indexOf("4D") != -1) {
                            item.label = item.label.slice(0, -3);

                            cuatrod = "1";

                            $("#ctl00_slotContent_buy_express_is4d").val(cuatrod);
                        }

                        var shoxPromoUPS = "0";
                        var typePromoUPS = "0";

                        if (title.indexOf("PROMOUPS") != -1) {
                            typePromoUPS = title.substring(17, 18)

                            item.label = item.label.slice(0, -10);

                            shoxPromoUPS = "1";

                            $("#ctl00_slotContent_buy_express_isPromoUPS").val(shoxPromoUPS);
                            $("#ctl00_slotContent_buy_express_typePromoUPS").val(typePromoUPS);
                        }

                        var listFilmEspecial = $("#ctl00_slotContent_buy_express_FuncionesEspeciales").val();

                        var isEspecial = "0";

                        var filmId = $("#ctl00_slotContent_buy_express_SelectedShowId").val();

                        if (listFilmEspecial.indexOf(filmId) != -1) {
