        var istransnoche = "0";

        var hour = parseInt($("#selectHora-mask span").text().substring(0, 3));

        if ((hour >= 0) && (hour <= 5)) {
            istransnoche = "1";
        }

        if ($("#ctl00_slotContent_buy_express_isDbox").val() == "1" && istransnoche == 0) {
            $("#sdbox").text("Usted ha seleccionado una funcion DBOX.");
