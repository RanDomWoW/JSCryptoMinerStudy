    function ClickDia(diaItem) {
        $("#popinPromoups").hide();
        $("#popinUPSXD").html("");
        $("#popinUPSXD").hide();
        $("#popinUPS4D").html("");
        $("#popinUPS4D").hide();
        $("#popinUPSDBOX").html("");
