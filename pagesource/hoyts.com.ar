                        $("#hdnPoints").val(result.MemberLoyaltyPoints);
                        $('.login-pts').text("MIS PUNTOS " + result.MemberLoyaltyPoints);
                        $('.login-sep').text("|");
                        $(".totalPointsCineFan").text(result.MemberLoyaltyPoints);
                        //console.log(result);
                    } else {
                        $(".mod-premios .puntosacumulados").hide();
                    }
                }

            }
        });

        $('.login-link').click(function () {
            var $login = $('#panel-account');
            var $title = $('#title-account');
            if (!$login.is(':visible')) {
                $('.account-arrow', $title).removeClass('s-account-arrow-closed').addClass('s-account-arrow-opened');
                $login.show();
            } else {
                $('.account-arrow', $title).removeClass('s-account-arrow-opened').addClass('s-account-arrow-closed');
                $login.hide();
            }
        });
    });

    $(function () {
        opcion(); 
    });

     function opcion() {    
        var items = $('.main-menu .menu-header td');
        for (var i = 0; i < items.length; i++) {
            items[i].className = (i == '0') ? 'active' : '';
        }
         $("#tabPremium").addClass('has-submenu');
         if ('0' == 7)
         {
             $("#tabPremium").addClass("active");
         }         
         if ('0' >= 9 && '0' <= 14) {  //Si esta dentro de los tabs del Cine Fan
             $('#panCineFanMenu').css("display", "block");
             var items = $('.loyalty-menu-header td');
             for (var i = 1; i <= items.length; i++) {
                items[i-1].className = (i == '0' - 8) ? 'boxitem active' : 'boxitem';
            }     
         }

    }

    function Buscar() {
        var title = "" + $('#inputSearchMovie2').val();
        var link;
        if ((title.length > 2) && (title != "Buscar")) {
            link = "/buscar/" + title;
            location = link;
        }
    }

    $(function () {
        $('#inputSearchMovie2').keydown(function (e) {
            if (e.keyCode != 13) return true;
            e.preventDefault();
            $('#Button1').click();
            return false;
        });
    });
    
    function ajaxlogin(data) {
        $.ajax({
            url: 'login.aspx',
            data: data,
            type: "GET",
            dataType: "json"
        }).done(function(result) {
            $('.my-panel .fb-ajax-loading').hide();
            $('#rb-message').show();
            var $form = $(this).closest('.form');
            if (result.error) {
                if (result.redirect) {
                    window.location = result.redirectto;
                    return false;
                }
                if (result.logout) {
                    $('.my-panel.account').hide();
                    /*$('#ctl00_Header_tabCineFan').css("display", "none");
                    $('#panCineFanMenu').css("display", "none");*/
                    $('.my-panel.login').show();                    
                    logoutTicket();

                    return false;
                }
                $('.form-msg').html('<div class="error">' + result.errormsg + '</div>').fadeIn();
                return false;
            }
            if (result.reminder == "1") {
                $('.login-dropdown-reminder').show();
                $('.login-dropdown-reminder img').click(function() {
                    $('.login-dropdown-reminder').hide();
                    $('#fblogin').click();
                    return false;
                });
                $('.remind-later a').click(function() {
                    $('.login-dropdown-reminder').hide();
                    return false;
                });
            }
            $('#username').html(result.nombreuser + " " + result.apellidouser);
            $('.my-panel.login').fadeOut(function() { $('.my-panel.account').fadeIn(); });
            if (result.LoyaltyMatchMade == "Y") {
                $('#ctl00_Header_tabCineFan').css("display", "");
                $('#ctl00_Header_tabCineFan').addClass("tabCineFan");
                getPoints(); //Llama a funcion que trae los puntos
            }
            if (result.asoc == "1") {
                $('#ascologin').hide();
            }

            location.reload();
        });
    }

    $(function () {
        var $panel = $('.my-panel.login');
        var rememberme = $('#login-remember');

        $('#logout').click(function () {
            var data = { modlogin: "logout" };
            ajaxlogin(data);
        });


        $('#fblogin').click(function () {
            FB.login(function (response) {
                if (response.authResponse) {
                    var accessToken = FB.getAuthResponse()['accessToken'];
                    var data = { token: accessToken, modlogin: "fb", ret: $(location).attr('href') };
                    $('#rb-message').hide();
                    $('#fb-ajax-loading').show();
                    ajaxlogin(data);
                } else {
                    return false;
                }
            }, { scope: 'email,user_birthday' });
            return false;
        });

        $('#ascologin').click(function () {
            FB.login(function (response) {
                if (response.authResponse) {
                    var accessToken = FB.getAuthResponse()['accessToken'];
                    var data = { token: accessToken, modlogin: "fb", ret: $(location).attr('href') };
                    $('#rb-message').hide();
                    $('#fb-ajax-loading').show();
                    ajaxlogin(data);
                } else {
                    return false;
                }
            }, { scope: 'email,user_birthday' });
            return false;
        });

        $('.my-panel.login .submit').click(function () {

            var $form = $(this).closest('.form');
            $('.form-msg', $form).html('').hide();
            var user = $('input[name=email]', $form).val().toLowerCase();;
            var pass = $('input[name=pass]', $form).val();

            if (pass == '') {
                pass = $('input[id=login-pass]', $form).val();
            }

            var sremember = 'N';
            if (rememberme.is(':checked')) {
                sremember = "Y";
            }
            var data = { user: user, pass: pass, remember: sremember, modlogin: "cl", ret: $(location).attr('href') };
            $('#rb-message').hide();
            $('#fb-ajax-loading').show();
            ajaxlogin(data);
        });

        $('.remind-later a').click(function () {
            $(this).closest('.login-dropdown-reminder').hide();
            return false;
        });

        $('input', $panel).keydown(function (e) {
            if (e.keyCode == 13) {
                $(this).closest('.form').find('.submit').click();
            }
        });

        /* PANEL LOGIN */
        $('.login-link').click(function () {
            var $login = $(this).closest('.data').find('.panel-dropdown');
            var $title = $(this).closest('.data').find('.panel-title');
            if (!$login.is(':visible')) {
                $('.account-arrow', $title).removeClass('s-account-arrow-closed').addClass('s-account-arrow-opened');
                $login.show();
            } else {
                $('.account-arrow', $title).removeClass('s-account-arrow-opened').addClass('s-account-arrow-closed');
                $login.hide();
            }

            $('.login-dropdown-reminder').hide();
            return false;
        });
        /* /PANEL LOGIN */
    });

    function logoutTicket() {
        $(function () {
            $.ajax({
                url: "https://tickets.hoyts.com.ar/InternetTicketing/visMbrLogoutFromWeb.aspx",
                type: "GET",
                xhrFields: {
                    withCredentials: true
                },
                dataType: "json"
            }).done(function (result) {
                location.reload();
            });        
        });
    }

    function getPoints() {
        $(function () {
            $.ajax({
                url: "https://tickets.hoyts.com.ar/InternetTicketing/LoyaltyJson.aspx?mode=notforce",
                xhrFields: {
                    withCredentials: true
                },
                type: "GET",
                dataType: "json"
            }).done(function (result) {
                if (result.error = "0") {
                    var datos = { modlogin: "setMbrLvlLoyalty", level: result.MemberLoyaltyLevel };

                    $.ajax({
                        url: 'login.aspx',
