        if ($("#ctl00_slotContent_buy_express_isDbox").val() == "0" && $("#ctl00_slotContent_buy_express_is4d").val() == "0" && istransnoche == 0 && $("#ctl00_slotContent_buy_express_isEspecialFuncion").val() == "1") {
