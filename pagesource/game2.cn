    $(document).ready(function(){
        // 切换
        var gids = [];
        var $updateHotLink = $('.updateHotLink');
        $updateHotLink.each(function(){
            var gid = parseInt($(this).attr('data-gid'));
            if (gid) gids.push(gid);
        });
        $.ajax({
            type: 'get',
            url: '/websiteAjax/op/getRecentlyserver/',
            data: {gids: gids.join(',')},
            success: function(result){
               if (result) {
                   $('.newSer').before(result);
                    $('#prelodeserver').hide();
                   if (window.newServerGame) {
                        var newserver = window.newServerGame.newserver || null;
                        var htserver = window.newServerGame.htserver || null;
                       $updateHotLink.each(function(){
                           var $that = $(this);
                           var gid = parseInt($that.attr('data-gid'));
                           var htmlArr = ['<p>'];
                           if (newserver && newserver[gid]) {
                               htmlArr.push('<span>最新服：<strong>' + newserver[gid].name.replace('服', '') + '</strong> ' + newserver[gid].dw + '</span>');
                               htmlArr.push('<a class="aIn" href="/joinGame/code/' + newserver[gid].code + '/" target="_blank">开始游戏</a>');
                           } else if (htserver && htserver[gid]) {
                               if (htserver[gid].state == 4) {
                                   htmlArr.push('<span>即将开启：<strong>' + htserver[gid].name.replace("服", "") + '</strong> 服</span>');
                               } else {
                                   htmlArr.push('<span>维护中</span>');
                               }
                               htmlArr.push('<a class="aIn" href="/server/code/' + $that.attr('data-gcode') + '/" target="_blank">开始游戏</a>');
                           }
                           htmlArr.push('</p>');
                           $that.html(htmlArr.join(''));
                       });
                   }
               }
            }
        });
    });
</script>

	
