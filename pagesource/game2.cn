    <script type="text/javascript">
        (function(){
             var is_cache = "1";
             if (is_cache) {
                 $.ajax({
                     type: 'get',
                     url: '/websiteAjax/op/getLoginState/',
                     success: function(result){
                        if (result) {
                            $('#loginForms').hide().after(result);
                        }
                     }
                 });
             }
        })();
        (function(){
            var keepcode= getCookie("KEEPCODE");
            window.hascode = true;
                window.ucode = '';
                function checkPwdInput(icode) {
                    var code = $('#code').val();
                    code = code.replace(/^\s*|\s*$/g,'');
                    $('#code').val(code);
                    if(code) {
                        if(/^[a-z_][a-z_0-9]*$/i.test(code)) {
                            if(code.length > 20 || code.length <3) {
                                $('.pWarn').html('您输入的登录账号/手机号有误');
                                $('#code').parent().addClass('pErr');
                                return false;
                            }
                        } else if(!/^[0-9]{6,14}$/i.test(code)) {
                            $('.pWarn').html('您输入的登录账号/手机号有误');
                            $('#code').parent().addClass('pErr');
                            return false;
                        }
                    } else {
                        $('.pWarn').html('请输入您的登录账号/手机号');
                        $('#code').parent().addClass('pErr');
                        return false;
                    }

                    if(icode == 'code') {
                        $('.pWarn').html('');
                        $('#code').parent().removeClass('pErr');
                        return true;
                    }
                    var pwd = $('#password').val();
                    if(!pwd) {
                        $('.pWarn').html('请输入您的登录密码');
                        $('#password').parent().addClass('pErr');
                        return false;
                    } else if(pwd.length < 6) {
                        $('.pWarn').html('您输入的密码有误');
                        $('#password').parent().addClass('pErr');
                        return false;
                    }

                    if(icode == 'password') {
                        $('.pWarn').html('');
                        $('#password').parent().removeClass('pErr');
                        return true;
                    }
                    return true;
                }
                function checkInput(icode)
                {
                    var vcode = $('#vcode').val();
                    if(!vcode) {
                        $('.pWarn').html('请输入验证码');
                        $('#vcode').parent().addClass('pErr');
                        return false;
                    } else if(!(/^\d{4}$/.test(vcode))) {
                        $('.pWarn').html('验证码错误');
                        $('#vcode').parent().addClass('pErr');
                        return false;
                    }
                    $('#vcode').parent().removeClass('pErr');
                    return true;
                }

                $(document).ready(function(){
                    var formsec = '';
                    $('#loginForms').bind('submit', function(evt, label) {
                        if(checkPwdInput()) {
                            $('.pWarn').html('');
                            var _postData = "op=login&usercode=" + $('#loginForms #code').val() + '&password=' + hex_md5($('#loginForms #password').val());
                            if($('#loginForms').find('.checkbox').hasClass('checked')) {
                                _postData += '&keeplive=1';
                            }

                            
                                if(window.hascode == true) {
                                    if(checkInput('vcode')) {
                                        _postData += '&vcode=' + $('#loginForms #vcode').val();
                                    } else {
                                        if($('.uc .logVi').length) {
                                            $('.uc .login').hide();
                                            $('.uc .logVi').show();
                                            document.getElementById('vimg').src = '/verifyCode.php?id=' + Math.random();
                                        }
                                        return false;
                                    }
                                }

                                window.ucode = $('#code').val();

                                $.ajax({
                                    type: 'post',
                                    url: '/websiteAjax/' + (formsec ? 'formsec/' + formsec + '/' : ''),
                                    data: _postData,
                                    success: function(data){
                                        formsec = '';
                                        var pos = data.indexOf('|');
                                        if (pos > -1) {
                                            var $d = [data.substr(0, pos), data.substr(pos+1)];
                                        } else {
                                            var $d = [data];
                                        }
                                        switch($d[0]) {
                                            case 'login':
                                            case '1':
                                                window.location.href = window.location.href + '';
                                                break;
                                            case '-6':
                                                if($('.uc .logVi').length) {
                                                    $('.uc .login').show();
                                                    $('.uc .logVi').hide();
                                                }
                                                $('.pWarn').html('操作过于频繁，请15分钟后再试');
                                                break;
                                            case '-11':
                                                $('#vcode').parent().addClass('pErr');
                                                $('#vimg').trigger('click');
                                                break;
                                            case 'vcode_err':
                                                if($('.uc .logVi').length) {
                                                    $('.uc .login').hide();
                                                    $('.uc .logVi').show();
                                                    $('#count_log').html($d[1]);
                                                }
                                                $('.pWarn').html('验证码错误');
                                                $('#vcode').parent().addClass('pErr');
                                                window.hascode = true;
                                                document.getElementById('vimg').src = '/verifyCode.php?id=' + Math.random();
                                                break;
                                            case 'vcode_show':
                                                if($('.uc .logVi').length) {
                                                    $('.uc .login').hide();
                                                    $('.uc .logVi').show();
                                                    $('#count_log').html($d[1]);
                                                    $('.pWarn').html('');
                                                }
                                                window.hascode = true;
                                                document.getElementById('vimg').src = '/verifyCode.php?id=' + Math.random();
                                                break;
                                            case '-1':
                                                if($('.uc .logVi').length) {
                                                    $('.uc .login').show();
                                                    $('.uc .logVi').hide();
                                                }
                                                $('.pWarn').html('您输入的登录账号/手机号不存在');
                                                $('#code').parent().addClass('pErr');
                                                document.getElementById('vimg').src = '/verifyCode.php?id=' + Math.random();
                                                break;
                                            case '-5':
                                                $('.pWarn').html('操作过于频繁，请稍后再试');
                                                document.getElementById('vimg').src = '/verifyCode.php?id=' + Math.random();
                                                break;
                                            case 'formsec':
                                                if (label) {
                                                    if ($('.uc .logVi').length) {
                                                        $('.uc .login').show();
                                                        $('.uc .logVi').hide();
                                                    }
                                                    $('.pWarn').html('操作过于频繁，请稍后再试');
                                                } else {
                                                    formsec = eval('('+$d[1]+')');
                                                    $('#loginForms').trigger('submit', 'sub');
                                                }
                                                break;
                                            default:
                                                if($('.uc .logVi').length) {
                                                    $('.uc .login').show();
                                                    $('.uc .logVi').hide();
                                                }
                                                $('.pWarn').html('账号或密码错误');
                                                $('#code').parent().addClass('pErr');
                                                $('#password').parent().addClass('pErr');
                                                break;
                                        }
                                    }
                                });
                            } else if($('.uc .logVi').length){
                                $('.uc .login').show();
                                $('.uc .logVi').hide();
                            }
                            return false;
                        });

                    $(".login .i:input").bind('blur', function(){
                        var _id = $(this).attr('id');
                        checkPwdInput(_id);
                    });

                    $(".logVi .i:input").bind('blur', function(){
                        var _id = $(this).attr('id');
                        checkInput('vcode');
                    });

                    setTimeout(function() {
                        if(keepcode) {
                            $('#code').val(keepcode).addClass('ii');
                            $('#code').next().show();
                            $('#password').val("");
                        }
                        $("#code, #password").each(function() {
                            var _val = $(this).val();
                            if(_val != '') {
                                $(this).siblings("span").hide();
                            }
                        });
                    }, 100);

                    $('.uc .logVi .vix').length && $('.uc .logVi .vix').bind('click', function(){
                        $('.uc .login').show();
                        $('.uc .logVi').hide();
                        $('.pWarn').html('');
                        return false;
                    });

                    $('#code').bind('keyup', function(){
                        var _val = $(this).val();
                        if(_val == '') {
                            $(this).next().hide();
                        } else {
                            $(this).next().show();
                        }
                    });

                    $('#p1 .del').bind('click', function(){
                        $('#code').val('').focus();
                        $(this).hide();
                        return false;
                    });

                    $('.logp input').bind('focus', function(){
                        $(this).parent().removeClass('pErr');
                    });
                });
            })();
    </script>
