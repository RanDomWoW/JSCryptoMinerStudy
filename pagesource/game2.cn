        {
          $("#tsSwf").hide().html('')
          $("html,body").css({"overflow":"auto","overflow-x":"hidden"});
        };
        function closeSwf()
        {
          $("#tsSwf").hide().html('')
          $("html,body").css({"overflow":"auto","overflow-x":"hidden"});
        }
