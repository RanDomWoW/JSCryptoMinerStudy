        <script>
            function MobileIsLive() {
                return true;
            }

            function SiteIsEnglish() {
                var link = $('#englishLink')[0];
                if (link.style.fontWeight == "bold") return true;

                return false;
            }

            function BrowserIsMobile() {

                if (navigator.userAgent.match(/Android/i)
                     || navigator.userAgent.match(/webOS/i)
                     || navigator.userAgent.match(/iPhone/i)
                     || navigator.userAgent.match(/iPad/i)
                     || navigator.userAgent.match(/iPod/i)
                     || navigator.userAgent.match(/BlackBerry/i)
                     || navigator.userAgent.match(/Windows Phone/i)
                   ) {
                    return true;
                }
                else {
                    return false;
                }
            }

            function setCookieAcceptance() {
                var d = new Date();
                d.setMonth(d.getMonth() + 1);
                var expires = "expires=" + d.toUTCString();
                document.cookie = "lw.com.cookie-acceptance=1;path=/;" + expires;
            }

            function getCookieAcceptance() {

                var cookieArray = document.cookie.split(';');
                for (i = 0; i < cookieArray.length; i++) {
                    var cookie = cookieArray[i];
                    while (cookie.charAt(0) == ' ') {
                        cookie = cookie.substring(1);
                    }
                    if (cookie.indexOf("lw.com.cookie-acceptance=") == 0) {
                        return cookie.substring(25, cookie.length);
                    }                    
                }
                return "";
            }

            function setMobilePreference(preference) {
                var d = new Date();
                d.setMonth(d.getMonth() + 1);
                var expires = "expires=" + d.toUTCString();
                document.cookie = "m.lw.com.mobile-preference=" + preference + ";" + expires;
            }

            function getMobilePreference() {

                var cookieArray = document.cookie.split(';');
                for (i = 0; i < cookieArray.length; i++) {
                    var cookie = cookieArray[i];
                    while (cookie.charAt(0) == ' ') {
                        cookie = cookie.substring(1);
                    }
                    if (cookie.indexOf("m.lw.com.mobile-preference=") == 0) {
                        return cookie.substring(27, cookie.length);
                    }
                }

                return "";
            }

            $(document).ready(function () {
                $('#PersonalInfo a[href*="mailto:"], #ContactList a[href*="mailto:"], .businessCardInfo a[href*="mailto:"], #EmailLabel a[href*="mailto:"]').on('click', function (event) {
                    event.preventDefault();
                    $('#overlay').fadeIn('fast');
                    $('#email-disclaimer-button-continue').prop('href', this.href);
                });
                $('#email-disclaimer-button-continue').on('click', function (event) {
                    event.preventDefault();
                    var loc = $(this).prop('href');
                    window.location = loc;
                    $('#overlay').fadeOut('fast');
                });
                $('#email-disclaimer-button-cancel, #overlay').on('click', function () {
                    $('#overlay').fadeOut('fast');
                    return false;
                });
                $('#acceptCookieButton').on('click', function () {
                    setCookieAcceptance();
                    $('#cookieDisclaimer').hide();
                });
                // hide the footer mobile link and the pipe separator ( "|" ) next to it
                $('.linkToMobile').hide().next().hide();

                if (MobileIsLive() && BrowserIsMobile() && SiteIsEnglish()) {

                    // show the link to the mobile site, and make sure that
                    // clicking it sets the preference cookie to mobile
                    $('.linkToMobile').show().next().show();
                    $('.linkToMobile').click(function () {
                            setMobilePreference("mobile");
                        });
                }
                if (getCookieAcceptance() == "") {
                    $('#cookieDisclaimer').show();
                }
            });
        </script>
