<script type="text/x-template" id="autocomplete">
  <div :class="{
    'form-group': true,
    'has-error': validateErrors && validateErrors.has(inputProps.id)
  }">

    <label :for="inputProps.id" v-if="label">
      {{ label }}
      <span class="color-required" v-if="inputProps.required"><i class="icon-star"></i></span>
    </label>

    <input
      :type="inputProps.type"
      :name="inputProps.id"
      :id="inputProps.id"
      :autocomplete="'off'"
      :placeholder="inputProps.placeholder"
      @keyup.prevent.stop.up.down="keyup"
      @keyup.prevent.stop.enter="enter"
      @focusout="inputFocusOut"
      v-model="input"
      ref="input"
    >

    <span class="has-feedback" v-if="validateErrors && validateErrors.has(inputProps.id)">{{ validateErrors.first(inputProps.id) }}</span>

    <div class="autocomplete" v-if="items.length">
      <ul>
        <li
          v-for="(item, idx) in items"
          v-if="idx < maxItems"
          @click="selectItem(item)"
          :class="{selected: selectedIndex === idx}"
          v-html="item.matched.value"
        ></li>
      </ul>
    </div>

  </div>
</script>



