<script>
    var $container = $('#whats-on-all'),
        filter = {
            tabs: '',
            detail: {
                genre: [],
                format: [],
                certificate: [],
                special: []
            }
        };

    $('#whats-on-all').isotope({
        // options
        itemSelector : '.film-box-single',
        hiddenClass: 'hide',
        getSortData : {
            name : function ( $elem ) {
              return $elem.find('.filmTitle').text();
            },
            rating : function ( $elem ) {
              return $elem.attr('data-filmrating');
            },
            releasedate : function ( $elem ) {
              return $elem.attr('data-releasedate');
            }
        },
        filter: '.category_topFilms'
      },refreshShowMoreContainer);

    

    $('.tab_topFilms').addClass('active');


    $('.genre input').change(function(){
        clearRefreshShowmoreTimer();
        var selector = $(this).attr('id');
        if ($(this).prop('checked')) {
            filter.detail.genre.push('.'+selector);
        } else {
            filter.detail.genre = $.grep(filter.detail.genre, function(value){
                return value !== '.'+selector;
            });
        }
        filter_isotope();
        return false;
    });

    $('.format input').change(function(){
        clearRefreshShowmoreTimer();
        var selector = $(this).attr('id');
        if ($(this).prop('checked')) {
            filter.detail.format.push('.'+selector);
        } else {
            filter.detail.format = $.grep(filter.detail.format, function(value){
                return value !== '.'+selector;
            });
        }
        filter_isotope();
        return false;
    });

    $('.certificate input').change(function(){
        clearRefreshShowmoreTimer();
        var selector = $(this).attr('id');
        if ($(this).prop('checked')) {
            filter.detail.certificate.push('.'+selector);
        } else {
            filter.detail.certificate = $.grep(filter.detail.certificate, function(value){
                return value !== '.'+selector;
            });
        }
        filter_isotope();
        return false;
    });

    $('.special input').change(function(){
        clearRefreshShowmoreTimer();
        var selector = $(this).attr('id');
        if ($(this).prop('checked')) {
            filter.detail.special.push('.'+selector);
        } else {
            filter.detail.special = $.grep(filter.detail.special, function(value){
                return value !== '.'+selector;
            });
        }
        filter_isotope();
        return false;
    });

    $('.film-grid_tab a').click(function(){
        clearRefreshShowmoreTimer();
        var selector = $(this).attr('data-tab-filter');
        $('.film-grid_tab').removeClass('active');
        $(this).parent().addClass('active');
        filter.tabs = selector;
        filter_isotope();

            if (selector == '.category_highestRated') {
            $container.isotope({ sortBy: 'rating', sortAscending: false},refreshShowMoreContainer)
        } else {
            $container.isotope({ sortBy: 'original-order', sortAscending: true},refreshShowMoreContainer)
        }
    
        return false;
    });


    function preSelectTab()
    {
        try{
            var selectedTab = localStorage.getItem("selectedTab");
            if(selectedTab != null)
            {
            localStorage.removeItem("selectedTab");
            $('.film-grid_tab').removeClass('active');
            var selector = $('a[data-tab-filter^="'+selectedTab+'"]');
            selector.parent().addClass('active');
            filter.tabs = selectedTab;
            filter_isotope();
            }
            return false;
        }catch(e){
            return false;
        }
    }
    function filter_isotope()
    {
        muh = [filter.tabs];
        muh = parseFilter(muh, filter.detail.genre);
        muh = parseFilter(muh, filter.detail.format);
        muh = parseFilter(muh, filter.detail.certificate);
        muh = parseFilter(muh, filter.detail.special);

        selector = muh.join(',');
        $container.isotope({ filter: selector}, refreshShowMoreContainer);
    }

    function parseFilter(first, second)
    {
        ret = [];
        if(first.length == 0 && second.length > 0 ) {
            return second;
        } else if (first.length > 0 && second.length == 0) {
            return first;
        } else if (first.length == 0 && second.length == 0) {
            return ret;
        }

        $.each(first, function(firstKey, firstValue){
            $.each(second, function(secondKey, secondValue){
                ret.push(firstValue+secondValue);
            });
        });

        return ret;
    }

    $('#whats-on-sort-films').change(function(){
       var $sortBy = $(this).val();
       if($sortBy == 'order_desc') {
           $container.isotope({ sortBy : 'name', sortAscending : false },refreshShowMoreContainer);
       } else if ($sortBy == 'order_asc') {
           $container.isotope({ sortBy : 'name', sortAscending : true },refreshShowMoreContainer);
       } else if ($sortBy == 'order_rating') {
           $container.isotope({ sortBy : 'rating', sortAscending : false },refreshShowMoreContainer);
       } else if ($sortBy == 'order_releasedate') {
           $container.isotope({ sortBy : 'releasedate', sortAscending : true },refreshShowMoreContainer);
       }
    });

    $('#whats-on-quick-select').change(function(){
        selectedOption = $($(this).find('option:selected'));
        window.location.href= selectedOption.attr('data-filmurl');
    });
    preSelectTab();
