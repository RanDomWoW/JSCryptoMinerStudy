<script type="text/javascript">
function updatecss(Searchalljobs) {
    var index = document.forms[Searchalljobs].ntypeofwork.selectedIndex;
    var category = document.forms[Searchalljobs].ntypeofwork[index].value;
    switch (category)
    {
	    case "6":
	        document.forms[Searchalljobs].css.value = "e";
