<script type="text/javascript">
        google.maps.event.addDomListener(window, 'load', function () {
            //ECS - 604 START
            jQuery('#locationId').keydown(function(e) { 
        	if(e.which==13)
        	{
        		e.preventDefault();
        		return false;
        		 
        	}
            }); 
            //ECS - 604 END
        	var options = {
        		types: ["geocode"]
        	};
            var places = new google.maps.places.Autocomplete(document.getElementById('locationId'), options);
            google.maps.event.addListener(places, 'place_changed', function () {
                var place = places.getPlace();
//                 var address = place.formatted_address;
                var latitude = place.geometry.location.lat();
                var longitude = place.geometry.location.lng();
//                 var mesg = "Address: " + address;
//                 mesg += "\nLatitude: " + latitude;
//                 mesg += "\nLongitude: " + longitude;
//                 alert(mesg);
                document.getElementById("latitude").value = latitude;
				document.getElementById("longitude").value = longitude;

				var country = place.address_components;
				document.getElementById("country").value = "United Knigdom";
				if((country[country.length-1].types[0] == "country") && (country[country.length-1].types[1] == "political")) {
 					document.getElementById("country").value = country[country.length-1].long_name;
				} else {
					document.getElementById("country").value = country[country.length-2].long_name;
				} 

				if(country.length == 1) {
					document.getElementById("location").value = country[country.length-1].long_name;
				} else {
					for(var i=0;i<country.length-1;i++) {
						if(country[i].types[0] == "postal_town") {
							document.getElementById("location").value = country[i].long_name;
							break;
						}
						if (country[i].types[0] == "locality") {
							document.getElementById("location").value = country[i].long_name;
							break;
						}
					}
				}
            });
        });
    </script>
