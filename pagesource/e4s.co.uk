	        document.forms[Searchalljobs].action = "/search/entry-level-job-results";
	        break;
        case "4":
            document.forms[Searchalljobs].css.value = "p";
            document.forms[Searchalljobs].act.value = "p";
            document.forms[Searchalljobs].action = "/search/part-time-holiday-job-results";
            break;
        case "3":
            document.forms[Searchalljobs].css.value = "t";
            document.forms[Searchalljobs].act.value = "t";
            document.forms[Searchalljobs].action = "/search/gap-temp-job-results";
            break;
        case "2":
            document.forms[Searchalljobs].css.value = "i";
            document.forms[Searchalljobs].act.value = "i";
            document.forms[Searchalljobs].action = "/search/internship-job-results";
            break;
        case "1":
            document.forms[Searchalljobs].css.value = "g";
            document.forms[Searchalljobs].act.value = "g";
            document.forms[Searchalljobs].action = "/search/graduate-job-results";
            break;
        default:
            document.forms[Searchalljobs].css.value = "a";
            document.forms[Searchalljobs].act.value = "a";
            document.forms[Searchalljobs].action = "/search/all-jobs-results";
    }
}
