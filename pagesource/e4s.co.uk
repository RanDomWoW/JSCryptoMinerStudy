    <script type="text/javascript">
        google.maps.event.addDomListener(window, 'load', function () {
        	var options = {
            		types: ["geocode"]
            	};
            var places = new google.maps.places.Autocomplete(document.getElementById('locationId1'), options);
            google.maps.event.addListener(places, 'place_changed', function () {
                var place = places.getPlace();
                var address = place.formatted_address;
                var latitude = place.geometry.location.lat();
                var longitude = place.geometry.location.lng();
                document.getElementById("latitude1").value = latitude;
				document.getElementById("longitude1").value = longitude;
				
				var country = address.substring(address.lastIndexOf(',') + 1);
				if(country.match(/UK/gi)) {
                	country = "United Kingdom";
                }
				if(country.match(/United States/gi)) {
                	country = "USA";
                }
                document.getElementById("country1").value = country;
                dValue= address.slice(0,address.lastIndexOf(','));
                var str2 = dValue.slice(dValue.lastIndexOf(',')+1,dValue.length).trim();
                dValue = dValue.slice(0,dValue.lastIndexOf(',')+1);
                if(str2.indexOf(' ')>0) {  
                    str2=str2.slice(0,str2.indexOf(' ')); 
                }
                dValue += str2;
                document.getElementById("location1").value = dValue;

				if (dValue.length > 1) {
					var county = dValue.substring(dValue.lastIndexOf(',') + 1);
					document.getElementById("location1").value = county;
				}
            });
        });
    </script>
