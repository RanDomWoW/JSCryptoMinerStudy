<script>
var noServiceDates = ["25-12-2014","25-12-2015"];
function blackoutDates(date) {
	var m = date.getMonth(), d = date.getDate(), y = date.getFullYear();
	for (i = 0; i < noServiceDates.length; i++) {
		if($.inArray(d + '-' + (m+1) + '-' + y,noServiceDates) != -1){
			return {0: false}
		}
	}
	return {0: true}
}

