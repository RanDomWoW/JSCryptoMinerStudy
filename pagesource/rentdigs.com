<script>
    $(document).ready(function () {

        //$("#ContentPlaceHolder1_Search4_txtZip").mask("99999");
        //$("#ContentPlaceHolder1_Search4_txtRentMin").mask("$9?9999");
        //$("#ContentPlaceHolder1_Search4_txtRentMax").mask("$9?9999");

        $("#ContentPlaceHolder1_Search4_cbBedsAny").change(function (e) 
        {
            if ($("#ContentPlaceHolder1_Search4_cbBedsAny").is(':checked') == true) {
                $("#ContentPlaceHolder1_Search4_cbBedsStudio").prop("checked", false);
                $("#ContentPlaceHolder1_Search4_cbBeds1").prop("checked", false);
                $("#ContentPlaceHolder1_Search4_cbBeds2").prop("checked", false);
                $("#ContentPlaceHolder1_Search4_cbBeds3").prop("checked", false);
                $("#ContentPlaceHolder1_Search4_cbBeds4").prop("checked", false);
            }
        })
        $("#ContentPlaceHolder1_Search4_cbBedsStudio").change(function (e) {
            if ($("#ContentPlaceHolder1_Search4_cbBedsStudio").is(':checked') == true) {
                $("#ContentPlaceHolder1_Search4_cbBedsAny").prop("checked", false);
            }
        })
        $("#ContentPlaceHolder1_Search4_cbBeds1").change(function (e) {
            if ($("#ContentPlaceHolder1_Search4_cbBeds1").is(':checked') == true) {
                $("#ContentPlaceHolder1_Search4_cbBedsAny").prop("checked", false);
            }
        })
        $("#ContentPlaceHolder1_Search4_cbBeds2").change(function (e) {
            if ($("#ContentPlaceHolder1_Search4_cbBeds2").is(':checked') == true) {
                $("#ContentPlaceHolder1_Search4_cbBedsAny").prop("checked", false);
            }
        })
        $("#ContentPlaceHolder1_Search4_cbBeds3").change(function (e) {
            if ($("#ContentPlaceHolder1_Search4_cbBeds3").is(':checked') == true) {
                $("#ContentPlaceHolder1_Search4_cbBedsAny").prop("checked", false);
            }
        })
        $("#ContentPlaceHolder1_Search4_cbBeds4").change(function (e) {
            if ($("#ContentPlaceHolder1_Search4_cbBeds4").is(':checked') == true) {
                $("#ContentPlaceHolder1_Search4_cbBedsAny").prop("checked", false);
            }
        })



        $("#ContentPlaceHolder1_Search4_btnSearch").click(function (e) {

            var errorMsg = "";



            if ($("#ContentPlaceHolder1_Search4_txtCity").val() == "" && $("#ContentPlaceHolder1_Search4_ddState").val() == "" && $("#ContentPlaceHolder1_Search4_txtZip").val() == "")
            {
                errorMsg = errorMsg + "Please Enter the City and State <b>OR</b> Zip Code to Continue.<br/>";
            }
            else
            {
                //They entered zip code so go with it
                if ($("#ContentPlaceHolder1_Search4_txtZip").val() != "")
                {
                    var zipCheck = /^[0-9]{5}/;
                    if (!zipCheck.test($("#ContentPlaceHolder1_Search4_txtZip").val()))
                    {
                        errorMsg = errorMsg + "The Zip Code entered is not in a valid format.<br/>";
                    }
                }
                else //No Zip so make sure there is a city and state.
                {
                    if ($("#ContentPlaceHolder1_Search4_txtCity").val() == "")
                    {
                        errorMsg = errorMsg + "City is Required.<br/>";
                    }

                    if ($("#ContentPlaceHolder1_Search4_ddState").val() == "")
                    {
                        errorMsg = errorMsg + "State is Required.<br/>";
                    }
                }
            }
         

           


            if ($("#ContentPlaceHolder1_Search4_txtRentMin").val() != "")
            {
                $("#ContentPlaceHolder1_Search4_txtRentMin").val($("#ContentPlaceHolder1_Search4_txtRentMin").val().replace("$", ""));
                $("#ContentPlaceHolder1_Search4_txtRentMin").val($("#ContentPlaceHolder1_Search4_txtRentMin").val().replace(",", ""));
            }

            if ($("#ContentPlaceHolder1_Search4_txtRentMax").val() != "")
            {
                $("#ContentPlaceHolder1_Search4_txtRentMax").val($("#ContentPlaceHolder1_Search4_txtRentMax").val().replace("$", ""));
                $("#ContentPlaceHolder1_Search4_txtRentMax").val($("#ContentPlaceHolder1_Search4_txtRentMax").val().replace(",", ""));
            }


            var max;
            var min;
            if ($("#ContentPlaceHolder1_Search4_txtRentMin").val() != "") {
                min = parseInt($("#ContentPlaceHolder1_Search4_txtRentMin").val());
                if (isNaN(min)) {

                    errorMsg = errorMsg + "Rent minimum is not a valid number.<br/>";
                }
            }
            if ($("#ContentPlaceHolder1_Search4_txtRentMax").val() != "") {
                max = parseInt($("#ContentPlaceHolder1_Search4_txtRentMax").val());
                if (isNaN(max)) {

                    errorMsg = errorMsg + "Rent maximum is not a valid number.<br/>";
                }
            }

            if (max < min)
            {
                errorMsg = errorMsg + "Rent minimum can't be greater than the Rent maximum.<br/>";
            }

            //If errors, show and prevent submit.
            if (errorMsg != "") 
            {
                $("#errorsSearch").html("<h3>Please Correct the Following Errors:</h3>" + errorMsg);
                $("#errorsSearch").addClass("alert alert-warning");
                e.preventDefault();
            }
        })
    })

</script>
