// こだわり検索入力フォームの切替処理
$( 'input[name="kodawari"]:radio' ).change( function() {  
	$('#store_station_name').hide();
	$('#airport_name').hide();
	$('#express_name').hide();

	if($(this).attr("id") == "radio-shop"){
		$('#store_station_name').show();

	} else if($(this).attr("id") == "radio-station"){
		$('#store_station_name').show();

	} else if($(this).attr("id") == "radio-airport"){
		$('#airport_name').show();

	} else if($(this).attr("id") == "radio-shinkansen"){
		$('#express_name').show();

	}

});  

	// こだわり検索
	function kodawariSearch() {
		var i;
		for (i=0; i<document.kodawarisearch.kodawari.length; i++) {
			if (document.kodawarisearch.kodawari[i].checked) {
				break;
			}
		}

		if (i == 0 || i == 1) {
			if ($('input[name="kodawaritext"]').val() == '') {
				if(i == 0) { 
					document.getElementById('kodawaritext').value = "店舗名を入力してください";
				} else if(i == 1) {
					document.getElementById('kodawaritext').value = "駅名を入力してください";
				}
			}
		} else if(i == 2) {
			if ($('input[name="kodawariAirports"]').val() == '') {
				return false;
			}
		} else if(i == 3) {
			if ($('input[name="kodawariSuperexpress"]').val() == '') {
				return false;
			}
		}

		if (i == 0) {
			location.href='https://www.2525r.com/search_detail.php?id=store&word=' + encodeURI(document.getElementById('kodawaritext').value);
		} else if (i == 1) {
			location.href='https://www.2525r.com/search_result.php?word=' + encodeURI(document.getElementById('kodawaritext').value);
		} else if (i == 2) {
			location.href='https://www.2525r.com/search_detail-Airport-' + encodeURI(document.getElementById('kodawariAirports').value) + '.php';
		} else if (i == 3) {
			location.href='https://www.2525r.com/search_result-Superexpress-' + encodeURI(document.getElementById('kodawariSuperexpress').value) + '.php';

		}

		return false;
	}

</script>


