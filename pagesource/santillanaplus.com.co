function validar_email(valor)
	{
		// creamos nuestra regla con expresiones regulares.
		var filter = /[\w-\.]{3,}@([\w-]{2,}\.)*([\w-]{2,}\.)[\w-]{2,4}/;
		// utilizamos test para comprobar si el parametro valor cumple la regla
		if(filter.test(valor))
			return true;
		else
			return false;
	}
	
function suscripcion(){
if($.trim($("#email_suscripcion").val()) == "")
		{
			alert("Ingrese un email");
		}else if(!validar_email($("#email_suscripcion").val()))
		{
		alert("El email no es valido");
		return false;
		}else
		{
			$.ajax({
					async: false,
					url: 'commons/suscripcion.php',
					type: 'post',
					beforeSend: function(){
						
					$('#cargando').show();	
					},
					data: datos,
					success: function(data){
					if (data==1){
					alert ("La operaciÛn fue realizada con exito. Revisa tu correo electronico para terminar con tu proceso de suscripcion.");
					window.location.reload();	
					}else{
						$('#cargando').hide();
					alert ("Ocurrio un error. "+data+". Initenta de nuevo. Si el error persiste contacte al equipo de soporte.");	
					}
					
					},
					error: function(jqXHR, textStatus, errorThrown){
					$('#cargando').hide();
					alert ("Error Ajax. Intenta de nuevo. Si el error persiste contacta al equipo de soporte.");
					}
					
				});
		}	
	
	
}

function cargarLibros(){
		
$.ajax({
			type:'POST',
			url:'commons/libros_por_activar.php',
			beforeSend:function(){
				busqueda_icon('libros_por_activar');
			},
			dataType:'json',
            // utilizamos la funciÛn post, para hacer una llamada AJAX
            success: function(libros_array) {

              
                // lo vaciamos
                $("#libros_por_activar").empty();
   $("#libros_por_activar").append("<option value='-1'>Seleccione</option>");
                // iteramos a trav√©s del arreglo de paises encontrados 
                $.each(libros_array, function(index, libro) {

                    // agregamos opciones al combo
					
                     $("#libros_por_activar").append("<option value='"+ libro.id +"'>"+ libro.name  +"</option>");
					
                });
			
				 
				 limpiar_icon('libros_por_activar');

				
            }, 
			error:function(jqXHR, textStatus, errorThrown){alert ("Error al intentar obtener listados de Libros por Activar. "+jqXHR+ textStatus+ errorThrown)}
			
			});	
	
	
}


	


