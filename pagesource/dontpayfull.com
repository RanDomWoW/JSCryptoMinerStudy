<script type="text/javascript">
$(document).ready(function () {
var $container = $('.viewstore__products-wrap');
var $loading = $('.loading');
new Clipboard('.partial-code.seen-code');
$('.coupon-box').couponbtn();
$('#related-blog').owlCarousel({
center: false,
items: 6,
loop: true,
margin: 10,
dots: 0,
stagePadding: 40,
responsive: {
0: {
items: 2,
nav: false
},
600: {
items: 2
},
798: {
items: 3
},
998: {
items: 3
},
1200: {
items: 5
},
1780: {
items: 6
},
2070: {
items: 7
},
2270: {
items: 8
}
}
});
var pageLoading = 0;
var isFinished = 0;
var page = 2;
$loading.show();
$container.imagesLoaded(function () {
$('.viewstore__product').show();
$container.masonry({
transitionDuration: 0,
itemSelector: '.viewstore__product',
gutter: 15,
isResizable: true
});
$('.viewstore__product').css({'visibility': 'visible'}).animate({opacity: '1'}, {
step: function (now, fx) {
$(this).css('-webkit-transform', 'scale(' + now + ')');
}
}, 300);
$loading.hide();
});
setInterval(function () {
$container.masonry();
$('.viewstore__product').css('-webkit-transform', 'scale(1)');
}, 500);
$(window).resize(function () {
$container.masonry();
$('.viewstore__product').css('-webkit-transform', 'scale(1)');
});
$(window).scroll(function () {
if ($('.popular-stores').isOnScreen()) {
if (!isFinished && !$('.search-response').is(':visible') && $('.viewstore__product').length % 10 == 0) {
if (!pageLoading) {
pageLoading = 1;
$.ajax({
url: '/site/lazyproducts',
method: 'get',
data: {page: page, limit: 10},
beforeSend: function () {
$loading.show();
},
success: function (data) {
var $data = $(data);
$container.append($data).masonry('addItems', $data);
$container.find('.viewstore__product').show();
$container.imagesLoaded(function () {
$loading.hide();
$container.masonry();
page += 1;
if ($data.length < 10) {
isFinished = 1;
}
pageLoading = 0;
$('.viewstore__product').css({'visibility': 'visible'}).animate({opacity: '1'}, {
step: function (now, fx) {
$(this).css('-webkit-transform', 'scale(' + now + ')');
}
}, 300);
});
}
});
}
}
}
});
$.fn.isOnScreen = function () {
var viewport = {};
viewport.top = $(window).scrollTop();
viewport.bottom = viewport.top + $(window).height();
var bounds = {};
bounds.top = this.offset().top;
bounds.bottom = bounds.top + this.outerHeight();
return ((bounds.top <= viewport.bottom) && (bounds.bottom >= viewport.top));
};
});
</script>
