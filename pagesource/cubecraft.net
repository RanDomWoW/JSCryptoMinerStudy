<script>

////////////////// SEARCH BAR

$('#QuickSearch input.button.primary.BRPL_showHide').click(function(){
	
	if($('#QuickSearch.active').length)
	{
		$('#QuickSearch .secondaryControls').css('display','none');
	}
	
		if($('#QuickSearchQuery').attr('value') == '')
		{
			if(!$('#QuickSearch .formPopup .primaryControls input.textCtrl.BRPL_enableSubNav').length && $(window).width() < 480)
			{
			}
			else
			{
				$('#QuickSearch .formPopup .primaryControls input.textCtrl').animate({width: "toggle"}, 200);
			}
			$(this).closest('#searchBar').toggleClass('BRPL_Responsive');
			return false;
		}else
		{
			$('#searchBar .formPopup').submit();
		}	
});

$('#QuickReply').find('.redactor_box').addClass('aaa');
//////////////////// page nav link group

$exits = $('.pageNavLinkGroup').next('form.section').next('.pageNavLinkGroup');

if($($exits).length)
{
	$('.pageNavLinkGroup').addClass('titleNext');
	$('form.section').addClass('BRPL_style');
	$($exits).addClass('BRPL_pageNavLinkGroup_2');
}

if($('.titleBar').next('.pageNavLinkGroup').length)
{
	$('.titleBar').addClass('BRPL_style');
	$('.titleBar').next('.pageNavLinkGroup').addClass('titleNext');
}

if($('.forum_view .titleBar').next('.nodeList.sectionMain'))
{
	$('.forum_view .titleBar').addClass('BRPL_style');
	$('.forum_view .titleBar').next('.nodeList.sectionMain').addClass('titleNext');
}
	
$('.pageNavLinkGroup').each(function( key, value ) {

	$linkGroup = $(this).find('.linkGroup');
	$pageNav = $(this).find('.PageNav');
	var linkGroupNone = $($linkGroup).css("display");
	
	if(!$(this).find('.linkGroup').children().length)
	{
		$(this).find('.linkGroup').remove();
	}
	
	if($($linkGroup).length || $($pageNav).length)
	{
		$(this).addClass('BRPL_style');
		$(this).removeClass('none');
	}
	
	if(linkGroupNone == 'none' && !$(this).children('.PageNav').length)
	{
		$(this).addClass('none');
		$(this).removeClass('BRPL_style');
	}
	
	if(!$(this).children().length)
	{
		$(this).addClass('none');
		$(this).removeClass('BRPL_style');
	}
});

$('.PageNav').each(function(){
	if(!$(this).parent().hasClass('pageNavLinkGroup'))
	{
		$(this).wrap('<div class="pageNavLinkGroup"></div>');
	}
});

</script>





