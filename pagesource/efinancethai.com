        var ConfirmPwd = jQuery('#uc_Confirm_password').val();

        if (OldPwd == "" || OldPwd.length < 4) {
            jQuery("#ChgPwdMessage").text("Please specify the correct current password.");
            return false;
        }
        if (NewPwd == "" || NewPwd.length < 4) {
            jQuery("#ChgPwdMessage").text("Your new password must be at least 4 characters.");
            return false;
        }
        if (ConfirmPwd == "" || ConfirmPwd.length < 4) {
