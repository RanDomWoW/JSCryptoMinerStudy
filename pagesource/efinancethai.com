            return false;
        }
        if (NewPwd.length > 16) {
            jQuery("#ChgPwdMessage").text("Your new password must not be more than 16 Characters.");
            return false;
        }
        if (ConfirmPwd.length > 16) {
            jQuery("#ChgPwdMessage").text("Your confirm password must not be more than 16 Characters.");
            return false;
        }

        if (NewPwd != ConfirmPwd) {

            jQuery("#ChgPwdMessage").text("Your new password entries did not match.");
            return false;
        }

        var sendInfo = {
            UserId: UserId,
            OldPwd: OldPwd,
