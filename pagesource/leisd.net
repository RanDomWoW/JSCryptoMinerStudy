                <script>
                function outerDivCenter() {
                    var foo = $("#outerDiv");
                    foo.css("left", Math.max(0, (($(window).width() - $(foo).outerWidth()) / 2) + $(window).scrollLeft()) + "px");
                    return this;
                }

                $(document).ready(function() {
                    var IOS = '';
                    if (!IOS) {
                        $(document).mouseup(function (e) {
                            var container = $('#outerDiv');
                            if (!container.is(e.target) // if the target of the click isn't the container...
                                && container.has(e.target).length === 0) {// ... nor a descendant of the container
                                if ($("#outerDiv").is(":visible")) {
                                    $("#outerDiv").toggle( "drop",{direction: "right"}, 1000 );
                                }
                            }
                        });

                        $(window).on('resize', function() {
                            outerDivCenter();
                        });

                        $('#downArrow').click(function() {
                            outerDivCenter();

                            if (!$("#outerDiv").is(":visible")) {
                                $("#outerDiv").toggle( "drop",{direction: "left"}, 600 );
                            }

                            heightOfPage = $('body').outerHeight();
                            var howWide = $('#howWide').val();
                            widthOfInner = howWide * 185;
                            $('#innerDiv').css("width", widthOfInner);
                            centerOfInner = widthOfInner / 2;
                            var centerpoint = 487 - centerOfInner;
                            $('#innerDiv').css("margin-left", centerpoint);
                            var insideHeight = $('#innerDiv').outerHeight();

                            $('#outerDiv').css("-webkit-box-shadow", "45px -57px 0px "+heightOfPage+"px rgba(51,51,51,0.55)");
                            $('#outerDiv').css("-moz-box-shadow", "45px -57px 0px "+heightOfPage+"px rgba(51,51,51,0.55)");
                            $('#outerDiv').css("box-shadow", "45px -57px 0px "+heightOfPage+"px rgba(51,51,51,0.55)");
                        });
                    } else {
                        //start mobile device
                        $('#outerDiv').css('display','none');

                        $('#downArrow').click(function() {
                            $('#outerDiv').toggle('blind');
                            var howWide = $('#howWide').val();
                            widthOfInner = howWide * 185;
                            $('#innerDiv').css("width", widthOfInner);
                            centerOfInner = widthOfInner / 2;
                            var centerpoint = 487 - centerOfInner;
                            $('#innerDiv').css("margin-left", centerpoint);
                            var insideHeight = $('#innerDiv').outerHeight();
                            $('#outerDiv').css("height", insideHeight);
                        }); //downArrow click function

                    }
                });
