<script type="text/javascript">
	var frm = $('#reviewForm');
	frm.submit(function (ev) {
		if ($("#form-request-event").val().length < 3) {
			alert('Пожалуйста, введите мероприятие');
			return false;
		}			
		if ($("#form-request-contact").val().length < 2) {
			alert('Пожалуйста, введите контактное лицо');
			return false;
		}
		if ($("#form-request-phone").val().length < 2) {
			alert('Пожалуйста, введите телефон');
			return false;
		}
		if ($("#form-request-email").val().length < 6) {
			alert('Пожалуйста, введите email');
			return false;
		}
	    $.ajax({
	        type: frm.attr('method'),
	        url: frm.attr('action'),
	        data: frm.serialize(),
	        success: function (data) {
				result = JSON.parse(data);
				if (result.result == 'success') {
					alert('Спасибо, ваш запрос отправлен!');
				} else {
					alert('Произошла ошибка. Пожалуйста, попытайтесь позже!');
				}
				$("#form-request-event").val('');
				$("#form-request-contact").val('');
				$("#form-request-phone").val('');
				$("#form-request-email").val('');
				$("#form-request-submit").removeAttr('disabled');
			},
			error: function (xhr, str) {
				alert("Возникла ошибка!");
				$("#form-request-submit").removeAttr('disabled');
			}
		});

	    ev.preventDefault();
	});
</script>
