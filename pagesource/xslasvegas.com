<script type='text/javascript'>
var nload=12;
var startflag=0;

jQuery('.uv-content').scroll(function()
{	
	var temcontent = jQuery('.uv-content').get(0).scrollHeight-jQuery('.uv-content').height();
    //if(jQuery('.uv-content').scrollTop()==jQuery('.uv-content').get(0).scrollHeight-jQuery('.uv-content').height())
    if(jQuery('.uv-content').scrollTop()-temcontent<2 && jQuery('.uv-content').scrollTop()-temcontent>-2)
    {
    	var end, start=jQuery('.loadonscroll-item').last().data('eq');  	
    	if(start>startflag)
    	{
    		start=parseInt(start)+1;
    		end=parseInt(start)+parseInt(nload);
	
    		jQuery.ajax(
    		{
				dataType: 'html',
				charset: 'UTF-8',
				url: 'http://xslasvegas.com/features/features.load.html',
				type: 'post',
				data: 'start='+start+'&end='+end+'&djid=',
				success:
				function(response)
				{
					jQuery('.loadonscroll-loader').remove();
					jQuery('.loadonscroll').append(response);
				}
			});
			startflag=start;
		}
    }
});
</script>
