<script>
(function($){
  var slideIndex = 0;
  showDivs(slideIndex);
  carousel();
  function carousel() {
    var i;
    var x = $(".mySlides");
    var dots = $(".item");
    var itm_thumb = $(".itm_thumb");
    for (i = 0; i < x.length; i++) {
       x.eq(i).hide();
    }
    for (i = 0; i < dots.length; i++) {
        dots.eq(i).removeClass("w3-opacity-off active-thumb");
        x.eq(i).removeClass("slider-is-active");
    }
    slideIndex++;
    if (slideIndex > x.length) {slideIndex = 1}
    x.eq(slideIndex-1).show();
    x.eq(slideIndex-1).addClass("slider-is-active");
    dots.eq(slideIndex-1).addClass("w3-opacity-off active-thumb");

    item_height = 129;
    if(slideIndex < 9 && slideIndex!= 1 && slideIndex!= 10){
      itm_thumb.css('top','-='+item_height);
    }
    if(slideIndex==1)
    {
      itm_thumb.attr('style','top:0;');
    }
    setTimeout(carousel, 10000); // Change image every 4 seconds
  }
  function plusDivs(n) {
    showDivs(slideIndex += n);
    var itm_thumb = $(".itm_thumb");
    var top = (parseInt(slideIndex)-1)*129;
    /*console.log(slideIndex);
    console.log(top);*/
    if(slideIndex < 9 && slideIndex!= 1 && slideIndex!= 10){
      //itm_thumb.attr('style','top:-'+top+'px');
      itm_thumb.css('top','-'+top+'px');
    }
    if(slideIndex==1)
    {
      itm_thumb.attr('style','top:0;');
    }
  }
  function currentDiv(n) {
    showDivs(slideIndex = n);
  }
  function showDivs(n) {
    var i;
    var x = $(".mySlides");
    var dots = $(".item");

    if (n > x.length) {slideIndex = 1}
    if (n < 1) {slideIndex = x.length}
    for (i = 0; i < x.length; i++) {
       x.eq(i).hide();
    }
    for (i = 0; i < dots.length; i++) {
       dots.eq(i).removeClass(" w3-opacity-off active-thumb");
    }
    x.eq(slideIndex-1).show();
    dots.eq(slideIndex-1).addClass("w3-opacity-off active-thumb");
  }
  $('.plus_div_next').click(function(){
    plusDivs(1);
  });
  $('.plus_div_prev').click(function(){
    plusDivs(-1);
  });
  $(".item").click( function(event)
  {
      n = $(this).parent().parent().index();
      currentDiv(n+1);
  });
  setInterval(function(){
      var clas = $('.slider_image').parent();
      if (clas.hasClass("slider-is-active")) {
        $(this).addClass('mehdi');
      }
  }, 1000);
})(jQuery);
</script>
