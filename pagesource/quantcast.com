<section id="" class="qc-page-section-wrapper qc-page-custom-html-wrapper flex-order-20 custom-html "  >
  <div class="section-inside qc-page-section-inside">
    <div class="container">
            <div class="wysiwyg-container--qc_page_section wysiwyg-container--mta_master_plugin"> <style type='text/css' media='screen'>
.slick-slider .slick-list {padding-bottom:1rem;}
[data-hl-swapt] {
  display: block;
  text-align: center;
  position: absolute;
  width: 100%;
  opacity: 0;
}
.qc-page-section-wrapper.bs-padding-3x .qc-page-section-inside > .container {padding-left:30px; padding-right:30px;}

@media all and (max-width:991px) {
  .slick-slider .slick-list .carousel-text-col__inside {margin-top:2rem;}
  .slick-slider .slick-list figure {display:inline-block;margin:0 auto;}
  body {
    padding-top: 52px;
  }
}
</style>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<script type="text/javascript">
$(function(){
  var $swap_headlines = $(".headline-item--qc_hero").find("[data-hl-swapt]");
  var $hero_container = $(".qc-hero-inside");
  var viewportTop = 0;
  //settings
  var transitionTime = 0.75;
  var slogansDelayTime = 5;

  //internal
  var totalSlogans = $swap_headlines.length;

  var oldSlogan = 0;
  var currentSlogan = -1;
  var oldHeight = 0;
  var newHeight = 0;
  var heroBtn = $('.hero-content-item--qc_hero .btn');
  var origButtonTxt = $('.hero-content-item--qc_hero .btn').html();
  var origButtonUrl = $('.hero-content-item--qc_hero .btn').attr('href');
  var oldButtonClass = '';
  var newButtonClass = '';
  var newBtnTxt = '';
  var newBtnUrl = '';

  //initialize
  switchSlogan();

  function switchSlogan(){
    viewportTop = $(window).scrollTop();

    if( viewportTop < $hero_container.offset().top ) {
      oldSlogan = currentSlogan;

      if(currentSlogan < totalSlogans-1){
        currentSlogan ++
      } else {
        currentSlogan = 0;
      }

      oldHeight = $swap_headlines.eq(oldSlogan).outerHeight();
      newHeight = $swap_headlines.eq(currentSlogan).outerHeight();
      newBtnTxt = $swap_headlines.eq(currentSlogan).attr('data-hl-swap-btn-txt') || origButtonTxt;
      newBtnUrl = $swap_headlines.eq(currentSlogan).attr('data-hl-swap-btn-url') || origButtonUrl;
      oldBtnClass = $swap_headlines.eq(oldSlogan).attr('data-hl-swap-btn-class') || '';
      newBtnClass = $swap_headlines.eq(currentSlogan).attr('data-hl-swap-btn-class') || '';

      TweenLite.to($swap_headlines.eq(oldSlogan), transitionTime,
      {
        className:'-=active-item',
        top:-20,
        alpha:0,
        rotationX:90,
        onStart:function(){
          $('.column-wrapper--qc_hero .headline-item--qc_hero').height(newHeight+'px');
          heroBtn
            .html(newBtnTxt)
            .attr('href', newBtnUrl)
            .parent()
              .addClass(newBtnClass)
              .removeClass(oldBtnClass);
        }
      });
      TweenLite.fromTo($swap_headlines.eq(currentSlogan), transitionTime,
        {
          className:'+=active-item',
          top:20,
          alpha:0,
          rotationX: -90,
        },
        {
          top:0, alpha:1, rotationX:0
        }
      );
    }
    else {
      // dont rotate the headline, the use has started scrolling past it
    }
    
    TweenLite.delayedCall(slogansDelayTime, switchSlogan);
  }
});
</script> </div>    </div>
  </div>
</section>

