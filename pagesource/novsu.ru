<script type="text/javascript">
var slider_position=1;
var slider_total=9;
var slider_auto=true;

$(window).resize(function() {
	$(".slider_element").each(function( index ) {
		$(this).css('left',(parseInt($(this).attr('id').substring(7,99))*$('#slider').width())+'px');
		});
	slider_position=1;
	$('#slider_moving_container').css({left: -((slider_position-1)*$('#slider').width())+'px'});

	});

$(".slider_element").each(function( index ) {
	$(this).css('left',(parseInt($(this).attr('id').substring(7,99))*$('#slider').width())+'px');
	});

$('#slider_right').click(function() {
	slider_auto=false;
	slider_position++;
	if (slider_position>slider_total) slider_position=1;
	$('#slider_moving_container').animate({left: -((slider_position-1)*$('#slider').width())+'px'});
	return false;
	});

$('#slider_left').click(function() {
	slider_auto=false;
	slider_position--;
	if (slider_position<1) slider_position=slider_total;
	$('#slider_moving_container').animate({left: -((slider_position-1)*$('#slider').width())+'px'});
	return false;
	});

function slider_auto_run() {
	setTimeout(function() {
		if (!slider_auto) return;
		slider_position++;
		if (slider_position>slider_total) slider_position=1;
		$('#slider_moving_container').animate({left: -((slider_position-1)*$('#slider').width())+'px'});
		slider_auto_run();
		}, 10000);
	};

slider_auto_run();

</script>

