        <script>
            /*
             * 这段代码为什么放在这？<br>
             * 因为自有渠道是从搜索引擎过来的，所以移动端会从这里跳转到/m/index.php，<br>
             * 应该在跳转前就设置好来源cookie，以标识来源。
             * @authro 李俊俊  <rockxsj@gmail.com>
             */
                
                
            function setTg() {
                var arr = new Array('baidu.com', 'google.com', 'soso.com',
                    'youdao.com', 'sogou.com', 'bing.com', 'yahoo.com',
                    'my.2345.com', 'www.2345.com', 'jifen.2345.com');
                var scFrom = new Array('tieba.baidu.com', 'zhidao.baidu.com', 'baike.baidu.com');
                var ref = '';
                if (document.referrer.length > 0) {
                    ref = document.referrer;
                }
                try {
                    if (ref.length == 0 && opener.location.href.length > 0) {
                        ref = opener.location.href;
                    }
                } catch (e) {
                }
                if (ref.length > 0 && checkCookie('my_tg').length <= 0) {
                    var date = new Date();
                    var expireDays = 10;
                    date.setTime(date.getTime() + expireDays * 24 * 3600 * 1000);
                    for (x in scFrom) {
                        var re = new RegExp(scFrom[x], "gi");
                        if (re.test(ref)) {
                            document.cookie = "my_tg=sc;expires=" + date.toGMTString() + ";path=/;domain=shouji.2345.cn";
                            return ref;
                        }
                    }
                    for (x in arr) {
                        var re = new RegExp(arr[x], "gi");
                        if (re.test(ref)) {
                            document.cookie = "my_tg=zy|zy;expires=" + date.toGMTString() + ";path=/;domain=shouji.2345.cn";
                            return ref;
                        }
                    }
                }
                return ref;
            }


            function invite() {
                var invate_code = getInvateCode();
                if (invate_code.length > 0) {
                    var date = new Date();
                    var expireDays = 10;
                    date.setTime(date.getTime() + expireDays * 24 * 3600 * 1000);
                    document.cookie = "invite_code=" + invate_code + ";expires=" + date.toGMTString() + ";path=/;domain=shouji.2345.cn";
                }
            }

            function getInvateCode() {
                var url = location.search;
                var patrn = /^[a-z\d]{1,20}$/;
                if (url.indexOf("?") != -1) {
                    var spos = url.indexOf("&");
                    if (spos != -1) {
                            url = url.substr(1, spos-1);
                    } else {
                            url = url.substr(1);
                    }
                    var res = patrn.test(url);
                    if (res) {
                        return url;
                    }
                }
                return '';
            }

            (function() {
                setTg();
                invite();
                // com跳转到cn,暂时要求只对首页生效
                var locationHref = window.location.href;
                if (locationHref.indexOf('2345.com') > 0)
                {
                    window.location.href = locationHref.replace("2345.com", "2345.cn");
                }
                if (locationHref.indexOf('yd126.com') > 0)
                {
                    window.location.href = "http://www.yd126.com/yd126/";
                }
            })();
        </script>
