<script language="javascript">
$(function(){
    $(".btn_close").bind("click",function(){
        $(this).closest(".down_box").fadeOut()
        $(this).closest(".down_box").addClass("noshow")
    })
})

function showWxcode(){
	jQuery('.wxcode').css("display","block");
}
function changeBanner()
{
	var now = commonCookie.getTimeStamp();

    var myDate = new Date();
    myDate.setDate(myDate.getDate()+1);
    var month = myDate.getMonth()+1;//获得当前月  月份是从0开始
    var nextDate = myDate.getFullYear()+'-'+month+'-'+myDate.getDate();
    var date1= new Date();  //开始时间
    var date2= new Date(Date.parse(nextDate.replace(/-/g, '/')));    //结束时间
    var timeStamp=(Date.parse(date2)-Date.parse(date1))/1000;  //时间差的秒数

	commonCookie.set("J_trig_close", {e:now + timeStamp });
	$('#J_ipo_banner').animate({height: 'hide', opacity: 'hide'}, 1500, function(){ $('#J_ipo_banner').hide();});
	$('#J_ipo_banner_close').show();
	$('#back_top').css('bottom', '200px');
}

function topBanner() {
    var bannerOpt = '';
            bannerOpt = {bigImgSrc:'url(http://shouji.2345.cn/upload/media/201803/jpg-pXw8XNz3qx183050.jpg)',
        smallImgSrc:'url(http://shouji.2345.cn/upload/media/201803/gif-HiuKDGnZLn183050.gif)',
        closeImgSrc:'url(http://shouji.2345.cn/shoujiimg/2014/img/tps/top_banner_close2.png?v=20151028)',
        topBannerPack:'#dc2629',
        topBannerExpand:'#dc2629',
        linkUrl:'http://shoujibbs.2345.cn/thread.php?fid=15&pid=3967461'};
        
    $("#J_ipo_banner").css('backgroundColor',bannerOpt.topBannerExpand);
	$("#J_ipo_banner_close").css('backgroundColor',bannerOpt.topBannerPack);
	$("#J_ipo_banner .top-banner-in").css('backgroundImage',bannerOpt.bigImgSrc);
	$("#J_ipo_banner_close .top-banner-in").css('backgroundImage',bannerOpt.smallImgSrc);
	$("#J_trig_close").css('backgroundImage',bannerOpt.closeImgSrc);
    $(".top-banner-in a").attr('href', bannerOpt.linkUrl);

    if(bannerOpt == '')
    {
        $('#J_ipo_banner_close').css('display', 'none');
    }
    else
    {
        if(!commonCookie.get("J_trig_close"))
        {
            $('#back_top').css('bottom', '10px');
                            $('#J_ipo_banner').css("display","block");
                setTimeout('changeBanner()', 10000);
                    }
        else
        {
            $('#J_ipo_banner_close').css("display","block");
        }
    }

	$("#J_trig_close").click(function(){
		changeBanner();
	});
}

jQuery(document).ready(function(){
		topBanner();
	//上市活动  2014-10-16 00:00:00到2014-10-18 00:00:00
	
	if(!commonCookie.get("credit_tip")){
		jQuery('#ui-poptip').css("display","block");
	}
	if(!jQuery.cookie('app_tip_close')){
		jQuery('.wxcode').css("display","block");
	}
    jQuery('.viewwx').bind('mouseover',function(){
    	t=setTimeout("showWxcode()",500);
      })
    jQuery('.viewwx').bind('mouseout',function(){
    	clearTimeout(t);
      })
      jQuery('#back_top .closer').bind('click',function(){
        jQuery('.wxcode').css("display","none");
          jQuery.cookie('app_tip_close', '1', { expires: 7 });
      })
      if(jQuery(window).width()<1024){
        jQuery('.wxcode').css("display","none");
      }
      jQuery(window).bind('resize',function(){
          if(jQuery(window).width()<1024){
          jQuery('.wxcode').css("display","none");
      }
    })

    if( jQuery.browser.msie && jQuery.browser.version == '6.0' ){
        var toTop = jQuery(document).scrollTop()+jQuery(window).height()-jQuery('#back_top').height()-100;
        jQuery('#back_top').css('top',toTop);
      }

      jQuery(window).scroll(function(){
    	  var toTop = jQuery(document).scrollTop();
          if( jQuery.browser.msie && jQuery.browser.version == '6.0' ){
            toTop = jQuery(document).scrollTop()+jQuery(window).height()-jQuery('#back_top').height()-100;
            jQuery('#back_top').css('top',toTop);
          }
                if(toTop >= 500){
                    jQuery('#back_top .goback').css("display","block");
                }else{
                    jQuery('#back_top .goback').css("display","none");
                }
            });
		var compressing = 0;
		$("#downpack").click(function(){
			if(!isLogin()){
				return ;
			}
			//按钮不可点
			if(compressing == 1) {
				alert("请不要重复点击");
				return;
			}
			compressing = 1;
			//ajax
			$.post('/jifen/getcompress.php',{},function(data){
				data = data.split("|");
				if(data[0] == 'no')
				{
					location.href=data[1];
					compressing = 0;
				}
				else if(data[0] == 'compressing')
				{
					alert("正在压缩中，请稍后再试");
					compressing = 0;
					return;
				}
				else if(data[0] == 'yes')
				{
					var count = 30;
					var countdown = setInterval(function(){
					str = '<span class="fred">'+count+'</span> 秒后自动下载';
					$("#gather").html(str);
					if (count == 0) {
						$.post('/jifen/getcompress.php',{},function(data1){
							data1 = data1.split("|");
							if(data1[0] == 'no')
							{
								location.href=data1[1];
								compressing = 0;
								str = '<strong><img  src="http://shouji.2345.cn/shoujiimg/2014/img/index/ico_gather.png" alt=""> <a href="javascript:;"><span class="fblue" id="downpack">下载手机应用压缩包</span><i class="ico_new"></i></a></strong><span class="f12">（包含所有推广软件）</span>';
								$("#gather").html(str);
							}
							else
							{
								alert("正在压缩中，请稍后再试");
								window.reload();
							}
						});
						window.clearInterval(countdown);
					}
					count--;
				}, 1000);
				}
			});
		});

	if (!$.cookie("task_notice"))
	{
		$("#task_notice_point").show();
	}
		
});

//此段JS执行一些依赖footer中加载的JS库的方法
//waitFunctions 为全局JS变量必须为一个数组
//add by wangwei
(function(){
	if(typeof waitFunctions === 'object')
	{
		for(var key in waitFunctions)
		{
			var functionBody = waitFunctions[key];
			if(typeof functionBody === 'function')
			{
				var args = functionBody.myArgs;
				if(typeof args === 'object')
				{
					try{
						functionBody.call(this, args);
					}catch(e){}
				}
				else
				{
					try{
						functionBody.call(this);
					}catch(e){}
				}
			}
		}
	}

})();

