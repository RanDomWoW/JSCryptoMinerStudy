				openItem.siblings('h3').children('i').removeClass().addClass('fa fa-plus');
			} else {
			}

			// 現在のアイコン状態で＋にするか−にするか判定
			if($(this).children('i').is('.fa-minus')){

			  // ＋アイコンに変更
			  $(this).children('i')
				.removeClass()
				.addClass('fa fa-plus');

			}else{

			  // −アイコンに変更
			  $(this).children('i')
				.removeClass()
				.addClass('fa fa-minus');
			}

		});
});
