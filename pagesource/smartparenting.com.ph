            $(document).on('click', '.sns-hex', function(event) {
                     $(this).parent().parent().parent().toggleClass('feed-share-active');
            });
            $(document).on('click', '.sns-cancel', function(event) {
                    $(this).parent().parent().parent().toggleClass('feed-share-active');
                    if (!$('.sns-feed').parent().parent().parent().hasClass('feed-share-active')) {
                        $('.sns-content').toggleClass('sns-shareitem-close');
                    }
                    $('.sns-feed').parent().parent().parent().parent().removeClass('feed-share-active');
                });

            if($('img.lazy').length) {
                $('img.lazy').lazyload({threshold:300, effect:'fadeIn'}).removeClass('lazy');
            }

            console.log('leaderboard ' + $('.leaderboard-bottom').height());
            // if($('.stripAd').height() == 0){
            //     $('.stripAd').hide();
            // }else

            //$('.stripAd').css({'display':'flex'});

            if($('.leaderboard').height() == 0){
                $('.leaderboard').hide();
            }else if($('.leaderboard-bottom').height() == 0){
                $('.leaderboard-bottom').hide();
            }

            $(window).scroll(function () {
                if($(".article-story")[0]){
                    if ($(window).scrollTop() >= $(".article-viewmorestories").position().top) {
                        show_articleomt();
                    }
                }

                $('.widget-weRecoInsert').each(function(i,n){
                    $(n).attr('id','count' + (i + 1));
                });

                $('.widget-weRecoInsert').map(function() {
                    if($(window).scrollTop() >= ( $(this).offset().top - 600)){
                        var cnt = $(this).attr('id');
                        $('#'+cnt).addClass("widget-weRecoInsert-expand");
                        $('#'+cnt).removeClass("widget-weRecoInsert-hide");
                    }
                });
            });

            function show_articleomt(){
                show_articleomt = Function("");
                $( ".widget-readnext" ).toggleClass('widget-readnext-expand');
                $( ".widget-readnext" ).toggleClass('gtm-readnext');
            }

            $('.close-readnext').click(function(){
                $( ".widget-readnext" ).toggleClass('widget-readnext-expand');
                $( ".widget-readnext" ).toggleClass('gtm-readnext');
                return false;
            });

            if($('.skinning').length){
                $('header').attr('style', 'margin:0 auto;position:relative;width:978px;');
            }

        });

        if(($.inArray(SEGMENT, excluded_channels) == -1) && SLUG !=""){
            sd_segment = 'Article';
        }

        //NOTE: hardcoded because of server issues
        
        var header, headerY = 0;
        (function() {
            if($('header').length > 1){
                header = document.getElementsByTagName('header');

                if (header !== undefined && header.length > 0) {
                    header  = header[0];
                    headerY = document.querySelector('.billboard').offsetHeight;
                }
            }
        })();

        var oldY = 0;
        document.addEventListener('scroll', function() {
            var y = 0;

            if( typeof( window.pageYOffset ) == 'number' ) {
                y = window.pageYOffset;
            } else if( document.body && ( document.body.scrollLeft || document.body.scrollTop ) ) {
                y = document.body.scrollTop;
            } else if( document.documentElement && ( document.documentElement.scrollLeft || document.documentElement.scrollTop ) ) {
                y = document.documentElement.scrollTop;
            }

            $('header').attr('style', 'z-index:1000;');

            if (y > headerY) {
                if($('header').length > 1){
                    header.classList.add('fixed');
                }
                $('header').css({'position':'absolute'});
                $('#main').css({'padding-top':'93px'});
                $('#final-menu').css({'position':'relative','top':'0px'});
            } else {
                if($('header').length > 1){
                    header.classList.remove('fixed');
                }
                if($('.skinning').length){
                    $('header').attr('style', 'margin:0 auto;position:relative;width:978px;');
                } else{
                    $('header').attr('style', 'position:relative');
                }
                $('#main').removeAttr("style");
                $('#final-menu').css({'position':'absolute','top':'initial'});
            }
        });
