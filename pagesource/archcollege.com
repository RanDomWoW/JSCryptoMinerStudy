<script>
    $(".package_fltbtn .close").click(function(e){ e.stopPropagation(); $(".package_fltbtn").hide() });
    function more_posts(callback){
        var d = $("#last_day").val();
        var post_time = $("#post_time").val();
        $.ajax({
            url:"/?d="+d+"&post_time="+post_time,
            type:"get",
            dataType:"html",
            success:function(data,text){
                $("#last_day").val($("#tmp_last_day",data).val());
                $("#post_time").val($("#tmp_post_time",data).val());
                var ajaxItems = $(".ajax-month-item",data);
                if(ajaxItems.length>1){
                    ajaxItems.appendTo(".month-item-list");
                    $("img.lazy",ajaxItems).lazyload({
                        skip_invisible:true,
                        placeholder:"/images/lazy_default.jpg",
                        effect : "fadeIn",
                        threshold : 100
                    }).load(function(){
                        if( $(this).parents("li").eq(0).hasClass("tvs_item") ){
                            if( this.src.indexOf("lazy_default") < 0 ){
                                var src = this.src;
                                $(this).parent().css("background-image","url("+src+")");
                                $(this).hide();
                            }
                        }
                    })
                }

                callback&&callback();
            }
        });
    }

    //阅读更多;
    $('#more_list').click(function(){
        var This = $(this);
        This.addClass('loading_more');
        more_posts(function(){
            This.removeClass('loading_more');
        });
    });

    //固定侧边栏
    scroll_fixed($(".fixed_wrp"),$(".lt_main"));
    //banner轮播
    (function(){
        var baseSlider = $(".slider").clone(true, true);
        var defaultIndex = 0;
        var randI;

        function bannerSlide(){
            //模拟暂停定时器
            $(".slider").trigger('mouseover');
            //判断唯一
            randI = Math.random();
            //保证每次都是新的 slider 不然会叠加
            sliderClone = $(baseSlider).clone(true, true);
            sliderClone.attr("data-i",randI);
            $(".slider").replaceWith(sliderClone);

            jQuery(".slider").slide({
                titCell: ".hd ul",
                mainCell: ".bd ul",
                effect: "leftLoop",
                autoPlay: true,
                vis: 3,
                playStateCell:".playState",
                autoPage: true,
                trigger: "click",
                //接着上次的  index
                defaultIndex:defaultIndex,
                interTime:4000,
                endFun:function(i,length,oEl){
                    if(oEl.attr("data-i")!=randI)return;
                    defaultIndex = i;
                    $(".slider .bd li").find("span").hide();
                    $(".slider .list_"+i).find("span").show();
                }
            });

            var zb = {};
            //模拟滑动 轮播
            $(".slider").on({
                touchstart:function(e){
                    var e = e.originalEvent;
                    var target = e.touches[0];
                    zb.xMove = zb.xStart = target.clientX;
                    zb.yMove = zb.yStart = target.clientY;
                    zb.btn = false;
                    $(".slider").trigger('mouseover');
                },
                touchmove:function(e){
                    var e = e.originalEvent;
                    var target = e.touches[0];
                    zb.xMove = target.clientX;
                    zb.yMove = target.clientY;
                    if(  Math.abs( zb.xMove - zb.xStart ) > Math.abs( zb.yMove - zb.yStart ) ){
                        //是否阻止默认
                        if(!zb.btn)$(".slider").trigger('mouseout');
                        e.preventDefault();
                        zb.btn = true;
                    }
                },
                touchend:function(){
