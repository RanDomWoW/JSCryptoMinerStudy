<script>
	var setMenuImages = function(SC, render) {
		var request = JSON.stringify(SC, null, '  ');
		var sections = ["sentarse","recamara","almacenar","mesas","iluminacion","accesorios"];
		var items = [];

		request = jQuery.parseJSON( request );
		var products = request.page.products;

		for (var i = products.length - 1; i >= 0; i--) {

			var res1 = products[i].category.search("Muebles>");
			var res2 = products[i].category.substring(res1, res1+20);
			cat = res2.split('>');

			var item = clear(cat[1]);
			if ( !(item in items) ){
				items[item] = [];
			}

			if ( jQuery.inArray(item, sections) >= 0 ){
				if (items[item].length < 3 ) {
					items[item][items[item].length] = {
						container: jQuery('#img-'+item+'-'+items[item].length),
						product:  products[i],
						category: item
					};
				};
			};
		};

		setData(items);
	};

	function setData(items){
		var sections = Object.keys(items);

		jQuery.each(sections, function(i, section) {
				if (items[section].length == 3) {
					jQuery.each(items[section], function(j, data) {
						var container = data.container;
						var product = data.product;
						var percent = 0;

						if ( product.msrp > 0 ) {
							percent = ((1 - product.price / product.msrp) * 100).toFixed(0);
						};

						if ( percent > 0 ){
							container.children('.emarsys-menu').children('.discount').html('-' + percent + '%');
							container.children('.emarsys-menu').children('.discount').show();
						}else{
							container.children('.emarsys-menu').children('.discount').hide();
						}
						container.children('.emarsys-menu').children('.subcategory-thumb').attr('href',product.link);
						container.children('.emarsys-menu').children('.subcategory-thumb').find('img').attr('src', product.image);
						container.children('.emarsys-menu').children('.subcategory-thumb').children('.name').html(product.title);
					});
				};
		});
	}
 
	function clear(text){
		var text = text.toLowerCase();
		text = text.replace(/[áàäâå]/, 'a');
		text = text.replace(/[éèëê]/, 'e');
		text = text.replace(/[íìïî]/, 'i');
		text = text.replace(/[óòöô]/, 'o');
		text = text.replace(/[úùüû]/, 'u');
		text = text.replace(/[ýÿ]/, 'y');
		text = text.replace(/[ñ]/, 'n');
		text = text.replace(/[ç]/, 'c');
		text = text.replace(/['"]/, '');
		text = text.replace(/[^a-zA-Z0-9-]/, ''); 
		text = text.replace(/\s+/, '-');
		text = text.replace(/' '/, '-');
		text = text.replace(/(_)$/, '');
		text = text.replace(/^(_)/, '');
		return text;
   }

	/*ScarabQueue.push(['recommend', {
