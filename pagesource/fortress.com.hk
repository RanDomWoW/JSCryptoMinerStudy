quickAddProductview.ajax_getAllBrand = function(){
    var categorycode = headerCompareCounter.data('categorycode');
    if (typeof categorycode == null) {
        categorycode = "";
    }
    $.ajax({
        dataType: 'json',
        type: 'GET',
        url: ACC.config.encodedContextPath+'/search/facets?q=::allCategories:'+encodeURI(categorycode),
        success: function(data) {

            var facets = data['facets'];
            for(var i in facets) {
                if (facets[i].code == 'brand') {
                    $("#brandNameList").empty();
                    $("#brandNameSelector").empty();
                    $('#brandNameSelector').append($('<option>', {
                        value: 'Please choose brand',
                        text: 'Brand'
                    }));
                    var brandData = facets[i].values;
                    for(var j in brandData) {
                        $('#brandNameSelector').append($('<option>', {
                            value: brandData[j].code,
                            text: brandData[j].name
                        }));
                        var query = brandData[j].query.query.value;
                        

                        $("#brandNameList").append('<li><a href="javascript:;" query="' + query + '" code="'+ brandData[j].name + '">' + brandData[j].name + '</a></li>');
                    }
                }
            }
            if($(window).width() > 768) {
                $('.quick-add-compare .list ul#brandNameList').each(function() {
                        qapjsp.push($(this).jScrollPane().data().jsp);
                });
            }
        },
        error: function(jqXHR, textStatus, errorThrown) {
            console.log("Fail To get Brand");
        }
    });
}
quickAddProductview.ajax_getProductsForBrand = function(query){
     $.ajax({
