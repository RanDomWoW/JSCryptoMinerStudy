<script>
$(function() {
    newsletter.init()
});

$(function() {}), $(window).load(function() {
    home.init()
}), $(window).resize(function() {
    home.init()
});

var newsletter = {
    init: function() {
        $(".sign-up-newsletter .input-wrapper input").keypress(function(e) {
            13 == e.which && newsletter.updateClass(newsletter.checkEmail($(this).val()))
        }), $(".sign-up-newsletter .input-wrapper input").keyup(function() {
            null !== $(this).val() && "" != $(this).val() || $("section.sign-up-newsletter").removeClass("subscribed need-valid-email")
        }), $(".btn-sign-up-newsletter").click(function() {
            var e = $(".input-sign-up-newsletter").val();
            newsletter.updateClass(newsletter.checkEmail(e))
        }), $(".alert-icon").click(function() {
            $(".input-sign-up-newsletter").val("");
            var n = $("section.sign-up-newsletter");
            n.removeClass("subscribed need-valid-email");
        })
    },
    checkEmail: function(e) {
        var email_pattern = /(?:[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*|"(?:[\x01-\x08\x0b\x0c\x0e-\x1f\x21\x23-\x5b\x5d-\x7f]|\\[\x01-\x09\x0b\x0c\x0e-\x7f])*")@(?:(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?|\[(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?|[a-z0-9-]*[a-z0-9]:(?:[\x01-\x08\x0b\x0c\x0e-\x1f\x21-\x5a\x53-\x7f]|\\[\x01-\x09\x0b\x0c\x0e-\x7f])+)\])/i;
        return email_pattern.test(e);
    },
    updateClass: function(e) {
        var form_data = "email="+$(".input-sign-up-newsletter").val();
        var registerNewletterUrl = "/register/register-newsletter";
        function success(data){
            //console.log(data)
            var n = $("section.sign-up-newsletter");
            if(data.result=="success"){
                e=true;
                gtmNewsLetterSubscription(data.emailSHA);
            }else{
                e=false;
            }
            n.removeClass("subscribed need-valid-email"), e ? n.addClass("subscribed") : n.addClass("need-valid-email")
        }
        if(e){
            doAjaxAsync(form_data, registerNewletterUrl, success, "get","json", true);
        }else{
            success("fail");
        }
    }
};







</script>

