}




search.ajax=function(text) {
    if (!text)
        return;
    var formdata = [];
    formdata.push({name: 'q', value: text});
    formdata.push({name: 'sort', value: 'relevance'});
    $.ajax({
        dataType: 'json',
        type: 'GET',
        url: ACC.config.encodedContextPath+'/search/headerResults',
        data: formdata,
        success: function(data) {
            try {
                $('.search-wrapper .product .totalNumberOfResults').html(data.productResults.pagination.totalNumberOfResults);
                $('.search-wrapper .product .list').html('');
                for(var i in data.productResults.results) {
                    var productHtml = $('.search-wrapper .product .list-layout li').clone();
                    var productName = data.productResults.results[i].name;
                    var regexp = new RegExp(text, "ig");
                    var matchStrings = _.uniq(productName.match(regexp));
                    for (var j in matchStrings) {
                        productName = productName.replace(new RegExp(matchStrings[j], 'g'), '<>'+matchStrings[j]+'</>');
                    }
                    productName = productName.replace(new RegExp('<>', 'g'), '<span class="mainBlue">');
                    productName = productName.replace(new RegExp('</>', 'g'), '</span>');
                    productHtml.find('.txt').find('a').html(productName);
                    productHtml.find('a').prop('href',ACC.config.encodedContextPath+data.productResults.results[i].url);
                    try {
                        if (data.productResults.results[i].changePrice == null) {
                        	if (data.productResults.results[i].ownBrand == false){
                            	productHtml.find('.price').html('RRP '+data.productResults.results[i].price.formattedValue+(data.productResults.results[i].priceUpFlag?' Up':''));
                        	}else{
                        		productHtml.find('.price').html(data.productResults.results[i].price.formattedValue+(data.productResults.results[i].priceUpFlag?' Up':''));
                        	}
                            productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-price', data.productResults.results[i].price.value);
                            productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-currency', data.productResults.results[i].price.currencyIso);
                        } else {
                        	if (data.productResults.results[i].ownBrand == false){
                            	productHtml.find('.price .cross').html('RRP '+data.productResults.results[i].price.formattedValue);
                        	}else{
                        		productHtml.find('.price .cross').html(data.productResults.results[i].price.formattedValue);
                        	}
                            productHtml.find('.price .mainRed').html(data.productResults.results[i].igcMarkDownPrice.formattedValue+(data.productResults.results[i].priceUpFlag?' Up':''));
                            productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-price', data.productResults.results[i].igcMarkDownPrice.value);
                            productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-currency', data.productResults.results[i].igcMarkDownPrice.currencyIso);
                        }
                    }
                    catch(e) {
                        productHtml.find('.price').html('');
                        productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-price', '');
                        productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-currency', '');
                    }
                    for(var j in data.productResults.results[i].images) {
                        if (data.productResults.results[i].images[j].format == 'cat') {
                            productHtml.find('img').prop('src',data.productResults.results[i].images[j].url);
                            productHtml.find('img').prop('alt',data.productResults.results[i].name);
                            productHtml.find('img').prop('title',data.productResults.results[i].name);
                            break;
                        }
                    }
                    productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-name', data.productResults.results[i].gtmBaseProductName);
                    productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-id', data.productResults.results[i].gtmBaseProductCode);
                    productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-rangename', data.productResults.results[i].gtmRangeName);
                    productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-category', data.productResults.results[i].gtmCategoryPath);
                    productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-category-id', '');
                    productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-brand', data.productResults.results[i].gtmBrandName);
                    productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-variant', data.productResults.results[i].gtmProductCode);
                    productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-variantname', data.productResults.results[i].gtmProductName);
                    productHtml.find('.gtmProductImpressions').data('gtm-product-impressions-position', parseInt(i)+1);
                    productHtml.appendTo('.search-wrapper .product .list');
                }
            }
            catch(err) {
                $('.search-wrapper .product .totalNumberOfResults').html(0);
                $('.search-wrapper .product .list').html('');
            }


            try {
                $('.search-wrapper .brand_totalNumberOfResults').html(data.brandResults.pagination.totalNumberOfResults);
                $('.search-wrapper .list-search-brand').html('');
                for(var i in data.brandResults.results) {
                    var brandHtml = $('.search-wrapper .list-search-brand-layout li').clone();
                    var brandName = data.brandResults.results[i].name;
                    brandHtml.find('.name').html(brandName);
                    brandHtml.find('a').prop('href',ACC.config.encodedContextPath+'/brands/'+data.brandResults.results[i].code + '/' + data.brandResults.results[i].seoBrandName);
                    if (data.brandResults.results[i].smallLogoUrl) {
                        brandHtml.find('a').find('img').prop('src',data.brandResults.results[i].smallLogoUrl);
                    }
                    brandHtml.find('a').find('img').prop('alt',data.brandResults.results[i].name);
                    brandHtml.appendTo('.search-wrapper .list-search-brand');
                }
            }
            catch(err) {
                $('.search-wrapper .brand_totalNumberOfResults').html(0);
                $('.search-wrapper .list-search-brand').html('');
            }


            try {
                $('.search-wrapper .category_totalNumberOfResults').html(data.categoryResults.pagination.totalNumberOfResults);
                $('.search-wrapper .list-search-category').html('');
                for(var i in data.categoryResults.results) {
                    var categoryHtml = $('.search-wrapper .list-search-category-layout li').clone();
                    var categoryName = data.categoryResults.results[i].name;
                    var regexp = new RegExp(text, "ig");
                    var matchStrings = _.uniq(categoryName.match(regexp));
                    for (var j in matchStrings) {
                        categoryName = categoryName.replace(new RegExp(matchStrings[j], 'g'), '<>'+matchStrings[j]+'</>');
                    }
                    categoryName = categoryName.replace(new RegExp('<>', 'g'), '<span class="mainBlue">');
                    categoryName = categoryName.replace(new RegExp('</>', 'g'), '</span>');
                    categoryHtml.find('a').html(categoryName);
                    categoryHtml.find('a').prop('href',ACC.config.encodedContextPath+data.categoryResults.results[i].url);
                    categoryHtml.appendTo('.search-wrapper .list-search-category');
                }
            }
            catch(err) {
                $('.search-wrapper .category_totalNumberOfResults').html(0);
                $('.search-wrapper .list-search-category').html('');
            }
            search.checkVal();
        },
        error: function() {
            search.checkVal();
        }
    });
}




search.wsajax=function(text) {
    if (!text)
        return;
    $.ajax({
        dataType: 'json',
        type: 'GET',
        url: ACC.config.encodedContextPath+'/workshops/results',
        data: 'text='+encodeURI(text),
        success: function(data) {
            try {
                $('.search-wrapper .ws').html('');
                $('.others_totalNumberOfResults').html($('.search-wrapper .list.ws').find('li').length+$('.search-wrapper .list.other').find('li').length);
                for(var i in data.results) {
                    var wsHtml = $('.list-search-ws-layout li').clone();
                    var wsName = data.results[i].name;
                    var regexp = new RegExp(text, "ig");
                    var matchStrings = _.uniq(wsName.match(regexp));
                    for (var j in matchStrings) {
                        wsName = wsName.replace(new RegExp(matchStrings[j], 'g'), '<>'+matchStrings[j]+'</>');
                    }
                    wsName = wsName.replace(new RegExp('<>', 'g'), '<span class="mainBlue">');
                    wsName = wsName.replace(new RegExp('</>', 'g'), '</span>');
                    wsHtml.find('a').html(wsName);
                    wsHtml.find('a').prop('href',ACC.config.encodedContextPath+'/workshops/c/'+data.results[i].code);
                    wsHtml.appendTo('.search-wrapper .list.ws');
                }
            }
            catch(err) {
                $('.search-wrapper .ws').html('');
            }
            $('.others_totalNumberOfResults').html($('.search-wrapper .list.ws').find('li').length+$('.search-wrapper .list.other').find('li').length);
            search.checkVal();
        },
        error: function() {
            search.checkVal();
        }
    });
}

search.othersajax=function(text) {
    //temporary remove newsfeed search
    return;

    if (!text)
        return;

    $.ajax({
        dataType: 'html',
        type: 'GET',
        url: ACC.config.encodedContextPath+'/newsfeed/search/'+ encodeURI(text),
        success: function(data) {
            try {
                $('.search-wrapper .other').html('');
                $('.others_totalNumberOfResults').html($('.search-wrapper .list.ws').find('li').length+$('.search-wrapper .list.other').find('li').length);
                $('.search-wrapper .list.other').append($(data).find('.search-other').find('.list').html());
            }
            catch(err) {
                $('.search-wrapper .other').html('');
            }
            $('.others_totalNumberOfResults').html($('.search-wrapper .list.ws').find('li').length+$('.search-wrapper .list.other').find('li').length);
            search.checkVal();
        },
        error: function() {
            search.checkVal();
        }
    });
}

search.checkVal=function(){
	var searchWrapper = $('.search-wrapper');
	if($('.input-search input,.input-search-mobile').val()) {
	    if ($('.search-wrapper .product .totalNumberOfResults').html() == '0' &&
	        $('.search-wrapper .brand_totalNumberOfResults').html() == '0' &&
	        $('.search-wrapper .category_totalNumberOfResults').html() == '0' &&
	        $('.search-wrapper .others_totalNumberOfResults').html() == '0') {
	        searchWrapper.addClass('no-record');
	        $('.search-wrapper .search-title-no-result').show();
	        $('.search-wrapper .search-title').hide();
	    } else {
	        searchWrapper.removeClass('no-record');
	    }
	} else {
	    searchWrapper.addClass('no-record');
	    $('.search-wrapper .search-title').show();
	    $('.search-wrapper .search-title-no-result').hide();
	}
}

});


