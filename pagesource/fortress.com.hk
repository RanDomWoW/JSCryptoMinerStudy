<script>
quickAddProductview.pageRefresh = false;
quickAddProductview.init = function(){
        var productcode = '';

        $('body').on('click','.quick-add-compare .col>div:first-child .list ul li a',function(){
            $('.quick-add-compare .col>div:first-child .list ul li a').removeClass('active');
            $(this).addClass('active');
            $('.quick-add-compare .col>div:nth-child(2)').animate({'opacity':1},500);
            $(".quick-add-compare .products").hide();
            $(".product-select-wrapper").show();
            var query = $(this).attr( "query" );
            quickAddProductview.ajax_getProductsForBrand(query);
            $.each(qapjsp, function(e) {
                this.destroy()
            });

        });
        $('body').on('click','.quick-add-compare .col>div:nth-child(2) .list ul li a',function(){
            $('.quick-add-compare .col>div:nth-child(2) .list ul li a').removeClass('active');
            $(this).addClass('active');
            $('.quick-add-compare .col>div:nth-child(3)').animate({'opacity':1},500);
            $(".quick-add-compare .products").show();
            var code = $(this).attr( "code" );
            productcode = code;
            quickAddProductview.ajax_getProductForCode(code);
        });
        $('body').on('click','.btn-add-product-compare',function(e){
            e.preventDefault();
            var pageRefresh = $(this).attr('pageRefresh');
            if(pageRefresh == "true"){
                quickAddProductview.pageRefresh = true;
            }else{
                quickAddProductview.pageRefresh = false;
            }
            quickAddProductview.open();
        });

        $('.quick-add-compare .btn-close').on('click',function(e){
            e.preventDefault();
            $(".product-select-wrapper").hide();
            $(".quick-add-compare .products").hide();
            quickAddProductview.close();
        });

        $('.btn-quick-add-product').click(function(){
            quickAddProductview.close();
            comparisonBarTool.ajax_add(productcode,quickAddProductview.pageRefresh);
        });

        clickBgClose('.quick-add-compare', function(){
            quickAddProductview.close();
        });
}
