    function removeFromBasket(e){
        commonjs.showLoadingDiv();
        e.preventDefault();
        var productEntry = $(this).parent().next('.pCode').val();
        var form = $(this).parent().parent().find('.updateCartForm');
        var formSer = form.serialize(true);
        var productCode = form.find("input[name='productCode']").val();
        $.ajax({
            type: "GET",
            data: {removeFromMiniCart: true},
            context: this,
            url: "/zt/cart/removeEntry"+"?"+formSer
            }).success(function(data){
            	gtmProductRemoveFromCart(productCode);
            }).done(function(data) {
                $("#cart_totalItems").html($(data).find("#cartpopup_totalItems").val());
                $("#miniCartLayer").empty().html($('<div>').html(data));
                $(".header-menu .cart-wrapper").trigger('mouseenter');
                setTimeout(function() {$('.header-menu .cart-wrapper').trigger('mouseleave')}, 3000);
                commonjs.closeLoadingDiv();
                if(typeof(refleshCart)=="function")refleshCart();

            });
    }
</script>


