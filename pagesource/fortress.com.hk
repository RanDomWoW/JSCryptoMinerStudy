    function submitInterestProduct(productCode, type){

		
			location.href = "/zt/my-account/setProductInterest?productCode=" + productCode + "&type=" + type;
		
    }

    var commonjs = {
        showLoadingDiv:function(){
            $.isLoading({
                'text': "",
                'tpl': '<span style="display:block;margin:300px auto;width:100%;text-align:center;">%text%<img style="margin: auto;" src="'+ACC.config.themeResourcePath+'/images/loading/page-loading.gif" /></span>'
            });
        },
        closeLoadingDiv:function(){
            $.isLoading("hide");
            $('.isloading-overlay').hide();
        },
        removeStockAlertFlagInCart:function(){
            $.ajax({
                type: "GET",
                url: '/cart/removeStockAlertFlag',
                success: function(data) {

                    if(data == "true"){
                    }

                }
            });
        }

    }

    function doAjaxAsync(form_data, url, myCallback, methodType, dataType,isAsync) {
        commonjs.showLoadingDiv();
        responseData = null;
        if (methodType == undefined || methodType == null)
            methodType = "get";
        if (dataType == undefined || dataType == null)
            dataType = "html";
        $
            .ajax({
                type : methodType,
                url : url,
                data : form_data,
                cache : false,
                async : isAsync,
                dataType : dataType,
                success : function(responseData) {
                    if (myCallback != undefined
                            && myCallback != null
                            && typeof (myCallback) == 'function') {
                        myCallback.call(this, responseData);
                    } else {
                        callback(responseData, 0);
                    }
                    commonjs.closeLoadingDiv();
                },
                error : function(responseData) {
                    //alert("error:"+responseData.status);
                    commonjs.closeLoadingDiv();
                }
            });
        return false;
    }

    function doGet(form_data, url,myCallback,methodType,dataType,errorCallBack) {
    	responseData = null;
    	if(methodType==undefined||methodType==null)methodType="get";
    	if(dataType==undefined||dataType==null)dataType="html";
    	$.ajax({
    		type: methodType,
    		url: url,
    		data: form_data,
    		cache: false,
    		dataType : dataType,
    		success: function(responseData) {
    		    //console.log(responseData);
    			var hasError = isErrorPopup(responseData);
    			if (hasError) {
    				openContentPopup(responseData);
    			}else {
    				if(errorCallBack!=undefined&&errorCallBack!=null&&typeof(errorCallBack)=='function'
    					&&$(responseData).find("input[name='errors']").length>0){
    					errorCallBack.call(this,responseData);
    				}else{
    					if(myCallback!=undefined&&myCallback!=null&&typeof(myCallback)== 'function' ){
    						myCallback.call(this,responseData);
    					}else
    						openContentPopup(responseData);
    						//callback(responseData, 0);
    				}
    			}
    			/* $(".branchBankCodeSelectId-menu").show().css({"z-index":"-1"});
    			$(".branchBankCodeSelectId-menu").mCustomScrollbar("update"); */
    		},
    		error:function(responseData){
    			//alert("error:"+responseData.status);
    		}
    	});
    	return false;
    }

    function isErrorPopup(responseData) {

    	var openErrorPopup = $(responseData).andSelf().find("#serviceUnavailable");
    	var minicartShippableToPRCErrorPopup = $(responseData).find("#minicartShippableToPRCErrorPopup");

    	var hasError = $(openErrorPopup).size() > 0;
    	var hasMinicartError = $(minicartShippableToPRCErrorPopup).size() > 0;

    	var errorFlag=false;
    	if(hasError || hasMinicartError){
    		errorFlag=true;
    	}
    	return errorFlag;
    }
</script>


