<script>
$(window).load(function () {
    search.clearSearch();

    $('.input-search input,.input-search-mobile').unbind('keyup');
	$('.input-search input,.input-search-mobile').keyup(function (e) {
        if(e.keyCode == 17 || e.keyCode == 91) return;
        else if(e.keyCode == 27) {
            $('.btn-close-search')[0].click();
            $('.input-search input,.input-search-mobile').blur();
            return;
        } else if (e.keyCode == 13) {
            var searchString = $(this).val();
            //common.setCookie('searchVal',searchString);
            window.location.href = ACC.config.encodedContextPath+'/search?q='+encodeURI(searchString);
        } else {
            if(!$('body').is('.showSearch')) $('.btn-search').click();

            if($('.input-search input').val() != $(this).val()){
                $('.input-search input').val($(this).val());
            }

            if($('.input-search-mobile').val() != $(this).val()){
                $('.input-search-mobile').val($(this).val());
            }

            search.checkVal();
            search.ajax($(this).val());

            search.wsajax($(this).val());
            search.othersajax($(this).val());
        }
        return;
    });

	$(".btn-close-search").click(function(){
        search.clearSearch();
	});

    $("header .header-menu .nav > li.input-search a.btn-search").unbind("click");
    $("header .header-menu .nav > li.input-search a.btn-search").on("click", function() {

        if($("header .header-menu .nav > li.input-search a.btn-close-search").is(":visible")){
            var searchString = $(this).siblings("input").val();
            //common.setCookie('searchVal',searchString);
            window.location.href = ACC.config.encodedContextPath+'/search?q='+encodeURI(searchString);
		}else{
            $(".nav").addClass("showSearchInputOnly"), $("header .input-search input").stop(!0, !0).animate({
                width: 400
            }, 500), search.openSearch()
		}
	});
});

$(document).ready(function () {

// clear search input text and results
search.clearSearch=function(){
    $('.input-search input,.input-search-mobile').val("");
    $(".search-wrapper").addClass("no-record");
};

search.defaultOpenSearch=search.openSearch;

search.openSearch=function(){
    search.defaultOpenSearch();
    if($('.input-search input,.input-search-mobile').val()) {
        search.ajax($('.input-search input,.input-search-mobile').val());

        search.wsajax($('.input-search input,.input-search-mobile').val());
        search.othersajax($('.input-search input,.input-search-mobile').val());
    } else {
        ajaxAddFeatureProducts($('#form-ajax-add-feature-products').serializeArray());
    }
}

function ajaxAddFeatureProducts(formdata) {
    if ($('.feature-products .products').is(':empty')) {
        $.ajax({
            dataType: 'html',
            type: 'GET',
            url: ACC.config.encodedContextPath+'/search/htmlResults',
            data: formdata,
            success: function(data) {
                $('.feature-products .products').html('');
                $('.feature-products .products').html($(data).find('.products').html());
                $('.feature-products .products .grid-item').removeClass('grid-item');
                $('.feature-products .products .grid-sizer').removeClass('grid-sizer');
                quickview.init();
                productQuickView.init();
            },
            error: function() {
            }
        });
