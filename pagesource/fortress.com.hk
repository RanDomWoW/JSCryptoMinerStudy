comparisonBarTool.init = function(){
    $('body').on('click','.btn-compare',function(){
        var _this = $(this);
        if(_this.parent().is('.nav')) return;
        comparisonBarTool.ajax_add(_this.data('productcode'));
    }).on('mouseenter','.btn-compare',function(){
        var _this = $(this);
        _this.find('.bubble').removeClass('show-not-available-for-comparing show-max-number-items-for-comparing show-available-for-comparing');
        if(headerCompareCounter.data('categorycode') && headerCompareCounter.data('categorycode')!='' && _this.data('categorycode') != headerCompareCounter.data('categorycode')) {
            _this.find('.bubble').addClass('show-not-available-for-comparing');
            return;
        }
        if(headerCompareCounter.html() == '4') {
            _this.find('.bubble').addClass('show-max-number-items-for-comparing');
        } else {
            _this.find('.bubble').addClass('show-available-for-comparing');
        }
    });

    // comparison btm tool bar click del btn and add blank new at last
    $('body').on('click','.comparison-btm-popup .comparison-btm-popup-innerwrapper .btn-close', function(){
        var selectedNum = $(this).attr('selectedproduct');
        comparisonBarTool.removeProduct($(this),selectedNum);
    });

    $('.comparison-btm-popup').children('.btn-close').click(function(){
        comparisonBarTool.closeToolBar();
    });
    comparisonBarTool.ajax_count();
}
comparisonBarTool.insertProduct = function(product, Index, effect){
    if (effect === void 0) {effect=true;}
    $.each($('.comparison-btm-popup .btn-add-product-compare'), function(index, value) {
        if(index !== 0) return;
        var currentNum = parseInt(headerCompareCounter.html());
        var _targetLi = $(this).parents('li');
        selectedCompareIndex +=1;
        _targetLi.html('');
        _targetLi.append($('.comparison-list-targetli').clone().html());
        _targetLi.find('a').data('productcode',product.code);
        _targetLi.find('img').prop('alt',product.name);
        _targetLi.find('img').prop('title',product.name);
        for(var j in product.images) {
            if (product.images[j].format == 'prdthumb') {
                _targetLi.find('img').prop('src',product.images[j].url);
                break;
            }
        }

        //_targetLi.html("<a href=\"javascript:;\" class=\"btn-close\" selectedProduct=\"selectedNum"+Index+"\" data-productcode=\""+product.code+"\"><span class=\"globalIcon icon-no-black w10\"></span></a> "+_productImghtml);
        if (effect) {
            TweenMax.from(_targetLi , 0.5, {css:{scale: 0, opacity:0, transformOrigin:'50% 50%'}, ease:Power2.easeOut});
        }
    });
}
comparisonBarTool.removeProduct = function(_this,_selectedNum){
	comparisonBarTool.ajax_remove(_this.data('productcode'));
}
comparisonBarTool.ajax_add = function(productcode, pageRefresh) {
    if (pageRefresh === void 0) {pageRefresh=false;}
    $.ajax({
        dataType: 'json',
        type: 'GET',
        url: ACC.config.encodedContextPath+'/compare/add',
        data: 'code='+productcode,
        success: function(data) {
            $('.comparison-list').html($('.comparison-list-ori').html());
            if (data) {
                headerCompareCounter.html(data.length);
                if (data.length > 0) {
                    headerCompareCounter.data('categorycode',data[0].secondLvCategoryCode);
                } else {
                    headerCompareCounter.data('categorycode','');
                }
                selectedCompareIndex = 0;

                for(var i in data) {
                    var _popupBtmBar = $('.comparison-btm-popup');
                    if (data[i].code == productcode) {
                        if(_popupBtmBar.is('active')){
                            comparisonBarTool.insertProduct(data[i], selectedCompareIndex);
                        }
                        else {
                            _popupBtmBar.slideDown(500,function(){
                                comparisonBarTool.insertProduct(data[i], selectedCompareIndex);
                                comparisonBarTool.openToolBar();
                            }(i));
                        }
                    } else {
                        comparisonBarTool.insertProduct(data[i], selectedCompareIndex, false);
                    }
                }

                if(!$('.comparison-btm-popup').is('active')) {
                    comparisonBarTool.openToolBar();
                }
                if(pageRefresh === true){
                    location.reload();
                }
            }
        },
        error: function() {
            console.log("Fail To add Product");
        }
    });
}

comparisonBarTool.ajax_remove = function(productcode, pageRefresh) {
    if (pageRefresh === void 0) {pageRefresh=false;}
    $.ajax({
        dataType: 'json',
        type: 'GET',
        url: ACC.config.encodedContextPath+'/compare/remove',
        data: 'code='+productcode,
        success: function(data) {
            $('.comparison-list').html($('.comparison-list-ori').html());
            if (data) {
               headerCompareCounter.html(data.length);
               if (data.length > 0) {
                   headerCompareCounter.data('categorycode',data[0].secondLvCategoryCode);
               } else {
                   headerCompareCounter.data('categorycode','');
               }
               selectedCompareIndex = 0;
               for(var i in data) {
                    var _popupBtmBar = $('.comparison-btm-popup');
                    comparisonBarTool.insertProduct(data[i], selectedCompareIndex, false);
               }
               if(pageRefresh  === true){
                   location.reload();
               }
            }

        },
        error: function() {
            console.log("Fail To remove Product");
        }
    });
}

comparisonBarTool.ajax_count = function() {
    $.ajax({
        dataType: 'json',
        type: 'GET',
        url: ACC.config.encodedContextPath+'/compare/count',
        success: function(data) {
            if (data) {
               headerCompareCounter.html(data[0]);
               headerCompareCounter.data('categorycode',data[1]);
            }
        },
        error: function() {
        }
    });
}

