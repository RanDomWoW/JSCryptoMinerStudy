                            alert('Musíte souhlasit se zpracováním osobních údajů.');
                            event.preventDefault();
                            return false;
                        }

                        // check email
                        var email = $('#emailForm #email').val();
                        if (email == '') {
                            alert('Zadejte Váš e-mail.');
                            event.preventDefault();
                            return false;
                        }
                        else {
                            if (regEx_email.test(email) == false) {
                                alert('Zadejte spravný e-mail.');
                                event.preventDefault();
                                return false;
                            }
                        }

                    });
                });
            </script>


