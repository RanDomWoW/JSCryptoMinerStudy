<script type="text/javascript">
$(document).ready(function()
{
var lead_index = 1;
var modality = 0;
var type = '';
var course = '';
var url = '';
var email = '';
var name = '';
var tel = '';
function change_lead_box(index)
{
$('#lead-block-'+lead_index).removeClass('show').addClass('hide');
lead_index++;
$('#lead-block-'+lead_index).addClass('show');
if(lead_index == 3)
{
$('#lead-title').html('Seu cadastro foi realizado com sucesso :)');
}
console.log('change');
if( $(window).width() < 770 )
{
console.log('LEAD:'+lead_index);
var box_heigth = ( lead_index > 2 ) ? '160px' : '510px';
$('#lead-box').css('min-height', box_heigth);
}
}
function select_course(index)
{
$('#lead-course-sel').removeClass('attention');
$('#lead-course-sel').next().addClass('hidden');
course = $('#lead-course-sel option').eq(index).val();
url = $('#lead-course-sel option').eq(index).attr('data-url');
$('#lead-course').html(course);
$('#lead-url').attr('href', url);
}

$('.lead-modality').click(function(){
modality = $(this).attr('data-modality');
if(modality == 0)
{
var tec = $('.lead-type')[1];
$(tec).css('display', 'none');
}
change_lead_box(lead_index);
});

$('.lead-next').click(function(e){
e.preventDefault();
console.log( lead_index );
/*if(lead_index == 2 && course == '')
{
$('#lead-course-sel').addClass('attention');
$('#lead-course-sel').next().removeClass('hidden');
}*/
if( (lead_index == 2 && email == '') || (lead_index == 2 && name == '') || (lead_index == 2 && tel == '') || (lead_index == 2 && !tel.match(/\d{8,}/gi) ) )
{
if(email == '')
{
var aux = $('#lead-block-2 input')[0];
$(aux).addClass('attention');
$(aux).next().removeClass('hidden');
}
if(name == '')
{
var aux = $('#lead-block-2 input')[1];
$(aux).addClass('attention');
$(aux).next().removeClass('hidden');
}
if(tel == '' || !tel.match(/\d{8,}/gi) )
{
var aux = $('#lead-block-2 input')[2];
$(aux).addClass('attention');
$(aux).next().removeClass('hidden');
}
}
else if(lead_index == 2)
{
var interest = (type == 1) ? 'Grad' : (type == 2) ? 'Tec' : 'Pos';
if(modality == 0)
{
interest = 'EAD';
}
var modality = interest;
interest += ' - Tenho Interesse';

$.ajax({
url: '/crontab/cadastrar-email-dinamize',
type: 'POST',
data: {
cmp4: encodeURI(interest),
cmp2: encodeURI(name),
cmp1: encodeURI(email),
cmp9: encodeURI(course),
cmp12: encodeURI(tel),
modality: modality
},
success: function (msg) {
console.log(msg);
}
});
change_lead_box(lead_index);
}
else
{
change_lead_box(lead_index);
}
});
$('.lead-back').click(function(event){
event.preventDefault();
$("#lead-block-2").removeClass("show");
$("#lead-block-1").removeClass("hide");
$("#lead-block-1").addClass("show");
lead_index = 1;
});
$('#lead-block-2 input').focusout(function() {
if( $(this).attr('type') == 'text' )
{
if( $(this).val().length > 2)
{
name = $(this).val();
$(this).removeClass('attention');
$(this).next().addClass('hidden');
}
}
else if( $(this).attr('type') == 'email' )
{
if( $(this).val().length > 2 && ($(this).val().indexOf('@') > 1) )
{
email = $(this).val();
$(this).removeClass('attention');
$(this).next().addClass('hidden');
}
}
else if( $(this).attr('type') == 'tel' )
{
var aux = $(this).val().match(/\d{8,}/gi);
if( $(this).val().length > 7 && aux.length > 0 )
{
tel = $(this).val();
$(this).removeClass('attention');
$(this).next().addClass('hidden');
}
}
});
$('#lead-course-sel').change(function () {
select_course( $('#lead-course-sel')[0].selectedIndex );
$('#lead-block-2 input').show();
$('#lead-block-2 .lead-next').show();
});

$('div.lead-type').click(function(e){
if( $(this).hasClass('sel') )
{
$('#lead-block-'+lead_index+' div.lead-option').removeClass('sel');
$('#lead-course').html( '<option >Selecione</option>' );
}
else
{
$('#lead-block-'+lead_index+' div.lead-option').removeClass('sel');
$(this).children().addClass('sel');
type = $(this).attr('data-type');
var x = courses[modality][type].length;
var html = '';
for(var i = 1; i < x; i++)
{
html += '<option data-url="'+courses[modality][type][i][1]+'" value="'+courses[modality][type][i][0]+'">'+courses[modality][type][i][0]+'</option>';
}
$('#lead-course-sel').html(html);
select_course(0);
}
$('#lead-course-sel').show();
});
});
</script>
</section>
