<script>
function showCookieSettingsInHeader() {
    var cc_cookie = getCookie('cc_necessary');
    var cc_ineu = getCookie('cc_ineu');
    if (cc_cookie == null && cc_ineu == null && !cc.setupcomplete) {
         setTimeout(showCookieSettingsInHeader, 1000);
         return;
    } else {
        
        if ((cc_isEuDomain() || cc_ineu == 'yes') && !(cc_cookie == null && typeof cc.approved.necessary === 'undefined')) {
            $('.cookie-settings-link').addClass('show-cookie-settings');
            $('.cookie-settings-link').show();
        } else {
            $('.cookie-settings-link').hide();
        }
    }
}

function replaceCookieCategoryNames() {
    $('.euCookieMessage').each(function(index) {
        var html = $(this).html();
        html = html.replace(/<!-- cc- -->(.*?)<!-- -cc -->/, function(match, group1) {
            var cookies = window.cc.initobj.cookies[group1];
            var rtnval;
            if (cookies) {
                rtnval = cookies.title;
            } else {
                rtnval = '';
            }

            return rtnval;
        });
        $(this).html(html);
    });
}

$(document).ready(function() {
    showCookieSettingsInHeader();
    replaceCookieCategoryNames();
    $(document).on('click', '.cookieSettings', function(e) {
        e.preventDefault();
        cc.showmodal();
    });
});
</script>

