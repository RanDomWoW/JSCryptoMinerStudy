    function LoginClick(UserName, Password, RememberMe) {
        $('#loginButton').prop('disabled', true);

        if (UserName == "" || Password == "") {
            $('#loginButton').prop('disabled', true);
            document.getElementById("ErrorDiv").innerHTML = 'لطفاً نام کاربری و رمز عبور را وارد نمائید.';
            document.getElementById("ErrorDiv").innerHTML = 'لطفاً نام کاربری و کلمه عبور را وارد نمائید.';
            return;
        }

        if (!$.browser.chrome && !$.browser.mozilla) {
            $('#loginButton').prop('disabled', true);
            document.getElementById("ErrorDiv").innerHTML = 'به منظور استفاده بدون مشکل از سامانه از مرورگر Chrome یا FireFox استفاده نمائید.';
        } else {
            document.getElementById("Spinner").style.display = 'inherit';
            $.ajax({
                type: 'POST',
                url: "/Account/_LoginJson",
                data: { "UserName": UserName, "Password": Password, "RememberMe": RememberMe, "returnUrl": window.location.href.toString().split(window.location.host)[1] },
                success: function (data) {

                    if (data.result == "Success") {
                        var ff = "";
                        if (data.returnUrl != "Empty") {
                            ff = data.returnUrl.toString().replace('#', '');
                        } else {
                            ff = "/";
                        }
                        window.location = ff;
                        return;

                    } else if (data.result == "VerifyRequired") {
                        window.location = "/Account/VerifyEmail?email=" + data.email + "&userId=" + data.userId;
                        return;
                    }
                    else if (data.result == "twofactorneeded")
                    {
                        window.location = "/Account/SendTwoFactorCode?email=" + data.email + "&userId=" + data.userId + "&ReturnUrl=" + window.location.href.toString().split(window.location.host)[1];
                        return
                    }
                    else {
                        $('#loginButton').removeAttr('disabled');
                    }

                    document.getElementById("ErrorDiv").innerHTML = data.result;
                    document.getElementById("Spinner").style.display = 'none';
                },
                Error: function (data) {
                    $('#loginButton').removeAttr('disabled');

                    document.getElementById("ErrorDiv").innerHTML = 'رخداد خطا';
                    document.getElementById("Spinner").style.display = 'none';
                }
            });
        }

    }

