    function isValidPassword(password) {
        var unicodeRange = "\\u0100-\\u0105";
        var criteria = ["A-Z", "a-z", "\\d", "!$#%", unicodeRange];

        // check whether it doesn't include other characters
        var re = new RegExp("^[" + criteria.join("") + "]{6,}$");
        if (!re.test(password)) return false;

        var minSatisfiedCondition = 3;
        var satisfiedCount = 0;
        for (var i = 0; i < criteria.length; i++) {
            re = new RegExp("[" + criteria[i] + "]");
            if (re.test(password))++satisfiedCount;

        }
        return (satisfiedCount >= minSatisfiedCondition);
    }
    var today = new Date();
    var h = today.setHours('17');
    var m = today.setMinutes('14');
    var s = today.setSeconds('11');

    $(function () {

        setInterval(function () {

            starttime(today);
        }, 1000);

        setInterval(function () {

            getdate();
        }, 500);
    });


    var t_point = false;
    function starttime(today1) {
        var temp = new Date(today1);
        temp.setSeconds(temp.getSeconds() + 1);
        today = temp;
    }

    function getdate() {

        var h = String(today.getHours());
        var m = String(today.getMinutes());

        var s = String(today.getSeconds());
        if (s.length == 1) {
            s = '0' + s;

        }
        if (m.length == 1) {
            m = '0' + m;

        }
        if (h.length == 1) {
            h = '0' + h;

        }
        // console.log(h + ":" + m + ":" + s);
        if (!t_point) {
            document.getElementById('times').innerHTML =
           h + "<span>:</span>" + m + "<span>:</span>" + s;
            t_point = true;
        } else {

            document.getElementById('times').innerHTML =
              h + "<span style='color:#eee'>:</span>" + m + "<span style='color:#eee'>:</span>" + s;
            t_point = false;
        }

    }

    function checkTime(i) {
        if (i < 10) { i = "0" + i };  // add zero in front of numbers < 10
        return i;
    }
