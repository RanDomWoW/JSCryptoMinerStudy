	require(["jquery", "domReady!"], function() {
		var root = $("[data-guid=2048b504-ffd9-4e45-b016-a340d1d3c725]");
		
		var toggleItem = function(node) {
			if (node.hasClass("open")) { 
				root.removeClass("darken");
				node.removeClass("open");
				node.find(".title i.fa").removeClass("fa-minus").addClass("fa-plus");
			} else {
				root.addClass("darken");
				root.find(".item").removeClass("open");
				node.addClass("open");
				node.find(".title i.fa").removeClass("fa-plus").addClass("fa-minus");
			}
		} 

		root.find(".item").click(function(e) {
			if ($(window).width() > 1080) return;
			toggleItem($(this));
		});

		root.find(".item .content a").click(function(e) {
			if ($(window).width() < 1080 && $(this).closest(".item").hasClass("open") == false) e.preventDefault();
		});

		root.find(".item .title .fa").click(function(e) {
			e.stopPropagation();
			e.preventDefault();
			toggleItem($(this).closest(".item"));
		});
	});
</sv_sv_script></div>
	
