	function getBGImagesForScreenSize(slideClass, imgId){
		var slide = slideClass;
		var imgId = imgId;
		var width = $(window).width();

		getImage(width);
		$(window).resize(function() {
			getImage($(window).width());
		});

		function getImage(width){
			var width = width;
			if (width > 1064) {//load large
				$(slide).each(function( x ) {
					$(this).find(imgId).css("background-image", "url("+ $(this).find(imgId).attr("data-large-src")  +")" ) ;
				});
			}
			else if (width > 624) {//load med
				$(slide).each(function( x ) {
					$(this).find(imgId).css("background-image", "url("+  $(this).find(imgId).attr("data-medium-src") +")" ) ;
				});
			}
			else {//load small
				$(slide).each(function( x ) {
					$(this).find(imgId).css("background-image", "url("+  $(this).find(imgId).attr("data-small-src") +")" ) ;
				});
			}
		}
	}




