					<!-- poll start -->
					<style>
						.votes_result_wrapper {
							width: 350px;
							margin-bottom: 26px;
						}

						.votes_percent_progress {
							background:#c1b790 !important;
							color:#fff;
							height:24px;
							padding-left:2px;
							min-width: 24px !important;
							text-align:right;
							display:inline-block;
							margin-bottom:4px;
						}
						.percent {
							margin-right: 4px;
							margin-top: 7px;
							display: inline-block;
							min-width:24px;
						}
						.poll_question {
							text-align: justify;
							color: #8a7b51;
							font-weight: bold;
							font-size: 13px;
							line-height: 13px;
							margin-bottom: 8px;
						}
						.vote_answer {
							color: #8a7b51;
							font-size: 12px;
						}
						.already_voted {
							text-align: center;
							color: #8a7b51;
							font-weight: bold;
						}
						.vote_btn {
							background: #8a7736;
							border: 1px solid #8a7736;
							border-radius: 3px;
							padding: 5px;
							color: #fff;
							font-weight: bold;
							outline: none;
							text-align: center;
							margin-top: 7px;
							cursor: pointer;
							height: 21px;
							width: 135px;
							line-height: 20px;
							text-transform: uppercase;
						}
					</style>
					<script>
						function visualiseVotesPercent(selector, options)
						{
							$(selector).each(function(){
								var _this = $(this);
								var variantVotes = _this.attr(options.votesAttr);

								var percent 	 = options.totalVotes != 0 ? (variantVotes / options.totalVotes) * 100 : 0;
								var progress	 = _this.next().find('.percent');
								var percentWidth = (variantVotes / options.totalVotes) * options.maxWidth;
								progress.animate({
									width: percentWidth+'px', 
									duration: 700
								}).text(percent+'%');
							});
						}
						$(document).ready(function(){
							$('.vote_btn').on('click', function(){
								var question = $('.poll_question').attr('id');
								var answer   = $('.vote_radio:checked');
								if (answer.val()) {
									$.ajax({
										url: 'http://edu.gov.az/az/vote',
										data: {question: question, answer: answer.val()},
										type: 'POST',
										dataType: 'json',
										success: function(response){
											$.each(response, function(k, v){
												if (k != 'totalVotes') {
													var _this = $(this)[0];
													$('#vote_answer_'+_this.id).attr('data-votes', _this.votes);
												}
											});
											$('.votes_percent_progress, .already_voted').show();
											$('.vote_radio').remove();
											$('.vote_btn').hide();
											var options = {
												totalVotes: response.totalVotes,
												votesAttr: 'data-votes',
												maxWidth: 350,
												progressSelector: '.votes_percent_progress',
												percentContainer: '.progress_percent_container'
											};
											visualiseVotesPercent('.vote_answer', options);
										}
									});
								}
							});
						});
					</script>
										<!-- poll end -->
