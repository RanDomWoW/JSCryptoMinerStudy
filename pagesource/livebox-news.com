  <script type="text/javascript">

  (function($){

    // .reclame
    $('.info').on('click', function(){
      $(this).children('.text-info').fadeToggle(200);
    });

    // Submitted
    if( $('.submitted').length != 0 ){
      var sub = $('.submitted');
      var txtSub = sub.html();
      var replaced = txtSub.replace('Soumis', 'Publié');
      sub.html(replaced);
    }

    // Near Empty Breadcrumb
    if( $('.breadcrumb').find('> a').length <= 1 ){
      $('.breadcrumb').hide();
    }

    // Infobulle
    $('.pub-info-btn').on('mouseover', function(){
      $(this).parent().find('.pub-info-txt').css({
        bottom: '0px',
        padding: '20px 10px 10px 10px'
      });
    });
    $('.pub-info-btn').on('mouseleave', function(){
      $(this).parent().find('.pub-info-txt').css({
        bottom: '100%',
        padding: '0px 10px'
      });
    });

    // page Fournisseur - manageHeight '.details-list'
    function manageHeight(status){
      var list = $('.details-list');
      var size = 0;

      if( status == true ){
        // Define
        list.each(function(){
          var el = $(this);
          if( el.height() >= size ){
            size = el.height();
          }
        });

        // Apply
        list.each(function(){
          $(this).height( size );
        });
      }
      else{
        list.each(function(){
          $(this).css('height', 'auto');
        });
      }
    }

    function structuration(status){
      if( status == true ){
        // High Res
        manageHeight(true);
      }
      else{
        // Low Res
        manageHeight(false);
      }
    }

    var currentRes = true; // true = HighRes / false = LowRes
    $(window).resize(function(){
      if( $(window).width() >= 768 ){
        if( currentRes != true ){
          structuration(true);
          currentRes = true;
        }
      }
      else{
        if( currentRes != false ){
          structuration(false);
          currentRes = false;
        }
      }
    });

    if( $(window).width() >= 768 ){
      structuration(true);
      currentRes = true;
    }
    else{
      structuration(false);
      currentRes = false;
    }

  })(jQuery);

  </script>

