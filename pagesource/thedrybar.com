			var c = 'newsletter';

			if ( $.sdg.getCookie(c) !== 'set' ) {

				var getVal;

				var newsletterModal = new Object($.sdg.modal({
					obj				: '#modalNewsletter',
					openCallback	: function() {
						var getVal = '';

						mobile = new $.sdg.mq({
							view : 'mobile',
							callback : function() {
								var getVal = $('#newsletterFormFooter').find('.newsletter-input-field').val();

								$('#modalNewsletter').find('#mce-EMAIL').val(getVal);
							}
						});

						desktop = new $.sdg.mq({
							view : 'desktop',
							callback : function() {


								// getVal = $('#newsletterProductTeaserDesktop').find('#newProductEmail').val();
								getVal = $('#newsletterFormFooterDesktop').find('.newsletter-input-field').val();

								$('#modalNewsletter').find('#mce-EMAIL').val(getVal);
							}
						});

						$.sdg.placeholder('.input-placeholder input');

					},
					closeCallback 	: function() {
						if ( $('#checkShow').is(':checked') ) {
							$.sdg.setCookie(c, 'set', 365);
						}
					}
				}));

				var newsletterModalTeaser = new Object($.sdg.modal({
					obj				: '#modalNewsletter',
					trigger 		: '.teaser__trigger',
					openCallback	: function() {

						getVal = $('#newsletterProductTeaserDesktop').find('#newProductEmail').val();
						$('#modalNewsletter').find('#mce-EMAIL').val(getVal);

						$.sdg.placeholder('.input-placeholder input');

					},
					closeCallback 	: function() {
						if ( $('#checkShow').is(':checked') ) {
							$.sdg.setCookie(c, 'set', 365);
						}
					}
				}));

				var newsletterModalTeaserPhone = new Object($.sdg.modal({
