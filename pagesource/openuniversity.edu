    <script type="text/javascript">
        function setPopupFocus()
        {
            $('#popup-country-list').focus();
        };

        $(document).ready(function()
        {
            OUApp.Modules.modal.openModal('#country_selection_dialog');
            
            $(document).focusin(function(e)
            {
                setPopupFocus();
                e.preventDefault();
            });

            $('#popup-country-form').focusin(function(e)
            {   
                e.stopPropagation();
            });

            $('.js-close-country').click(function(){
                $('.js-popup-country-inferred-flag').val('true');
                $('#popup-country-form').submit();
            });

        });
    </script></body>
