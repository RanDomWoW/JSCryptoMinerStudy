<script>
$(document).ready(function(){
  var chamada = $.ajax({
    url: "../informes/index_informes.json",
    type: "GET",
    dataType: "json",
    headers: {
      Accept: "application/json"
    } 
  });

  $.when(chamada).done(function(data, status, jqXHR){

    var index = [];
    var aux = [];
    for(var i=0; i<15; i++){
      if(data[i][2]){
        index.push(data[i]);
      }
    }      

    for(var j=0; j < index.length-1; j++){
      for(var k=j+1; k < index.length; k++){
        if(index[k][2] > index[j][2]){
          aux = index[j];
          index[j] = index[k];
          index[k] = aux;
        }
      }
    }

    for(var l in index){
      $('#listaNoticias').prepend('<li><span class="dataNoticia">'+index[l][0]+'</span> - '+index[l][1]+'</li>');
    }
  });
});
</script>

