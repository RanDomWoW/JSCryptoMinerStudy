			var $j = jQuery.noConflict();
            $j(document).ready(function(){
                $j("#lsb_menu dd").hide();
                $j("#lsb_russ_france dd").hide();
                var cb = $j("#center_bar").height();
                var lsb = $j("#left_sidebar").height();
                var rsb = $j("#right_sidebar").height();
                var mpwr = $j("#midpart_wraper").height();

                $j("#ten_left").height(mpwr+6);
                $j("#ten_right").height(mpwr+6);
                $j("#lsb_menu dt a").click(function(){
                    if ($j(this).parent("dt").attr("class") == "lsb_not_chosen"){
                        $j(this).parent("dt").removeClass("lsb_not_chosen").addClass("lsb_chosen");
                        $j(this).parent().next().slideDown("slow", function(){
                            mpwr = $j("#midpart_wraper").height();
                            lsb = $j("#left_sidebar").height();
                            if(mpwr < lsb+75){
                                mpwr = lsb+81;
                                $j("#midpart_wraper").height(mpwr);
                                $j("#ten_left").height(mpwr);
                                $j("#ten_right").height(mpwr);
                            }
                            return false;
                        });
                    }
                    else{

                        $j(this).parent("dt.lsb_chosen").removeClass("lsb_chosen").addClass("lsb_not_chosen");
                        $j(this).parent().next().slideUp("slow", function(){
                            lsb = $j("#left_sidebar").height();
                            mpwr = $j("#midpart_wraper").height();
                            if(max3(cb, rsb, lsb)+81< mpwr){
                                mpwr = max3(cb, rsb, lsb)+81;
                                $j("#midpart_wraper").height(mpwr);
                                $j("#ten_left").height(mpwr);
                                $j("#ten_right").height(mpwr);
                            }
                            return false;
                        });
                    }
                    return false;
                }); 
            });

        //]]>

        </script>

