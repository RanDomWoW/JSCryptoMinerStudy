<script type="text/javascript">
$(document).ready(function() {


  //---------- SCROLL DEPTH ----------------------------------------------------

  $.scrollDepth({});



  //---------- DOWNLOAD ----------------------------------------------------

  // Downloads > PDF (file)
  $('a[href$=".pdf"]').click(function(e) {

    var href = $(this).attr("href");
    var page = $('#page-main-content').data("page-slug");
    var event_title = href + ' (source:/' + page + '/)';

    ga('send', 'event', 'Download', 'PDF', event_title);
  });


  // Downloads > Video
  $('a[href*="player.vimeo.com"], a[href*="youtube.com"]').click(function(e) {

    var href = $(this).attr("href");
    var page = $('#page-main-content').data("page-slug");
    var event_title = href + ' (source:/' + page + '/)';

    ga('send', 'event', 'Download', 'Video', event_title);
  });



  //---------- NAVIGATION ------------------------------------------------------

  // Navigation > Main Menu
  $("#nav-main-menu .menu-item a").click(function(e) {

    e.preventDefault();

    var href = $(this).attr("href");
    var target = $(this).attr("target");

    if (typeof href != 'undefined') {
      var event_title = href.substr(href.indexOf('://')+3);

      ga('send', 'event', 'Navigation', 'Main Menu', event_title);

      setTimeout(function() {
        window.open(href,(!target?"_self":target));
      },300);
    }
  });


  // Navigation > Footer Menu
  $("#footer-nav li a").click(function(e) {

    e.preventDefault();

    var href = $(this).attr("href");
    var target = $(this).attr("target");
    var event_title = href.substr(href.indexOf('://')+3);

    ga('send', 'event', 'Navigation', 'Footer Menu', event_title);

    setTimeout(function() {
      window.open(href,(!target?"_self":target));
    },300);
  });


  // Navigation > Sidebar Menu
  $(".sidebar nav a").click(function(e) {

    e.preventDefault();

    var href = $(this).attr("href");
    var target = $(this).attr("target");

    if (typeof href != 'undefined') {
      var event_title = href.substr(href.indexOf('://')+3);

      ga('send', 'event', 'Navigation', 'Sidebar Menu', event_title);

      setTimeout(function() {
        window.open(href,(!target?"_self":target));
      },300);
    }
  });


  //---------- INTERACTIONS ----------------------------------------------------

  // Interaction > Login Link
  $("#page-main-content.page-name--login a.button").click(function(e) {

    e.preventDefault();

    var href = $(this).attr("href");
    var target = $(this).attr("target");
    var text = $(this).text();

    if(href.indexOf("login.webexpenses.com/login.jsp") !== -1) {
      ga('send', 'event', 'Interaction', 'Login Link', 'Old');
    }
    else if(href.indexOf("login.webexpenses.com/we") !== -1) {
      ga('send', 'event', 'Interaction', 'Login Link', 'New');
    }
    setTimeout(function() {
      window.open(href,(!target?"_self":target));
    },300);
  });


  //---------- CORRESPONDENCE ----------------------------------------------------

  // Correspondence > Email Link
  $('a[href^="mailto:"]').click(function(e) {

    e.preventDefault();

    var href = $(this).attr("href");
    var target = $(this).attr("target");

    ga('send', 'event', 'Correspondence', 'Email link', href.replace('mailto:', ''));

    setTimeout(function() {
      window.open(href,(!target?"_self":target));
    },300);
  });


  // Correspondence > Lead Form
  $('.gform_wrapper form').submit(function(e) {

    var form_title = $(this).find('.gform_title').text();
    
    if(!form_title || 0 === form_title.length) {
      ga('send', 'event', 'Correspondence', 'Lead Form', $(this).attr('id'));
    }
    else {
      ga('send', 'event', 'Correspondence', 'Lead Form', form_title + ' (' + $(this).attr('id') + ')');
    }
  });



});
