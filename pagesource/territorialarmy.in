         html  +='<img class="grey-rs" src="images/rs.png" align="absmiddle"><span class="book-discount">' +$discount+ '</span>';
         }
         if($rs != null){
         html  +='<img class="red-rs" src="images/rs.png" align="absmiddle"><span class="book-rs">' +$rs+ '</span>';
         }
         if($off != null){
         html  +='<span class="book-rs-off">' +$off+ '</span>';
         }
         html  +='</Div>';                  
         if($buynow !=null){
        html += '<Div class="book-buynow">' + $buynow + '</Div>';
        }
                                                              
         html  +='</Div>';
         if(($i+1)%3!=0){
         html  +='<Div class="book-vline"></Div>';
         }
         html += '<Div class="book-status" style="display:none;" >' + $(element).find('.book-status').html() + '</Div>';
          if($desc != null){
        html += '<Div class="book-description" style="display:none;">' + $desc + '</Div>';
        html += '<Div class="book-readmore" style="display:none;">' + $(element).find('.book-readmore').html() + '</Div>';
        }
        html  +='<div class="view-book-name" style="display:none;">' + $(element).find('.view-book-name').html() + '</div>';
			$(element).html(html);
			$i++;
		});
		$('#list-buttons').html('<a  onclick="'+$list+'"><span id="sort-list-inactive">List View</span></a><span id="sort-grid-active">Grid View</span>');
		usefacebox();
		$.cookie('display', 'grid');
		}	
	}
view = $.cookie('display');

if (view) {
	display(view);
} else {
	display('grid');
}
function usefacebox(){
jQuery(document).ready(function($) {
      $('a[rel*=facebox]').facebox({
        loadingImage : 'images/loading.gif',
        closeImage   : 'images/closelabel.png',
      })
    })
}
