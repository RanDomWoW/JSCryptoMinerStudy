    <script>
      //@code_start
      $(function(){
        $('#world-map-gdp').vectorMap({
          map: 'world_mill_en',
          series: {
            regions: [{
              values: gdpData,
              scale: ['#C8EEFF', '#0071A4'],
              normalizeFunction: 'polynomial'
            }]
          },

　　　　onRegionOver: function(event, code){　
          if(gdpData[code]==null) {}　else{
　　　　　　　　　　$('#world-map-gdp').css({cursor:'pointer'});　　
		  }
　　　　},
　　　　onRegionOut: function(event, code){
　　　　　　　　　　$('#world-map-gdp').css({cursor:'auto'});
　　　　},

　　　　onMarkerOver: function(event, code){　　
　　　　　　　　　　$('#world-map-gdp').css({cursor:'pointer'});　　
　　　　},
　　　　onMarkerOut: function(event, code){
　　　　　　　　　　$('#world-map-gdp').css({cursor:'auto'});
　　　　},

          onRegionLabelShow: function(e, el, code){
			if(gdpData[code]==null) {}
			else if(code=='TW'){el.html(el.html()+', China<br/>7 Universities in ARWU Top 500');}
			else if(code=='CN'){el.html(el.html()+'<br/>45 Universities in ARWU Top 500');}
			else{
            el.html(el.html()+'<br/>'+gdpData[code]+'  Universities in ARWU Top 500');
			}
          },
		  onRegionClick: function(event, code){
             if(gdpData[code]==null) {}else{
			 var map = $('#world-map-gdp').vectorMap('get', 'mapObject');
             window.location.href = 'World-University-Rankings-2017/'+ map.getRegionName(code).replace(/Taiwan/, "China-tw").replace(/United States of America/, "USA").replace(/United Kingdom/, "UK").replace(/Republic of /, "").replace(/ Republic/, "").replace(/ /, "-")+'.html';
			 }
　　　　}

        });
      });
      //@code_end
    </script>

