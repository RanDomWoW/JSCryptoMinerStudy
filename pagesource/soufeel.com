<script type="text/javascript">
    	jQuery(function($) {
			var check_cookie = $.cookie('header_notice');
			var notice = $('#header_notice_div');
		//search top;
		if(notice.css("display") == "block"){
			$(".tsearch").addClass("notice-search-top");
		}else{
			$(".tsearch").removeClass("notice-search-top");
		}
			if(window.location!=window.parent.location){
				notice.remove();
			}else{
				if(check_cookie == null || check_cookie == 'shown') {
					setTimeout(function(){
						notice.show();
					}, 3000);
				}
				$('#header_notice_close').on('click', function(){
					$(".tsearch").removeClass("notice-search-top");
					var cookietime = new Date(); 
					//coockie save on hour ,default is day;
					cookietime.setTime(cookietime.getTime() + (2 * 60 * 60 * 1000));
					//set time  if is null， brower cloese is expires set null;
					$.cookie('header_notice','dontshowitagain',{path:'/', expires:cookietime});
					notice.slideUp("600");
					
				});
			}
		});
		
	</script>

