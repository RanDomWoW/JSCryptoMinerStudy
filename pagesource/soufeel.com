<script type="text/javascript">
var exclamation = '<i class="photofont icon photo-jingshi"></i>';
function subCheckForm() {
    email = jQuery("#newsletter_footer").val();    
    var checkForm = true;
    if(email == 'Enter Your Email Here') {
        jQuery("#sub_error_email").html(exclamation+'Please enter your email address.');
        jQuery("#sub_error_email").show();
        checkForm = false;
    } else if(!validateEmail(email)) {
        jQuery("#sub_error_email").html(exclamation+"Please enter a valid email.");
        jQuery("#sub_error_email").show();
        checkForm = false;
    }
    
    if(!checkForm) {
        return false;
    } else {        
       return  subAjaxAutoRegister(email);
    }
}
function validateEmail(email){
    var reg = /^[\w-]+(\.[\w-]+)*@[\w-]+(\.[\w-]+)+$/ ;
    return reg.test(email);
}    

function subAjaxAutoRegister(email){    
    jQuery.ajax({
        type: "POST",
        url : '/mart/customer_account/autoRegister',
        data: {
            email : email,
        },
        async: true,
        timeout: 120000,
        beforeSend: function(){                        
            jQuery("#footer-newsletter-validate-detail .button").attr({disabled: "disabled"});
            jQuery("#footer-newsletter-validate-detail .button").addClass('sub-submiting-jack');
            jQuery("#footer-newsletter-validate-detail .button span span").text("WAITING...");
            jQuery('#loading-mask').show();
        },
        complete: function () {            
             jQuery("#footer-newsletter-validate-detail .button").removeAttr("disabled");
             jQuery("#footer-newsletter-validate-detail .button").removeClass('sub-submiting-jack');
             jQuery("#footer-newsletter-validate-detail .button  span span").text('Get My Discount');
            jQuery('#loading-mask').hide();
        },
        success: function(response, textStatus) {
            var obj = jQuery.parseJSON(response);
            if (obj.status == 1) {
                jQuery("#footer-newsletter-validate-detail .button").removeClass('sub-submiting-jack');
                var msg = "You've Successfully Registered!<br>The coupons have been put into your account. Please find your account info in your email box. Remember to login first to use the coupons!";
                jQuery("#sub_error_email").html(msg);
                jQuery("#sub_error_email").show();
                return false;
            }else {
                jQuery("#sub_error_email").html(exclamation+obj.message);
                jQuery("#sub_error_email").show();
            }
            return false;
        }
    });         
    return false;
}
</script></div>
                
                                
