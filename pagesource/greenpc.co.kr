    $(".ssm_wrap").on("mouseover", ".ssm_glitm", function() {
        $(this).find(".ssm_gbx").css("display", "block");
        $(this).find(".ssm_gbx_del").css("display", "block");
    }).on("mouseout", ".ssm_glitm", function() {
        $(this).find(".ssm_gbx").css("display", "none");
        $(this).find(".ssm_gbx_del").css("display", "none");
    });

    $(".ssm_wrap").on("click", ".ssm_fav_close", function() {
        $(".ssm_wrap .ssm_frm .ssm_fav_box").stop();
        $(".ssm_wrap .ssm_frm .ssm_fav_box").fadeOut(300);
    }).on("click", ".ssm_fav_del", function() {
        var strBrand = $(this).attr("strBrand");
        var nFavCnt  = parseInt($(".ssm_is_favorite .ssm_badge em").html());
        $(this).parent(".brd_itm").remove();
        $(".ssm_is_favorite .ssm_badge em").html(nFavCnt-1);
        $.ajax({
            url: "/common/ajax/favor/brandFavorAjax.html?f_mode=D&f_bno=" + strBrand,
            dataType: "json",
            success: function(objRes) {
                strResult   = objRes.strResult;
                strMsg      = objRes.strMsg;
                strMsgErr   = objRes.strMsgErr;
                if (strResult == "SUCCESS") {
                    alert(strMsg);
                } else {
                    alert(strMsgErr);
                }
            }
        });
    });    

    $(".ssm_wrap").on("click", "#ssm_banner .btn_close", function(e) {
        e.preventDefault();

        var dateToday = new Date(); 
        dateToday.setHours(dateToday.getHours() +  24);
        
        $.jQKSetCookie("ck_ssmbnr", "1", dateToday, "/");
        $("#ssm_banner").animate({"bottom":"-110px"}, 500, "swing", function() {
            $(this).css("display", "none");
        });
    });

    $(".ssm_wrap").on("mouseover", ".ssm_fav_box .brd_box .brd_lst .brd_itm", function() {
        $(this).find(".ssm_fav_del").css("display","inline-block");
    }).on("mouseout", ".ssm_fav_box .brd_box .brd_lst .brd_itm", function() {
        $(this).find(".ssm_fav_del").css("display","none");
    });

    $(window).on('resize', function() {
        var nSWidth  = $('body').prop('scrollWidth');

        if (nSWidth < 1086) {
            $(".ssm_wrap .ssm_frm").fadeOut(100);
        } else {
            $(".ssm_wrap .ssm_frm").fadeIn(100);
        }
    });
});
