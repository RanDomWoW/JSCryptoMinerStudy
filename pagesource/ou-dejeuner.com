<script type="text/javascript">
    $(document).ready(function() {
        initCovers();
        $("#main-search").tokenInput("/ajax/home-search", {
            hintText: 'Entrez un mot-clé (restaurant, type de restaurant...)',
            noResultsText: 'aucun résultat',
            placeholderText: 'Je saisis la VILLE ou le CODE POSTAL',
            searchingText: '<i><img src="/www/images/mini-loader.gif" alt=""></i>',
            resultsFormatter: function(item) { return '<li><a href="' + item.url + '">' + item.name + '</a></li>' },
            minChars: 2,
            tokenLimit: 1,
            method: 'post'
        });
        
        $("#main-search-form").submit(function(event) {
            if ($("#main-search").tokenInput("get").length == 0) {
                event.preventDefault();
            }
        });

        function initCovers(){
            // list images cover 
            var arrSlider = ['cover-1.jpg', 'cover-2.jpg', 'cover-3.jpg', 'cover-4.jpg'];
            var urlImages = "/www/images/home-covers/desktop/";
            var widthImg = 1920,
                heightImg = 520;

            if( $(window).width() < 639) {
                urlImages = "/www/images/home-covers/mobile/";
                widthImg = 1024;
                heightImg = 430;

            }else if ( $(window).width() > 639 && $(window).width() <= 1024 ) {
                urlImages = "/www/images/home-covers/tablette/";
                widthImg = 1024;
                heightImg = 430;
            }
            
            var htmlSlider = "<ul>";
            for(var i=0; i<arrSlider.length-1; i++){
                htmlSlider += ' <li style="background-image: url('+urlImages+''+arrSlider[i]+')"> ';
                htmlSlider += ' <img src="'+urlImages+''+arrSlider[i]+'" alt="" width="'+widthImg+'" height="'+heightImg+'" /> ';
                htmlSlider += ' </li> ';
            }
            htmlSlider += '</ul>';
            $('#images-covers').append(htmlSlider);

            if($('#images-covers ul').length )
            var sliderCover =  $('#images-covers ul').bxSlider({
                mode: 'fade',
                preloadImages: 'visible',
                controls: false,
                pager: false,
                pause: 10000,
                speed: 800,
                onSliderLoad: function(){
                    setTimeout(function(){sliderCover.startAuto();},1000);
                }
            });
        }
    });
</script>
