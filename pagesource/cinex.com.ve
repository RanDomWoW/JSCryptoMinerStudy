<script type="text/javascript" language="javascript">

		function scrollbar_width() {
		    if( $('body').height() > $(window).height()) {
		         
		        /* Modified from: http://jdsharp.us/$/minute/calculate-scrollbar-width.php */
		        var calculation_content = $('<div style="width:50px;height:50px;overflow:hidden;position:absolute;top:-200px;left:-200px;"><div style="height:100px;"></div>');
		        $('body').append( calculation_content );
		        var width_one = $('div', calculation_content).innerWidth();
		        calculation_content.css('overflow-y', 'scroll');
		        var width_two = $('div', calculation_content).innerWidth();
		        $(calculation_content).remove();
		        return ( width_one - width_two );
		    }
		    return 0;
		}

		var wscr = $(window).width() + scrollbar_width();
		var hscr = $(window).height();
		var middle = (hscr/2) - 122;
		$('#registro-pop-slider').css("top",  middle+"px");
		$('#registro-div-content').css("top",  middle+"px");
		$('#registro-div-slider').css("width",  wscr+"px");
		$('#registro-div-slider').css("height", hscr+"px");
		$('#registro-div-slider').css("right",  wscr+"px");
		
		$(window).resize(function() {
			var wscr = $(window).width() + scrollbar_width();
			var hscr = $(window).height();
			var middle = (hscr/2) - 122;
			$("#registro-div-slider").hide();	
			$('#registro-div-slider').css("width",  (wscr)+"px");
			$('#registro-div-slider').css("height", hscr+"px");
			$('#registro-div-slider').css("right",  wscr+"px");
			$('#registro-pop-slider').css("right", "0px");
			$('#registro-pop-slider').css("top",  middle+"px");
			$('#registro-div-content').css("top",  middle+"px");					
		});		

		$('#registro-pop-slider').click(function(){
			// obtenemos ancho y alto de la ventana del explorer
			var wscr = $(window).width() + scrollbar_width();
			var hscr = $(window).height();				
			$('#registro-pop-slider').animate({right: wscr+"px"}, 1500);	
			$('#registro-div-slider').show();		
			$('#registro-div-slider').css("right",  "-"+(wscr)+"px");	
			$('#registro-div-slider').show();	
			$('#registro-div-slider').animate({right: 0+"px"}, 1500);				
		});

		$('#registro-div-slider').click(function(){		
			var wscr = $(window).width() + scrollbar_width();
			var hscr = $(window).height();
			$('#registro-pop-slider').animate({right: "0px"}, 1500);	
			$('#registro-div-slider').animate({right: "-"+wscr+"px"}, 1500, function(){
	             $("#registro-div-slider").hide();
	        });			
		});
</script>
