 * comboDefault - String - Opción por Default del combo
 * comboValues - Array - Array con objetos json (itemValue, itemText)
 * comboIndex - String - Indice de la opción seleccionada del combo (si aplica)
 **/
function generar_combo(comboID, comboDefault, comboValues,  comboIndex)
{
	var _selected = '';
	var opciones = '<option value="">'+comboDefault+'</option>';
	var comboSelected = comboDefault;
	for(var i in comboValues) {
		if(typeof(comboIndex) != 'undefined' && comboIndex != null)
		{
			if(comboValues[i]['itemValue'] == comboIndex) 
			{ 
				comboSelected = comboValues[i]['itemText'];
				_selected = 'selected="selected"';
			}
		}
		opciones += '<option value="' + comboValues[i]['itemValue'] + '"' + _selected + ' >';
		opciones += comboValues[i]['itemText'];
		opciones += '</option>';
		_selected = '';
	}
	$(comboID).next().html('<span class="customSelectInner" style="width: 158px; display: inline-block; height:43px;">' + comboSelected + '</span>'); 
	$(comboID).html(opciones);
}
