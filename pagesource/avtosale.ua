            $(".search_action .mainButton").click(function() {
                var arr =[];
                arr =$('#oldform').serialize();
                arr = arr.replace(/&/g,";");
                temp = arr.split(';');

                var some_url = '';
                var hashAr = {};

                for(var i=0; i<temp.length; i++) {
                    var result = temp[i].split('=');
                    hashAr[result[0]] = result[1];
                    if(result[0]=='priceFrom' || result[0]=='priceTo') {
                        if(parseInt(result[1])!=result[1]) {
                            result[1]='';
                        }
                    }
                    if(result[0]!="event" && result[0]!="_page" && result[0]!="ajax" && result[1]!=undefined && result[1]!='') {
                        some_url += result[0] + "=" + result[1] + "&";
                    }
                }
                if(some_url.charAt(some_url.length-1)=="&") {
                    some_url = some_url.substring(0, some_url.length - 1);
                }
                var typeOfCar = 'car';
                ($("#CarAdvertType").is(":checked")==true)?typeOfCar = 'new_car':typeOfCar = 'car';

                formFlag=false;
                $.each($("#oldform").serializeArray(), function(key, val){
                    if(val['name']!='Perform' && val['name']!='CarAdvertType') {
                        if(val['value']!='') {
                            formFlag=true;
                        }
                    }
                });
            })
        }
