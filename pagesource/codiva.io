<script>
      function clickHandlerSetFocus(modalDialogId, focusedElementId) {
          var textBox = document.getElementById(focusedElementId);
          var modalDialog = document.getElementById(modalDialogId);
          
          var opacity = window.getComputedStyle(modalDialog).getPropertyValue("opacity");
          console.log(opacity);
          if (textBox && opacity > 0.05) {
            textBox.focus();
            return true;
          } else {
            setTimeout(function() {clickHandlerSetFocus(modalDialogId, focusedElementId)}, 100);
          }
          return true;
      }
      function addHandlerToSetFocus(linkId, modalDialogId, focusedElementId) {
        var link = document.getElementById(linkId);
        if (!link) {
          return;
        }

        link.onclick = clickHandlerSetFocus.bind(this, modalDialogId, focusedElementId);
      }

      addHandlerToSetFocus("registerLink", "registerModal", "register-email");
      addHandlerToSetFocus("registerFallback", "registerModal", "register-email");
      addHandlerToSetFocus("registerLinkMain", "registerModal", "register-email");
      addHandlerToSetFocus("signinLink", "signinModal", "login-email");
      addHandlerToSetFocus("signinFallback", "signinModal", "login-email");

    </script>
