<script type="text/javascript">
	$(document).ready(function () {
		var pageWidth = $(document).width();

		if (pageWidth > 987) {
			//Fix Menubar on scroll
			//var headerOffset = 0;
			var isHidden1 = false;
			var isHidden3 = false;
			var scrollTop = $(document).scrollTop();
			var top1offset = $('#top1').offset().top;
			var top3offset = $('#top3').offset().top;
			var distTop1 = (top1offset - scrollTop);
			var distTop3 = (top3offset - scrollTop);

			$(document).scroll(function () {
				if ($(document).width() > 987) {
					scrollTop = $(document).scrollTop();
					//headerOffset = $('header').offset().top; // - $('header').outerHeight(true); // - $('#top3').outerHeight(false);
					if (!isHidden1) top1offset = $('#top1').offset().top;
					if (!isHidden3) top3offset = $('#top3').offset().top;
					distTop1 = (top1offset - scrollTop);
					distTop3 = (top3offset - scrollTop - $('#top1').outerHeight(false) - $('#top3').outerHeight(false) + 40);

					if (distTop1 < 0 && !isHidden1) {
						if (!$("#top1").hasClass("fixed")) {
							$("#top1").addClass("fixed").addClass("box");
							$("#top2").addClass("mt34");
						}
						isHidden1 = true;
					}
					else if (distTop1 >= 0 && isHidden1) {
						if ($("#top1").hasClass("fixed")) {
							$("#top1").removeClass("fixed").removeClass("box");
							$("#top2").removeClass("mt34");
						}
						isHidden1 = false;
					}

					if (distTop3 < 0 && !isHidden3) {
						if (!$("#top3").hasClass("fixed")) {
							$("#top3").addClass("fixed");
							$("body").addClass("pt40");
						}
						isHidden3 = true;
					}
					else if (distTop3 >= 0 && isHidden3) {
						if ($("#top3").hasClass("fixed")) {
							$("#top3").removeClass("fixed").removeClass("box");
							$("body").removeClass("pt40");
						}
						isHidden3 = false;
					}

					if ($("#txtHintBig")) { //Pferdesuche
						if (isHidden1 && isHidden3)
							if (distTop3 + 400 < 0)
								$("#txtHintBig").addClass("fixed");
							else
								$("#txtHintBig").removeClass("fixed");
					}
				}
			});
		}

		$(".mip .subMenu").mouseenter(function () { $(this).parent().find(".menuItem").first().addClass("active2"); })
		$(".mip .subMenu").mouseleave(function () { $(this).parent().find(".menuItem").first().removeClass("active2"); })

		// move header etc.
		$('#navi').click(function () {
			$('header').toggleClass('moveHeader');
			$('#top3').toggleClass('moveNav');
			$('#main-container, footer').toggleClass('moveCont');
			$('body').toggleClass('scrollBlocked');
		});
	});

	// exchanged the user logo if an error occurred
	function imgError(userlogo) {
		$(userlogo).remove();
		$('.userlogoHidden').addClass("userlogo").removeClass("userlogoHidden");
	}

	//toggle menu items for the mobile view
	function toggleMenuItem(item, obj) {
		$('.' + item).slideToggle();
		obj.text == "b" ? obj.text = "e" : obj.text = "b";
	}
</script>


