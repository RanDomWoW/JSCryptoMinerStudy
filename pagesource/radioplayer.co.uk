			/* request intialisation of radioplayer cookie */
			function cookieInit() {
				$.ajax({
					url: "http://cookie.radioplayer.co.uk/cm/all/?callback=callback",
					cache: "false",
					dataType: "script",
				});
			}
			function callback(data) {
				 var date = new Date();
				 var minutes = 1;
				 date.setTime(date.getTime() + (minutes * 60 * 1000));
				 cookie('rpcookie', JSON.stringify(data), { expires: date, path : "/" });

				 // Force the page to reload, but tell php we've already done this so to avoid
				 // loops in case the user isn't using cookies
