         <script type='text/javascript'>
			var cookie = $.proxy( $.cookie, this );
			var removeCookie = $.proxy( $.removeCookie, this );

			function openPlayer(url) {
				window.open(url, 'UKRadioPlayerPopup', 'toolbar=0,scrollbars=0,location=0,statusbar=0,menubar=0,resizable=0,width=380,height=660');
