        <script>
            $(document).ready(function(){

                $('.home-network-box').on('click', function(){

                    $('.home-landing-networks .container-flex > div ').each(function(){
                        $(this).find('.home-network-box').removeClass('active');
                    })
                    $(this).addClass('active');
                    var blogname = $(this).attr('id');
                    var blog_id = $('#' + blogname + '-blog-id').val();

//                    if(blogname != 'marketing' && blogname != 'intel'){
//                        return false;
//                    }
                    $.ajax({
                        type: "POST",
                        dataType: 'json',
                        url: "/wp-admin/admin-ajax.php", // our PHP handler file
                        data: {
                            action: 'load_network_top_stories',
                            blog_id: blog_id,
                            blog_name: blogname,
                        },
                        success: function (results) {

                            $("#network-latest-posts-global").empty();
                            $("#network-latest-posts-global").append(results.html).show('slow');

                            /* Remove the current border class and add the new */
                            $("#posts-container").removeClass(function (index, css) {
                                return (css.match (/\b\S+-border-20/g) || []).join(' ');
                            });
                            $("#posts-container").addClass(blogname + "-border-20");
                            $("#blog-visit").addClass(blogname + "-border-20");

                            /* Remove the current visit site link and add the new one - selected */
                            $("#blog-visit-link").empty();
                            $("#blog-visit-link").append(results.blog_link);

                        }


                    });

                });

//        Do Ajax on load html
                var blogname = 'intel';
                $.ajax({
                    type: "POST",
                    dataType: 'json',
                    url: "/wp-admin/admin-ajax.php", // our PHP handler file
                    data: {
                        action: 'load_network_top_stories',
                        blog_id: 2,
                        blog_name: blogname
                    },
                    success: function (results) {
                        $("#network-latest-posts-global").empty();
                        $("#network-latest-posts-global").append(results.html).show('slow');

                        /* Remove the current border class and add the new */
                        $("#posts-container").removeClass(function (index, css) {
                            return (css.match (/\b\S+-border-20/g) || []).join(' ');
                        });
                        $("#posts-container").addClass(blogname + "-border-20");
                        $("#blog-visit").addClass(blogname + "-border-20");

                        /* Remove the current visit site link and add the new one - selected */
                        $("#blog-visit-link").empty();
                        $("#blog-visit-link").append(results.blog_link);
                    }
                });
            });
        </script>


