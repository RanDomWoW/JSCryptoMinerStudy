function SearchTypeEdit(dom){
	if($(dom).val() == '2')
	{
		$("#searchForm").attr('action','BuyList.php?id=1');
	}
	else if($(dom).val() == '3')
	{
		$("#searchForm").attr('action','BuyList.php?id=2');
