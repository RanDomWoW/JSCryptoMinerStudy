    <script>
        function sendOpinionMain(){
            $("#main_opinion_form_text_error").html('');
            $("#main_opinion_form_danger").hide();
            if ($('#main_broker_id').val() == ''){$("#main_opinion_form_text_error").html('Заполните поле Брокер');$("#main_opinion_form_danger").show();return false;}
                        if ($('#main_opinion_name').val() == ''){$("#main_opinion_form_text_error").html('Заполните поле Имя');$("#main_opinion_form_danger").show();return false;}
            if ($('#main_opinion_email').val() == '' || !checkmail($('#main_opinion_email').val())){$("#main_opinion_form_text_error").html('Заполните поле Email');$("#main_opinion_form_danger").show();return false;}
                        if ($('#main_opinion_topic').val() == ''){$("#main_opinion_form_text_error").html('Заполните поле Заголовок');$("#main_opinion_form_danger").show();return false;}
            if ($('#main_opinion_message').val() == ''){$("#main_opinion_form_text_error").html('Заполните поле Сообщение');$("#main_opinion_form_danger").show();return false;}
            if ($('#opinion_broker_1').val() == '0'){$("#main_opinion_form_text_error").html('Заполните поле Качество обслуживания');$("#main_opinion_form_danger").show();return false;}
            if ($('#opinion_broker_3').val() == '0'){$("#main_opinion_form_text_error").html('Заполните поле Технические характеристики');$("#main_opinion_form_danger").show();return false;}
                        if (!$("#main_opinion_accept").prop("checked")){$("#main_opinion_form_text_error").html('Вам необходимо согласиться с пользовательским соглашением');$("#main_opinion_form_danger").show();return false;}
                        $("#main_broker_opinion_spin").show();$("#btn_main_broker_opinion_form").addClass('disabled');
            var data=$("#main_opinionForm").serialize();
            $.ajax({
                type: 'POST',
                url: '/brokers/addopinionmain/',
                data:data,
                success:function(data){$("#div_form_opinion_main").html(data);},
                error: function(data) {},dataType:'html'
            });
            return true;
        }
    </script>
