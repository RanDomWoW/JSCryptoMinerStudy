	@charset "utf-8";

.cf::after {
	content: "";
	display: table;
	clear: both;
}
/*　記事詳細　*/
.singleSubtitle{float: right;}

h1.title {line-height:32px;font-size:24px;/*margin:15px 0;*/border-bottom:0px solid #000}
h2 {line-height:32px;font-size:24px;/*margin:15px 0;*/border-bottom:0px solid #000}

.cliplistHead { font-size: 16px; color: #5B1D1E; border-bottom: 3px solid #5B1D1E; border-left: 10px solid #FED6D6; }

.postTitle {border-bottom:4px solid #000;width: 100%;margin-bottom: 25px;}
.postTitle .singleView{margin-top: 5px; padding-left:20px;font-size:11px;line-height:24px;background:url(/wp-content/themes/toretameVer1.0/images/viewIcon.png) left 0 no-repeat;float: left;}

.clipArea {float: left;width:142px;margin-left: 10px;}
.clipArea img{width:100px;float: left;}
.clipArea .clip_Count{width:40px;height: 35px; background:url(/wp-content/themes/toretameVer1.0/images/clip_count_bg.png) left 0 no-repeat; float: left;margin-top: -5px;padding:9px 0 5px 3px;text-align:center;}

/*　クリップリスト */
.clip_list {width: 610px;color: #666;}
.clip_list .caption {margin-top:10px; color: #666; font-size: 14px;float: right; }
.float_r{border-radius: 3px;height: 30px; width: 150px; font-size: 12px; color:  #747372; border:1px solid #CCC; display: block;}
.clip_list .box{margin-left: 10px;line-height: 1.8em; font-weight: bold; font-size: 14px; }
.clip_list .box_left{width:32.33%;float: left;position: relative; text-align: center; margin-top: 15px;}
.clip_list .clip_arrow { position: absolute; top: 30px; left: 10px; color: #C8C8C8;}
.clip_list .box_left .view {position: absolute; top: 2px;left: 28px; color:#fff; cursor: pointer;}
.clip_list .box_left .view:hover{ opacity: 0.9 }
.clip_list .box_left img{width: 75px; height: 75px; object-fit: cover;}
.clip_list .box_right{width:66.666%;float: left;}
.clip_list .box_right h3 {margin-bottom: 10px;font-size: 16px;line-height:2.0rem}
.clip_list .box_right p {font-size: 12px;line-height: 1.6em;text-align:left;}
.corporationArticle.clip_list h3{margin:0 0 10px;padding:18px 0 10px;text-align:left;font-size:18px;color:#000;font-weight:700;background:none;}
.btn_edit {width:70px;height:auto;}
/*Icon*/
.icn_comment em img,.icn_clip em img {width:auto;height:16px;margin-right:2px;}

.clip_list.sp {width: 414px;color: #666;}
.clip_list .box_right{width:74%;float: left;position: relative;}
.clip_list.sp .box_right h3 {margin: 0px;font-size: 16px;}
.clip_list.sp .box_right p {font-size: 12px;line-height: 1.6em;}
.clip_list .box_left{width:22%;float: left;position: relative;margin-right: 1%;}
.clip_list .box_left .clip {position: absolute; top: 0px;left: 0px; padding:5px;}
.clip_list .box_left .clip img {width: 35%;}
/* .clip_list .delete {position:absolute;right:0;bottom: 0; height:25px;width: auto} */
/*Clip List Add*/
.modalBody {overflow-y: scroll;}
.clip_list_new {width:100%;max-width:960px;margin-left: auto;margin-right: auto;height:100%}
.clip_list_new h3 {color: #666; margin: 0; font-weight: bold;}
.clip_list_new h3 .btn {margin-left:10px; vertical-align:middle;}
.clip_list_new .title{font-size:16px;font-weight:bold;}
.clip_list_new .colmun{float:left;width: 48%;margin-left: 1%;margin-right: 1%;height:220px;overflow: hidden;}
.clip_list_new.sp .colmun{float:left;width: 98%;margin-left: 1%;margin-right: 1%;}
.clip_list_new .l_c{float:left;width:56%; overflow: hidden; height:160px;}
.clip_list_new .r_c{float:left;width:43%; height: 75px; overflow: hidden;}
.clip_list_new .r_c:nth-child(odd){margin-top: 10px;}
.clip_list_title {margin-top:10px;display: block;text-align: right;font-size: 12px;color: #666;font-weight: bold;display: block; }
.modal-bg {height:100%; background-color: black;position: absolute;top: 0;}
html,body {height: 100%}

/*Form*/
.clip_list_form{padding:10px; width: 350px;color: #666;}
.clip_list_form .logo {margin-left: auto;margin-right: auto;width: 275px;margin-bottom: 15px;}
.clip_list_form h4 {font-weight: bold;margin-bottom: 10px;}
.clip_list_form p{font-size:12px;}
.clip_list_form .form_style{width: 100%;}
.clip_list_form .radio_box {float:right;}
.clip_list_form .btn_add {width:100%; margin-top: 25px;}
.clip_list_form .btn_add img {width:100%;}

hr { border-top: 1px dashed #FEBE34; margin: 0;}

.corporationArticle h3 { border-bottom: none;}
.corporationArticle.clip_list h3 {  font-size: 15px; }

#user_clip { margin-bottom: 30px; }
</style>


