    <script type="text/javascript">

	var numPag = 0;
	var ACT_ID_PAGINA = 0;

	//funzione di caricamento degli annunci e dei bottoni per il paginate
	function loadAnnunci(actId, paginate, limit){

		if(actId=='')
			actId=0;
		if(paginate=='')
			paginate=0;
		if(limit=='')
			limit=0;

		$.ajax({
			type: "POST",
			url: "/json/annunci.php",
			data:{
				ACTION: 'getAnnunci',
				PAGINATE: paginate,
				ACT_ID: actId,
				LIMIT: limit
			},
			dataType: "json",
			success: function(e) {
				if(e.success===true) {
					ACT_ID_PAGINA = actId;
					//console.log(ACT_ID_PAGINA);
					if(location.pathname.substring(1)!="altri-servizi/"+actId && actId > 0) {
						//console.log(location.pathname.substring(1));
						//ACT_ID_PAGINA = actId;
						window.location.replace("/altri-servizi/"+actId);
					} else {

						if(paginate==1) {
								//creo i bottoni per le relative pagine (in base al numero di prodotti che ho)
								//arrotondo in eccesso il numero di annunci diviso 9
								var pagine = Math.ceil(e.results[0].N / 9);
								numPag = pagine;

								//svuoto il paginate
								$('.page_pagination').empty();

								//if(pagine>1)
								$('.page_pagination').append('<li><a href="#" class="tran3s" id="tran3s-left"><i class="fa fa-angle-left" aria-hidden="true"></i></a></li>');

								for (var i = 1; i < pagine+1; i++) {
									//creo il bottone
									$('.page_pagination').append('<li><a href="#" onclick="loadAnnunci(0,0,'+i+')" class="tran3s" id="pagination-a-'+i+'">'+i+'</a></li>');
								}

								//if(pagine>1)
								$('.page_pagination').append('<li><a href="#" class="tran3s" id="tran3s-right"><i class="fa fa-angle-right" aria-hidden="true"></i></a></li>');


								//attivo la classe active al primo
								$('#pagination-a-1').addClass('active');

								//e poi carico le prime 9
								loadAnnunci(actId,0,1);

						} else {
							//svuoto la div con i prodotti
							$('#row-prodotti').empty();
							//e li ricarico corretti
							$.each( e.results, function( k, v ) {
								$('#row-prodotti').append(generateDivAnnuncio(v.ANN_ID,v.ANN_TITOLO,v.ANN_DESC_ABB,v.AIM_FILE,v.AIM_NOME,v.ANN_PREZZO));
							});

							var plus = parseInt(limit)+1;
							var minus = parseInt(limit)-1;

							//attivo la classe corrispondente
							$('.tran3s').removeClass('active');
							$('#pagination-a-'+limit).addClass('active');

							//aggiorno l'onclick delle frecce direzionali
							if(limit < numPag)
								$('#tran3s-right').attr("onclick","loadAnnunci("+actId+",0,"+plus+")");
							else
								$('#tran3s-right').click(function(e) { e.preventDefault(); });

							if(limit>1)
								$('#tran3s-left').attr("onclick","loadAnnunci("+actId+",0,"+minus+")");
							else
								$('#tran3s-left').click(function(e) { e.preventDefault(); });


						}
					}
				}
			},
			error: function(result) {

			}
		});
	}

	//funzione di caricamento delle categorie degli annunci
	function loadAnnunciCat(){
		$.ajax({
			type: "POST",
			url: "/json/annunci.php",
			data:{
				ACTION: 'getCategorie'
			},
			dataType: "json",
			success: function(e) {
				if(e.success===true) {
					$.each( e.results, function( k, v ) {
						$('#ul-categorie-list').append('<li><a href="#" onclick="loadAnnunci('+v.ACT_ID+',1,0)">'+v.ACT_NOME+' ( '+v.NUM_PRODOTTI+' )</a></li>');
					});
				}
			},
			error: function(result) {

			}
		});
	}

	//funzione di creazione della div
	function generateDivAnnuncio(annId,titolo,descab,file,nome,prezzo){

		var div = '<div class="col-md-4 col-sm-6 col-xs-12 hover-effect">\
						<div class="single-shop-item">\
							<div class="img-box">';

		div += '<img src="/doc/prodotti/'+annId+'/'+file+'" alt="'+nome+'" height="268" width="268">\
				<div class="default-overlay-outer">\
									<div class="inner">\
										<div class="content-layer">';

		div += '<a href="/altri-servizi-articolo/'+annId+'" class="thm-btn thm-tran-bg">'+prezzo+'</a>\
										</div>\
									</div>\
								</div>\
							</div>\
							<div class="content-box">\
								<h4><a href="/altri-servizi-articolo/'+annId+'">'+titolo.toUpperCase()+'</a></h4>\
							</div>\
						</div>\
					</div>';
					//<div class="item-price">'+descab+'</div>\
		return div;

	}


	//funzione di caricamento dell'elenco delle news
	function loadNews(){
		$.ajax({
			type: "POST",
			url: "/json/news.php",
			data:{
				ACTION: 'getElenco'
			},
			dataType: "json",
			success: function(e) {
				if(e.success===true) {
					$.each( e.results, function( k, v ) {

						var testo= [];
						var txt='';

						testo=v.testo.split(' ');
						if(testo.length >= 50){

							testo= [];
							testo=v.testo.split(' ',50);

						}else{
							testo= [];
							testo=v.testo.split(' ');

						}

						for (i = 0; i < testo.length; i++) {
							txt += testo[i] + " ";
						}

						$('#ul-news-list').append(' <section class="bg three-column p-t-60">\
								<div class="container">\
									<div class="row">\
									    <div class="col-md-2 col-md-offset-2  col-sm-2 col-xs-2 text-right"></div>\
										 <div class="col-md-8  col-sm-10 col-xs-10">\
											<a href="news-dettaglio/'+v.id+'" class="a-news"><h2>'+v.titolo+'</h2></a>\
											<div class="section-title"></div>\
											<p>'+v.data_format+'</p>\
											<p class="margin_top_20 txt-news">'+txt+'</p>\
											<br/>\
												<a href="news-dettaglio/'+v.id+'">Leggi di più </a>\
										</div>\
									</div>\
								</div>\
							</section>');
					});
				}
			},
			error: function(result) {

			}
		});
	}

	//funzione che permette la visualizzazione del dettaglio di una news

	function loadDettaglioNews(NEWS_ID){
		$.ajax({
			type: "POST",
			url: "/json/news.php",
			data:{
				ACTION: 'getDettaglio',
				NEWS_ID: NEWS_ID
			},
			dataType: "json",
			success: function(e) {
				if(e.success===true) {
					console.log(e);
					console.log(e.storico);
					console.log(e.storico[0].titolo);
					$.each( e.storico, function(index, news ) {

						$('#news-storico').append('<li>\
																				<div class="text-box">\
																					<p>\
																						<a href="/news-dettaglio/'+news.id+'" class="a-news">\
																						'+news.titolo+'\
																						</a>\
																					<br\>'+news.data_format+'\
																					</p>\
																				</div>\
																			</li>');

					});



					$('#dettaglio').append('<div class="row">\
															<div class="col-md-12 col-sm-12 col-xs-12 p-b-30">\
																<h1>'+e.results[0].titolo+'</h1>\
																<div class="section-title bg-regel"></div>\
																<div class="text margin_top_20">\
																	<p class="f-s-20 font-b-400">'+e.results[0].sommario+'</p>\
																</div>\
															</div>\
															<div class="col-md-12 col-sm-12 col-xs-12 txt-news">'+e.results[0].testo+'</div>\
														</div>');
					var titolo=e.results[0].titolo.toLowerCase();
					$('#titBread').text(titolo.charAt(0).toUpperCase() + titolo.slice(1));


				}

			},
			error: function(result) {

			}
		});
	}

</script>
