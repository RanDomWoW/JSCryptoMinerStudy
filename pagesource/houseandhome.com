<script>
	function myhh_show_count() {
		var myhh_json = getCookie('myhh');
		if (myhh_json != "") {
			var myhh_arr = JSON.parse(myhh_json);
			if(myhh_arr.length == 0) {
				return "My H&H";
			} else {
				return myhh_arr.length;
			}
		} else {
			return "My H&H";
		}
	}

	function myhh_show_button(id) {
		if (getCookie('myhh')) {
			var myhh_json = getCookie('myhh');
			var myhh_arr = JSON.parse(myhh_json);
			if (myhh_arr.indexOf(id) != -1 ) {
				document.getElementById("myhh_link-"+id).className = document.getElementById("myhh_link-"+id).className + " myhh_saved";
				document.getElementById("myhh_link-"+id).innerHTML = "Saved";
			} else {
				document.getElementById("myhh_link-"+id).className = document.getElementById("myhh_link-"+id).className.replace(/\bmyhh_saved\b/,'');
				document.getElementById("myhh_link-"+id).innerHTML = "Save This";
			}
		} else {
			document.getElementById("myhh_link-"+id).className = document.getElementById("myhh_link-"+id).className.replace(/\bmyhh_saved\b/,'');
			document.getElementById("myhh_link-"+id).innerHTML = "Save This";
		}
	}

	function myhh_button_click(id, notext) {
					window.location.href='/myhh/';
					jQuery.get('/myhh/', {id: id},function(response) {
			if(response != undefined && response == 'removed') {
				document.getElementById("myhh_link-"+id).className = document.getElementById("myhh_link-"+id).className.replace(/\bmyhh_saved\b/,'');
				if(notext == 0)
				document.getElementById("myhh_link-"+id).innerHTML = "Save This";
				else
				document.getElementById("myhh_link-"+id).innerHTML = "";
			} else if(response != undefined && response == 'added') {
				document.getElementById("myhh_link-"+id).className = document.getElementById("myhh_link-"+id).className + " myhh_saved";
				if(notext == 0)
				document.getElementById("myhh_link-"+id).innerHTML = "Saved";
				else
				document.getElementById("myhh_link-"+id).innerHTML = "";
			}
		});
	}

	function getCookie(cname) {
		var name = cname + "=";
		var ca = document.cookie.split(';');
		for(var i=0; i<ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1);
			if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
		}
		return "";
	}

	function setCookie(cname, cvalue, exdays) {
		var d = new Date();
		d.setTime(d.getTime() + (365*24*60*60*1000));
		var expires = "expires="+d.toUTCString();
		document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
	}
	</script>
