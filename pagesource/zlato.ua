<script type="text/javascript">
    jQuery(document).ready(function ($) {
        if ($(window).width() < 980) {
            $(".sub_menu").click(function (e) {
                e.stopPropagation();
                //  href = $(this).find('a').attr('href');
                //  window.location = href;
            });
            var scrolls = $('.filters-block .scrols_activ');
            var show_more_filter = $('.filter-options-toggle');
            scrolls.each(function (index, el) {
                $(el).closest('.filter-options-scroll-wrap').addClass('full');
                $(el).removeClass('scrols_activ');
            })
            show_more_filter.each(function (index, el) {
                $(el).remove();
            });

            $('.give_me_more').click(function (e) {
                e.preventDefault();
                $(this).toggleClass('active');
            })

            $('.main-menu:not(.header-top-menu)>ul>li').click(function (e) {
                e.preventDefault();
                //$('.top-menu ul li ul').toggleClass('top-menu-ul-li-ul');
                $(this).find('div').toggleClass('active');
                if (!$(this).find('div').length) {
                    location.href = $(this).find('a').attr('href');
                }
            });
            $('.catalog-item-tabs li a').on('click', function (e) {
                if (screen.width > 1023) {
                    $('html, body').animate({
                        scrollTop: $(".tab.active").offset().top - 250
                    }, 0);
                }
            });
            var slider = $('.item_slider li a');

            slider.each(function (index, el) {
                $(el).removeClass('popup');
            });
            $('.item_slider li a').click(function (e) {
                e.preventDefault();
            })

            function price() {
                var prices = [];
                var price_url = '';
                var min = 0;
                var max = 0;
                var redirect_url = $('#price_url_redirect').val();
                $('#price_adaptive li a').click(function (e) {
                    e.preventDefault();
                    $(this).closest('li').toggleClass('active');

                    collectAllData();
                })

                Array.prototype.max = function () {
                    return Math.max.apply(null, this);
                };

                Array.prototype.min = function () {
                    return Math.min.apply(null, this);
                };

                function collectAllData() {
                    price_url = redirect_url;
                    prices = [];
                    var allVariants = $('#price_adaptive li');
                    $.each(allVariants, function (index, el) {
                        if ($(el).hasClass('active')) {
                            if (!$(el).hasClass('flt_checked')) {
                                $(el).removeClass('flt_checkbox');
                                $(el).addClass('flt_checked');
                            }
                        } else {
                            $(el).removeClass('flt_checked');
                            $(el).addClass('flt_checkbox');
                        }
                    })
                    var selectedItems = $('#price_adaptive li.active');
                    if (selectedItems.length) {
                        $('#magicPriceButton').show();
                    } else {
                        $('#magicPriceButton').hide();
                    }
                    $.each(selectedItems, function (index, el) {
                        prices.push($(el).find('a').attr('data-min'));
                        prices.push($(el).find('a').attr('data-max'));
                    });
                    min = prices.min();
                    max = prices.max();
                    price_url = price_url.replace('#VALUE#', min + '-' + max);
                    $('#magicPriceButton').attr('href', price_url);
                }
            }

            function weight() {
                var prices = [];
                var price_url = '';
                var min = 0;
                var max = 0;
                var redirect_url = $('#weight_url_redirect').val();
                $('#weight_adaptive li a').click(function (e) {
                    e.preventDefault();
                    $(this).closest('li').toggleClass('active');

                    collectAllData();
                })

                function collectAllData() {
                    price_url = redirect_url;
                    prices = [];
                    var allVariants = $('#weight_adaptive li');
                    $.each(allVariants, function (index, el) {
                        if ($(el).hasClass('active')) {
                            if (!$(el).hasClass('flt_checked')) {
                                $(el).removeClass('flt_checkbox');
                                $(el).addClass('flt_checked');
                            }
                        } else {
                            $(el).removeClass('flt_checked');
                            $(el).addClass('flt_checkbox');
                        }
                    })
                    var selectedItems = $('#weight_adaptive li.active');
                    if (selectedItems.length) {
                        $('#magicWeightButton').show();
                    } else {
                        $('#magicWeightButton').hide();
                    }
                    $.each(selectedItems, function (index, el) {
                        prices.push($(el).find('a').attr('data-min'));
                        prices.push($(el).find('a').attr('data-max'));
                    });
                    min = prices.min();
                    max = prices.max();
                    price_url = price_url.replace('#VALUE#', min + '-' + max);
                    $('#magicWeightButton').attr('href', price_url);
                }
            }

            price();
            weight();
        } else {
            $('.show_on_mobile').remove();
        }
    })
    ;
</script>

