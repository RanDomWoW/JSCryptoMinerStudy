<script type="text/javascript">
	$(document).ready(function(){
		
		// Omniture tracking
		function oTrack(action) {
			var s = scode;
			if (s) {
				s.pageName = 'bl: modalPopup '+action;
				s.events = action;
				var s_code = scode.t();
				if(s_code){
					document.write(s_code);
				}
			}
		}

		var cookieName = "emailDiscount";
		function setEmailDiscountCookie() {
			// Set the cookie to expire in XX days
			$.cookie(cookieName, 1, {expires: 10});
		}
		
		function subscribe(email) {
			$("#mce-EMAIL").val(email);
			$("#mc-embedded-subscribe-form").submit();
			fbq('track', 'Lead');
		}
		
		function forceEmailModalPopup() {
			var force = false;
			var match = location.search.match(new RegExp("[?&]showEmailModalPopup=([01])"));
			if (match && match[1] ==1) {
				force = true;
			}
			return force;
		}
		
		$("#emailSignUpBtn").css({
			'font-size': 'small',
			'width': 125
		});
		$("#emailSignUpThankYouClose").css({
			'font-size': 'small',
			'width': 125
		});
		
		$("#emailSignUpBtn").click(function(e){
			setEmailDiscountCookie();
			var inputs = $("#emailSignUpForm :input");
			$("#emailSignUpMessage").html("").removeClass("notice errors red").hide();
			$("#emailSignUpEmail").removeClass("red");
			$.ajax({
				type: "POST",
				url: "/EmailDiscount.do",
				data: inputs,
