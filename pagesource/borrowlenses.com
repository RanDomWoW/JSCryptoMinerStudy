	function CryptMailto(email){
        var n = 0;
        var r = "";
        var e = email;

        e = e.replace( /@/, " [at] ");
        e = e.replace( /\./g, " [dot] ");
        
        for( var i=0; i < e.length; i++ )
        {
            n = e.charCodeAt( i );
            if( n >= 8364 )
            {
                n = 128;
            }
            r += String.fromCharCode(n+1);
        }
        
        return r;
    }

	
