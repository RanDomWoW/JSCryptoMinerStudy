			<script>
					$(document).ready( function() {
						var products = new Bloodhound({
							datumTokenizer: Bloodhound.tokenizers.obj.whitespace('value'),
							queryTokenizer: Bloodhound.tokenizers.whitespace,
							limit:20,
							prefetch: '',
							remote: '/Suggest.do?searchString=%QUERY&count=20'
						});

					products.initialize();
					 
					$('#productSearch .typeahead, #productSearchMobile .typeahead').typeahead(
					    {hint: true, highlight: true, minLength: 1},
						{	name: 'products',
							displayKey: 'name',
							templates: {
								suggestion: Handlebars.compile($('#search-suggestion').html()),
								header:'<div class="suggHeader">Search suggestions</div>'
							},
							source: function (query, cb) { 
		                        products.get(query, function (suggestions) {
		                        	$.each(suggestions, function(index, result){
								    	if( result.code == 'Membership100' && "true" == 'true' ){
								    		result.smallImage = 'https://cdn.static-bl.com/product/BL-memberships-product2.jpg';
								    	}
								    });
									cb(suggestions.slice(0, 5));
									if((suggestions.length > 5) && (!$("#seeMore").length)){
									   $('.suggestion:last').after($('<div class="suggestion"><div id="seeMore">See More</div></div>'));
								    } 
								     $("#seeMore").click(function(event){
								        $('#seeMore').hide();
								        var moreResults = suggestions.slice(5,suggestions.length);
								        event.stopPropagation();
								        $.each(moreResults, function(index,result){
									        $('.suggestion:last').after($('<div class="suggestion"><a class="clearfix" href="/product/'+ result.code +'?source=auto-suggest"><div class="suggestion-image"><img src='+result.smallImage+'></div><div class="suggestion-name"><span>'+result.name+'</span></div></a></div>'));
									    });
										
									     });
									  })
								  }
							  }
						 );
					});
			</script>
