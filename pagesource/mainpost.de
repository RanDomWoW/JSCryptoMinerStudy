           16: fcmsJs._webtrekkHelper['articleTitle'],
          773: fcmsJs._webtrekkHelper['paywall'],           
		  775: fcmsJs._webtrekkHelper['keywords'],
          777: fcmsJs._webtrekkHelper['pageType'],
          778: fcmsJs._webtrekkHelper['wordCount'],
        };
		
        console.log("window._ti", window._ti);
