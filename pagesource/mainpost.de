      <script type="text/javascript">
      
              fcmsJs._webtrekkHelper['contentId'] = '/';        
        

		// Userhost        
		fcmsJs._webtrekkHelper['userhost'] = 'www.mainpost.de';
        
        // Paywall
        if (fcmsJs._webtrekkHelper['paywall'] !== '1'){
          fcmsJs._webtrekkHelper['paywall'] = '0';
        }

        // Replace Quotes
        str = fcmsJs._webtrekkHelper['articleTitle'];
		str = str.replace('&bdquo;', '\"');
        str = str.replace('&ldquo;', '\"');
        fcmsJs._webtrekkHelper['articleTitle'] = str;

        // pageType URL
        pageTypeUrl = '/';

        // pageType Regex
        if (fcmsJs._webtrekkHelper['paywall'] == '0'){
          if (pageTypeUrl.match(/\/$/g)){
            if ( '' == '122364' || pageTypeUrl.match(/\/meine-themen\/$/g) ){
              fcmsJs._webtrekkHelper['pageType'].push('member');            	
            }
            if ( '' !== '122364' && !pageTypeUrl.match(/\/meine-themen\/$/g) && !pageTypeUrl.match(/nlsetup.html$/g) ){
              fcmsJs._webtrekkHelper['pageType'].push('overview');         	
            }
          }
          if (pageTypeUrl.match(/nlsetup.html$/g)){
          	fcmsJs._webtrekkHelper['pageType'].push('overview');
          }
          if ( 'art' == 'art' && pageTypeUrl.match(/;art/g) && '' == 0 ) {
              fcmsJs._webtrekkHelper['pageType'].push('article');
          }
          if ( 'art' == 'art' && pageTypeUrl.match(/;art/g) && '' >= 1 && '' < 960 && !pageTypeUrl.match(/::pic/g) && !pageTypeUrl.match(/::cme/g)) {
              fcmsJs._webtrekkHelper['pageType'].push('article_pic');
          }
          if ( 'art' == 'art' && pageTypeUrl.match(/;art/g) && '' >= 1 && '' >= 960 && !pageTypeUrl.match(/::pic/g) && !pageTypeUrl.match(/::cme/g)) {
              fcmsJs._webtrekkHelper['pageType'].push('article_bigpic');
          }
          if ( pageTypeUrl.match(/;art/g) && pageTypeUrl.match(/::pic/g) ) {
              fcmsJs._webtrekkHelper['pageType'].push('pic_article');
          }
          if ( pageTypeUrl.match(/;art/g) && pageTypeUrl.match(/::cme/g) ) {
              fcmsJs._webtrekkHelper['pageType'].push('dia_article');
          }
          if ( !pageTypeUrl.match(/;art/g) && pageTypeUrl.match(/\/cme/g) ) {
              fcmsJs._webtrekkHelper['pageType'].push('dia');
          }
          if ( 'art' == 'sts' && pageTypeUrl.match(/\/sts/g) ) {
              fcmsJs._webtrekkHelper['pageType'].push('stream');
          }
          if ( 'art' == 'epa' && pageTypeUrl.match(/\/epa/g) ) {
              fcmsJs._webtrekkHelper['pageType'].push('epaper');
          }
          if ( 'art' == 'xmv' && pageTypeUrl.match(/\/xmv/g) ) {
              fcmsJs._webtrekkHelper['pageType'].push('prospekt');
          }
          if ( 'art' == 'ticker' && pageTypeUrl.match(/\/ticker/g) ) {
              fcmsJs._webtrekkHelper['pageType'].push('liveticker');
          }
          if ( '' == '1544' ){
              fcmsJs._webtrekkHelper['pageType'].push('overview');
          }
          if ( 'art' == 'art' && pageTypeUrl.match(/-pg/g) ) {
              fcmsJs._webtrekkHelper['pageType'].push('overview');
          }           
        }
        if (fcmsJs._webtrekkHelper['paywall'] == '1'){
          if ( pageTypeUrl.match(/;art/g) ) {
            fcmsJs._webtrekkHelper['pageType'].push('paywall_article');
          }
          if ( pageTypeUrl.match(/\/cme/g) ) {
            fcmsJs._webtrekkHelper['pageType'].push('paywall_dia');
          }
          if ( pageTypeUrl.match(/\/epa/g) ) {
            fcmsJs._webtrekkHelper['pageType'].push('paywall_epaper');
          }
        }
        if (fcmsJs._webtrekkHelper['pageType'].length == 0) {
            fcmsJs._webtrekkHelper['pageType'].push('No Match Found');
        }        
        
        // Implode Arrays
        fcmsJs._webtrekkHelper['keywords'] 		= fcmsJs._webtrekkHelper['keywords'].join("; ");
        fcmsJs._webtrekkHelper['pageType'] 		= fcmsJs._webtrekkHelper['pageType'].join("; ");
        fcmsJs._webtrekkHelper['articleAuthor'] = fcmsJs._webtrekkHelper['articleAuthor'].join("; ");        
        
        // Seiten-Parameter
        window._ti['customParameter'] = {
           10: fcmsJs._webtrekkHelper['contentId'],
           11: fcmsJs._webtrekkHelper['userhost'],
           12: fcmsJs._webtrekkHelper['articleDate'],
           13: fcmsJs._webtrekkHelper['articleAuthor'],
           14: fcmsJs._webtrekkHelper['dossierName'],
