.hba-yt-video {
	position: relative;
	
	overflow: hidden;
	
	cursor: pointer;
}
.hba-yt-video div {
	transition: all 0.35s ease-in-out;
	transform: scale(1);
	
	max-height: auto;
	max-width: auto;
	
	width: 100%;
	height: 100%;
	
	background-size: cover;
	background-position: center center;
	background-repeat: no-repeat;
}
.hba-yt-video:hover div {
	transform: scale(1.15);
}

.hba-yt-video:after {
	position: absolute;
	
	content: '';
	
	width: 60px;
	height: 60px;
	
	top: 50%;
	left: 50%;
	
	margin: -30px 0 0 -30px;
	
	background-image: url('https://hackbrightacademy.com/content/uploads/2017/03/video-play-button@2x.png');
	background-size: 60px 60px;
	background-position: center center;
}





