.hba-form-inline label {
	font-weight: normal !important;
	
	position: relative;
	
	padding-left: 35px;
	
	cursor: pointer;
}

.hba-form-inline label input[type=checkbox] {
	display: none
}




.hba-form-inline label input[type=checkbox] + span {
	content: "";
	
	position: absolute;
	
	top: 1px;
	left: 0;
	
	width: 20px;
	height: 20px;
	
	padding-left: 35px;
	
	border-radius: 4px;
	
	border: solid 2px #cdcdcd;
	
	background #fff;
	
	padding-left: 0;
}

.hba-form-inline label input[type="checkbox"]:checked + span:before {
	background-color: #fb3530;
	
	color: #fff;
	content: "✔";
	text-align: center;
	position: absolute;
	
	top: -2px;
	left: -2px;
	
	width: 100%;
	line-height: 20px;
	
	border-radius: 4px;
	
	border: solid 2px #c42321;

}


