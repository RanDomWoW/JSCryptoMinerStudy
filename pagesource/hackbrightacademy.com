/* entire container, keeps perspective */
.flip-container, hba-team-flip-car {
	perspective: 1000px;
}

	.flip-container.flip .flipper, .hba-team-flip-card.flip .hba-team-card-hook {
		transform: rotateY(0deg);
	}
	
	.flip-container.flip .fc-front {
		transform: rotateY(180deg) !important;
	}
	
	.flip-container.flip .fc-back {
		transform: rotateY(0deg) !important;
z-index: 3;
	}
	

.flip-container, 
.flip-container .flipper .fc-front, 
.flip-container .flipper .fc-back {
	width: 100%;
	height: auto;
	
	transform-style: preserve-3d;
	transition: 0.6s;
}

/* flip speed goes here */
.flip-container .flipper, .hba-team-card-hook {
	transition: 0.6s;
	transform-style: preserve-3d;

	position: relative;
}

/* hide back of pane during swap */
.flip-container .flipper .fc-front, .hba-team-flip-card .card_front,
.flip-container .flipper .fc-back, .hba-team-flip-card .card_back {
	position: absolute;
	
	top: 0;
	left: 0;
	
	background-color: #ffffff !important;
	box-shadow: 0 3px 7px 0 rgba(102, 73, 70, 0.2)  !important;
	
	-webkit-backface-visibility: hidden; /* Chrome, Safari, Opera */
    backface-visibility: hidden;
}

/* front pane, placed above back */
.flip-container .flipper .fc-front, .hba-team-flip-card .card_front {
	z-index: 2;
	/* for firefox 31 */
	transform: rotateY(0deg);
	position: relative;
}

/* back, initially hidden pane */
.flip-container .flipper .fc-back, .hba-team-flip-card .card_back {
	transform: rotateY(-180deg);
}


