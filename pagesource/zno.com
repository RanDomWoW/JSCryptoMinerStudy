<script type="text/javascript">
    function isPageOf(pageName) {
        var url = window.location.href;
        if(url.indexOf('?') != -1){
            url = url.substr(0,url.indexOf('?'));
        }
        if (url.indexOf(pageName) > 0) {
            return true;
        }
        return false;
    }
    function getRoute(sUrl) {
        if(sUrl) {
            // e.g.   http://www.artisanstate.com/xxx/yyy.html
            sUrl = sUrl.replace('http://', '').replace('https://', '');
            if(sUrl.indexOf('?') != -1){
                sUrl = sUrl.substr(0,sUrl.indexOf('?'));
            }
            if(sUrl.indexOf('/') !== -1) {
                var fromIdx = sUrl.indexOf('/');
                sUrl = sUrl.substr(fromIdx);
                return sUrl;
            }else{
                return '';
            }
        }
        else {
            return '';
        }
    }
    function layer_show(layer_name, action) {
        if (action == "show") {
            document.getElementById(layer_name).style.visibility = "visible";
        }
        else if (action == "hide") {
            document.getElementById(layer_name).style.visibility = "hidden";
        }
    }
</script>

