<script>

    var airportRate = parseFloat("0.0969"),
        bankRate = parseFloat("0.03064");

    $(function () {
        getFixedRate(getGBPRepresentativeRates);
    });

    function getFixedRate(rates) {

        var thread = null;
        var data = rates;

        // Savings Calculator
        // Leave a line blank for correct js suturing
        $('#dest_fixed, #base_fixed').change(function() {
            $('#amt_fixed').trigger('keyup');
        });

        $("#amt_fixed").bind('keyup mouseup', function() {

            clearTimeout(thread);

            thread = setTimeout(function() {

                var amt = parseFloat($("#amt_fixed").val()),
                    base = $("#base_fixed").val(),
                    dest = $("#dest_fixed").val(),
                    caxtonMargin;

                // display error message if GBP value is not between 100 & 6500
                if (amt < 50 || amt > 12000) {
                    $("#MsgGBPLimit").removeClass('hidden');

                    $("#bankResult").attr("placeholder", "");
                    $("#airportResult").attr("placeholder", "");
                    $("#caxtonResult").attr("placeholder", "");

                    return;
                } else {
                    $("#MsgGBPLimit").addClass('hidden');
                }
                console.log('yo');

                // Pricing tiers
                if (amt >= 0 && amt < 1000) {
                    caxtonMargin = 0.964; // 1 - 0.036 (3.6%)
                }
                else if (amt >= 1000 && amt < 2000) {
                    caxtonMargin = 0.975; // 1 - 0.025 (2.5%)
                } else {
                    caxtonMargin = 0.975; // 1 - 0.025 (2.5%)
                }

                // e.g. convert EUR to EU
                flagCode = dest.toLowerCase().slice(0, -1);

                caxtonRateAnswer = convertToCaxtonRate(amt, base, dest, data, caxtonMargin); // 1 - 0.025 (2.5%)
                //bankRateAnswer    = convert(amt, base, dest, data, 0.9718); // 1 - 0.0282 (2.82%)
                //airportRateAnswer = convert(amt, base, dest, data, 0.8633); //  1 - 0.1367 (13.67%)

                bankRateAnswer = convertFromCaxtonRate(caxtonRateAnswer, bankRate);  // 2.402% in decimal format -> 0.02402
                airportRateAnswer = convertFromCaxtonRate(caxtonRateAnswer, airportRate); // 7.46% in decimal format -> 0.0746

                if (amt !== '' && amt !== ' ') {
                    bankRateResult    = bankRateAnswer.formatMoney(2, '.', ',');
                    airportRateResult = airportRateAnswer.formatMoney(2, '.', ',');
                    caxtonRateResult  = caxtonRateAnswer.formatMoney(2, '.', ',');
                }

                // bank result
                $("#bankResult").attr("placeholder", bankRateResult);
                // airport result
                $("#airportResult").attr("placeholder", airportRateResult);
                // Caxton rate
                $("#caxtonResult").attr("placeholder", caxtonRateResult);


                // insert flag icon for each destination block (could be an input group)
                $('[data-id="flagIconDest"]').each(function() {
                    $(this).html(createFlagIcon(flagCode, "small"));
                });

                // insert currency symbol for input-group (e.g. EUR)
                $('[data-id="currencyCodeDest"]').each(function() {
                    $(this).text(dest);
                });

            }, 500);
        }).trigger('keyup');
    }


    /* Interbank conversion = 1 */
    /* Caxton rate = 2.5% of Interbank */
    function convertToCaxtonRate(amount, base, to, data, conversion) {
        //console.log(data);
        if (data.error) {
            alert(data.description);
            return false;
        } else {
            for (curr in data.Rates) {
                if (curr == to) {
                    rate = data.Rates[curr] * conversion;
                    result = (amount / data.Rates[base]) * rate;

                    return rnd(result);
                } // end if
            } // end for
        } // end error check
    }

    /* Bank rate = 2.82% of Interbank */
    /* Airport rate = 13.67% of Interbank */
    function convertFromCaxtonRate(caxRate, conversion) {
        result = caxRate - (caxRate * conversion);
        return rnd(result);
    }
</script>
