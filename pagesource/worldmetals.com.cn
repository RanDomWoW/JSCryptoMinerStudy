function changeUL(num,id,className){
	var uls = $("."+className);//document.getElementsByClassName(className);//获取到带有指定class名称的对象集合   
	var as = document.getElementById(id).getElementsByTagName("a");//获取到ID为"jingzheng_menu"下的所有"a"标签对象集合
	for(var i=0;i<uls.length;i++){//循环对象集合，取出每个ul对象
		//判断是否是点击的对象，如果是--让对象显示，否则隐藏
		if(i+1 == num){
			uls[i].style.display="block";
			as[i].className="active1"; 
		}else{
			uls[i].style.display="none";
			as[i].className="active2"; 
		}
	}
}
</script>
