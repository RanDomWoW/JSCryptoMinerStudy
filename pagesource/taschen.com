		var jqxhr = $.ajax({
			url: param_data_ajax_url, 
			context: document.body,
			statusCode: {
			  500: function() {
				func_ajax_error("500");
			  }
			}
		})
		  .done(function(data) {
			  
			  
			//console.log( "done  quick view -- jetzt in modal zeigen -- data B" + data + "B");
			
			$('#QuickViewProduct .modal-body').html(data);
			$('#QuickViewProduct .modal-body #quick_view .royalSlider').css('visibility', 'hidden');
			
			loading_indicator(0);
			
			$('#QuickViewProduct').modal('show');	
			
			// summiert sich sonst auf!
			$( "#QuickViewProduct" ).off('shown.bs.modal');	
				
			$( "#QuickViewProduct" ).on('shown.bs.modal', function () {
				
				//console.log("QuickViewProduct - shown.bs.modal");
				
				//galleryInModal();
				
				//var gallery_height = $(window).height();
