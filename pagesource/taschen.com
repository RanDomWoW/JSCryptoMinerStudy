				$('#QuickViewProduct .modal-body #quick_view .royalSlider').css('visibility', 'visible');
				});						
			
		 
		  })
		  .fail(function() {
				//console.log( "error quick view" );
		  })
		  .always(function() {
		  		//console.log( "always quick view" );
			
		  });
		
	}




