/**
 * AJAX-Quick-View Product
 * Klick auf quick-view-Button abfangen
 */
	function init_ajax_quick_view()
	{
	//console.log("function init_ajax_quick_view()");
	
		$( "body" ).on( "click", ".quick-view button", function(event) {
		
		event.preventDefault();
		
		var tmp_data_ajax_url = $(this).attr('data-ajax-url');
		
			if(tmp_data_ajax_url !== undefined)
			{
			func_load_quick_view(tmp_data_ajax_url, 1);
			}

	  });
	  
	}
	
	


/**
 * Quick View AJAX laden
 * @param {string} param_data_ajax_url - zu ladende URL
 */
