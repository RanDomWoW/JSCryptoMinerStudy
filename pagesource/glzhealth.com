</script> <script src="/public/js/jquery.js"></script> <script type="text/javascript">	/*右边栏toTop等功能init;
	 argument:监听高度，默认100;
	 手动关闭class:".closeRightBar";
	 返回顶部并消失class:".toTop";*/
	$.fn.myRightBar=function(h){
		var height=h||100;
		var scrolling=false;
		if(!scrolling){
			return this.each(function() {
				var $this = $(this);
				$(window).scroll(function () {
					if(!scrolling){
						var sd = $(window).scrollTop();
						if (sd > height) {
							$this.fadeIn();
						} else {
							$this.fadeOut();
						}
					}
				});
				$this.find(".closeRightBar").click(function () {
					scrolling = true;
					$this.fadeOut();
				});
				$this.find(".toTop").click(function () {
					scrolling = true;
					$('html, body').animate({
						scrollTop: 0
					}, 500, function () {
						scrolling = false;
						$this.fadeOut();
					});
				});
			})
		}
	};
	$("#myE_right_bar").myRightBar();
	
      /*  定义mySilde轮播图函数
    参数
    list:unit的list父元素选择器，字符串，例如："#area_org_list_main"；
    unit:图片单元元素选择器，字符串；
    left_arrow:左箭头选择器，字符串；
    right_arrow:右箭头选择器，字符串；
    unit_height:单元的高度，number;
    unit_width:单元的宽度，number；
    view_count:首屏显示的图片unit个数，number;
    step_width:一次点击移动的距离，number；
    hover_array:箭头hover样式数组，[0]是否有hover效果，boolean值；[1]hover样式calss名称,字符串
*/
    function mySlide(obj) {
        var list=obj.list,unit=obj.unit,left_arrow=obj.left_arrow,right_arrow=obj.right_arrow,unit_height=obj.unit_height,unit_width=obj.unit_width,view_count=obj.view_count,
                step_width=obj.step_width,isArrowHover=obj.isArrowHover,arrowHoverClass=obj.arrowHoverClass,speed_time=obj.speed_time;
        unit_height=unit_height||$(unit).height();
        unit_width=unit_width||$(unit).width();
        step_width=step_width||unit_width;
        speed_time=speed_time||1000;
        var can_right=false,can_left= false,can_click=true;
        var $area_org_list_main=$(list);
        $area_org_list_main.parent().css('height',unit_height);
        var $area_org_unit_count=$(unit).length;
        var $area_left_arrow=$(left_arrow);
        var $area_right_arrow =$(right_arrow);
        var $area_org_list_main_width= $area_org_unit_count*unit_width;
        var move_width=( $area_org_unit_count- view_count)* unit_width;
        $area_org_list_main.css("width",$area_org_list_main_width);
        if(move_width >= 0){
            can_right=true;
        }
        if(isArrowHover){
            $area_left_arrow.mouseover(function(){
                if(can_left==true){
                    $(this).addClass(arrowHoverClass);
                }
            });
            $area_left_arrow.mouseout(function(){
                $(this).removeClass(arrowHoverClass);
            });
            $area_right_arrow.mouseover(function(){
                if(can_right== true){
                    $(this).addClass(arrowHoverClass);
                }
            });
            $area_right_arrow.mouseout(function(){
                $(this).removeClass(arrowHoverClass);
            });
        }
        $area_left_arrow.click(function (){
            if(can_click){
                can_click= false;
                if(can_left== true){
                    var curr_position=$area_org_list_main.css("left").slice(0,-2);
                    if(curr_position< 0){
                        curr_position=+curr_position;
                        curr_position+= step_width;
                        $area_org_list_main.animate({left:curr_position},speed_time,function(){can_click=true;});
                        can_right=true;
                        if(curr_position>=0){
                            can_left=false;
                            if(isArrowHover){
                                $(this).removeClass(arrowHoverClass);
                            }
                        }
                    }
                }else{
                    can_click=true;
                }
            }
        });
        $area_right_arrow.click(function(){
            if(can_click){
                can_click= false;
                if(can_right== true){
                    var curr_position=$area_org_list_main.css("left").slice(0,-2);
                    if(Math.abs(curr_position)<move_width){
                        curr_position-= step_width;
                        $area_org_list_main.animate({left: curr_position},speed_time, function(){can_click= true;});
                        can_left=true;
                        if(Math.abs(curr_position)==move_width){
                            can_right=false;
                            if(isArrowHover){
                                $(this).removeClass(arrowHoverClass);
                            }
                        }
                    }
                }else{
                    can_click=true;
                }
            }
        });
    }
    mySlide({"list": "#area_serves_list_main", "unit": ".index_serves_unit", "left_arrow": "#area_serves_left_arrow", "right_arrow":"#area_serves_right_arrow", "unit_height":180, "unit_width":275, "view_count":3, "step_width":275, "isArrowHover":true, "arrowHoverClass":"area_arrow_hover"});
    mySlide({"list": "#area_org_list_main", "unit": ".area_org_unit", "left_arrow": "#area_left_arrow", "right_arrow":"#area_right_arrow", "unit_height":250, "unit_width":250, "view_count":4, "step_width":250, "isArrowHover":true, "arrowHoverClass":"area_arrow_hover"});
    mySlide({"list": "#index_evaluation_list_main", "unit": ".index_evaluation_unit", "left_arrow": "#index_left_arrow", "right_arrow":"#index_right_arrow", "unit_height":650, "unit_width":980, "view_count":1, "step_width":980, "isArrowHover":false});
    mySlide({"list": "#fy_org_list_main", "unit": ".fy_org_unit", "left_arrow": "#area_left_arrow", "right_arrow":"#area_right_arrow", "unit_height":350, "unit_width":980, "view_count":1, "step_width":980, "isArrowHover":true, "arrowHoverClass":"area_arrow_hover"});



