                    jQuery('#news .container').html('Obrigado por se cadastrar na Kaisan!');
                    jQuery('#news .container').css('font-size', '15px');
                    jQuery('#news .container').css('font-weight', 'bold');
                    jQuery('#news .container').css('color', '#ffffff');
                    setTimeout(function(){ fecharnews(); }, 3000);
                });
        }
    </script>
