            jQuery.ajax({
                url: "/subscribenewsajax/index/index?email=" + jQuery('#newsemail').val() + '&nome=' + jQuery('#newsnome').val() + '&whatsapp=' + jQuery('#newswhatsapp').val(),
                beforeSend: function( xhr ) {
                    jQuery('#news .container .carregandonews').show();
