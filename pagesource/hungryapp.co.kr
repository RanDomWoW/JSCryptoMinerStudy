				function onSearchTotal()
				{
					if(jsTrim($('#tsearchstr').val()) == "")
					{
						 $('#searchResult').hide();
						 return;

					}

				    var key= $('#tsearchstr').val() ;
				    $.ajax({
						  url: "/updator/getSearchTotal.php",
						  global: false,
						  type: "POST",
						  dataType: 'json',
						  data: ({ "key" : key}),
						  async:false,
						  success: function(data){
								if(data.length == 0)
									$('#searchResult').hide();
								else
								{
									if($('#searchResult').css('display') == "none")
										$('#searchResult').show();

								}
								var obj = $('#schlist_result');
								obj.children().remove();
								$.each(data, function(key, val) {
								    	obj.append(val);
								});


						  }
					   }
					);

				}

