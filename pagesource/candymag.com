      feedTemplates = {"article":"<div class=\"feed-item feed-item--ntv hide\"> <div class=\"card\"> <div class=\"card__fig\"> <div class=\"btn btn--shre\" sns-button-feed> <svg class=\"icon icon--sm\"> <use xlink:href=\"#share\"><\/use> <\/svg> <\/div> <div class=\"sns-ovly sns-ovly--feed\" sns-feed-overlay> <div class=\"btn btn--shre btn--shre--close btn--shre--transparent\" sns-overlay-close> <div class=\"close-feed\"> <svg class=\"icon icon--btn\"> <use xlink:href=\"#close\"><\/use> <\/svg> <\/div> <\/div> <div class=\"sns-ovly__btns\"> <div class=\"sns-ovly__btns--share hide\"><span data-share-count>0<\/span> Shares<\/div> <div class=\"btn btn--sns btn--sns--clear btn--sns--facebook\"> <a href=\"#\" class=\"sns-item\" data-sns=\"facebook\" data-url data-image data-desc data-title> <svg class=\"icon icon--btn\"> <use xlink:href=\"#facebook\"><\/use> <\/svg> <\/a> <\/div> <div class=\"btn btn--sns btn--sns--clear btn--sns--twitter\"> <a href=\"#\" class=\"sns-item\" data-sns=\"twitter\" data-url data-image data-desc data-title> <svg class=\"icon icon--btn\"> <use xlink:href=\"#twitter\"><\/use> <\/svg> <\/a> <\/div> <div class=\"btn btn--sns btn--sns--clear btn--sns--pinterest\"> <a href=\"#\" class=\"sns-item\" data-sns=\"pinterest\" data-url data-image data-desc data-title> <svg class=\"icon icon--btn\"> <use xmlns:xlink=\"http:\/\/www.w3.org\/1999\/xlink\" xlink:href=\"#pinterest\"><\/use> <\/svg> <\/a> <\/div> <\/div> <\/div> <a href=\"#\" data-article-url> <img data-article-image src=\"\"> <\/a> <\/div> <div class=\"card__body\"> <div class=\"ch\"> <a href=\"#\" data-article-channel data-article-channel-url><\/a> <\/div> <div class=\"ttle\"> <h3> <a href=\"#\" data-article-url data-article-title><\/a> <\/h3> <\/div> <div class=\"blrb\" data-article-blurb><\/div> <div class=\"byln\" data-article-author> <a href=\"#\" data-article-author-url data-article-author-name><\/a> <\/div> <\/div> <\/div> <\/div>","facebook":"<div class=\"feed-item feed-item--fb\"> <div class=\"feed-idf\"> <div class=\"card\"> <div class=\"card__fig\"> <svg class=\"icon icon--sm\"> <use xlink:href=\"#facebook\"><\/use> <\/svg> <\/div> <div class=\"card__body\">via Facebook<\/div> <\/div> <\/div> <div class=\"fb-post\" data-href=\"\" data-width=\"610\" data-sns-feed><\/div> <\/div>","twitter":"<div class=\"feed-item feed-item--twitter\"> <div class=\"feed-idf\"> <div class=\"card\"> <div class=\"card__fig\"> <svg class=\"icon icon--sm\"> <use xlink:href=\"#twitter\"><\/use> <\/svg> <\/div> <div class=\"card__body\">via Twitter<\/div> <\/div> <\/div> <div data-sns-feed><\/div> <\/div>","instagram":"<div class=\"feed-item feed-item--insta\"> <div class=\"feed-idf\"> <div class=\"card\"> <div class=\"card__fig\"> <svg class=\"icon icon--sm\"> <use xlink:href=\"#instagram\"><\/use> <\/svg> <\/div> <div class=\"card__body\">via Instagram<\/div> <\/div> <\/div> <div data-sns-feed><\/div> <\/div>"},
