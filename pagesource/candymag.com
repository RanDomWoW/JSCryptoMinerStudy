            gptAdSlots[dynamic_ad.ad_num] = googletag.defineSlot("/"+dynamic_ad.network_code+"/" + type, size, divAdId).addService(googletag.pubads()).setTargeting("Tag", keywords).setSafeFrameConfig({allowPushExpansion: true});

            //checks if ad slot is not empty
            googletag.pubads().addEventListener('slotRenderEnded', function(event) {

                    setTimeout(function(){
	                    var itemDisplay = $(placeholder).find('.dfp_ad').css('display');
	                    if (itemDisplay == 'none' || itemDisplay == undefined) {
	                        $(placeholder).parent().hide();
	                    }
                    }, 1000);

                    $(placeholder).addClass('dynamic-ad-expand');
                    $(placeholder).addClass(keywords);
					$(placeholder).parent().css({'height' : 'auto', 'background-color' : '#f8f8f8' });
					
					$(el).removeClass('dynamic-ad-rendering');
            });

            googletag.cmd.push(function(){ googletag.display(divAdId); });
            googletag.pubads().refresh([gptAdSlots[dynamic_ad.ad_num]]);
            googletag.pubads().collapseEmptyDivs();
		});  

		dynamic_ad.ad_num++;
    };
}

var adCounter= 0;

function getAdTargettingTags(adcode, counter) {
	var tags;

	switch(adcode) {
		case 'feed':
			counter = (adCounter > 1) ? ((counter - adCounter) + 1) : (counter+1);
			tags = 'dynamic-feed-'+counter;
		break;
		default: '';
	}

	return tags;
}

$(document).ready(function() {
	var 
	  mobileMode = (deviceType == 'mobile') ? 'mobile_' : '',
	  section = dynamic_ad.is_nsfw == '1' ? 'nsfw' : dynamic_ad.section.replace('-','_');

 	$(window).on('scroll', function() {

        $('.dynamic-ad').each(function(index, el) {
			if (isInView(el) && !$(el).hasClass('dynamic-ad-expand') && !$(el).hasClass('dynamic-ad-rendering')) {
				var id = dynamic_ad.ad_num;
				$(el).addClass('dynamic-ad-rendering');
				$(el).attr('id', 'dynamid-ad-'+id); 
				$(el).parent().css('height','0px');
				console.log('Dynamic ad load start', 'ID: '+id);
            	loadDynamicAd(el, id, WEBSITE_AD_UNIT+'_'+dynamic_ad.type+'_'+mobileMode+section, dynamic_ad.size, getAdTargettingTags(dynamic_ad.type, index));
            }
        });
    });
});
</script>

	
	
    
	

