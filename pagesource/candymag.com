        };

    loadJS('/js/app/custom-js.js', function(){});

    var getData = function(config) {
        var 
          u = config.url || '',
          t = config.type || 'GET',
          dt = config.dataType,
          d = config.data || {};

        return $.ajax({
            url : u,
            type : t,
            dataType : dt,
            data: d,
            success : function(response, status) {
                return response;
            },
            error : function(response) {
                console.error(response.statusText);
            }
        });
    };

    var generateFeedSocial = function(data, container, templates) {
        $.each(data, function(key, value){
            var 
              type = typeof value.type === 'undefined' ? 'article' : value.type,
              template = templates[type],
              clone = $(template).clone();

            lastDatePublish = value.date_published;

            try {
                if(type == 'article') {
                    clone.find('[data-article-url]').attr('href', value.url+'?ref=home_feed_'+(page+1));
                    clone.find('[data-article-title]').html(value.title);
                    clone.find('[data-article-image]').attr('src', value.images.main);
                    clone.find('[data-article-blurb]').html(value.blurb);

                    if(value.authors) {
                        var author = makeAuthorByLine(value.authors);

                        clone.find('[data-article-author]').html(author);
                    }

                    clone.find('.sns-item[data-sns]').attr('data-url', PUBLIC_DOMAIN+value.url);
                    clone.find('.sns-item[data-sns]').attr('data-title', value.title);
                    clone.find('.sns-item[data-sns]').attr('data-desc', value.blurb);
                    clone.find('.sns-item[data-sns]').attr('data-image', value.images.main);

                    if(value.shares) {
                        clone.find('[data-share-count]').html(value.shares);
                        clone.find('[data-share-count]').parent().removeClass('hide');
                    }

                    if(value.sponsored_indication) {
                        clone.find('[data-article-channel]').html(value.sponsored_indication.feed);
                        clone.find('[data-article-channel-url]').attr('href', '');
                        clone.find('[data-article-channel-url]').css('pointer-events', 'none');
                    } else {
                        clone.find('[data-article-channel]').html(value.channel.name);
                        clone.find('[data-article-channel-url]').attr('href', value.channel.url+'?ref=channel');
                    }
                } else {
                    var el = clone.find('[data-sns-feed]');

                    el.attr('data-href', value.url);

                    if(type != 'facebook') {
                        var g = type,
                            h = value.url;

                        a[g].params.url = decodeURIComponent(h);

                        if(a[g] && a[g].params.url) {
                            $.ajax({
                                url: a[g].apiUrl,
                                type: "GET",
                                dataType: "jsonp",
                                data: a[g].params,
                                crossDomain: !0
                            }).done(function(response) {
                                $(el).html(response.html);
                                if(type == 'instagram' && instgrm) instgrm.Embeds.process();
                                if(value.type == 'twitter' && twttr) twttr.widgets.load(clone);
                            });
                        }
                    }
                }

                $(container).append(clone);
                $(container).addClass('feed-expanded');
                $(clone).removeClass('hide');

                if(value.type == 'facebook' && typeof FB !== 'undefined') {
                    FB.XFBML.parse(clone[0]);
                }
            } catch(err) {
                console_errors.push({type : value.type, error : err});
                console.warn('Error catched. Will go to console errors', value.type, err);
            }
        });
    };

    var isInView = function(el, threshold) {
        var docViewTop = $(window).scrollTop();
        var elemTop = $(el).parent().offset().top;
        var threshold = threshold || 400;
        var viewIn = $(window).height() + threshold;
        return (docViewTop >= (elemTop - viewIn));
    };

    $(document).ready(function(){
        feedLoading = true;

        getData({
            url : apiUrl+'0/'+feedChunk
        }).then(function(response){
            generateFeedSocial(response, '[data-feed-container]:eq(0)', feedTemplates);

            feedLoading = false;
        });
    });

    $(window).on('scroll', function() {
        $('[data-feed-container]').each(function(index, el){
            if(isInView(el) && !feedLoading && $(el).children().length == 0) {
                feedLoading = true;
                page = index;

                getData({
                    url : apiUrl+page+'/'+feedChunk,
                    data: { "lastDatePublish": lastDatePublish }
                }).then(function(response){
                    generateFeedSocial(response, el, feedTemplates);

                    feedLoading = false;
                });

                if(page == ($('[data-feed-container]').length - 1)) {
                    $("<section />", { html: '<div class="wrap__innr"><div class="wrap__side"></div><div class="wrap__main"><div data-feed-container class="feed feed--1"></div></div></div>' }).appendTo('#wrapper .main');
                    $("<section />", { html:  adTemplate.dynamic_ad }).appendTo('#wrapper .main');
                }
            }
        });
    });
