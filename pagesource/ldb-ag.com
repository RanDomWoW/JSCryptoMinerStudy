<script>
	$(function() {
		$('.pageTop a').attr('href','javascript:void(0);');
		$('.pageTop a').click(function(){
			$('html, body').animate({
					scrollTop: $("#top").offset().top
			}, 800);
			return false;
		});


		var winW = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
		if(winW > 767){
			$(function() {
				$(window).scroll(function() {
					var offset = $("#top .ul_works").offset();
					if ($(window).scrollTop() > offset.top) {
						$("#headerWrap").slideDown(100);
					} else {
						$("#headerWrap").slideUp('fast');
					};
				});
			});

		}

		if(winW <= 767){
			$(function() {
				$(window).scroll(function() {
					var offset = $("#top #headerWrap").offset();
					if ($(window).scrollTop() > offset.top) {
						$("#headerWrap").css('height', 51);
						$("#headerWrap > .inner").addClass('fixed');
					} else {
						$("#headerWrap > .inner").removeClass('fixed');
					};
				});
			});

		}


		$(window).resize(function(){
			var winW = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
			if(winW >= 767 && winW <= 769){
				location.reload();
			}
		});

	});

</script>

