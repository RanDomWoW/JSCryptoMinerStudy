<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&amp;sensor=false&amp;language=ja"></script>
<script>
    var locations = [
      ['〒510-1253 三重県三重郡菰野町潤田１１２６−１', 35.062451, 136.499396, 1],
      ['〒514-0062 三重県津市観音寺町４４３−１', 34.726899, 136.504448, 2],
      ['〒510-1326 三重県三重郡菰野町大字大杉39-1', 35.024539, 136.507989, 3]
    ];
	var style = [
    {
        "featureType": "all",
        "elementType": "all",
        "stylers": [
            {
                "saturation": -100
            },
            {
                "gamma": 1.18
            },
			{
				"lightness": -8
			}
        ]
    }
];

	var marker_image ='img/common/other/ico_pin.png';
	var pinIcon = new google.maps.MarkerImage(marker_image,null,null, null,new google.maps.Size(21, 34));
	var map = new google.maps.Map(document.getElementById('map'), {
	  zoom: 9,
	  icon: pinIcon,
	  center: new google.maps.LatLng(35.024539, 136.507989),
	  mapTypeId: google.maps.MapTypeId.ROADMAP,
	  //draggable: false,
	  scrollwheel: false,
	  style:google.maps.ZoomControlStyle.SMALL
	});

	var infowindow = new google.maps.InfoWindow();

	var marker, i;

	for (i = 0; i < locations.length; i++) {
	  marker = new google.maps.Marker({
		position: new google.maps.LatLng(locations[i][1], locations[i][2]),
		icon: pinIcon,
		map: map,
		style:google.maps.ZoomControlStyle.SMALL
	  });

	var mapType = new google.maps.StyledMapType(style, {name:"Grayscale"});
				map.mapTypes.set('grey', mapType);
				map.setMapTypeId('grey');

	  google.maps.event.addListener(marker, 'click', (function(marker, i) {
		return function() {
		  infowindow.setContent(locations[i][0]);
		  infowindow.open(map, marker);
		}
	  })(marker, i));
	}
</script>

