    function drawCalender(add) {
        $.ajax({
            url: '/Home/DrawCalender',
                type: "post",
                data: "addMonth=" + add,
                dataType: Consts.Json,
                timeout: 3000,
                error: showErrorMessage,
                success: function (result) {
                    var val = result.Message;
                    $("#calenderWrapper").empty();
                    $("#calenderWrapper").html(val);
                    $("#calenderWrapper").data("addmonth", add);
                }
            });
    }
    $(document).ready(function () {
        $(".prevCalenderDraw").live('click',function () {
            var addMonth = parseInt($("#calenderWrapper").data("addmonth")) - 1;
            drawCalender(addMonth);
        });
        $(".nextCalenderDraw").live('click', function () {
            var addMonth = parseInt($("#calenderWrapper").data("addmonth")) + 1;
            drawCalender(addMonth);
        });
    });
</script>
		

