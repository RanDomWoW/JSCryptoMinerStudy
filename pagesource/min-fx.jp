					<script type="text/javascript">
$("document").ready(function(){
	var arr = ["USDJPY","EURJPY","EURUSD","GBPJPY"];
	_getRate();
	setInterval(function(){
		_getRate();
	}, 5000);
	

	function _getRate(){	
		var currentdate = new Date(); 
		utc = currentdate.getTime() + (currentdate.getTimezoneOffset() * 60000);
		nd = new Date(utc + (3600000*9));
		dtupdate = nd.getFullYear()+"/"+(nd.getMonth()+1)+"/"+nd.getDate()+" "+ (nd.getHours() < 10 ? "0" : "") + nd.getHours() + ":" + (nd.getMinutes() < 10 ? "0" : "") + nd.getMinutes() + ":" + (nd.getSeconds() < 10 ? "0" : "") + nd.getSeconds();
		
		$.getJSON("https://min-fx.jp/marketdata/ratedatamodule.php", function(datajson) {
			$.each(datajson.data, function(i,v){
				if($.inArray(v['symbolName'],arr) >= 0){
					ask = parseFloat(v['ask']);
					bid = parseFloat(v['bid']);
					scale = parseInt(v['scale']);
					rate = (ask-bid)*Math.pow(10,scale-1);

					$("li[data-pair="+v['symbolName']+"] .content span.rate").html(rate.toFixed(1));
					$("li[data-pair="+v['symbolName']+"] .content span.ask").html(ask.toFixed(scale));
					$("li[data-pair="+v['symbolName']+"] .content span.bid").html(bid.toFixed(scale));
					$("li[data-pair="+v['symbolName']+"] .title .timeupdate").html(dtupdate);
				}
			});
		});
	}
});
</script>
