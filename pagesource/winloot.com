    <script>
        function showAlreadyRegForm(data, theEmail) {
            $('#loginError').hide();
            $('#mdlLogin_txtEmailAddress').val(theEmail);
            BootstrapDialog.show({
                type: BootstrapDialog.TYPE_INFO,
                title: data['message'],
                message: 'It looks like the email address provided, ' + theEmail + ', is already registered. If you have forgotten your password or would like to login, please use one of the options below.',
                buttons: [{
                    label: 'Login',
                    cssClass: 'btn-primary',
                    action: function (d) {
                        d.close();
                        $("#loginModal").modal("show");
                    }
                }, {
                    label: 'Send My Password',
                    cssClass: 'btn-primary',
                    action: function (d) {
                        $.ajax({
                            type: "POST",
                            dataType: "json",
                            url: '/Home/ForgotPassword',
                            data: { email: theEmail },
                            success: function (data) {
                                d.setMessage("An email has been sent to, " + theEmail +
                                            ". Please check your inbox (and SPAM folders) " +
                                            "for your password. ");
                            },
                            failure: function () {
                                d.message("Sorry, please try again later.");
                            },
                            error: function () {
                                d.message("Sorry, please try again later.");
                            }
                        });
                    }
                }, {
                    label: 'CANCEL',
                    cssClass: 'btn-default',
                    action: function (d) {
                        d.close();
                    }
                }]
            });
        }

        $('#fpwSend').on('click', function (e) {
            e.preventDefault();
            var data = { email: $('#fpwEmail').val() }
            jQuery.ajax({
                'dataType': 'json',
                'success': function (data) {
                    if (data.success) {
                        $("#loginError > div > strong").html(data.response);
                        alert("Password sent!");
                        $("#loginError").show();
                    }
                    else {
                        $("#loginError > div > strong").html(data.response);
                        $("#loginError").show();
                    }
                },
                'type': 'POST',
                'url': '/Home/ForgotPassword',
                'cache': false,
                'data': data
            });
        });
    </script>
