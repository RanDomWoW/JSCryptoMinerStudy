		<script type="text/javascript">
			popupCurrentPopUp = ""
			FadeSpeed = 15;
			PopupSmokeTrans = 80;
			ScreenHeight = 0;
			ScreenWidth = 0;
			startedImgPopup = false;

			function PopUp_popup(popupname, PopupWidth, PopupHeight) {
				if(PopupWidth == 0){
					PopupWidth = 'auto';
				}
				if(PopupHeight == 0){
					PopupHeight = 'auto';
				}
				
				document.getElementById('PopUp_MainPopup').appendChild(document.getElementById(popupname));
				
				document.body.style.overflowY = 'hidden';

				document.getElementById('PopUp_MainPopup').style.visibility = "visible";
				document.getElementById('PopUp_MainPopup').style.display = "block";

				document.getElementById(popupname).style.filter = "alpha(opacity=" + 0 + ")";
				document.getElementById(popupname).style.MozOpacity = .01 * 0;
				document.getElementById(popupname).style.opacity = .01 * 0;
				document.getElementById(popupname).style.KhtmlOpacity = .01 * 0;
				document.getElementById(popupname).style.zIndex = 10302;
				document.getElementById(popupname).style.width = PopupWidth;
				document.getElementById(popupname).style.height = PopupHeight;
				document.getElementById(popupname).style.display = "block";
				document.getElementById(popupname).style.visibility = "visible";

				if (document.body && document.body.offsetWidth) {
					ScreenWidth = document.body.offsetWidth;
					ScreenHeight = document.body.offsetHeight;
				}
				if (document.compatMode == 'CSS1Compat' && document.documentElement && document.documentElement.offsetWidth) {
					ScreenWidth = document.documentElement.offsetWidth;
					ScreenHeight = document.documentElement.offsetHeight;
				}
				if (window.innerWidth && window.innerHeight) {
					ScreenWidth = window.innerWidth;
					ScreenHeight = window.innerHeight;
				}

				popup_leftMargin = Math.round((ScreenWidth - document.getElementById(popupname).offsetWidth) * .5);
				popup_topMargin = Math.round((ScreenHeight - document.getElementById(popupname).offsetHeight) * .5);

				document.getElementById(popupname).style.left = popup_leftMargin + 'px';
				document.getElementById(popupname).style.top = popup_topMargin + 'px';

				document.getElementById('PopUp_smoke').style.filter = "alpha(opacity=" + 0 + ")";
				document.getElementById('PopUp_smoke').style.MozOpacity = .01 * 0;
				document.getElementById('PopUp_smoke').style.opacity = .01 * 0;
				document.getElementById('PopUp_smoke').style.KhtmlOpacity = .01 * 0;

				startedImgPopup = false;
				PopUp_fadeIn('PopUp_smoke', PopupSmokeTrans, 0, popupname);
				popupCurrentPopUp = popupname;
			}

			function PopUp_closepopup(elm) {
				document.body.style.overflowY = 'auto';
				PopUp_fadeOut("PopUp_smoke", 100);
				PopUp_fadeOut(elm, 100);
			}

			function PopUp_fadeIn(itim, maxOpac, opac, popupname) {
				document.getElementById(itim).style.visibility = "visible";
				document.getElementById(itim).style.display = "block";
				if (opac < maxOpac) {
					opac += FadeSpeed;
					if (opac > maxOpac) {
						opac = maxOpac;
					}
					if ((popupname != "") && (itim == 'PopUp_smoke') && (opac >= maxOpac) && (startedImgPopup == false)) {
						startedImgPopup = true;

						PopUp_fadeIn(popupname, 100, 0, '');
					}

					document.getElementById(itim).style.filter = "alpha(opacity=" + opac + ")";
					document.getElementById(itim).style.MozOpacity = .01 * opac;
					document.getElementById(itim).style.opacity = .01 * opac;
					document.getElementById(itim).style.KhtmlOpacity = .01 * opac;

					setTimeout("PopUp_fadeIn('" + itim + "'," + maxOpac + "," + opac + ", '" + popupname + "')", 45);
				} else {
					opac = 0;
				}
			}

			function PopUp_fadeOut(itim, opac) {
				if (opac > 0) {
					opac = opac - FadeSpeed;
					document.getElementById(itim).style.filter = "alpha(opacity=" + opac + ")";
					document.getElementById(itim).style.MozOpacity = .01 * opac;
					document.getElementById(itim).style.KhtmlOpacity = .01 * opac;
					document.getElementById(itim).style.opacity = .01 * opac;
					setTimeout("PopUp_fadeOut('" + itim + "'," + opac + ")", 20);
				} else {
					document.getElementById(itim).style.visibility = "hidden";
					document.getElementById(itim).style.display = "none";

					if (itim == 'PopUp_smoke') {
						document.getElementById("PopUp_MainPopup").style.visibility = "hidden";
						document.getElementById("PopUp_MainPopup").style.display = "none";
						popupCurrentPopUp = "";
					}
				}
			}

			// catches the ESC key press to close whichever popup is open.
			document.onkeyup = (function (e) {
				if (popupCurrentPopUp != "") {
					if (e.keyCode == 27) {
						PopUp_closepopup(popupCurrentPopUp);
					}
				}
			});

			function showLargeImage(ImgNum, ImageWidth, ImageHeight) {
				ImageMaxSize = 550;
				if(ImageWidth > 0 && ImageHeight > 0) {
					if(ImageWidth > ImageHeight){
						Ratio = ImageHeight / ImageWidth;
						ImageWidth = ImageMaxSize;
						ImageHeight = ImageWidth * Ratio
					} else {
						Ratio = ImageWidth / ImageHeight;
						ImageHeight = ImageMaxSize;
						ImageWidth = ImageHeight * Ratio;
					}
				} else {
					ImageHeight = ImageMaxSize;
					ImageWidth = ImageMaxSize
				}
				PaddingTop = ((ImageMaxSize - ImageHeight) /2);

				document.getElementById('imgLargeImageDisplayed').src = document.getElementById('ProductImage_' + ImgNum).src;
				document.getElementById('imgLargeImageDisplayed').style.width = ImageWidth + 'px';
				document.getElementById('imgLargeImageDisplayed').style.height = ImageHeight + 'px';
				document.getElementById('divLargeImageTopPadding').style.height = PaddingTop + 'px';
			}
		</script>

