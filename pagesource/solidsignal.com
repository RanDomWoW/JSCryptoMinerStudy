		<tr>
			<td style="height: 45px;">
				<!-- Search Box -->
				<!-- I put the "background" image simply on a layer beneath the form elements via z-index instead of a real background. I did this because the PNG problems in IE6 and under. -->
				<div id="SearchBox" name="SearchBox" style="text-align: left; background-color: #FFCC66; width: 950px; height: 45px; background-repeat:no-repeat;z-index: 1000; font-size: 19px; position: relative;">
                        <table style="width: 100%; height: 28px;">
                            <tr>
                                <td>

						            <div style="top: 0px; height: 28px; padding-top: 8px; float: left; display: inline;">
							            &nbsp;&nbsp;&nbsp;&nbsp; <b>SEARCH:</b>
							            <input onkeyup="edValueKeyPress(event)" type="text" autocomplete="off" name="q" id="q" value="" style="font-size: 13pt; font-family: Arial; color:#888888; width: 275px; height: 20px; border: 1px solid black;-webkit-appearance: none; -webkit-border-radius:0px; padding: 1px;" />
										<input name="aButtonClick" id="aButtonClick" onclick="submitSearch('')" style="text-align: center; width: 40px; display: inline-block; border: 2px solid #3f6373; font-size: 12pt; font-family: Arial; background-color: #2b4f5d; height: 21px; color: white; font-weight: bold; cursor: pointer; text-decoration: none; -webkit-appearance: none; border-radius: 0;padding: 1px;" value="GO" />
						            </div>
                                </td>
								<script  type="text/javascript">
									function submitSearch(strSuggest){
										txtSearchQuery = document.getElementById('q').value;

										try{
											var searchData = {};
											txtClickType = 'submit';
											if(strSuggest != ''){
												searchData["aq"] = strSuggest;
												txtClickType = 'click';
											}
											searchData["q"] = txtSearchQuery;

											BrTrk.getTracker().logEvent(
													"suggest",
													txtClickType,
													searchData,
													{},
													true);
										} catch(err) {}
										
										window.location = '/search.asp?q=' + txtSearchQuery;
									}
									
									currenthighlight = 0;
									currentPredcitiveLength = 0;
									function edValueKeyPress(e){
										var KeyPressed = e.keyCode || e.which;
										partialText = document.getElementById('q').value;
										txtBox = document.getElementById("q")
										txtID = txtBox.id;
										AutoCompleteID =  txtID + '_AutoComplete';
										
										if (partialText.length > 0) {
											if (!((KeyPressed == 37) || (KeyPressed == 38) || (KeyPressed == 39) || (KeyPressed == 40) || (KeyPressed == 9) || (KeyPressed == 16) || (KeyPressed == 17) || (KeyPressed == 18) || (KeyPressed == 32) || (KeyPressed == 13))) {
												AutoCompleteHoverOver = '_AutoCompleteHoverOver';
												if (document.getElementById(AutoCompleteHoverOver) != null) {
													document.getElementById(AutoCompleteHoverOver).style.display = 'none';
												}
										
												if (window.XMLHttpRequest) {
													ajxSuggest = new XMLHttpRequest();
												} else {
													ajxSuggest = new ActiveXObject("Microsoft.XMLHTTP");
												}

												var ajxSuggestURL = "/BloomReachSuggest.asp?q=" + partialText;

												ajxSuggest.onreadystatechange = funcAjxSuggestResponse;
												ajxSuggest.open("GET", ajxSuggestURL, true);
												ajxSuggest.send();
											} else {
												try{
													if ((document.getElementById(AutoCompleteID) != null) || (document.getElementById(AutoCompleteID).style.display == 'block')) {

														if (KeyPressed == 38) { //Up
															if (currenthighlight != 1) {
																UnhoverSuggestedSearch(AutoCompleteID + '_' + currenthighlight);
																newHighlight = currenthighlight - 1;

																elmToHover = AutoCompleteID + '_' + newHighlight;
																HoverSuggestedSearch(elmToHover);
																currenthighlight = newHighlight;
															} 
														}
														if (KeyPressed == 40) { //Down
															if (currenthighlight != currentPredcitiveLength) {
																newHighlight = currenthighlight + 1;
																if (currenthighlight != 0) {
																	UnhoverSuggestedSearch(AutoCompleteID + '_' + currenthighlight);
																}

																elmToHover = AutoCompleteID + '_' + newHighlight;
																HoverSuggestedSearch(elmToHover);
																currenthighlight = newHighlight;
															}
														}
														if (KeyPressed == 13) { // Enter
															if (currenthighlight != 0){
																optionid = AutoCompleteID + '_' + currenthighlight;
																clickSuggestedSearch(optionid, true)
															} else {
																submitSearch('');
															}
														}
													}
												} catch(err){}
											}
											
											if (KeyPressed == 13) {
												submitSearch('');
											}
										} else {
											if (document.getElementById(AutoCompleteID) != null) {
												document.getElementById(AutoCompleteID).style.display = 'none';
											}
										}
									}


									function funcAjxSuggestResponse() {
										if (ajxSuggest.readyState == 4) {
											strSuggestions = ajxSuggest.responseText;
											
											if(strSuggestions != '') {
												txtBox = document.getElementById("q")

												// Get Text Box Location
												txtBoxLeft = getPosition('q').x.valueOf();
												txtBoxTop = getPosition('q').y.valueOf();
												txtBoxWidth = txtBox.offsetWidth;
												txtBoxHeight = txtBox.offsetHeight;
												txtID = txtBox.id;
												
												// Set autocomplete info
												AutoCompleteTop = txtBoxTop + txtBoxHeight;
												AutoCompleteLeft = txtBoxLeft;
												AutoCompleteWidth = 300;
												AutoCompleteID = txtID + '_AutoComplete';
												

												// Create Autocomplete object
												if (document.getElementById(AutoCompleteID) == null) {
													var div = document.createElement("div");
													div.id = AutoCompleteID;
													document.body.appendChild(div);
												}

												document.getElementById(AutoCompleteID).style.display = 'none';
												document.getElementById(AutoCompleteID).style.font = txtBox.style.font;
												document.getElementById(AutoCompleteID).style.fontFamily = txtBox.style.fontFamily;
												document.getElementById(AutoCompleteID).style.top = AutoCompleteTop + 'px';
												document.getElementById(AutoCompleteID).style.left = AutoCompleteLeft + 'px';
												document.getElementById(AutoCompleteID).style.width = AutoCompleteWidth + 'px';
												//document.getElementById(AutoCompleteID).style.height = 180 + 'px';
												document.getElementById(AutoCompleteID).style.background = 'white';
												document.getElementById(AutoCompleteID).style.border = '1px solid black';
												document.getElementById(AutoCompleteID).style.position = 'fixed';
												document.getElementById(AutoCompleteID).style.zIndex = "1001";
												document.getElementById(AutoCompleteID).style.boxShadow = "0px 2px 12px #000000";

												newHTML = "";
												
												var res = strSuggestions.split('\n');
												currentPredcitiveLength = res.length 
												for (i = 0; i < currentPredcitiveLength; i++) {
													strSuggestion = res[i];
													if (strSuggestion != '') {
														strSuggestionHTML = strSuggestion;
														strSuggestionFull = strSuggestion;
														newHTML = newHTML + '<div style="padding: 2px; cursor: pointer; white-space: nowrap; overflow: hidden;" onclick="clickSuggestedSearch(this.id, true)" onmouseover="HoverSuggestedSearch(this.id)" id="' + AutoCompleteID + '_' + (i + 1) + '" name="' + AutoCompleteID + '_' + (i + 1) + '" onmouseout="UnhoverSuggestedSearch(this.id)">' + strSuggestionHTML + '</div>';
													}
												}

												if (newHTML != '') {
													currenthighlight = 0;
													replaceHtml(AutoCompleteID, newHTML);
													document.getElementById(AutoCompleteID).style.display = 'block';

													document.onmouseup = function () { setTimeout('hideSuggestedSearch()', 20) };
												}
											}
										}
									}

									function hideSuggestedSearch() {
										txtBox = document.getElementById("q")
										txtID = txtBox.id;
										AutoCompleteID = txtID + '_AutoComplete';
										
										AutoCompleteHoverOver = txtID + '_AutoCompleteHoverOver';
										if (document.getElementById(AutoCompleteHoverOver) != null) {
											document.getElementById(AutoCompleteHoverOver).style.display = 'none';
										}
										if (document.getElementById(AutoCompleteID) != null) {
											document.getElementById(AutoCompleteID).style.display = 'none';
										}
									}

									function HoverSuggestedSearch(HoverElementId) {
										document.getElementById(HoverElementId).style.background = '#DDDDEE';

										txtBox = document.getElementById("q")
										txtBoxLeft = findAbsolutePositionLeft(txtBox);
										txtBoxTop = findAbsolutePositionTop(txtBox);
										txtBoxWidth = txtBox.offsetWidth;
										txtBoxHeight = txtBox.offsetHeight;
										txtID = txtBox.id;

										AutoCompleteHoverOver = txtID + '_AutoCompleteHoverOver';
										if (document.getElementById(AutoCompleteHoverOver) == null) {
											var div = document.createElement("div");
											div.id = AutoCompleteHoverOver;
											document.body.appendChild(div);
										}

										textInBox = document.getElementById(HoverElementId).innerHTML.replace('<b>', '').replace('</b>', '');

										document.getElementById(AutoCompleteHoverOver).style.display = 'none';
										document.getElementById(AutoCompleteHoverOver).style.position = 'fixed';
										document.getElementById(AutoCompleteHoverOver).style.top = (txtBoxTop + 1) + 'px';
										document.getElementById(AutoCompleteHoverOver).style.left = (txtBoxLeft + 1) + 'px';
										document.getElementById(AutoCompleteHoverOver).style.width = (txtBoxWidth - 1) + 'px';
										document.getElementById(AutoCompleteHoverOver).style.height = (txtBoxHeight - 1) + 'px';
										document.getElementById(AutoCompleteHoverOver).style.background = 'white';
										document.getElementById(AutoCompleteHoverOver).style.overflow = 'hidden';
										document.getElementById(AutoCompleteHoverOver).style.whiteSpace = 'nowrap';
										document.getElementById(AutoCompleteHoverOver).style.font = txtBox.style.font;
										document.getElementById(AutoCompleteHoverOver).style.fontFamily = txtBox.style.fontFamily;
										document.getElementById(AutoCompleteHoverOver).style.fontSize = txtBox.style.fontSize;
										document.getElementById(AutoCompleteHoverOver).style.color = txtBox.style.color;
										document.getElementById(AutoCompleteHoverOver).style.zIndex = "1002";
										document.getElementById(AutoCompleteHoverOver).innerHTML = textInBox;
										document.getElementById(AutoCompleteHoverOver).style.display = 'block';
										document.getElementById(AutoCompleteHoverOver).onclick = function () { clickSuggestedSearch(HoverElementId, false) };
									}

									function clickSuggestedSearch(HoverElementId, withSubmit) {
										filltext = document.getElementById(HoverElementId).innerText.replace('<b>', '').replace('</b>', '').replace('<b>', '').replace('</b>', '').replace('<b>', '').replace('</b>', '').replace('<b>', '').replace('</b>', '');

										partialText = document.getElementById("q").value;

										try{
											var searchData = {};
											searchData["aq"] = partialText;
											searchData["q"] = filltext;

											BrTrk.getTracker().logEvent(
													"suggest",
													"click",
													searchData,
													{},
													true);
										} catch(err) {}
										
										document.getElementById("q").value = filltext;
										currenthighlight = 0;

										submitSearch(partialText);
									}

									function UnhoverSuggestedSearch(HoverElementId) {
										document.getElementById(HoverElementId).style.background = '#FFFFFF';

										AutoCompleteHoverOver = txtID + '_AutoCompleteHoverOver';
										if (document.getElementById(AutoCompleteHoverOver) != null) {
											document.getElementById(AutoCompleteHoverOver).style.display = 'none';
										}
									}

									if ("onmousewheel" in document) {
										document.onmousewheel = function () { hideSuggestedSearch() };
									} else {
										document.addEventListener('DOMMouseScroll', function () { hideSuggestedSearch() }, false);
									}

									function findAbsolutePositionTop(element) {
										var yPosition = 0;

										while (element) {
											yPosition += (element.offsetTop - element.scrollTop + element.clientTop);
											element = element.offsetParent;
										}
										return yPosition;
									}
									function findAbsolutePositionLeft(element) {
										var xPosition = 0;

										while (element) {
											xPosition += (element.offsetLeft - element.scrollLeft + element.clientLeft);
											element = element.offsetParent;
