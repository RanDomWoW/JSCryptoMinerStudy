<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.2/angular.min.js"></script>
<script src="/gd_assets/pagination.js"></script>

<script type="application/javascript">
    var API_ENDPOINT = "https://marin-basic.com/mbapi/api";
    //var API_ENDPOINT = "http://127.0.0.1:1234/api";

    var app = angular.module('gamedeals',['ng-pagination']);
    app.controller('HomeController', ['$scope','$http', function($scope,$http) {
        $scope.loading = true;
        console.log("Angular Init!");
        $scope.deals = [];
        $scope.onlyOne = false;
        $scope.chrono = false;

        $http.get(API_ENDPOINT+"/game_deals").then(function(res){
            $scope.deals = [];
            if(!res.data.results) {
                return;
            }

            angular.forEach(res.data.results, function(val){
                switch (val.store) {
                    case "Steam":
                        val.icon = "steam";
                        break;
                    case "Gamers Gate":
                        val.icon = "gamersGate";
                        break;
                    case "Fanatical":
                        val.icon = "bundleStars";
                        break;
                    case "GOG":
                        val.icon = "gog";
                        break;
                    case "Green Man Gaming":
                        val.icon = "gmg";
                        break;
                    case "Win Game Store":
                        val.icon = "wgs";
                        break;
                    case "Sila Games":
                        val.icon = "silaGames";
                        break;
                    case "Amazon":
                        val.icon = "amazon";
                        break;
                    case "Humble Bundle":
                        val.icon = "humble";
                        break;
                    default:
                        val.icon = val.store;
                }
                if (val.store === "Chrono") {
                    $scope.chrono = {
                        name: val.games[0].name,
                        special: val.games[0].special,
                        discount: val.games[0].discount,
                        url: val.games[0].url
                    }
                }else{
                    $scope.deals.push(val);
                }
            });
            $scope.loading = false;
        });

        $scope.showOnly = function(d) {
            $scope.onlyOne = {
                page: 1,
                take: 12,
                list: [],
                data: d.games,
                store: d.store,
                icon: d.icon,
                gamesLenght: d.games.length,
                activate: $scope.activate
            };
        };

        $scope.hideOnly = function() {
            $scope.onlyOne = false;
        };

        $scope.paginationLimit = function(data) {
            return 3;
        };

        $scope.activate = function(page, take) {
            $scope.onlyOne.page = page;
            var list = [];
            for (var i = (page - 1) * take; i < page * take; i++) {
                if($scope.onlyOne.data[i]) {
                    list.push($scope.onlyOne.data[i]);
                }
            }
            $scope.onlyOne.list = list;
        }

    }]);

</script>

