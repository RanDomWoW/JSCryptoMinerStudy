                <span class="dealerName {{d.icon}}">{{d.store}}</span>
                <a class="showMore" href="javascript:void(0);" ng-click="showOnly(d)">More</a>
            </div>

            <div class="dealers-3-col dealerGameList">

                <a ng-repeat="g in d.games | filter:searchText | limitTo: searchText.length > 1 ? 20 : 3" href="{{g.url}}" target="_blank">
                    <div class="imgWrap">
                        <img class="horizontal" src="{{g.thumb}}" alt="" />
                    </div>
                    <div class="dealPrice">
                        <span class="oldPrice">-{{g.discount}}%</span>
                        <span class="gamePrice">{{d.store == 'Amazon'  || d.store == 'Humble Bundle'  ?  '$' : '€'}}{{g.special}}</span>

