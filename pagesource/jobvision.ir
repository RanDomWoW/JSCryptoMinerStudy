        $(document).ready(function () {

            //$('#custom-nav').addClass('affix');
            //$(".navbar-fixed-top").addClass("top-nav-collapse");
            $("#img-logo").show();
            $("#img-home").css('display', 'block');

            var charCount = $('a.UlLoginName').text().length - 96;
            if (charCount > 30) {
                $('.UlLoginName').addClass("UlLoginNameFontSize");
            }

            $(".suit").click(function () {
                if ($(".linkableSlide.resumeBuilder").hasClass("active"))
                {
                    location.href = '/Home/Employee';
                }
                
                else if ($(".linkableSlide.matchingSystem").hasClass("active")) {
                    location.href = '/Home/Employee';
                }
                else if ($(".linkableSlide.assessmentPackages").hasClass("active")) {
                    location.href = '/Home/Employer';
                }
            });
            $(".suit").mouseover(function () {
                //if ($(".linkableSlide.resumeBuilder").hasClass("active") || $(".linkableSlide.jobFair").hasClass("active") || $(".linkableSlide.matchingSystem").hasClass("active") || $(".linkableSlide.assessmentPackages").hasClass("active")) {
                if ($(".linkableSlide.resumeBuilder").hasClass("active") || $(".linkableSlide.matchingSystem").hasClass("active") || $(".linkableSlide.assessmentPackages").hasClass("active")) {
                    $(".suit").addClass("hoverSlide");
                }
                else {
                    $(".suit").removeClass("hoverSlide");
                }
            });
            $(".suit").mouseleave(function () {
                $(".suit").removeClass("hoverSlide");
            });



            $("div.EmployerPart").click(function () {
                location.href = '/Home/Employer';
            });
            $("div.EmployeePart").click(function () {
                location.href = '/Home/Employee';
            });

            $("div.EmployerPart").mouseover(function () {
                $("div.EmployerPart img").animate({
                    "transform": "rotate(3deg)",
                    "-webkit-transform": "rotate(3deg)",
                    opacity: .7
                }, 100);
            });
            $("div.EmployerPart").mouseleave(function () {
                $("div.EmployerPart img").animate({
                    "transform": "rotate(0deg)",
                    "-webkit-transform": "rotate(0deg)",
                    opacity: 1
                }, 100);
            });
            $("div.EmployeePart").mouseover(function() {
                $("div.EmployeePart img").animate({
                    "transform": "rotate(-3deg)",
                    "-webkit-transform": "rotate(-3deg)",
                    opacity: .7
                }, 100);
            });
            $("div.EmployeePart").mouseleave(function () {
                $("div.EmployeePart img").animate({
                    "transform": "rotate(0deg)",
                    "-webkit-transform": "rotate(0deg)",
                    opacity: 1
                }, 100);
            });

            //نمایشگاه کار عادی
            $("#jobFairHeader span.close").click(function () {
                $("#jobFairHeader").slideUp("slow");
                $("#custom-nav").animate({ top: "0px" }, 1000);
                $("#main-header-container").animate({ top: "0px" }, 1000);
                $("#body-container").animate({ top: "59px" }, 1000);
                $("#free-space").animate({ "padding-bottom": "5%" }, 1000);
                setTimeout(function () { $("#jobFairHeader").remove(); }, 1000);
            });
            //نمایشگاه کار ملی
            $("#nationalJobFairHeader span.close").click(function () {
                $("#nationalJobFairHeader").slideUp("slow");
                $("#custom-nav").animate({ top: "0px" }, 1000);
                $("#main-header-container").animate({ top: "0px" }, 1000);
                $("#body-container").animate({ top: "59px" }, 1000);
                $("#free-space").animate({ "padding-bottom": "5%" }, 1000);
                setTimeout(function () { $("#nationalJobFairHeader").remove(); }, 1000);
            });
        });

