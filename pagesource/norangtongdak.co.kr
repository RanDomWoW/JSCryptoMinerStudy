        <script>
        	$(function (){

								setInterval(function (){
					$("body > div.slider > div.wrap > #right").click();
				},4000);
				
				setInterval(function (){
					var ind = Number($(".slide_shop #button2 > div.active").index());
					ind += 1;
					if(ind == 3){
						$(".slide_shop #button2 > div:nth-child(1)").click();
					}else{
						ind++;
						console.log(ind);
						$("#button2 > div[data-nth="+ind+"]").click();
					}
				},4000);
				
								
				ok = true;
				ok2 = true;
				slider = 1;
				$(document).on('click','#left',function (){
					if(ok){
						ok = false;	
						if(slider==1){
							slider = 3;
						}else{
							slider --;	
						}
						var left = (slider - 1) * 100;
						$(".slider .slider_content").animate({left:'-'+left+"%"},1000,function (){
							ok = true;	
						});
						$(".slider .wrap #button div").attr('class','');
						$(".slider .wrap #button div:nth-child("+slider+")").attr('class','active');
					}
				});
				$(document).on('click','#right',function (){
					if(ok){
						ok = false;	
						if(slider==3){
							slider = 1;
						}else{
							slider ++;	
						}
						var left = (slider - 1) * 100;
							$(".slider .slider_content").animate({left:'-'+left+"%"},1000,function (){
								ok = true;	
							});
						$(".slider .wrap #button div").attr('class','');
						$(".slider .wrap #button div:nth-child("+slider+")").attr('class','active');
					}
				});
				$(document).on('click',".slider .wrap #button div",function (){
					if(ok){
						ok = false;
						slider = $(this).attr('data-nth');
						var left = (slider - 1) * 100;
						$(".slider .slider_content").animate({left:'-'+left+"%"},1000,function (){
							ok = true;	
						});
						$(".slider .wrap #button div").attr('class','');
						$(".slider .wrap #button div:nth-child("+slider+")").attr('class','active');
					}
				});


				$(document).on('click',".slide_shop #button2 div",function (){
					if(ok2){
						ok2 = false;
						slider = $(this).attr('data-nth');
						var left = (slider - 1) * 100;
						$(".slide_shop div:nth-child(3) > div").animate({left:'-'+left+"%"},1000,function (){
							ok2 = true;	
						});
						$(".slide_shop #button2 div").attr('class','');
						$(".slide_shop #button2 div:nth-child("+slider+")").attr('class','active');
					}
				});
								
				$(document).on('click','.close',function (){
					$("header").animate({height:'0px'},500);
					if($("body > header > input").prop('checked')){
						setCookie("header","header",1);
					}
				});
				$(document).on('click','.bottom .right',function (){
					var it = $(this).attr('data-name');
					var date = new Date();
					var date = date.getFullYear()+"-"+date.getMonth()+"-"+date.getDate();
					if($("."+it+" > div > div.left > input").prop('checked')){
						localStorage[it] = date;
					}
					$("."+it).remove();
				});
				
				
				
				for(var k =1; k<=4; k++){
					checkPoupCookie("popup"+k);	
				}
				
				function checkPoupCookie(cookieName){
					var date = new Date();
					var date = date.getFullYear()+"-"+date.getMonth()+"-"+date.getDate();					
					if(localStorage[cookieName]){
						if(localStorage[cookieName] == date){
							$("."+cookieName).remove();	
						}
					}
				}

			});		
        </script>
