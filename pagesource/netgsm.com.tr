		$(".evezneclick").click(function(){			
			if(dolumu==0)
			{
				$('<iframe width="100%" height="250" src="/evezne/index.asp"  style="border:0px solid #ccc" frameborder="0"  class="loading_iframe" ></iframe>').appendTo(".evezne_iframe");
				$('.loading_iframe').load(function() {	
					heg=parseInt(this.contentWindow.document.body.offsetHeight);
					if (heg==0){
						heg=650;
					}
					if (heg>650){
						heg=650;
					}
					this.style.height =( heg+25 )+ 'px'; 
					$('#mymodala').modal('show');
				});
				dolumu=1;
				return false;
			}
			$('#mymodala').modal('show');
		});
	});
</script>

