<script>
	var metinler = "Yeni nesil telekom operatörü | İletişim kontrolü sizde! | Mobil kullanım imkanı | Ödeme kolaylığı | Kesintisiz hizmet, pratik kurulum ";
	var metinler_alt = "Kurumsal kimliğinizi güçlendirin! | Firmanızın 0850 numara, telefon, santral, sms ve mobil iletişim ihtiyaçlarını karşılayın! | Tüm Netgsm hizmetlerini mobil uygulamalar ile kullanın | Güvenilir ödeme platformu ve anlaşmalı bankalar ile hızlı ve %100 güvenli ödeme seçenekleri | İşletmenizin büyüklüğüne bakılmaksızın pratik kurulum, esnek yapı ";
	
	$(document).ready(function() {
		start();
	})
	
	var $el, txt, txtLen, timeOut, char, durma_suresi, altcumle;
	var ilkcumle = 0;
	
	function start() {
		
		var mySplitResult = metinler.split("|");
		if (ilkcumle >= parseInt(mySplitResult.length)) {
			ilkcumle = 0;
		}
		
		var cumleler_alt = metinler_alt.split("|");
		altcumle = "";
		
		if (cumleler_alt[ilkcumle]) {
			altcumle = cumleler_alt[ilkcumle];
			$(".writer_alt").html(altcumle);
			$(".writer_alt").fadeIn('slow');
		}
		
		$('.writer').text(mySplitResult[ilkcumle]);
		
		$el = $('.writer'),
		
		txt = $el.text(),
		txtLen = txt.length,
		timeOut,
		char = 0;
		durma_suresi = 5000;
		$el.text('|');
		ilkcumle++;
		typeIt();
	}
	
	function typeIt() {
		var humanize = Math.round(Math.random() * (200 - 30)) + 30;
		var humanize = 100;
		var humanize_del = Math.round(Math.random() * (20)) + 30;
		
		function silme() {
			
			$el.text($el.text());
			$el.html($el.text().slice(0, -1)); // remove the '|'   
			
			if ($el.text() != "") {
				var myVary = setInterval(function() {
					clearInterval(myVary);
					silme();
				}, humanize_del);
				} else {
				$(".writer_alt").fadeOut('fast');
				var myVary = setInterval(function() {
					clearInterval(myVary);
					start();
				}, humanize);
			}
		}
		
		function kontrol() {
			char++;
			var type = txt.substring(0, char);
			
			$el.text(type);
			
			if (char == txtLen) {
				
				$el.html(type + " &nbsp;"); // remove the '|'  
				
				var myVarx = setInterval(function() {
					$el.text(type);
					clearInterval(myVarx);
					silme();
				}, durma_suresi);
				} else {
				typeIt();
			}
		}
		var myVar = setInterval(function() {
			clearInterval(myVar);
			kontrol();
		}, humanize);
	};
</script>

