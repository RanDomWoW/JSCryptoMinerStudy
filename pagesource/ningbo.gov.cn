<style>
.dh_href{ height:18px; float:right; overflow:hidden;margin-right:15px; cursor:pointer;}
#BG_Switch{ position:absolute;top:0px; left:0px; z-index:-999; text-align:center; height:100%; overflow:hidden; width:100%; min-width:1000px;}
</style>
<script  language="JavaScript" type="text/javascript">
var ie 
if ((navigator.userAgent.indexOf('MSIE') >= 0) 
    && (navigator.userAgent.indexOf('Opera') < 0)){
     ie = 1
}
var body_H = window.innerHeight;
	
	$(document).ready(function(e) {
		 if(ie==1){
			 body_H=document.documentElement.clientHeight;
			 }
         
		 if(565<=body_H<=700){
			 $("#logo").height(body_H+"px");
			 $("#BG_Switch").height(body_H+"px");
			 $("#nav").css("position","fixed");
			 $("#nav").css("bottom","0px");
			 $("#nav").css("left","0px");
			 }
		 if(body_H<565){
			 $("#BG_Switch").height("700px");
			 $("#logo").height("700px");
			 $("#nav").css("position","absolute");
			 $("#nav").css("top","496px");
			 $("#nav").css("left","0px");
			 }
		 if(700<body_H){
			 var mar = (body_H-700)/2;
			 $("#BG_Switch").height("700px");
			 $("#logo").height("700px");
			 $("#logo").css("margin-top",mar+"px");
			 $("#BG_Switch").css("margin-top",mar+"px");
			 $("#nav").css("position","absolute");
			 $("#nav").css("top","496px");
			 $("#nav").css("left","0px");
			 }
   			 
			
    });
    </script>
</head>
