<script>
    $(document).ready(function() {
        var tempcount = 0
        $('.goodnews-checkbox-container').click(function() {
            if (tempcount < 2) {
                tempcount += 1;
                //console.log('on  ' + tempcount);
                caleid_good();
                yaCounter35840930.reachGoal('goodnews'); return true;
            } else if (tempcount >= 2) {
                tempcount++;
                //console.log('off  ' + tempcount);
                caleid_now();
                if (tempcount == 4) {
                  tempcount = 0;
                };
            };
            return tempcount;
        })
		
	
		
    });
</script>




