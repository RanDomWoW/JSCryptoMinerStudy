.feed_inner {
  position: fixed;
	height: 100%;
	left: calc((100% - 1037px)/2 - 143px);
	left: -webkit-calc((100% - 1037px)/2 - 143px);
  left: -moz-calc((100% - 1037px)/2 - 143px);
	opacity: 1;
	transform: translateY(0);
	-webkit-transform: translateY(0);
	-moz-transform: translateY(0);
	-o-transform: translateY(0);
	transition: transform .5s ease;
	-webkit-transition: transform .5s ease;
	-moz-transition: transform .5s ease;
	-o-transition: transform .5s ease;
}

.feed_inner.hidden {
	opacity: 0;
	transform: translateY(100%);
	-webkit-transform: translateY(100%);
	-moz-transform: translateY(100%);
	-o-transform: translateY(100%);
}

.feed_inner.abs {
	position: absolute;
	left: 3px;
}

#feed_show.abs {
	position: absolute;
	transition: none;
}

.feed_inner.sm {
	left: 0;
}

@keyframes move_left {
	0% {
		transform: translate(0, 0);
		-webkit-transform: translate(0, 0);
		-moz-transform: translate(0, 0);
		-o-transform: translate(0, 0);
	}
	100% {
		transform: translate(-125px, 0);
		-webkit-transform: translate(-125px, 0);
		-moz-transform: translate(-125px, 0);
		-o-transform: translate(-125px, 0);
	}
}

@keyframes move_right {
		0% {
			transform: translate(0, 0);
			-webkit-transform: translate(0, 0);
			-moz-transform: translate(0, 0);
			-o-transform: translate(0, 0);
		}
		100% {
			transform: translate(125px, 0);
			-webkit-transform: translate(125px, 0);
			-moz-transform: translate(125px, 0);
			-o-transform: translate(125px, 0);
		}
}

@keyframes move_leftx2 {
		0% {transform: translate(0, 0)}
		100% {transform: translate(-286px, 0)}
}

@keyframes move_rightx2 {
		0% {
			transform: translate(0, 0);
			-webkit-transform: translate(0, 0);
			-moz-transform: translate(0, 0);
			-o-transform: translate(0, 0);
		}
		100% {
			transform: translate(286px, 0);
			-webkit-transform: translate(286px, 0);
			-moz-transform: translate(286px, 0);
			-o-transform: translate(286px, 0);
		}
}

.main_columnzx.animated {
		transition: transform .8s ease;
		-webkit-transition: transform .8s ease;
		-moz-transition: transform .8s ease;
		-o-transition: transform .8s ease;
		animation: move_left .8s forwards;
		-webkit-animation: move_left .8s forwards;
		-moz-animation: move_left .8s forwards;
}

.main_columnzx.unanimated {
		transition: transform .8s ease;
		-webkit-transition: transform .8s ease;
		-moz-transition: transform .8s ease;
		-o-transition: transform .8s ease;
		animation: move_right .8s forwards;
		-webkit-animation: move_right .8s forwards;
		-moz-animation: move_right .8s forwards;
}

.main_columnzx.animatedx2 {
		transition: transform .8s ease;
		animation: move_leftx2 .8s forwards;
		-webkit-animation: move_leftx2 .8s forwards;
		-moz-animation: move_leftx2 .8s forwards;
}

.main_columnzx.unanimatedx2 {
		transition: transform .8s ease;
		-webkit-transition: transform .8s ease;
		-moz-transition: transform .8s ease;
		-o-transition: transform .8s ease;

		animation: move_rightx2 .8s backwards;
		-webkit-animation: move_rightx2 .8s backwards;
		-moz-animation: move_rightx2 .8s backwards;
}

.main_columnzx.tight {
	transition: transform .8s ease;
	-webkit-transition: transform .8s ease;
	-moz-transition: transform .8s ease;
	-o-transition: transform .8s ease;
}


