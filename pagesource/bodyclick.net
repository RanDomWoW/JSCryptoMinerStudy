    function loadImage(url)
    {
        var image = new Image();
        if (image.addEventListener) {
            image.addEventListener('load', imageLoaded, false);
        } else {
            image.attachEvent('onload', imageLoaded);
        }

        image.src = url;
        return image;
    }

    function imageLoaded()
    {
        imagesLoaded++;
        if (imagesLoaded == imageFiles.length) {
            $('.homeBg').attr('src', imageFiles[0]);
            $('.adv-smile img').attr('src', imageFiles[1]);
            $('.web-smile img').attr('src', imageFiles[2]);
            $('.homeBg, .adv-smile img, .web-smile img').css('visibility', 'visible');
        }
    }
