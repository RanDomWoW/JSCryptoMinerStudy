  function wait_jquery() {
    if (window.jQuery)
      netstudio_kouris_skin();
    else
      setTimeout(function() { wait_jquery() }, 50);
  }
  function netstudio_kouris_skin() {
    jQuery(document).ready(function() {
      if (jQuery(window).width() >= 960) {
        $left = '';
        $right = '';
        $link = '';
        $layoutContainer = jQuery(".layout-container");
        $layoutContainer.css('position', 'relative');
        if ($left.length > 0) {
        // append left bannner
          $leftbanner = jQuery('<div><img src="' + $left + '"></div>');
          $tempimg = $leftbanner.find('img').css('max-width', 'none');
          $leftbanner.css('position', 'fixed');
          $leftbanner.css('top', '0');
          $leftbanner.css('right', jQuery(window).width() - $layoutContainer.offset().left);
          $leftbanner.appendTo(jQuery(".layout-container"));
          $leftbanner.css('width', 'auto');
          $leftbanner.css('height', 'auto');
        }
        if ($right.length > 0) {
        // append right bannner
          $rightbanner = jQuery('<div><img src="' + $right + '"></div>');
          $tempimg = $rightbanner.find('img').css('max-width', 'none');
          $rightbanner.css('position', 'fixed');
          $rightbanner.css('top', '0');
          $rightbanner.css('left', $layoutContainer.offset().left + $layoutContainer.width());
          $rightbanner.appendTo(jQuery(".layout-container"));
          $rightbanner.css('width', 'auto');
          $rightbanner.css('height', 'auto');
        }
        if ($link.length > 0) {
          if (typeof ($leftbanner) != 'undefined')
            $leftbanner.wrapInner('<a href="' + $link + '" target="_blank"></a>');
          if (typeof ($rightbanner) != 'undefined')
            $rightbanner.wrapInner('<a href="' + $link + '" target="_blank"></a>');
        }
        jQuery(window).resize(function() {
          if (typeof ($leftbanner) != 'undefined')
            $leftbanner.css('right', jQuery(window).width() - $layoutContainer.offset().left);
          if (typeof ($rightbanner) != 'undefined')
            $rightbanner.css('left', $layoutContainer.offset().left + $layoutContainer.width());
        });
        
      }
    });
  }
  wait_jquery();
</script>
    
