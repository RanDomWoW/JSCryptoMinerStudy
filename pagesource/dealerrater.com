                            .show();


                    });


                // hide reviews content sections
                var percentWidth = (100 *
                    parseFloat($('.review-module').css('width')) /
                    parseFloat($('.review-module').parent().css('width')));
                if (percentWidth >= 50) {
                    $("#review_1").hide();
                    $("#review_2").hide();
                    $("#review_0").addClass("singleReview");
                } else if ((percentWidth < 50) && (percentWidth > 40)) {
                    $("#review_2").hide();
                }
                $('#review_3').hide();
                $('#review_4').hide();
                $('#review_5').hide();


                // initialize video
                var playerInstance = jwplayer("consumerVideo");
                    playerInstance.setup({
                        file: "https://video.dealerrater.com/video/DealerRater-Consumer-FC-SFX-03.mp4",
