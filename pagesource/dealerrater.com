                        mediaid: "PTyIiS3g",
                        width: '100%',
                        aspectratio: '16:9',
                        skin: 'five'
                });

                playerInstance.on("ready", function() { $("#consumerVideo").addClass("ready");});

                // Retrieve last searched postal code.
                if($.cookie('cZipCode'))
                    $('.omniSearchInput').val($.cookie('cZipCode'));
            });

        var toggleMoreReviews = function() {
            var percentWidth = (100 *
                parseFloat($('.review-module').css('width')) /
                parseFloat($('.review-module').parent().css('width')));
            var counter = 0;
            // for mobile show 1 at a time
            if (percentWidth >= 50) {
                $(".review-module")
                    .each(function() {
                        if (($(this).css("display") === "none") && (counter < 1)) {
                            $(this).show();
                            counter++;
                        }
                    });
            }
            // for tablet show 2 at a time
            else if ((percentWidth < 50) && (percentWidth > 40)) {
                $(".review-module")
                    .each(function() {
                        if (($(this).css("display") === "none") && (counter < 2)) {
                            $(this).show();
                            counter++;
                        }
                    });
            } else {
                $(".review-module").show();
            }

            var allReviewsVisible = true;
            $('.review-module').each(function () {

                if ($(this).css('display') == 'none') {
                    allReviewsVisible = false;
                }
            });
            if (allReviewsVisible) {
                $('#moreReviews').hide();
            }

        };

        var doDealerLink = function(src) {
