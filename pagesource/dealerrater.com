                            var hostname = "https://" + window.location.hostname;
                            window.location.href = searchUrlHelper.RecallsSearch(hostname, make, model);
                        }
                        return false;
                    });

                // on click of MOBILE RECALLS search, complete search
                var clickCount = 0;
                $("#mobileRecalls .omniSearchModel")
                    .click(function() {
                        clickCount++;

                        if (clickCount === 2) {
                            $('#mobileRecalls .omniSearchSubmit').trigger('click');
                            return false;
                        }
                    });

                // Recalls button handler
                $("#mobileRecalls .omniSearchSubmit")
                    .click(function() {
                        $("#mobileRecalls").validate();

                        if ($("#mobileRecalls").valid()) {
                            var model = $(this).closest(".searchWrapper").find(".omniSearchModel").val().split("|")[0];
                            var make = $(this).closest(".searchWrapper").find(".omniSearchManu").val().split("|")[0];

                            var hostname = "https://" + window.location.hostname;
                            window.location.href = searchUrlHelper.RecallsSearch(hostname, make, model);
                        }
                        return false;
                    });

                // initial make model load
                $(".omniSearchManu.make-model")
                    .each(function() {
                        var parentDiv = $(this).closest(".searchWrapper");
                        if ($(parentDiv).find(".omniSearchManu").val() !== "") {
                            var manu = $(parentDiv).find(".omniSearchManu").val().split("|");
                            manu = manu[1];
                            showModelsForMake(manu, parentDiv);
                        }
                    });

                // on change of make with model, load models
                $(".omniSearchManu.make-model")
                    .change(function() {
                        var parentDiv = $(this).closest(".searchWrapper");
                        var manu = $(parentDiv).find(".omniSearchManu").val().split("|");
                        manu = manu[1];
                        showModelsForMake(manu, parentDiv);
                    });

                // on click of search button, add wedge and and show search boxes
                $('.search-option-btn')
                    .click(function(event) {
                        $('.search-option-btn').removeClass('search-option-active');
                        $(event.currentTarget).addClass('search-option-active');

                        $('.search-option-wedge-active').removeClass('search-option-wedge-active');
                        $('#' + event.currentTarget.id + 'wedge').addClass('search-option-wedge-active');

                        // hide all search wrappers
                        $("#searchComponent .searchWrapper").hide();

                        // show targeted search wrapper
                        $("#searchComponent .searchWrapper")
                            .eq($(".search-option-btn").index(event.currentTarget))
                            .show();
                    });

                $('.search-option-btn-mobile')
                    .click(function(event) {
                        $('.search-option-btn-mobile').removeClass('search-option-active-mobile');
