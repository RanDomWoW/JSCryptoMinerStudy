                        }
                        return false;
                    });
                // Service Center search
                $('#service .omniSearchSubmit, #mobileService .omniSearchSubmit')
                    .click(function() {
                        $(this).closest('form').validate();

                        if ($(this).closest('form').valid()) {
                            var hostname = "https://" + window.location.hostname;
                            var make = $(this).closest(".searchWrapper").find(".omniSearchManu").val().split("|")[0];
                            var postalCode = $(this).closest('.searchWrapper').find('.omniSearchInput').val();

                            SetPostalCodeCookie(postalCode);

