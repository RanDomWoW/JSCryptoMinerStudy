        loadBanner("https://www.dealerrater.com/ncdn/s/110.20180319.1/Graphics/GettyImages_187892085_1900.jpg");
        loadReviewContentBg();

        // Client-side search url helper.
        var searchUrlHelper = {
            DealerSearch: function(hostname, searchZip, searchManufacturer) {
                return hostname + "/directory/" + searchZip + "/" + ToDealerRaterURLFragment(searchManufacturer) + "/";
            },
            ServiceSearch: function(hostname, searchZip, searchManufacturer) {
                return hostname +
                    "/directory/" +
                    searchZip +
                    "/Service-Center/" +
                    ToDealerRaterURLFragment(searchManufacturer) +
                    "/";
            },
            ClassifiedsSearch: function(hostname, searchZip, searchManuId, searchModelId) {
                return hostname + "/classifieds/" + searchManuId + "/" + searchModelId + "/" + searchZip + "/25/";
            },
            RecallsSearch: function(hostname, make, model) {
                return hostname +
                    "/recalls/" +
                    ToDealerRaterURLFragment(make) +
                    "/" +
                    ToDealerRaterURLFragment(model) +
                    "/";
            }
        };

        // Set postal code cookie
        function SetPostalCodeCookie(postalCode) {
            $.cookie("cZipCode", postalCode, { expires: 10, path: '/' });
        }

