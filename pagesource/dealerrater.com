        // load model list for selected make (classifieds and recalls)
        function showModelsForMake(id, parent) {
            var modelSelect = $(parent).find(".omniSearchModel");
            // get models for this make
            $.getJSON("/json/manufacturers/" + id + "/models",
                function(data) {
                    if (data.length > 0) {
                        // empty previous list of models
                        $(modelSelect).empty();

                        for (var x = 0; x < data.length; x++) {
                            var modelOption = $(document.createElement("option")).addClass("notranslate")
                                .val(data[x].Name + "|" + data[x].Id)
                                .append(document.createTextNode(data[x].Name));
                            $(modelSelect).append(modelOption);
                        }
                    }
                });
        }

        $(document)
            .ready(function() {
                // truncate review content
                $(".review-content")
                    .each(function() {
                        $(this).text($.trim($(this).text()));
