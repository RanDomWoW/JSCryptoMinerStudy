    $('#feedbackTime1, #feedbackTime2').keyup(function(){
               
        hours = $('#feedbackTime1').val().replace(/_/g, '');
        minutes = $('#feedbackTime2').val().replace(/_/g, '');
            
        if(hours == '' || minutes == '') {
            $('#feedbackTimeFull').val('');
            return;
        }
        
        hours = hours * 1;
        minutes = minutes * 1;
        
        if(hours >= 24) {
            hours = '0';
        }
        
        if(hours < 10) {
            hours = '0' + hours;
        }
        
        if(minutes > 59) {
            minutes = 59;
        }
        
        if(minutes < 10) {
            minutes = '0' + minutes;
        }
        
        $('#feedbackTimeFull').val(hours + ':' + minutes);
        
    });

