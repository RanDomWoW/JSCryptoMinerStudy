    var gotosearch = false;
    $(document).ready(function () {



        $(".SearchMaster").click(function () {

            SearchLocation();
        });

        function SearchLocation() {
            var txt_se = $(".search-input").val();
            if (txt_se != "") {
                gotosearch = true;
                location.href = "/tag/" + $('#ddlGroup option:selected').val() + "/" + txt_se.trim() + "/";
            }

            event.preventDefault();
        }
        $(".search-input").keypress(function (event) {
            if (event.keyCode == 13) {
                SearchLocation();
            }
        })

        $(document).on("keyup", ".search-input", function (event) {


            clearTimeout($.data(this, 'timer'));

            if ($(".search-input").val() == "") {
                $(".searchProduct1").slideDown(300);
                return;
            }

            var wait = setTimeout(fnGetProductsWithTitle, 500);
            $(this).data('timer', wait);

        });
    });







