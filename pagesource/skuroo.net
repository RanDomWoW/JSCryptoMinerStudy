$(document).on('click', '.more-comment', function(e) {
    var hk = $(this).closest('.post-box').find('[name=hk]').val();
    var moreLink = $(this).hide();
    showFLoading($(this));
    $.ajax({
        url: '/forums/moreComment/' + hk,
        type: 'GET',
        dataType: 'json'
    }).done(function(data, textStatus, jqXHR) {
        if (data.result.status) {
            for (var i = 0; i < data.result.comments.length; i++) {
                var rawComment = data.result.comments[i];
                var comment = commentElement(rawComment.name, rawComment.icon, rawComment.comment, rawComment.isPrivate, rawComment.hash_key, rawComment.date);
                moreLink.parent().append(comment);
            }
        }
        removeFLoading();
    }).fail(function(jqXHR, textStatus, errorThrown) {
        removeFLoading();
    });
});

var c = 153;
$(document).on('click', '#more-forum', function(e) {
    var hk = $('[name=hk]').last().val();
    var moreLink = $(this).hide();
    showFLoading($(this));
    $.ajax({
        url: '/forums/moreForum/' + hk,
        type: 'GET',
        dataType: 'json'
    }).done(function(data, textStatus, jqXHR) {
        if (data.result.status) {
            $('.more-forum').before(data.result.forums);
        }
        initPhotoSwipeFromDOM('.post-gallery');
        removeFLoading();
        if ($('.post-box').length < c) {
            moreLink.show();
        }
    }).fail(function(jqXHR, textStatus, errorThrown) {
        removeFLoading();
    });
});

$(document).on('click', '.do-good', function(e) {
    if ($(this).hasClass('done')) return;
    var hk = $(this).closest('.post-box').find('[name=hk]').val();
    $(this).addClass('done');
    $(this).closest('.reactions').find('.fa.fa-thumbs-o-up').addClass('fa-thumbs-up').removeClass('fa-thumbs-o-up');
    if ($(this).prev('.good-count-label').length > 0) {
        $(this).prev('.good-count-label').addClass('done');
        var gc = $(this).prev('.good-count-label').find('.good-count');
        var count = parseInt(gc.text());
        gc.text(++count);
    } else {
        $(this).before('<span class="good-count-label done"><span class="good-count">1</span>人 </span>');
    }
    $.ajax({
        url: '/forums/good/' + hk,
        type: 'GET',
        dataType: 'json'
    });
