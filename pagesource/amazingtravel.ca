					$("#email").val("输入邮箱订阅邮件");
				}else{
					emailT=1;
				}
			}
		})
}
function sub_form(){
	var myReg = /^[-_A-Za-z0-9-.]+@([-_A-Za-z0-9]+\.)+[A-Za-z0-9]{2,3}$/;
	var email = $("#email").val();
	if(email=="" || email=="输入邮箱订阅邮件" || !myReg.test(email)){
		alert("请正确填写邮箱格式！");
		return false;
	}

	if(emailT!=1){
		return false;
	}
	return true;
					
	
}
