    <script>
        function detectTarget() {
            var target_regex = /\?target_.+/gi;
            var target_clear = /\?----/gi;
            var targetMatch = target_regex.exec(location.href);
            var clearTargetMatch = target_clear.exec(location.href);
            if (targetMatch) {
                localStorage.setItem(targetMatch[0].substring(1), '1');
                window.history.replaceState({}, null, "/");
                location.reload(true);
            }
            if (clearTargetMatch) {
                Object.keys(localStorage).forEach(function (x) {
                    if (x.startsWith('target_')) {
                        localStorage.removeItem(x);
                    }
                });
                window.history.replaceState({}, null, "/");
                location.reload(true);
            }
            window.build = Object.keys(localStorage).find(function (x) {
                return x.startsWith('target_');
            });

            if (!build) {
                if (location.hostname === 'dev.fex.net') {
                    build = 'target_local';
                } else {
                    build = "target_public";
                }
            }
        }
        window.onhashchange = detectTarget;
        detectTarget();

        function add(e, attrs) {
            var deferred = $.Deferred();
            var element = document.createElement(e);
            Object.keys(attrs).forEach(function (key) {
                element.setAttribute(key, attrs[key]);
            })
            element.onload = function () {
                deferred.resolve();
            }
            element.onerror = function (err) {
                console.error('Failed to load artifact');
                throw err;
            }
            document.head.appendChild(element);
            return deferred;
        }
        loadBuild = $.ajax({
            url: '/d.json',
            cache: false
        }).then(function (d) {
            bundlePath = d[build];
            add('link', { rel: 'stylesheet', href: bundlePath + 'css/index.css' });
            add('link', { rel: 'stylesheet', href: bundlePath + 'css/mobile.css', id: 'mobileStylesLink'});
            return add('script', { src: bundlePath + 'js/index.js' });
        });
    </script>
