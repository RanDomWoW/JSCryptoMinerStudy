/*Menu V1.0.2 - CSS Code V1.0.0*/
/*main*/
    #nav {
        position: relative;
        height: 50px;
        padding: 0 10px;
        text-align: right;
        background-color: #3b78e7;
        color: #e3edfe;
        border-bottom: 2px solid #265cbd;
        overflow: visible;
        direction: rtl;
		margin: 20px 0 0 0;
		opacity: 0;
}
    /*ul*/
    #nav ul {
        position: absolute;
        background: #fff;
        top: 45px;
        right: 0;
        margin: 0;
        padding: 5px;
        width: 150px;
        border: 1px solid #dedede;
        border-radius: 5px;
        -webkit-border-radius: 5px;
        -moz-border-radius: 5px;
        -o-border-radius: 5px;
        -ms-border-radius: 5px;
		z-index: 9;
}
    /*li*/
    #nav > li {
        position: relative;
        float: right;
}
    #nav li {
        list-style: none;
}
    #nav .first_drop > li, #nav .second_drop > li {
        position: relative;
}
    #nav .first_drop > ul > li:hover, #nav .second_drop > ul > li:hover {
        background: whitesmoke;
}
    /*a*/
    #nav a {
        margin: 0px 2px;
        line-height: 50px;
        font-size: 15px;
        text-decoration: none;
        cursor: pointer;
        color: #e3edfe;
        padding: 5px 15px;
        border-radius: 5px;
        -webkit-border-radius: 5px;
        -moz-border-radius: 5px;
        -o-border-radius: 5px;
        -ms-border-radius: 5px;
}
    #nav .home a:before {
        font-family: "FontAwesome";
        content: "\f015";
        font-size: 15px;
        margin: 0 0 0 5px;
}   
    #nav .first_drop > ul > li a {
        color: #3b78e7;
        line-height: 1.5;
        display: block;
}
    #nav > li:hover > a {
        background: rgba(6, 47, 144, 0.18);
        color: #fff;
}
    /*first drop*/
    #nav .first_drop > ul {
        position: absolute;
        background: #fff;
        top: 45px;
        right: 0%;
        margin: 0;
        padding: 5px;
        width: 165px;
        border: 1px solid #dedede;
        border-radius: 5px;
        -webkit-border-radius: 5px;
        -moz-border-radius: 5px;
        -o-border-radius: 5px;
        -ms-border-radius: 5px;
        transform: scale(0);
        -webkit-transform: scale(0);
        -moz-transform: scale(0);
        -o-transform: scale(0);
        -ms-transform: scale(0);
        transition: .2s ease;
        -webkit-transition: .2s ease;
        -moz-transition: .2s ease;
        -o-transition: .2s ease;
        -ms-transition: .2s ease;
}
    #nav  .first_drop:hover > ul {
        transform: scale(1);
        -webkit-transform: scale(1);
        -moz-transform: scale(1);
        -o-transform: scale(1);
        -ms-transform: scale(1);
        transition: .3s cubic-bezier(0, 1.13, 0.17, 1.3);
        -webkit-transition: .3s cubic-bezier(0, 1.13, 0.17, 1.3);
        -moz-transition: .3s cubic-bezier(0, 1.13, 0.17, 1.3);
        -o-transition: .3s cubic-bezier(0, 1.13, 0.17, 1.3);
        -ms-transition: .3s cubic-bezier(0, 1.13, 0.17, 1.3);
}
    #nav .first_drop > ul:before {
        content: '';
        width: 0;
        height: 0;
        border-top: 8px solid transparent;
        border-right: 8px solid transparent;
        border-bottom: 8px solid #fff;
        border-left: 8px solid transparent;
        position: absolute;
        top: -16px;
        right: 20px;
}
    #nav .first_drop > a:before {
        font-family: "FontAwesome";
        content: "\f107";
        font-size: 15px;
        float: left;
        transition: all .3s;
        -webkit-transition: all .3s;
        -moz-transition: all .3s;
        -o-transition: all .3s;
        -ms-transition: all .3s;
}
    #nav .first_drop:hover > a:before, #nav .second_drop:hover > a:before {
        transform: rotate(180deg);
        -webkit-transform: rotate(180deg);
        -moz-transform: rotate(180deg);
        -o-transform: rotate(180deg);
        -ms-transform: rotate(180deg);
}
    /*second drop*/
    .second_drop {
        position: relative;
}
   #nav .second_drop ul {
        transform: scale(0);
        -webkit-transform: scale(0);
        -moz-transform: scale(0);
        -o-transform: scale(0);
        -ms-transform: scale(0);
        transition: .2s ease;
        -webkit-transition: .2s ease;
        -moz-transition: .2s ease;
        -o-transition: .2s ease;
        -ms-transition: .2s ease;
        position: absolute;
        top: -10%;
        right: 165px;
		width: 165px;
} 
    #nav .second_drop > a:before {
        font-family: "FontAwesome";
        content: "\f104";
        font-size: 15px;
        float: left;
        transition: all .3s;
        -webkit-transition: all .3s;
        -moz-transition: all .3s;
        -o-transition: all .3s;
        -ms-transition: all .3s;
}
    #nav .second_drop:hover ul {
        transform: scale(1);
        -webkit-transform: scale(1);
        -moz-transform: scale(1);
        -o-transform: scale(1);
        -ms-transform: scale(1);
        transition: .3s cubic-bezier(0, 1.13, 0.17, 1.3);
        -webkit-transition: .3s cubic-bezier(0, 1.13, 0.17, 1.3);
        -moz-transition: .3s cubic-bezier(0, 1.13, 0.17, 1.3);
        -o-transition: .3s cubic-bezier(0, 1.13, 0.17, 1.3);
        -ms-transition: .3s cubic-bezier(0, 1.13, 0.17, 1.3);
}
    #nav .second_drop > ul:before {
        content: '';
        width: 0;
        height: 0;
        border-top: 9px solid transparent;
        border-right: 9px solid transparent;
        border-bottom: 9px solid transparent;
        border-left: 9px solid #dcdcdc;
        position: absolute;
        top: 10px;
        right: -18px;
}
    #nav .second_drop > ul:after {
        content: '';
        width: 0;
        height: 0;
        border-top: 8px solid transparent;
        border-right: 8px solid transparent;
        border-bottom: 8px solid transparent;
        border-left: 8px solid #ffffff;
        position: absolute;
        top: 11px;
        right: -16px;
}
    /*responsive*/
    .responsive_menu{
        display: none;
    }
    #show_menu{
        height: 50px;
        line-height: 50px;
        cursor: pointer;
        border-bottom: 2px solid #265cbd;
        -webkit-touch-callout: none; 
        -webkit-user-select: none; 
        -khtml-user-select: none; 
        -moz-user-select: none; 
        -ms-user-select: none;
            user-select: none;
    }
    #show_menu:before{
        font-family: "FontAwesome";
        content: "\f0c9";
        margin: 0 10px;
    }
    @media all and (min-width:0px) and (max-width: 1020px) {
        .responsive_menu{
            display: block;
        }
        #nav > li{
            display: none;
        }
        #nav a{
            display: block;
        }
        #nav a:hover{
            opacity: .8;
        }
        #nav{
            overflow: hidden;
            text-align: center;
            max-height: 500px;
        }
}



