    var itemtemplate = Hogan.compile('<div class="clearfix"><a class="pull-left" href="/items/{{objectID}}"><img class="pull-left" style="width: 40px; height: 40px; margin-right: 10px;" src="/items/{{objectID}}/picture"></a><div class="media-body"><div style="font-size: 14px;"><a style="color: #676a6c;" href="/items/{{objectID}}">{{{_highlightResult.title.value}}}</a></div><small style="font-size: 12px;" class="text-overflow"><a style="color: #676a6c;" href="/items/{{objectID}}">{{{_highlightResult._user_name.value}}}</a></small></div></div>');
    $('#top-search').typeahead({hint: false}, {
      source: item_index.ttAdapter({hitsPerPage: 10}),
      displayKey: 'title',
      templates: {
        suggestion: function(hit) {
          return itemtemplate.render(hit);
        }
      }
    }
    );
  });
</script>

<script type="text/javascript">
  $(function() {
    countDown();
  });
  function countDown() {
    var startDateTime = new Date();
    var endDateTime = new Date('2018/3/25 23:59:59');
    var left = endDateTime - startDateTime;
    var a_day = 24 * 60 * 60 * 1000;
    var d = Math.floor(left / a_day);
    var h = Math.floor((left % a_day) / (60 * 60 * 1000));
    var m = Math.floor((left % a_day) / (60 * 1000)) % 60;
    var s = Math.floor((left % a_day) / 1000) % 60 % 60;
    if(left > 0){
      $("#campaign-time-left").text('残り' + d + '日' + h + '時間' + m + '分' + s + '秒');
      setTimeout('countDown()', 1000);
    }else{
      $("#campaign-time-left").text('終了しました');
    }
  }
</script>

