        <script type="text/javascript" id="process_login_js">
            $(document).ready(function() {
                var loginForm = $('#loginForm');
                $('#login-layer').find('.module-h5--header .headline').html('Login');
                loginForm.validate({
                    rules: {
                        'userId': { required: true, minlength: 2 },
                        'phrase': { required: true, minlength: 2 }
                    },
                    errorPlacement: function(error, element) {
                        var msgEl = loginForm.find('.error-place');
                        msgEl.addClass('alert-error');
                        msgEl.html('Die eingegebenen Daten sind falsch.<br>Bitte überprüfe Deine E-Mail und Dein Passwort.');
                        msgEl.parent().show();
                        if($('#login-email').hasClass('error')) {
                            $('#login-email').focus();
                        }

                    },
                    submitHandler: function(form) {
                        Maxdome.loginFormUserId = loginForm.find('input[name=userId]').val();
                        var formData = {
                            userId: Maxdome.loginFormUserId,
                            phrase: loginForm.find('input[name=phrase]').val(),
                            autoLogin: loginForm.find('input[name=autoLogin]').prop('checked'),
                            campaignId: $.url().param('campaignId'),
                            mediaCode: $.url().param('mc'),
                            userRef: $.url().param('userRef'),
                            hash: $.url().param('hash')
                        };
                        Maxdome.loginProcessInstance.start('login0', formData);
                    }
                });
            });
        </script>

