        var $form = $('#resetPasswordForm');
        $form.validate({
            rules: {
                email: { required: true, minlength: 2 }
            },
            errorPlacement: function(error, element) {
                var msgEl = $form.find('.error-place');
                msgEl.addClass('alert-error');
                msgEl.html('ungültige E-Mail-Adresse');
                msgEl.parent().show();
            },
            submitHandler: function(form) {
                var url = '/_ajax/password/forgot';
                $.post(url,
                    {
                        email: $(form).find('[name=email]').val()
                    },
                    function(response) {
                        var container = '#reset-password-content';
                        $(container).html(response);
                    }
                );
            }
        });
            });
</script>

