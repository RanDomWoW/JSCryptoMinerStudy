function compter(){
	if($('#compteurbasket').length > 0){
		var d = new Date(dt_evenement);
		var d1 = new Date();
		var dif = d.getTime() - d1.getTime();
		var dif_s = dif / 1000;
		var j = Math.floor(dif_s/60/60/24);
		reste = dif_s - (j*24*60*60);
		var h = Math.floor(reste/60/60);
		reste = dif_s - (j*24*60*60) - (h*60*60);
		var m = Math.floor(reste/60);
		reste = dif_s - (j*24*60*60) - (h*60*60) - (m*60);
		var s = Math.round(reste);
		if(j>1){jst='jours';}else{jst='jour';}
		if(h>1){hst='heures';}else{hst='heure';}
		if(dif > 0){
			$('#compteurbasket').html(titre_evenement + ' ' + j + ' '+jst+ ' ' + h + ' '+hst+ ' ' + m + ' min'+ ' ' + s + ' sec');
		}else{
			$('#compteurbasket').html('C\'est parti !');
		}
	}
}

