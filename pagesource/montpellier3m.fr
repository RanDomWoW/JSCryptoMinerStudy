		$('.galleria').parent().prepend('<div id="compteurbasket" style="text-align: center;height: 30px;font-size: 18px;font-size: 16px;font-weight: normal;letter-spacing: 0.05em;background-color: #694b9b;margin-top: -10px;padding-top: 5px;color: white;">'+titre_evenement+' ...</div>');
		compter();
		setInterval('compter()',1000);
	}
}

function aff_flash(){
	var d1 = new Date(dt_flash_debut);
	var d2 = new Date(dt_flash_fin);
	var d = new Date();
	if(d > d1 && d < d2){
		$('#block-block-1000012').parent().prepend('<div id="flashinfos">'+texte_flash+'</div>');
	}
}
