		$('.block-carte-infos').click(function() {
			window.location = lienCarte;
		});
	}).done(function() {
		caTourne();
	});
	var timerCarte = setInterval(caTourne, 8000);
}
lienCarte  = "#";
onBouge = 1;
onBougePlus = -1;
onTourne = 1;
// création des éléments du bloc

var cAct = -1;
$('.carte-vide').mousemove(function( event ) {
	//onTourne = 0;
	clearInterval(timerCarte);
	if(onBouge==1){
		//alert(tabCommunes[16].node.title);
		x = event.pageX - $(this).offset().left;
		y = event.pageY - $(this).offset().top;
		//$("#block-carte-infos").hide();
		$(this).css("cursor","default");
		for(i=0 ; i < tabCommunes.length ; i++){
			if(x > tabCommunes[i].minX && x < tabCommunes[i].maxX && y > tabCommunes[i].minY && y < tabCommunes[i].maxY && cAct != i){
				$(this).css("cursor","pointer");
				afficher_commune(i);
                                cAct = i;
			}
		}
		if(onBougePlus==1){
			onBouge=0;
		}
	}
});

$('.carte-vide').mouseout(function() {
  onBougePlus = -1;
  onBouge = 1;
  onTourne = 1;
  //caTourne();
  clearInterval(timerCarte);
  timerCarte = setInterval(caTourne, 8000);
});

/**
* Click sur la carte : si on est sur un mobile, on affiche le block (car sinon il n'apparaitra jamais)
* Si on est sur un PC (avec une souris) on va directement à la page de la commune
*/
$('.carte-vide').click(function() {
	window.location = lienCarte;
});

var rc = Math.floor((Math.random() * 31));
function caTourne(){
	var rc2 = Math.floor((Math.random() * 31));
	if(rc2!=rc){
		afficher_commune(rc2);
		rc=rc2;
	}else{
		if(rc2==0){
			rc = 1;
		}else{
			rc=rc2-1;
		}
		afficher_commune(rc);
	}
	if(onTourne==1){
		//setTimeout(caTourne,3000);
	}
}

function afficher_commune(ind){
	// Option 1 on efface les 31 images et blcosk
	$(".carte-commune").hide();
	$(".block-carte-infos").hide();
	$("#com-"+tabCommunes[ind].node.Nid).show();
	lienCarte = tabCommunes[ind].node.path;
	// $("#block-carte-comm-"+tabCommunes[ind].node.Nid).show();
	// taile police en fonction nombre de caractères
	var nomCommune = tabCommunes[ind].nom;
	$('#carte-nom-commune').html(nomCommune);
	if(tabCommunes[ind].logo==''){
		$('#carte-hab-commune').html(tabCommunes[ind].hab);
		$('#carte-txt-commune').html(tabCommunes[ind].txt);
		$('#carte-src-commune').show();
		$('#carte-logo-commune').hide();
	}else{
		$('#carte-src-commune').hide();
		$('#carte-txt-commune').hide();
		$('#carte-hab-commune').hide();
		$('#img-logo-commune').attr('src',tabCommunes[ind].logo);
		$('#carte-logo-commune').show();
	}
	// image
        /* $("#carte-img-commune").stop();
	$("#carte-img-commune").fadeTo(400,0.10, function() {
           $("#carte-img-commune").html('<a href="'+lienCarte+'"><img src="'+tabCommunes[ind].image+'" height="230" width="284" style="margin:10px"></a>');
        }).fadeTo(200,1); */
         $("#carte-img-commune").html('<a href="'+lienCarte+'"><img src="'+tabCommunes[ind].image+'" height="230" width="284" style="margin:10px"></a>');
}
