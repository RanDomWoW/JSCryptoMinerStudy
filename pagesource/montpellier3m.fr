<script type="text/javascript">
$ = jQuery.noConflict( true );
$( document ).ready(function() {
	$('#bt-BAI').click(function() {
		$('#bai-page1').show();
		$('#bai-page2').hide();
		$('#dial-BAI').show();
		$('#cont-BAI').attr('style','position: fixed;top: 0px;left:0px;z-index: 5000;width:100%;height:100%;;overflow:scroll;background-color: rgba(169, 169, 169, 0.8);');
	});
	$('#BAI-MOB').click(function() {
                $('#menu-cache').hide();
		$('#menu-mobile').hide();
		$('#bai-page1').show();
		$('#bai-page2').hide();
		$('#dial-BAI').show();
		$('#cont-BAI').attr('style','position: fixed;top: 0px;left:0px;z-index: 5000;width:100%;height:100%;background-color: rgba(169, 169, 169, 0.8);overflow:scroll');
                $('.helpForm').hide();
	});
	$('.bt-bai-fermer').click(function() {
		$('#dial-BAI').hide();
		$('#cont-BAI').attr('style','');
	});
	$('#bt-bai-suggerer').click(function() {
		// vérifications
		var ok = true;
		if($('#title').val()==''){
			ok = false;
			alert('Vous devez saisir un titre pour votre suggestion');
			$(this).addClass('ng-invalid-required');
		}
		if($('#description').val()==''){
			$(this).addClass('ng-invalid-required');
			if(ok){
				alert('Vous devez saisir une description');
			}
			ok = false;
		}
		if(ok && $('#prenom').val()==''){
			$(this).addClass('ng-invalid-required');
			if(ok){
				alert('Vous devez saisir un prénom ou un pseudo');
			}
			ok = false;
		}
		if(ok){
			var t = $('#title').val();
			var n = $('#prenom').val();
			var d = $('#description').val();
			var c = $('#category').val();
			var m = $('#mail').val();
			$.ajax({
				url:"/sites/all/themes/zen_agglo/boiteAidee.php",
				method: "POST",
				data:{ titre: t, nom: n,description:d,categorie:c,mail:m }
			}).success(function(data) {
				$('#bai-page1').hide();
				$('#bai-message').html(data);
				$('#bai-page2').show();
			}).error(function(resultat, statut, erreur){
				alert(erreur);
				$('#bai-message').html("Votre suggestion n'a pas pu être prise en compte.");
			});
			
		}
	});
});
