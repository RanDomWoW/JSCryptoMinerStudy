    <script>
if($('#widget_163512').length > 0){
        if($(window).width() > 800){
            var titreBlockComm = 'Les 31 communes de la Métropole';
        }else{
            var titreBlockComm = 'Les 31 communes';
        }
	var eltHTML = '<div class="portlet hash-e4ceb8b025d032505c2ba42a3b5ee8d0288a00a2 ui-widget ui-widget-content ui-helper-clearfix">';
	eltHTML += '<div class="portlet-header no-close header-agenda no-refresh ui-widget-header" style="background-color:#694b9b;">';
	eltHTML += '<span class="portlet-title widget-icon-aquarium">'+titreBlockComm+'</span></div>';
		eltHTML += '<div id="block-carte" style="height:250px;">';
		eltHTML += '</div>';
	eltHTML += '</div>';
	$('#widget_163512').after(eltHTML);
	obs = $('#block-carte');
	var largCarte=140;
	// les coordonées enregistrées ont été établies sur une carte de 238 sur 226, si on réduit la carte, il faut appliquer un coeff à celles-ci ...
	var coef = largCarte / 238;
	// AJOUT DE LA CARTE
	obs.append('<img class="img-carte" src="/sites/default/files/commune/carte/cartefond.png" style="z-index:1;width:'+largCarte+'px;">');
	obs.append('<img class="img-carte carte-vide" src="/sites/default/files/commune/carte/cartevide.png" style="z-index:100;width:'+largCarte+'px;">');
	// Ajout des composants textes
	obs.append('<div id="carte-nom-commune"></div>');
	obs.append('<div id="carte-hab-commune"></div>');
	obs.append('<div id="carte-txt-commune"></div>');
	obs.append('<div id="carte-img-commune"></div>');
	obs.append('<div id="carte-logo-commune"><img src="" style="max-width:76px;max-height:76px" id="img-logo-commune"></div>');
	obs.append('<div id="carte-src-commune">*Source INSEE Millèsime 2012</div>');
	// CREATION DU TABLEAU DES COMMUNES A PARTIR DE LA VUE LISTE DES COMMUNES => JSON
	var tabCommunes = [];
	$.getJSON( "communes-json", function( data ) {
		//alert(String(data.nodes[16].node.field_coordonnes_carte));
		for(i = 0; i < data.nodes.length ; i++){
			// on précharge les images :
			obs.append('<img src="'+data.nodes[i].node.field_vignette.src+'" style="display:none">');
			tabCommunes.push(data.nodes[i]);
			if(data.nodes[i].node.field_coordonnes_carte == ''){
				tabCommunes[i].minX = 0;
				tabCommunes[i].maxX = 0;
				tabCommunes[i].minY = 0;
				tabCommunes[i].maxY = 0;
			}else{
				// les données sont du type : left,top,couleur, amplitude (def=10)
				coords = data.nodes[i].node.field_coordonnes_carte.split(',');
				if(coords[3]==undefined){
					amplitude = 10 * coef;
				}else{
					amplitude = Number(coords[3])* coef;
				}
				tabCommunes[i].minX = Number(coords[0])* coef - amplitude;
				tabCommunes[i].maxX = Number(coords[0])* coef + amplitude;
				tabCommunes[i].minY = Number(coords[1])* coef - amplitude;
				tabCommunes[i].maxY = Number(coords[1])* coef + amplitude;
			}
			// Nom de la commune
			tabCommunes[i].nom = data.nodes[i].node.title_1;
			// habitants
			tabCommunes[i].hab = data.nodes[i].node.field_habitants + ' habitants*';
			// texte sup (vide pour l'instant)
			tabCommunes[i].txt = '';
			// IMAGE
			tabCommunes[i].image =  data.nodes[i].node.field_vignette.src;
			// Ajout du composant image de fond :
			nid = data.nodes[i].node.Nid;
			// logo
			tabCommunes[i].logo = data.nodes[i].node.field_logo_commune;
