<script>
var start_delay = 100;
var title_delay = 300;
var overlay_delay = 3000;
var nav_items_op_delay = 100;
var ss_animated = false;
var body = document.body,
    html = document.documentElement;
var documentHeight = Math.max( body.scrollHeight, body.offsetHeight,
                       html.clientHeight, html.scrollHeight, html.offsetHeight );
var windowHeight = $(window).height();
var maxVariance = 8;
var yOffset = -5;
var docViewTop = $(window).scrollTop();
var docViewBottom = docViewTop + $(window).height();
var elemTop;
var elemBot;
var elemPos;
var elemHeight;
var img;

$(document).ready(function() {
  loadVideo();
  loadAnimations();
  screenSizeCheck();
  checkScrolledOnLoad();
  squareThis('.circle');
  resetArrowGifs();
});

// Delay video start for overlay
function loadVideo() {
  var video = document.getElementById("video-bg");
  if(typeof anime === 'undefined') { // IE play video immediately
    video.play();
  } else { // Non-IE play videoafter delay
    setTimeout(function() {
      video.play();
    }, 1500);
  }
}

// Home screen animations
function loadAnimations() {
  if(typeof anime === 'undefined') { return; }
  //video overlay
  anime({
    targets: '#video-bg',
    opacity: {value:[0,1], duration:2000, elasticity:0, delay: overlay_delay}
  })
  //title
  anime({
    targets: '.hero-title',
    opacity: {value:[0,1], duration:start_delay, elasticity:0, delay: 0},
    easing:'linear'
  });
  anime({
    targets: '.title-slide',
    translateX: {value:[-50,0], duration:3000, elasticity:100,
      delay: function(target, index) {
        return parseInt(target.dataset.delay) + start_delay + title_delay;
      }
    },
    opacity: {value:[0,1], duration:3000, elasticity:0,
      delay: function(target, index) {
        return parseInt(target.dataset.delay) + start_delay + title_delay;
      }
    },
    easing:'easeOutExpo'
  });
  //nav
  anime({
    targets: '.navbar-fixed-top',
    translateY: {value:[-40,0], duration:2000, elasticity:0},
    opacity: {value:[0,1], duration:2000, elasticity:0},
    delay: start_delay,
    easing:'easeOutExpo'
  });
  //nav items
  anime({
    targets: '#primary-menu li.menu-item',
    translateY: {value:[-40,0], duration:1400, elasticity:500,
      delay: function(target, index) {
        return index*100 + start_delay;
      }
    },
    opacity: {value:[0,1], duration:400, elasticity:0,
      delay: function(target, index) {
        return index*100 + start_delay + nav_items_op_delay;
      }
    },
    easing:'easeOutExpo'
  });
  //scroll down
  anime({
    targets: '.scroll-down-mouse',
    opacity: {value:[0,1], duration:2000, elasticity:0, delay:start_delay+title_delay+1500},
    easing:'easeOutExpo'
  });
  anime({
    targets: '.scroll-down-mouse',
    translateY: {value:[5,-5], duration:700, delay:0},
    direction: 'alternate',
    loop: true,
    easing:'easeInOutQuad'
  });
}

/* To make sure the gif resets on refresh */
function resetArrowGifs() {
  var timeout = 0;
  var left = 'url(http://www.nextjump.com/wp-content/uploads/2017/07/ani-left-delay.gif?' + Math.random() + ')';
  var right = 'url(http://www.nextjump.com/wp-content/uploads/2017/07/ani-right-delay.gif?' + Math.random() + ')';
  $(".social-arrow-left-img").css('background-image',left);
  $(".social-arrow-right-img").css('background-image',right);
}

function screenSizeCheck() {
  // Expand video on super widescreen
  if($(window).width() / $(window).height() >= 1.75) {
    $("#video-bg").css("transform","scale(1.5)");
    $(".video-overlay-image").css("transform","scale(1.5)");
  } else {
    $("#video-bg").css("transform","scale(1)");
    $(".video-overlay-image").css("transform","scale(1)");
  }
  // Hide Learn More on short
  if($(window).height() < 700) {
    $("#learn-more-button").css("display","none");
  }
}

(function ( $ ) {

    $.fn.YouTubePopUp = function(options) {

        var YouTubePopUpOptions = $.extend({
                autoplay: 1
        }, options );

        $(this).on('click', function (e) {

            var youtubeLink = $(this).attr("href");

            if( youtubeLink.match(/(youtube.com)/) ){
                var split_c = "v=";
                var split_n = 1;
            }

            if( youtubeLink.match(/(youtu.be)/) || youtubeLink.match(/(vimeo.com\/)+[0-9]/) ){
                var split_c = "/";
                var split_n = 3;
            }

            if( youtubeLink.match(/(vimeo.com\/)+[a-zA-Z]/) ){
                var split_c = "/";
                var split_n = 5;
            }

            var getYouTubeVideoID = youtubeLink.split(split_c)[split_n];

            var cleanVideoID = getYouTubeVideoID.replace(/(&)+(.*)/, "");

            if( youtubeLink.match(/(youtu.be)/) || youtubeLink.match(/(youtube.com)/) ){
                var videoEmbedLink = "https://www.youtube.com/embed/"+cleanVideoID+"?autoplay="+YouTubePopUpOptions.autoplay+"";
            }

            if( youtubeLink.match(/(vimeo.com\/)+[0-9]/) || youtubeLink.match(/(vimeo.com\/)+[a-zA-Z]/) ){
                var videoEmbedLink = "https://player.vimeo.com/video/"+cleanVideoID+"?autoplay="+YouTubePopUpOptions.autoplay+"";
            }

            $("body").append('<div class="YouTubePopUp-Wrap"><div class="YouTubePopUp-Content"><span class="YouTubePopUp-Close"></span><iframe src="'+videoEmbedLink+'" allowfullscreen></iframe></div></div>');


            $(".YouTubePopUp-Wrap, .YouTubePopUp-Close").click(function(){
                $(".YouTubePopUp-Wrap").addClass("YouTubePopUp-Hide").delay(515).queue(function() { $(this).remove(); });
            });

            e.preventDefault();

        });

        $(document).keyup(function(e) {

            if ( e.keyCode == 27 ){
                $('.YouTubePopUp-Wrap, .YouTubePopUp-Close').click();
            }

        });

    };

}( jQuery ));

