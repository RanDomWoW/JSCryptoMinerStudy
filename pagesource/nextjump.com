}

function scrollPageDown() {
    $('html, body').stop().animate({
      scrollTop: $("#secondSection").offset().top - 74
    }, 1800);
}

function elementScrolled(elem) {
    docViewTop = $(window).scrollTop();
    docViewBottom = docViewTop + $(window).height();
    elemTop = $(elem).offset().top;
    elemBot = $(elem).offset().top + $(elem).height();
    return ((elemTop <= docViewBottom) && (elemTop >= docViewTop) // Top is in frame
      || (elemBot <= docViewBottom) && (elemBot >= docViewTop) // Bot is in frame
      || (elemBot >= docViewBottom) && (elemTop <= docViewTop)); // Middle is in frame
}

var pull_from_right = {
  translateX: {value:[100,0], duration:4000, elasticity:100,},
  opacity: {value:[0,1], duration:4000, elasticity:0},
  delay: 200,
  easing:'easeOutExpo'
};

var anyDDOImageAnimated = false;
// Check scroll animations on page load for if already partly down page
function checkScrolledOnLoad() {
    docViewTop = $(window).scrollTop();
    docViewBottom = docViewTop + $(window).height();

    // Announcements Section Animation ONLOAD
    $(".slash-block").each(function(index) {
      if(this.dataset && this.dataset.animated === "0" && elementScrolled(this)) {
        this.dataset.animated = "1"
        animateAnnouncementSection(this, index);
      }
      elemPos = $(this).offset().top;
      elemHeight = $(this).height();
      // Bottom of window minus the middle of the element divided by the windows height.
      yPosition = -1*(((docViewBottom - (elemPos + 0.5*elemHeight)) / windowHeight) * maxVariance + yOffset);
      img = this.getElementsByClassName("slash-image-holder");
      $(img).css("transform","translateY("+yPosition+"%)");
    });

    // BMBYBU Section Animation ONLOAD
    $(".bmbybu").each(function(index) {
      if(this.dataset && this.dataset.animated === "0" && elementScrolled(this)) {
        this.dataset.animated = "1";
        animateBMBYBUSection(this);
      }
    });

    // DDO images ONLOAD
    $("#ddo-img-container span img").each(function(index) {
        if(this.dataset && anyDDOImageAnimated == false && elementScrolled(this)) {
            anyDDOImageAnimated = true;
            anime((pull_from_right.targets = '#ddo-img-container span img', pull_from_right));
        }
    });
}

$(window).scroll(function(){
    docViewTop = $(window).scrollTop();
    docViewBottom = docViewTop + $(window).height();

    // Announcements Section Animation ONLOAD
    $(".slash-block").each(function(index) {
      if(this.dataset && this.dataset.animated === "0" && elementScrolled(this)) {
        this.dataset.animated = "1"
        animateAnnouncementSection(this, index);
      }
      elemPos = $(this).offset().top;
      elemHeight = $(this).height();
      // Bottom of window minus the middle of the element divided by the windows height.
      yPosition = -1*(((docViewBottom - (elemPos + 0.5*elemHeight)) / windowHeight) * maxVariance + yOffset);
      img = this.getElementsByClassName("slash-image-holder");
      $(img).css("transform","translateY("+yPosition+"%)");
    });

    // BMBYBU Section Animation ONLOAD
    $(".bmbybu").each(function(index) {
      if(this.dataset && this.dataset.animated === "0" && elementScrolled(this)) {
        this.dataset.animated = "1";
        animateBMBYBUSection(this);
      }
    });

    // DDO images ONSCROLL
    $("#ddo-img-container span img").each(function(index) {
        if(this.dataset && anyDDOImageAnimated == false && elementScrolled(this)) {
            anyDDOImageAnimated = true;
            anime((pull_from_right.targets = '#ddo-img-container span img', pull_from_right));
        }
    });
});

// Animations for BMBYBU Section
function animateBMBYBUSection(el) {
  if(typeof anime === 'undefined') { return; }
  var bmbybu_delay = 150;
  var bmbybu_duration = 2000;
  var bmbybu_distance = 50;
  //Fade up
  anime({
    targets: el.getElementsByClassName("bmbybu-slide"),
    translateY: {value:[bmbybu_distance,0], duration:bmbybu_duration, elasticity:100},
    opacity: {value:[0,1], duration:bmbybu_duration, elasticity:0},
    easing:'easeOutExpo',
    delay: function(target, index) {
      return parseInt(target.dataset.delay)*2 + bmbybu_delay;
    }
  });
}

function animateAnnouncementSection(el, index) {
  if(typeof anime === 'undefined') { return; }
  var announcement_delay = 150;
  var announcement_duration = 2000;
  var announcement_distance = 50;
  //Fade up
  anime({
    targets: el,
    opacity: {value:[0,1], duration:announcement_duration, elasticity:0},
    easing:'easeOutExpo',
    delay: announcement_delay
  });
  anime({
    targets: el.getElementsByClassName("slash-content-slide"),
    translateY: {value:[announcement_distance,0], duration:announcement_duration, elasticity:100},
    opacity: {value:[0,1], duration:announcement_duration, elasticity:0},
    easing:'easeOutExpo',
    delay: function(target, index) {
      return parseInt(target.dataset.delay) + announcement_delay;
    }
  });
}

function squareThis (element, ratio, minLimit)
{
    // First of all, let's square the element
    square(ratio, minLimit);

    // Now we'll add an event listener so it happens automatically
    window.addEventListener('resize', function(event) {
        square(ratio, minLimit);
    });

    // This is just an inner function to help us keep DRY
    function square(ratio, minLimit)
    {
        if(typeof(ratio) === "undefined")
        {
            ratio = 1;
        }
        if(typeof(minLimit) === "undefined")
        {
            minLimit = 0;
        }
        var viewportWidth = window.innerWidth;

        if(viewportWidth >= minLimit)
        {
            var newElementHeight = $(element).width() * ratio;
            $(element).height(newElementHeight);
            $(".social-arrow").height(newElementHeight);
        }
        else
        {
            $(element).height('auto');
        }
    }
}

