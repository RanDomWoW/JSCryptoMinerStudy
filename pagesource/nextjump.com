function trackTrendingLinkClick(id) {
    ga('send', 'event', 'Trending Topic', 'Click', id);
}

function trackDDOLinkClick(id) {
    ga('send', 'event', 'DDO', 'Click', id);
}

function trackLink(category,label) {
