	cookiedata = document.cookie; 
	var cookiedataindex = 0;
	
	if ( cookiedata.indexOf("eventcookiedisplay=done") < 0 ){
		if ( cookiedata.indexOf("eventcookie") >= 0 ){
			var cookiedataArray = new Array();
			cookiedataArray = cookiedata.split(";");
			//alert(cookiedataArray.length);		
			for (var i = 0; i < cookiedataArray.length; i++) {
				//alert(cookiedataArray[i]);
				if(cookiedataArray[i].indexOf("eventcookie") >= 0){
					cookiedataindex = i;
				}
				//alert("cookiedataindex == "+ cookiedataindex);
			}
		}
	}
	
	
	if ( cookiedata.indexOf("eventcookiedisplay=done") < 0 ){
		if ( cookiedata.indexOf("eventcookie") < 0 ){
			//쿠키음슴
			setCookie("eventcookie",location.pathname+"," ,24);
		}else {
			//alert("쿠키있다"+location.pathname);
			if(cookiedata.split(";")[cookiedataindex].indexOf(location.pathname) < 0){
				var a = cookiedata.split(";")[cookiedataindex].split("=")[1].replace(/%2C/gi, ','); 
				var results = a.match(/,/g); 
				if(results.length < 2 && location.pathname !="/event/view/332" && location.pathname !="/event/ing/view/332" && location.pathname != "/event/end/view/332" && location.pathname.indexOf("/login") < 0){
					setCookie("eventcookie",cookiedata.split(";")[cookiedataindex].split("=")[1].replace(/%2C/gi, ',')+location.pathname+"," ,24);
				}else{
					if(location.pathname !="/event/view/332" && location.pathname !="/event/ing/view/332" && location.pathname != "/event/end/view/332" && location.pathname.indexOf("/login") < 0){
