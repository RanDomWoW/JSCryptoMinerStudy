function offeduapp(idx) {
	var CSRFToken = $("input[name=CSRFToken]").val();
	$.ajax
	({
		type:		"post"
	  , dataType:	"text"
	  , url:		"/offedu_check.do"
	  , data:		{ idx:idx, CSRFToken:CSRFToken }
	  , async:		false
	  , success:	function(data) {
			var json = jQuery.parseJSON(data);
			if (json.result == "nologin") {
				if (confirm(json.cause+"\n로그인 하시겠습니까?")) {
					document.location.replace("/login?redirect=/nikonschool/offline/view/" + idx);
				}
			}
			else if (json.result == "fail") {
				alert(json.cause);
			}
			else {
				if (confirm(json.cause)) {
					window.location.href = "/nikonschool/offline/view/" + idx;
				}
			}
		}
	  , error:		function(xhr, status, error) {
			alert(error);
		}
	});
}

