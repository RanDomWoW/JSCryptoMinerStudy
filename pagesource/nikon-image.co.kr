					}
				}
			}else{
				//같은페이지라서 쿠키안찍어
			}
			
			
			var a = cookiedata.split(";")[cookiedataindex].split("=")[1].replace(/%2C/gi, ','); 
			var results = a.match(/,/g); 
			if(results.length == 2 ){
				if(location.pathname !="/event/view/332" && location.pathname !="/event/ing/view/332" && location.pathname != "/event/end/view/332" && location.pathname.indexOf("/login") < 0){
					$("#emoticon_div").show();
				}
			}
			
			
		}
	}
	
	function setCookie( name, value, expirehours ) {
		var todayDate = new Date(); 
		todayDate.setHours( todayDate.getHours() + expirehours ); 
		document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";" ;
	}
	
	function CloseRenewEvent(){
		$("#emoticon_div").hide();
		var todayDate = new Date();
		todayDate.setDate(todayDate.getDate() + 1); 
		document.cookie = "eventcookiedisplay" + "=" + escape( "done" ) + "; path=/; expires=" + todayDate.toGMTString() + ";" ;
	}
	
