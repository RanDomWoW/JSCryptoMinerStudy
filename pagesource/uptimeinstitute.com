#sp-header a:hover {color: #000000 !important;}*/
#sp-header #sp-menu .sp-megamenu-parent > li.current-item.active > a, #sp-header #sp-menu .sp-megamenu-parent > li.sp-has-child.active > a {color: #000000 !important;}

/* LANGUAGE MENU */
.ui-nav-languages {font-size:12px;}
.ui-nav-languages a {line-height:14px !important; padding-top: 5px!important;}
.ui-nav-languages .dropdown-menu li {padding:5px 20px; font-size:12px;}
.ui-nav-languages {margin-right: 80px !important;} /* To accommodate Search */

/* STICKY HEADER */
#fading-header #sp-header #sp-menu .sp-column .sp-megamenu-parent > li > a {color: #777777;}
#fading-header #sp-header #sp-menu .sp-column .sp-megamenu-parent > li > a:hover {color: #006892;}

/* MENU */
.sp-megamenu-parent > li:first-child {display: none;} /* Hides Home from top nav menu */
#sp-header #sp-menu .sp-column .sp-megamenu-parent > li > a {color: #777777;}

#sp-header #sp-menu .sp-column .sp-megamenu-parent > li > a:hover {color: #006892;}
#sp-header #sp-menu .sp-column .sp-megamenu-parent li.padding-top {padding-top: 15px}
.sp-megamenu-parent > li > a { padding: 0 8px }


@media (max-width: 1360px){ /* If too many nav menu items or if items are too long for smaller browsers, reduce line-height */
#sp-header #sp-menu .sp-column .sp-megamenu-parent > li > a {line-height: 30px;}
#fading-header #sp-header #sp-menu .sp-column .sp-megamenu-parent > li > a {line-height: 30px;}
}

.top-search-wrapper .icon-top-wrapper .search-close-icon, .top-search-wrapper .icon-top-wrapper .search-open-icon {color:#777777 !important;}

.sp-module {margin-top: 20px;} /* Deceases spacing - used for breadcrumbs */
.breadcrumb {margin-bottom:0;}

.img-shadow img, .logo-slide img {box-shadow: 3px 3px 6px 2px rgba(153,153,153,0.66);} /* Shadows for relevant logo images */

/* HOME */
/*.feature-row div.col-box {background-color:#ffffff;}*/
.feature-row h2 {font-size:18px; font-weight:500; padding-right:15px; padding-left:15px;}
.feature-row p {font-size:14px; padding-right:15px; padding-left:15px;}
.front-content h3.sppb-addon-title {text-align:center !important; padding-right: 0px; box-shadow: none; display: block;}
.front-content h3.sppb-addon-title::after {background:none !important;}

/* Featured News & event Items */
.featured-news-row .sppb-img-responsive, .featured-events-row .sppb-img-responsive {max-width: 80%;}

/* Articles */
.item h2 {font-weight:400;}

.entry-header {margin-bottom:20px} /* Decreases header bottom space */
.entry-header h2 {font-size:3.3rem; font-weight:300; box-shadow:inherit;} /* Removes header underline */
.entry-header h2::after {display:none;} /* Removes header underline */
.entry-header h2 .divider {display:none;} /* Removes header underline */

.item td {padding:12px; vertical-align:top;} /* Adds in default padding zero-ed out by bootstrap */

.ui-content-header-mer {font-size:18px;}

.btn {margin-right:inherit;}
.btn-primary {background-color: #428bca;}

.program-container img {display:inline-block} /* Corrects images not centering in content */

/* SIDEBAR */
.main-content-side .side-left > img {
    max-width: 262px;
}

.main-content-side .side-box {
width: 262px;
padding: 10px;
text-align: left !important;
}
.main-content-side .side-box-light {
background-color: #ebebeb;
}
.main-content-side .side-box-dark {
background-color: #252525;
color: #ebebeb;
}
