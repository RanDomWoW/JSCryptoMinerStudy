table td img {display:inherit;}

/* HEADER */
#sp-top-bar {background-color: #ffffff !important; color: #cccccc; padding-top:3px !important;}
#sp-top-bar a {color: #999999 !important;}
#sp-top-bar a:hover {color: #666666 !important;}

#sp-header {background-color: #ffffff !important; color: #cccccc;}
