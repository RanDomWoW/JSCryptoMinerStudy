font-size: 14px;
}
.main-content-side .side-box p {
margin-right: 10px;
margin-left:10px;
}
.main-content-side .side-box-title {
font-size: 20px;
/*font-family: robotolight;*/
}

.main-content-side h3.sp-module-title {display:none;} /*Hide sidebar titles*/

.main-content-side h3 {font-size: 17px; font-weight: 500;}
.main-content-side .custom {font-size: 13px;}
.main-content-side .custom ul {padding-left: 18px;}
.main-content-side .custom li {margin-bottom: 5px;}
.main-content-side .side-box {font-size: 13px;}
.main-content-side .side-box ul {list-style: inherit; padding-left: 24px; padding-bottom: 2px;}
.main-content-side .side-box ul > li {display:list-item;}
.main-content-side .side-box ul > li > a {line-height:100%; color:#428bca;}
.main-content-side .side-box ul > li > a:hover {color:#006892;}
.main-content-side .side-box ul > li {margin-bottom:6px;}

