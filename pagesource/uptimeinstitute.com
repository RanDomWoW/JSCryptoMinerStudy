                if (regEx.test(emailAddress)) {
                    $.ajax({
                        type: 'GET',
                        data: {
                            'option' : 'com_ajax',
                            'module' : 'ui_marketo_form',
                            'cmd'    : 'addToList',
                            'email'   : emailAddress,
                            'format' : 'json'
                        },
                        success: function(response) {
                            target.parent().html(('Thank You! We will send our latest updates to your inbox.'));
                        }
                    });
                }
            }
        });
    })(jQuery)
</script>
