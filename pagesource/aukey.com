            jQuery(".uk-container").show(); //图标栏
            jQuery(".uk-list").show(); //菜单栏
            jQuery(".recom-pro").hide();//quick link
            jQuery(".back-index").hide();//搜索页返回箭头
            jQuery(".uk-sticky-placeholder").toggleClass("fixed");
        });

        //移动端-点击搜索框时
        jQuery("#mobilemenu .prompt").click(function(){
            jQuery(".uk-container").hide();//图标栏
            jQuery(".uk-list").hide(); //菜单栏
            jQuery(".back-index").show();
            jQuery(".recom-pro").show();
        });
       
        //移动端-搜索框值的判断
        jQuery("#mobilemenu .prompt").keyup(function(){
             var inpVal =  jQuery("#mobilemenu .prompt").val();
             if(inpVal != ''){
               jQuery(".clear-inp").show();
             }else{
                jQuery(".clear-inp").hide();
             }
        });

        //移动端-点击清空按钮
        jQuery(".clear-inp").click(function(){
            jQuery("#mobilemenu .prompt").val('');
            jQuery(this).hide();
        });
        
        //移动端-搜索页点击返回主页按钮
        jQuery(".back-index").click(function(){
            jQuery(".uk-container").show();
            jQuery(".uk-list").show();
            jQuery(".clear-inp").hide();
            jQuery("#mobilemenu .prompt").val('');
            jQuery(this).hide();
            jQuery(".recom-pro").hide();
        });

         //移动端点击菜单
        jQuery('.pro-li').click(function(){
            jQuery(this).siblings().removeClass('active');
            jQuery(this).siblings().find('.mob-pro-con').slideUp();
            jQuery(this).find('.mob-pro-con').slideToggle();
            jQuery(this).toggleClass('active');
        });

        //移动端foot文字展开收起
         if ((screen.width < 640)) {
                jQuery(".foot_c li").click(function () {
                jQuery(this).toggleClass('active');
                jQuery(this).find("div").toggle("slow");
            });
         }
         
          if ((screen.width > 640)) {
             //首页中间部分文字居中
            jQuery(".pic-tex").each(function(){
                var texWid = jQuery(this).width();
                var texHei = jQuery(this).height();
                jQuery(this).css({'margin-left':-texWid/2,'margin-top':-texHei/2})
            });
          }

           //supoort等页面面包屑固定-18.01.04
           function crumbs(){
               var top = jQuery(document).scrollTop();
                if(top > crumbTop - 100){
                    jQuery(".crumbs").addClass('fixed');
                } else{
                    jQuery(".crumbs").removeClass('fixed');
                }
           }
            if(jQuery('.crumbs').length != 0 ){
                var crumbTop = jQuery(".crumbs").offset().top;
                jQuery(window).scroll(function(){ 
                    crumbs();
                });
               crumbs();
            }
    });

   
