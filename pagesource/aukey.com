                            jQuery("#nav").css({'position':'','background':'#fff'});
                            jQuery("#home-slider").css({'margin-top':'initial'});
                        });
                    }
                    before = after;
                };
            });
        });

        //点击Need support form us?
        jQuery('.needSup').click(function(e){
            e.stopPropagation();  
			jQuery(".support-ul").slideToggle();
            jQuery(window).scroll(function(){
                jQuery(".support-ul").hide();
            });
        });
        jQuery(document).click(function(event) {  
            jQuery(".support-ul").hide();  
        });

        //点击搜索
        jQuery(".searchimg").click(function(){
            jQuery("html").css('overflow','hidden');
        });

        //点击搜索关闭按钮
        jQuery(".search-bg .basic").click(function(){
            jQuery("html").css('overflow','inherit');
        });

        //搜索页，点击空白地方关闭搜索页
        jQuery("#searchform").click(function(){
            jQuery("html").css('overflow','inherit');
        });

        //点击首页中间图片切换，文字跟着变
        jQuery(".pic-single .uk-slidenav").click(function(){
            jQuery(".previous-tex").toggle();
            jQuery(".next-tex").toggle();
        });

        //移动端点击菜单按钮
        jQuery('.icon-bars-btn').click(function(){
            jQuery(this).toggleClass('closed');
            jQuery("#mobilemenu").toggleClass('uk-hidden').toggleClass('uk-show');
