            jQuery(window).scroll(function() {
                var after = jQuery(window).scrollTop();
                if (before<after) { //鼠标往下滚动
                        jQuery("#nav").css({'position':'fixed','background':'#fff', 'box-shadow': '#ccc 0 6px 11px -6px'});
                        jQuery('.menu-ul li').click(function(){ //点击菜单
                            jQuery("#nav").css({'position':'fixed'});
                        });
                    before = after;
                     if ((jQuery(window).width() < 980)) { //分辨率小于980时菜单
                        jQuery(".uk-sticky-placeholder").addClass("fixed");
                    }
                };
                if (before>after) { //鼠标往上滚动
                    if (jQuery(document).scrollTop()<=0){  //滚动到顶部
                        jQuery("#nav").css({'position':'fixed','box-shadow':''});
                        jQuery("body").removeClass('white-nav');
                        jQuery(".uk-sticky-placeholder").removeClass("fixed");
                        jQuery("#home-slider").css({'margin-top':'-165px'});
