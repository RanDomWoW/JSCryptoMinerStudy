                        <script>
                            var selectedPlaceString = "Ashburn";

                            function clearAutoComplete()
                            {
                                document.getElementById('ContentPlaceHolderFullWidth_CityAutoTextBox').value = '';
                                document.getElementById('ContentPlaceHolderFullWidth_CityTextBox').value = '';
                                document.getElementById('ContentPlaceHolderFullWidth_RegionTextBox').value = '';
                                document.getElementById('ContentPlaceHolderFullWidth_CountryCodeHidden').value = '';
                                $('#place-hint-wrapper').hide();
                            }

                            function initCity() {
                                // START fix fastclick bug on iOS
                                $(document).on({
                                    'DOMNodeInserted': function () {
                                        $('.pac-item, .pac-item span', this).addClass('needsclick');
                                    }
                                }, '.pac-container');
                                // END fix fastclick bug on iOS

                                // START select first element on enter key
                                var input = document.getElementById('ContentPlaceHolderFullWidth_CityAutoTextBox');
                                var addEventListener = input.addEventListener ? input.addEventListener : input.attachEvent;
                                function addEventListenerWrapper(type, listener) {
                                    if (type === "keydown") {
                                        var origListener = listener;
                                        listener = function (event) {
                                            var suggestionSelected = $(".pac-item-selected").length > 0;
                                            if ((event.which === 13 || event.which === 9) && !suggestionSelected) {
                                                if ($(".pac-container").length > 0 && $(".pac-container").is(":visible")) {
                                                    var simulatedDownarrow = $.Event("keydown", { keyCode: 40, which: 40 });
                                                    origListener.apply(input, [simulatedDownarrow]);
                                                }
                                            }
                                            origListener.apply(input, [event]);
                                        };
                                    }
                                    addEventListener.apply(input, [type, listener]);
                                }
                                input.addEventListener = addEventListenerWrapper;
                                input.attachEvent = addEventListenerWrapper;
                                // END select first element on enter key
                                
                                var autocomplete = new google.maps.places.Autocomplete(document.getElementById('ContentPlaceHolderFullWidth_CityAutoTextBox'), { types: ['(cities)'] });

                                autocomplete.addListener('place_changed',
                                    function () {
                                        var autoCompleteErrors = '';
                                        clearAutoComplete();

                                        try {
                                            var place = autocomplete.getPlace();
                                            var countryCode = ''; var country = ''; var region = ''; var city = '';

                                            //document.getElementById('ContentPlaceHolderFullWidth_CustomErrorMessage').innerHTML = JSON.stringify(place);

                                            if (place.status !== undefined && place.status !== 'OK') {
                                                autoCompleteErrors += ' status: ' + place.status;
                                            }
                                            else {
                                                if (place.address_components !== undefined) {
                                                    for (var i = 0; i < place.address_components.length; i++) {
                                                        if (place.address_components[i].types !== undefined) {
                                                            for (var j = 0; j < place.address_components[i].types.length; j++) {
                                                                var type = place.address_components[i].types[j];
                                                                if (type === 'country') {
                                                                    if (place.address_components[i].short_name !== undefined) countryCode = place.address_components[i].short_name.toUpperCase();
                                                                    if (place.address_components[i].long_name !== undefined) country = place.address_components[i].long_name;
                                                                }
                                                                if (type === 'administrative_area_level_1') {
                                                                    if (place.address_components[i].long_name !== undefined) region = place.address_components[i].long_name;
                                                                }
                                                                if (type === 'locality') {
                                                                    if (place.address_components[i].long_name !== undefined) city = place.address_components[i].long_name;
                                                                }
                                                            }
                                                        }
                                                        else {
                                                            autoCompleteErrors += ' address_components[' + i + '].types[0] === undefined';
                                                        }
                                                    }
                                                }
                                                else {
                                                    autoCompleteErrors += ' address_components === undefined' + ' for ' + encodeURIComponent(place.name);
                                                }

                                                //if (city === '') city = place.name; // fallback so that the user should be able to submit something including the error if present

                                                document.getElementById('ContentPlaceHolderFullWidth_CountryCodeHidden').value = countryCode;
                                                document.getElementById('ContentPlaceHolderFullWidth_RegionTextBox').value = region === '' ? 'N/A' : region;
                                                document.getElementById('ContentPlaceHolderFullWidth_CityTextBox').value = document.getElementById('ContentPlaceHolderFullWidth_CityAutoTextBox').value = city;

                                                if (region !== '' && region !== city) {
                                                    $('#place-hint').text(region + ', ' + country);
                                                    $('#place-hint-wrapper').show();
                                                } else if (country !== city) {
                                                    $('#place-hint').text(country);
                                                    $('#place-hint-wrapper').show();
                                                }

                                                selectedPlaceString = document.getElementById('ContentPlaceHolderFullWidth_CityAutoTextBox').value;
                                            }
                                        }
                                        catch (e) {
                                            autoCompleteErrors += ' Exception: ' + e.message;
                                        }

                                        document.getElementById('ContentPlaceHolderFullWidth_AutoCompleteErrorsHidden').value = autoCompleteErrors;
                                        if (autoCompleteErrors !== '') $.ajax('/AutoCompleteFailed/' + encodeURIComponent(autoCompleteErrors));
                                    });
                                }

                            $('#ContentPlaceHolderFullWidth_CityAutoTextBox').focusin(function () { cnUtil.scrollTo('#city-auto-label', 0, 200); });
                            $('#ContentPlaceHolderFullWidth_CityAutoTextBox').keydown(function () { cnUtil.scrollTo('#city-auto-label', 0, 200); });
                            $('#ContentPlaceHolderFullWidth_CityAutoTextBox').keyup(function () { cnUtil.scrollTo('#city-auto-label', 0, 200); });
                            $('#ContentPlaceHolderFullWidth_CityAutoTextBox').focusout(function () {
                                if (selectedPlaceString !== document.getElementById('ContentPlaceHolderFullWidth_CityAutoTextBox').value) {
                                    clearAutoComplete();
                                }
                            });
