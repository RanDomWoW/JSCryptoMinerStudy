<script type="text/javascript">
$(document).ready(function()
  { 
      $("#popup_right").show();

      var show_popup_right = false;

      if( window.location.href == 'http://lamro.tv/index.php' || window.location.href == 'http://lamro.tv/')
          show_popup_right = true;
      
      if ($("#watchfree_popup").attr("block_view") == 1)
       {inter_id = setInterval(func_free_view, 1000);}
      
      
      function func_free_view()
      {

         var time_str = $("#watchfree_popup").find("span").html();
         var arr = time_str.split(':');
             v_sec = parseInt(arr[0])*60 + parseInt(arr[1]);
             
             v_sec = v_sec - 1;
             
             min = parseInt(v_sec/60);
             sec = v_sec - (min*60);
             
             min_str = (min+100); 
             sec_str = (sec+100);
             
             time_res = min_str+":"+sec_str+"_";
             time_res1 = sec_str+"_";

             time_res = time_res.substring(1, 4)+time_res1.substring(1, 3);
             
           $("#watchfree_popup").find("span").html(time_res);  
            
         if (parseInt(v_sec) < 1)    
           {
             clearInterval(inter_id);
               
             $("#watchfree_popup").attr("block_view", 0);
             var v_html = 'Смотри Lamro.TV бесплатно<span>5</span> минут <button id="tryfreeinregform_popup" class="smal-btn">Смотреть</button></div>';
             $("#watchfree_popup").html(v_html);
               
           }
          
      }
      
      

      $(window).scroll(function() {

        var windowWidth = window.innerWidth;

        if(($(this).scrollTop() != 0) && ($("#watchfree_popup").attr("auto_show") == 1) && show_popup_right && windowWidth > 600) 
         {$("#popup_right").addClass('active');} 
        else 
         {$("#popup_right").removeClass('active');}
      });
     
      
   $("#cross").click(function(){

   if ($("#popup_right").hasClass('active'))
    {$("#popup_right").removeClass('active');
     $("#watchfree_popup").attr("auto_show", 0);}
   else
    {$("#popup_right").addClass('active');} 

 });
 
   $("#tryfreeinregform_popup").click(function(){
     

     /*Устанавливаем время начала просмотра*/ 
     url = "/php_interface/connect_5free_user.php";
     $.get(url, function(data)
      {document.location.href = 'http://iptv.lamro.tv/tv/?id=1';});
   });

  });


</script>              
                                
