<script>

    $(document).ready(function () {

        $("#memberinfo").hide();
        ReplaceTRChars();
        $(".up").click(function () {
            window.location.href = "https://online.atlasglb.com/unlimited/user/registration?lang=" + _language.toLowerCase();
        });

        $(".btn-buy").click(function () {
            window.location.href = "https://online.atlasglb.com/unlimited/user/registration?lang=" + _language.toLowerCase();
        });

        $(".btn-signin").click(function () {
            $.ajax({
                url: '/BussinesCampaing/Login',
                data: { username: $("#lastname").val(), password: $("#password").val(), lang: _language },
                dataType: 'json',
                async: true,
                success: function (ds) {
                    if (ds.success) {
                        var params = { username: $("#lastname").val(), password: $("#password").val(), xmlLang: _language.toUpperCase() };
                        var action = "https://online.atlasglb.com/unlimited/user/login";
                        postGivenParameters(params, action);
                    }

                    else {

                        $("#memberinfo").show();
                        $("#lastname").addClass('error');
                        $("#password").addClass('error');
                    }
                }
            });
        });

        function postGivenParameters(params, action) {
            var my_form = document.createElement('FORM');
            my_form.name = 'myForm';
            my_form.method = 'POST';
            my_form.action = action;
            for (var name in params) {
                var my_tb = document.createElement('INPUT');
                my_tb.type = 'HIDDEN';
                my_tb.name = name;
                my_tb.value = params[name];
                my_form.appendChild(my_tb);
            }
            document.body.appendChild(my_form);
            my_form.submit();
        }


        $(".send").click(function () {

            $.ajax({
                url: '/BussinesCampaing/ForgotPassword',
                data: { email: $("#mailname").val() , lang: _language},
                dataType: 'json',
                async: true,
                success: function (ds) {
                    if (ds.success) {
                        $(".fpFooter").hide();
                        $("#flyok").show();
                        $("#mailok").show();
                        $("#forinput").hide();
                        $("#mail").hide();
                    }
                    else {
                        $("#mailfalse").show();
                        $("#mailname").addClass('error');
                    }
                }
            });


        });
        $("#forgotpassword").click(function () {
            $("#memberinfo").hide();
            $("#flyok").hide();
            $("#mailok").hide();
            $("#mailfalse").hide();
            $(".fpFooter").show();
            $("#forinput").show();
            $("#mail").show();
        });
        

    });

    function ReplaceTRChars() {
        $('.uppercase').keyup(function () {
            var text = $(this).val().toUpperCase();
            text = text.TurkishToEnglish();
            $(this).val(text);
        });
    }
    String.prototype.TurkishToEnglish = function () {
        var string = this.toUpperCase();
        var chars = {
            "i": "I", "ş": "S", "ğ": "G", "ü": "U", "ö": "O", "ç": "C", "ı": "I", "İ": "I"
        , "Ş": "S", "Ğ": "G", "Ü": "U", "Ç": "C", "Ö": "O"
        };
        string = string.replace(/(([İŞĞÜÇÖ]))/g, function (letter) { return chars[letter]; });
        //  console.log(string);
        return string;
    }
	</script>
