<script type="text/javascript">
function search(){
  var mysearchword=$("#js_searchword2").val();
  var channelid =$('#channelid option:selected').val();
  var mytype=$('#js_type option:selected').val();
    mysearchword=mysearchword.substr(0,40);
    mysearchword=mysearchword.replace(/-/img,' and ');
    if (mytype) {
    window.open("/search/search?channelid="+channelid+"&searchword="+mysearchword+"&searchscope="+mytype);
    }else{
         window.open("/search/search?channelid="+channelid+"&searchword="+mysearchword);
    }
}
function settype(channelid){
    //222615 产品测评;207501 服务资质测评;274045 人员注册
    var prd_option='<option value="" selected = "selected" >默认</option><option value="cardid">证号</option><option value="productname">产品名称</option><option value="company">单位</option><option value="3">认证级别</option>';
    var srv_option='<option value="" selected = "selected">默认</option><option value="cardid">证号</option><option value="orgunit">单位</option><option value="cardlevel">认证级别</option>';
    var cise_option='<option value="" selected = "selected">默认</option><option value="cardid">证号</option><option value="name">姓名</option><option value="cardlevel">认证级别</option>';
    switch (channelid)
        {
            case '222615':
                $('#js_type').html(prd_option);
            break;
            case '207501':
                $('#js_type').html(srv_option);
            break;
            case '274045':
                $('#js_type').html(cise_option);
            break;
        }
}
settype('222615');
$('#channelid').change(function(event){//监控
    var channelid=$('#channelid option:selected') .val();
    settype(channelid);
});
$("#js_searchword2").keypress(function(event) {//监控输入框enter按键
    var keycode = (event.keyCode ? event.keyCode : event.which);
    if (keycode == '13') {
event.preventDefault();
        search();
    }
});
$("#js_dosearch2").click(function(event) {//监控输入框enter按键
event.preventDefault();
        search();
});
</script>
	
