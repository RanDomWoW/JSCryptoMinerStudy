    <div class="modal-home">
        <div class="text-white text-center padding-b-medium">
            <p class="home-modal-maintext">
                Dall'esperienza di <br> CartaSi e ICBPI nasce Nexi
            </p>
            <p class="home-modal-text">
                per costruire insieme alle nostre Banche Partner il futuro dei <br> pagamenti digitali nel nostro Paese.
            </p>
        </div>
        <div class="d-flex flex-column mt-0 mt-md-4 uppercase">
            <div class="align-self-center mt-3">
                <a class="link-cta is-button empty-h1" href="javascript:void(0)" data-dismiss="modal" aria-label="Close">Scopri di più</a>
            </div>

        </div>
    </div>
</div>

<script>
    window.launchModal = function($) {
        var cookieName = 'icpbi-welcome';
        var cookieDays = 60;

        function getCookieDomain() {
            var domainName = document.location.hostname;

            if (domainName) {
                // verifico se è un indirizzo IP oppure localhost
                if (domainName.match(/^(?:(?:[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3})|(?:localhost))$/g)) {
                    return '';
                }

                // ricavo l'ultima parte del dominio e l'estensione
                var domainNameSplit = domainName.split('.');
                if (domainNameSplit.length >= 2) {
                    return '.' + domainNameSplit.splice(-2).join('.');
                }
            }

            return '';
        }

        function getCookie(cookieName) {
            var cookieIdentifier = cookieName + '=';
            var cookieArray = document.cookie.split(';');
            for (var i = 0; i < cookieArray.length; i++) {
                var currentCookie = cookieArray[i] && cookieArray[i].trim();
                if (currentCookie.indexOf(cookieIdentifier) === 0) {
                    return currentCookie.substring(cookieIdentifier.length, currentCookie.length);
                }
            }
            return '';
        }

        function setCookie(cname, cvalue, exdays) {
            var expireDate = new Date();
            expireDate.setTime(expireDate.getTime() + exdays * 24 * 60 * 60 * 1000);
            var expires = 'expires=' + expireDate.toUTCString();
            var domain = getCookieDomain();

            var cookieStr = cname + '=' + cvalue + ';' + expires + ';path=/';
            if (domain) {
                cookieStr += ';domain=' + domain;
            }
            document.cookie = cookieStr;
        }

        function modalCloseHandler() {
            setCookie(cookieName, 1, cookieDays);
        }

        if (window.wcmmode_disabled && !getCookie(cookieName)) {
            setTimeout(function() {
                $('#modal-container').fragmentHtml($('#launchModal').html(), {
                    modalParams: {
                        size: 'modal-large', // '', 'modal-large', 'modal-full'
                        background: 'bg-blue', // 'bg-white', 'bg-blue', 'bg-black'
                        modalCenter: true, // true, false
                        noPadding: true, // true, false
                        hideHeader: true, // true, false,
                        closeIcon: false, //true = icon, false = text
                        externalClose: 'external-close',
                        title: '',
                        extraclass: 'launch-modal'
                    }
                });

                $('#modal-container').on('hidden.bs.modal', function(event) {
                    modalCloseHandler();
                    $('#modal-container').off('hidden.bs.modal', modalCloseHandler);
                });

                $('body').on('click', '.close-modal', function(event) {
                    var target = $(event.target).data('href');
                    console.log(target);
                    modalCloseHandler();
                    window.location.href = target;
                });

            }, 1000);
        }
    };

    if (window.htmlBoxFunctionsArray && Array.isArray(window.htmlBoxFunctionsArray)) {
        window.htmlBoxFunctionsArray.push(window.launchModal);
    }
</script>

