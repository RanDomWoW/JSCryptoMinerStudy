<script type="text/javascript">
	
		
	//Show enquiry form only for pages in array 
	var pages = ['index','videos','enlight-cloud-hosting','private-cloud-hosting','enlight360','dedicated-server-hosting','iot',
                    'sap-hana-cloud-hosting','waf','ecos','gdns','mtvscan-security','gst','sap','sap-hosting','core-banking-hosting',
                    'disaster-recovery-hosting',
                    'mobile-app-hosting','application-support','cross-platform-data-migration','database-administration-services',
                    'database-consultancy-services', 'database-hosting-services','backup-recovery','colocation','emagic',
                    'emagic-application-monitoring','our-datacenter','dc-network','dc-security','project-green-connect','dc-tour'];
	var url = window.location.href;
	var lastPart = url.split("/").pop();
	//console.log('url',url);
	//console.log('lastPart',lastPart);

	if(pages.indexOf(lastPart) == -1){
		//console.log('not array');
		if(lastPart != ''){
			$('#hiddenPanel').hide();
		}
	}else{
		//console.log('in array');
	}

	var speed = 300;
    $('#close-bar').on('click', function(){

        var $$ = $(this),
            panelWidth = $('#hiddenPanel').outerWidth();

        if( $$.is('.myButton') ){
            $('#hiddenPanel').animate({left:0}, speed);
            $$.removeClass('myButton')
        } else {
            $('#hiddenPanel').animate({left:-panelWidth}, speed);
            $$.addClass('myButton')
        }

    });

    jQuery.validator.addMethod("specialChars", function( value, element ) {
        var regex = new RegExp("^[a-zA-Z ]+$");
        var key = value;

        if (!regex.test(key)) {
           return false;
        }
        return true;
    }, "Please use only alphabetic characters");

    // Enquiry form validation
	$(function() {
	  // Initialize form validation on the registration form.
	  // It has the name attribute "registration"
	  $("#enquiry_form").validate({
	    // Specify validation rules
	    rules: {
	      // The key name on the left side is the name attribute
	      // of an input field. Validation rules are defined
	      // on the right side
	      name: {
	      		required:true,
	      		specialChars: true
	  		},
	      email: {
	        required: true,
	        // Specify that email should be validated
	        // by the built-in "email" rule
	        email: true
	      },
	      phone: {
	        required: true,
	        number: true,
	        maxlength: 10,
	        minlength:10
	      }
	    },
	    // Specify validation error messages
	    messages: {
	      firstname: "Please enter your firstname",
	      lastname: "Please enter your lastname",
	      phone: {
	        required: "Please provide your phone number",
	      },
	      email: "Please enter a valid email address"
	    },
	    // Make sure the form is submitted to the destination defined
	    // in the "action" attribute of the form when valid
	    submitHandler: function(form) {
	      form.submit();
	    }
	  });
	});
</script>

