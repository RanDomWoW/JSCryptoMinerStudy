<script type="text/javascript">
	var contentsRotatorIndex104=0;
	function rotatorSwitchTo104(ord) {
		//ord es el numero de pagina
		//calculo el inicio y el fin
		var inicio=ord * 9;
		var fin = inicio + 9;
		fin--;
		if(fin>=19){
			fin = 19-1;
		}
		var pagina = ord;
		//Apago todos los contenidos	
				
			$("#content-rotator-104 div.items div.item").hide();

		$("#content-rotator-104 div.item-page-selector ul li").removeClass("selected-item");
