		for(var i=inicio;i<=fin;i++){
			ord=i;				

				$($("#content-rotator-104 div.items div.item")[ord]).show();

		}
		$("#content-rotator-104 div.item-page-selector ul li:nth("+(pagina+1)+")").addClass("selected-item");
		contentsRotatorIndex104=pagina;
	}
	function rotatorSwitchToPrevius104() {
		var ord;
		if (contentsRotatorIndex104==0) ord=19;
		else ord=contentsRotatorIndex104;
		rotatorSwitchTo104(ord-1);
	}
	function rotatorSwitchToNext104() {
		var ord;
		if (contentsRotatorIndex104==2) ord=0;
		else ord=contentsRotatorIndex104+1;
		rotatorSwitchTo104(ord);
	}
	var contentsRotatorTimeout104=setInterval("rotatorSwitchToNext104()", 5000);
	function rotatorRestart104() {
		rotatorClearTimeout104();
		contentsRotatorTimeout104=setInterval("rotatorSwitchToNext104()", 5000);
	}
	function rotatorClearTimeout104() {
		clearTimeout(contentsRotatorTimeout104);
	}
