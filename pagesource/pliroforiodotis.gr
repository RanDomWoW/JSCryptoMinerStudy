  var nsp_pro1_unique_id = "pro1";
  var nsp_pro1_name_prefix = "nspro_pro1_";
  var nsp_pro1_thank_you_text = "<h4>Ευχαριστούμε\nγια\nτην\nΕγγραφή.</h4>";
  var nsp_pro1_confirmation_sent_text = "<h4>Ευχαριστούμε\nγια\nτην\nεγγραφή.\nΈνα\nμήνυμα\nεγγραφής\nέχει\nαποσταλεί\nστο\nemail\nσας.</h4>";
  var nsp_pro1_blacklist_error = "Your email has been blacklisted.";
  var nsp_pro1_general_error = "There was a problem with completing your subscription.";
  var nsp_pro1_confirmation_error = "There was a problem with sending you a confirmation link.";
  var nsp_pro1_additional_field_ids = "";
  var juri_base = "http://pliroforiodotis.gr/";
  var nsp_pro1_nspro_aj;

  function nsp_pro1_nspro_submit() {
    // switch to loading state.
    document.getElementById(nsp_pro1_unique_id+"_nspro_form").setAttribute("style", "display:none;");
    var m = document.getElementById(nsp_pro1_name_prefix+"messages");
    m.innerHTML = "<img src=\""+juri_base+"components/com_nspro/img/loading.gif\" class=\"nspro_loading_image\" width=\"64\"/>";
    m.removeAttribute("style");

    if (window.XMLHttpRequest) { // Mozilla, Safari, IE7+ ...
        nsp_pro1_nspro_aj = new XMLHttpRequest();
    } else if (window.ActiveXObject) { // IE 6 and older
        nsp_pro1_nspro_aj = new ActiveXObject("Microsoft.XMLHTTP");
    }

    nsp_pro1_nspro_aj.open("POST", "index.php?option=com_nspro&task=handle_ajax_task", true);
    nsp_pro1_nspro_aj.onreadystatechange = nsp_pro1_nspro_handleResponse;
    nsp_pro1_nspro_aj.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
    var form = document.getElementById(nsp_pro1_unique_id+"_nspro_form");
    var q = [];
    for (var i = form.elements.length - 1; i >= 0; i--) {
      if (form.elements[i].name === "") { continue; }
      switch (form.elements[i].nodeName) {
      case "INPUT":
        switch (form.elements[i].type) {
        case "text":
        case "email":
        case "hidden":
        case "button":
          q.push(form.elements[i].name + "=" + encodeURIComponent(form.elements[i].value));
          break;
        case "checkbox":
        case "radio":
          if (form.elements[i].checked) {
            q.push(form.elements[i].name + "=" + encodeURIComponent(form.elements[i].value));
          }
          break;
        }
        break;
      case "TEXTAREA":
        q.push(form.elements[i].name + "=" + encodeURIComponent(form.elements[i].value));
        break;
      case "SELECT":
        q.push(form.elements[i].name + "=" + encodeURIComponent(form.elements[i].value));
        break;
      }
    }
    nsp_pro1_nspro_aj.send(q.join("&"));
  }


  function nsp_pro1_nspro_handleResponse() {
    if ((nsp_pro1_nspro_aj.readyState === 4) && (nsp_pro1_nspro_aj.status === 200)) {
      nsp_pro1_nspro_doRespond(nsp_pro1_nspro_aj.responseText);
    }
  }

  function nsp_pro1_nspro_doRespond(response) {
    document.getElementById(nsp_pro1_name_prefix+"messages").setAttribute("style", "display:none;");
    if (response == "success") {
      document.getElementById(nsp_pro1_unique_id+"_nspro_form_wrapper").innerHTML = nsp_pro1_thank_you_text;
    }
    else if (response == "confirmation_sent") {
      document.getElementById(nsp_pro1_unique_id+"_nspro_form_wrapper").innerHTML = nsp_pro1_confirmation_sent_text;
    }
    else if (response.substring(0, 9) == "redirect:") {
      window.location.assign(response.replace("redirect:", ""));
    }
    else if (response.substring(0, 6) == "debug:") {
      alert(response);
    }
    else {
      nsp_pro1_nspro_parseErrors(response);
    }
    document.getElementById(nsp_pro1_unique_id+"_nspro_form").removeAttribute("style");
  }

  function nsp_pro1_nspro_parseErrors(response) {
    var m = document.getElementById(nsp_pro1_name_prefix+"messages");
    m.setAttribute("style", "display:none;");
    m.innerHTML = "";
    //m.removeAttribute("style");
    //m.innerHTML = response;

    var field_array = [ "name", "email" ];
    if (nsp_pro1_additional_field_ids != "") {
      field_array = field_array.concat(nsp_pro1_additional_field_ids.split(","));
    }
    for (var i = 0; i < field_array.length; i++) {
      var d = document.getElementById(nsp_pro1_name_prefix+field_array[i]);
      if (d) {
        // Sometimes, custom fields do not use IDs in this way.
        d.className = d.className.replace(/\bnspro_error\b/,"");
        if (response.indexOf(field_array[i]+"_error") != -1) {
          d.className = d.className + " nspro_error";
        }
      }
    }

    if (response.indexOf("blacklist_error") != -1) {
      m.removeAttribute("style");
      m.innerHTML = m.innerHTML + nsp_pro1_blacklist_error+"<br/>";
    }

    if (response.indexOf("confirmation_error") != -1) {
      m.removeAttribute("style");
      m.innerHTML = m.innerHTML + nsp_pro1_confirmation_error+"<br/>";
    }

    if (response.indexOf("general_error") != -1) {
      m.removeAttribute("style");
      m.innerHTML = m.innerHTML + nsp_pro1_general_error+"<br/>";
    }
  }

