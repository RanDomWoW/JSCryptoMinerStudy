}
.rate:before, .rate:after {
    position: absolute;
    top: 3px;
    left: 0;
    content: '★★★★★';
    display: inline-block;
    height: 20px;
    line-height: 20px;
}
.rate:before {
    color: #ddd; /*星色*/
}
.rate:after {
    color: #ffa500;
    overflow: hidden;
    white-space: nowrap; /*重要*/
}

.rate00:after{width: 0;}
.rate05:after{width: 10px;}
.rate10:after{width: 20px;}
.rate15:after{width: 30px;}
.rate20:after{width: 40px;}
