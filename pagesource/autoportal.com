AP.cmd = AP.cmd || [];

AP.cmd.push(function(container) {
    var cookieStorage = new Storages.CookieStorage();
    var storageKey = "snsShowedOnce";
    var popupKey = "sns_popup";
    var templateSelector = "#template-fade-popup_sns";
    var idleTimeEvents = [
        'mousemove.sns',
        'touchmove.sns',
        'pointermove.sns',
        'mousewheel.sns'
    ];

    if (!cookieStorage.get(storageKey)) {
        createSnsPopup();
    }

    function createSnsPopup() {
        var $popup = renderPopup();

        var snsPopup = new Popup.BasePopup($popup.get(0));
        container.get("popup_service").add(popupKey, snsPopup);

        snsPopup.onClose(function() {
            $popup.fadeOut(300);
        });

        snsPopup.onOpen(function() {
            $popup.fadeIn(300);
            cookieStorage.set(storageKey, 1);
        });

        var socialInited = false;
        snsPopup.onOpen(function () {
            if (!socialInited) {
                initSocialNetworks($popup);
            }
        });

        var timeout = 45000;
        var timeoutId = setTimeout(openPopup, timeout);

        enableListener();

        function wait() {
            restartTimer();
            disableListener();
            enableListener();
        }

        function restartTimer() {
            clearTimeout(timeoutId);
            timeoutId = setTimeout(openPopup, timeout);
        }

        function openPopup() {
            clearTimeout(timeoutId);
            $(document).off(idleTimeEvents.join(' '), wait);
            container.get("popup_service").showPopup(popupKey);
        }

        function enableListener() {
            $(document).on(idleTimeEvents.join(' '), wait);
        }

        function disableListener() {
            $(document).off(idleTimeEvents.join(' '), wait);
        }

        snsPopup.onOpen(function() {
            clearTimeout(timeoutId);
            disableListener();
        });
    }

    function initSocialNetworks($popup) {
        container.get("social_networks_service").requireGoogleApi().done(function() {
            gapi.ytsubscribe.go($popup.get(0));
        });

        container.get("social_networks_service").requireFacebookApi().done(function() {
            if ($popup.find(".fb-root-none").length) {
                $popup.find(".fb-root-none").remove();
                FB.XFBML.parse($popup.get(0));
            }
        });
    }

    function renderPopup() {
        var $tmpl = $(templateSelector);
        var $popup = $tmpl.tmpl();
        $tmpl.before($popup);

        return $popup;
    }
});
