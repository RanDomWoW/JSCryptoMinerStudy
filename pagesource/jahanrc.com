	<script>
	// jQuery('.tab_active').fadeTo(100,0.9);
	jQuery('#ao_tabs > a').click(function() {
			clearInterval(i);
			i = setInterval(cycle, 5000);
			var tab = jQuery('.tab_' + jQuery(this).attr('tab'));
			if (tab.length) {
					// Hide active tab & selected style:
					jQuery('.tab_active').fadeTo(50,0);
					jQuery('.tab_active').removeClass('tab_active');
					jQuery('#ao_tabs .active').removeClass('active');

					// Show clicked tab content
					tab.addClass('tab_active');
					jQuery(tab).css("opacity", 0);
					jQuery(tab).fadeTo(50,0.9);
					jQuery(this).addClass('active');

					tab.show("slide", {
							direction: "down"
					}, 1000);
					jQuery(this).show("puff", {}, 10);
			}
	});

	// starting index
	var currTab = 0;

	// count of all tabs
	var totalTabs = jQuery("#ao_tabs > a").length;

	// function to pass to setInterval


	function cycle() {

			// simulate click on current tab
			jQuery("#ao_tabs > a").eq(currTab).click();

			// increment counter
			currTab++;

			// reset if we're at the last one
			if (currTab == totalTabs) {
					currTab = 0;
			}
	}

	// go!
	var i = setInterval(cycle, 5000);

	</script>


