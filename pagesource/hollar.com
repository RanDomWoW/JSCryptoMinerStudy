</div></section></div><div class="floating-filter-menu"><div class="b-row grey-row"><h3 class="on-mobile">Refine</h3><div class="close-filter"><img src="https://assets.hollar.com/pipeline/production/icon-light-grey-close-b482705fbace10a6b38bbde774fb2c4a46edf95b42bf8b54a86db723dda082cd.png" width="16" /></div></div><div class="b-row"><input class="floatingfilterByHollarItems ios8-switch" id="hollar-only" type="checkbox" /><label for="hollar-only"><img class="hollar-initial" src="https://assets.hollar.com/pipeline/production/h-hollar-initial-1b2df225025b03c4c1951662753b431cf7163acf1c1a0794ef449cb3142df306.png" width="13" />Show Hollar Items Only</label></div><div class="b-row grey-row"><h4>Sort By</h4></div><div class="b-row"><div class="radio-orange"><input checked="checked" name="sortby" ref="1" type="radio" value="conversions-desc" /><label class="radio-orange">Bestsellers</label></div></div><div class="b-row"><div class="radio-orange"><input name="sortby" ref="2" type="radio" value="available_on-desc" /><label class="radio-orange">New Items</label></div></div><div class="b-row"><div class="radio-orange"><input name="sortby" ref="3" type="radio" value="price-asc" /><label class="radio-orange">Price: Low to High</label></div></div><div class="b-row"><div class="radio-orange"><input name="sortby" ref="4" type="radio" value="price-desc" /><label class="radio-orange">Price: High to Low</label></div></div><div class="floating-action-button"><img src="https://assets.hollar.com/pipeline/production/TabButton@2x-3f3f380a4b6a874d3fc1103b01d76ad6bef7b74090616bf6fb9f13544e646f53.png" /></div></div><div class="floating-filter-confirm-row"><div class="confirm-inner"><button class="apply">Apply</button><span class="reset">Reset</span></div></div><div class="dim-bg"></div><script>$(document).ready(function() {
  const filterButton        = $('.floating-action-button');
  const filterMenu          = $('.floating-filter-menu');
  const filterbyCheckBox    = $('.filterby')
  const filterConfirmRow    = $('.floating-filter-confirm-row');
  const hollarOnlyFilter    = $('input#hollar-only');
  const bestSellerRadio     = $('.radio-orange input[ref="1"]')
  const closeFilter         = $('.floating-filter-menu .close-filter');
  const applyButton         = $('.floating-filter-enabled button.apply');
  const resetButton         = $('.floating-filter-enabled .reset');
  const dimBackground       = $('.dim-bg');
  const promoPod            = $('.m-promoPod-container');
  const taxonProductFeed    = $('div[data-hook="taxon_products"]');
  const searchProductFeed   = $('div[data-hook="products_search_results_heading"]');
  const homeProductFeed     = $('div[data-hook="homepage_products"]');
  const merchantProductFeed = $('div[data-hook="merchant_products"]')
  const productSort         = new APP.ProductSort;

  const getContactUsButton = function() {
    return $('#designstudio-button');
  };

  const getAppBanner = function () {
    return $('#branch-banner-iframe');
  };

  const mobileView = function() {
    return $('.mobile-view');
  };

  const isVisible = function(element) {
    let top_of_element = element.offset().top;
    let bottom_of_element = element.offset().top + element.outerHeight();
    let bottom_of_screen = $(window).scrollTop() + window.innerHeight;
    let top_of_screen = $(window).scrollTop();

    if((bottom_of_screen > top_of_element) && (top_of_screen < bottom_of_element)){
      return true;
  } else {
      return false;
    }
  };

  filterMenu.find('.radio-orange input').on('change', function(e) {
    option = $(e.currentTarget).parent().find('label').text()
    productSort.reportHollarSortEvent('sideFilter', option)
  });

  filterMenu.find('.checkbox-wrapper input').on('change', function(e) {
    option = $(e.currentTarget).closest('.b-row').find('label').text()
    productSort.reportHollarFilterEvent('sideFilter', option)
  });

  filterButton.on('mousedown', function (e) {
    if (mobileView().length > 0) {
      getContactUsButton().hide();
      getAppBanner().hide();
    };
    dimBackground.fadeToggle(100);
    filterMenu.toggleClass('slide-in');
    filterConfirmRow.toggleClass('slide-in');
    $('body').toggleClass('body-no-scroll');

    productSort.reportHollarSideFilterOpenEvent();
  });

  closeFilter.on('mousedown', function (e) {
    if (mobileView().length > 0) {
      getContactUsButton().show();
      getAppBanner().show();
    };
    dimBackground.fadeOut(100);
    filterMenu.removeClass('slide-in');
    filterConfirmRow.removeClass('slide-in');
    $('body').removeClass('body-no-scroll');
  });

  applyButton.on('mousedown', function (e) {
    e.preventDefault();
    if (mobileView().length > 0) {
      getContactUsButton().show();
      getAppBanner().show();
    };

    productSort.filterOrderContent();
    dimBackground.fadeOut(100);
    filterMenu.removeClass('slide-in');
    filterConfirmRow.removeClass('slide-in');
    $('body').removeClass('body-no-scroll');

    productSort.reportHollarSideFilterApplyEvent();
  });

  resetButton.on('mousedown', function(e) {
    e.preventDefault();
    hollarOnlyFilter.prop('checked', false);
    bestSellerRadio.prop('checked', true)
    filterbyCheckBox.prop('checked', false)

    productSort.reportHollarSideFilterResetEvent();
  });

  dimBackground.on('mousedown', function (e) {
    dimBackground.fadeOut(100);
    filterMenu.removeClass('slide-in');
    filterConfirmRow.removeClass('slide-in');
    $('body').removeClass('body-no-scroll');
  });

  if (homeProductFeed.length > 0) {
    $(window).scroll(function (e) {
      if (isVisible(homeProductFeed) && $(window).scrollTop() > 400 && mobileView().length == 0) {
         filterButton.fadeIn(300);
      }
      else if (mobileView().length == 0) {
         filterButton.fadeOut(300);
      }
    });
  };

  if (taxonProductFeed.length > 0) {
    $(window).scroll(function (e) {
      if (isVisible(taxonProductFeed)) {
        filterButton.fadeIn(300);
      }
      else {
        filterButton.fadeOut(300);
      }
    });
  };

  if (merchantProductFeed.length > 0) {
    $(window).scroll(function (e) {
      if (isVisible(merchantProductFeed)) {
        filterButton.fadeIn(300);
      }
      else {
        filterButton.fadeOut(300);
      }
    });
  };

});</script></div></div><footer class="clearfix">
