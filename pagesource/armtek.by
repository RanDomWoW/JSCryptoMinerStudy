            text-align:center;
            margin-bottom:5px;
        }
        .col-xs-6 {
            position: relative;
            min-height: 1px;
            padding-right: 15px;
            padding-left: 15px;
        }

        .col-xs-12 {
            float: left;
            width: 100%;
        }

        .contact, .link-shop {
            padding-top: 20px;
        }

        .link-shop {
            clear: both;
        }

        .col-xs-6 {
            float: left;
            width: 50%;
        }

        .col-xs-12 {
            position: relative;
            min-height: 1px;
            padding-right: 15px;
            padding-left: 15px;
        }

        .logo, .search-head {
            text-align: center;
        }

        .primary-menu > ul > li > ul {
            display: none;
        }
        .primary-menu>ul>li.current-menu-item>a, .primary-menu>ul>li.current-menu-parent>a{
            color: #ff7119;
            border-bottom: 3px solid #ff7119;
        }
        .primary-menu>ul>li>a {
            display:block;
            font-weight: 600;
            color: #3e464c;
            font-size: 14px;
            text-transform: uppercase;
            line-height: 40px;
        }
        .primary-menu {
            padding: 0 15px;
        }
        .primary-menu>ul>li {
            border-bottom: 1px solid #e0e3e8;
            position: relative;
        }
        .primary-menu>ul>li.current-menu-item, .primary-menu>ul>li.current-menu-parent{
            border-bottom: none;
        }
        .toggle-menu {
            position: absolute;
            top: 0;
            right: 0;
            width: 40px;
            height: 40px;
            text-align: center;
            line-height: 40px;
        }
        .primary-menu>ul>li.current-menu-item>.toggle-menu, .primary-menu>ul>li.current-menu-parent>.toggle-menu {
            color: #ff7119;
        }
        .hidden-md-down {
            display: none!important;
        }
        .bottom-tel .tel.active{
            box-shadow: none;
        }
        .bottom-tel .single-phone {
            width: 310px;
            z-index: 9;
            left: -170px;
        }
        .bottom-tel .single-phone .left-block {
            padding-left: 15px;
        }
        .bottom-tel .single-phone .right-block {
            padding-right: 14px;
            max-width: 155px;
        }
        .bottom-tel .single-phone .title {
            font-size: 16px;
            padding: 5px 0 0 10px;
        }
        .bottom-tel .tel.active .icon-arrow:before{
            left: 125px;
            right: inherit;
        }
    }
