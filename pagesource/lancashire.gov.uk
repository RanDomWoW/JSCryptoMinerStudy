			$(function () {
				$('.globalAlert .close').click(function (e) {
					e.preventDefault();
					var alertID = $(this).closest(".alert").attr('id');
					var date = new Date();
					date.setDate(date.getDate() + 14);
					$.cookie(alertID, 'closed', { path: '/', expires: date });
					$(this).closest(".alert").hide();
				});
			});				
