				var esconder_smiles = '';
				var retirarOu = '';
				var cemPorCento = '';
				var programasTitle				=	'Programa de Pontos e Milhas';
			}
			//shopCount = 1;
			var pontosZerados = (pontosSmiles == 0 && pontosMultiplus == 0) ? 'sem_pontos_opacity_zero' : '';

			var styleSemMaxSaving = '';
			if (maxSaving < 1) { 
				styleSemMaxSaving = 'width: 100%; border-right: 0px';
			 }

			var aPartirDe = 'A partir de:';
			var dePor = 'De:';
			var hasAteStyle = '';
			
			if (shopCount < 2) {
				aPartirDe = 'Por:';
				dePor = 'Por:';
				hasAteStyle = 'display:none';
			}
			
			if (maxSaving > 0 && shopCount > 1 ) {
				classe= "col-lg-5 col-md-5 col-md-12 col-sm-12 text-left";
				classe2= "col-lg-7 col-md-7 col-md-12 col-sm-12 text-left";
			}else{
				classe="col-lg-12 col-md-12 col-md-12 col-sm-12 text-center";
			}
			if (shopCount == 1){
				var lblBtn = "Compre em 1 loja";
			}else{
				var lblBtn = "Compare em " + shopCount + " lojas"; 
			}
			// div produto
			var conteudo = 	''+
							'<div itemscope itemtype="https://schema.org/AggregateOffer" class="produtos-do-dia-home col-lg-4 col-md-4 col-sm-6 col-xs-12 bg-branco boxSeloBlackFriday">'+
							'	<meta itemprop="priceCurrency" content="BRL" />';
			if (priceDropStamp == true) {
				conteudo +=	'	<a itemprop="url" title="Gráfico de Preços" href="' + url + '#grafico" class="classeParaHover">'+
							'		<div class="selo_ja_baixou">'+
							'			<span class="center-block sprite-geral-home selo"></span>'+
							'			<div itemprop="description" class="conteudo_baixou">'+
							'				DESCONTO REAL: menor preço dos últimos meses.'+
							'				<span itemprop="url" class="btn btn-default">Confira o gráfico</span>'+
							'			</div>'+
							'		</div>'+
							'	</a>';
			}
				conteudo += '	<a itemprop="url" href="' + url + '" class="classeParaHover">'+
							'		<figure class="veraqui">'+
							'			<img itemprop="image"'+
						    ' 				alt="' + name + '"'+
						    ' 				data-original="' + imgUrl + '"'+
						    '				title="'+categoryName+" "+brandName+'"'+
						    ' 				class="img-responsive lazy" />'+
							'		</figure>'+
							'	</a>'+
							'	<h4 itemprop="name" class="descricao-produtos-do-dia-home bold-class font-18 text-center">'+
							'	<a itemprop="url" title="'+categoryName+" "+brandName+'" href="' + url + '" class="classeParaHover">' + name + '</a></h4>'+
							'	<a itemprop="url" href="' + url + '" class="classeParaHover">'+
							'		<div class=" ' + classe2 + ' padding-zero alturaParaVertical" style=" ' + styleSemMaxSaving + ' ">'+
							'			<span itemprop="lowPrice" class="palavra_De">'+dePor+'</span>'+
							'			<span class="preco_De">' + 'R$' + formataMoney(minPrice) + '</span>' + '</span>'+							
							'  			<meta itemprop="priceCurrency" content="BRL">'+
							'			<span class="bold-class font-18" style="' + hasAteStyle + '">'+
							'				<span itemprop="lowPrice" class="palavra_Ate">Até:</span>'+
							'				<span itemprop="lowPrice" class="preco_Ate">R$ ' + formataMoney(maxPrice) + '</span>'+
							//'				<br />em ' + shopCount + ' loja' + (shopCount > 1 ? "s" : "")+
							'			</span>'+
							'		</div>';
			if (maxSaving > 0 && shopCount > 1 ) {
				conteudo +=	'		<div class="' + classe + ' divisoria-esquerda">'+
							'			<div class="tableVerti">'+
							'				<div class="centralVerti">'+
							'					<span class="economiaDe laranja center-block text-center bold-class font-20">' + maxSaving + '%</span>'+
							'					<span class="fraseEconomia">de economia</span>'+
							'				</div>'+
							'			</div>'+
							'		</div>';
			}
							
				conteudo +=	'		<div class="clearfix"></div>'+
							'		<button itemprop="url" type="button" class="compare-lojas-home font-16 bold-class">' + lblBtn + ' </button>'+
							'	</a>'+
							'	<div class="jaCompreiJaGanhei jaBaix '+ pontosZerados + ' hidden-xs jacompreiVertical">'+
							'		<div class="containerProgramas">'+
							'			<a itemprop="url" href="/jacomprei-jaganhei" title="'+programasTitle+'"> '+
							'				<div class="clearfix"></div>'+
							'				<p class="container-fluid text-center">JáComprei JáGanhei</p>'+
