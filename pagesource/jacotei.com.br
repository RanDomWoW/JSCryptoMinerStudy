// pega os dados via ajax
function getRssGuia(url, qtd){
	$('#container_loading_dicas_guia').css('display','block');
	
	$.ajax({
        url: url,
        method: "GET",
        dataType: "xml",
		crossDomain: true,
		xhrFields: {withCredentials: true},
    })
    .done(function( response ){
		// ativa visualizacao do segundo item
    	$('.div-dica-1').show();

    	// imagens para celular (POG)
		var img_cel = ["https://img.ijacotei.com.br/guia/wp-content/uploads/2016/06/24121313/lg-g4Mini.png","https://img.ijacotei.com.br/guia/wp-content/uploads/2016/08/10091609/pokemonMini.png"];
    	
    	// pega o retorno xml e procura pela tag item
    	var x_loop = 0;
        var $XML = $(response);
        $XML.find("item").each(function() {
            if (x_loop >= qtd){
            	return false;
            }
            // gera uma variavel com os dados do post
            var $this = $(this),
                item = {
                    title:       $this.find("title").text(),
                    link:        $this.find("link").text(),
                    description: $($this.find("description").text()).text(),
                    pubDate:     $this.find("pubDate").text(),
                    author:      $('dc\\:creator, creator', this).text(),
                    post:		 $('content\\:encoded, content', this).text(),
                    image:		 'xxx'
                };

            if (item.description.length > 60){
            	item.description = item.description.substring(0,60)+'...';
            }

            // procura por uma imagem destacada
            $post = $(item.post);
            $post.find("img").each(function(){
               	var img = $(this);
               	
				if (item.image == 'xxx' && img.context.className.match(/destacada/g) == 'destacada'){
					item.image = img.context.src;
				}
            });

            // gambiarra para celular (POG)
            if ( url.match(/celular/g) == "celular" ) {
         	   item.image = img_cel[x_loop];
            }

            // se nao achou imagem destacada procura uma div destacada que tenha imagem
			if (item.image == 'xxx'){
				$post.find("div").each(function(){
					var divBlock = $(this);
					if (item.image == 'xxx' && divBlock.context.className.match(/destacada/g) == 'destacada'){
						$div = $(divBlock);
			           	$div.find("figure").find("a").find("img").each(function(){
			               	var img = $(this);
							if (item.image == 'xxx'){
								item.image = img.context.src;
							}
			            });
					}
				});
			}

            // se ainda assim nao achou coloca uma imagem default
			if (item.image == 'xxx'){
				item.image = 'https://img.ijacotei.com.br/imagens-jacotei-site/guia_padrao.png';
			}

            // coloca os dados no html
			$('.titulo-dica-'+x_loop).text(item.title);
			$('.desc-dica-'+x_loop).text(item.description);
			$('.link-dica-'+x_loop).attr('href', item.link);
			$('.img-dica-'+x_loop).attr('src', item.image);

            x_loop++;
        });

        // se nao tem segundo item
        if (x_loop == 1){
			$('.div-dica-1').hide();
        }

    })
    .error( function (jqXHR, textStatus, errorThrown) {
    	 console.log("erro box produtos jabaixou guia:" + textStatus );
    })
    .always(function() {
    	$('#container_loading_dicas_guia').css('display','none');
    });
}

function loadDocJabaixou (categoriaId, textoBusca, numPagina, prodPorPagina, ordenarPor, produtoId, retorno) {

	var divProdutosJabaixou = document.getElementById('produtos_jabaixou');
	divProdutosJabaixou.innerHTML = '<div.clear></div><div class="home-loading" style="background-color: inherit;"></div>';
	
	var xhttp = new XMLHttpRequest();
	xhttp.onreadystatechange = function(){
  		if (xhttp.readyState == 4 && xhttp.status == 200) {
  			var arrJson = JSON.parse(xhttp.responseText);
  			preparaDadosJabaixou(arrJson);
  		}
	};

	var url = '/elasticsearch/buscaProdutos?a=a';

	if (categoriaId != null && categoriaId != '' && categoriaId != undefined){
		url += '&categoriaId='+categoriaId;
	}
	if (textoBusca != null && textoBusca != '' && textoBusca != undefined){
		url += '&textoBusca='+textoBusca;
	}
	if (numPagina != null && numPagina != '' && numPagina != undefined){
		url += '&numPagina='+numPagina;
	}
	if (prodPorPagina != null && prodPorPagina != '' && prodPorPagina != undefined){
		url += '&prodPorPagina='+prodPorPagina;
	}
	if (ordenarPor != null && ordenarPor != '' && ordenarPor != undefined){
		url += '&ordenarPor='+ordenarPor;
	}
	if (produtoId != null && produtoId != '' && produtoId != undefined){
		url += '&produtoId='+produtoId;
	}
	if (retorno != null && retorno != '' && retorno != undefined){
		url += '&retorno='+retorno;
	}

	xhttp.open("GET", url, true);
	xhttp.send();
}

function preparaDadosJabaixou(response){
	var divProdutosJabaixou = document.getElementById('produtos_jabaixou');
	divProdutosJabaixou.innerHTML = '';

	var baseUrl = response.baseUrl;
	var totalHits = response.totalHits;

	var y = 1;
	if (totalHits > 0){
		var categoryId = response.resultSet[0].category.id;

		// div do sider
	    var divSlider = document.createElement('div');
	    divSlider.className = "";

	    var conteudo_total = '';
