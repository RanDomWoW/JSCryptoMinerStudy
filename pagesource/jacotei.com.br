function loadDocSlider(categoriaId, produtosOrdemId, textoBusca, numPagina, prodPorPagina, ordenarPor, produtoId, retorno) {
	var divProdutosQueInteressam = document.getElementById('produtos_que_interessam');
	divProdutosQueInteressam.innerHTML = '<div.clear></div><div class="home-loading"></div>';

	var xhttp = new XMLHttpRequest();
	xhttp.onreadystatechange = function(){
  		if (xhttp.readyState == 4 && xhttp.status == 200) {
  			var arrJson = JSON.parse(xhttp.responseText);
  			preparaDados(arrJson);
  		}
	};

	var url = '/elasticsearch/buscaProdutosSliderHome?a=a';

	if (categoriaId != null && categoriaId != '' && categoriaId != undefined){
		url += '&categoriaId='+categoriaId;
	}
	if (produtosOrdemId != null && produtosOrdemId != '' && produtosOrdemId != undefined){
		url += '&produtosOrdemId='+produtosOrdemId;
	}
	if (textoBusca != null && textoBusca != '' && textoBusca != undefined){
		url += '&textoBusca='+textoBusca;
	}
	if (numPagina != null && numPagina != '' && numPagina != undefined){
		url += '&numPagina='+numPagina;
	}
	if (prodPorPagina != null && prodPorPagina != '' && prodPorPagina != undefined){
		url += '&prodPorPagina='+prodPorPagina;
	}
	if (ordenarPor != null && ordenarPor != '' && ordenarPor != undefined){
		url += '&ordenarPor='+ordenarPor;
	}
	if (produtoId != null && produtoId != '' && produtoId != undefined){
		url += '&produtoId='+produtoId;
	}
	if (retorno != null && retorno != '' && retorno != undefined){
		url += '&retorno='+retorno;
	}

	xhttp.open("GET", url, true);
	xhttp.send();
}

$(document).ready(function(){
	var catid = $('#listaTabbb li').first().attr('data-catid');
	var prods = $('#listaTabbb li').first().attr('data-prods');
	loadDocSlider(catid, prods, null, 1, 16, 1, null, null);
	altera_aba();
});
</script>


						
