		var validaLogin = function() {
	        // se usuario possui cookie
	        if ($.cookie('user')) {
	            var user = $.parseJSON($.cookie('user'));
				var nomeLogin = user.name;
				var alteraNomeMobile = nomeLogin.substring(0,1);
				$('#inicial_logado').text(alteraNomeMobile);
	        }
	    }
	    validaLogin();

		function topo_ajustes(valorPadding){
			tamanho1 = $('#menu_topo').width();
			tamanho2 = $('#logo_topo').width();
			tamanho3 = $('#login_topo').width();
			tamanhoItens = tamanho1 + tamanho2 + tamanho3 + valorPadding;
			valorContainer = $('#topo_principal_site .container').width();

			totalParaBusca = valorContainer - tamanhoItens;
			$('#busca_topo').width(totalParaBusca);
		}
		topo_ajustes(10);
		$( window ).resize(function() {
			if ($(window).width() < 480) {
			   topo_ajustes(10);
			}
			else {
			   topo_ajustes(10);
			}
		});

		$('.box_busca').focus(function(){
		   $('#busca_topo').addClass('focusBusca');
		});

		$('.box_busca').focusout(function(){
		   $('#busca_topo').removeClass('focusBusca');
		});
		//Fixar menu mobile no rolagem e mostrar a busca quando o scroll for em direcao ao topo
		var scrollInicial = $(window).scrollTop();
		$(window).scroll(function(){
			var scrollSeguntaEtapa = $(window).scrollTop();

			if(scrollInicial < scrollSeguntaEtapa){//indo pra direcao do bottom
				// $("#container_busca_topo_fixar").removeClass('fixa_mobile_na_rolagem');
				$('#container_menu_topo').removeClass('modal_busca_rolagem');
				$('#menu_fixado,#menu_fixo_mobile').addClass('fixar_zero');
				scrollInicial = $(window).scrollTop();
			}else if(scrollInicial > scrollSeguntaEtapa){//indo pro topo da página
				// $("#container_busca_topo_fixar").addClass('fixa_mobile_na_rolagem');
				$('#container_menu_topo').addClass('modal_busca_rolagem');
				$('#menu_fixado,#menu_fixo_mobile').removeClass('fixar_zero');	
				scrollInicial = $(window).scrollTop();
					if($(window).scrollTop() == 0){
						// $("#container_busca_topo_fixar").removeClass('fixa_mobile_na_rolagem');
						$('#container_menu_topo').removeClass('modal_busca_rolagem');
						$('#menu_fixado,#menu_fixo_mobile').removeClass('fixar_zero');
					}
			}

		});


		if ($.cookie('user')) {
			 $('a.link_completa_cadastro').click(function (e) {
