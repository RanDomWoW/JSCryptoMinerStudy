				var esconder_smiles = 'centralizarPrograma';
				var retirarOu = 'retirarOu';
				var cemPorCento = 'cemPorCento';
				var programasTitle	=	'Programa de Milhas';
			} else if(location.host.indexOf('multiplus.') > -1){
				var esconder_multiplus = 'centralizarPrograma';
				var esconder_smiles = 'retirarLogoSmiles';
				var retirarOu = 'retirarOu';
				var cemPorCento = 'cemPorCento';
				var programasTitle	=	'Programa de Pontos';
			} else {
