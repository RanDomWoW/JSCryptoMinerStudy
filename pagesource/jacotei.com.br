	</div>
</div>

<script>
$(document).ready(function(){ 
	$("#btnNewsletterSection").click(function() {

		var codRecaptcha = grecaptcha.getResponse(newsrec2);
    	
        if(codRecaptcha.length == 0) {
            alert("Não deixe o código Captcha vazio");
            return false;
        }
        
	    var error = false;
	    if ($("#emailNewsletterSection").val() == "") {
	        $('.error').fadeIn();
	        error = true;
	    }

	    if (!error) {
	    	$('#loadingBoxNewsletter').show();

	    	var email = $("#emailNewsletterSection").val();
		    var res = email.split("@");
		    var nome =	res[0];

	        $.ajax({
	            url: "/newsletter/insereNewsletter/"+codRecaptcha+"/"+email+"/"+nome,
	            type: "GET",
	            success: function(retorno) {
	            	$('#loadingBoxNewsletter').hide();
	                retorno = $.parseJSON(retorno);

					if (retorno['status'] == 'ok'){
						$("#emailNewsletterSection").val('');
						grecaptcha.reset();
						$('.success').fadeIn();
						 
					} else if (retorno['status'] == 'erro') {
						$('.emailCad').fadeIn();
						
					} else if (retorno['status'] == 'invalido') {
						$('.novalid').fadeIn();
						
					} else if (retorno['status'] == 'recaptcha invalido') {
						$('.recaptchaErro').fadeIn();
					}
	            }
	        });
	    }

	});
});
</script>

						
						
