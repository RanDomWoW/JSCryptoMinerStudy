	<div id="openX" class="hidden-sm hidden-xs">
		<span id="closeOpenXBanner">Fechar X</span>
		<iframe id='ifrOpenx' name='ifrOpenx' width='265' height='265'
				src='https://ad.jacotei.com.br/www/delivery/afr.php?zoneid=208&amp;cb=34413872'>        	
		</iframe>
	</div>
	
	<script>
	$(document).ready(function(){
		
		// fecha banner no clique
		$('#closeOpenXBanner').click(function(){
			$('#openX').fadeOut();
		});
	
		// inicializa o objeto de data
	 	var objDate = new Date();
	 	var today 	= objDate.getTime();
	 	
		// pega ultima data salva
		var dateOpenXBannerDisplay = localStorage.getItem('dateOpenXBannerDisplay');
		if (dateOpenXBannerDisplay == null || dateOpenXBannerDisplay == "") {
			dateOpenXBannerDisplay	= objDate.getTime();
		}
		
		// salva data atual
		localStorage.setItem('dateOpenXBannerDisplay', objDate.getTime());
	
	 	// calcula os dias desde a ultima gravacao
	 	var days = Math.round( (today - dateOpenXBannerDisplay) / (1000 * 60 * 60 * 24) );
	
	
	 	// pega qtd ja visualizadas
		var qtdOpenXBannerDisplay = parseInt(localStorage.getItem('qtdOpenXBannerDisplay'));
		if (isNaN(qtdOpenXBannerDisplay)){
			qtdOpenXBannerDisplay = 0;
		}
		
		// incrementa + 1
		qtdOpenXBannerDisplay += 1;
	
		// mostra banner se ja foi visualizado ate X vezes inclusive
	 	if (qtdOpenXBannerDisplay <= 3){
			// abre banner
	        $('#openX').slideDown();
	
	     	// salva novo incremental
	     	localStorage.setItem('qtdOpenXBannerDisplay', qtdOpenXBannerDisplay);
	
	    // se visualizou o banner mais de 3 vezes so deve mostrar dnovo depois de 7 dias
	 	}else{
	 	 	// verifica qtd de dias passados
	 	 	if (days >= 1){
	 	 		// salva novo incremental
