		for (x = 0; x < response.resultSet.length ; x++) {

			var url 	  		= response.resultSet[x].url ? response.resultSet[x].url : response.resultSet[x].storeUrl ;
			var name	  		= response.resultSet[x].name;
			var imgUrl	  		= response.resultSet[x].imgUrl ? response.resultSet[x].imgUrl : baseUrl+response.resultSet[x].ImgSrcBig;
			var maxSaving 		= parseInt(response.resultSet[x].maxSaving);
			var minPrice  		= parseFloat(response.resultSet[x].minPrice);
			var maxPrice  		= parseFloat(response.resultSet[x].maxPrice);
			var shopCount 		= response.resultSet[x].shopCount;
			var pontosSmiles 	= 0;
			var pontosMultiplus = 0;
			var priceDropStamp  = response.resultSet[x].priceDropStamp;

			var data = response.resultSet[x].rewardProgram;

			var brandName		= response.resultSet[x].brand.name;
			var categoryName	= response.resultSet[x].category.name;

			for (y = 0; y < data.length ; y++) {
				if (data[y].rewardProgramName == 'Smiles'){
					pontosSmiles = data[y].rewardValue;
				}else if (data[y].rewardProgramName == 'Multiplus'){
					pontosMultiplus = data[y].rewardValue;
				}
			}

