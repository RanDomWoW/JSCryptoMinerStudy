          "TF2": '440',
          "H1Z1:JS": '295110',
          "H1Z1:KOTK": '433850'
        },

        recaptcha_key: "6LckwRwTAAAAABigDuqT-U7p2S_mLq5e2QglAOVt", //google recaptha api key

        // http://35.185.211.54 - Dev server
        remote_url: "", // for HTTP requests, if falsy - serving origin is hit  
        remote_checkhost: true,//use remote_url only if page accessed from one of the "remote_users_hosts" array?  

        // //35.185.211.54 - Dev server
        remote_socket_url: "", // for a webSocket connection, used ONLY if host is among "remote_users_hosts" array; Different socket urls are wired base on host in app.controller.js. 
         
        remote_users_hosts: ['localhost'], //used in remote checks above, if you need to - do NOT remove completely, leave as [] instead.
        society_hosts: ['society.gg'],//used for
