<script type="text/javascript">
  var myPlayer,
    playerHTML,
    playerData = {
      'accountId': '5105380679001',
      'playerId': 'default',
      'videoId': '5215700422001'
    };

  function addPlayer() {
    if (document.getElementById('livePlayerArea')){
      playerHTML = '<video title=\"LIVE配信中\" id=\"myPlayer\" data-video-id=\"' + playerData.videoId + '\"  data-account=\"' + playerData.accountId + '\" data-player=\"' + playerData.playerId + '\" data-embed=\"default\" class=\"video-js\" controls muted></video>';
      document.getElementById('livePlayerArea').innerHTML = playerHTML;
      bc(document.getElementById('myPlayer'));
      videojs('myPlayer').ready(function(){
        var startSec = 0;
        var pauseSec = 5;

        var myPlayer = this;
        myPlayer.controls(false);
        myPlayer.muted(true);

        myPlayer.one ("play", function () {
          startSec = myPlayer.currentTime();
        });

        myPlayer.on ("timeupdate", pauseVideo);
        myPlayer.play();

        function pauseVideo () {
          if (myPlayer.currentTime() - startSec > pauseSec) {
            myPlayer.pause();
            myPlayer.off ("timeupdate", pauseVideo);
            myPlayer.el_.style.pointerEvents = "none";
          }
        }
      });
    }
  }
</script>


<script>
$(document).ready(function(){
  nextReloadTime = getNextReloadTime();
  getTvJson();
  setInterval("checkReloadTime(); ", 30000 * 1); // 30秒毎にチェック
});
function getTvJson(){
  $.getJSON("https://site777.tv/prm/iptv/epgdailyInternal",{},data);
  function data(data)
  {
    if(!data || data.result != 'ok' && !data.result)
    {
      $("#tvWrap").hide();
      return;
    }
    else
    {
      $("#tvList").empty();
      $.each(data.lists,
        function(index, tv) {
          tv.index = index;
          tv.nextTime = tv.start.substr(11,5) + '～' + tv.end.substr(11,5);
        }
      );
      $("#tvWrap").show();
      $('#tvParts').tmpl(data.lists).hide().appendTo("#tvList").fadeIn(100);

      addPlayer();
    }
  }
}

function getNextReloadTime(){
  var now = new Date();
  if(now.getMinutes() < 30)
  {
    now.setMinutes(30);
    now.setSeconds(00);
  }
  else
  {
    now.setHours(now.getHours() + 1);
    now.setMinutes(00);
    now.setSeconds(00);
  }

  return now;
}

function checkReloadTime()
{
  var now = new Date();
  if(nextReloadTime.getTime() < now.getTime()){
    nextReloadTime = getNextReloadTime();
    getTvJson();
  }
}
</script>


