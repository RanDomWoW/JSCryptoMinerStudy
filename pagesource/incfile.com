<script>
    $(document).ready(function () {
        var zFullUrl = window.location.href;
        $('#z_from_url').val(zFullUrl);
        $('#z_phone').mask('(000) 000-0000');
        $('#z_ordernum').mask('00000000000');

        $('[data-toggle="tooltip"]').tooltip({
            container: "body",
            html: true,
            trigger: 'hover'
        });

        $('#z_inquiry').change(function () {
            if ($(this).val() == 2) {
                $('#z_order_comp').removeClass('hide');
            } else {
                $('#z_order_comp').addClass('hide');
                $('#z_ordernum,#z_comp_name').val('');
            }
        });
    });

    function zvalidEmail(email) {
        var reg = new RegExp(/^([\w\.\-]+)@([\w\-]+)((\.(\w)+)+)$/i);
        return reg.test(email);
    }

    function zMessage() {
        var zName = $('#z_username'),
            zEmail = $('#z_email'),
            zPhone = $('#z_phone'),
            zMess = $('#z_message'),
            come = "homepage";
        var regPhone = new RegExp(/^\(\d{3}\)(-|\s)?\d{3}(-|\s)?\d{4}$/);


        var error = false;

        $('#z___form input, #z___form select, #z___form textarea').each(function () {
            var $this = $(this).attr('id');
            var field = $('#' + $this);

            if($this === 'z_fname' && $.trim(field.val()) == '') {
                field.focus();
                field.addClass('error');
                error = true;
            }

            if($this === 'z_lname' && $.trim(field.val()) == '') {
                field.focus();
                field.addClass('error');
                error = true;
            }

            if($this === 'z_email' && ($.trim(field.val()) == '' || !zvalidEmail(field.val()))) {
                field.focus();
                field.addClass('error');
                error = true;
            }
            if($this === 'z_phone' && ($.trim(field.val()) == '' || !regPhone.test(field.val()))) {
                field.focus();
                field.addClass('error');
                error = true;
            }
            if($this === 'z_inquiry') {
                if($.trim(field.val()) == '') {
                    field.focus();
                    field.addClass('error');
                    error = true;
                } else if ($.trim(field.val()) == 2) {
                    if ($('#z_ordernum').val() == '' && $('#z_comp_name').val() == '') {
                        $('#z_ordernum').focus();
                        $('#z_ordernum').addClass('error');
                        error = true;
                    }
                }
            }

            if($this === 'z_message' && $.trim(field.val()) == '') {
                field.focus();
                field.addClass('error');
                error = true;
            }
        });

        if(!error) {
            // disable the button
            $('p.submit-message a').attr('disabled', true);

            $.ajax({
                type: "POST",
                dataType: 'json',
                data: {
                    'zFName': $('#z_fname').val(),
                    'zLName': $('#z_lname').val(),
                    'zEmail': zEmail.val(),
                    'zPhone': zPhone.val(),
                    'z_inquiry': $('#z_inquiry').val(),
                    'z_ordernum': $('#z_ordernum').val(),
                    'z_comp_name': $('#z_comp_name').val(),
                    'zMessage': zMess.val(),
                    'fromUrl': $('#z_from_url').val(),
                    'come': come
                },
                url: '/zMessage',
                success: function (data) {
                    if (data.status) {
                        $('#widget-message .main-form-message #send_mess_ok').show();
                        $('#z_fname, #z_lname').val('');
                        zEmail.val('');
                        zPhone.val('');
                        zMess.val('');
                        $('#z_inquiry').val('');
                        $('#z_ordernum').val('');
                        $('#z_comp_name').val('');
                        $('#widget-message .main-form-message #z___form').hide();
                    } else {
                        alert(data.msg);
                    }

                    // re-enable the button
                    $('p.submit-message a').attr('disabled', false);
                }
            });
        }
    }
</script>
