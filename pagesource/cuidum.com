            $('#nombre_cuidadora_click_to_call').html($('#nombre_click_to_call').val());

            var url = window.location.protocol + "//" + window.location.host + "/clicktocallcuidadora";
            var nombre_click_to_call = $('#nombre_click_to_call').val();
            var telefono_click_to_call = $('#telefono_click_to_call').val();

            $.ajax({
                type: "POST",
                url: url,
                cache: false,
                dataType: "json",
                data: {
                    "nombre_click_to_call": nombre_click_to_call,
                    "telefono_click_to_call": telefono_click_to_call
                },
                success: function (result, success) {
                    console.log(result);
                }
            });
        });

        $('#formulario-click-to-call').submit(function (event) {
            $('#presentacion').hide();
            $('#tipo-partner').show();
            event.preventDefault();
        });

        $('#btn_cerrar_modal-call').click(function (event) {
            $('#formulario-familia').hide();
            $('#formulario-familia-div').hide();
            $('#tipo-partner').hide();
            $('#mensaje-cuidadora').hide();
            $('#gracias-familia').hide();
            $('#llamada-familia').hide();
            $('#presentacion').show();
        });

        var myVar = setInterval(myTimer, 3000);//5-sg
        var contador = 0;
        function myTimer() {
            if (contador == 0) {
                $('.btn-c2c').css('background-image', 'url(/landing-nueva/images/c2c-call-01.svg)');
                contador = 1;
            }else{
                $('.btn-c2c').css('background-image', 'url(/landing-nueva/images/c2c-call-02.svg)');
                contador = 0;
            }
        }
    });

    function verificarClickToCall() {
        $('#tipo-partner').hide();
        $('#llamada-familia').show();

        var url_registro = window.location.pathname.replace('/','');
        var url = window.location.protocol + "//" + window.location.host + "/clicktocallfamilia";

        var cookieMarketing_click_call = $('#cookieMarketing_click_call').val();
        var dispositivo_click_call = $('#dispositivo_click_call').val();
        var nombre_click_to_call = $('#nombre_click_to_call').val();
        var telefono_click_to_call = $('#telefono_click_to_call').val();

        $.ajax({
            type: "POST",
            url: url,
            cache: false,
            dataType: "json",
            data: {
                "cookieMarketing_click_call": cookieMarketing_click_call,
                "dispositivo_click_call": dispositivo_click_call,
                "nombre_click_to_call": nombre_click_to_call,
                "telefono_click_to_call": telefono_click_to_call,
                "url": url_registro
            },
            success: function (result, success) {
                console.log(result);
                $('#gracias-familia').show();
                $('#llamada-familia').hide();
                $('#mensaje-click-call').html(result.mensaje);
                if (result.iniciativa != 0){
                    /*ga('send', 'event', 'click2call', 'call');*/
                    registrarConversionCall();
                    console.log("Registra conversion click to call");
                }
            },
            error: function (xhr, status) {
                alert('Estamos teniendo problemas en estos momento puede volver a intentarlo nuevamente');
                $('#tipo-partner').show();
                $('#llamada-familia').hide();
            }
        });
    }

    function registrarConversionCall(){
        var conversion = readCookieCall('lead');
        if (conversion == null || conversion == undefined) { //Nueva iniciativa
            setCookieCall('lead', '1', 30);
            console.log("Registrar lead analytics");
            gtag('event','formulario',{'event_category': 'formulario','event_label': 'formulario'});
            console.log("Registrar lead adwords");
            goog_report_conversion();
            console.log("Registrar lead bing");
            window.uetq = window.uetq || [];
            window.uetq.push({ 'ec':'formulario', 'ea':'formulario', 'el':'formulario'});
        }else{
            console.log("Se encuentra la cookie de conversion");

        }
    }

    /**
     * Guardar en cookie cualquier informacion de tipo text
     * @param  name
     * @param  value
     * @param  days
     */
    function setCookieCall(name, value, days) {
        var date = new Date();
        date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
        var expires = "; expires=" + date.toGMTString();
        document.cookie = name + "=" + value + expires + ";path=/";
    }

    /**
     * Leer la cookie para cualquier cookie
     * @param  name
     * @returns  {*}
     */
    function readCookieCall(name) {
        var n = name + "=";
        var cookie = document.cookie.split(';');
        for (var i = 0; i < cookie.length; i++) {
            var c = cookie[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1, c.length);
            }
            if (c.indexOf(n) == 0) {
                return c.substring(n.length, c.length);
            }
        }
        return null;
    }

</script>

