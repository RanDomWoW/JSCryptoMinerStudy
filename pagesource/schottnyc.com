		<script>
			$(function() {
				var height = 0,
					width = 0,
					customEventFunctionName = '',
					cookieName = 'newsletter_onEntry'.toUpperCase(),
					seenCookieName = 'newsletter_onEntry_SEEN'.toUpperCase(),
					content = $('.' + cookieName.toLowerCase() + '_content').html(),
					triggered = 0,
					horizontal = 'bottom',
					vertical = 'left';

					Popup = function(width, height, cookieName, content, trigerred, seenCookieName, horizontal, vertical) {
					    "use strict";

					    var prepare = function() {
					    	var autoSize = true;
					    	if(width && height) {
					    		autoSize = false;
					    	}
							width = 'auto';

					    	$('.' + cookieName.toLowerCase() + '_content').remove();

					        $.fancybox({
					        	autoSize: autoSize,
								height: height,
								width: width,
								content: content,
								closeClick: false,
								wrapCSS: 'auto-popups ' + cookieName,
								helpers: {
									overlay: {
										locked: false,
										closeClick: false
									}
								},
								afterClose: function() {
									popupVisible = false;
								},
								beforeShow: function() {
									$('.fancybox-wrap.auto-popups').addClass(vertical).addClass(horizontal).addClass('loading');
									$('.fancybox-overlay').remove();
									popupVisible = true;
								}
							});

							// create session cookie for manually triggered popup to prevent incrementing 'showPopupCount' more than once during single session
							if(triggered) {
		                     	$.cookie(seenCookieName, 1, { path: '/' });
							}
	                     	$.cookie(cookieName, parseInt($.cookie(cookieName), 10) + 1, { expires: 99999, path: '/' });
					    };

					    var show = function(funcName) {
					        if (typeof window[funcName] === 'function') {
					            window[funcName](prepare);
					        } else if (typeof funcName !== 'undefined' && funcName.length > 0) {
					            console.error('Unable to find "' + funcName + '" function in global (window) scope!');
					        } else if(!popupVisible) {
					            prepare();
					        }
					    };

					    return {
					        show: show
					    };
					};

				var popup = new Popup(width, height, cookieName, content, triggered, seenCookieName, horizontal, vertical);
				popup.show(customEventFunctionName);

			});
		</script>
	<script type="text/javascript">
	function toggleSubTree(ddID) {

		//this UL
		var thisSubMenu = $('#'+ddID);

		// parent LI
		var thisParentLI = thisSubMenu.parent();

		//show UL
		if(thisParentLI.hasClass('expanded')) {
			thisSubMenu.slideUp();
			thisParentLI.removeClass('expanded');
		} else {
			thisSubMenu.slideDown();
			thisParentLI.addClass('expanded');
		}

		// parent UL
		var thisParentMenu = thisParentLI.closest('ul');

		//has parent UL
		if(thisParentMenu.length === 1) {
			//hide UL elements on the same level
			thisParentMenu.find('> li > ul').not(thisSubMenu).slideUp();
			thisParentMenu.find('> li').not(thisParentLI).removeClass('expanded');
		}

		return false;
	};
</script>

