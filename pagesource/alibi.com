<script>
function mailing_list_STFU() {
	// set long-lasting cookie
	document.cookie="newsletter_nag_reprieve=1; expires=Thu, 13 Sep 2018 00:00:00 UTC; path=/";
}
function mailing_list_subscribe() {
	// send ajax request instead of doing another pageload

	var f=document.forms["subscribe_form"].elements;
	var email=f["email"].value;
	if (email.length<1) {
		alert("Enter your email address");
		f["email"].focus();
		// I can't figure out if houstonpress does any serious validation here
	} else {
		p="email="+encodeURIComponent(email);
		if (f["list_36dea493ba"]) {
			if (f["list_36dea493ba"].checked) {
				p += "&list_36dea493ba=1";
			}
		}

		if (f["list_ea85af1eba"]) {
			if (f["list_ea85af1eba"].checked) {
				p += "&list_ea85af1eba=1";
			}
		}

		//alert(p);

		new Ajax.Request("/index.php?ajax_function=newsletter_subscribe", {
			method: "post",
			parameters: p,
			evalJSON: true,
			onSuccess: function(transport) {
				mailing_list_STFU(); // set long-term cookie so we stop bothering them
			},
			onFailure: function(transport) {
				alert(transport.responseJSON.status);
			}
		});


		Modalbox.hide();
	}
	return false;
}
function mailing_list_later() {
	Modalbox.hide();
	return false;
}
function mailing_list_decline() {
	mailing_list_STFU();
	Modalbox.hide();
	return false;
}
getCookie = function(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(";");
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==" ") {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length,c.length);
        }
    }
    return "";
}
function mailing_list_showad(voluntary) {
	var optional_buttons;
	if (optional_buttons=$("mailing_list_popup_involuntary")) {
		if (voluntary)
			optional_buttons.style.display="none";
		else
			optional_buttons.style.display="block";
	}
	document.cookie="newsletter_nag_reprieve=1; expires=Sun, 1 Apr 2018 00:00:00 UTC; path=/";
	Modalbox.show($("mailing_list_popup"),{title:"Alibi Newsletters", width: 320, autoFocusing: false, transitions: !voluntary});
}
function mailing_list_maybe_showad() {
	// show the ad if we think their browser stores cookies.  if they don't take cookies, then don't do it.
	document.cookie="cookie_test=YES; expires=Tue, 14 Mar 2028 00:00:00 UTC; path=/";
	if (getCookie("cookie_test")=="YES" && getCookie("newsletter_nag_reprieve")=="") {
		mailing_list_showad(false);
	}
}
