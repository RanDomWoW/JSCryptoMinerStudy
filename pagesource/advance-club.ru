      <script>

        var activeLink = document.getElementsByClassName('js-headLink'),
            headToggle = document.querySelector('.head-toggle'),
            headNav = document.querySelector('.head-nav');

          for(var i = 0; i < activeLink.length; i++) {
            activeLink[i].addEventListener('click', function(e) {
              e.preventDefault();
            }, false)
          }

        function getMenuClick() {
          for(var i = 0; i < activeLink.length; i++) {
            activeLink[i].addEventListener('click', function(e) {
              e.preventDefault();
              var el = e.target;
              el.parentNode.previousElementSibling.classList.remove('active');
              el.parentNode.nextElementSibling.classList.remove('active');
              el.parentNode.classList.toggle('active');
              var listener = function(e) {
                if(e.target !== el) {
                  el.parentNode.classList.remove('active');
                  document.removeEventListener('click', listener, false);
                }
                return;
              }
              document.addEventListener('click', listener, false);
            }, false);
          }
        }

        if(this.screen.width < 1200) {
          getMenuClick();
        }

        headToggle.addEventListener('click', function() {
          this.classList.toggle('head-toggle--open');
          headNav.classList.toggle('head-nav--open');
        }, false);
      </script>
