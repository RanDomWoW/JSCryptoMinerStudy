<script type="text/javascript">
    //Баннер предзаказа
    $(document).ready(function() {
        var dlg_preorder = $('#preorder-banner');
        
        $('[data-preorder-id]').on('click', function (e) {
            e.preventDefault();
            $('[name="model"]', dlg_preorder).val($(this).data('preorder-id'));
            $.magnificPopup.open({
                items: {
                    src:'#preorder-banner'
                },
                type:'inline',
                fixedContentPos: false,
                removalDelay: 200,
                showCloseBtn: false,
                mainClass: 'mfp-fade'
            });
            return false;
        });

        dlg_preorder.on('click', '.close', function (e) {
            e.preventDefault();
            e.stopPropagation();
            $.magnificPopup.close();
        });
        
        dlg_preorder.on('click', function (e) {
            e.stopPropagation();
        });

        $('[name="city"]', dlg_preorder).change(function() {
            var cityId = $(this).val();
            $.post("/dealers", "&act=get_dealers&city_id=" + cityId, function(data) {
                var dataResponse = JSON.parse(data);
                var $dealerSelect = $('[name="dealer"]', dlg_preorder).html('');

                if (!dataResponse.length) {
                    return false;
                }

                for (var i=0; i<dataResponse.length; i++) {
                    $dealerSelect.append('<option value="' + dataResponse[i].id + '">' + dataResponse[i].name + '</option>');
                }

                $dealerSelect.find('OPTION').eq(0).prop("selected");
                $dealerSelect.trigger('refresh');
            });
        });

        $('[name="preorder-submit"]', dlg_preorder).click(function(event) {
            event.preventDefault();

            var $button = $(this);
            if ($button.hasClass('submited')) {
                return false;
            }

            var $preorderForm = $('[name="preorder-form"]' , dlg_preorder);
            var error = "";

            $preorderForm.find('INPUT[type="text"], SELECT').each(function() {
                if (!$(this).val()) {
                    error = "Для того чтобы оставить заявку, нужно заполнить все поля формы";
                }
            });

            if (error) {
                alert(error);
                return;
            }

            $button.addClass('submited').attr('title', "Ваши регистрационные данные отправляются на сервер.\nПожалуйста, подождите...");

            $.post("/ajax/send_preorder_form.php", $preorderForm.serialize(), function(data) {
                if (data == 1) {
                    $button.removeClass('submited').removeAttr('title');
                    alert('Форма отправлена\nСпасибо! С\u00A0вами в\u00A0ближайшее время свяжется менеджер');
                    // Yandex metrika hash
                    //setTimeout("document.location.href='/?sent'", 5000);
                }
            });
        });
    });
</script>

