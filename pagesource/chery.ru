<script type="text/javascript">
    $(document).ready(function() {
        var favs = JSON.parse(readCookie('loc_favs'));
        if (favs == null)
            favs = [];
        var fav_cnt = $('#cntFav');
        fav_cnt.text(favs.length);
        if (favs.length > 0)
            fav_cnt.parent().addClass('active');
        else
            fav_cnt.parent().removeClass('active');
    });

    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1, c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
        }
        return null;
    }
</script>

<div class="favours">
    <a href="/shop_favourites">
        <span id="cntFav"></span>
    </a>
</div></body>
</html>