    <script type="text/javascript">



        function RemoveAllItems(sender, eventArgs) {

            var s = sender.get_id();

            var directionCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_comboBoxDirection");
            var boardCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_comboBoxBoard");
            var alightCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_comboBoxAlight");




            if (s.indexOf("Route") != -1 || s.indexOf("Direction") != -1 || s.indexOf("Board") != -1) {
                if (alightCombo != null) {
                    alightCombo.clearSelection();
                    alightCombo.get_items().clear();
                    document.getElementById('ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_lnkAlight').style.display = "none";
                }
            }

            if (s.indexOf("Route") != -1 || s.indexOf("Direction") != -1) {
                if (boardCombo != null) {
                    boardCombo.clearSelection();
                    boardCombo.get_items().clear();
                    document.getElementById('ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_lnkBoard').style.display = "none";
                }
            }

            if (s.indexOf("Route") != -1) {
                if (directionCombo != null) {
                    directionCombo.clearSelection();
                    directionCombo.get_items().clear();

                }

                var item = eventArgs.get_item();
                if (item != null) {
                    IsStagedRoute(item.get_attributes().getAttribute("staged"));
                }
            }
        }

        function IsStagedRoute(isStaged) {

            var directionCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_comboBoxDirection");
            var boardCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_comboBoxBoard");
            var alightCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_comboBoxAlight");

            directionCombo.set_enabled(isStaged);
            boardCombo.set_enabled(isStaged);
            alightCombo.set_enabled(isStaged);

            ChangeColor(directionCombo, isStaged);
            ChangeColor(boardCombo, isStaged);
            ChangeColor(alightCombo, isStaged);
        }

        function ChangeColor(sender, enabled) {

            var inputElement = sender.get_inputDomElement();

            if (enabled == true) {
                inputElement.style.color = "black";
                sender.set_text('');
                document.getElementById("flatfare").style.visibility = 'hidden';
            }
            else {
                inputElement.style.color = "#C0C0C0";
                sender.set_text('Not Required');
                document.getElementById("flatfare").style.visibility = 'visible';
            }
        }

        function OnClientItemsRequesting_DubBus_Route(sender, eventArgs) {

            var context = eventArgs.get_context();
            context["Filter"] = sender.get_text();
            context["MinStringLength"] = 2;
            context["NumberOfItems"] = -999;
        }

        function OnClientItemsRequesting_DubBus_Direction(sender, eventArgs) {

            var context = eventArgs.get_context();
            context["Filter"] = '';
            context["MinStringLength"] = 2;
            context["Text"] = '';

            var routeCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_comboBoxRoute");
            var selectedValue = '';
            if (routeCombo.get_items().get_count() > 0) {

                var selectedItem = routeCombo.get_selectedItem();
                if (selectedItem != null) {
                    selectedValue = selectedItem.get_value();

                    context["Filter"] = selectedValue;
                    context["MinStringLength"] = 2;
                    context["Text"] = selectedValue;
                }
            }
        }

        function OnClientItemsRequesting_DubBus_Board(sender, eventArgs) {

            var routeCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_comboBoxRoute");
            var directionCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_comboBoxDirection");



            var context = eventArgs.get_context();
            context["Direction"] = '';
            context["MinStringLength"] = 2;
            context["Text"] = '';

            var selectedValue = '';
            var selectedDir = '';

            if (routeCombo.get_items().get_count() > 0 && directionCombo.get_items().get_count() > 0) {

                var selectedItem = routeCombo.get_selectedItem();
                var dirSelectedItem = directionCombo.get_selectedItem();

                if (selectedItem != null && dirSelectedItem != null) {
                    selectedValue = selectedItem.get_value();
                    selectedDir = dirSelectedItem.get_value();

                    context["Direction"] = selectedDir;
                    context["MinStringLength"] = 2;
                    context["Text"] = selectedValue;
                    document.getElementById('ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_lnkBoard').style.display = "inline";
                }
            }
        }

        function OnClientItemsRequesting_DubBus_Alight(sender, eventArgs) {

            var context = eventArgs.get_context();
            context["Direction"] = '';
            context["MinStringLength"] = 2;
            context["Text"] = '';
            context["SeqNum"] = 0;

            var routeCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_comboBoxRoute");
            var directionCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_comboBoxDirection");
            var boardCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_comboBoxBoard");

            var selectedRoute = '';
            var selectedDir = '';
            var selectedSeq = '';

            if (routeCombo.get_items().get_count() > 0 &&
                    directionCombo.get_items().get_count() > 0) {

                var selectedItem = routeCombo.get_selectedItem();
                var dirSelectedItem = directionCombo.get_selectedItem();
                var boardSelectedItem = boardCombo.get_selectedItem();

                if (selectedItem != null &&
                        dirSelectedItem != null &&
                            boardSelectedItem != null) {

                    selectedValue = selectedItem.get_value();
                    selectedDir = dirSelectedItem.get_value();
                    selectedSeq = boardSelectedItem.get_value();

                    context["Direction"] = selectedDir;
                    context["MinStringLength"] = 2;
                    context["Text"] = selectedValue;
                    context["SeqNum"] = selectedSeq;
                    document.getElementById('ctl00_FullRegion_MainRegion_HomePageTabs_pnlFares_uscFareCalc_lnkAlight').style.display = "inline";
                }
            }
        }

    </script>

