        <script type="text/javascript">
            $(document).ready(function() {
                $("#fromHelpdiv").addClass("display_none");
                $("#toHelpdiv").addClass("display_none");
                $(".route_planner_info-icon span").hover(function() {
                    $(this).next("em").stop(true, true).animate({ opacity: "show", top: "-116" }, "slow");
                }, function() {
                    $(this).next("em").animate({ opacity: "hide", top: "-116" }, "fast");
                });
            });

            function OnDropDownOpenedFrom_Default(sender, eventArgs) {
                //console.log('OnDropDownOpenedFrom_Default');
                $("#fromHelpdiv").addClass("display_none");
            }

            function OnDropDownClosedFrom_Default(sender, eventArgs) {
                //console.log('OnDropDownClosedFrom_Default');
                var s = sender.get_text().trim();
                if (s != '') {
                    //console.log('s is not an empty string');
                    var radCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlRoutePlanner_RadComboBoxFrom");
                    var item = radCombo.findItemByText(s);
                    var radToolTip = $("#fromHelpSpan");
                    var selectedItemTooltip = item != null ? item.get_attributes().getAttribute("ToolTip") : s;
                    if (selectedItemTooltip.indexOf("Routes serving this stop:") != -1) {
                        //Generate the tooltip content
                        //console.log('Generate the tooltip content');
                        var split = selectedItemTooltip.indexOf(":") + 1;
                        var subText = "Click ? To View Stop on Map";
                        radToolTip.html("<p>" + selectedItemTooltip.substring(split, selectedItemTooltip.length) + "<br/><br/>" + subText + "</p>");
                        //Generate the tooltip link
                        var hfStopMapUrl = document.getElementById("ctl00_FullRegion_MainRegion_hfStopMapUrl");
                        var stopmapurl = "";
                        if (hfStopMapUrl != null) {
                            //console.log('hfStopMapUrl is not null');
                            var radComboTo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlRoutePlanner_RadComboBoxto");
                            var toValue = "";
                            if (radComboTo != null) {
                                toValue = radComboTo.get_value().trim();
                            }
                            if (toValue.length > 0)
                                stopmapurl = hfStopMapUrl.value + "&stopnumber=" + item.get_value() + "&toBoxOrNot=false&toBoxValue=" + toValue;
                            else
                                stopmapurl = hfStopMapUrl.value + "&stopnumber=" + item.get_value() + "&toBoxOrNot=false";
                        }
                        //console.log(stopmapurl);
                        $("a[name='fromHelpAnchor']").attr("href", stopmapurl);

                        $("#fromHelpdiv").removeClass("display_none");

                        //update session data
                        //console.log("From stop number: " + item.get_value());
                        //console.log("From stop text: " + s);
                        var seshData = [["RoutePlannerFromValue", item.get_value()], ["RoutePlannerFromText", decodeURI(s.replace("%27", "'"))]];
                        PageMethods.SetSessionValues("fromVars", seshData, OnSetSessionFromVarsComplete);
                    }
                }
            }

            function OnSetSessionFromVarsComplete(result, userContext, methodName) {
                //alert(result);
                //document.location.href = result;
                if (result == 'successful') {
                    //show popup
                    $("#fromHelpdiv").next("em").stop(true, true).animate({ opacity: "show", top: "-116" }, "slow");

                    //hide popup after 3 seconds
                    setTimeout(function() { $("#fromHelpdiv").next("em").animate({ opacity: "hide", top: "-116" }, "fast") }, 3000);                    
                }
            }

            function OnDropDownOpenedTo_Default(sender, eventArgs) {
                $("#toHelpdiv").addClass("display_none");
            }

            function OnDropDownClosedTo_Default(sender, eventArgs) {
                var s = sender.get_text().trim();
                if (s != '') {
                    var radCombo = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlRoutePlanner_RadComboBoxto");
                    var item = radCombo.findItemByText(s);
                    var radToolTip = $("#toHelpSpan");
                    var selectedItemTooltip = item != null ? item.get_attributes().getAttribute("ToolTip") : s;
                    if (selectedItemTooltip.indexOf("Routes serving this stop:") != -1) {
                        //Generate the tooltip content
                        var split = selectedItemTooltip.indexOf(":") + 1;
                        var subText = "Click ? To View Stop on Map";
                        radToolTip.html("<p>" + selectedItemTooltip.substring(split, selectedItemTooltip.length) + "<br/><br/>" + subText + "</p>");
                        //Generate the tooltip link
                        var hfStopMapUrl = document.getElementById("ctl00_FullRegion_MainRegion_hfStopMapUrl");
                        var stopmapurl = "";
                        if (hfStopMapUrl != null) {
                            var radComboFrom = $find("ctl00_FullRegion_MainRegion_HomePageTabs_pnlRoutePlanner_RadComboBoxFrom");
                            var fromValue = "";
                            if (radComboFrom != null) {
                                fromValue = radComboFrom.get_value().trim();
                            }
                            if (fromValue.length > 0)
                                stopmapurl = hfStopMapUrl.value + "&stopnumber=" + item.get_value() + "&toBoxOrNot=true&fromBoxValue=" + fromValue;
                            else
                                stopmapurl = hfStopMapUrl.value + "&stopnumber=" + item.get_value() + "&toBoxOrNot=true";
                        }
                        $("a[name='toHelpAnchor']").attr("href", stopmapurl);

                        $("#toHelpdiv").removeClass("display_none");

                        //update session data
                        //console.log("From stop number: " + item.get_value());
                        //console.log("From stop text: " + s);
                        var seshData = [["RoutePlannerToValue", item.get_value()], ["RoutePlannerToText", decodeURI(s.replace("%27", "'"))]];
                        PageMethods.SetSessionValues("toVars", seshData, OnSetSessionToVarsComplete);
                    }
                }
            }

            function OnSetSessionToVarsComplete(result, userContext, methodName) {
                //alert(result);
                //document.location.href = result;
                if (result == 'successful') {
                    //show popup
                    $("#toHelpdiv").next("em").stop(true, true).animate({ opacity: "show", top: "-116" }, "slow");

                    //hide popup after 3 seconds
                    setTimeout(function() { $("#toHelpdiv").next("em").animate({ opacity: "hide", top: "-116" }, "fast") }, 3000);
                }
            }            

