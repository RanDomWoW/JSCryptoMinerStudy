            function OnClientItemsRequesting_Default(sender, eventArgs) {
                //console.log('OnClientItemsRequesting_Default');
                var context = eventArgs.get_context();
                context["Filter"] = sender.get_text();
                context["MinStringLength"] = 0;
                var btn = $("#ctl00_FullRegion_MainRegion_HomePageTabs_pnlRoutePlanner_btnPlanJourney");
                if (btn != null)
                    btn.hide();
            }

            function OnClientItemsRequested_Default(sender, eventArgs) {
                //console.log('OnClientItemsRequested_Default');
