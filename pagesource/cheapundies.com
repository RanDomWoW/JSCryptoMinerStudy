   $( document ).ready(function() {
    if ($('#CartCount').text() == 0 ){
      $('.cart-empty').show();
      $('.sticky').hide();
    }
    else{
    	$('.cart-empty').hide();
      $('.sticky').show();
    }
     $('.mx-auto .comman_class').click(function(){
     	
       var quantity_total = $(this).find('.soldout-label').text();
       var qty_tt = quantity_total.split(" ");
       if(qty_tt[0] != "Sold")
       {
         var qqq = qty_tt[0];
         localStorage.setItem('ravi_max', qqq);
       }
     });
  });
   // input type + - js
  jQuery('<div class="quantity-nav"><div class="quantity-button quantity-up">+</div><div class="quantity-button quantity-down">-</div></div>').insertAfter('.quantity input');
    jQuery('.quantity').each(function() {
      var spinner = jQuery(this),
        input = spinner.find('input[type="number"]'),
        btnUp = spinner.find('.quantity-up'),
        btnDown = spinner.find('.quantity-down'),
        min = input.attr('min'),
        max = input.attr('max');

      btnUp.click(function() {
        var oldValue = parseFloat(input.val());
        if (oldValue >= max) {
          var newVal = oldValue;
        } else {
          var newVal = oldValue + 1;
        }
        spinner.find("input").val(newVal);
        spinner.find("input").trigger("change");
      });

      btnDown.click(function() {
        var oldValue = parseFloat(input.val());
        if (oldValue <= min) {
          var newVal = oldValue;
        } else {
          var newVal = oldValue - 1;
        }
        spinner.find("input").val(newVal);
        spinner.find("input").trigger("change");
      });

    });
  
 function updateCart(itemVariantId, lineItemIndex) {
   // this gets the new quantity value from the .quantity-selector_[itemVariantId] class
   var newQuantity = document.getElementsByClassName('quantity-selector_' + itemVariantId)[0].value;
   // if the quantity is 0, just go to that link.. this is the same as clicking on the "Remove" link
   if (newQuantity == 0) {
     $.ajax({
       method: 'POST',
       url: "/cart/change.js",
       data: {
         quantity: newQuantity,
         id: itemVariantId
       },
       dataType: 'json'
     }).then(function(data) {
       // first update the current product's new price
       
        $('#CartCount').text(data.item_count);
       $('.cart-total-price').text(Shopify.formatMoney(data.total_price));
       
       $('#'+itemVariantId).remove();
       
       if ($('#CartCount').text() == 0 ){
         $('.cart-empty').show();
         $('.sticky').hide();
       }
       else{
         $('.cart-empty').hide();
         $('.sticky').show();
       }
      // alert('done');
     });
     //window.location = '/cart/change?line=' + lineItemIndex + '&quantity=0';
   } else {
     $.ajax({
       method: 'POST',
       url: "/cart/change.js",
       data: {
         quantity: newQuantity,
         id: itemVariantId
       },
       dataType: 'json'
     }).then(function(data) {
       // first update the current product's new price
       $('#CartCount').text(data.item_count);
       var price_single = $('.product-price-amount_'+itemVariantId).next().val();
       var quantity_single = parseInt($('.quantity-selector_'+itemVariantId).val());
       var maxi_quan = parseInt($('.quantity-selector_'+itemVariantId).attr("max"));
       if( quantity_single > maxi_quan ){
         $('.quantity-selector_'+itemVariantId).val(maxi_quan);
       }
       var price_str = String(price_single);
        var price_ary = price_str.split("$");
         var final_price= parseFloat(price_ary[1]);
      
      var calc_pprr = final_price * quantity_single;
      var cal_ppp = calc_pprr.toFixed(2);
       
       $('.product-price-amount_'+itemVariantId).text('$ '+cal_ppp);
       $('.cart-total-price').text(Shopify.formatMoney(data.total_price));
       
     });
   }
 }

