            function updatePopUpCookie() {
                if ($.isNumeric(Cookies.get('WebsiteMasterPopup'))) {
                    var intValue = Cookies.get('WebsiteMasterPopup')
                    if (intValue > 1) { $.colorbox.close(); return; }
                    intValue = parseInt(intValue) + 1;
                    Cookies.set('WebsiteMasterPopup', intValue, { expires: 1 })
                }
                else { Cookies.set('WebsiteMasterPopup', '0', { expires: 1 }) }
            }

