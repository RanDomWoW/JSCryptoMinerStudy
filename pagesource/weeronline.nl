		<script type="text/javascript">
		function setCookie(cname, cvalue, exdays) {
			var d = new Date();
			d.setTime(d.getTime() + (exdays*24*60*60*1000));
			var expires = "expires="+d.toUTCString();

			document.cookie = cname + "=" + cvalue + "; " + expires + "; domain=" + location.hostname;
		}

		$(document).ready(function(){

			$(document).keyup(function(event){
			if(event.keyCode == 13){
				$(".btn-accept").click();
			}
			});

			$(".btn-accept").click(function(){ setCookie("cookieConsent", "true", 365); location.reload() });

			$(".btn-explain").click(function(){
				$(".policy").show();
				$(".introduction").hide();

				return false;
			});

			$(".btn-close").click(function(){
				$(".policy").hide();
				$(".introduction").show();

				return false;
			});

		});
	</script>
