		body {
			font-size: 14px;
			font-family: 'Open Sans', Arial, Helvetica, sans-serif;
			color: #616161;
			background-image:url('https://storage.weeronline.cloud/cookies/background.jpg');
			background-size:auto;
			background-position: top center;
		}

		h2 {
			color:#FF8B22;
			font-size: 13pt;
			text-align: center;
		}

		h3 {
			font-size:14pt;
			font-weight: normal;
		}

		a {
			color: #03A9F4;
			text-decoration:none;
		}

		a:hover {
			color: #68CBF8;
			text-decoration:none;
		}

		table, th, td {
			border: 1px solid black;
			vertical-align: top;
		}

		.wrapper {
			max-width: 600px;
			margin: 50px auto 0px auto;
			background:#ffffff;
			-webkit-border-radius: 4px;
			-moz-border-radius: 4px;
			border-radius: 4px;
			-webkit-box-shadow: 0px 0px 50px 0px rgba(0, 0, 0, 0.5);
			-moz-box-shadow: 0px 0px 50px 0px rgba(0, 0, 0, 0.5);
			box-shadow: 0px 0px 50px 0px rgba(0, 0, 0, 0.5);
		}

		.main {
			background-color:#ffffff;
			border-top-left-radius: 4px;
			border-top-right-radius: 4px;
			float: none;
			margin: 0px auto;
		}

		.content {
			margin: 50px 0px;
		}

		.content h3 {
			text-align: center;
		}

		.content img, .content button {
			display: block;
			margin: 0 auto;
		}

		a.btn-explain {
			width:115px;
			margin:10px auto;
		}

		button + a.btn-explain {
			display: block;
		}

		.explanation {
			background-color:#f2f2f2;
			border-bottom-left-radius: 4px;
			border-bottom-right-radius: 4px;
		}

		.explanation .content {
			margin: 0px;
			padding:20px;
		}

		.explanation .policy {
			display:none;
			max-height:215px;
			overflow: scroll;
		}

		.explanation .policy-test {
			color:#9e9e9e;
		}

		.btn-accept {
			width: 250px;
			height: 45px;
			background: #03A9F4;
			font-size: 14px;
			font-family: 'Open Sans', Arial, Helvetica, sans-serif;
			color: #ffffff;
			border: 0px;
			-webkit-border-radius: 4px;
			-moz-border-radius: 4px;
			border-radius: 4px;
			margin:0px auto;
			display:inline-block;
		}

		.btn-accept:hover {
			background-color:#2BBAFD;
			cursor:pointer;
		}

		/*==========  Mobile First Method  ==========*/

		@media(max-width:767px){
			.mainContainer {
				padding: 0px 20px;
			}

