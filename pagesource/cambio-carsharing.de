<script>
$(function(){

  $('body.mobil.knschl_KUNDEWERDENKEINMANDANT').addClass('javascript');

  $('#lokalisierencwrweiche .zielgruppenwahl input').prop('checked', false);
  
  $('#lokalisierencwrweiche div#lokalisierenclose').on('click', function() {
    $("#lokalisierencwrweiche").fadeOut();
  });

  $('#lokalisierencwrweiche .zielgruppenwahl input').on('click', function(event) {
    
    $('#lokalisierencwrweiche .inhalt.inaktiv').fadeTo('fast', 1.0, function () {
      $('#lokalisierencwrweiche .inhalt.inaktiv').removeClass('inaktiv');
    });
    
  });

  
  $('#lokalisierencwrweiche .staedte ul li a, #lokalisierencwrweiche .karte a').on('click', function(event) {
    event.preventDefault();
    
    if (!$(this).parents('div.inhalt').hasClass('inaktiv')) {
      $('#lokalisierencwrweiche .staedte ul li a, #lokalisierencwrweiche .karte a').removeClass('selected');
      
      $('#lokalisierencwrweiche a[data-index=' + $(this).data('index') + ']').addClass('selected');
      
      if ($('#lokalisierencwrweiche .zielgruppenwahl input[type=radio]:checked').val()==='2') {
        window.Lokalisieren.lokalisiereMandantUndKnotenschluessel(
            $('#lokalisierencwrweiche .staedte ul li a.selected').data('mandanteurocode'),
            'KUNDEWERDEN',
            '2'
        );
        
      } else {
        window.Lokalisieren.lokalisiereCWRKundeWerden(
            $('#lokalisierencwrweiche .staedte ul li a.selected').data('mandanteurocode'), 
            $('#lokalisierencwrweiche .staedte ul li a.selected').text()
        );
      }

    }
    

  });
  
  $('ul.menu li.KUNDEWERDENKEINMANDANT a, div.KUNDEWERDENSTARTBOX a, body.mobil .menu li.KUNDEWERDENKEINMANDANT a, a[data-globalknschluessel=KUNDEWERDENKEINMANDANT]').on('click', function(e) {

    if (e) {
      e.preventDefault();
    }

    $('#lokalisierencwrweiche').show();
        
  });
  
  
  

  
});
</script>


      		
      	
      	
      
    
      
