<script>
//<![CDATA[

var scripts = new Array();
var cur_ind = 0;
var isInited = false;
if( navigator.userAgent.toLowerCase().indexOf('msie') == -1 )
{
	scripts[cur_ind] = '/js/jquery-1.9.1.min.js'; cur_ind++;
}
scripts[cur_ind] = '/js/ajaxupload.3.5.js'; cur_ind++;

var scripts_state = new Array();
for( var i = 0; i < scripts.length; i++ ) { scripts_state[i] = 0; }

function afterLoad()
{

	var btnUpload = jQuery('#upload_file');
	var status = jQuery('#status');
	new AjaxUpload(btnUpload,  {
		element: document.getElementById('upload_file'),
		action: 'upload.php?act=1&inv=' + document.getElementById('inv').value,
		name: 'userfile',
		onSubmit: function(file, ext)
		{
			jQuery('#files_msg').html('');
			if( $('#confirm').prop('checked') == false )
			{
				jQuery('#files_msg').html('<br><span style="color:#FF0000;font-size:18px">Вы должны согласиться с условиями пользовательского соглашения!</span>');
				return false;
			}
			if (/^(php|exe)$/.test(ext))
			{
				jQuery('#files_msg').html('<br><span style="color:#FF0000;font-size:18px">Недопустимый тип файла!</span>');
				return false;
			}
			btnUpload.hide();
			status.html('<br><img src="/img/progress.gif" alt="" />Файл загружается...<br><br>');
			jQuery('#files_msg').html("");
		},
		onComplete: function(file, response)
		{
			var arr = response.split('::');
			status.html('');
			if( (arr[0] == "2") )
			{
				jQuery('#files_msg').html('<br><span style="color:#FF0000;font-size:18px">Ошибка загрузки файла' + arr[1] + '</span>');
			}
			else if( (arr[0] == "1") )
			{
				jQuery('#files_msg').html('<br><span style="color:#009900;font-size:18px">Файл успешно загружен<br><br>' + arr[1] + '</span>');
			}
			else if( (arr[0] == "5") )
			{
				jQuery('#files_msg').html('<br><span style="color:#FF0000;font-size:18px">Недопустимый тип файла!' + arr[1] + '</span>');
			}
			else
			{
				jQuery('#files_msg').html('<br><span style="color:#FF0000;font-size:18px">' + arr[1] + '</span>');
			}
			btnUpload.show();
		}
	});
}
function afterLoadScript( scriptid )
{
	var isAllLoaded = true;
	scripts_state[scriptid] = 1;

	for( var i = 0; i < scripts_state.length; i++ ) { if( scripts_state[i] == 0 ) { isAllLoaded = false; } }
	if( (isAllLoaded == true) && ( isInited == false ) ) { isInited = true; afterLoad(); }
}
function create_async_script( script_url, no_script )
{
	var scr_async = document.createElement('script');
	scr_async.type = 'text/javascript';
	scr_async.async = true;
	scr_async.src = script_url;
	scr_async.noscr = no_script;

	scr_async.onload = scr_async.onerror = function()
	{
		// run once
		if( !this.executed ) { this.executed = true; afterLoadScript( this.noscr ); }
	};

	scr_async.onreadystatechange = function()
	{
		var self = this;
		if( this.readyState == "complete" || this.readyState == "loaded" )
		{
			setTimeout(function() { self.onload() }, 0 );// save "this" for onload
		}
	};

	var scr_pointer = document.getElementsByTagName('script')[0];
	scr_pointer.parentNode.insertBefore(scr_async, scr_pointer);
}
(function()
{
	function async_load()
	{
		for( var i = 0; i < scripts.length; i++ )
		{
			create_async_script( scripts[i], i );
		}
	}
	if( window.addEventListener )	window.addEventListener("load", async_load, false);
	else if( window.attachEvent )	window.attachEvent("onload", async_load);
	else							window.onload = async_load;

})();

//]]>
</script>
