<script type="text/javascript">
    $(function () {
        $(".menu-block").on("mouseleave", function () {
            //팝업 마우스 벗어나면 사라짐
            $(".popup_preview_facet").stop(true, true).fadeOut(600);
            $(".popup_preview_facet").on("mouseover", function () {
                //팝업 마우스 벗어나면 사라짐
                $(".popup_preview_facet").stop(true, true).fadeIn(300);
            });

        });

        //팻시미리보기 팝업
        var facetPopup = $(".popup_preview_facet").on("mouseleave", function () {
            //팝업 마우스 벗어나면 사라짐
            facetPopup.stop(true, true).fadeOut(300);
        });

        $(".facet_menu").bind("ajaxStart", function () {
            $('#preview_facet_layer').addClass('load');
        }).bind("ajaxComplete", function () {
            $('#preview_facet_layer').removeClass('load');
        }).on("mouseenter", ".facet_link", function () {
            $(this).addClass("ajx_target");
            $('.popup_preview_facet .popup_list').html('');
            var getId = $(this).data("id"); //펫시미리보기 팝업 내용 구성할 구분값
            var getPos = $(this).offset().top - 40;
            facetPopup.css("top", getPos).stop(true, true).fadeIn(300);

            var q = $('input[name=q]').val();
            var targetCode = $(this).attr('nk');
            var targetLevel = $(this).attr('nF');
            var target = (getId + "^_" + $(this).attr('nk') + "^_" + $(this).attr('nn')).replace(/§/g, " ").replace(/♧/g, "\|").replace(/◐/g, "\(").replace(/◑/g, "\)").replace(/↗/g, "\[").replace(/↙/g, "\]").replace(/↕/g, "&");

            var fCategory = (getId == 76) ? target : params.fCategory;
            var fContentsType = (getId == 14 && targetLevel == '') ? target : params.fContentsType;
            var fPublication = (getId == 74) ? target : params.fPublication;
            var fPublishYear = (getId == 43) ? target : params.fPublishYear;
            var fRegister = (getId == 79) ? target : params.fRegister;
            var fDpTypeCode = (getId == 14 && targetLevel == 'fDpTypeCode') ? '80' + target.slice(2) : params.fDpTypeCode;
            var fSort = "1";
            if ('journal' == 'home') {
                fSort = "3";
            }

            var ev = $._data($("#facetPreview_search")[0], "events");
            if (ev && ev.click) {
                $("#facetPreview_search").unbind("click");
            }

            // 좁혀보기 링크
            var handlerElem = $(this);
            $("#facetPreview_search").on("click", function () {
                handlerElem.trigger("click");
            });

            getFacetPreview(getId, q, targetCode, fCategory, fContentsType, fPublication, fPublishYear, fRegister, fDpTypeCode, fSort);

            return false;
        }).on("mouseout", ".facet_link", function () {
            $(this).removeClass("ajx_target");
        });

        getFacetPreview = function (facetID, q, targetCode, fCategory, fContentsType, fPublication, fPublishYear, fRegister, fDpTypeCode, fSort) {
            var ajaxUrl = ProxyUrl + '/SearchResult/FacetPreview';
            var dataString = {
                'facetID': facetID
                , 'fCategory': fCategory
                , 'fContentsType': fContentsType
                , 'fPublication': fPublication
                , 'fPublishYear': fPublishYear
                , 'fRegister': fRegister
                , 'fDpTypeCode': fDpTypeCode
                , 'q': q
                , 'fSort': fSort
            };

            $("#PreviewTitle").text($("span[tabId=" + facetID + "]").text());  // 미리보기제목
            $("#PreviewText").text(fnTextCut($(".ajx_target").attr("nn"), 28));  // 미리보기패싯제목

            setTimeout(function () {
                var tCategoryCode = $('.ajx_target').attr('nk');
                if (tCategoryCode != targetCode) {
                    return false;
                }

                //alert(facetID + "," + q + "," + targetCode + "," + fCategory + "," + fContentsType + "," + fPublication + "," + fPublishYear + "," + fRegister + "," + fDpTypeCode + "," + fSort);

                $.ajax({
                    type: 'POST',
                    url: ajaxUrl,
                    data: dataString,
                    dataType: "json",
                    cache: false,
                    success: function (data) {
                        var target = $('.popup_preview_facet .popup_list');
                        target.empty();
                        var li;
                        var p;
                        var a;
                        for (var i = 0; i < data.List.length; i++) {
                            var ttle = "";
                            var url = "";
                            var cntsId = "";
                            var footer = "";
                            var depth = data.List[i].DEPTH;
                            var brws_target = "";

                            var code = '';

                            if (facetID == '43')
                                code = 'L402';
                            else if (facetID == '76')
                                code = 'L302';
                            else if (facetID == '74')
                                code = 'L202';
                            else if (facetID == '79')
                                code = 'L144';
                            else if (facetID == '14') {

                                switch (targetCode) {
                                    case '067001': code = 'L131'; break;
                                    case '067011': code = 'L132'; break;
                                    case '067003', '10703_067003': code = 'L133'; break;
                                    case '067002', '10702': code = 'L134'; break;
                                    case '067004', '10704_067004': code = 'L135'; break;
                                    case '067012', '10712_067012': code = 'L136'; break;
                                    case '067005', '10705': code = 'L137'; break;
                                    case '067006', '10706': code = 'L138'; break;
                                    case '067007', '10707': code = 'L139'; break;
                                    case '067013', '10713': code = 'L140'; break;
                                    case '067009': code = 'L143'; break;
                                }
                            }

                            console.log(depth);

                            if (depth == 1) {
                                ttle = data.List[i].PublicationName;
                                cntsId = data.List[i].PublicationId;
                                url = '/Publication/_cntsId_';
                                footer = data.List[i].PublisherName;
                            } else if (depth == 2) {
                                ttle = data.List[i].IssueName;
                                cntsId = data.List[i].IssueId;
                                if (data.List[i].SystemDivision == "10508" || data.List[i].SystemDivision == "10509") {
                                    brws_target = "_blank";
                                    url = 'http://www.bookrail.co.kr/view/product_detail.asp?co_id=' + data.List[i].ExIssueId;
                                }
                                else {
                                    url = '/Issue/_cntsId_';
                                }
                                footer = data.List[i].AuthorName;
                            } else {
                                ttle = data.List[i].ArticleName;
                                cntsId = data.List[i].ArticleId;
                                if (data.List[i].SystemDivision == "10508" || data.List[i].SystemDivision == "10509") {
                                    brws_target = "_blank";
                                    url = 'http://www.bookrail.co.kr/view/product_detail.asp?co_id=' + data.List[i].ExIssueId;
                                }
                                else {
                                    url = '/Article/_cntsId_';
                                }
                                footer = data.List[i].AuthorName;
                            }
                            url = url.replace("_cntsId_", cntsId);

                            li = $("<li></li>");
                            p = $("<p></p>").addClass("number").text((i + 1) + ".").appendTo(li);

                            if (code != '') {
                                if (brws_target != "") {
                                    a = $("<a></a>").attr("href", url).attr("target", brws_target).attr('onclick', "fn_statistics('" + code + "',fnGetB2BId(), fnGetB2CId());").html(ttle).appendTo(li);
                                }
                                else {
                                    a = $("<a></a>").attr("href", url).attr('onclick', "fn_statistics('" + code + "',fnGetB2BId(), fnGetB2CId());").html(ttle).appendTo(li);
                                }
                            } else {

                                if (brws_target != "") {
                                    a = $("<a></a>").attr("href", url).attr("target", brws_target).html(ttle).appendTo(li);
                                }
                                else {
                                    a = $("<a></a>").attr("href", url).html(ttle).appendTo(li);
                                }
                            }

                            p = $("<p></p>").addClass("info").html(footer).appendTo(li);

                            li.appendTo(target);
                        }
                    }
                });
            }, 500);

            return false;
        };
    });
</script>

