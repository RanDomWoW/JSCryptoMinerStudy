<!-- 20171012 알림서비스추천레이어 팝업 -->

<script>
    $(function () {
        //기본 팝업 닫기
        $("#popup_alarm_rcmd").on("click", ".btn_popup_close", function () {
            $("#backgroundLayer").stop(true, true).animate({ opacity: 0 }, 300, function () {
                $(this).hide();
            });
            $(this).closest("#popup_alarm_rcmd").stop(true, true).fadeOut(300);
            return false;
        });

        //팝업 열기
        $("#btn_alarm_rcmd_temp").on("click", function () {
            $("#backgroundLayer").stop(true, true).show().css("opacity", 0).animate({ opacity: 0.5 }, 300);
            $("#popup_alarm_rcmd").stop(true, true).fadeIn(300);
            return false;
        });

        //오늘 하루 열지 않기
        $("#popup_alarm_rcmd .today_check .check").on("change", function () {
            ALRCMDP_setCookie();
            if (this.checked) {
                $("#popup_alarm_rcmd .btn_popup_close").trigger("click");
            }
        });


        //체크박스 스타일
        function checkBoxActive(target) {
            $(target).each(function () {
                if (this.checked) $(this).parent().addClass("active");
                else $(this).parent().removeClass("active");
            });
        }

        $("#popup_alarm_rcmd").on("change", ".check_box .check", function () {
            checkBoxActive(this);
        });
        checkBoxActive("#popup_alarm_rcmd .check_box .check");


    });
    function ALRCMDP_setAlrimRcmdList() {

        var alrimCode = $("#popup_alarm_rcmd").attr("alarm_type");
        var targetIdList = $.map($("#popup_alarm_rcmd .alarm_list input[type=checkbox]:checked"), function (mapData, mapIndex) {
            return $(mapData).val();
        });

        var alarmTypeCode = "";
        if (alrimCode === "219001") {
            alarmTypeCode = "iprd";
        } else if (alrimCode === "219002") {
            alarmTypeCode = "plct";
        } else if (alrimCode === "219010" || alrimCode === "219011") {
            alarmTypeCode = "autr";
        }
        var popupStrMap = ALRCMDP_STR[alarmTypeCode];

        var notSelectTitle = popupStrMap['notSelectTitle'];

        if (targetIdList.length <= 0) {
            alert(notSelectTitle);
            return false;
        }

        var targetIdListStr = targetIdList.toString();
        var paramData = { alrimCode: alrimCode, targetIdListStr: targetIdListStr };

        $.ajax({
            type: "POST",
            url: ProxyUrl + "/alrimService/setAlrimRcmdList",
            data: paramData,
            dataType: "json",
            success: function (data) {
                var setCount = targetIdList.length;
                var successCount = data['setCount'];
                if (parseInt(successCount) == setCount) {
                    // alert("알림서비스에 추가되었습니다.");

                    $("#backgroundLayer").stop(true, true).animate({ opacity: 0 }, 300, function () {
                        $(this).hide();
                    });
                    $("#popup_alarm_rcmd").stop(true, true).fadeOut(300);
                    location.href = "/AlrimService";
                } else {
                    alert("insertCount_X");
                }
            },
            error: function (data, status, err) {

            }
        });
    }
    function ALRCMDP_setCookie() {
        var date = new Date().toISOString().slice(0, 10).replace(/-/g, "");
        window.sessionStorage.setItem("DBPIA_ALRCMDP_cookie", date);
    }

    function ALRCMDP_isNowShowYnDay() {
        var nowDate = new Date().toISOString().slice(0, 10).replace(/-/g, "");
        var isShowDate = window.sessionStorage.getItem("DBPIA_ALRCMDP_cookie");
        if (isShowDate == nowDate) {
            return true;
        } else {
            window.sessionStorage.removeItem("DBPIA_ALRCMDP_cookie");
            return false;
        }
    }

    ALRCMDP_STR = {};

    var iprdTitleStr = popupRcmdAlarmMsg001;
    var iprdSubTitleStr = popupRcmdAlarmMsg002;
    var iprdListTitle = popupRcmdAlarmMsg003;
    var iprdEmptyListTitle = popupRcmdAlarmMsg004;
    var iprdNotSelectTitle = popupRcmdAlarmMsg013;
    $.extend(ALRCMDP_STR, { iprd: { title: iprdTitleStr, subTitle: iprdSubTitleStr, listTitle: iprdListTitle, emptyListTitle: iprdEmptyListTitle, notSelectTitle: iprdNotSelectTitle} });

    var plctTitleStr = popupRcmdAlarmMsg005;
    var plctSubTitleStr = popupRcmdAlarmMsg006;
    var plctListTitle = popupRcmdAlarmMsg007;
    var plctEmptyListTitle = popupRcmdAlarmMsg008;
    var plctNotSelectTitle = popupRcmdAlarmMsg014;
    $.extend(ALRCMDP_STR, { plct: { title: plctTitleStr, subTitle: plctSubTitleStr, listTitle: plctListTitle, emptyListTitle: plctEmptyListTitle, notSelectTitle: plctNotSelectTitle} });

    var autrTitleStr = popupRcmdAlarmMsg009;
    var autrSubTitleStr = popupRcmdAlarmMsg010;
    var autrListTitle = popupRcmdAlarmMsg011;
    var autrEmptyListTitle = popupRcmdAlarmMsg012;
    var autrNotSelectTitle = popupRcmdAlarmMsg015;
    $.extend(ALRCMDP_STR, { autr: { title: autrTitleStr, subTitle: autrSubTitleStr, listTitle: autrListTitle, emptyListTitle: autrEmptyListTitle, notSelectTitle: autrNotSelectTitle} });


    function PAR_OPEN(data) {
        var targetId = data['target_id'];
        var alarmType = data['alarm_type'];
        var paramData = { targetId: targetId, alarmType: alarmType };

        $("#backgroundLayer").stop(true, true).show().css("opacity", 0).animate({ opacity: 0.5 }, 300);

        var alarmTypeCode = "";
        if (alarmType === "219001") {
            alarmTypeCode = "iprd";
        } else if (alarmType === "219002") {
            alarmTypeCode = "plct";
        } else if (alarmType === "219010" || alarmType === "219011") {
            alarmTypeCode = "autr";
        }

        $("#popup_alarm_rcmd").attr("alarm_type", alarmType);

        var popupStrMap = ALRCMDP_STR[alarmTypeCode];

        var title = popupStrMap['title'];
        var subTitle = popupStrMap['subTitle'];
        var listTitle = popupStrMap['listTitle'];
        var emptyListTitle = popupStrMap['emptyListTitle'];

        $("#popup_alarm_rcmd .popup_title.title").html(title);
        $("#popup_alarm_rcmd .sub-title-box").html(subTitle);
        $("#popup_alarm_rcmd .list-info-box").html(listTitle);


        $.ajax({
            type: "POST",
            url: ProxyUrl + "/alrimService/getAlrimRcmdList",
            data: paramData,
            dataType: "json",
            success: function (data) {
                $("#popup_alarm_rcmd .alarm_list").html("");
                $("#popup_alarm_rcmd").stop(true, true).fadeIn(300);


                if (data !== null && data !== undefined && data.length > 0) {
                    $(data).each(function (index, roopData) {
                        var id = roopData['id'];
                        var title = roopData['title'];
                        var subTitle = roopData['sub_title'];

                        var item = "<li><p>" + title + " <span>" + subTitle + "</span></p><input type='checkbox' name='' value='" + id + "' class='check PAR_check-box'  /></li>";
                        $("#popup_alarm_rcmd .alarm_list").append(item);
                    });
                    $("#popup_alarm_rcmd .btn_box").show();
                } else {
                    $("#popup_alarm_rcmd .alarm_list").html("<li style='list-style: none;color:#a7a7a7;'>" + emptyListTitle + "</li>");
                    $("#popup_alarm_rcmd .btn_box").hide();
                }
            },
            error: function (data, status, err) {

            }
        });

    }

    $(document).ready(function () {
        if (typeof (localStorage.DBPIAT_ALRCMD_P_DATA) === "string" && localStorage.DBPIAT_ALRCMD_P_DATA !== "undefined") {
            var data = localStorage.DBPIAT_ALRCMD_P_DATA;
            try {
                data = JSON.parse(data);
            } catch (e) {
                console.log(e);
            }
            localStorage.DBPIAT_ALRCMD_P_DATA = undefined;
            localStorage.removeItem("DBPIAT_ALRCMD_P_DATA");
            if (data['target_id'] !== undefined && data['alarm_type'] !== undefined) {
                PAR_OPEN(data);
            }
        }
        $("#popup_alarm_rcmd").on("click", ".PAR_check-box", function (e) {
            var alrimCode = $("#popup_alarm_rcmd").attr("alarm_type");
            var code = "";

            if (location.pathname.indexOf("/AlrimService") > -1) {
                code = "A003";
            } else {
                code = "A001";
            }
            fn_statistics(code, fnGetB2BId(), fnGetB2CId())
        });
        $("#popup_alarm_rcmd .set-alarm-rcmd-btn").on("click", function (e) {
            var alrimCode = $("#popup_alarm_rcmd").attr("alarm_type");
            var code = "";

            if (location.pathname.indexOf("/AlrimService") > -1) {
                code = "A004";
            } else {
                code = "A002";
            }
            fn_statistics(code, fnGetB2BId(), fnGetB2CId());

            ALRCMDP_setAlrimRcmdList();
        });
    });
</script>
