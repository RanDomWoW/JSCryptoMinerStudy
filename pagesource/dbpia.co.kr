    <script type="text/javascript">

        $(function () {

            var random_num = Math.floor(Math.random() * 3);
            
            //변수 선언
            var index = random_num;
            var rolling_second = 5000;
            var numOfBanner = $('#main_slide li').length;
            var timer = setInterval(timerHandler, rolling_second);
            var isAuto = true;
            var _btn = '';
            $('#main_slide li').each(function (i) {
                _btn += "<li>";
                _btn += "<button type='button'";
                //_btn += 'title=' + (i + 1) + '번째&#32;배너';
                _btn += "></button>";
                _btn += "</li>";
                $('.roll_control ol').html(_btn);
            });

            var roll_btn = $('.roll_control li');

            //초기화
            $('.roll_control li').eq(index).addClass('active');
            $('#main_slide li').hide();
            $('#main_slide li').eq(index).fadeIn();

            //이벤트
            roll_btn.mouseenter(function () {
                index = $(this).index();
                $('#main_slide li').each(function (i) {
                    if (index == i) {
                        $(this).fadeIn();
                    } else {
                        $(this).fadeOut();
                    }
                });
                roll_btn.removeClass('active');
                roll_btn.eq(index).addClass('active');
            });

            function timerHandler() {
                index++;
                if (index == numOfBanner) {
                    index = 0;
                }
                $('#main_slide li').fadeOut();
                $('#main_slide li').eq(index).fadeIn();
                roll_btn.removeClass('active');
                roll_btn.eq(index).addClass('active');
            }

            roll_btn.mouseenter(function () {
                clearInterval(timer);
            });
            roll_btn.mouseleave(function () {
                timer = setInterval(timerHandler, rolling_second);
            });

            $('.play_controll button').focus(function () {
                if (isAuto == true) {
                    clearInterval(timer);
                    $(this).css('background-position', '-22px 0');

                } else {
                    timer = setInterval(timerHandler, rolling_second);
                    $(this).css('background-position', '0 0');
                }
                isAuto = !isAuto;
            });
        });
    </script>

