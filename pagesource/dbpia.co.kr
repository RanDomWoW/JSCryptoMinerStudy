<script type="text/javascript">

    function setUseNodeList(type, nodeId) {

        if (fnGetB2CId() !== "" || (type == "pageLoad" && fnGetB2BId() !== "")) {
            return;
        }
        var dbpiaUseNodeList = window.sessionStorage.getItem('DBPIA_USE_NODE_LIST');

        var nowDate = new Date().toISOString().slice(0, 10).replace(/-/g, "");
        var data = {};
        var nodeList = "";
        if (typeof (dbpiaUseNodeList) === "string" && typeof (dbpiaUseNodeList) !== "undefined" && dbpiaUseNodeList !== null) {
            try {
                data = JSON.parse(dbpiaUseNodeList);
                if (data['date'] === nowDate) {
                    nodeList = data['node_list'] === undefined ? "" : data['node_list'];
                } else {
                    window.sessionStorage.removeItem('DBPIA_ALERT_MEM_JSON');
                }
            } catch (e) {

            }
        }
    

        if (nodeId != undefined) { 
            if (nodeList === "") {
                nodeList = nodeId;
            } else if (nodeList.indexOf(nodeId) === -1) {
                nodeList += "," + nodeId;
            }
            data['date'] = nowDate;
            data['node_list'] = nodeList;

            window.sessionStorage.setItem('DBPIA_USE_NODE_LIST', JSON.stringify(data));
        }
        toggleMemJoinLaey();
    }

    function toggleMemJoinLaey() {
        try {

            var data = $.parseJSON(window.sessionStorage.getItem('DBPIA_USE_NODE_LIST'));
            var isShowDay = window.sessionStorage.getItem('DBPIA_ALERT_MEM_JSON');
            var nodeList = data['node_list'];
            var count = nodeList.split(",").length;

            if (fnGetB2CId() === "" && count >= 3 && isShowDay !== "N") {
                $(".member-join-info-popup").show();
            }
        } catch (e) { 
        
        }
    }

    $(document).ready(function () {
        if (fnGetB2CId() === "" && (location.pathname.indexOf("/SearchResult") > -1 || location.pathname.indexOf("/Journal/ArticleDetail") > -1)) {
            toggleMemJoinLaey();
        } 
        //안내 레이어 닫기
        $(".member-join-info-popup .btn_info_close").on("click", function () {
            $(".member-join-info-popup").stop(true, true).fadeOut(300);
            window.sessionStorage.setItem('DBPIA_ALERT_MEM_JSON', "N");
            return false;
        });
    });
</script>



