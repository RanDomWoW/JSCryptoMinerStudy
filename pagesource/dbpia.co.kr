<script type="text/javascript">
    $(function () {
        $('#btnCompanyCheck').on('click',
            function () {
                $('#companyAuthCheck').val("");
                $("#search_use_icstId").val("");
                $("#popup_company_check .company_check_box .desc").empty();
                $("#popup_company_check").stop(true, true).fadeIn(200);
            });

        //기간회원 확인하기 자동완성
        if ($("#companyAuthCheck").length > 0) {
            //팝업 닫기
            $(".popup_layer .btn_popup_close").off("click").on("click",
                function () {
                    $(this).closest(".popup_layer").stop(true, true).fadeOut(200);
                    return false;
                });

            //placeholder
            $("#companyAuthCheck").on("focusin",
                function () {
                    var getPlaceholder = $(this).attr("placeholder");
                    if ($(this).val() == getPlaceholder) $(this).val("");
                }).on("focusout",
                function () {
                    if ($(this).val() == "") $(this).val($(this).attr("placeholder"));
                }).val(function () {
                    if ($(this).val() == "") return $(this).attr("placeholder");
                });

            // 기관회원 로그인하기
            $("#popup_company_check .btn_company_check").off('click').on("click",
                function () {
                    fn_statistics2('H07', 'A002', fnGetB2BId(), fnGetB2CId());
                    $('#popup_company_check').stop(true, true).fadeOut(200);
                    setTimeout(function () {
                        $('._loginLayer').filter('[targetVal=organ]').trigger('click');

                        $('#b2bId').val($("#search_use_icstId").val());
                        if ($('#companyAuthCheck').val() != "" && $('#companyAuthCheck').val() != $('#companyAuthCheck').attr('placeholder')) {
                            $('#txtB2BName').val($('#companyAuthCheck').val());
                            $('#loginID').focus();
                        }
                    }, 250);
                });

            //조회하기
            $("#popup_company_check .btn_check").on("click",
                function () {
                    if ($("#search_use_icstId").val() === "" || $("#search_use_icstId").val().length === 0) {
                        alert('DBpia를 구독하지 않는 기관입니다. \r기관명을 다시 확인하시거나, 개인회원 구매를 통해 이용해주세요.');
                        $("#companyAuthCheck").focus();
                        return false;
                    }

                    if ($("#companyAuthCheck").val() != $("#search_use_icstNm").val()) {
                        alert('DBpia를 구독하지 않는 기관입니다. \r기관명을 다시 확인하시거나, 개인회원 구매를 통해 이용해주세요.');
                        $("#companyAuthCheck").focus();
                        return false;
                    }

                    fn_statistics2('H07', 'A001', fnGetB2BId(), fnGetB2CId());
                    $.ajax({
                        url: ProxyUrl + "/Home/CompanyCheck",
                        type: "POST",
                        dataType: "json",
                        data: { s: $("#search_use_icstId").val() },
                        success: function (data) {
                            $('#popup_company_check .company_check_box .desc').empty().html(data);
                            $("#popup_company_check .company_check_box .desc").show();
                            setTimeout(function () {
                                $('#popup_company_check .company_check_box .desc')
                                        .empty();
                            }, 8000);
                            showProgressbar('#popup_company_check > div > div.popup_content > div.company_check_box', false);
                        },
                        error: function (error) {
                            showProgressbar('#popup_company_check > div > div.popup_content > div.company_check_box', false);
                            alert('error: ' + error);
                            console.log(error);
                        }
                    });

                    $('#b2bId').val("");
                    $('#txtB2BName').val("");

                    $('#popup_company_check .company_check_box .desc').empty();
                    showProgressbar('#popup_company_check > div > div.popup_content > div.company_check_box', true);
                    return false;
                });

            $('#companyAuthCheck').on('keypress',
                function (e) {
                    if (e.which === 13) {
                        if ($(this).val() && $("#search_use_icstId").val()) {
                            $('#popup_company_check > div > div.popup_content > div.company_check_box > div.input_box > a').trigger('click');
                        }
                    }
                });

            //자동완성 설정
            $("#companyAuthCheck").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: ProxyUrl + "/AutoComplete/GetB2BList",
                        type: "POST",
                        dataType: "json",
                        data: { s: $("#companyAuthCheck").val().trim() },
                        success: function (data) {
                            response($.map(data.list,
                                function (v, i) {
                                    return {
                                        label: __highlight(v.b2b_showName, request.term),
                                        value: v.b2b_name,
                                        icst_id: v.b2b_id,
                                        icst_nm: v.b2b_showName,
                                        icst_localYN: v.b2b_localYN,
                                        icst_memberYN: v.b2b_memberYN
                                    };
                                }));
                        },
                        error: function (error) {
                            alert('error: ' + error);
                            console.log(error);
                        }
                    });
                },
                appendTo: "#AutoAuthComplete", //자동완성 리스트 표시 위치
                focus: function (event, ui) {
                    event.preventDefault();
                },
                select: function (event, ui) {
                    $.ajax({
                        type: "POST",
                        url: ProxyUrl + "/SearchResult/GetContentsSearchConfirm",
                        data: { icstId: ui.item.icst_id, cntsId: $("#search_use_node").val() },
                        dataType: "json",
                        success: function (data) {
                            if (data.useYN == "Y") {
                                $("#compLogin_desc").html(mes51);
                            } else {
                                $("#compLogin_desc").html(mes52);
                            }
                            $("#search_use_icstId").val(ui.item.icst_id);
                            $("#search_use_icstNm").val(ui.item.icst_nm);
                            $("#search_use_icst_localYN").val(ui.item.icst_localYN);
                            $("#search_use_icst_memberYN").val(ui.item.icst_memberYN);
                        },
                        error: function (data, status, err) {
                            alert('error!');
                            $(document).attr('location', $(document).attr('location'));
                            return false;
                        }
                    });
                },
                autoFocus: true
            }).data("autocomplete")._renderItem = function (ul, item) {
                return $("<li></li>")
                    .data("item.autocomplete", item)
                    .append($("<a></a>").html(item.label))
                    .appendTo(ul);
            };
        }

        if ("" == "Y") {
            $('#companyAuthCheck').val("");
            $("#search_use_icstId").val("");
            $("#popup_company_check .company_check_box .desc").empty();
            $("#popup_company_check").stop(true, true).fadeIn(200);
        }

    });


</script>



