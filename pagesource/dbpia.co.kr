<!-- 알림서비스통합신청 팝업 -->
<script>
    $(function () {
        //기본 팝업 닫기
        $("#popup_total_alarm").on("click", ".btn_popup_close", function () {
            $("#backgroundLayer").stop(true, true).animate({ opacity: 0 }, 300, function () { $(this).hide(); });
            $(this).closest("#popup_total_alarm").stop(true, true).fadeOut(300);
            return false;
        });

        //팝업 열기
        $(".sns_menu .btn_alarm").on("click", function () {

            $("#backgroundLayer").stop(true, true).show().css("opacity", 0).animate({ opacity: 0.5 }, 300);
            var articleId = $(this).attr('node-id');
            getJsonArticleInfo(articleId);
            return false;
        });

        //체크박스 스타일
        function checkBoxActiveAlrim(target) {
            $(target).each(function () {
                if (this.checked) $(this).parent().addClass("active");
                else $(this).parent().removeClass("active");
            });
        }

        function getJsonArticleInfo(articleId) {
            var dataString = { totID: articleId };
            $.ajax({
                type: "POST",
                url: ProxyUrl + "/Journal/getJsonArticleInfo",
                data: dataString,
                dataType: "json",
                success: function (data) {
                    if (data != null) {
                        setCombineAlarmModal(data);
                    }
                },
                error: function (data, status, err) {

                }
            }); //$.ajax
        }

        $("#popup_total_alarm").on("click", ".already-alrim-item", function (e) {
            if ($(e.target)[0].tagName !== "A") {

                e.stopImmediatePropagation();
                e.preventDefault();
                e.stopPropagation();
                alert(mes203);
                return false
            }
        });

        $("#popup_total_alarm").on("change", ".form_box .check_box .check", function () {
            if ($(this).closest(".check_box").hasClass("already-alrim-item")) {
                e.stopImmediatePropagation();
                e.preventDefault();
                e.stopPropagation();
                return false;
            }
            checkBoxActiveAlrim(this);
            if ($(this).hasClass("check_all")) {

                if ($(this).closest(".input_box").find(".check_area .check_box:not(.already-alrim-item)").length == 0) {
                    $(this).closest(".check_box").removeClass("active");
                    $(this).prop("checked", false);
                    return false;
                }

                if (this.checked) {
                    $(this).closest(".input_box").find(".check_area .check_box:not(.already-alrim-item) .check").prop("checked", this.checked).parent().addClass("active");
                }
                else {
                    $(this).closest(".input_box").find(".check_area .check_box:not(.already-alrim-item) .check").prop("checked", this.checked).parent().removeClass("active");
                }
            }
            else {
                var allCount = $(this).closest(".input_box").find(".check_area .check_box:not(.already-alrim-item)").length;
                var checkeCount = $(this).closest(".input_box").find(".check_area .check_box.active:not(.already-alrim-item)").length;
                if (this.checked) {
                    $(this).closest(".input_box").find(".check_all").prop("checked", this.checked).parent().addClass("active");
                }
                else if ($(this).closest(".input_box").find(".check_area .check:not(.already-alrim-item):checked").length == 0) {
                    $(this).closest(".input_box").find(".check_all").prop("checked", this.checked).parent().removeClass("active");
                }
            }
        });
        checkBoxActiveAlrim("#popup_total_alarm .form_box:not(.already-alrim-item) .check_box .check");


        function setCombineAlarmModal(data) {
            var articleInfo = data['article'];
            var alrimServiceList = data['alrimServiceList'];

            //        논문제목
            var ArticleTitleEn = articleInfo['ArticleTitleEn']; // "The Effect of Introversion&#183;Extroversion and Interpersonal Satisfaction on Smart Phone Addiction Perceived by University Students"	string
            var ArticleTitleKr = articleInfo['ArticleTitleKr']; // 	"대학생이 지각하는 내&#183;외향성과 대인관계만족도가 스마트폰 중독에 미치는 영향"	string



            //        논문ID
            var ArticleId = articleInfo['ArticleId']; // 	"NODE06666601"	string


            var ContentsTypeCode = articleInfo['ContentsTypeCode']; // 	"025001"	string


            //        발행기관
            var PublisherName = articleInfo['PublisherName']; // 	"한남대학교 교육연구소"	string
            var PublisherId = articleInfo['PublisherId']; // 	"IPRD00013707"
            var PublisherNameEN = articleInfo['PublisherNameEN']; // 	"Hannam University Research Institute for Studies in Education"	string



            //        전자저널 권호
            var PublicationId = articleInfo['PublicationId']; // 	"PLCT00002121"	string
            var PublicationName = articleInfo['PublicationName']; // 	"학술저널"	string
            var PublicationNameEN = articleInfo['PublicationNameEN']; // 	"Academic journal"	string



            //        저널
            var ArticleAuthorList = articleInfo['ArticleAuthorList']; // 	Count = 2
            //        AUTHOR	"장혜진"	string
            //        AUTHOREN	"Jang, Hye Jin"	string
            //        AUTHORIPRD	"인제대학교 교육학과"	string
            //        Id	""	string
            //
            //        AUTHOR	"전영자"	string
            //        AUTHOREN	"Jeon, Young Ja"	string
            //        AUTHORIPRD	"인제대학교 가족상담복지·소비자학부"	string
            //        Id	"13045"	string

            //        키워드
            var ArticleKeywordList = articleInfo['ArticleKeywordList']; // 	"university students§introversion·extroversion§interpersonal satisfaction§smart phone addiction§대학생§내·외향성§대인관계만족도§스마트폰 중독"	string
            ArticleKeywordList = (ArticleKeywordList == undefined || ArticleKeywordList == null ? "" : ArticleKeywordList);

            //        관련분야
            var mainSubjectName = articleInfo['mainSubjectName']; // 	"자연과학"	string
            var mainSubjectNameEn = articleInfo['mainSubjectNameEn']; // 	"Natural science"	string
            var mainSubjectCode = articleInfo['mainSubjectCode']; // 	"NC03"	string

            var $CA_iprdBox = $(".combine-alarm-iprd-box");
            var $CA_plctBox = $(".combine-alarm-plct-box");
            var $CA_authNewNodBox = $(".combine-alarm-auth-new-node-box");
            var $CA_authNotnBox = $(".combine-alarm-auth-notn-box");
            var $CA_nodeNotnBox = $(".combine-alarm-node-notn-box");
            var $CA_keyWordBox = $(".combine-alarm-keyword-box");
            var $CA_mainSubjectBox = $(".combine-alarm-main-subject-box");

            $(".already-alrim-item").removeClass("already-alrim-item");
            $("#popup_total_alarm .check_box.active").removeClass("active");

            // 발행기관
            $CA_iprdBox.find("input[code-value]").attr("code-value", PublisherId);
            $CA_iprdBox.find(".combine-alarm-iprd-title").html(PublisherName);
            var $CA_iprdLinkBox = $CA_iprdBox.find("a");
            $CA_iprdLinkBox.attr("href", $CA_iprdLinkBox.attr("base-url") + PublisherId);
            var isAlready = false;
            if (alrimServiceList != undefined && alrimServiceList != null) {
                $(alrimServiceList['AlrimList_pub']).each(function (index, alrimItem) { if (alrimItem['TOT_ID'] === PublisherId) { isAlready = true; return false; } });
            }
            if (isAlready) { $CA_iprdBox.addClass("already-alrim-item"); }

            //간행물
            $CA_plctBox.find("input[code-value]").attr("code-value", PublicationId);
            $CA_plctBox.find(".combine-alarm-plct-title").html(PublicationName);
            var $CA_plctLinkBox = $CA_plctBox.find("a");
            $CA_plctLinkBox.attr("href", $CA_plctLinkBox.attr("base-url") + PublicationId);
            isAlready = false;
            if (alrimServiceList != undefined && alrimServiceList != null) {
                $(alrimServiceList['AlrimList_issue']).each(function (index, alrimItem) { if (alrimItem['TP_IDX'] === PublicationId) { isAlready = true; return false; } });
            }
            if (isAlready) { $CA_plctBox.addClass("already-alrim-item"); }

            //저자
            $CA_authNewNodBox.find(".check_area").html("");
            $CA_authNotnBox.find(".check_area").html("");


            $(ArticleAuthorList).each(function (index, ArticleAuthorData) {
                var authId = ArticleAuthorData['Id'];
                if (authId == undefined || authId === "" || authId === null) return true;
                var authItemHtml = "<label class='check_box combine-alarm-auth-item'>" +
                        "<input type='checkbox' class='check' code-value='" + authId + "'>" +
                        "<span class='icon'></span>" +
                        "<span class='text'><a  href='/Author/AuthorInfo?arcId=&ancId=" + authId + "' target='_blank' class='link CA_auth-link-box'>" + ArticleAuthorData['AUTHOR'] + "</a></span>" +
                        " </label>";

                var isAuthNewNodeAlready = false;
                var isAuthNotnAlready = false;
                if (alrimServiceList != undefined && alrimServiceList != null) {
                    $(alrimServiceList['AlrimList_Author']).each(function (index, alrimItem) {
                        if (alrimItem['AUTR_ID'] === authId) {
                            if (alrimItem['MCode'] === '219010') {
                                isAuthNewNodeAlready = true;
                            }
                            if (alrimItem['MCode'] === '219011') {
                                isAuthNotnAlready = true;
                            }
                            //return false;
                        }
                    });
                }
                if (isAuthNewNodeAlready) {
                    $CA_authNewNodBox.find(".check_area").append(authItemHtml.replace("combine-alarm-auth-item", "combine-alarm-auth-item already-alrim-item"));
                } else {
                    $CA_authNewNodBox.find(".check_area").append(authItemHtml);
                }

                if (isAuthNotnAlready) {
                    $CA_authNotnBox.find(".check_area").append(authItemHtml.replace("combine-alarm-auth-item", "combine-alarm-auth-item already-alrim-item"));
                } else {
                    $CA_authNotnBox.find(".check_area").append(authItemHtml);
                }


            });

            if ($CA_authNewNodBox.find(".check_area .combine-alarm-auth-item").length === 0) {
                $CA_authNewNodBox.hide();
                $CA_authNotnBox.hide();
            }

            //노드피인용
            $CA_nodeNotnBox.find("input[code-value]").attr("code-value", ArticleId);
            isAlready = false;
            if (alrimServiceList != undefined && alrimServiceList != null) {
                $(alrimServiceList['AlrimList_NodeCttn']).each(function (index, alrimItem) { if (alrimItem['ArticleId'] === ArticleId) { isAlready = true; return false; } });
            }
            if (isAlready) { $CA_nodeNotnBox.addClass("already-alrim-item"); }

            //키워드
            $CA_keyWordBox.find(".check_list").html("");
            var ArticleKeywordArr = ArticleKeywordList.split("§");
            if (ArticleKeywordArr.length === 0 || (ArticleKeywordArr.length === 1 && ArticleKeywordArr[0] === "")) {
                $CA_keyWordBox.hide();
            } else {
                $(ArticleKeywordArr).each(function (index, keywordStr) {
                    if (keywordStr == null || keywordStr == "") return true;
                    var keyWordItem = "<li class='combine-alarm-auth-keyword-item'>" +
                        "<label class='check_box'>" +
                "<input type='checkbox' class='check' code-value='" + keywordStr + "'  />" +
                "<span class='icon'></span>" +
                "<span class='text'>" + keywordStr + "</span>" +
                "</label>" +
                "</li>";

                    isAlready = false;
                    if (alrimServiceList != undefined && alrimServiceList != null) {
                        $(alrimServiceList['AlrimList_keyword']).each(function (index, alrimItem) { if (alrimItem['Keyword'] === keywordStr) { isAlready = true; return false; } });
                    }
                    if (isAlready) {
                        $CA_keyWordBox.find(".check_list").append(keyWordItem.replace("check_box", "check_box already-alrim-item"));
                    } else {
                        $CA_keyWordBox.find(".check_list").append(keyWordItem);
                    }

                });

            }

            //주제별
            $CA_mainSubjectBox.find("input[code-value]").attr("code-value", mainSubjectCode.substr(0, 2));
            $CA_mainSubjectBox.find(".combine-alarm-main-subject-title").html(mainSubjectName);
            isAlready = false;
            if (alrimServiceList != undefined && alrimServiceList != null) {
                $(alrimServiceList['AlrimList_Subject']).each(function (index, alrimItem) { if (alrimItem['GCode'].toLocaleUpperCase() === mainSubjectCode.substr(0, 2).toLocaleUpperCase()) { isAlready = true; return false; } });
            }
            if (isAlready) { $CA_mainSubjectBox.addClass("already-alrim-item"); }


            $("#popup_total_alarm").stop(true, true).fadeIn(300);
        }


    });
    $(document).ready(function () {



        $(".combine-alarm-submit-btn").on("click", function (e) {
            if (!isLogin()) {
                $("#popup_total_alarm").stop(true, true).fadeOut(0);
                return;
            }

            if ($("#popup_total_alarm").find(".active").length == 0) {
                alert("선택된 항목이 없습니다.");
                return false;
            }

            var paramData = getSetingAlarmData();
            setCombineArticle(paramData);
        });

        $("#popup_total_alarm").on("click", ".CA_auth-link-box", function (e) {
            var $checkBox = $(this).closest(".input_box ");
            var code = "";
            if ($checkBox.hasClass("combine-alarm-auth-new-node-box")) {
                code = "A008";
            } else if ($checkBox.hasClass("combine-alarm-auth-notn-box")) {
                code = "A010";
            }
            fn_statistics(code, fnGetB2BId(), fnGetB2CId())
        });

        $("#popup_total_alarm").on("click", ".combine-alarm-auth-item", function (e) {
            if (e.target.tagName === "A") {
                return;
            }
            var $checkBox = $(this).closest(".input_box ");
            if ($checkBox.hasClass("combine-alarm-auth-new-node-box")) {
                fn_statistics('A007', fnGetB2BId(), fnGetB2CId());
            } else if ($checkBox.hasClass("combine-alarm-auth-notn-box")) {
                fn_statistics('A009', fnGetB2BId(), fnGetB2CId());
            }
        });

        $("#popup_total_alarm").on("click", ".combine-alarm-auth-keyword-item input", function (e) {
            fn_statistics('A012', fnGetB2BId(), fnGetB2CId());
        });

        var setCombineArticle = function (paramData) {
            $.ajax({
                type: "POST",
                url: ProxyUrl + "/alrimService/setCombineArticle",
                data: paramData,
                dataType: "json",
                success: function (data) {
                    var successCount = data['setCount'];
                    if (parseInt(successCount) > -1) {
                        alert(mes204);
                        $("#backgroundLayer").stop(true, true).animate({ opacity: 0 }, 300, function () { $(this).hide(); });
                        $("#popup_total_alarm").stop(true, true).fadeOut(300);
                    } else {
                        alert(mes205);
                    }
                },
                error: function (data, status, err) {

                }
            }); //$.ajax
        }

        function getSetingAlarmData() {

            var paramData = {};


            if ($(".combine-alarm-iprd-box .check_box").hasClass("active")) {
                var iprdId = $(".combine-alarm-iprd-box input").attr("code-value");
                $.extend(paramData, { iprdId: iprdId });
            }

            if ($(".combine-alarm-plct-box .check_box").hasClass("active")) {
                var plctId = $(".combine-alarm-plct-box input").attr("code-value");
                $.extend(paramData, { plctId: plctId });
            }


            var authNewNodeList = $.map($(".combine-alarm-auth-new-node-box .combine-alarm-auth-item.active:not(.already-alrim-item)"), function (data, index) { return $(data).find("input").attr("code-value") }).toString();
            $.extend(paramData, { authNewNodeList: authNewNodeList });

            var authNotnList = $.map($(".combine-alarm-auth-notn-box .combine-alarm-auth-item.active:not(.already-alrim-item)"), function (data, index) { return $(data).find("input").attr("code-value") }).toString();
            $.extend(paramData, { authNotnList: authNotnList });


            if ($(".combine-alarm-node-notn-box .check_box").hasClass("active")) {
                var nodeNotn = $(".combine-alarm-node-notn-box input").attr("code-value");
                $.extend(paramData, { nodeNotn: nodeNotn });
            }

            var keywordList = $.map($(".combine-alarm-keyword-box .combine-alarm-auth-keyword-item .active:not(.already-alrim-item)"), function (data, index) { return $(data).find("input").attr("code-value") }).toString();
            $.extend(paramData, { keywordList: keywordList });


            if ($(".combine-alarm-main-subject-box .check_box").hasClass("active")) {
                var sujectCode = $(".combine-alarm-main-subject-box input").attr("code-value");
                $.extend(paramData, { sujectCode: sujectCode });
            }

            return paramData;
        }
    });
</script>


