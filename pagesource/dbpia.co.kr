                    <input type="text" id="companyAuthCheck" name="companyAuthCheck" placeholder="학교, 기관, 기업명" class="input_company ui-autocomplete-input ui-autocomplete-loading" autocomplete="off" role="textbox" aria-autocomplete="list" aria-haspopup="true"/>
                    <a href="javascript:;" class="btn_check">조회하기</a>
                    <input type="hidden" name="b2bId" id="b2bId" value=""/>
                </div>
                <div id="AutoAuthComplete" class="autocomplete_list">
                    <ul class="ui-autocomplete ui-menu ui-widget ui-widget-content ui-corner-all" role="listbox" aria-activedescendant="ui-active-menuitem" style="z-index: 1001; top: 0px; left: 0px; display: none;"></ul>
                </div>
                <p class="desc">
                </p>
            </div>
