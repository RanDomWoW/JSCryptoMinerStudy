            $(document).bind('keydown',
                function(e) {
                    if (e.keyCode == 27) paperPopup.find(".btn_close").eq(0).trigger("click");
                });
            // B2C 로그인 안되어 있는 경우 Recommand 팝업 닫기                
            if ($('#recommand_layer').data('hidden')) {
                paperPopup.find(".btn_close").eq(0).click();
            }
        }

        $("._recommandLayer").on("click",
            function(e) {
                var getPopupCookie = $.cookie('recommandLayer');
                if (getPopupCookie != "close") {
                    popupResize(false, paperPopup);
                    var totId = $(this).attr('targetVal');
                    $.ajax({
                        type: "POST",
                        //url: ProxyUrl + "/Journal/JsonArticleRecommand",
                        url: ProxyUrl + "/Journal/LayerPopupDownloadAfter",
                        data: { "id": totId },
                        dataType: "html",
                        // success: recommandPop,
                        success: onloadRecommandPopup,
                        error: function(data, status, err) {
                            alert(mes29);
                        }
                    });
                }
            });

        var emptyRecommand = function() {
            $('#recommand_ul').empty();
        };

        paperPopup.on("click",
            ".btn_expand",
            function(e) {
                var $target = $(this).closest(".expand_wrap").find(".expand_content");
                if (!$(this).hasClass("on")) {
                    var totId = $(this).attr('targetVal');

                    $.ajax({
                        type: "POST",
                        url: ProxyUrl + "/SearchResult/GetAbstract",
                        data: { "articleId": totId },
                        dataType: "json",
                        error: function(data, status, err) {
                            alert(mes29);
                        }
                    }).done(function(data) {
                        fn_statistics('Z058', fnGetB2BId(), fnGetB2CId());
                        $target.append($('<p></p>').text(data.Contents));
                    });
                } else {
                    $target.find("p").remove();
                }

                $(this).toggleClass("on");
                $target.stop(true, true).slideToggle(200);

                return false;
            }).on("click",
            ".btn_close",
            function(e) {
                popupMask.stop(true, true).animate({ opacity: 0 },
                    200,
                    function() {
                        $(this).hide();
                    });
                paperPopup.stop(true, true).fadeOut(200);
                emptyRecommand();

                return false;
            }).on("click",
            ".btn_today",
            function(e) {
                $.cookie('recommandLayer', 'close', { expires: 1 });
                paperPopup.find(".btn_close").eq(0).trigger("click");
            });

        //기본 팝업 닫기
        $(".popup_window").on("click", ".btn_popup_close",
            function() {
                //$("#backgroundLayer").stop(true, true).animate({ opacity: 0 }, 300, function() { $(this).hide(); });
                $(this).closest(".popup_window").stop(true, true).fadeOut(300);

                hideMask();
                return false;
            });


        $(".btn_search_use").on("click",
            function() {
                $("#backgroundLayer").stop(true, true).show().css("opacity", 0).animate({ opacity: 0.5 }, 300);
                $(".popup_search_use.type_01").stop(true, true).fadeIn(300);
                $("#search_use_node").val($(this).attr("target"));
                $(".type_01 .btn_buy").attr("targetVal", $(this).attr("target") + "|147003");
                $(".type_01 .prce").text("(" + $(this).attr("prce") + ")");
                return false;
            });

        $(".btn_search_use_02").on("click",
            function() {
                $("#backgroundLayer").stop(true, true).show().css("opacity", 0).animate({ opacity: 0.5 }, 300);
                $(".popup_search_use.type_02").stop(true, true).fadeIn(300);
                $("#search_use_node").val($(this).attr("target"));
                $(".type_02 .btn_buy").attr("targetVal", $(this).attr("target") + "|147003");
                $(".type_02 .prce").text("(" + $(this).attr("prce") + ")");
                return false;
            });

        $(".btn_search_use_03").on("click",
            function() {
                $("#backgroundLayer").stop(true, true).show().css("opacity", 0).animate({ opacity: 0.5 }, 300);
                $(".popup_search_use.type_03").stop(true, true).fadeIn(300);
                $(".type_03 .btn_copy").attr("href", ProxyUrl + "/Article/" + $(this).attr("target"))
                    .attr("onclick",
                        "_gaq.push(['_trackEvent', 'GetAccess', 'Request_to_Library(copy)', fnGetB2BId()]);fn_statistics('Z141',fnGetB2BId(),fnGetB2CId());");
                $(".type_03 .btn_buy").attr("targetVal", $(this).attr("target") + "|147003");
                $(".type_03 .prce").text("(" + $(this).attr("prce") + ")");
                return false;
            });

        $(".btn_search_use_04").on("click",
            function() {
                $("#backgroundLayer").stop(true, true).show().css("opacity", 0).animate({ opacity: 0.5 }, 300);
                $(".popup_search_use.type_04").stop(true, true).fadeIn(300);
                //$(".type_03 .btn_copy").attr("href", ProxyUrl + "/Article/" + $(this).attr("target")).attr("onclick", "_gaq.push(['_trackEvent', 'GetAccess', 'Request_to_Library(copy)', fnGetB2BId()]);fn_statistics('Z141',fnGetB2BId(),fnGetB2CId());");
                $(".type_04 .btn_buy").attr("targetVal", $(this).attr("target") + "|147003");
                $(".type_04 .prce").text("(" + $(this).attr("prce") + ")");
                return false;
            });

        $(".popup_search_use .btn_buy").on("click",
            function() {
                _gaq.push(['_trackEvent', 'GetAccess', 'Purchase', fnGetB2BId()]);
                fn_statistics('Z143', fnGetB2BId(), fnGetB2CId());
                $(this).closest(".popup_window").stop(true, true).fadeOut(300);
            });

        $(".popup_search_use .btn_subscribe").on("click",
            function() {
                _gaq.push(['_trackEvent', 'GetAccess', 'Request_to_Library', fnGetB2BId()]);
                fn_statistics('Z142', fnGetB2BId(), fnGetB2CId());
                $.get('/Common/Subscribe',
                    { nodeId: $("#search_use_node").val() },
                    function(data) {
                        $("#partialSubscribe").empty();
                        $("#partialSubscribe").html(data);
                    });

                $("#backgroundLayer").stop(true, true).animate({ opacity: 0 }, 300, function() { $(this).hide(); });
                $(this).closest(".popup_window").stop(true, true).fadeOut(300);
            });

        $(".popup_search_use .btn_comp_login").on("click",
            function() {
                _gaq.push(['_trackEvent', 'GetAccess', 'Login_B2B', fnGetB2BId()]);
                fn_statistics('Z144', fnGetB2BId(), fnGetB2CId());

                $.fnUseIcst();

                $(this).closest(".popup_window").stop(true, true).fadeOut(300);
            });

        $(".ui-autocomplete").css({ position: "fixed", top: "50%", left: "50%" });

        $('#compLoginName').autocomplete({
            source: function(request, response) {
                $.ajax({
                    url: ProxyUrl + "/AutoComplete/GetB2BList",
                    type: "POST",
                    dataType: "json",
                    data: { s: $("#compLoginName").val() },
                    success: function(data) {
                        response($.map(data.list,
                            function(v, i) {
                                return {
                                    label: __highlight(v.b2b_showName, request.term),
                                    value: v.b2b_name,
                                    icst_id: v.b2b_id,
                                    icst_nm: v.b2b_showName,
                                    icst_localYN: v.b2b_localYN,
                                    icst_memberYN: v.b2b_memberYN
                                };
                            }));
                    },
                    error: function(error) {
                        alert('error: ' + error);
                    }
                });
            },
            minLength: 2,
            select: function(event, ui) {
                $.ajax({
                    type: "POST",
                    url: ProxyUrl + "/SearchResult/GetContentsSearchConfirm",
                    data: { icstId: ui.item.icst_id, cntsId: $("#search_use_node").val() },
                    dataType: "json",
                    success: function(data) {
                        if (data.useYN == "Y") {
                            $("#compLogin_desc").html(mes51);
                        } else {
                            $("#compLogin_desc").html(mes52);
                        }
                        $("#search_use_icstId").val(ui.item.icst_id);
                        $("#search_use_icstNm").val(ui.item.icst_nm);
                        $("#search_use_icst_localYN").val(ui.item.icst_localYN);
                        $("#search_use_icst_memberYN").val(ui.item.icst_memberYN);
                    },
                    error: function(data, status, err) {
                        alert('error!');
                        $(document).attr('location', $(document).attr('location'));
                        return false;
                    }
                });
            },
            autoFocus: true,
            focus: function(event, ui) {
                event.preventDefault();
            }
        }).data("autocomplete")._renderItem = function(ul, item) {
            return $("<li></li>")
                .data("item.autocomplete", item)
                .append($("<a></a>").html(item.label))
                .appendTo(ul);
        };

        $.fnUseIcst = function() {
            $("#txtB2BName").val($("#search_use_icstNm").val());
            $("#b2bId").val($("#search_use_icstId").val());
            if ($("#search_use_icstId").val() != "") {
                if ($("#search_use_icst_localYN").val() == "Y") {
                    $("#loginLayer_desc").text(mes53).show();
                } else if ($("#search_use_icst_memberYN").val() == "Y") {
                    $("#loginLayer_desc").text(mes54).show();
                } else {
                    $("#loginLayer_desc").text(mes55).show();
                }
            }

            setTimeout(function() { $('._loginLayer').filter('[targetVal=organ]').trigger('click'); }, 100);
        };

        //floating menu
        var recentList = $("#floating_menu .recent_list");
        $("#floating_menu .btn_more").on("click",
            function() {
                $(this).toggleClass("active");
                if ($(this).hasClass("active")) {
                    recentList.find(".hide").stop(true, true).fadeIn(300);
                } else {
                    recentList.find(".hide").stop(true, true).fadeOut(100);
                }
                return false;
            });

        var floatingMenu = $("#floating_menu");
        var wingBanner = $(".wing_banner");
        var wingBannerTop = 0;
        if (wingBanner.length) {
            wingBannerTop = wingBanner.offset().top;
        }

        if (floatingMenu.length) {
            if (wingBanner.length) {
                wingBannerTop = wingBannerTop + floatingMenu.height() + 25;
                wingBanner.css("top", wingBannerTop + "px");
            }
        } else {
            wingBannerTop = 25;
            wingBanner.addClass("fixed");
            wingBanner.css("top", wingBannerTop + 0 + "px");
        }

        function floatingScroll() {

            var getTop = $(window).scrollTop();

            // 원래 160px 인데 상단 배너 추가시 부자연스러워 205px로 늘림
            if (getTop > 205 && getTop < 500 ) {
                if (!floatingMenu.hasClass("fixed")) {
                    floatingMenu.addClass("fixed");
                }

                if (!wingBanner.hasClass("fixed")) {
                    wingBanner.addClass("fixed");
                }
                if (floatingMenu.length) {
                    wingBanner.css("top", wingBannerTop + 0 + "px");
                } else {
                    wingBanner.css("top", 5+ "px");
                }

                floatingMenu.removeClass('topbanner1').removeClass('topbanner2');
            } else if (getTop >= 500 ) {
                if (!floatingMenu.hasClass("fixed")) {
                    floatingMenu.addClass("fixed");
                }

                if (!wingBanner.hasClass("fixed")) {
                    wingBanner.addClass("fixed");
                }

                if (floatingMenu.length) {
                    wingBanner.css("top", wingBannerTop + 40 + "px");
                } else {
                    wingBanner.css("top", wingBannerTop + 20+ "px");
                }

                floatingMenu.removeClass('topbanner1').removeClass('topbanner2');
            }
            else {

                if ($("#floating_menu").data('topbanner1')) {
                    floatingMenu.addClass('topbanner1');
                }

                if ($("#floating_menu").data('topbanner2')) {
                    floatingMenu.addClass('topbanner2');
                }

                if (floatingMenu.hasClass("fixed")) {
                    floatingMenu.removeClass("fixed");
                }
                if (wingBannerTop > 0) {

                    if (wingBanner.hasClass("fixed")) {
                        wingBanner.removeClass("fixed");
                    }
                    wingBanner.css("top", wingBannerTop + "px");
                }
            }
        }

        $(window).on("scroll",
            function() {
                floatingScroll();
            });
        floatingScroll();
    });



</script>
