var nslfooterAjaxCallAllowed = true;
//1511
function onNewFooterNewsletterError(__form, __error) {
        if (__error) {
            __form.find('.s-error-new').text(__error);
        }
        __form.find('.s-error-new').show();
    }
    function onNewFooterNewsletterSubmit() {
        var _form  = $('#footerSignUpNew form');
        _form.find('.s-success-new, .s-error-new').hide();
        var _email = _form.find('#footer-newsletter-email').val();
        if(nslfooterAjaxCallAllowed == true){
            nslfooterAjaxCallAllowed = false;
            
            if(_email.valid('email')) {            
                var _base   = '/newsletter/validate/';
                var _token  = _form.find('#token').val();
                var _url    = _base + '?YII_CSRF_TOKEN=' + _token + '&email=' + _email ;
                $('.newsletterLoader').show();
                $.get(_url, function(__data) {
                    __data = $.parseJSON(__data);
                    $('.newsletterLoader').hide();
                    if(typeof wt == 'object'){
                        wtmessage= 'Footer~Banner_'+ _email+'_' + __data['error'];
                        wtmessage = wtmessage.trim();
                        wtmessage = wtmessage.replace(/ /g, "~");
                        wt.linkId= wtmessage;
                        wt.customEcommerceParameter = {13:"lead"};
                        wt.customSessionParameter = {
                          6: myCookie.set("subID", __data['newsletter_subscription_id'])
                        };
                        wt.sendinfo();
                     }
                     if(__data['validation']) {
                         if(__data['error']=="This Email Id is already subscribed to Newsletter"){
                             onNewFooterNewsletterError(_form, __data['error']);
                         } else{                     
                            _form.find('.s-success-new').show();
                            _form.find('.s-success-new').html(__data['error']); //NEWSLETTER_SUBSCRIBE_NOCHANGE
                         }
                    } else {
                        onNewFooterNewsletterError(_form, __data['error']);
                    }
                        nslfooterAjaxCallAllowed = true;
                });
            } else {
                onNewFooterNewsletterError(_form, 'Invalid email address');
                nslfooterAjaxCallAllowed = true;
            }
        }
        
        return false;
    }
</script>

