<script type="text/javascript">
function lazyloadCampaignPopup(element, e){
    if(!element.hasClass('loaded') && !element.children('.itm-img').length) {
            var distY = element.offset().top;
            var distReal = distY - $(window).scrollTop();
            if (distReal < $(window).height()) {
                var imgDiv = element.children('.hp-itm-imageWrapperCampaignPopup');
                var imgSrc = imgDiv.attr('img-data');
                var productName = imgDiv.attr('title');
                if (imgSrc.indexOf('lazyloadShowCampaignPopup') == -1) {
                    imgDiv.html('<img alt="' + productName + '" class="itm-img" style="display:none" src="' + imgSrc + '" onload="lazyloadShowCampaignPopup($(this));" >');
                }
             }
      }
 }
    function lazyloadShowCampaignPopup(el) {
        el.parents('.hplazyImageCampaignPopup').addClass('loaded');
        el.parent().show();
        el.fadeIn();
    
    }
    function afterLoadCampaignPopup(){
        $('.hplazyImageCampaignPopup').each(function(){lazyloadCampaignPopup($(this), 1000);});
        }
        if (window.addEventListener) {
              window.addEventListener("load", afterLoadCampaignPopup, false);
        } else{
        window.attachEvent('onload', afterLoadCampaignPopup);
        }
           
</script>
       
