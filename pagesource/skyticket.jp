<script>
function bookingCheck(f) {
	var arrSelect = document.getElementById('arrival_airport');
	var arrIATA = arrSelect.options[arrSelect.selectedIndex].text.replace(/[^A-Z]/g, '');
	if (arrIATA === 'OSA') arrIATA = 'KIX';

	// 出発日の YYYY-MM-DD
	var checkInDate = document.getElementById('js-DP').value.replace(/\//g, '-');

	// エポックms換算
	var checkInDateEpoch = new Date(checkInDate);
	var checkInDateNextEpoch = new Date(checkInDateEpoch.setDate(checkInDateEpoch.getDate() + 1));

	// 出発日翌日の YYYY-MM-DD
	var checkInDateNext = String(checkInDateNextEpoch.getFullYear() + '-' + ('0' + (checkInDateNextEpoch.getMonth() + 1)).slice(-2) + '-' + ('0' + checkInDateNextEpoch.getDate()).slice(-2));

	if(document.getElementById('checkForHotel').checked) {
		// 別タブで航空券検索
		f.target = '_blank';

		// document.search_form.addEventListener('submit', function() {
		document.getElementById('js-btnSearchTicket').addEventListener('click', function() {
			setTimeout(function() {
				location = 'http://hotels.skyticket.jp/searchresults.html?aid=1277972;checkin=' + checkInDate + ';checkout=' + checkInDateNext + ';iata_orr=1;iata=' + arrIATA + ';label=';
			}, 100);
		});
	} else {
		f.removeAttribute('target');
		document.getElementById('js-btnSearchTicket').addEventListener('click', function() {
		// document.search_form.addEventListener('submit', function() {
			setTimeout(function() {
				document.search_form.submit();
			}, 100);
		});
	}
}

(function() {
	document.getElementById('checkForHotel').checked = false;
})();
</script>
