        }

        // need to only trigger this in specific trigger
        // dataLayer.push({'event': 'popup-home-page-clicked-download-button'});
        //return;
      });

      $('#auto-popup-form .popup-close').click(function (event) {
        event.preventDefault();
        CreateCookie("popup-12-step-14-day-pause", "1", 14);
        hidePopup();
      });

      $('#auto-popup-form').click(function (event) {
        event.preventDefault();
        if (event.target.className == "overlay-popup") {
          CreateCookie("popup-12-step-session", "1");
          hidePopup();
        }
      });

      $(".red-home-optin .open-cta-popup").click(function (event) {
        event.preventDefault();
        //$('.g-recaptcha').detach().appendTo('.recaptcha-placeholder');
        $("#cta-popup-form").fadeIn();
