<script type="text/javascript">
    $('#txt_mobSearch').keypress(function (e) {
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 13) {
            $('#btn_mobSearch').click();
            return false;
        }
    });

    $('#btn_mobSearch').click(function () {
        doSearch_Mobile();
    });

    $('#txt_mobSearch').click(function () {
        $('#txt_mobSearch').val("");
    });

</script>
<script type="text/javascript">
    Url = function () { }
    Url.prototype = {
        relativeRoot: '/',
        resolve: function (relative) {
            var resolved = relative;
            if (relative[0] == '~') resolved = this.relativeRoot + relative.substring(2);
            return resolved;
        }
    }

    $Url = new Url();

    function doSearch_Mobile() {
        var shearchUrl = 'Search2/List.aspx?query=';
        if (location.href.toLowerCase().indexOf('/cwto/') > -1) {
            shearchUrl = '~/CWTO/' + shearchUrl;
        }
        else if (location.href.toLowerCase().indexOf('/ewto/') > -1) {
            shearchUrl = '~/EWTO/' + shearchUrl;
        }
        else if (location.href.toLowerCase().indexOf('/english/') > -1) {
            shearchUrl = '~/English/' + shearchUrl;
        }
        else {
            shearchUrl = '~/' + shearchUrl;
        }

        var searchStr = document.getElementById("txt_mobSearch").value;
        window.location.href = $Url.resolve(shearchUrl + searchStr);
    }

</script>

