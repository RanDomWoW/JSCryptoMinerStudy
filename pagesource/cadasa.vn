        $(".pass").blur(function () { ktpass() });
        ktuser();
        ktpass();
    });
    function ktuser() {
        if ($(".user").val() == "") { $(".user").css('backgroundPosition', '0px 0px ')}
        else
            $(".user").css('backgroundPosition', '0px -31px');
    }
    function ktpass() {
        if ($(".pass").val() == "") { $(".pass").css('backgroundPosition', '0px 0px ') }
        else
            $(".pass").css('backgroundPosition', '0px -31px ');
    }

  
</script>
