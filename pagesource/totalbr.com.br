<script type="text/javascript">
function banner_setCookie(name,v,h) {
    var expDate = new Date()
    expDate.setTime(expDate.getTime() + (h * 3600 * 1000))
    document.cookie = name + "=" + escape(v) + ";path=/;expires=" + expDate.toGMTString()
}

function banner_getCookie(name) {
    deb = document.cookie.indexOf(name + "=")
    if (deb >= 0) {
        deb += name.length + 1
        fin = document.cookie.indexOf(";",deb)
        if (fin < 0) fin = document.cookie.length
        return unescape(document.cookie.substring(deb,fin))
    }
    return "";
}

	$(window).load(function(){
		$('.close_box').click(function(){
			$("#cookies-banner").slideToggle();
		});
		if (banner_getCookie('CookiesAcceptedByUser') == 'yes') {
			$("#cookies-banner").hide();
		}else {
			if ($('#cookies-banner').length) {
				banner_setCookie('CookiesAcceptedByUser','yes',30*24);
				//$("#cookies-banner").show( "slow" );		
				$("#cookies-banner").slideToggle();
			}
		}
	});
		$(document).delegate('click','.close_box',function(){
			$(this).parent().remove();
		});	

</script> 







	
	
	








	
	
	






	
	
	
	
	
	

		
		
		
		
			
		
		
		
		
		
		
		
		
