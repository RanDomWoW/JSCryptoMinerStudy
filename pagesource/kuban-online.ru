                    <script type="text/javascript">
  (function ($) {
    $(function () {
      $('#poll1').submit(function (e) {
        var form = $(this);
        e.preventDefault();
        $.ajax({
          url: form.attr('action'),
          data: $(this).serialize(),
          type: form.attr('method'),
          beforeSend: function () {
            form.find('.error-msg').hide();
          },
          dataType: 'json'
        }).success(function (response) {
          if (response.success) {
            window.location.reload();
          } else {
            $.each(response.errors, function (index, val) {
              form.find('#q' + index).find('.error-msg').show();
            });
          }
        });
      });

      $('#poll1').bind("change", "input", function (e) {
        var el = e.target,
            $el = $(el),
            name = $el.attr("name"),
            siblings = $("[name=\"" + name + "\"]", this);

        siblings.each(function () {
          var question_pk = $(this).data('depend-question');

          if (el !== this && question_pk) {
            $('#q' + question_pk).hide();
            $('#q' + question_pk).find('input').attr('checked', false).trigger("change");

          }
        });

      });


      $('input[data-depend="true"]').change(function (e) {
        var el = $(this),
            checked = el.prop('checked'),
            question_pk = el.data('depend-question');
        if (checked) {
          $('#q' + question_pk).show();
        } else {
          $('#q' + question_pk).hide();
          $('#q' + question_pk).find('input').attr('checked', false).trigger("change");
        }
      });
    })
  })(window.jQuery);
</script>

