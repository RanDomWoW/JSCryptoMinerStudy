        <!--back to top-->
        <script>
            $(document).ready(function(){
    
                // hide #back-top first
                $("#back-top").hide();
                // fade in #back-top
                $(function () {
                    $(window).scroll(function () {
                        if ($(this).scrollTop() > 100) {
                            $('#back-top').fadeIn();
                        } else {
                            $('#back-top').fadeOut();
                        }
                    });
                    $('#back-top a').bind("mouseover", function(){
                        var color  = $('#back-top').css("background-color");
                        $('#back-top').css("background", "#999");

                        $(this).bind("mouseout", function(){
                            $('#back-top').css("background", color);
                        })    
                    })   
                    // scroll body to 0px on click
                    $('#back-top a').click(function () {
                        $('body,html').animate({
                            scrollTop: 0
                        }, 800);
                        return false;
                    });
                });
                // pop up
                $('#my-button').bind('mouseover', function(e) {
                    $('#popup').bPopup({
                        speed: 950,
                        transition: 'slideIn'
                    });
                });
                var org = $('#cookiez').val();
                var checker = "item="+ org;
                //   alert(checker);
                if (document.cookie.indexOf(checker) == -1) {
                    var fifteenDays = 1000*60*60*24;
                    //  var fifteenDays = 10000;
                    var expires = new Date((new Date()).valueOf() + fifteenDays);
                    document.cookie = checker + ";expires=" + expires.toUTCString();
                    $('#about').bPopup({
                        easing: 'easeOutBack', //uses jQuery easing plugin
                        speed: 2000,
                        transition: 'slideDown'
                    });
                }
               
                $('#photo a').lightBox();
    
            });
        </script>
