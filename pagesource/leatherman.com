<script type="text/javascript">
	function CriteoDataCalls(){
		var prod = true;
		var log = !prod && window.console;
		var payLoad = {};
				
		if(log){
			console.log("Initializing Criteo Remarketing Data Call");			
		}
		
		if(LTGSFMCPIData.userKey != null){
			if(log){
				console.log("CriteoDataCalls.dataLayer: email="+LTGSFMCPIData.userKey);			
			}	
			payLoad.email = LTGSFMCPIData.userKey.toLowerCase();
									
		}
		else{
			payLoad.email = '';
		}
		
		if(LTGSFMCPIData.pageType != null){
			var pageType = LTGSFMCPIData.pageType;
			if(LTGSFMCPIData.pageType==='Category' || LTGSFMCPIData.pageType==='SearchResults'){
				pageType='ListingPage'				
			}
			if(log){
				console.log("CriteoDataCalls.dataLayer: pageType="+pageType);			
			}	
			payLoad.PageType = pageType;
			if('displayedVariants' in LTGSFMCPIData){
				payLoad.ProductIDList = LTGSFMCPIData.displayedVariants;
				if(log){
					console.log("CriteoDataCalls.dataLayer: products displayed= "+LTGSFMCPIData.displayedVariants.join(','));			
				}	
			}
		}
		if('trackPageView' in LTGSFMCPIData && 'item' in LTGSFMCPIData.trackPageView){
			payLoad.PageType = 'ProductPage';
			payLoad.ProductID = LTGSFMCPIData.trackPageView.item;
			if(log){
				console.log("CriteoDataCalls.dataLayer: ProductPage = "+payLoad.ProductID);			
			}
		}
		
		if('trackCart' in LTGSFMCPIData && LTGSFMCPIData.trackCart.length>0){
			payLoad.PageType = 'BasketPage';
			payLoad.ProductBasketProducts = criteoProductList(LTGSFMCPIData.trackCart);
			if(log){
				console.log("CriteoDataCalls.dataLayer: BasketPage = "+LTGSFMCPIData.trackCart.length + ' in cart');			
			}
		}
		
		if('trackConversion' in LTGSFMCPIData){
			payLoad.PageType = 'TransactionPage';
			payLoad.TransactionID = LTGSFMCPIData.trackConversion[1].order_number;
			payLoad.ProductTransactionProducts = criteoProductList(LTGSFMCPIData.trackConversion[1].cart);
			if(log){
				console.log("CriteoDataCalls.dataLayer: TransactionPage = "+payLoad.ProductTransactionProducts.length + ' in cart');			
			}
		}
		
		dataLayer.push(payLoad);
	}
	
	function criteoProductList(products){
		var productList = [];
		for(var i = 0; i<products.length;i++){
			var product = {};
			product.id = products[i].item;
			product.price = products[i].price;
			product.quantity = products[i].quantity;
			productList.push(product);
		}		
		return productList;
	}
	
	CriteoDataCalls();
</script>








<script type="text/javascript">
	function LTGProcessSFMCPIDataCalls(){
		var prod = true;
		var log = !prod && window.console;
		if(log){
			console.log("Initializing SFMC PI Data Call");			
		}
		etmcPush(["setOrgId","7279667"]);
		if(log){
			console.log("setOrgId: "+"7279667");			
		}
		if(LTGSFMCPIData.userKey != null){
			if(log){
				console.log("setUserInfo: email="+LTGSFMCPIData.userKey);			
			}	
			LTGSFMCPIData.setUserserInfo = {
					"email":LTGSFMCPIData.userKey,
					"details": { 
						"WebBrand":"Leatherman",
						"WebLanguage":"en",
						"WebCountry": "US"
					}	
			}
			etmcPush(["setUserInfo",LTGSFMCPIData.setUserserInfo]);			
		}
		
		if(LTGSFMCPIData.trackCart!=null){
			if(LTGSFMCPIData.trackCart.length>0){
				etmcPush(["trackCart",{"cart":LTGSFMCPIData.trackCart}]);
				if(log){
					console.log("trackCart");			
				}
			}
			else{
				if(log){
					console.log("trackCart: clear_cart");			
				}
				etmcPush(["trackCart",{"clear_cart":true}]);						
			}
		}
		
		if(LTGSFMCPIData.trackConversion!=null){
			etmcPush(LTGSFMCPIData.trackConversion);
			if(log){
				console.log("trackConversion");			
			}
		}		
		
		if(log){
			var ldetail = '';
			if('item' in LTGSFMCPIData.trackPageView){
				ldetail +=' PID: '+LTGSFMCPIData.trackPageView.item;
				if('itemViewSrc' in LTGSFMCPIData.trackPageView){
					ldetail +=' src: '+LTGSFMCPIData.trackPageView.itemViewSrc;				
				}
				
			}
			console.log("trackPageView: "+Object.keys(LTGSFMCPIData.trackPageView)+' '+ldetail);			
		}
		if('itemViewSrc' in LTGSFMCPIData.trackPageView){
			delete 	LTGSFMCPIData.trackPageView['itemViewSrc'];
		}
		etmcPush(["trackPageView",LTGSFMCPIData.trackPageView]);
	}
	
	function etmcPush(obj){
		if(typeof _etmc === 'object'){
			//console.log('pushing');
			_etmc.push(obj);
		}
	}	
	LTGProcessSFMCPIDataCalls();
</script>





