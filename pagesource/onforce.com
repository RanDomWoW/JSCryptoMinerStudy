            jQuery(this).parent().find(".resp-menu-pane").slideToggle(500, function () {
                            jQuery(this).parent().toggleClass("open");
            });
        });

		jQuery(".comment-metadata a > time").unwrap();
		jQuery( '.form-group .col-sm-9' ).removeClass( 'col-sm-9' ).addClass( 'col-sm-12' );
		jQuery( '.form-group .control-label' ).addClass( 'hide' );

		jQuery('.moreSearch').on('click focusin', function() {
			jQuery('.moreSearch').val('');
		});

        jQuery('.form-control-clear').on('click focusin', function() {
            jQuery('.form-control-clear').val('');
        });

		jQuery('input,textarea').focus(function(){
		   jQuery(this).removeAttr('placeholder');
		});
    });
</script>



