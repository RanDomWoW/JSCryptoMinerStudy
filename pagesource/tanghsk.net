var left = 50;
function fnPopupShow(url, name, width, height){
	//alert(url);
    if(getCookieValue(name) != 'popup'){
        fnWinOpen(url, name, width, height);
    }
}
function fnWinOpen(url, name, width, height){
	///alert(name);
	window.open(url,name,"height="+height+",width="+width+",toolbar=no,menubar=no,scrollbars=no,resizable=yes,location=no,status=no"); 

}

function fnPopupHide(name, obj)
{
    if(obj.checked == true)
    {
        setCookie(name, 'popup' , 1);  // ??
        window.close();
    }
}

function setCookie(name, value, expiredays)
{ 
    var todayDate = new Date();
    //test用  有效时间1分钟内设定    //todayDate.setMinutes(todayDate.getMinutes() + expiredays);

    //有效时间1天内设定
    todayDate.setDate(todayDate.getDate() + expiredays);
    
    //alert("ccc"+todayDate.toGMTString() );
    document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";";
}

function getCookieValue(name){
    var strCookieName = name + "=";
    var objCookie = document.cookie;
    
    if (objCookie.length > 0)
    {
        var nBegin = objCookie.indexOf(strCookieName);
        if (nBegin < 0){ return ""; }
        nBegin += strCookieName.length;
        var nEnd = objCookie.indexOf(";", nBegin);
        if (nEnd == -1){ nEnd = objCookie.length; }
    }
    
    return unescape(objCookie.substring(nBegin, nEnd));
}

function operate(str) {
	if(str == 'change'){
		var ss = $("#btn_searchTxt").html();
        if(ss=="신분증조회"){
            $("#show_id").html("신분증번호：");
            $("#btn_searchTxt").html("수험표조회");
            $("#txtFlag").val("1");
        }else{
            $("#show_id").html("수험번호：");
            $("#btn_searchTxt").html("신분증조회");
            $("#txtFlag").val("0");
        }
	}else if(str == 'r_search'){
		var flag = $("#txtFlag").val();
		var r_id = $("#r_id").val();
		var r_username = $("#r_username").val();
		if(flag == '0'){
			if(r_id == null || r_id == ''){
				alert("수험번호를 기입해 주시기 바랍니다.");
				return;
			}
			window.location.href = "../common/result_searchByRid.action?examineeNumber=" + r_id + "&idcardName=" + r_username;
		}else{
			if(r_id == null || r_id == ''){
				alert("신분증번호를 기입해 주시기 바랍니다.");
				return;
			}
			window.location.href = "../common/result_searchByCid.action?idcardNumber=" + r_id + "&idcardName=" + r_username;
		}
		if(r_username == null || r_username == ''){
			alert("신분증상 이름을 기입해 주시기 바랍니다.");
			return;
		}
	}else if(str == 'cer_search'){
		var cer_id = $("#cer_id").val();
		if(cer_id == null || cer_id == ''){
			alert("증서 번호를 기입해 주시기 바랍니다.");
			return;
		}
		var cer_username = $("#cer_username").val();
		if(cer_username == null || cer_username == ''){
			alert("신분증상 이름을 기입해 주시기 바랍니다.");
			return;
		}
		window.location.href = "../common/cer_search.action?cerNumber=" + cer_id + "&idcardName=" + cer_username;
	}else if(str == 'bjd_index'){
		window.open("../common/bjd_index.action");
	}
}
</script>

