<script type="text/javascript">
ematras_callmeback_inprogress = false;
jQuery(document).ready(function(){
   jQuery("#callback_header_phone").keyup(function(e){
        var el=jQuery("#callback_header_phone").get(0);
        if (!el.value.match(/^[\+\-0-9() ]+$/)) {
            el.value = el.value.replace(/[^\+\-0-9() ]/g, '');
        }
        if(e.keyCode && e.keyCode == 13){
            ematras_callmeback();
