function ematras_callmeback(){
  var phoneel=jQuery('#callback_header_phone');
  if(phoneel.val() && phoneel.val().match(/^[\+\-0-9() ]{9,}$/)){
    if(ematras_callmeback_inprogress){
        alert('Ваш запрос уже отправляется, пожалуйста подождите...');
        return;
    }
    ematras_callmeback_inprogress = true;
    jQuery.ajax({
    type: "POST",
    url: 'https://e-matras.ua/callmeback/',
    data: {
       phone: phoneel.val(),
       city: jQuery(".region-title").text(),
    },
    dataType: 'json',
    success: function(data){
      jQuery('#callback_header_phone').val('');
      ematras_callmeback_inprogress = false;
      if(data['error']){
        alert(data['error']);
      }else{
        ga('send', {
          hitType: 'event',
          eventCategory: 'callBack',
          eventAction: 'send'
        });
        alert("Спасибо! \nНаш менеджер свяжется с Вами в ближайшее время. \nПерезваниваем с 10:00 до 20:00");

      }
    },
    error: function(){ alert('Ошибка при отправке данных - ошибка в AJAX ответе'); }
    });
  } else {
    alert('Введите номер Вашего телефона');
    phoneel.focus();
  }
}
