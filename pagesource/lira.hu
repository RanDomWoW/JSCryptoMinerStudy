function isValidEmailAddress(emailAddress) {
	var pattern = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);
	return pattern.test(emailAddress);
};

function lep3(szallitasi_opciok, szemelyes_adatok, jovahagyas, Szallitas_adat_kell) {
	//console.log("lep3: ");
	var mehet=1;
	if (szallitasi_opciok==1) { // szállítási opciók
		mehet=0;
		var i=0;
		var elemek=document.forms["frm_szallitasi_opciok"].elements;
		var kulfold_hol=0;
		var kulfold_mfiz=0;
		var belfold_sz_fizmod=0;
		var belfold_f_mikorhol=0;
		var belfold_f_fizmod=0;
		var belfold_atvetel=0;
		var belfold_sz_bolt=0;
		var lapker_id=0;
		var belfold_l_fizmod=0;
		
		var webox_machine_id=0;
		var foxpost_id=0;
		var belfold_fp_fizmod=0;
		var postapont_id=0;
		var belfold_w_fizmod=0;
		var belfold_pp_fizmod=0;
		
		var erzsebetkartya_code_belfold_sz=0;
		var erzsebetkartya_code_belfold_f=0;
		var erzsebetkartya_code_belfold_l=0;
		var erzsebetkartya_code_belfold_w=0;
		var erzsebetkartya_code_belfold_fp=0;
		var erzsebetkartya_code_belfold_pp=0;
		var erzsebetkartya_code_kulfold=0;
		var belfold_pp_fizmod=0;

		while (i<elemek.length) {
			//console.log(elemek[i].name);
			if ((elemek[i].name=='kulfold_hol')&&(elemek[i].checked))        kulfold_hol=elemek[i].value;
			if ((elemek[i].name=='kulfold_mfiz')&&(elemek[i].checked))       kulfold_mfiz=elemek[i].value;
			if ((elemek[i].name=='belfold_sz_fizmod')&&(elemek[i].checked))  belfold_sz_fizmod=elemek[i].value;
			if ((elemek[i].name=='belfold_f_mikorhol')&&(elemek[i].checked)) belfold_f_mikorhol=elemek[i].value;
			if ((elemek[i].name=='belfold_f_fizmod')&&(elemek[i].checked))   belfold_f_fizmod=elemek[i].value;
			if ((elemek[i].name=='belfold_sz_bolt'))    belfold_sz_bolt=elemek[i].value;
			if ((elemek[i].name=='belfold_atvetel')&&(elemek[i].checked))    belfold_atvetel=elemek[i].value;
			
			if ((elemek[i].name=='lapker_radio')&&(elemek[i].checked))       lapker_id=elemek[i].value;
			if ((elemek[i].name=='belfold_l_fizmod')&&(elemek[i].checked))   belfold_l_fizmod=elemek[i].value;
			if ((elemek[i].name=='foxpost_id'))                              foxpost_id=elemek[i].value;
			if ((elemek[i].name=='belfold_fp_fizmod')&&(elemek[i].checked))  belfold_fp_fizmod=elemek[i].value;
			if ((elemek[i].name=='belfold_pp_fizmod')&&(elemek[i].checked))   belfold_pp_fizmod=elemek[i].value;
			
			if ((elemek[i].name=='erzsebetkartya_code_belfold_sz'))   erzsebetkartya_code_belfold_sz=elemek[i].value;
			if ((elemek[i].name=='erzsebetkartya_code_belfold_f'))    erzsebetkartya_code_belfold_f=elemek[i].value;
			if ((elemek[i].name=='erzsebetkartya_code_belfold_l'))    erzsebetkartya_code_belfold_l=elemek[i].value;
			if ((elemek[i].name=='erzsebetkartya_code_belfold_w'))    erzsebetkartya_code_belfold_w=elemek[i].value;
			if ((elemek[i].name=='erzsebetkartya_code_belfold_fp'))   erzsebetkartya_code_belfold_fp=elemek[i].value;
			if ((elemek[i].name=='erzsebetkartya_code_belfold_pp'))   erzsebetkartya_code_belfold_pp=elemek[i].value;
			if ((elemek[i].name=='erzsebetkartya_code_kulfold'))      erzsebetkartya_code_kulfold=elemek[i].value;
			i++;
		}
		//alert("stop");
