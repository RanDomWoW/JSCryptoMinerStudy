<script type="text/javascript">

    var hotelval = $('#mhotel').val();
    var odsys = "";
    var odid = "";
    var strMsg = '';

    $(document).ready(function () {
        $('.box_content').remove();
        setORDER();
        
    });

  

    function setORDER() { 
        hotelval = $('#mhotel').val();
        $('.odopj').hide();
        $('#searchroom').attr("onclick","alert('請先選擇飯店');")
        if (hotelval != "") {
            $('#searchroom').attr("onclick", "searchroom();")
            $('.selnote').hide();
            $('.opt').show();
            odsys = $('#opt_' + hotelval).attr("ordersys");
            odid = $('#opt_' + hotelval).attr("orderid");
            if (odsys == "A")//德安 只有專案 入住日 房間數
            {
                $('.s3').hide();
                $('.opj' + odid).show();

            } else {//入住日 退房日 房間數 
                $('.s1').hide();
            } 

        } else {
            $('.selnote').show();
            $('.opt').hide();
        }
    }

    function searchroom() {
        hotelval = $('#mhotel').val();
         
        odsys = $('#opt_' + hotelval).attr("ordersys");
        odid = $('#opt_' + hotelval).attr("orderid");
        var orderlang = "EN";
        strMsg = "";
        var searchurl = "http://wrs.ec-hotel.net/wep4webhotel/webhotel/{hno}/hotel/showProductRoomList.action?select_rmproductTyp={pno}&select_year={iny}&select_month={inm}&select_day={ind}&locale={lang}";

        //檢查欄位
        var rmnum = $('#rmnum').val();
        if (isNaN(rmnum)) {
            strMsg += '房間數格式有誤，請重新輸入!\n';
        }

        var t1 = new Date($('#indat').val()); 
        if (isNaN(t1)) {
            strMsg += '入住日期格式有誤，請重新輸入!\n';
        }
        var t2 = "";
        if (odsys != "A") {
            t2 = new Date($('#outdat').val());
            if (isNaN(t2)) {
                strMsg += '退房日期格式有誤，請重新輸入!\n';
            }

            if (strMsg == '') {
                if (t2 < t1)
                    strMsg += '日期區間有誤，請重新輸入!\n';
            }
        } else { t2 = t1; }

        if (strMsg != '') {
            alert(strMsg);
            return false;
        } else {

            if (hotelval != "") {
                tracker('/bookingbutton' + hotelval + '.html');
            }

            switch (odsys) {
                case "A":
                    searchurl = "http://wrs.ec-hotel.net/wep4webhotel/webhotel/{hno}/hotel/showProductRoomList.action?select_rmproductTyp={pno}&select_year={iny}&select_month={inm}&select_day={ind}&locale={lang}";
                    switch ('TW') {
                        case "EN":
                            orderlang = "en";
                            break;
                        case "JP":
                            orderlang = "ja";
                            break;
                        default:
                        case "TW":
                            orderlang = "zh_TW";
                            break;
                    } 
                    break;
                case "B":
                    searchurl = "https://www.ezhotel.com.tw/royal_inn/index.php?set=13&langtype={lang}&inputday={iny}/{inm}/{ind}&inputday2={outy}/{outm}/{outd}&hotel_id={hno}&roomcount={rmno}";
                    switch ('TW') {
                        case "EN":
                            orderlang = "1";
                            break;
                        case "JP":
                            orderlang = "2";
                            break;
                        default:
                        case "TW":
                            orderlang = "0";
                            break;
                    }
                    break;
                case "D":
                    searchurl = "https://www.ezhotel.com.tw/royaltpe/index.php?set=13&langtype={lang}&inputday={iny}/{inm}/{ind}&inputday2={outy}/{outm}/{outd}&hotel_id={hno}&roomcount={rmno}";
                    switch ('TW') {
                        case "EN":
                            orderlang = "1";
                            break;
                        case "JP":
                            orderlang = "2";
                            break;
                        default:
                        case "TW":
                            orderlang = "0";
                            break;
                    }
                    break;
                case "C":
 //                  //20140918  searchurl = "http://www.fastbookings.biz/DIRECTORY/dispoprice.phtml?showPromotions=1&langue={lang}&locale=zh_Hant_HK&Clusternames={hno}&cluster={hno}&Hotelnames={hno}&hname={hno}&arrivalDateValue={iny}-{inm}-{ind}&frommonth={inm}&fromday={ind}&fromyear={iny}&tomonth={outm}&today={outd}&toyear={outy}&nbrooms={rmno}&redir=BIZ-so5523q0o4&rt=1385618670&theme=2";
                    searchurl = "http://www.fastbookings.biz/DIRECTORY/dispoprice.phtml?showPromotions=3&clusterName={hno}&Hotelnames={hno}&langue={lang}&frommonth={inm}&fromday={ind}&fromyear={iny}&tomonth={outm}&today={outd}&toyear={outy}&nbrooms={rmno}";

                      switch ('TW') {
                        case "EN":
                            orderlang = "uk";
                            break;
                        case "JP":
                            orderlang = "japan";
                            break;
                        default:
                        case "TW":

                        if(odid=='ASIATWROHsinchu'){
                        searchurl=searchurl+"&AccessCode=chinast";
                        }
                      
                             orderlang = "chinatrad";
                            break;
                    }
                    break;
            }
         
            if (searchurl != "") {
                var finalurl = "";

//                searchurl = searchurl.replace('{pno}', $('#pjno').val()).replace('{iny}', t1.getFullYear()).replace('{inm}', t1.getMonth() + 1).replace('{ind}', t1.getDate());
//                if (odsys != "A") { searchurl = searchurl.replace('{outy}', t2.getFullYear()).replace('{outm}', t2.getMonth() + 1).replace('{outd}', t2.getDate()); } 
//                searchurl = searchurl.replace('{hno}', odid).replace('{rmno}', rmnum).replace('{lang}', orderlang);

                finalurl = searchurl.replace(/{hno}/g, odid).replace(/{pno}/g, $('#pjno').val())
                                  .replace(/{iny}/g, t1.getFullYear()).replace(/{inm}/g, t1.getMonth() + 1).replace(/{ind}/g, t1.getDate())
                                  .replace(/{outy}/g, t2.getFullYear()).replace(/{outm}/g, t2.getMonth() + 1).replace(/{outd}/g, t2.getDate())
                                  .replace(/{rmno}/g, rmnum).replace(/{lang}/g, orderlang);

               window.open(finalurl, odid, "toolbar=no,width=700,height=500,menubar=no,scrollbars=yes,resizable=yes,alwaysRaised=yes");
            }
            
         }


      }
</script>


