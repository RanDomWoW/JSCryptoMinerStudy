<script>
try{
    var chatInterval = setInterval(function(){
      function readCookie(name) {
        var nameEQ = 'ib_'+name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
        }
        return null;
      }

      function createCookie(name,value,days) {

        if (days) {
          var date = new Date();
          date.setTime(date.getTime()+(days*24*60*60*1000));
          var expires = "; expires="+date.toGMTString();
        }else{
          var expires = "";
        }

        document.cookie = 'ib_'+name+"="+value+expires+"; path=/";
      }

      function eraseCookie(name) {createCookie(name,"",-1);}
    
    if(window['LC_API']){

      LC_API.on_chat_window_opened = function(){
        ga('gaTracker.send', 'event', 'Live Chat', 'Chat Window', 'Opened');
        ga('gtmTracker.send', 'event', 'Live Chat', 'Chat Window', 'Opened');
      };

      LC_API.on_chat_started = function(data) {
      ga('gaTracker.send', 'event', 'Live Chat', 'Chat Started', 'Agent name: ' + data.agent_name);
      ga('gtmTracker.send', 'event', 'Live Chat', 'Chat Started', 'Agent name: ' + data.agent_name);

      if(readCookie('chtStrt') === null){
        var theDate = new Date();
        createCookie('chtStrt', theDate.getTime(), 1);
      }
      };

      LC_API.on_chat_ended = function(){

        window.dataLayer = window.dataLayer || [];
      window.dataLayer.push({
        event: 'gtmUtilChatEnded'
      });

      var startTime = readCookie('chtStrt');
      var endTime = new Date();
      var chatLasted = Math.round((endTime-startTime)/10000);

      ga('gaTracker.send', 'event', 'Live Chat', 'Chat Ended', 'Lasted: '+chatLasted+' sec',chatLasted);
      ga('gtmTracker.send', 'event', 'Live Chat', 'Chat Ended', 'Lasted: '+chatLasted+' sec',chatLasted);
      };

      LC_API.on_ticket_created = function(data){

        window.dataLayer = window.dataLayer || [];
      window.dataLayer.push({
        event: 'gtmUtilChatTicketSent'
      });

      ga('gaTracker.send', 'event', 'Live Chat', 'Ticket Sent', 'Agent name: ' + data.ticket_id);
      ga('gtmTracker.send', 'event', 'Live Chat', 'Ticket Sent', 'Agent name: ' + data.ticket_id);
      };

      clearInterval(chatInterval);
    }
  }, 2000);

}catch(err){
  console.log(err);
  ga('gaTracker.send', 'event', 'Error - JS', err.name, err.message);
  ga('gtmTracker.send', 'event', 'Error - JS', err.name, err.message);
}



