<script>
/* <![CDATA[ */
$(function(){
    var msie = window.navigator.userAgent.indexOf('MSIE ');
	if (msie > -1 && msie <= 8) {
		// DON'T DOES THIS FOR <IE8
	} else {
		$('.tastecard_login_popup').bind('tc-popup-open', function(){ var ajaxbar = new Image(); ajaxbar.src = '/images/layout/icons/ajax-loader-bar.gif'; });
		$('.tastecard_login_popup form.login_form').submit(function(e){
			e.preventDefault();
			$(this).closest('.tc-popup-content').append(
				$('<div/>').addClass('tc-loading-overlay').append( $('<div/>').html('Signing in...<br><img src="/images/layout/icons/ajax-loader-bar.gif"/>') )
				);
			$.ajax({
				url: $(this).attr('action'),
				type: 'post',
				dataType: 'json',
				data: $(this).serialize() + '&response_format=json',
				xhrFields: {
					withCredentials: true
				},
				beforeSend: function( jqXHR, settings ) {
					// GA Track
					ga_login_submit('Onsite');
				},
				success: function( data ) {
					if ( typeof data.success != 'undefined' && data.success ) {
						// SUCCESS
						// GA Track
						ga_login_successful('Onsite', 'Yes', function(){
							location.reload(true);
						});
						
					} else if ( typeof data.errors != 'undefined' ) {
						// ERROR
						$('.tastecard_login_popup form.login_form .tastecard_login_status').addClass('error').html(data.errors.join('/'));
						$('.tastecard_login_popup .tc-loading-overlay').remove();
						$('.tastecard_login_popup form.login_form').trigger('tc-popup-refresh');
						$('.tastecard_login_popup form.login_form:visible input:visible').first().focus();
						
						// GA Track
						ga_login_failed('Onsite', data.errors.join('/'));

					} else {
						// DEFAULT ERROR
						
						// GA Track
						ga_login_failed('Onsite', 'Unable to sign in due to a technical fault [Invalid response]');
						
						if (confirm('Unable to sign in due to a technical fault. Please click OK to proceed to our sign in page.')) {
							window.location = '/user/login';
						} else {
							$('.tastecard_login_popup .tc-loading-overlay').remove();
						}
					}
				},
				error: function( jqXHR, textStatus, errorThrown ) {
				
					// GA Track
					ga_login_failed('Onsite', 'Unable to sign in due to a technical fault [' + textStatus + ': ' + errorThrown + ']');
				
					if (confirm('Unable to sign in due to a technical fault. Please click OK to proceed to our sign in page.')) {
						window.location = '/user/login';
					} else {
						$('.tastecard_login_popup .tc-loading-overlay').remove();
					}
				}
			});
		});
	}
});
/* ]]> */
</script>



						
															
