					$('.tc-popup-overlay[data-tc-popup-index-id="'+$(this).data('tc-popup-index-id')+'"]').fadeOut( function(){ $(this).remove(); } );
				})
				.bind('tc-popup-refresh', function(){ 
					if ($('.tc-popup-content', this).length > 0) {
						$('.tc-popup-content', this).css('max-height','none');
						var maxheight = $(this).height();
						if ($('.tc-popup-content', this).siblings().length > 0) {
							$('.tc-popup-content', this).siblings().each(function(){
								if ($(this).css('position') != 'absolute') {
									maxheight -= $(this).outerHeight(true);
								}
							});
						}
						$('.tc-popup-content', this).css('max-height', Math.ceil(maxheight));
					}
					$(this).trigger('tc-popup-center'); 
				})
				.bind('tc-popup-center', function(){
					$(this)
						.css({
							position: 'fixed',
							top: Math.max(0, (($(window).height() - $(this).outerHeight()) / 2)) + "px",
							left: Math.max(0, (($(window).width() - $(this).outerWidth()) / 2)) + "px"
							});
				})
				;

			var popup_ele = $(this);
			$( window )
				.resize(function(){ if (popup_ele.hasClass('tc-popup-open')) popup_ele.trigger('tc-popup-refresh'); })
				.on('orientationchange', function(){ $( window ).resize(); });
		});

		$('[data-tc-popup-open]').each(function(){
			$(this).click(function(e){
				e.preventDefault();
				var popup = $('.tc-popup[data-tc-popup-id="' + $(this).data('tc-popup-open') + '"]').first();
				if (popup.length > 0) {
					popup.trigger('tc-popup-open');
				}
			});
		});

	}
});

/* ]]> */
</script>




