$(document).ready(function () {

	$(".select_evt").bind("change", function () {
		var idx = $(this).data('index');
		if ($(this).prop('selectedIndex') > 0) {
			$(".check_evt")[idx].checked = true;
		} else {
			$(".check_evt")[idx].checked = false;
		}
	});

	$(".check_evt").change(function (evt) {
		var idx = $(this).data('index');
		var selectIdx = $(".select_evt")[idx].selectedIndex;


		if($(this).is(":checked")) {
			if (selectIdx < 1) $(".select_evt")[idx].selectedIndex = 1;
		} else {
			$(".select_evt")[idx].selectedIndex = 0;
		}
	});
});

