function fapply_textbook(f){
  if(f.mber_id.value == ""){
    alert("교재 신청은 로그인 후 가능합니다.");
    return;
  }

	var chk = document.getElementsByName('chk[]');
    var bchk = false;
    var schk = false;

    for (i=0; i<chk.length; i++)
    {
        if (chk[i].checked) {
            bchk = true;
			if (document.getElementsByName('text_num['+i+']')[0].value > 0) schk = true;
			else schk = false;
		}
    }

    if (!bchk) {
        alert("신청할 교재를 하나 이상 선택하세요.");
        return;
    }
    if (!schk) {
        alert("신청할 교재 수량을 선택하세요.");
        return;
    }

	f.action="./course/apply_madal_accounts.php";
	f.submit();

}

function add_basket(f){
	if(f.mber_id.value == ""){
		alert("장바구니 담기는 로그인 후 가능합니다.");
		return;
	}

	var chk = document.getElementsByName('chk[]');
	var bchk = false;
	var schk = false;

	for (i=0; i<chk.length; i++)
	{
		if (chk[i].checked) {
			bchk = true;

			if (document.getElementsByName('text_num['+i+']')[0].value > 0) schk = true;
			else schk = false;
		}
	}

	if (!bchk)
	{
		alert("장바구니에 담을 교재를 하나 이상 선택하세요.");
		return;
	}

	if (!schk) {
		alert("신청할 교재 수량을 선택하세요.");
		return;
	}

	window.open("", "add_Basket", "left=50,top=50,width=520,height=383");
	f.action = "./course/add_basket.php";
	f.target = "add_Basket";
	f.submit();
}
function select_textbook(num){
	var checkbox = document.getElementsByName('lecture_num[]');
	var t_checkbox = document.getElementsByName('t_lecture_num[]');

	var str = "document.apply_lecture.t_value"+num;
	var t_num = eval(str).value;


	num = num-1;
	if( checkbox[num].checked  ){
		if( !(t_num == 0 || t_num == 999999 || t_num == 888888) ){
			t_checkbox[num].disabled = false;
		}
	}else{
		t_checkbox[num].disabled = true;
		t_checkbox[num].checked = false;
	}
}
