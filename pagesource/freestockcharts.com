<script>// <![CDATA[

    $(function(){
        var emailChartID = getParameterByName("emailChartID");
        if (emailChartID)
        {
           window.location = "/platform/v1?emailChartID=" + emailChartID;
        }                 
        if (isChrome()){
            $(".chromeOnly").show();            
            $(".getSilverlight").hide();
        }
    })


    function getParameterByName(name) {
        name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
        var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
            results = regex.exec(location.search);
        return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
    }

  
    function isChrome(){
        // please note, that IE11 now returns undefined again for window.chrome
        var isChromium = window.chrome,
        vendorName = window.navigator.vendor;
        return (isChromium !== null && isChromium !== undefined && vendorName === "Google Inc.");
    }
                
</script>

    
    
