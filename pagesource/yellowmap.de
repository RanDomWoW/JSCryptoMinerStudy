<script id="popupTemplate" type="text/template">
		<div class="poiPopUp">
			<h4><a href="/Details/<%=BasicData.Identifiers.YMIDPathEncoded %>.aspx">
				<% if (BasicData.Address.Title) { %>
					<%= BasicData.Address.Title + " " %>
				<% }
				if (BasicData.Address.FirstName) { %>
					<%= BasicData.Address.FirstName + " " %>
				<% }
				if (BasicData.Address.CompanyName) { %>
					<%= BasicData.Address.CompanyName + " " %>
				<% }
				if (BasicData.Address.CompanyAddon1) { %>
					<%= BasicData.Address.CompanyAddon1 + " " %>
				<% }
				if (BasicData.Address.CompanyAddon2) { %>
					<%= BasicData.Address.CompanyAddon2 + " " %>
				<% }
				if (BasicData.Address.CompanyAddon3) { %>
					<%= BasicData.Address.CompanyAddon3 + " " %>
				<% } %>
			</a></h4>			
			<address>
				<%=BasicData.Address.Street %> <%=BasicData.Address.HouseNo %><br />
				<%=BasicData.Address.Zip %> <%=BasicData.Address.City %> <%=BasicData.Address.CityPart %>
				<% if (BasicData.Contact.Phone && utilities.trim(BasicData.Contact.Phone) != '') { %>
					<div><i class="ymicon-phone"></i>&nbsp;<%=BasicData.Contact.Phone %></div>
				<% } %>
			</address>
			<div class="MoreInfos">
				<% if (_.find(BasicData.MemoFields, function(item) { return item.Type === "SPECIAL_OFFER"; })) { %>
                    <% var offer = _.find(BasicData.MemoFields, function(item) { return item.Type === 'SPECIAL_OFFER'; }).Text.split('|'); %>
                    <% if (offer.length == 2) { %>
                    <div>Aktuelle Angebote:</div>
					<a href="<%=offer[0] %>" target="_blank">
                        <%=offer[1] %>
					</a>
                    <% } else if (offer.length == 3) { %>
                    <div>Aktuelle Angebote:</div>
                    <a href="<%=offer[2] %>" target="_blank">
                        <%=offer[0] %>
                    </a>
                    <% } %>
				<% } %>
				<% if (BasicData.AdditionalInfo.BusinessNews) { %>
				<div class="businessnews">
					<h5><span class="label label-businessnews"><i class="ymicon-rss"></i></span>&nbsp;Business-News</h5>
					<%=BasicData.AdditionalInfo.BusinessNews %>
				</div>
				<% } %>
				<% if (BasicData.MemoFields) { %>
					<% _.each(BasicData.MemoFields.MemoItems, function(value) { %>
						<% if (value.Type == 9) { %>
						<div class="openingHours">
							<h5><span class="label label-openinghours"><i class="ymicon-time"></i></span>&nbsp;&Ouml;ffnungszeiten</h5>
							<%=value.Text %>
						</div>
						<% } %>
					<% }); %>
				<% } %>
				<% if (typeof(Trackers) !== "undefined") { %>
					<% _.each(Trackers, function(value) { %>
						<% if (value.AddressProviderID == "YEXT") { %>
							<img src="http://pl.yext.com/plpixel?source=serp&action=click&target=mappin&pid=<%=value.TrackingID%>&ids=<%=value.ProviderForeignKey%>&property=<%=value.Application%>" width="1" height="1" />
						<% } %>
					<% }); %>
				<% } %>
			</div>
		</div>
	</script>

	
