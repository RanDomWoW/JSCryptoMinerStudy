<script>
ymaps.ready(init);

function init (url) 
{
    myMap = new ymaps.Map('ymap', {

            center: [55.76, 37.64],
            zoom: 12        }),

        

        objectManager = new ymaps.ObjectManager({
            clusterize: true,
            gridSize: 32
        });

	ymaps.geolocation.get({

		provider: 'yandex',
		mapStateAutoApply: true,
		json: true

		}).then(function (result) {
			//myMap.geoObjects.add(result.geoObjects);
		});

	objectManager.objects.options.set('preset', 'islands#redDotIcon');
	objectManager.clusters.options.set('preset', 'islands#redClusterIcons');

	myMap.geoObjects.add(objectManager);

	$.ajax({ url: "/md.json?1" }).done(function(data) {  objectManager.add(data); });

	myMap.controls.remove('searchControl');
//	myMap.removeControl('toolbar');
}



$(document).ready(function(){



	$( "#msearch" ).autocomplete({
		source: function(request, response){
			$.ajax({
				url: "/map/search/",
				dataType: "json",

				data:{
					featureClass: "P",
					style: "full",
					maxRows: 10,
					search: request.term
				},

				
				success: function(data) {
						response($.map(data.result, function(item){

							return{
								label: item.name,
								section: item.section,
								coords: item.coords,
							}
					}));
				},
				
			});
		},

		select: function(event, ui) { 	

			var str=ui.item.coords;
			var coordsarray=str.split(' ');
			myMap.setCenter([coordsarray[1], coordsarray[0]],17);

			if (ui.item.section=="Магнит у дома") showMapSection(1,"/md.json");
			if (ui.item.section=="Магнит Косметик") showMapSection(2,"/mk.json");
			if (ui.item.section=="Магнит Семейный") showMapSection(5,"/ms.json");


		},

		focus: function( event, ui ) {

			if(isMobile.any()){

				var str=ui.item.coords;
				var coordsarray=str.split(' ');
				myMap.setCenter([coordsarray[1], coordsarray[0]],17);
				$( "#msearch" ).autocomplete( "close" );
			}
		},
		  		
		delay: 300,
		minLength: 3
	})
	.autocomplete( "instance" )._renderItem = function( ul, item ) {
      		return $( "<li>" ).append( "<div class='ui-fias-item'>" + item.label + "<span class='ui-fias'>" + item.section + "</span></div>" ).appendTo( ul );
    	};


})

</script>
