<script>
jQuery(document).ready(function() {
    
    jQuery('main.content').find('a').each(function(e) {
        var strVal = jQuery(this).attr('href');
        var arr = strVal.split('/');
        if(arr.length==1){
            jQuery(this).attr('href','https://cafeastrology.com/wpsite/index/'+arr[0]);
            jQuery(this).removeClass('broken_link');
        }else if(arr.length==2 && arr[0]=='birthday'){
            jQuery(this).attr('href','https://cafeastrology.com/wpsite/'+strVal);
            jQuery(this).removeClass('broken_link');
        }
    });


    jQuery('.site-footer1 a').each(function(){
        var url = jQuery(this).attr('href');

        if(url.indexOf("cafeastrology.com") >= 0){
            url = url.replace('https://www.cafeastrology.com', 'https://cafeastrology.com/wpsite');
            jQuery(this).attr('href', url);
        }   
    }); 
}); 

</script>