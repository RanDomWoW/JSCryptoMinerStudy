				function projectSpesificErrorHandler (errors, action) {
					if (action == 'login') {
						var message = ''
						$.each(errors, function(type, error) {
							message += ' '+ error;
						});
						$('#wrongInfoLogin').html(message).show();
					} else if (action == 'register') {
						onRegistrationError();
						$('input.wrong-info').removeClass('wrong-info');
