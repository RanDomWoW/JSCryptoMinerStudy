				$('.popup-overlay').show();
			}

			function hidePartBox (type) {
				$('#'+ type).hide();				
				$('.popup-overlay').hide();
			}

			$.urlParam = function(name){
				var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(window.location.href);
				if (results==null) {
					return null;
				}
				else {
					return results[1] || 0;
				}
			}
			
