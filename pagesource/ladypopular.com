							if (type == 'Terms and Conditions') {
								$('#termsStatus').html(error).removeClass('valid').show();
								$('#termsStatus .errow').show();
							} else {
								$('#reg'+upperFirstLetter(type)).removeClass('valid-info').addClass('wrong-info');
								$('#reg_'+type).removeClass('valid-info').addClass('wrong-info');
								$('#register_'+type).html(error).removeClass('valid').show();
								$('#register_'+type+' .errow').show();
