                  <script type="text/javascript">


    function subscribe() {
        url = '/ajax.aspx?m=Nv.SqlModule&name=Newsletter';

        $.ajax({
            url: url,
            type: "GET",
            dataType: "html",
            data: ({ 's': $('#s').val() }),
            success: function (data) {
                var final_msg = data;

                if (final_msg.indexOf('#') == -1) {
                    $(".errorMsg").css("display", "none");
                    $(".okMsg").css("display", "block");
                    $(".okMsg").html(final_msg.replace("#", ""));
                }
                else {
                    $(".errorMsg").css("display", "block");
                    $(".okMsg").css("display", "none");
                    $(".errorMsg").html(final_msg.replace("#",""));
                }
               // alert(final_msg);

            }
        });

        return false;
    }


  </script>

