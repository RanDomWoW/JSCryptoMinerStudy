}

function do_search() {
    if ($('#searchbox').val().length < 3 || jQuery('#searchbox').val() == "ΑΝΑΖΗΤΗΣΗ") {
        alert('Η λέξη προς αναζήτηση πρέπει να έχει μήκος μεγαλύτερο από 2 χαρακτήρες.')
