<script type="text/javascript">

$(document).ready(function() {
  
    $('#loginform').submit(function() {
      //alert('Handler for .submit() called.');
      return false;
    });
 
});

    function email_IsEmpty(){
     if($('#email').val() == "Username"){
        $('#email').val("");
      }
    }            
    function email_onblur(){
     if($('#email').val() == ""){
        $('#email').val("Username");
      }
    }           
     
    function password_IsEmpty(){
     if($('#password').val() == "Password"){
        $('#password').val("");
      }
    }            
    function password_onblur(){
     if($('#password').val() == ""){
        $('#password').val("Password");
      }
    }                   
                

    function ShowHideloginBox() {

        if ($('#loginbutton').attr('class') == 'logIn') {
           $('#loginbutton').removeClass('logIn');
           $('.logIn_popUp').css("display", "none");
        } else {
            $('#loginbutton').addClass('logIn');
            $('.logIn_popUp').css("display", "block");
        }


        return false;
    }



    function TryLogin() {
        url = '/ajax.aspx?m=Nv.SqlModule&name=makeLogin';

        $.ajax({
            url: url,
            type: "GET",
            dataType: "html",
            data: ({ 'email': $('#email').val(), 'password': $('#password').val() }),
            success: function (data) {
                var final_login_msg = data;
               
                if (final_login_msg.indexOf('#') == -1) {
                   $("#mainLoginPane").html($("#divislogin").html());
                    $(".username0 span").html("<a href='/aggelies/profile/'>" + final_login_msg + "</a>"); 
                     fLogin() ;

                  if (!($('#my_aggelies_button') == undefined)){
                    $('#my_aggelies_button').attr('href',$('#default_href_List').attr('href') + '?mylist=1');
                    $('#my_aggelies_button').unbind('click');
                    $('#my_profile_button').unbind('click');
                    $('#my_submit_button').unbind('click');
                  }
                  
                  SetCookie('Login');
                  
                }
                else {
                      alert(final_login_msg.replace("#", ""));
                     
                }
      

            }
        });

        return false;
    }

function TryLogout() {
        url = '/ajax.aspx?m=Nv.SqlModule&name=Logout';

        $.ajax({
            url: url,
            type: "GET",
            dataType: "html",
            data: ({ 'logout': '1'}),
            success: function (data) {
                var final_login_msg = data;
                 $("#mainLoginPane").html($("#divnotlogin").html());
                 $('.logIn_popUp').css("display", "none");
                      jQuery.cookie('Login',null, { path: '/' });
                      if (!($('#my_aggelies_button') == undefined)){
                            
                         if(NvPage.PageId == 147 || NvPage.PageId == 148 || NvPage.PageId == 149){
                          window.location.href = $('#default_href_List').attr('href');
                         }
                         
                      }
            }
        });

        return false;
    }



function fLogin() {
        url = '/ajax.aspx?m=Nv.SqlModule&name=fMakeLogin';

        $.ajax({
            url: url,
            type: "GET",
            dataType: "html",
            data: ({ 't': '1'}),
            success: function (data) {
                 SetCookie('Login');
                var final_login_msg = data;
                  $('<iframe />', {
                        'name': 'myFrame',                      
                        'id':   'myFrame',
                        'width': '0',
                        'height': '0',
                        'border': '0',
                        'frameborder': '0',
                        'scrolling': 'no',
                        'src': final_login_msg
                          }).appendTo('body');
                      }
           });
 }
        
  

</script>

