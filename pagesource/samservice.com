        $('#totalSearchField').keyup(function (event) {
            if (event.keyCode == 13) {
                totalSearch();
            }
            else {
                if ((($(this).val().charCodeAt(0) > 0x600) && ($(this).val().charCodeAt(0) < 0x6FF)) || ($(this).val() == "")) { $('#totalSearchField').css("direction", "rtl"); }
                else { $('#totalSearchField').css("direction", "ltr"); }
            }
        }
