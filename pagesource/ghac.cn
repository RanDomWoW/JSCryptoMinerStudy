    function showHomeVideo() {
        if (isMobile.Status() == "desktop") {
            //Pc版
            var s1 = new SWFObject("/video/mediaplayer.swf", "mediaplayer", "716", "403", "8");
            s1.addParam("allowfullscreen", "true");
            s1.addVariable("width", "716");
            s1.addVariable("height", "403");
            s1.addVariable("file", "/video/EHomeVideo.flv");
            s1.write("videoContainer");
        } else {
            //手机版
            $(".showbox-video").css("margin-left", 0).css("left", 10);
            var videoContent = "<video id=\"odysseyVideo\" src=\"/video/EHomeVideo.mp4\" style=\"width:100%\" controls=\"controls\">您的浏览器不支持视频播放</video>";
            $("#videoContainer").html(videoContent);
        }
        $.showPopup(".showbox-video");
        //统计访问量
        _hmt.push(['_trackEvent', 'video', 'play', 'EHomeVideo',1]);
    }

    $.closePopup = function (obj) {
        $(obj).fadeOut(200);
        $(".popupBg").hide();
        $("#videoContainer").html("");
    }


