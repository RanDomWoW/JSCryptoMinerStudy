<script>
    $(function () {
        var menu_top_height = $(window).height()-323;
        $(".chengdiv").attr("style","height:"+menu_top_height+"px");
    });

    function slideUp(){
        var top_top_logo = $(window).height()/2-200;
        var top_top_search = $(window).height()/2-50;
        $(".top_logo").animate({left:'430px',top:top_top_logo},1500);
        $(".top_search").animate({right:'350px',top:top_top_search},1500);
    }

    function slideDown(){
        $(".top_logo").animate({left:'0',top:'20px'},1500);
        $(".top_search").animate({right:'0',top:'44px'},1500);
        $(".chengdiv").slideUp(1500);
//      $(".box_footer").slideDown(1500);
        $(".box_footer").animate({height:'636px'},1500);

    }
    setTimeout(slideUp,1500);
    setTimeout(slideDown,5000);

    $(function(){
        $(".nav ul li").hover(function () {
            setTimeout(slideDown,200);
        });
    });
</script>

