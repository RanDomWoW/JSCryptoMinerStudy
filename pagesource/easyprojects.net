				if (isInsertDone) {return true};
				var insert_wistia = function () {
					$video_popup.tmpl('#tmpl-wistia');
					setTimeout(function() {
						$video_popup.removeClass('is_loading');
					}, 150);

					isInsertDone = true;
				}
				var load_json = function() {
					lazy_load_scripts("//fast.wistia.com/embed/medias/ba707gi3m5.jsonp", insert_wistia);
				}
				lazy_load_scripts("//fast.wistia.net/assets/external/E-v1.js", load_json);
			}
			$(function() {
				$('input').each(function() {
					if($(this).val() !== '') $(this).closest('.input-block').addClass('active');
				});
				$('.input-block input').focus(function() {
					$(this).closest('.input-block').addClass('active');
				});
				$('.input-block input').blur(function() {
					if(!this.value) {
						$(this).closest('.input-block').removeClass('active');
					}
				});
				
				$('.tab-block').on('click', function() {
					$('.tab-block').removeClass('active');
					$(this).addClass('active');
					var contentNum = $(this).attr('data-content');
					$('.tab-content').removeClass('active');
					$($('.tab-content')[contentNum]).addClass('active');
				});

				$('.video-block').on('click', function(e) {
					if (!isInsertDone) {
						$video_popup.addClass('is_loading');
					}
					insert_wistia_embed();
					e.preventDefault();
					$video_popup.fadeIn();
				});

				$video_popup.on('click', function(e) {
					if(e.target !== $(this)[0]) return;
					$(this).fadeOut();
				});
			});
