function timeslots(datefortime, service, duration, location_val)
{
		$.ajax({
		url: 'index.php?route=information/appointment/getTimeSlots',
		type: 'post',
		dataType: 'json',
		data: 'datefortime='+datefortime+'&duration='+duration+'&service='+service+'&location_val='+location_val,
		beforeSend: function() {
			$('.success, .warning, .error').remove();
			$('#timeslot_load_icon').show();
		},
		complete: function() {
			$('#timeslot_load_icon').hide();
		},
		success: function(data) {
		    if (data['success']) {		
				document.getElementById("timings").innerHTML = data['success'];
				$('#timings li[rel="'+ document.getElementById('schedule_time').value + '"]').find('a').css('color','#ce9500');
		    }
		}
	});
}
