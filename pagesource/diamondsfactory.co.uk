	function setcookie()
	{
	var hidden_infinityid = $("#hidden_infinityid").val();
	document.cookie = "infinityvisitorid=" + hidden_infinityid + ";expires=30;path=/";
	}

	function appendGclid()
	{
		var gclid = $("#hidden_gclid").val();
		if(gclid != '')
		{
			$("#SnapABug_W").remove();
			$("#SnapABug_WP").remove();
			insertParam('gclid', gclid+'&openchat=yes');
			//window.location.href = window.location.href+'?gclid='+gclid+'&openchat=yes';
			return false;
		}
	}

	//To append the gclid to current URL
	function insertParam(key, value)
	{
		key = encodeURI(key); value = encodeURI(value);

		var kvp = document.location.search.substr(1).split('&');

		var i=kvp.length; var x; while(i--)
		{
			x = kvp[i].split('=');

			if (x[0]==key)
			{
				x[1] = value;
				kvp[i] = x.join('=');
				break;
			}
		}

		if(i<0) {kvp[kvp.length] = [key,value].join('=');}

		//this will reload the page, it's likely better to store this until finished
		document.location.search = kvp.join('&');
	}
</script>
