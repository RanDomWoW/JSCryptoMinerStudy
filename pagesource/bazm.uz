			jQuery('#calc table tr').mouseout(function(){
				var dollar = jQuery('input[name=kurs]').val();
					price = jQuery(this).find('td.price input').val();
					count = jQuery(this).find('td.count input').val();
					paid = jQuery(this).find('td.paid input').val();

					if (price != 'undefined' && count != 'undefined' && price >= 0 && count >= 0) {
						jQuery(this).find('td.total input').val(price*count);
						jQuery(this).find('td.total2 input').val(Math.ceil((price*count)/dollar));

						if (paid >= 0 && paid !='undefined') {
							jQuery(this).find('td.need input').val((price*count)-paid);
						}
						var sum = 0;
						var sum_need = 0;
						//Считаем сумму
						jQuery(this).parent().find('td.total input').each(function(){
							sum += parseFloat(jQuery(this).val());
						});
						jQuery(this).parent().find('td.need input').each(function(){
							sum_need += parseFloat(jQuery(this).val());
						});
						jQuery(this).parent().find('td.sum input').val(sum);
						jQuery(this).parent().find('td.sum2 input').val(Math.ceil(sum/dollar));
						jQuery(this).parent().find('td.sum3 input').val(sum_need);
					}
					//Считаем общую сумму
					var itog = 0;
					var itog_paid = 0;
					jQuery('#calc table tr td.sum input').each(function(){
						itog += parseFloat(jQuery(this).val());
					});
					jQuery('#calc table tr td.need input').each(function(){
						itog_paid += parseFloat(jQuery(this).val());
					});
					//console.log(itog);
					jQuery('#calc .itog').text(formatNumber(itog));
					jQuery('#calc .itog2').text(formatNumber(Math.ceil(itog/dollar)));
					jQuery('#calc .itog3').text(formatNumber(itog_paid));
					jQuery('#calc .itog4').text(formatNumber(Math.ceil(itog_paid/dollar)));

			});

