<script>
	$('.p-top-interview img').each(function(){
		$(this).load(function(){
			var frameX = $('.p-top-interview .featured-thumbnail').width();
			var frameY = $('.p-top-interview .featured-thumbnail').height();
			var ratioX = frameX / $(this).width();
			var ratioY = frameY / $(this).height();
			var fitX;
			var fitY;
			var fitLeft;
			var fitTop;
			if(ratioX > ratioY) {
			    fitX = frameX;
			    fitY = $(this).height() * (frameX / $(this).width());
			} else {
			    fitX = $(this).width() * (frameY / $(this).height());
			    fitY = frameY;
			}
			fitLeft = (frameX - fitX) / 2;
			fitTop = (frameY - fitY) / 2;
			//$(this).css('cssText':'width:' + fitX + 'px !important; height:' + fitY + 'px !important; left:' + fitLeft + 'px; top:' + fitTop + 'px; position:relative;');
			if(ratioX > ratioY) {
			} else {
				$(this).css('width', fitX);
			}
			$(this).css('left', fitLeft);
			$(this).css('top', fitTop);
			$(this).css('position', 'relative');
		});
	});
</script>


