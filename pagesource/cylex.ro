            $('#lawyersHeading').attr('class','h1 hidden');
            $('#petrolstationHeading').changeElementType('p');
            $('#petrolstationHeading').attr('class', 'h1 hidden');
            $("#search-what").css("display","block");
          break;
          case 'lawyers':
            $('#lawyersHeading').changeElementType('h1');
            $('#lawyersHeading').attr('class', 'hugeh-heading');
            $('#doctorsHeading').changeElementType('p');
            $('#doctorsHeading').attr('class', 'h1 hidden');
            $('#directoryHeading').changeElementType('p');
            $('#directoryHeading').attr('class', 'h1 hidden');
            $('#petrolstationHeading').changeElementType('p');
            $('#petrolstationHeading').attr('class', 'h1 hidden');  
            $("#search-what").css("display","block")
          break;
          case 'petrolstation':
            $('#petrolstationHeading').changeElementType('h1');
            $('#petrolstationHeading').attr('class', 'hugeh-heading');
            $('#directoryHeading').changeElementType('p');
            $('#directoryHeading').attr('class', 'h1 hidden');            
            $('#doctorsHeading').changeElementType('p');
            $('#doctorsHeading').attr('class', 'h1 hidden');
            $('#lawyersHeading').changeElementType('p');
            $('#lawyersHeading').attr('class', 'h1 hidden');
            $("#search-what").css("display","none");
          break;          
        }      
      }
      
    </script><script type="text/javascript">
      

      function InitCatalogs()
      {
        $('.material-card > .mc-btn-action, img.big-image').unbind();
        $('.material-card > .mc-btn-action, img.big-image').click(function () {
            var card = (($(this).parent('.material-card').length == 0) ? $(this).closest(".material-card") : $(this).parent('.material-card'));
            var icon = (($(this).children('i').length==0) ? $(this).closest(".material-card").find("i.fa-info-circle") : $(this).children('i'));
            var date = (($(this).parent('.material-card').find('.catalog-aditional-info').length == 0) ? $(this).closest(".material-card").find(".catalog-aditional-info") : $(this).parent('.material-card').find('.catalog-aditional-info'));
            icon.addClass('fa-spin-fast');

            if (card.hasClass('mc-active')) {
                card.removeClass('mc-active');
                date.show();
                window.setTimeout(function() {
                    icon
                        .removeClass('fa-arrow-left')
                        .removeClass('fa-spin-fast');

                }, 800);
            } else {
                card.addClass('mc-active');
                date.hide();
                window.setTimeout(function() {
                    icon
                        .removeClass('fa-spin-fast')
                        .addClass('fa-arrow-left');

                }, 800);
            }
        });
      }

      $(function() {
        InitCatalogs();
    });
      
      </script><script type="text/javascript">
      
       function OpenCatalogModal()
      {
           $(".open-catalog-modal").click(function(e){
              e.preventDefault();
              $('#catalogModal').addClass('in loading').show();         
              $("#catalogIframe").attr('src',this);
              $('#catalogModal').modal('show');
            });
          
            $("#catalogModal").find(".close").click(function(){
              $('#catalogModal').removeClass('loading');
              $("#catalogIframe").attr("src", null);
            });
       }
       
       $(document).ready(function(){ OpenCatalogModal(); });
       
      
    </script></body></html>