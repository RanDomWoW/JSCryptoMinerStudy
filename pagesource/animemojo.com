<script type="text/javascript">
    $("#hLazyHeadlines").attr("class", "grid");

    var l_hPage = 1;
    var l_hLoading = false;
    Hide('hLazyHeadlinesLoading');
    

    function HeadlinesLoad(ignoreScroll) {
        if (!l_hLoading && (IsVisible('dhTrigger') || ignoreScroll)) {
            l_hLoading = true;
            l_hPage++;
            GetHeadlines();
            setTimeout(function () { l_hLoading = false }, 1000);
        }
    }

    function GetHeadlines() {
        Show('hLazyHeadlinesLoading');
        
        Hide('lazyHButton');
        

        $.getJSON("/plugins/SHeadlinesFetcher.ashx?cat=0&type=0&a=&userowned=False&p=" + l_hPage + "&thumbs=True&show=12&cb=201831735",
            { get_param: "headlines" },
            function (data) {
                $.each(data, function (index, headline) {
                    var l_headline = document.createElement("div");
                    l_headline.id = "headline-" + headline.ID;
                    l_headline.className = "headline";
                    l_headline.innerHTML = "<a href='" + headline.URL + "'><picture>" + 
                        //"<source media='(min-width: 480px)' srcset='" + headline.BannerURL + "'>" +
                        "<source media='(min-width: 0px)' srcset='" + headline.ThumbURL + "'>" +
                        "<img src='" + headline.ThumbURL + "' class='image' alt='" + headline.TitleNoHTML.replace(/'/g, "") + "' />" + 
                        "</picture></a>" +
                        "<div class='byline'>" +
                        "<div class='author'>" + headline.AUName + "</div>" +
                        "<div class='time'>" + headline.PDate + "</div>" +
                        "</div>" +
                        "<a href='" + headline.URL + "' class='title'>" + headline.Title + "</a>";
                        
                    
                    l_headline.innerHTML = l_headline.innerHTML + "<div class='summary'>" + headline.Summary + "</div>";
                    

                    $('#hLazyHeadlines').append(l_headline);
                });

                Hide('hLazyHeadlinesLoading');
                
                Show('lazyHButton');
                
            });
    }

    function ChangeHView(a_view)
    {
        $("#hLazyHeadlines").attr("class", a_view);
        Cookies.set('HViewMode', a_view, { expires: 30 });
    }

    $(document).ready(GetHeadlines());
</script>


