<script type="text/javascript">
    var tSCItem = 0;
    var tSPItem = 0;
    var tSTimerOn = false;

    function TSShow(a_id) {
        tSPItem = tSCItem;
        tSCItem = a_id;

        $('#tSItem' + tSPItem).attr('class', 'tItem');
        $('#tSItem' + tSCItem).attr('class', 'tActiveItem');

        $('#tImageLNK').attr("href", $('#tSItem' + tSCItem).data('tsaurl'));
        $('#tStories').css("background-image", "url('" + $('#tSItem' + tSCItem).data('tsabannerurl') + "')");
        
        $('#tTitleLNK').attr("href", $('#tSItem' + tSCItem).data('tsaurl'));
        $('#tTitleLNK').text($('#tSItem' + tSCItem).data('tsatitlenohtml'));
        $('#tCatLNK').attr("href", $('#tSItem' + tSCItem).data('tscurl'));
        $('#tCatLNK').text($('#tSItem' + tSCItem).data('tscname'));
        $('#tCatLNK').text($('#tSItem' + tSCItem).data('tscname'));
        $('#tCatLNK').text($('#tSItem' + tSCItem).data('tscname'));
        $('#tTimeTXT').text($('#tSItem' + tSCItem).data('tsaptime'));
    }


    function TSTimerToggle() {
        if (!tSTimerOn) {
            tSTimerInt = setInterval("TSNext()", 4000);
            tSTimerOn = true;
        }
        else {
            clearInterval(tSTimerInt);
            tSTimerOn = false;
        }
    }

    function TSNext() {
        if (tSCItem >= $('#tNav div').length - 1) {//start at first
            TSShow(0);
        }
        else {//show the next one
            TSShow(tSCItem + 1);
        }
    }

    function TSStart() {
        $.getJSON("/plugins/TopHeadlinesFetcher.ashx?show=3&cb=201831730",
            { get_param: "headlines" },
            function (data) {
                $.each(data, function (index, headline) {
                    var l_item = document.createElement('div');
                    l_item.id = 'tSItem' + index;
                    l_item.onmouseover = function () { TSShow(index); };
                    l_item.innerHTML = '&#9679';
                    l_item.className = 'tItem';
                    l_item.setAttribute('data-tsaurl', headline.URL);
                    l_item.setAttribute('data-tsatitle', headline.Title);
                    l_item.setAttribute('data-tsatitlenohtml', headline.TitleNoHTML);
                    l_item.setAttribute('data-tsabannerurl', headline.BannerURL + "?cb=201831735");
                    l_item.setAttribute('data-tsathumburl', headline.ThumbURL);
                    l_item.setAttribute('data-tscurl', headline.CURL);
                    l_item.setAttribute('data-tscname', headline.CName);
                    l_item.setAttribute('data-tsaptime', headline.PDate);

                    $.get(headline.BannerURL);

                    $('#tNav').append(l_item);
                });

                TSShow(0);
                TSTimerToggle();

                Hide('tLoading');
                Show('tWrapper');
            });
    }
</script>
