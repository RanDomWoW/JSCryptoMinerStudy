						return -1;
					}
					if(an < bn) {
						return 1;
					}
					return 0;
	    		}

		    	$container = $("#social .wrapper");
		    	$items = $("#social .wrapper > .item").not(".w2");
		    	$viditems = $("#social .wrapper > .item.w2");

		    	$items.sort(timesort);
		    	$items.detach().appendTo($container);

