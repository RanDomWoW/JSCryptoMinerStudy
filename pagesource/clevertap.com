<script>
    $(document).ready(function(){
        var formId = '643700df-e938-4d6f-85dc-734ee3c7a2e7';
        if(!window.hbspt){
            $.getScript('//js.hsforms.net/forms/v2.js' , function(){
                hbspt.forms.create({
                    css: '',
                    portalId: '897267',
                    formId: formId,
                    onFormReady : function($form, cts){
                        $form.hide();
                        $('[data-modal="signUpP"] #sdfirstname').val( $form.find('[name="firstname"]').val() );
                        $('[data-modal="signUpP"] #sdlastname').val( $form.find('[name="lastname"]').val() );
                        $('[data-modal="signUpP"] #sdemail').val( $form.find('[name="email"]').val() );
                        $('[data-modal="signUpP"] #sdphone').val( $form.find('[name="phone"]').val() );
                    }
                });

            });
        }


        function validateEmail(email) {
            var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\. [0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            return re.test(email);
        }

        var rejectList = ["gmail", "yahoo", "hotmail", "rediff", "email"];
        function validateEmailDomain(emailAddr) {
            var email = emailAddr;
            var emailDomain = '';
            if (email.split('@')[1]) {
                emailDomain = email.split('@')[1].split('.')[0];
                if (rejectList.indexOf(emailDomain) >= 0) {
                    return false;
                }
            }
            return true;
        }

        $('#schedule_demo_p').click(function(){

            var $firstname = $('[data-modal="signUpP"] #sdfirstname');
            var $lastname = $('[data-modal="signUpP"] #sdlastname');
            var $email = $('[data-modal="signUpP"] #sdemail');
            var $phone = $('[data-modal="signUpP"] #sdphone');

            $firstname.css("border", $firstname.val() === "" ?"solid 1px #b30324":"solid 1px #f2eaf6");
            $firstname.on('click',function(event){
                $firstname.css("border", "solid 1px #f2eaf6");
            });
            $lastname.css("border", $lastname.val() === "" ?"solid 1px #b30324":"solid 1px #f2eaf6");
            $lastname.on('click',function(event){
                $lastname.css("border", "solid 1px #f2eaf6");
            })
            $email.css("border", ($email.val() === "" || !validateEmail($email.val()) || !validateEmailDomain($email.val())) ?"solid 1px #b30324":"solid 1px #f2eaf6");
            $email.on('click',function(event){
                $email.css("border", "solid 1px #f2eaf6");
            })
            $phone.css("border", $phone.val() === "" ?"solid 1px #f2eaf6":"solid 1px #f2eaf6");
            $phone.on('click',function(event) {
                $phone.css("border", "solid 1px #f2eaf6");
            })

            var $form = $('#hsForm_'+formId);
            if($firstname.val() && $lastname.val() && $email.val() && validateEmail($email.val()) && validateEmailDomain($email.val())){
                var phone_number = $phone.val() || '';
                $form.find('[name="firstname"]').val($firstname.val());
                $form.find('[name="lastname"]').val($lastname.val());
                $form.find('[name="email"]').val($email.val());
                $form.find('[name="phone"]').val(phone_number);
                $form.trigger('submit');
                $('.schedule-form').hide();
                typeof invokeGoogleAdwordsScript === 'function' && invokeGoogleAdwordsScript(967950620, 'gA0FCO_9_nAQnILHzQM');
                $('.schedule-demo-thankyou').show();

            }

        });


        var btn = document.getElementById('js-linkedinSignIn');
        btn.addEventListener('click', function() {
            var $form = $('#hsForm_' + formId);
            IN.User.authorize(function(data) {
                // Handle the successful return from the API call
                function onSuccess(profile) {
                    console.warn(profile);
                    var data = profile;

                    var firstname = data.firstName || '';
                    $form.find('[name="firstname"]').val(firstname);

                    var lastname = data.lastName || '';
                    $form.find('[name="lastname"]').val(lastname);

                    var email = data.emailAddress || '';
                    $form.find('[name="email"]').val(email);

                    $form.find('[name="phone"]').val('');

                    $form.trigger('submit');
                    $('.schedule-form').hide();
                    $('.schedule-demo-thankyou').show();

                }
                // Handle an error response from the API call
                function onError(error) {
                    console.log(error);
                    alert("Something went wrong. Please try again!");
                }
                IN.API.Raw("/people/~:(first-name,last-name,email-address,location,public-profile-url,picture-url,positions:(id,title,summary,start-date,end-date,is-current,company:(id,name,type,size,industry,ticker)))").result(onSuccess).error(onError);
            }, {
                scope: ['r_emailaddress', 'r_basicprofile', 'rw_company_admin']
            });

        });

    });
</script>

