<script>

    $(document).ready(function(){
        var formId = "643700df-e938-4d6f-85dc-734ee3c7a2e7";
        if(!window.hbspt){
            $.getScript('//js.hsforms.net/forms/v2.js' , function(){
                hbspt.forms.create({
                    css: '',
                    portalId: '897267',
                    formId: formId,
                    onFormReady : function($form, cts){
                        $form.hide();
                        $('.sdfooter #sdfirstname').val( $form.find('[name="firstname"]').val() );
                        $('.sdfooter #sdlastname').val( $form.find('[name="lastname"]').val() );
                        $('.sdfooter #sdemail').val( $form.find('[name="email"]').val() );
                        $('.sdfooter #sdphone').val( $form.find('[name="phone"]').val() );
                    }
                });
            });
        }
        function validateEmail(email) {
            var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.	[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            return re.test(email);
        }

        $('#footer-schedule-demo').on('click',function(){
            var $firstname = $('.sdfooter #sdfirstname');
            var $lastname = $('.sdfooter #sdlastname');
            var $email = $('.sdfooter #sdemail');

            $firstname.css("border", $firstname.val() === "" ?"solid 1px #b30324":"solid 1px #f2eaf6");
            $firstname.on('click',function(event){
                $firstname.css("border", "solid 1px #f2eaf6");
            });
            $lastname.css("border", $lastname.val() === "" ?"solid 1px #b30324":"solid 1px #f2eaf6");
            $lastname.on('click',function(event){
                $lastname.css("border", "solid 1px #f2eaf6");
            })
            $email.css("border", ($email.val() === "" || !validateEmail($email.val()) || !ctValidateEmailDomain($email.val()) ) ?"solid 1px #b30324":"solid 1px #f2eaf6");
            $email.on('click',function(event){
                $email.css("border", "solid 1px #f2eaf6");
            })

            var $form = $('#hsForm_'+formId)
            if($firstname.val() && $lastname.val() && $email.val() && validateEmail($email.val()) && ctValidateEmailDomain($email.val()) ){
                $form.find('[name="firstname"]').val($firstname.val());
                $form.find('[name="lastname"]').val($lastname.val());
                $form.find('[name="email"]').val($email.val());
                $form.trigger('submit');
                $('.sdfooter').hide();
                $('.thanktyoutext').show();
            }
        })

    });
</script><!-- getting started form-->

