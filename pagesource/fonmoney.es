    var country = countriesTypeHead[0].value;

    jQuery('#country').val(country);

    var countryLink = 'banking/transfer/index/country/';

    function findCountry(countryName){

        var result = jQuery.grep(countriesTypeHead, function(e){
            if (e.value) {
                var value = e.value.toLowerCase();
                return value == countryName.toLowerCase();
            } else return false;
        });

        console.log("result: "+result);
        return result[0];
    }

    function startTransfer() {
        country = findCountry(jQuery('#country').val()).origValue;
        var countryNormalized = country.toLowerCase().replace(' ', '-');
        document.location = '/banking/ajax/bankingEnabled/country/' + countryNormalized;
    }

    jQuery('.typeahead').bind('typeahead:select', function(ev, suggestion) {
        country = findCountry(suggestion.value).origValue;
    });

    jQuery('.typeahead').bind('typeahead:change', function(ev, value) {
        var ct = findCountry(value);

        if(ct !== undefined) {
            country = ct.origValue;
        }
    });

    jQuery('.typeahead').bind('typeahead:close', function(ev, suggestion) {
        var code = jQuery('.country-slider').find('.slick-center').data('code');
        countryObj = _.findWhere(countriesTypeHead, {'code': code});
        if(countryObj !== undefined) {
            country = countryObj.origValue;
            jQuery('#country').val(countryObj.value);
        }
    });

    jQuery('.typeahead').bind('typeahead:active', function(ev) {
        jQuery('.typeahead').typeahead('val', '');
        jQuery('#country').val('');
    });

    jQuery('.typeahead').on('keyup', function(e) {
        if(e.which == 13) {
            jQuery(jQuery(".tt-dataset .tt-suggestion")[0]).trigger('click');
        }
    });
</script>	</div>
