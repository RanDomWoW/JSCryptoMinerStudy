<script>
        $( document ).ready(function() {
            var alto = $( window ).height();
            var lastScrollLeft = 0;
            $('.banner_i').css('top',($(window).scrollTop()+Math.floor((alto-630)/2))+'px');
            $(window).bind('scroll', function() {
                var top_v = $(window).scrollTop();
                $('.banner_i').css('top',(top_v+Math.floor((alto-630)/2))+'px');
                var documentScrollLeft = $(document).scrollLeft();
                if (lastScrollLeft != documentScrollLeft) {
                    if(documentScrollLeft > lastScrollLeft)
                    {
                        lastScrollLeft = documentScrollLeft;
                        var margin_left = $('#banner_i').css('margin-left');
                        var newValue = margin_left.replace('px', '');
                        $('#banner_i').css('margin-left',(newValue-1)+'px');
                    }
                    else if(documentScrollLeft < lastScrollLeft)
                    {
                        lastScrollLeft = documentScrollLeft;
                        var margin_left = $('#banner_i').css('margin-left');
                        var newValue = margin_left.replace('px', '');
                        $('#banner_i').css('margin-left',(newValue+1)+'px');
                    }
                }
            });
        });
    </script>
