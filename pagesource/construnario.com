<script type="text/javascript">
    $(document).ready(function () {

     
        //Autocomplete de producto
        $('#menu-wrapper').keyup(function (e) {
            if (e.keyCode == 13 && $('#txb_buscarproductos').val() != '') {
                buscador_productos();
                //$(this).attr("disabled", "disabled");
            }
        });



        $('#txb_buscarproductos').autocomplete({
            serviceUrl: '/Handlers/product_autocomplete_handler.ashx',
            minChars: 2,
            params: { carp: $('#HF_suggestion').val(), query: $('#txb_buscarproductos').val() },

            //params: { carp: document.getElementById("HF_suggestion").value },
        });


        function buscador_productos() {

            var busqueda = $('#txb_buscarproductos').val()
            var carp = $('#HF_suggestion').val()

            $('#txb_buscarproductos').val('');
            $("#txb_buscarproductos").next().val('');



            $.colorbox({ href: "/diccionario/productos_buscador.aspx?marco=&buscar=" + busqueda + "&carp=" + carp, iframe: true, width: "95%", height: "95%" });
        };



        $("a[href='#nologin']").click(function () {


            sweetAlert({

                title: '<h3 style="font-size: 1.6rem; font-weight:600">Contenido exclusivo para usuarios registrados</h3>',
                html: 'Para poder visualizar este contenido o archivo, debes de estar registrado. <p><a href="https://panel.construnario.com/account/login?ReturnUrl=https://www.construnario.com//?sessionkey=qb1mepkyjqiozjhxi0wc53xh" style="font-weight: bold !important; text-decoration: none  !important; color: #333333  !important;">Iniciar sesión</a> o <a href="https://panel.construnario.com/account/Register?ReturnUrl=https://www.construnario.com//?sessionkey=qb1mepkyjqiozjhxi0wc53xh" style="font-weight: bold !important; text-decoration: none  !important; color: #333333  !important;">Registrate gratuitamente</a></p>',
                confirmButtonText: 'Cerrar',
                focusConfirm: false,
                width: '650px'
            })


        });


}); //End ready

</script>


