function LoginChk(frm)
{
	if (frm.id.value.length<1) {
		alert('아이디를 입력하세요.');
		frm.id.focus();
		return;
	}

	if (frm.passwd.value.length<1) {
		alert('패스워드를 입력하세요.');
		frm.passwd.focus();
		return;
	}
	frm.action = 'http://gundamshop.co.kr/member/Login_Pass.html';
	frm.submit();

}

$(document).ready(function() {

	//Login Layer
	$('a[name=modal]').click(function(e) {
		e.preventDefault();
		var id = $(this).attr('href');
		var maskHeight = $(document).height();
		var maskWidth = $(document).width();

		$('#mask').css({'width':maskWidth,'height':maskHeight});
		$('#boxes').show();
		$('#mask').show();

		var winH = $(window).height();
		var winW = $(document).width();
		$(id).css('top', winH/2-$(id).height()/2);
		$(id).css('left', winW/2-$(id).width()/2);
		$(id).show();
	});

	$('.lyrClose').click(function(e) {
		e.preventDefault();
		$('#boxes').hide();
		$('.window').hide();
	});

	$('#mask').click(function () {
		$('#boxes').hide();
	});
	
});

