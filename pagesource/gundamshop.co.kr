});

	function fnGetMyList(vName,page) {
		str = ""
		var str = $.ajax({
			type	: "GET",
			url	: "/happy/MyStorage.php",
			data	: "vName="+vName+"&page="+page,
			dataType: "text",
			async	: false
		}).responseText;
		
		if (str != "")
		{
			$('#lyMyContents').html(str);
		}
	}

	function fnPutMyList(vName,page,code) {
		str = ""
		var str = $.ajax({
			type	: "GET",
			url	: "/happy/MyStorage_Delete.php",
			data	: "vName="+vName+"&code="+code+"&page="+page,
			dataType: "text",
			async	: false
		}).responseText;
		
		if (str != "")
		{
			$('#lyMyContents').html(str);
		}
	}
	// 장바구니
	function FIxLayerCart(vCode) {

		str = ""
		var str = $.ajax({
			type	: "GET",
			url	: "/Order/GD_Basket_Pass.html",
			data	: "LayerCart=1&vFrom=TOP&code="+vCode,
			dataType: "text",
			async	: false
		}).responseText;
		
		if(str!="") {
			$('#btn_Cart_'+vCode).html('<a href="http://gundamshop.co.kr/Order/GD_Basket.html" target=_top ><img src=http://gundamshop.co.kr/pics/main/btnCartSmallgo.png border=0 alt="장바구니확인"></a>');
			
			C_CartNum = getCookie("C_CartNum");
			$('#td_CartNum').html(C_CartNum);
		}

	}
	// 위시리스트(찜) Zzim Wish
	function FIxLayerWish(vCode,vItemCode,vSellCost,vName) {

			str = ""
			var str = $.ajax({
				type	: "GET",
				url	: "/Goods/Zzim.php",
				data	: "ItemCode="+vItemCode+"&code="+vCode+"&sellcost"+vSellCost+"&name="+vName,
				dataType: "text",
				async	: false
			}).responseText;
			
			if(str!="") {

				if (member_id)
				{
					C_WishNum = getCookie("C_WishNum");
					if(!C_WishNum || C_WishNum=="NaN") C_WishNum = 1
					else C_WishNum = parseInt(C_WishNum,10) + 1
					
					$('#td_ZzimNum').html(C_WishNum);
					setCookie("C_WishNum",C_WishNum,0);
				}
				
				alert(str);
			}

	}
</script>


