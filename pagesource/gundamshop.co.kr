			  $('.uQuickSubLyr').hide();
			  $(".header").css("height","390px");
		 });
		 $(".mainGnb").mouseleave(function(){
			 $(".header").css("height","52px");
		 });
		
         } else {
		 $(".header").css("height","52px");
		 $('.uQuickSubLyr').hide();
         $(".guideBtn > p").css("display","block");
         $(".section_wrap").css("display","none");
         $(".header").css({"position":"","background-image":"url(http://gundamshop.co.kr/pics/main/tbg0_V2.png)","top":""});
		 $(".mainGnb").mouseover(function(){
			  $(".header").css("height","52px");
		 });
