//时间插件
function calcTime(city, offset) {
d = new Date();
utc = d.getTime() + (d.getTimezoneOffset() * 60000);
nd = new Date(utc + (3600000*offset));
Hour = nd.getHours(); 
Minute = nd.getMinutes(); 
if (Hour >= 10 ) 
{ 
Hour = Hour; 
} 
else 
{ 
Hour = "0" + Hour; 
} 
if (Minute >= 10 ) 
{ 
Minute = Minute ; 
} 
else 
{ 
Minute = "0" + Minute ; 
} 
return Hour + ":" + Minute + " " + city;
}
