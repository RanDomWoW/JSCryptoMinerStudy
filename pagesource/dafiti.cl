<script  type="text/cjs">
$(document).ready(function() {
    var s = $(".sticker-banner");
    var pos = s.position();                    
    $(window).scroll(function() {
        var windowpos = $(window).scrollTop();

        if (windowpos >= 40) {
            s.addClass("stick");
        } else {
            s.removeClass("stick"); 
        }
    });
});

</script>
