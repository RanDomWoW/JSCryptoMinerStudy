<script>
    $(function() { !
        function() {
            function switchs() {
                oUl.animate({
                    left: -iNow * (w + 8)
                })
            }
            function auto() {
                if (iNow++, iNow > len) {
                    iNow = 1;
                    try {
                        oUl[0].style.left = 0
                    } catch(e) {}
                }
                return switchs(),
                    !1
            }
            function openTimer() {
                timera = setTimeout(function() {
                        auto(),
                            openTimer()
                    },
                    2000)
            }
            function closeTimer() {
                clearInterval(timera)
            }
            var oBox = $(".media_report_switch");
            oUl = $(".media_report_tab"),
                aLi = oUl.children("li"),
                len = aLi.size(),
                w = aLi.eq(0).width(),
                oBtnL = $(".m_r_arrow_l"),
                oBtnR = $(".m_r_arrow_r"),
                iNow = 0,
                timera = null;
            try {
                oUl[0].innerHTML += oUl[0].innerHTML
            } catch(e) {}
            oBtnL.click(function() {
                iNow--,
                    iNow < 0 ? iNow = 0 : switchs()
            }),
                oBtnR.click(function() {
                    auto()
                }),
                openTimer(),
                oBox.hover(function() {
                        closeTimer()
                    },
                    function() {
                        openTimer()
                    }),
                $(window).bind("focus",
                    function() {
                        clearInterval(timera),
                            openTimer()
                    }),
                $(window).bind("blur",
                    function() {
                        closeTimer()
                    })
        } ()
    })
</script>
