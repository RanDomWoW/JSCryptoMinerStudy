<script type="text/javascript">
    var loginflag = 0;
    $(function() {
        $("#txtTopUser").keyup(function () {
            loginflag = 1;
        });
        $("#txtTopPwd").keyup(function () {
            loginflag = 1;
        });
        $("#txtTopSearch").keyup(function () {
            loginflag = 2;
        });
        $("#txtArticleTitle").keyup(function () {
            loginflag = 3;
        });

        $(document).keyup(function (e) {
            e = e || event;
            var keycode = e.which || e.keyCode;
            if (keycode == 13) {
                if (loginflag == 1) {
                    $("#btnTopLogin").triggerHandler("click");
                    return false;
                }
                else if (loginflag == 2) {
                    $("#imgTopSearch").triggerHandler("click");
                    return false;
                }
                else if (loginflag == 3) {
                    $("#btnArticleQuery").triggerHandler("click");
                    return false;
                }
                return false;
            }
        });
    });

    function TopUserLogin() {
        var topuser = $.trim($("#txtTopUser").val());
        var toppwd = $.trim($("#txtTopPwd").val());
        if (topuser == "") {
            $("#txtTopUser").focus();
            return false;
        }
        if (toppwd == "") {
            $("#txtTopPwd").focus();
            return false;
        }
        var myurl = "/";
        $.ajax({
            type: "POST",
            url: "/User/PopUserLogin.aspx/UserLogin",
            data: "{ 'username': '" + escape(topuser) + "', 'userpwd': '" + escape(toppwd) + "','url':'" + myurl + "'}",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (msg) {
                var res = $.parseJSON(msg.d);
                if (res[0] == "ok") {
                    location.reload();
                }
                else {
                    layer.msg(res[1]);
                }
            }
        });
    }

    function TopArticleSearch() {
        var title = document.getElementById("txtTopSearch").value.Trim();
        if (title == "") {
            layer.msg("搜索条件不能为空！");
        } else {
            title = escape(title);
            window.location.href = "/Article/ArticleQuery.aspx?title=" + title;
        }
    }
</script>
