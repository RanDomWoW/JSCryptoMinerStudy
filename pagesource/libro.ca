<section class="mapSection">
    <script type="text/template" id="tplLocationsError">
    <div>
        <i class="fa fa-exclamation-triangle" aria-hidden="true"></i>
        {{ message }}
    </div>
</script><script type="text/template" id="tplLocationsListView">
    <li>
        <strong class="locationName">
            {{#if isATM}}
                {{BranchName}}
            {{else}}
                <a href="{{url}}" title="View branch">{{BranchName}}</a>
            {{/if}}
        </strong>
        <a href="http://www.google.com/maps/place/{{Address}}" target="_blank">
            <small class="locationAddress">
                <span class="fa fa-map-marker" style="color:{{#if isATM}}#007CC0{{else}}#FDB813{{/if}}"></span>
                <span>{{Address}}, {{City}}, {{ProvinceAbbr}} {{PostalCode}}</span>
            </small>
        </a>
            </li>
</script><script type="text/template" id="tplMapLocationPopup">
    <div class="mapTooltip bg-white">
        <dl>
            <dt>
                <span class="invisible">Location</span>
                <span data-icon="map-marker"></span>
            </dt>
            <dd>
                <p>
                    <strong>{{BranchName}}</strong><br/>
                    {{Address}}<br/>
                    {{City}} {{ProvinceAbbr}}, {{PostalCode}}
                </p>
            </dd>
            {{#if Telephone}}
            <dt>
                <span class="invisible">Phone</span>
                <span data-icon="phone"></span>
            </dt>
            <dd>
                <p>
                    {{#if Telephone}}Tel: <a href="tel: {{Telephone}}"><strong>{{Telephone}}</strong></a><br/>{{/if}}
                    {{#if Fax}}Fax: <strong>{{Fax}}</strong>{{/if}}
                </p>
            </dd>
            {{/if}}
            {{#if isATM }}
            {{else}}
            <dt>
                <span class="invisible">Hours of Operation</span>
                <span data-icon="clock"></span>
            </dt>
            <dd>
                <p>
                    {{#each Hours}}
                    {{#if endDay}}
                    {{nameShort}} - {{endDay}}:
                    {{else}}
                    {{nameShort}}:
                    {{/if}}
                    <br> {{StartTime}} - {{EndTime}}<br><br>
                    {{/each}}

                </p>
            </dd>
            {{/if}}
            {{#if Attributes.Is24Hours}}
            <dt><span class="invisible">Location Details</span><span data-icon="credit-cards"></span></dt><dd><p>
{{#if Attributes.Is24Hours}}<strong>24 Hours a Day</strong><br/>{{/if}}
{{#if Attributes.AcceptsDeposits}}<strong>Accepts Deposits</strong>{{/if}}
</p></dd>            {{else}}
            {{#if Attributes.AcceptsDeposits}}
            <dt><span class="invisible">Location Details</span><span data-icon="credit-cards"></span></dt><dd><p>
{{#if Attributes.Is24Hours}}<strong>24 Hours a Day</strong><br/>{{/if}}
{{#if Attributes.AcceptsDeposits}}<strong>Accepts Deposits</strong>{{/if}}
</p></dd>            {{/if}}
            {{/if}}
        </dl>
        {{#if isATM }}
        {{else}}
        <a href="{{url}}" class="button" title="Call to Action">View location</a>
        {{/if}}
    </div>
</script>

