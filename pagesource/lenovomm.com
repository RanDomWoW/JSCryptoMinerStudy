  <script>
    function check(ev) {
      ev = ev || window.event;
      var target = ev.target || ev.srcElement;
      var t = target.getAttribute('t')
      if (!target.value) {
        target.nextElementSibling.innerHTML = '*';
        target.nextElementSibling.style.visibility = 'visible';
      } else if (t) {
        if (t == 'pwd') {
          var p1 = document.getElementById('pwd1').value
          if (p1 != target.value) {
            target.nextElementSibling.innerHTML = '*';
            target.nextElementSibling.style.visibility = 'visible';
          } else {
            target.nextElementSibling.style.visibility = 'hidden';
          }
        }
        if (t == 'idcard') {
          var regIdCard = /(^\d{15}$)|(^\d{18}$)|(^\d{17}(\d|X|x)$)/;
          if (!regIdCard.test(target.value)) {
            target.nextElementSibling.innerHTML = '*';
            target.nextElementSibling.style.visibility = 'visible';
          } else {
            target.nextElementSibling.style.visibility = 'hidden';
          }
        }
        if (t == 'phone') {
          var regPhone = /^[1][0-9]{10}$/;
          if (!regPhone.test(target.value)) {
            target.nextElementSibling.innerHTML = '*';
            target.nextElementSibling.style.visibility = 'visible';
          } else {
            target.nextElementSibling.style.visibility = 'hidden';
          }
        }
      } else {
        target.nextElementSibling.style.visibility = 'hidden';
      }
    }
    function zxf_show(t) {
      if (t == 1) {
        document.getElementById('__LOGIN').style.display = 'block'
        document.getElementById('__REGISTER').style.display = 'none'
      }
      if (t == 2) {
        document.getElementById('__LOGIN').style.display = 'none'
        document.getElementById('__REGISTER').style.display = 'block'
      }
    }
    function zxf_submit(t) {
      var err = document.querySelectorAll('#' + t + ' .form .msg')
      for (var i = 0; i < err.length; i++) {
        if (getComputedStyle(err[i])['visibility'] == 'visible') {
          return false;
        }
      }
      window.location.reload();
    }
   function zxf_close(){
      document.getElementById('zxf_mask').style.display='none';
   }

  </script>
