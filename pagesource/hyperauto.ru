                    // Eсли есть кука, или нет баннера, то не показываем, если есть - хуячим с таймаутом.
                    if (!$.cookie('doNotShowPromo', Boolean) && $("#promoModalImage").attr("src")) {

                        setTimeout(function() {
                            
                            // Достать модальку
                            var modal = $('#actionModal');
                            var jsModal = document.getElementById('actionModal');

                            // Достать элемент закрытия модальки
                            var span = $('.promo-modal-close');

                            // Закрываем модальку по клику на крестик
                            span.on('click', function() {
                                modal.fadeOut();
                            });

                            // Закрываем модальку по клику вне модальки
                            $(window).on('click', function(event) {
                                if (event.target == jsModal) {
                                    modal.fadeOut();
                                }
                            });

                            // Обработчик клика по "не показывать" - скрываем модалку, кладем куку.
                            $('#showNoMore').on('click', function() {
                                $.cookie('doNotShowPromo', 'true', { domain: 'hyperauto.ru', expires: 365, path: '/' });
                                modal.fadeOut();
                            });

                            // Ну че, емаарот, погнали нахуй
                            modal.fadeIn();

                        }, 300000);
                    }
    			}
    		});
    	});
    </script>
  

