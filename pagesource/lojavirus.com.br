    },
    nextModal: function(id, element){
      const data_looks = this.products_ids;
      if(id){
        var index = data_looks.indexOf(id)+1 != -1 ? data_looks.indexOf(id)+1 : false;
        if(index && data_looks[index] !== undefined){
          element.parent().parent().parent().find('.IBY-modal-background').fadeOut('fast');
          element.parent().parent().parent().parent().parent().find("#modal-"+data_looks[index]+"").fadeIn('fast');
        }else{
          $("body").removeClass('body-no-overflow');
        }
      }else{
