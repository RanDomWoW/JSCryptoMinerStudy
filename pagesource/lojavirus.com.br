    previousModal: function(){
      const data_looks = this.products_ids;
      $("body").on('click', ".previous", function(e){
        e.preventDefault();
        var id = $(this).data('go');
        console.log(id);
        var index = data_looks.indexOf(id)-1 != -1 ? data_looks.indexOf(id)-1 : "falso";
        console.log(data_looks.indexOf(id)-1);
        if(index != "falso"){
          $(this).parent().parent().parent().find('.IBY-modal-background').fadeOut('fast');
          $(this).parent().parent().parent().parent().parent().find("#modal-"+data_looks[index]+"").fadeIn('fast');
        }
      });
    },
    modalClose: function() {
        $('body').on('click', '.IBY-modalbg, .IBY-close', function() {
            $("body").removeClass('body-no-overflow');
            $(this).parent().parent().parent().parent().find('.IBY-modal-background').fadeOut('fast');
            $('.IBY-container').removeClass('z-index-plus');
        })
    },
    start: function(){
      const carousel = this;
      this.startTimeout();

      $('body').on('mouseover', '.product-box', function() {
        carousel.stopTimeout();
      });
      $('body').on('mouseout', '.product-box', function() {
        carousel.startTimeout();
      });
      $('.IBY-container').css('opacity', 1);
      setTimeout(function(){
        jQuery(".product-div").each(function(idx, el){
          console.log(idx);
          jQuery('.IBY').height(jQuery(el).width());
        });
      }, 1000);
    },

    startTimeout: function startTimeout(){
      const carousel = this;
      this.stopTimeout();
      this.timeout = setInterval(function(){
        carousel.processTimeout();
        console.log('ticked');
      }, carousel.timeoutTime);
    },

    stopTimeout: function stopTimeout() {
      clearTimeout(this.timeout);
    },

    processTimeout: function processTimeout(){
      const carousel = this;
      if(carousel.timeoutIndex < carousel.productLimit){
        carousel.productWidth = $(".product-box:first-child").width();
        $(".product-box").each(function(idx, el){
          $(el).animate({ left: '-='+(carousel.productWidth + 30.5) }, 1000);
        });
        carousel.timeoutIndex++;
      }else{
        $(".product-box").each(function(idx, el){
          $(el).animate({ left: '0' }, 1000);
        });
        carousel.timeoutIndex = 0;
      }
    },

    next: function next(){
      this.stopTimeout();
      this.processTimeout();
      this.startTimeout();
    },
    prev: function prev(){
      this.stopTimeout();
      const carousel = this;
      if(carousel.timeoutIndex > 0){
        $(".product-box").each(function(idx, el){
          $(el).animate({ left: '+='+(carousel.productWidth + 30.5) }, 1000);
        });
        carousel.timeoutIndex--;
      }else{
        $(".product-box").each(function(idx, el){
          $(el).animate({ left: '0' }, 1000);
        });
        carousel.timeoutIndex = 0;
      }
      this.startTimeout();
    }
  }
});
