		<script>
		/*
			The following on submit code was added in order to quietly correct the usage of email addresses
			as userid's when tryign to login to the portal. The code below takes the submitted username and strips out 
			the characters past the "@". 20170817
		
		var form = document.getElementById("loginForm");//get the form object
		form.addEventListener("submit",function(event){//add on submit listener 
			var val = email.value;//get the form input : email value
			//console.log("current value: " + val);//display if debugging
			if(email.value.indexOf("@") != -1){//make sure there is an @ used
				//console.log("current Value: " + email.value+" indexOf@: " +email.value.indexOf("@"));//display if debugging
				email.value = email.value.substring(0,email.value.indexOf("@")); //strip out the id to send
				//console.log("After Value: " + email.value+" indexOf@: " +email.value.indexOf("@"));//display if debugging
			}
			
		});
		*/

		var email = document.getElementById("email");//get the email object
		email.addEventListener("change",function(){//add on change listener
			var val = email.value;//get the input
			//console.log("current value: " + val);//display if debugging
			if(email.value.indexOf("@") != -1){//make sure there is an @ used
				var userid = val.substring(0,val.indexOf("@")); //grab the user id
				var domainid = val.substring(val.indexOf("@"),val.length);//grabe the domain
				email.value = userid;//replace the current value with the stripped version
			}
		});
		</script>
	

		
	</script>
