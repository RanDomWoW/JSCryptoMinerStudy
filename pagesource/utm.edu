			}
		],
		"feb01":  [
			{
				"number": "12838",
				"text": "Active alumni have ever made a gift or pledge."
			},{
				"prefix": "$",
				"decimals": 1,
				"number": "6.5",
				"affix": "M",
				"text": "Largest single gift in UT Martin history."
			},{
				"prefix":"$",
				"decimals": 2,
				"number": "153.48" , 
				"text": "Was the average gift during the 2016 Captain's Challenge event."
			},{
				"prefix": "1 in ",
				"number": "5",
