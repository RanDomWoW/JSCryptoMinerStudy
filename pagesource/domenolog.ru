		'trigger': 'hover'
	});

	// modalFeedback
	$('#formModalFeedback').submit(function(event){
		return submitModalFeedback(event);
	});
	if(window.location.href.indexOf('#feedback') != -1) {
		$('#modalFeedback').modal('show');
	}

	// modalOwner
	$('#formModalOwner').submit(function(event){
