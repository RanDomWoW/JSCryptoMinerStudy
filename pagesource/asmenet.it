 document.getElementById('cloak9175').innerHTML += '<a ' + path + '\'' + prefix + ':' + addy9175 + '\'>'+addy_text9175+'<\/a>';
 //-->
 </script>o  via  fax  al  numero  081  19721004.  Per  maggiori  informazioni  chiamare  lo  0817877540.</p>
<p style="text-align: right;">&gt;&gt; <a href="/images/avcp2018.pdf" target="_blank">Qui la Circolare</a></p></div>
