        var otgt = $( this ) ,mheight = otgt.height() , oul = otgt.find(".move-ele") , ulheight = oul.height() , _this = this ;

        this._index = 0;
        
        this._max = Math.ceil( ulheight / mheight ) ;

        oul.find(".ul").height( mheight * this._max);
        
        oul.append( oul.find(".ul").clone() );

        if( this._max <= 1 ){  return false; }

        var setTimer = function (){

            clearTimeout( _this._timer );

            _this._timer = setInterval(function(){

                var idis = ++_this._index * mheight;

                 if( _this._max < _this._index ){

                    _this._index = 0 ;

                    idis = 0;

                     oul.css({ "margin-top": 0 });
                }

                oul.animate({ "margin-top": -idis });

            },4000);
        }

        setTimer.call( _this );

         $( this ).on("mouseover",function(){
        
            clearTimeout( _this._timer );

        }).on("mouseout",function(){
            
            setTimer.call( _this );
        });

    }

    $(".move-bar").each(function(index, el) {
        move.call( this );
    });



});

</script>