<script>
$(function() {

	//ページ内スクロール
	$(".btn").click(function () {
		var i = $(".btn").index(this)
		var p = $(".scroll").eq(i).offset().top;
		$('html,body').animate({ scrollTop: p }, 'fast');
		return false;
	});

	//ページ上部へ戻る
	$(".btn_top").click(function () {
		$('html,body').animate({ scrollTop: 0 }, 'fast');
		return false;
	});

});
</script>

