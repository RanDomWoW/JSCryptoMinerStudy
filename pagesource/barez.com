<script>
    function showpic(thispic,idpic) {
        /*********loading********/
        $('img.slider')
//                .fadeOut(400, function() {
        $('img.slider').attr('src',thispic);
        $('img.slider').attr('bgcolor',bgcolor[idpic]);
        $('div.fade').attr('bgcolor',bgcolor[idpic]);
        $('div.css-slideshow').css('background-color',bgcolor[idpic]);
        $('div.Slider').css('background-color',bgcolor[idpic]);
//                    document.getElementById("Sliderbg").style.backgroundColor = color[idpic];
//                })
//                .fadeIn(400);
        $("#fade div").removeClass('desc-active');
        $(".descimg"+idpic).addClass('desc-active');
        var hreft = href[idpic] ;
        if (hreft !=''){
            $('img.slider').css('cursor','pointer');
            $('img.slider').attr('href',hreft);
            $('img.slider').attr('href',hreft);
        }else{
            $('img.slider').attr('title', '');
            $('img.slider').css('cursor','initial');
            $('img.slider').attr('href', '');
        }
    }
    var counter = '5';
    counter = parseInt(counter)-1;
    showpic(picture[1],1);
    /********right*******************************/
    $('.right').click(function(){
        thispic = $('img.slider').attr('src');
        if(thispic != picture[counter]){
            for (i = 1; i < counter;  i++){
                var n=i;
                if(thispic == picture[i]){
                    n++;
                    showpic(picture[n],n);
                }
            }
        }else{
            showpic(picture[1],1);
        }
    });
    /********left*******************************/
    $('.left').click(function(){
        thispic = $('img.slider').attr('src');
        if(thispic != picture[1]){
            for (i = 1; i <= counter;  i++){
                var n=i;
                if(thispic == picture[i]){
                    n--;
                    showpic(picture[n],n);
                }
            }
        }else{
            showpic(picture[counter],counter);
        }
    });
    /*******timer*************************************/
    var mytime = 30000;
    setInterval(function(){
        thispic = $('img.slider').attr('src');
        for (i = 1; i <= counter;  i++){
            var n=i;
            if(thispic == picture[i]){
                if(i == counter){n=0;}
                // document.getElementById("B"+n).style.backgroundColor = '#c3c9b8';
                for (var k = 1; k <= counter;  k++) {
                    document.getElementById("B" + k).className = 'BulletDective';
                }
                n++;
//           document.getElementById("B"+n).style.backgroundColor = '#2e6c4e';
                document.getElementById("B"+n).className = 'BulletActive';
                showpic(picture[n],n);
            }
        }
    },mytime)
    /***********bullet*********************/
    for (i = 1; i <= counter;  i++){
        var liMOVbullets = '<li id=B'+i+'></li>';
        $(liMOVbullets).appendTo('.MOVbullets');
    }
    $('.MOVbullets li').click(function(){
        var bulletid = $(this).attr('id');
        bulletid = bulletid.substr(1, 2);
        for (var q = 1; q <= counter;  q++) {
            document.getElementById("B" + q).className = 'BulletDective';
        }
        document.getElementById("B"+bulletid).className = 'BulletActive';
        showpic(picture[bulletid],bulletid);
        // document.getElementById("B"+bulletid).className = 'BulletDective';
    });
    /*****************link*********************/
    $('img.slider').click(function(){
        var hreftt = $(this).attr('href');
        if (hreftt !=''){
            window.location.assign(hreftt);
        }
    });
</script>
