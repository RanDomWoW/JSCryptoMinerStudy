	timestr=thour+":"+tmin+":"+tsec;
	document.getElementById('t1').innerHTML=timestr;
	document.getElementById('t2').innerHTML=timestr;
	document.getElementById('t3').innerHTML=timestr;
	window.setTimeout("time()",1000);
}
</script>



