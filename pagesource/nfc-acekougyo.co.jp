<script type="text/javascript">
var numImages = 3; 
var currentImage = 1;
var imageInterval ;
var image;
imageInterval = setInterval("changeImage('next')", 6000); 

function changeImage(move) {         
	$("#visualarea").fadeOut("slow", function() {
		clearInterval(imageInterval);

		if ( move == "prev"){
			currentImage--;
		} else {
			currentImage++;
		}
		if (currentImage > numImages) {
			currentImage = 1;
		} 
		if (currentImage <= 0) {
			currentImage = numImages;
		}

		if ( currentImage == 1 ){
			image = "top_img1.jpg"
		}
		if ( currentImage == 2 ){
			image = "top_img2.jpg"
		}
		if ( currentImage == 3 ){
			image = "top_img3.jpg"
		}
		$('#visualarea').css("background-image", "url(/img/top/"+ image +")");  
		$("#visualarea").fadeIn("slow", function() {			
		});
		imageInterval = setInterval("changeImage('next')",6000)
	}); 
} 
</script>
