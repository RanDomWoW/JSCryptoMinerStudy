        <script type="text/javascript">
            pointList = pgDefault.getPointerList().value;

            var banner1 = pgDefault.setBannerContent('1', '1').value;
            var banner2 = pgDefault.setBannerContent('1', '2').value;

            var bn1Posi = 0;
            var bn2Posi = 0;

            if (banner1.length >= 1) {
                var bn = document.getElementById('dvBanner1');
                bn.innerHTML = banner1[0];
            }
                    
            if (banner2.length >= 1) {
                var bn = document.getElementById('dvBanner2');
                bn.innerHTML = banner2[0];
            }

            var tm = setTimeout('getBannerContent();', 7000);


            function getBannerContent() {

                //alert('bn 1 : ' + (banner1.length - 1) + ' / ' + bn1Posi + ' -- ' + 'bn 2 : ' + (banner2.length -1) + ' / ' + bn2Posi);

                clearTimeout(tm);

                if (banner1.length > 1) {
                    if (bn1Posi < banner1.length - 1) {
                        var bn = document.getElementById('dvBanner1');
                        bn.innerHTML = banner1[bn1Posi];

                        bn1Posi += 1;
                    }
                    else {
                        if (bn1Posi == banner1.length - 1) {
                            var bn = document.getElementById('dvBanner1');
                            bn.innerHTML = banner1[bn1Posi];

                            bn1Posi = 0;
                        }
                    }
                }

                if (banner2.length > 1) {
                    if (bn2Posi < banner2.length - 1) {
                        var bn = document.getElementById('dvBanner2');
                        bn.innerHTML = banner2[bn2Posi];

                        bn2Posi += 1;
                    }
                    else {
                        if (bn2Posi == banner2.length - 1) {
                            var bn = document.getElementById('dvBanner2');
                            bn.innerHTML = banner2[bn2Posi];

                            bn2Posi = 0;
                        }
                    }
                }


                tm = setTimeout('getBannerContent();', 7000);
            }


            var blGelis = true;
            var blYurtIci = true;

            var hv = 2;
            document.getElementById('dvUcus').innerHTML = pgDefault.getUcusBilgi(hv, true, true).value;


            var monthNames = new Array("01", "02", "03", "04", "05", "06", "07", "08", "09", "10", "11", "12");
            var now = new Date();
            var strDay = now.getDate().toString();
            if (strDay.length == 1)
                strDay = "0" + now.getDate().toString();

            var strDate = strDay + "/" + monthNames[now.getMonth()] + "/" + now.getFullYear();

            var strTime = "";

            if (now.getHours().length <= 1)
                strTime = "0" + now.getHours();
            else
                strTime = now.getHours();

            strTime += ':';

            var d = now.getMinutes().toString();
            if(d.length <= 1)
                strTime += "0" + now.getMinutes().toString();
            else
                strTime += now.getMinutes().toString();

            document.getElementById('dvDate').innerHTML = strDate + " - " + strTime;


            function setGelYurtIci() {
                if ($(".flght-change").attr("class") == "flght-change active")
                    blYurtIci = true;
                else
                    blYurtIci = false;

                setGel('gelis');
            }

            function setGel(vl) {
                if ($(".flght-change").attr("class") == "flght-change active")
                    blYurtIci = true;
                else
                    blYurtIci = false;

                var yrt = '';

                if (vl == "gelis") {

                    $("#dvGelis").addClass("active");
                    $("#dvGidis").removeClass("active");

                    blGelis = true;

                } else {

                    $("#dvGidis").addClass("active");
                    $("#dvGelis").removeClass("active");

                    blGelis = false;
                }

                getUcus(hv);
            }

            function getUcus(id) {
                document.getElementById('dvUcus').innerHTML = pgDefault.getUcusBilgi(id, blGelis, blYurtIci).value;
                hv = id;
            }

            function getPageDetail(mnuID) {
                window.location.href = 'DHMIPage.aspx?mnuID=' + mnuID;
            }

        </script>

