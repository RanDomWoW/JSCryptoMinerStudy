				$('.viewer').find('li').eq(window.scroller.currentPage.pageX).find('img').hide().fadeIn();
				autoSlide();
			});

			$("#jsytplayer").one('mouseover', function () {
				var i = $(this).find("iframe");
				i.attr("src", i.attr("src") + "&autoplay=1");
			});

		});


		function autoSlide() {
			clearInterval(window.autoslideinterval || null);
			window.autoslideinterval = setInterval(function () { $('.viewer .next').trigger('click') }, 10000);
		}

		function streamLoaded(firstTime) {
			//$(document).trigger('init');
			window.streamCont.trigger('end');
			var images = new Array();
			$('#streamContainer').find('.tag:not(._positioned)').each(function (index, value) {
				var $tag = $(value);
				$.setTagPosition($tag, null, 'homepage');
				$tag.addClass('_positioned');
				var $img = $tag.siblings('img');
				images.push($img);
			});

			var img = imagesLoaded(streamCont);
			var items = streamCont.children(":not(.masoned)").addClass("masoned");
			img.on('always', function (instance) {
				window.streamCont.masonry(firstTime == true ? 'layout' : 'appended', items);
				window.streamCont.trigger('start');
				items.find('.tag').each(function (index, value) {
					$.setTagPosition($(this), null, 'homepage');
				});
			});
		};

		function RemoveContent(entityType, entityId) {
			$.confirm({ title: 'Rimozione contenuto dalla Home Page', message: 'Rimuovere il contenuto selezionato dalla Home Page?', confirmtext: 'Si', canceltext: 'No' }, function (isConfirmed) {
				if (isConfirmed) {
					$.ajax({
						url: $.route('home') + '/removecontent',
						contentType: 'application/json',
						data: JSON.stringify({ 'entityType': entityType, 'entityId': entityId }),
						type: 'post',
						success: function (data) {
							$.message('Rimuovi contenuto dalla Home Page', 'Il contenuto è stato correttamente rimosso dalla Home Page. Effettuare il refresh della cache.', 'Chiudi');
						}
					});
				}
			});
		};

	</script>



	
	



	
