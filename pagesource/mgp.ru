	<script>
		var MgpCountriesRanksWidget = function(el, country_rank_template, data, auto_change_in_seconds, after_render) {
			this.el = el;
			this.country_rank_template = country_rank_template.clone();
			this.data = data;
			this.auto_change_in_seconds = auto_change_in_seconds;
			this.timer_id = null;
			this.after_render = after_render;
			this.init_auto_change();
		};

		MgpCountriesRanksWidget.prototype.init_auto_change = function() {
			if (!this.data)	return;
			if (!this.country_rank_template) return;

			var self = this;
			var countries_ranks_elements = [];
			this.data.forEach(function(doc){
				var tpl = self.country_rank_template.clone();
				tpl.addClass('country_rank');
				tpl.find('.name').html(doc.country.name);
				tpl.find('.delta').html((doc.delta > 0?'+':'') + (doc.delta==undefined?'0.0':doc.delta.toString().replace('.', ',')));

				if (doc.delta != 0 && doc.delta!=undefined) {
					if (doc.delta < 0) {
						tpl.addClass('down');
					} else {
						tpl.addClass('up');
					}
				} else {
					tpl.addClass('none');
				}

				tpl.hide();
				countries_ranks_elements.push(tpl);
			});

			var holder_el = $('<div class="countries_ranks"></div>');
			holder_el.css('position: absolute');
			holder_el.append(countries_ranks_elements);

			this.el.append(holder_el);

			try {
				this.after_render();
			} catch(e) {

			}

			this.show_next();
		};

		MgpCountriesRanksWidget.prototype.show_next = function() {
			var self = this;
			var current = this.el.find('.countries_ranks .country_rank:visible');
			var next = null;

			if (current.length == 0) {
				next = this.el.find('.countries_ranks .country_rank:first')
			} else {
				if (current.next().length != 0) {
					next = current.next();
				} else {
					next = this.el.find('.countries_ranks .country_rank:first');
				}
			}

			if (current.length!=0 && next.length!=0) {
				current.fadeOut(400, function() {
					next.fadeIn();
				})
			} else {
				next.fadeIn();
			}

			this.timer_id = setTimeout(
				function(){
					self.show_next();
				},
				self.auto_change_in_seconds
			);
		};

