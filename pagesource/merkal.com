                    if (cmp == 0) {
                        className = " class='first'";
                    }
                    /* Génération du titre N+1 */
                   var n1name = new String(subcat['title']);
                    var n1SpaceFound = n1name.indexOf(' ');
                    if (n1SpaceFound != -1) {
                        n1name = '<span>'+n1name.substr(0, n1SpaceFound)+'</span>'+n1name.substr(n1SpaceFound);
