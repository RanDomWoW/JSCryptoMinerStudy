<script type="text/javascript">
  function setToday()
  {
    var today = new Date();

    if(today.getDay() === 0)
    {
      today.setDate(today.getDate() - 2);
    }
    else if(today.getDay() === 6)
    {
      today.setDate(today.getDate() - 1);
    }
  };

  function urlEdicaoPDF()
  {
    setToday();

    let month = today.getMonth() +1;
    let day = today.getDate();
    let year = today.getFullYear();

    if (month.toString().length < 2) month = '0' + month;
    if (day.toString().length < 2 ) day = '0' + day;

    var urlPDF = "http://pesquisa.in.gov.br/imprensa/core/consulta2.action?";
    urlPDF += "edicao.ano=" + year + "&";
    urlPDF += "edicao.dtFim=" + day + "/" + month + "&";
    urlPDF += "edicao.dtInicio=" + day + "/" + month + "&";
    urlPDF += "edicao.jornal=3,3000,3020,1040,526,530";

    window.location.href = urlPDF;
  }

  function urlEdicaoCompletaHTML(secao)
  {
    setToday();

    var urlHtml = "/consulta?";
    var searchRange = "";

    urlHtml+="_3_keywords=&";
    urlHtml+="p_p_id=3&";
    urlHtml+="p_p_lifecycle=0&";
    urlHtml+="p_p_state=maximized&";
    urlHtml+="p_p_mode=view&";
    urlHtml+="_3_struts_action=%2Fsearch%2Fsearch&";
    urlHtml+="_3_cur=1&";
    urlHtml+="_3_groupId=0&";

    urlHtml+="_3_modifiedfrom="+formattedDate(today, "normal")+"&";
    urlHtml+="_3_modifieddayFrom="+today.getDate()+"&";
    urlHtml+="_3_modifiedmonthFrom="+(today.getMonth()+1)+"&";
    urlHtml+="_3_modifiedyearFrom="+today.getFullYear()+"&";
    urlHtml+="_3_modifiedto="+formattedDate(today, "normal")+"&";
    urlHtml+="_3_modifieddayTo="+today.getDate()+"&";
    urlHtml+="_3_modifiedmonthTo="+(today.getMonth()+1)+"&";
    urlHtml+="_3_modifiedyearTo="+today.getFullYear()+"&";
    search_range = "[" + formattedDate(today, "range_from") + " TO " + formattedDate(today, "range_to") + "]";
    urlHtml+="_3_modified=" + search_range + "&";
    urlHtml+="_3_modifiedselection=1&";

    if(typeof secao !== 'undefined')
    {
      urlHtml+="_3_ddm_21040_pubName_pt_BR_sortable="+secao;
    }

    window.location.href = urlHtml;
  };

</script>



