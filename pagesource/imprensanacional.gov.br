<script type="text/javascript">
  var search_from, search_to, search_range;

  today = new Date();

  if(today.getDay() === 0)
    today.setDate(today.getDate()-2);

  if(today.getDay() === 6)
    today.setDate(today.getDate()-1);

  search_from = today;
  search_to = today;

  function set_basic_parameters()
  {
    set_element_value("p_p_id", 3);
    set_element_value("p_p_lifecycle", 0);
    set_element_value("p_p_state", "maximized");
    set_element_value("p_p_mode", "view");
    set_element_value("_3_struts_action", "/search/search");
    set_element_value("_3_cur", 1);
    set_element_value("_3_groupId", 0);
    set_element_value("_3_modifiedselection", 1);
  }

  function set_journal()
  {
    if (document.getElementById('secao1').checked)
    {
      set_element_value("_3_ddm_21040_pubName_pt_BR_sortable", "do1, do1a, do1e");
    }
    else if (document.getElementById('secao2').checked)
    {
      set_element_value("_3_ddm_21040_pubName_pt_BR_sortable", "do2, do2e");
    }
    else if (document.getElementById('secao3').checked)
    {
      set_element_value("_3_ddm_21040_pubName_pt_BR_sortable", "do3");
    }
  };

  function set_date()
  {
    if ((typeof search_from !== 'undefined')&&(typeof search_to !== 'undefined'))
    {
      set_element_value("_3_modifiedfrom", formattedDate(search_from, "normal"));
      set_element_value("_3_modifieddayFrom", search_from.getDate());
      set_element_value("_3_modifiedmonthFrom", search_from.getMonth());
      set_element_value("_3_modifiedyearFrom", search_from.getFullYear());
      set_element_value("_3_modifiedto", formattedDate(search_to, "normal"));
      set_element_value("_3_modifieddayTo", search_to.getDate());
      set_element_value("_3_modifiedmonthTo", search_to.getMonth());
      set_element_value("_3_modifiedyearTo", search_to.getFullYear());
      search_range = "[" + formattedDate(search_from, "range_from") + " TO " + formattedDate(search_to, "range_to")  + "]";
      set_element_value("_3_modified", search_range);
    }
  };

    function submit_form()
    {
      var imprensa_fm = document.getElementById("imprensa_fm");

      set_basic_parameters();
      set_journal();
      set_date();

      imprensa_fm.method = "POST";
      imprensa_fm.action = "/consulta"
      imprensa_fm.submit();
      imprensa_fm.reset();
    };

    function set_element_value(element_id, value) {
      document.getElementById(element_id).value = value;
    };

    function date_stripper(value)
    {
        let split_date = value.split('/');

        return split_date[1]+"/"+split_date[0]+"/"+split_date[2];
    }

    function from_change(value)
    {
      search_from = new Date(date_stripper(value));
    };

    function to_change(value)
    {
      search_to = new Date(date_stripper(value));
    };

    function formatted_number(value)
    {
      if (value.toString().length < 2) value = '0' + value;
      return value;
    };

    function formattedDate(d, type) {
      let month = d.getMonth() + 1;
      let day = d.getDate();
      const year = d.getFullYear();
      let hours_from = "000000";
      let hours_to = "235959";

      if (month.toString().length < 2) month = '0' + month;
      if (day.toString().length < 2) day = '0' + day;

      if(type==="normal"){
        return day + "/" + month + "/" + year;
      }
      else if(type==="range_from")
      {
        return year+''+month+''+day+''+hours_from;
      }
      else if(type==="range_to")
      {
        return year+''+month+''+day+''+hours_to;
      }
    };
</script>
<script>
$(function() {
  // Dia em que no calendario foi inicializado as versoes html do dou2 e dou3
  var dou_min_date = {'year':2017, 'month': 11-1, 'day': 30};

  load_date_picker();

  function change_min_date(dou)
  {
    if(dou==='todas')
    {
      dou_min_date['year'] = 2017;
      dou_min_date['month'] = 11-1;
      dou_min_date['day'] = 30;
    }
    else if(dou==='dou3')
    {
      dou_min_date['year']= 2018;
      dou_min_date['month']= 2-1;
      dou_min_date['day'] = 5;
    }
  }

  function load_date_picker()
  {
    $("#calendario_from").datepicker("destroy");
    $("#calendario_to").datepicker("destroy");

    from = $("#calendario_from").datepicker({
      changeMonth: true,
      changeYear: true,
      showOtherMonths: true,
      selectOtherMonths: true,
      showButtonPanel:true,
      dateFormat: 'dd/mm/yy',
      dayNames: ['Domingo','Segunda','TerÃ§a','Quarta','Quinta','Sexta','SÃ¡bado','Domingo'],
      dayNamesMin: ['D','S','T','Q','Q','S','S','D'],
      dayNamesShort: ['Dom','Seg','Ter','Qua','Qui','Sex','SÃ¡b','Dom'],
      monthNames: ['Janeiro','Fevereiro','MarÃ§o','Abril','Maio','Junho','Julho','Agosto','Setembro','Outubro','Novembro','Dezembro'],
      monthNamesShort: ['Jan','Fev','Mar','Abr','Mai','Jun','Jul','Ago','Set','Out','Nov','Dez'],
      minDate: new Date(dou_min_date['year'], dou_min_date['month'], dou_min_date['day']),
      maxDate: 0,
      currentText: 'Hoje',
      closeText: 'Fechar'
    }).on("change", function(){
      to.datepicker("option", "minDate", getDate(this));
    }).datepicker("setDate", new Date())

    to = $("#calendario_to").datepicker({
      changeMonth: true,
      changeYear: true,
      showOtherMonths: true,
      selectOtherMonths: true,
      showButtonPanel:true,
      dateFormat: 'dd/mm/yy',
      dayNames: ['Domingo','Segunda','TerÃ§a','Quarta','Quinta','Sexta','SÃ¡bado','Domingo'],
      dayNamesMin: ['D','S','T','Q','Q','S','S','D'],
      dayNamesShort: ['Dom','Seg','Ter','Qua','Qui','Sex','SÃ¡b','Dom'],
      monthNames: ['Janeiro','Fevereiro','MarÃ§o','Abril','Maio','Junho','Julho','Agosto','Setembro','Outubro','Novembro','Dezembro'],
      monthNamesShort: ['Jan','Fev','Mar','Abr','Mai','Jun','Jul','Ago','Set','Out','Nov','Dez'],
      minDate: 0,
      maxDate: 0,
      currentText: 'Hoje',
      closeText: 'Fechar'
    }).on( "change", function(){
      from.datepicker("option", "maxDate", getDate(this));
    }).datepicker("setDate", new Date());

    $("#calendario_from").datepicker("refresh");
    $("#calendario_to").datepicker("refresh");
  }

  function getDate(element)
  {
    var date;
    try {
      date = $.datepicker.parseDate( 'dd/mm/yy', element.value );
    } catch( error ) {
      date = null;
    }
    return date;
  }

  $('#todas').click(function() {
    change_min_date('todas');
    load_date_picker();
  });

  $('#secao3').click(function() {
    change_min_date('dou3');
    load_date_picker();
  });
});
</script>

