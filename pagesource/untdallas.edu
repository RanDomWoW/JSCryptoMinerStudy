    <script>
	webadmin = document.getElementById('webadmin');
	if(webadmin){document.getElementById('footer').style.marginBottom = webadmin.scrollHeight + 'px';}
	
      $('.no-js').removeClass('no-js').addClass('js');
      
      //- Toggles menu
      $('.header-nav__toggle').on('click', function(){
      	var $this = $(this),
      		$body = $('body'),
      		$toggled = $('.header-nav__toggle--toggled');
      
      	if( $toggled.length ) {
      		$toggled.removeClass('header-nav__toggle--toggled');
      		$body.removeClass('hide-overflow');
      	} else {
      		$this.addClass('header-nav__toggle--toggled');
      		$body.addClass('hide-overflow');
      	}
      });
      
      //- Initialize slider
      var _mobileBreakpoint = 750,
      	$slider = $('.info__slider'),
      	_timeout = 3000,
      	_dots = true,
      	_sliderOptions = {
      		items: 1,
      		autoplay: _timeout ? true : false,
      		autoplayTimeout: _timeout ? _timeout : 0,
      		dots: _dots ? _dots : false,
      		loop: true
      	};
      
      if ( $(window).width() < _mobileBreakpoint ) {
      	$slider.addClass('owl-carousel').owlCarousel(_sliderOptions);
      } else {
      	$slider.addClass('off');
      }
      
      //- Destry or re-initialize slider on resize
      $(window).resize(function() {
      	if ( $(window).width() < _mobileBreakpoint ) {
      		if ( $slider.hasClass('off') ) {
      			var owlActive = $slider.owlCarousel(_sliderOptions);
      			$slider.addClass('owl-carousel').removeClass('off');
      		}
      	} else {
      		if ( !$slider.hasClass('off') ) {
      			$slider.removeClass('owl-carousel').addClass('off').trigger('destroy.owl.carousel');
      			$slider.find('.owl-stage-outer').children(':eq(0)').unwrap();
      		}
      	}
      });
      
      //- Toggles submenus in main menu
      $('.header-nav__sub-toggle').on('click', function(event){
      	var $this = $(this),
      		$toggled = $('.header-nav__sub-toggle--toggled');
      
      	if( $this.hasClass('header-nav__sub-toggle--toggled') ) {
      		$this.removeClass('header-nav__sub-toggle--toggled');
      	} else {
      		$toggled.removeClass('header-nav__sub-toggle--toggled');
      		$this.addClass('header-nav__sub-toggle--toggled');
      	}
      });
      
      //- Toggles submenus in footer
      $('.footer__nav').on('click', '.footer__title', function(){
      	$(this).toggleClass('footer__title--toggled');
      });
      
      var didScroll = false;
      var checkStickyHeader = function(){
      	didScroll = true;
      }
      
      $(window).on('scroll', function(){
      	checkStickyHeader()
      });
      
      setInterval(function() {
          if ( didScroll ) {
              didScroll = false;
              if( $(window).scrollTop() >= 300) {
      			$('.header').addClass('sticky');
              } else if( $(window).scrollTop() == 0) {
              	$('.header').removeClass('sticky');
              }
          }
      }, 250);
      
    </script>
