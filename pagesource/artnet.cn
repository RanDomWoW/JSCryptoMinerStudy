function queryAD(ids){
	jQuery.ajax({
		url: "http://www.artnet.cn/common_json/getAdvertisement",
		type:"post",
		dataType:"json",
		data:{ids:ids},
		success:function(data){ 
			 var list = data.adList;
			 var id = ids.split(",");
			 for(var i=0;i<list.length;i++){ 
				 if(list[i].url && list[i].url.indexOf("http://")){
					 list[i].url = "http://"+list[i].url; 
				 }
				 for(var j=0;j<id.length;j++){
				 	if(list[i].adAreaId == id[j] && list[i].imagePath){
				 		$("#adimg"+id[j]).attr("href",list[i].url); 
				 		$("#adimg"+id[j]).attr("target","_blank"); 
	        			$("#adimg"+id[j]).html("<img src='http://www.artnet.cn/"+list[i].imagePath+"' title='"+list[i].title+"' height='"+list[i].height+"' width='"+list[i].width+"' style='border:none;'/>"); 
				 	}
				 } 
			 }
			 for(var j=0;j<id.length;j++){
				 var adimg =  $("#adimg"+id[j]).html();
				 if(!adimg && adimg.length<5 ){ 
					$("#adimg"+id[j]).html(" <img src='http://www.artnet.cn/images/ad/temp"+id[j]+".jpg' />"); 
				 }   
			 }   
		} 
    });
} 
function SetHome(obj){  
	var url = window.parent.location.href;
    try {
        obj.style.behavior = 'url(#default#homepage)';
        obj.setHomePage(url);
    }
    catch(e) {
        if (window.netscape) {
            try {
                netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
            }
            catch(e) {
                alert("此操作被浏览器拒绝！\n请在浏览器地址栏输入“about:config”并回车\n然后将 [signed.applets.codebase_principal_support]的值设置为'true',双击即可。");
            }
            var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
            prefs.setCharPref('browser.startup.homepage', url);
        }
    }
}  
