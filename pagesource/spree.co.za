    <script>

    var ScarabQueue = ScarabQueue || [];
   
    //
    //
    (function(fn){
        if (!fn.map) fn.map=function(f){var r=[];for(var i=0;i<this.length;i++)if(this[i]!==undefined)r[i]=f(this[i]);return r}
        if (!fn.filter) fn.filter=function(f){var r=[];for(var i=0;i<this.length;i++)if(this[i]!==undefined&&f(this[i]))r[i]=this[i];return r}
    })(Array.prototype);


    function makeLinksRelative (SC) {

        SC.page.products.map(function (product) {

            product.link = product.link.replace(/.*\/\/[^\/]*/, '');

            return product;
        });

        return SC;
    }

    function adjustImageSizes (SC) {

        SC.page.products.map(function (product) {

            product.image2x = product.image.replace(/\/w-serp-206x276-420-ll\//, '/w-serp-415x556-420/');
            product.c_hover_image2x = product.c_hover_image.replace(/\/w-serp-206x276-420-ll\//, '/w-serp-415x556-420/');

            return product;
        });

        return SC;
    }

    function extendCustomProperties (SC, title, section) {

        SC.page.custom = {
            title: title,
            section: section
        };

        return SC;

    }

    function registerRecommendationsWidget (type, title, section) {

        var category = window.categories || false;
        if (category) {
            if (type.toLowerCase() === 'category') {
                ScarabQueue.push(['category', category]);
            } else {
                ScarabQueue.push(['include', 'category', 'has', category]);
            }
        }

        if (typeof searchTerm !== 'undefined') {
            ScarabQueue.push(['searchTerm', searchTerm]);
        }

        ScarabQueue.push(['recommend', {
            logic: type,
            containerId: 'web-predict-' + type,
            templateId: 'web-predict-tmpl',
            limit: 5,
            success: function (SC, render) {
                SC = extendCustomProperties(SC, title, section);
                SC = makeLinksRelative(SC);
                SC = adjustImageSizes(SC);
                render(SC);
                trackProductListImpressions('#web-predict-' + type);

                //add to wishlist
                 var wishlistItem = document.querySelectorAll('[data-wishlist-id]');
                    wishlistItem.forEach(function(ele) {
                        ele.addEventListener('click', function(event) {
                        var WishListIcon = event.currentTarget;

                        if(WishListIcon.hasAttribute('data-wishlist-delete'))
                        {
                            window.WishList.removeProduct(WishListIcon);
                        }else
                        { 
                          var id = WishListIcon.getAttribute('data-wishlist-id');
                          window.WishList.addProduct(WishListIcon, id, false);
                        }
                         
                    });

                
        });
            }
        }]);

    }

</script>



<script type="text/html" id="web-predict-tmpl" >
<![CDATA[
{{ if(SC.page.products.length > 0) { }}
    <div class="customers-also-bought">
        <h2 class="recommend-for-you-header"><span>{{=SC.page.custom.title}}</span></h2>
        <ul class="products">
        {{ for (var i=0; i < SC.page.products.length; i++) { }}
            {{ var p = SC.page.products[i]; }}
            <li class="item" data-scarabitem="{{=p.id}}">
                <div class="cf"
                    data-impression-tracking
                    data-id="{{=p.id}}"
                    data-name="{{=p.title}}"
                    data-brand="{{=p.brand}}"
                    data-price="{{=p.price}}"
                    data-list="{{=SC.page.custom.title}}"
                >
                    {{ if (p.isTheIndexProduct) { }}
                        <div class="badge the-index-badge">The Index</div>
                    {{ } else if (p.price < p.msrp) { }}
                        <div class="badge sale-badge">OUTLET</div>
                    {{ } }}
                    <div class="product-image">
                        <a href="{{=p.link}}" title="{{=p.brand}} | {{=p.title}}" tabindex="-1" class="aspect-ratio">
                            <img alt="{{=p.brand}} {{=p.title}}"
                                class="lazyload"
                                src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7"
                                data-src="{{=p.image}}"
                                data-srcset="{{=p.image}} 1x, {{=p.image2x}} 2x"
                                {{ if (p.c_hover_image !== '') { }}
                                    onmouseover="this.src=this.getAttribute('data-onmouseover'); this.srcset=this.getAttribute('data-onmouseover-srcset');"
                                    data-onmouseover="{{=p.c_hover_image}}"
                                    data-onmouseover-srcset="{{=p.c_hover_image}} 1x, {{=p.c_hover_image2x}} 2x"
                                    onmouseout="this.src=this.getAttribute('data-onmouseout'); this.srcset=this.getAttribute('data-onmouseout-srcset');"
                                    data-onmouseout="{{=p.image}}"
                                    data-onmouseout-srcset="{{=p.image}} 1x, {{=p.image2x}} 2x"
                                {{ } }}
                                width="206" height="276" />
                        </a>
                    </div>
                    <div class="product-details">
                        <a href="{{=p.link}}" title="{{=p.brand}} | {{=p.title}}" class="title">{{=p.title}}</a>
                        <span title="{{=p.brand}}" class="brand">{{=p.brand}}</span>
                    </div>
                    <div class="price-container">
                            {{ if (p.price < p.msrp) { }}
                            <span class="price special-price">R{{=p.price}}</span>
                            <del class="price">R{{=p.msrp}}</del>
                        {{ } else { }}
                            <span class="price">R{{=p.price}}</span>
                        {{ } }}
                    </div>

                    <span data-wishlist-id="{{=p.c_entity_id}}" rel="{{=p.c_entity_id}}"  style="float:right" class="add-to-wishlist"  title="Add to wishlist">
                        <i class="wishlist-product-icon"></i>
                     </span>
                </div>
                 
                    
            </li>
        {{ } }}
        </ul>
    </div>
{{ } else { }}
    <!-- No products to recommend -->
{{ } }}
]]>
</script>

