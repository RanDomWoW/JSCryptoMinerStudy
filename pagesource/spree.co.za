    window.newsletter =  window.newsletter || (function ($) {
        "use strict";

        $(".newsletter-form").click(function(){
            $(".newsletter-form").hide();
            $("#news-subscr").show();
            return false;
        });

        function handleResponse(element, result) {

            if (element.hasClass('newsletter-form-footer')) {
                if (element.hasClass('newsletter-mobile')) {
                    element.siblings('.newsletter-subscription-status').hide(0);
                } else {
                    element.siblings('.newsletter-subscription-status').html(result.notice);
                }
            }

            if (result.status == "success")
            {
                element.siblings('.newsletter-subscr-status').html('<span>Thank you for subscribing to our newsletter.</span><br/><br/>');

                if (result.pre_status === false) {
                    element.siblings('.newsletter-subscr-status').append('<span>Please check your email for the R100 voucher.</span>');
                }

                if (result.is_mobile === true) {
                    element.siblings('.newsletter-subscr-status').append('<div><a class="button button-shop-now" href="/">SHOP NOW</a></div>');
                } else {
                    element.siblings('.newsletter-subscr-status').append('<div><a href="/" style="color: #fff !important;">SHOP NOW</a></div>');
                }

                $(document).trigger('newsletter-subscription-successful', [element]);
            } else {
                if (!element.hasClass('newsletter-form-footer')) {
                    element.siblings('.newsletter-subscr-status').hide(0);
                    element.parent().append('<p class="notice"><strong>' + result.notice + '</strong></p>');
                }
                element.hide();
            }
        }

        $(document).ready(function(){
            $('input[name="group-selector"]', '.newsletter-selection').on('click', function (event) {
                var checked = $(this).is(':checked');
                $(this).parent().parent().find('input[type="checkbox"]').prop("checked", checked);
            });

            $('input[type="checkbox"]', '.newsletter-selection').on('click', function (event) {
                var allChecked = true;
                $('input[name!="group-selector"]', $(this).parent().parent()).each(function (index, element) {
                    var checked = $(element).is(':checked');
                    if(!checked) {
                        allChecked = false;
                    }
                });
                $(this).parent().parent().find('input[name="group-selector"]').prop("checked", allChecked);
            });

            $('[data-error-type]').hide();

            $('.newsletter-signup-form').submit(function (e) {
                e.preventDefault();
            });

            $('.newsletter-signup-form').on('form-invalid', function (event) {
                $('.error').addClass('validation-advice');

                var mobile_selector = $('.__mobile .error.error-box.validation-advice > div');
                if (mobile_selector.length > 0) {
                    $(mobile_selector[0]).css({
                        fontSize: '14px',
                        color:'#f50f1b',
                        margin: '6px 0px 20px',
                    });
                    $(mobile_selector[1]).css({
                        fontSize: '14px',
                        color:'#f50f1b',
                        display: 'inline-block',
                        margin: '10px 0px 11px',
                    });
                    $('.signup-newsletter-page .float-left label').css('color','#444');
                    $('#email-subscr').removeClass('validation-advice');
                } else {
                    var selector=$('.error.error-box.validation-advice > div');
                    $(selector[0]).css({
                        fontSize: '14px',
                        color:'#f50f1b',
                        margin: '17px 0px 6px',
                        lineHeight: '1.5px'
                    });
                    $(selector[1]).css({
                        fontSize: '14px',
                        color:'#f50f1b',
                        margin: '18px 0 10px',
                        lineHeight: '1.5px'
                    });
                }
            });

            $('.newsletter-signup-form').on('form-valid', function (e)
            {
                var that = this;

                $('[data-error-type]').hide();

                var emailSub   = $('input[name="email"]', this).val();
                var signUpType = $('input[name="sign_up_type"]', this).val();
                var captchaResponse = $('textarea[name="g-recaptcha-response"]', this).val();
                var group      = [];

                $(that).find('input[name="group[]"]:checked').each(function () {
                    group.push($(this).val());
                });

                $(that).hide();

                if ($(that).hasClass('newsletter-form-footer')) {
                    $(that).siblings('.newsletter-subscription-status').html($(".loading-icon-footer").html()).show();
                } else {
                    $(that).siblings('.newsletter-subscr-status').html($(".loading-icon").html()).show();
                    $(".newsletter-subscr-status img[alt='Loading...']").addClass('loader');
                }

                if (! $(that).hasClass('newsletter-form-footer')) {
                    $(that).siblings('.newsletter-subscr-status').html('<br><br>Processing subscription').show();
                }

                var data = {};
                data["email"]         = emailSub;
                data["is_subscribed"] = 0;
                data["sign_up_type"]  = signUpType;
                data["group"]         = group;
                data["ajax"]          = true;
                data["captcha_response"] = captchaResponse;

                $.post('/en/newsletter/', data, function (result){
                    $("#email-subscr").val("Enter your email address");
                    $(that).siblings('.newsletter-subscr-status').removeClass('newsletter-subscription-error');

                    handleResponse($(that), result);

                    // automatically slide up the status message while redisplaying the form
                    setTimeout(function(){
                        $(".newsletter-status").slideUp("slow");
                        $('input[name="email"]', that).val('');
                        var captcha_widget = $(that).find('div.captcha_container')[0].id.replace("_captcha", "_widget");
                        grecaptcha.reset(window[captcha_widget]);
                        $(that).find('input[type="checkbox"]').each(function () {
                            $(this).attr('checked', false);
                        });
                        $("#form-newsletter-sign-up").show();
                        if($("#newsletter-sign-up").is(":visible")){
                            $.modal.close();
                            $(".newsletter-signup-form").show();
                        }
                    }, 3000);

                    if ($(that).hasClass('newsletter-form-footer')) {
                        // automatically slide up the status message while redisplaying the form
                        setTimeout(function(){
                            $(".newsletter-subscription-status").hide();
                            $('input[name="email"]', that).val('');
                            var captcha_widget = $(that).find('div.captcha_container')[0].id.replace("_captcha", "_widget");
                            grecaptcha.reset(window[captcha_widget]);
                            $(that).find('input[type="checkbox"]').each(function () {
                                $(this).attr('checked', false);
                            });
                            $("#form-signup").removeAttr('style');
                        }, 5000);
                    }

                    // Add the event to the GTM dataLayer
                    var dataLayer = window.dataLayer || [];
                        dataLayer.push({'admarula_user_id' : result.customer_id});
                        dataLayer.push({'event' : 'newsletter_signup'});
                }, "json")
                .fail(function (result){

                    $(that).show();
                    $(that).siblings('.newsletter-subscription-error').hide();
                    var visible = {};
                    visible['opacity'] = 1;
                    visible['display'] = "block";

                    if (typeof result.responseJSON !== "undefined") {

                        handleResponse($(that), result.responseJSON);

                        if (result.responseJSON.email_error == true) {
                            spree_FormValidator.addErrorState(
                                $(that).find('[data-validate-email]'),
                                $(that).find('[data-validate-email]').data('validateMessage')
                            );
                            $('.error').addClass('validation-advice');
                        }

                        if (result.responseJSON.category_error == true) {
                            spree_FormValidator.addErrorState(
                                $('[data-validate-min-checkbox]'),
                                $('[data-validate-min-checkbox]').data('validateMessage')
                            );
                            $('.error').addClass('validation-advice');
                        }
                    }


                });
            });
        });

        return true;
    }(jQuery));

    //]]>
</script>

