        <script type="text/javascript">
            /*** For Hero Slider ***/
            $('#hero-carousel').cycle({
                fx: 'fade',
            });

            /*** Header on scroll ***/
            $(window).scroll(function () {
                if ($(window).scrollTop() > 60) {
                    $('header').addClass('header-fix');
                }
                else {
                    $('header').removeClass('header-fix');
                }
            })

            function GotoAttorneySearchpage()
            {
                window.location.href = "/attorneys#bySearch=" + $("#bySearch").val();
            }
            /*** Slick Slider for News & Events ***/
            $('.news-slider').slick({
                dots: false,
                infinite: true,
                speed: 300,
                slidesToShow: 2,
                slidesToScroll: 1,
                variableWidth: true,
                responsive: [
                {
                    breakpoint: 1025,
                    settings: {
                        slidesToShow: 2,
                        slidesToScroll: 1,
                        infinite: true,
                        dots: false
                    }
                },
                {
                    breakpoint: 767,
                    settings: {
                        slidesToShow: 1,
                        slidesToScroll: 1,
                        infinite: true,
                        dots: false,
                        variableWidth: false
                    }
                },

                ]
            });
    
            if (window.devicePixelRatio >= 1.2) {
                var images = document.getElementsByTagName('*');
                for (var i = 0; i < images.length; i++) {
                    var attr = images[i].getAttribute('data-2x');
                    if (attr) {
                        images[i].style.cssText += 'background-image: url(' + attr + ')';
                    }
                }
            }
            
            //text shadow Javascript for about-statistic
            function createShadow(ele, color, maxValue) {
                if (navigator.userAgent.toString().indexOf("Android") >= 0) return;
                var i = 0;
                var css = "0 1px " + color + ",";
                for (i = 1; i <= maxValue; i++) {
                    css += i.toString() + "px " + (i - 1).toString() + "px " + color + ",";
                }
                $(ele).css({
                    "text-shadow": css.substring(0, css.length - 1),
                    "webkit-text-shadow": css.substring(0, css.length - 1),
                });
            }
            
            $(document).ready(function () {
                //Shadow Color add Javascript
                createShadow(".shadow1", "#428fa3", 160);
                createShadow(".shadow2", "#16565a", 160);
                createShadow(".shadow3", "#262f36", 160);
                createShadow(".shadow4", "#090909", 160);

                $("#lnkHome").attr("href", "javascript:;");
                $("#lnkHome").click(function (e) {
                    location.reload();
                });

                //LOCATIONS Hover Select class Javascript
                var $links = $('.locationList li');
                $links.mouseover(function () {
                    $(".our-offices").addClass("mapbg");
                    $(".our-offices").find('div.bg-overly').addClass("newbg");
                    $links.removeClass('selected');
                    $(this).addClass('selected');
                });
                //End LOCATIONS Hover Select class Javascript

                var imgLoaded = 0;
                $(".locationList li").each(function () {
                    var url1 = $(this).attr("data-imgurl");
                    var url2 = $(this).attr("data-img2x");
                   

                    var bgImg = new Image();
                    bgImg.onload = function () {
                        var myDiv = document.createElement("div");
                        myDiv.setAttribute("class", "disposableImgDiv");
                        myDiv.style.backgroundImage = 'url(' + bgImg.src + ')';
                        myDiv.style.display = "none";
                        $("body").append(myDiv);
                        imgLoaded++;
                        if (imgLoaded == ($(".locationList li").length * 2)) {
                            $(".disposableImgDiv").remove();
                        }
                    };

                    bgImg.src = url1
                    

                    var bgImg2x = new Image();
                    bgImg2x.onload = function () {
                        var myDiv = document.createElement("div");
                        myDiv.style.backgroundImage = 'url(' + bgImg2x.src + ')';
                        myDiv.style.display = "none";
                        myDiv.setAttribute("class", "disposableImgDiv");
                        $("body").append(myDiv);
                        imgLoaded++;
                        if (imgLoaded == ($(".locationList li").length * 2)) {
                            $(".disposableImgDiv").remove();
                        }
                    };

                    bgImg2x.src = url2
                   
                })

                $(window).on("orientationchange", function (event) {
                    setTimeout(function () {
                        location.reload();
                    }, 1000);
                });
            });

            //text shadow Javascript End
            //On hover image change

            function changeBgImage(image, id) {
                alert("agag");
                if (image == "") {
                    image = "/images/page-5-bg.jpg";
                }
                $("#" + id).css("background-image", "url(" + image + ")");
            }

            var mapTimer = null;
            $(".locationList li").mouseleave(function () {
              
                $(".our-offices").removeAttr("style");
                $(".our-offices").removeClass("mapbg");
                $(".our-offices").find('div.bg-overly').removeAttr("style");
                $(".our-offices").find('div.bg-overly').removeClass("newbg");
            });

            //Search focus Hide Menu
            $(document).ready(function () {
                if ($(window).width() > 1023) {
                    $(".searchBox").on({
                        mouseover: function () {
                            $("nav").hide();
                            $(".navbar .seperator").hide();

                        },
                        mouseout: function () {
                            $("nav").show();
                            $(".navbar .seperator").show();
                        }
                    });
                }
                $('#q').focus();
            });


        </script>

