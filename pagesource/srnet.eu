if($("#search_box").val().length > 3 ){
timer = setTimeout(function(){
			var searchField = $("#search_box").val();
if(searchField.length == 4 && $.isNumeric(searchField) && searchField < 2018){ max = 5; }
			var regex = new RegExp(makeComp(searchField), "i");
