			var regex3 = new RegExp(makeComp(searchField), "i");
			output = ''; output += '<a class="rsg" onclick="CloseResult()" href="javascript:void(0)"><img style="float:right; padding-top:2px;" src="/o/close_button.gif"></a><ul>';
            $.getJSON('/api/search.json', function(data) {
              $.each(data, function(key, val){
                if ((val.cat_name.search(regex) != -1) && num <= max || (val.cat_eng.search(regex2) != -1) && num <= max || (val.year.search(regex3) != -1) && num <= max) {
                  output += '<li><a href="/' + val.url + '-online">';
                  output += '<span style="float:left; padding-right:10px;"><img style="width:35px !important; height:51px !important;" src="http://srnet.eu/o/c/' + val.cat_id + '.jpg"></span>';
                  output += '<span style="font-weight: bold; display: block;line-height: 1;">' + val.cat_name + '</span>';
				  output += '<span style="font-size:10px;">Év: ' + val.year + '</span></a></li>';
                  num++;
                }
                if (num == 7) {
                    //output += '<li class="dots"></li><li><a href="http://moovie.cc/kereses/' + searchField + '"><span class="r-title">További találatok >></span></a></li>';
                    num++;
                }
              });
if(num == 0){ output = '<center style="padding:10px;">Nincs találat a keresett kifejezésre!</center>'; }
if(searchField.length == 4 && $.isNumeric(searchField) && searchField < 2018){ output += '<li style="text-align: center;"><a href="/channels.php?year=' + $("#search_box").val() + '">Összes ' + $("#search_box").val() + ' sorozat</a></li>'; }
output += '</ul>';
$('#suggest-results').show();
$('#suggest-results').html(output);
}); 

}, 1500);
}else{ output = '<center style="padding:10px;">Legalább 4 karakter kell a kereséshez!</center>'; }
$('#suggest-results').show();
$('#suggest-results').html(output);
});

});

function CloseResult() {
document.getElementById("suggest-results").innerHTML = "";
document.getElementById("suggest-results").style.border = "0px";
document.getElementById("search_box").value = "";
return;
}
