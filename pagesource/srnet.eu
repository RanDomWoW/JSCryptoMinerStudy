<script>
function makeComp (input){
    input = input.toLowerCase ();
    var output = '';
    for (var i = 0; i < input.length; i ++)
    {
        if (input.charAt (i) == 'a')
            output = output + '[aá]'
        else if (input.charAt (i) == 'e')
            output = output + '[eé]';
        else if (input.charAt (i) == 'i')
            output = output + '[ií]';
        else if (input.charAt (i) == 'o' || input.charAt (i) == 'ö' || input.charAt (i) == 'ő' || input.charAt (i) == 'ó')
            output = output + '[oóőö]';
        else if (input.charAt (i) == 'u' || input.charAt (i) == 'ü' || input.charAt (i) == 'ű' || input.charAt (i) == 'ú')
            output = output + '[uúűü]'
        else
            output = output + input.charAt (i);
    }
    return output;
}

$(document).ready(function(){
