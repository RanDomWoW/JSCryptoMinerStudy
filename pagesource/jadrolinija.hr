<script type="text/javascript">

	var STR_PAD_LEFT = 1;
	var STR_PAD_RIGHT = 2;
	var STR_PAD_BOTH = 3;

	function pad(str, len, pad, dir) {

		if (typeof (len) == "undefined") { var len = 0; }
		if (typeof (pad) == "undefined") { var pad = ' '; }
		if (typeof (dir) == "undefined") { var dir = STR_PAD_RIGHT; }

		if (len + 1 >= str.length) {
			switch (dir) {
				case STR_PAD_LEFT:
					str = Array(len + 1 - str.length).join(pad) + str;
					break;
				case STR_PAD_BOTH:
					var right = Math.ceil((padlen = len - str.length) / 2);
					var left = padlen - right;
					str = Array(left + 1).join(pad) + str + Array(right + 1).join(pad);
					break;
				default:
					str = str + Array(len + 1 - str.length).join(pad);
					break;
			} // switch
		}
		return str;
	}


	String.prototype.endsWith = function (suffix) {
		return this.indexOf(suffix, this.length - suffix.length) !== -1;
	};


	$(document).ready(function () {
		$(".lineSearchStar").hide();
		$("#ajaxLoader").hide();


		var model_date = '18.3.2018.';
		var model_from = '';
		var model_to = '';
		var is_page_first_load = true;

		$("#datepicker").datepicker({
			buttonImage: "/Custom/Imgs/cal_white.png",
			showOn:"both",
			buttonImageOnly: true
		}).datepicker($.datepicker.regional['hr']).datepicker('setDate', new Date());

        $.datepicker.setDefaults($.datepicker.regional['hr']);

		if (model_date == null || model_date.length == 0) {
			$("#datepicker").datepicker($.datepicker.regional['hr']).datepicker({ dateFormat: 'd.m.yy.' }).datepicker().datepicker('setDate', new Date());
		} else {
			$("#datepicker").datepicker($.datepicker.regional['hr']).datepicker({ dateFormat: 'd.m.yy.' });
			var parsedDate = $.datepicker.parseDate('d.m.yy.', model_date);
			$("#datepicker").datepicker('setDate', parsedDate);
		}

		$('#fromPort').val('');
		$('#toPort').val('');
		$('#fromPort').val(model_from);

		if (is_page_first_load && $('#fromPort').val() != "") {

			var date = $("#datepicker").datepicker("getDate");
			var date_string = pad(date.getDate().toString(), 2, '0', STR_PAD_LEFT) + pad((date.getMonth() + 1).toString(), 2, '0', STR_PAD_LEFT) + date.getFullYear().toString();

			$.ajax({
				url: '/pocetna/FindToPorts/',
				dataType: "json",
				type: 'POST',
				data: {
					fromPortId: $('#fromPort').val(),
					onDate: date_string
				},
				beforeSend: ShowLoader(),
				error: function (a, b, c) {
					alert('Dogodila se komunikacijska pogreška. Osvježite stranicu i pokušajte ponovno.');
				},
				success: function (data) {
					var items = '';
					$.each(data, function (i, toPort) {
						items += "<option value='" + toPort.PortId + "'>" + toPort.PortName + "</option>";
					});
					if (items != '')
						$("#toPort").html(items);
					else $("#toPort").html('<option value="">.. Nije pronađena niti jedna luka ..<option>');

					$('#toPort').val(model_to);
				},
				complete: function () {
					HideLoader();
					$("#toPortName").val($("#toPort option:selected").text());
				}
			});

			is_page_first_load = false;
		}


		$(function () {

			$('#fromPort').change(function (e) {
				$("#fromPortName").val($("#fromPort option:selected").text());
				if ($("#fromPortName").val() == "")
					return;

				var date = $("#datepicker").datepicker("getDate");
				var date_string = pad(date.getDate().toString(), 2, '0', STR_PAD_LEFT) + pad((date.getMonth() + 1).toString(), 2, '0', STR_PAD_LEFT) + date.getFullYear().toString();


				$.ajax({
					url: '/pocetna/FindToPorts/',
					dataType: "json",
