					error: function (a, b, c) {

						alert('Dogodila se komunikacijska pogreška. Osvježite stranicu i pokušajte ponovno.');
					},
					success: function (data) {
						var items = '';
						$.each(data, function (i, toPort) {
							items += "<option value='" + toPort.PortId + "'>" + toPort.PortName + "</option>";
						});
						if (items != '') {
							$("#toPort").html(items);
							$('#toPort').val(port_to);
						} else $("#toPort").html('<option value="">.. Nije pronađena niti jedna luka ..<option>');
					},
					complete: function () {
						HideLoader();
						$("#toPortName").val($("#toPort option:selected").text());
					}
				});
			});


			$('#toPort').change(function (e) {
				$("#toPortName").val($("#toPort option:selected").text());
			});


			$('#lineSearchBtn').click(function (e) {

				e.preventDefault();

				if (!IsValid()) return;


				var date = $("#datepicker").datepicker("getDate");
				var date_string = pad(date.getDate().toString(), 2, '0', STR_PAD_LEFT) + pad((date.getMonth() + 1).toString(), 2, '0', STR_PAD_LEFT) + date.getFullYear().toString();

				var url = '/' + 'hr' + '/schedule/LineSearchResults/Index/' + $('#fromPort').val() + '/' + $('#toPort').val() + '/' + date_string;
				window.location.href = url;

				

			});
		});


	});


	function ShowLoader() {
		$("#datepicker").prop("disabled", true);
		$("#fromPort").prop("disabled", true);
		$("#lineSearchBtn").prop("disabled", true);
		$("#ajaxLoader").show();
	}

	function HideLoader() {
		$("#datepicker").prop("disabled", false);
		$("#fromPort").prop("disabled", false);
		$("#lineSearchBtn").prop("disabled", false);
		$("#ajaxLoader").hide();
	}

	function IsValid() {

		$(".lineSearchStar").hide();

		var isValid = true;

		if ($('#fromPort').val() == "") {
			$("#lineSearchDeparturePortInputId").show();
			isValid = false;
		}

		if ($('#toPort').val() == "") {
			$("#lineSearchArrivalPortInputId").show();
			isValid = false;
		}

		return isValid;
	}
</script>
