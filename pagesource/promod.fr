		var timerPertimm;
		var paramsGetAutocompletion = new Object();
		var paramsGetLangAutocompletion = new Object();
		paramsGetLangAutocompletion.search_locale = "fr-FR";
		paramsGetLangAutocompletion.user_locale = "fr-FR";
				function selectSuggest() {
			delectSuggests();
			jQuery(this).addClass("select");
		}
				function delectSuggests() {
			jQuery("#autocompletion .autocomplete-item.select").removeClass("select");
		}
				function deleteSuggests() {
			clearTimeout(timerPertimm);
			jQuery("#autocompletion").hide();
			delectSuggests();
		}
				function displaySuggests() {
			var autocompletion = jQuery("#autocompletion");
			if (!autocompletion.is(':empty')) {
				autocompletion.show();
			}
		}
				function executeSuggest() {
			if (jQuery(this).hasClass("autocomplete-keywords")) {
								ga("send", "Autocompletion", "Suggestion", jQuery(this).text());
				jQuery("#lucenequery_text").val(jQuery(this).text());
				jQuery("#recherchemotclefhd").submit();
			} else if (jQuery(this).hasClass("autocomplete-produit")) {
								ga("send", "Autocompletion", "Article", jQuery(this).data("ref"));
				jQuery(this).find("a").get(0).click();
			}
		}
				function runPertimmSuggests() {
			paramsGetAutocompletion.query = jQuery("#lucenequery_text").val();
			if (paramsGetAutocompletion.query.length < 2) {
				deleteSuggests();
				return;
			}
			jQuery.ajax({
				type: "GET",
				dataType: "json",
				url: window.location.protocol+"//"+window.location.hostname+"/promod-prod.pertimm.net/001-promod/api/?format=json&action=wordWheel&max_suggests=10&"+jQuery.param(paramsGetLangAutocompletion)+"&"+jQuery.param(paramsGetAutocompletion),

				success: function(data, textStatus, jqXHR){
					appendSuggests(data);
				}
			});
		}

				function noAccent(str){
			var rules = {
					a:"àáâãäå",
					A:"ÀÁÂ",
					e:"èéêë",
					E:"ÈÉÊË",
					i:"ìíîï",
					I:"ÌÍÎÏ",
					o:"òóôõöø",
					O:"ÒÓÔÕÖØ",
					u:"ùúûü",
					U:"ÙÚÛÜ",
					y:"ÿ",
					c:"ç",
					C:"Ç",
					n:"ñ",
					N:"Ñ"
					}; 
		 
			 function  getJSONKey(key){
				 for (acc in rules){
				 	if (rules[acc].indexOf(key)>-1){return acc}
				 }
			 }
			 
			function replaceSpec(Texte){
				regstring=""
				for (acc in rules){
				 	regstring+=rules[acc]
				 }
				reg=new RegExp("["+regstring+"]","g" )
				return Texte.replace(reg,function(t){ return getJSONKey(t) });
			}
			 
			return replaceSpec(str);
		}
		
				function appendSuggests(reponsePertimm) {
			if (reponsePertimm.status == "failed" || reponsePertimm.returned_anwsers_count == 0) {
				jQuery("#autocompletion").hide();
				jQuery("#autocompletion").html("");
				return;
			}
			var htmlKeywords = "";
			var htmlProduits = "";
			for (var i in reponsePertimm.answers) {
				if (reponsePertimm.answers[i].section == "keywords") {
					htmlKeywords += "<li class='autocomplete-item autocomplete-keywords'>";
					htmlKeywords += "<span class='autocomplete-infos'>"+reponsePertimm.answers[i].value+"</span>";
					htmlKeywords += "</li>";
				} else if (reponsePertimm.answers[i].section == "produit") {
					htmlProduits += "<li class='autocomplete-item autocomplete-produit' data-ref='"+reponsePertimm.answers[i].ref+"'>";
					htmlProduits += "<img src='https://www.promod.fr"+noAccent(reponsePertimm.answers[i].photo)+"'>";
					htmlProduits += "<div class='autocomplete-infos'>"+reponsePertimm.answers[i].value+"</div>";
					htmlProduits += "<div class='autocomplete-prix'><span class='autocomplete-prix-normal'>"+displayPrice(reponsePertimm.answers[i].prix,'[unite],[centime] €')+"</span> ";
					if (!!reponsePertimm.answers[i].prix_barre && reponsePertimm.answers[i].prix_barre != reponsePertimm.answers[i].prix)
						htmlProduits += "<br/><span class='autocomplete-prix-barre'>"+displayPrice(reponsePertimm.answers[i].prix_barre,'[unite],[centime] €')+"</span>";
					htmlProduits += "</div>";
					htmlProduits += "<div class='autocomplete-famille'>"+reponsePertimm.answers[i].famille;
					if (!!reponsePertimm.answers[i].sous_familles)
						htmlProduits += "<span class='autocomplete-sous-famille'> > "+reponsePertimm.answers[i].sous_familles+"</span>";
					htmlProduits += "</div>";
					htmlProduits += "<a href='https://www.promod.fr"+noAccent(reponsePertimm.answers[i].url)+"'></a>";
					htmlProduits += "</li>";
				}
			}
			if (htmlKeywords != "")
				htmlKeywords = "<li class='autocomplete-titre'>suggestions</li>"+htmlKeywords;
			if (htmlProduits != "")
				htmlProduits = "<li class='autocomplete-titre'>article(s)</li>"+htmlProduits;
			jQuery("#autocompletion").html("<ul>"+htmlKeywords+htmlProduits+"</ul>");
			displaySuggests();
			jQuery("#autocompletion .autocomplete-item").mouseover(selectSuggest);
			jQuery("#autocompletion .autocomplete-item").click(executeSuggest);
		}
				jQuery("#lucenequery_text").keydown(function(e){
						if (e.keyCode == 9 || e.keyCode == 13) {
				var select = jQuery("#autocompletion .autocomplete-item.select");
				if (select.length != 0) {
					e.preventDefault();
					select.click();
					return;
				}
			}
						if (e.keyCode == 27) {
				clearTimeout(timerPertimm);
				e.preventDefault();
				deleteSuggests();
				return;
			}
						if (e.keyCode == 40) {  
				clearTimeout(timerPertimm);
				if (jQuery("#autocompletion").is(":hidden")) {
					displaySuggests();
					return;
				}
				var select = jQuery("#autocompletion .autocomplete-item.select");
				if (select.length != 0) {
					var next = select.next("#autocompletion .autocomplete-item").mouseover();
					if (next.length == 0) {
						if (select.hasClass("autocomplete-keywords")) {
							jQuery("#autocompletion .autocomplete-produit").first().mouseover();
						} else if (select.hasClass("autocomplete-produit")) {
							jQuery("#autocompletion .autocomplete-keywords").first().mouseover();
						}
					}
					return;
				} else {
					jQuery("#autocompletion .autocomplete-item").first().mouseover();
					return;
				}
			}
						if (e.keyCode == 38) {
				var select = jQuery("#autocompletion .autocomplete-item.select");
				var prev = select.prev("#autocompletion .autocomplete-item").mouseover();
				if (prev.length == 0) {
					clearTimeout(timerPertimm);
					if (select.hasClass("autocomplete-keywords")) {
						jQuery("#autocompletion .autocomplete-produit").last().mouseover();
					} else if (select.hasClass("autocomplete-produit")) {
						jQuery("#autocompletion .autocomplete-keywords").last().mouseover();
					}
				}
				return;
			}
						if (e.keyCode == 37 || e.keyCode == 39) {
				displaySuggests();
				return;
			}
		});
				if (navigator.appName.indexOf("Internet Explorer") != -1 && navigator.appVersion.indexOf("MSIE 9") != -1) {
			jQuery("#lucenequery_text").keyup(function(e){
								if (e.keyCode == 8) { 
					clearTimeout(timerPertimm);
					timerPertimm = setTimeout(runPertimmSuggests,200);
				}
			});
		}
				jQuery("#lucenequery_text").on("input",function(){
			clearTimeout(timerPertimm);
			timerPertimm = setTimeout(runPertimmSuggests,200);
		});
				jQuery("#lucenequery_text").blur(function(){
			clearTimeout(timerPertimm);
			setTimeout(deleteSuggests,200);
		});
				jQuery("#recherchemotclefhd").on("submit", function() {
			var keyword = jQuery("#lucenequery_text").val();
			ga("send", "pageview", "/?lucenequery=" + keyword);
		});
	</script>
	</div>

