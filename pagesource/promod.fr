<script type="text/javascript">
jQuery.noConflict();

var OnMenuOpenTimer = function () {
	ssmenuitem = ssmenuitem.addClass("visibility-on");
}

// Action au survol du titre d'un onglet : on change le survol de l'onglet et si sous menu, on l'affiche
var OnMenuOpen = function () {

		 	jQuery(this).addClass("item-level0-on");
	
	if(jQuery(this).children(".menu-level1")){
		ssmenuitem = jQuery(this).children(".menu-level1");
		timer = setTimeout( function () { OnMenuOpenTimer() }, 200);
	}
}


var ClearTimeOut = function () {
	clearTimeout(timer);
}


//Action quand on quite un sous menu
var OnMenuClose = function () {
	ClearTimeOut();
		 jQuery(this).removeClass("item-level0-on");
	
	if(ssmenuitem)  ssmenuitem.removeClass("visibility-on");
	ssmenuitem =0;
}

var OnSousMenuClose = function () {
	if(jQuery(this).children(".menu-level1").length){
		jQuery(this).children(".menu-level1").removeClass("visibility-on");
	}
}

var OnTouchSwitch = function () {

	jQuery(this).click(function(e){if(jQuery(this).parent("li").children(".menu-level1").length){return false;}});

	jQuery('#menu-level0 > li').not(jQuery(this).parent("li")).each(OnMenuClose).each(OnSousMenuClose);

		 jQuery(this).parent("li").toggleClass("item-level0-on");
	
	if(jQuery(this).parent("li").children(".menu-level1").length){
		jQuery(this).parent("li").children(".menu-level1").toggleClass("visibility-on");
	}
}

var OnAllClose = function () {
	jQuery("#menu-level0 > li").each(OnMenuClose).each(OnSousMenuClose);
}


function is_touch_device() {
	try{
		document.createEvent('TouchEvent');
		var touch = true;
	}catch(e){
		var touch = false;
	}
	
	return touch && ('ontouchstart' in window || 'onmsgesturechange' in window);
}

// Procédure d'initialisation
var ssmenuitem = 0;
var timer = 0;
var Init = function () {

	if(is_touch_device()){
		jQuery("#menu-level0 > li> div").bind('touchstart', OnTouchSwitch);
		jQuery("#menu-level0 > li> a").bind('touchstart', OnTouchSwitch);
	    jQuery("#page").bind('touchstart', OnAllClose);
	  	jQuery("#bottom").bind('touchstart', OnAllClose);
	   	jQuery("#nav_verticale").bind('touchstart', OnAllClose);
	}else{
		jQuery('#menu-level0 > li').mouseenter(OnMenuOpen).mouseleave(OnAllClose);
	}
     }

// On lance la procédure d'initialisation quand tout le document est chargé
jQuery().ready(Init);

</script>
