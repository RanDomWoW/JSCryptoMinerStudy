			<script language="javascript" type="text/javascript">
			$(document).ready(function() { 
					slideShow();
				});
				
				function slideShow() {
					$('#gallery a').css({opacity: 0.0});   
					$('#gallery a:first').css({opacity: 1.0});   
					$('#gallery .caption').css({opacity: 0.7});
					setInterval('gallery()',6000);
					
					
					$('#kur_gallery a').css({opacity: 0.0});   
					$('#kur_gallery a:first').css({opacity: 1.0});   
					$('#kur_gallery .kur_caption').css({opacity: 0.7});
					setInterval('kur_gallery()',6000);
					
				}
				
				function gallery() {
					var current = ($('#gallery a.show')?  $('#gallery a.show') : $('#gallery a:first'));
					var next = ((current.next().length) ? ((current.next().hasClass('caption'))? $('#gallery a:first') :current.next()) : $('#gallery a:first'));   
					var caption = next.find('img').attr('rel');   
					next.css({opacity: 0.0}).addClass('show').animate({opacity: 1.0}, 1000);
					current.animate({opacity: 0.0}, 1000).removeClass('show');   
				}
				
				function kur_gallery() {
					var current = ($('#kur_gallery div.show')?  $('#kur_gallery div.show') : $('#kur_gallery div:first'));
				    var next = ((current.next().length) ? ((current.next().hasClass('kur_caption'))? $('#kur_gallery div:first') :current.next()) : $('#kur_gallery div:first'));   
				    //var caption = next.find('img').attr('rel');   
				    next.css({opacity: 0.0}).addClass('show').animate({opacity: 1.0}, 1000);
				    current.animate({opacity: 0.0}, 1000).removeClass('show');   
				}
			</script>
			
