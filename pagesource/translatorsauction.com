        function recalcIndexTopTime() {
            var now = moment().utc().unix();
            var ls = $('.index-top-time');
            for (var i = 0; i < ls.length; i++) {
                var timer = $(ls[i]);
                var dt = parseInt(timer.attr('index-top-time')) - now;
                if (dt <= 0) {
                    var panelId = timer.attr('index-top-time-panel');
                    $('#' + panelId).remove();
                    return ;
                }
                dt = Math.ceil(dt / 60);
                var h = Math.floor(dt / 60);
                var m = dt - h * 60;
                if (m < 10) m = '0' + m;
                timer.text(h + ':' + m);
            }
        }
        
                
        function share_fb() {
