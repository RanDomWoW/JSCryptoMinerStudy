        function OpenValidation(type, message) {
            if (type == 1) {
                $('.alert').addClass('alert-success');
            }
            else if (type == 2) {
                $('.alert').addClass('alert-danger');
            }
            else if (type == 3) {
                $('.alert').addClass('alert-info');
            }
            else if (type == 4) {
                $('.alert').addClass('alert-warning');
            }

            $(".alert p").html(message);
            $('.alert').fadeIn();
        }

        function OpenValidationByClassName(type, message, className) {
            $("#alert").removeClass();
            $("#alert").addClass('alert');
            var name = "." + className;
            if (type == 1) {
                $(name).addClass('alert-success');
            }
            else if (type == 2) {
                $(name).addClass('alert-danger');
            }
            else if (type == 3) {
                $(name).addClass('alert-info');
            }
            else if (type == 4) {
                $(name).addClass('alert-warning');
            }

            $(name + " p").html(message);
            $(name).fadeIn();
        }
    </script>
