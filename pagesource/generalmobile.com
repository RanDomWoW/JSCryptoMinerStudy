<script type="text/javascript">
    function OrderTraking(type, value) {
        var orderNumber = "";
        var mail = $("#MailTrackingTextBox").val();
        if (type == "HeaderText") {
            orderNumber = $("#OrderTrackingTextBox").val();
        }
        else if (type == "OrderDetail") {
            orderNumber = value;
        }

        if (orderNumber != "") {
            mail = SetMail(mail, type);

            if (mail != "") {
                AjaxOperation(orderNumber, mail, type);
            }
            else {
                FieldErrorMessage(type);
            }
        }
        else {
            FieldErrorMessage(type);
        }
    }

    function SetMail(mail, type) {
        if ('False' == 'True') {
            mail = " ";
        }

        if (type == "OrderDetail") {
            mail = " ";
        }

        return mail;
    }

    function AjaxOperation(orderNumber, mail, type) {
        var url = "/ajax/tracking?language=" + 'en' + "&orderNumber=" + orderNumber + "&email=" + mail + "&type=" + type;
        $.ajax({
            type: 'GET',
            url: url,
            success: function (data) {
                if (!data.HasOrder) {
                    ResultTrackingErrorMessage(type);
                }
                else if (data.HasOrder && data.IsCargo) {
                    var trackingUrl = data.TrackingUrl;
                    window.open(trackingUrl);
                }
                else if (data.HasOrder && !data.IsCargo) {
                    var trackingUrl = data.TrackingUrl;
                    window.location.href = trackingUrl;
                }
            }
        });
    }

    function ResultTrackingErrorMessage(type) {
        if (type != "OrderDetail") {
            if ('False' == "True") {
                //.basketBag .userLoginInfo .ResultMessage
                $("#OrderTrackingResultMessage").empty().append('There are no records belonging to order numbers.');
            }
            else {
                $("#OrderTrackingResultMessage").empty().append(' The Order Number and Mail Address Registered There Was No Entry');
            }
        }
    }

    function FieldErrorMessage(type) {
        if (type != "OrderDetail") {
            if ('False' == "True") {
                $("#OrderTrackingResultMessage").empty().append('');
            }
            else {
                $("#OrderTrackingResultMessage").empty().append('Please fill in the Mail and Order Number field.');
            }
        }
    }
</script>
