			var timesOfToday = [ todayTime.imsak, todayTime.gunes, todayTime.ogle, todayTime.ikindi, todayTime.aksam, todayTime.yatsi ];
			var timesOfTomorrow = [ tomorrowTime.imsak ];
			var vakit_count=0;
			var kalansaat;
			var kalandakika;
			var kalansaniye;
			var which_vakit;

			function calculate(){
				date = new Date();
				hour = date.getHours();
				minute = date.getMinutes();
				second = date.getSeconds();

				for( i in timesOfToday ){
					minute = parseInt(minute);
					var vakit = timesOfToday[i].split(":");
					vakit[0] = parseInt(vakit[0]);
					vakit[1] = parseInt(vakit[1]);
					var tmp1 = vakit[0]*60*60+vakit[1]*60;
					var tmp2 = hour*60*60+minute*60+second;

					if( tmp2 <= tmp1 ){
						kalansaat = parseInt((tmp1-tmp2)/3600);
						kalandakika = parseInt(((tmp1-tmp2)/60)%60);
						kalansaniye = parseInt((tmp1-tmp2)%60);
						which_vakit = i;
						vakit_count = 1;
						break;
					}
				}
				var tmp3;

				if (vakit_count == 0) {
					var vakit = timesOfTomorrow[0].split(":");
					vakit[0] = parseInt(vakit[0]);
					vakit[1] = parseInt(vakit[1]);
					minute = parseInt(minute);
					tmp3 = 24*60*60-( hour*60*60+minute*60+second);
					tmp3 += (vakit[0]*60*60 + vakit[1]*60);
					kalansaat = parseInt((tmp3)/3600);
					kalandakika = parseInt(((tmp3)/60)%60);
					kalansaniye = parseInt((tmp3)%60);
					which_vakit = 0;
				}
			}

			var Vakit_name = ["Sabah","Sabah","Öğle","İkindi","Akşam","Yatsı"];
			function countdown() {
				if(vakit_count)
					$("#tr"+which_vakit).addClass("highlight");
				if (which_vakit == 1) {
					$('#cnt').html( "Sabah Namazının Çıkmasına<br><b>" + kalansaat + " Saat " + kalandakika + " Dakika " + kalansaniye + " Saniye</b><br>kaldı.");
				} else if (which_vakit != 1 && which_vakit <= 5) {
				   $('#cnt').html( Vakit_name[which_vakit] + " Namazına <br><b>" + kalansaat + " Saat " + kalandakika + " Dakika " + kalansaniye + " Saniye</b><br>kaldı.");
				}
				kalansaniye--;
				if (kalansaniye == -1 && kalandakika == 0 && kalansaat == 0) {
					location.reload();
				} else if (kalansaniye == -1 && kalandakika == 0) {
					kalansaniye = 59;
					kalandakika = 59;
					kalansaat--;
				} else if (kalansaniye == -1) {
					kalansaniye = 59;
					kalandakika--;
				}
			}

			var hijriDate = todayTime.hDate.split('-');
			hijriDate[1] = hijriMonthNames[parseInt(hijriDate[1])-1];
			hijriDate = hijriDate[2]+' '+hijriDate[1]+' '+hijriDate[0];

		</script>

