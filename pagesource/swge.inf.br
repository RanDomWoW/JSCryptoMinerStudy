	function f_SoNumero(e)
	{
		if (document.all) {
			var tecla = window.event.keyCode;
		} else
		{
			var tecla = e.charCode;
		}
		if ((tecla < 48 || tecla > 57) && (tecla != 8 /* backspace*/) && (tecla != 9 /* tab*/) && (tecla != 37 /* left arrow*/) && (tecla != 39 /* right arrow*/) && (tecla != 46 /* delete*/))
		//if (tecla < 48 || tecla > 57)
		{
			if (document.all) {
				window.event.keyCode = 0;
			} else
			{
				e.preventDefault();
			}
		}
	}
	
	function f_MascaraData(obj)
	{
		//RETIRANDO ., -
		vSTRING = obj.value;
		vSTRING = vSTRING.replace('/','');
		vSTRING = vSTRING.replace('/','');
		if (vSTRING.length >= 5)
		{
			vSTRING = vSTRING.substr(0, 4) + '/' + vSTRING.substr(4);
		}
		if (vSTRING.length >= 3)
		{
			vSTRING = vSTRING.substr(0, 2) + '/' + vSTRING.substr(2);
		}
		obj.value = vSTRING;
	}
	
	function f_MascaraCPF(obj)
	{
		//RETIRANDO ., -
		vSTRING = obj.value;
		vSTRING = vSTRING.replace('.','');
		vSTRING = vSTRING.replace('.','');
		vSTRING = vSTRING.replace('.','');
		vSTRING = vSTRING.replace('-','');
		if (vSTRING.length >= 10)
		{
			vSTRING = vSTRING.substr(0, 9) + '-' + vSTRING.substr(9);
		}
		if (vSTRING.length >= 7)
		{
			vSTRING = vSTRING.substr(0, 6) + '.' + vSTRING.substr(6);
		}
		if (vSTRING.length >= 4)
		{
			vSTRING = vSTRING.substr(0, 3) + '.' + vSTRING.substr(3);
		}
		obj.value = vSTRING;
	}
	
	function f_ValidaCPF(CPF)
	{
		//RETIRANDO ., -
		vCPF = CPF;
		vCPF = vCPF.replace('.','');
		vCPF = vCPF.replace('.','');
		vCPF = vCPF.replace('.','');
		vCPF = vCPF.replace('-','');
		if (vCPF.length != 11)
		{
			return false;
		}
		if (vCPF == '11111111111' 
				|| vCPF == '22222222222' 
				|| vCPF == '33333333333' 
				|| vCPF == '44444444444' 
				|| vCPF == '55555555555' 
				|| vCPF == '66666666666' 
				|| vCPF == '77777777777' 
				|| vCPF == '88888888888' 
				|| vCPF == '99999999999' 
				|| vCPF == '00000000000')
		{
			return false;
		}
		//CALCULO DO PRIMEIRO DIGITO
		vSOMA = 0;
		j = 0;
		for (k=0;k<9;k++)
		{
			j = j + 1;
			vSOMA += eval(vCPF.charAt(k)*(j));
		}
		vDIGITO1 = vSOMA%11;
		if (vDIGITO1 == 10)
		{
			vDIGITO1 = 0;
		}
		if (vDIGITO1 != vCPF.charAt(9))
		{
			return false;
		}
		//CALCULO DO SEGUNDO DIGITO
		vSOMA = 0;
		j = 0;
		for (k=0;k<9;k++)
		{
			j = j + 1;
			vSOMA += eval(vCPF.charAt(k + 1)*(j));
		}
		vDIGITO2 = vSOMA%11;
		if (vDIGITO2 == 10)
		{
			vDIGITO2 = 0;
		}
		if (vDIGITO2 != vCPF.charAt(10))
		{
			return false;
		}
		
		return true;
	}
	
	function f_MascaraCEP(obj)
	{
		//RETIRANDO -
		vSTRING = obj.value;
		vSTRING = vSTRING.replace('-','');
		if (vSTRING.length >= 6)
