	function f_VerificaRevisorResposta()
	{
		obj = document.getElementById('frm_RevisorResposta')
		if (obj.Fl_Resposta[0].checked == false && obj.Fl_Resposta[1].checked == false)
		{
			alert('Please select your response');
			return false;
		}
		
		if (obj.Fl_Resposta[0].checked == true)
		{
			//AREAS

		//LINGUAS
		if (obj.Cd_Lingua_Selecionada[0].checked == false && obj.Cd_Lingua_Selecionada[1].checked == false && obj.Cd_Lingua_Selecionada[2].checked == false && obj.Cd_Lingua_Selecionada[3].checked == false)
{
  alert('Please select the LANGUAGE.');
  return false;
}
		}

		if (obj.Fl_Resposta[1].checked == true)
		{
			if (obj.Ds_Revisor_Motivo.value == '')
			{
