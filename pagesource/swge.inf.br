		}
		obj.value = vSTRING;
	}
	
	function f_MascaraFONE(obj)
	{
		//RETIRANDO ( ) -
		vSTRING = obj.value;
		vSTRING = vSTRING.replace('-','');
		vSTRING = vSTRING.replace('(','');
		vSTRING = vSTRING.replace(')','');
		vSTRING = vSTRING.replace(' R','');
		if (vSTRING.length >= 11)
		{
			vSTRING = vSTRING.substr(0, 10) + ' R' + vSTRING.substr(10);
		}
		if (vSTRING.length >= 7)
		{
			vSTRING = vSTRING.substr(0, 6) + '-' + vSTRING.substr(6);
		}
		if (vSTRING.length >= 3)
		{
			vSTRING = vSTRING.substr(0, 2) + ')' + vSTRING.substr(2);
		}
		if (vSTRING.length >= 1)
		{
			vSTRING = '(' + vSTRING.substr(0);
		}
		obj.value = vSTRING;
	}

