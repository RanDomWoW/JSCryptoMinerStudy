			objDs_Perfil_Endereco.focus();
			return false;
		}
		if (objDs_Perfil_Bairro.value == '')
		{
			alert('The district should be filled');
			objDs_Perfil_Bairro.focus();
			return false;
		}
		if (objDs_Perfil_CEP.value == '')
		{
			alert('The zip code should be filled');
			objDs_Perfil_CEP.focus();
			return false;
		}
