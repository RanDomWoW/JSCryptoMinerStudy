	function f_VerificaProcessarRetorno()
	{
		objDs_Arquivo = document.getElementById("Ds_Arquivo");
		if (objDs_Arquivo.value == '')
		{
			alert('Informe o arquivo');
			return false;
			objDs_Arquivo.focus();
		}
		if (objDs_Arquivo.value.length < 5)
		{
			alert('Informe o arquivo TXT corretamente');
			return false;
		}
		if ((objDs_Arquivo.value.substring(objDs_Arquivo.value.length - 3).toUpperCase() != 'TXT') && (objDs_Arquivo.value.substring(objDs_Arquivo.value.length - 3).toUpperCase() != 'RET'))
		{
			alert('O arquivo deve ter extensão TXT ou RET');
			objDs_Arquivo.focus();
			return false;
		}

		return true;
	}

	function f_VerificaAssociadosABCM()
	{
		objDs_Arquivo = document.getElementById("Ds_Arquivo");
		if (objDs_Arquivo.value == '')
		{
			alert('Informe o arquivo');
			return false;
			objDs_Arquivo.focus();
		}
		if (objDs_Arquivo.value.length < 5)
		{
			alert('Informe o arquivo EXCEL corretamente');
			return false;
		}
		if ((objDs_Arquivo.value.substring(objDs_Arquivo.value.length - 3).toUpperCase() != 'XLS') && (objDs_Arquivo.value.substring(objDs_Arquivo.value.length - 4).toUpperCase() != 'XLSX'))
		{
			alert('O arquivo deve ter extensão XLS ou XLSX');
			objDs_Arquivo.focus();
			return false;
		}

		return true;
	}

