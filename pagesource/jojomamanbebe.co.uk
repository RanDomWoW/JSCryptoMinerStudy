            var el = $(Event.element(event));
            //validate email
            if(el.value && this.validateEmail(el.value)){
                //ajax call
                new Ajax.Request('https://www.jojomamanbebe.co.uk/' + 'connector/ajax/emailcapture', {
                    method: 'post',
