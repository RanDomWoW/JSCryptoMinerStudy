<script type="text/javascript">
    jQuery(document).ready(function($) {
        // Clone desktop nav in preparation for mmenu structure changes
        $('#nav').clone().insertAfter('#nav').attr('id', 'nav-mobile');

        // Remove li.section and it's child ul but keep the content within the ul
        // Sets up the navigation to work with mmenu
        $('#nav-mobile .section').each(function(){
            // save parent ul
            var parentUl = $(this).parent();
            // save content within .section
            var sectionContent = $(this).children().children();
            // remove $this section
            $(this).remove();
            // append content to parent ul
            parentUl.append(sectionContent);
        });

        // Remove any unwanted elements
        $('#nav-mobile img').parent().remove();
        $('.mm-listview').removeClass('col-5 col-4 col-3 col-2 col-1');
        $('#nav-mobile .arrow').remove();

        /**
         * Make all category sections the same height
         */
        var categories = $('#nav .level0');
        var sections;
        var maxHeight = 0;

        // Each category might have a different max height
        categories.each(function(){
            $(this).addClass('menu-active');
            sections = $(this).find('.section');

            sections.each(function(){
                if(maxHeight < $(this).children().height()) {
                    maxHeight = $(this).children().height();
                }
            });

            $(this).removeClass('menu-active');

            sections.height(maxHeight);
            // Reset maxHeight for next category
            maxHeight = 0;
        });

        /**
         * Initialise mmenu
         */
        $("#nav-mobile").mmenu({
            "extensions": [
                "pagedim-black"
            ],
            navbar: {
                title: "Categories"
            }}, {
            offCanvas: {
                noPageSelector: ['.zopim']
            }
        });

        /**
         * iPad menu
         */
        $('.level0').on('click', function(e){
            var windowWidth = $(window).width();
            if(windowWidth > 599 && windowWidth < 901) {
                e.preventDefault(); // Stop link triggering
                $('.menu-active').removeClass('menu-active');
                $(this).addClass('menu-active');
            }
        });

        $('#nav .nav-primary li.section a').on('click', function(e){
            e.stopPropagation();
        });
    });
</script>
