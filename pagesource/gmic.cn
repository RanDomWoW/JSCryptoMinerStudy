                var purl = result[i].photo_url;
                tmp.find("img").attr({ "src": purl, "alt": result[i].name });
                tmp.find(".i1 b").text(result[i].name);
                tmp.find(".i1 i").eq(0).text(result[i].title);
                tmp.find(".i1 i").eq(1).text(result[i].company);
                tmp.appendTo(obj);
            }
        }
        function exhibitorBind(data,typeid) {
            if (data.Status != "SUCCESS" || data.ExhibitorList == [])
                return;
            var obj;
            var result = data.ExhibitorList;
            if (typeid == 5 && result.length > 0){
                jQuery(".m7").show();
                obj = jQuery(".m7 #exhibitors");
            }else if (typeid == 67 && result.length > 0){
                jQuery(".m8").show();
                obj = jQuery(".m8 #exhibitors");
            }
            for (var i = 0; i < result.length; i++) {
                var t2 = jQuery("<div class='b-c-5'><div><a href='javascript:;' target='_blank'><img /></a></div></div>");
                t2.find("a").attr("href", result[i].WebSite);
                var purl = result[i].LogoUrl;
                t2.find("img").attr({ "src": purl, "alt": result[i].NativeName });
                t2.appendTo(obj);
            }
        }

         function summitBind(data) {
            if (data.message != "SUCCESS")
                return;
            var result = data.result.summits;
            var len = data.result.total;
            var objs = jQuery("#summits a");
            var sumId = [2493,2480,2492,2484,2494,2486];
            var k = 0;
            for (var n = 0; n < len; n++) {
                var inArr = jQuery.inArray(result[n].id,sumId);
                if(inArr != -1){
                var tmp = jQuery(objs[k]);
                tmp.attr({ "sid": result[n].id, "title": result[n].name, "href": "/summit" + result[n].id });
                tmp.css("background-image", "url('" + result[n].thumbnail_url + "')");
                tmp.find("i").before(result[n].name);
                tmp.find(".i2").html("<div><h3 class='i_title'>" + result[n].name + "</h3>" + result[n].summary + "</div>");
                k++;
                }              
            }
        }

        function summitListBind(data) {
          if (data.message != "SUCCESS")
              return;
          var result = data.result.summits;
          var len = data.result.total;
          var waterflowLeftColumn = jQuery(".waterflow-column:eq(0)");
          var waterflowRightColumn = jQuery(".waterflow-column:eq(1)");
          var sumId = [2582,2610,2588,2589,2591,2606];
          var k = 0;
          for (var n = 0; n < len; n++) {
              var inArr = jQuery.inArray(result[n].id, sumId);
              if(inArr != -1){
                  var tmp = jQuery(".waterflow-box").first().clone();
                  tmp.attr("style", "display:block;");
                  tmp.find("a").attr("href", "/summit/?summit=" + result[n].id);
                  tmp.find(".box-title").text(result[n].name);
                  tmp.find(".box-content").text(result[n].summary);
                  var photos = result[n].speaker_photos;
                  if (photos.length > 0) {
                      tmp.find(".box-gallery").attr("style", "display:block;");
                      for (var i = 0; i < photos.length && i < 4; i++) {
                          tmp.find(".box-gallery").append('<img src="' + photos[i] + '" alt="" class="box-gallery-image" />');
                      }
                  }
                  if (k % 2 == 0) {
                      waterflowLeftColumn.append(tmp);
                  }
                  else {
                      waterflowRightColumn.append(tmp);
                  }
                  k++;
              }              
          }
        }


