      window.qlik = {};
      qlik.isExternalLink = function(el) {
        if (el instanceof HTMLAnchorElement) {
            return (el.host !== window.location.host);
        }
      };
      
      window.onload = function () {
          $('a').each(function() {
            if (qlik.isExternalLink(this)) {
                $(this).attr('target', '_blank');
