        if ((((swipe_det.eX - min_x > swipe_det.sX) || (swipe_det.eX + min_x < swipe_det.sX)) && ((swipe_det.eY < swipe_det.sY + max_y) && (swipe_det.sY > swipe_det.eY - max_y)))) {
          if (swipe_det.eX > swipe_det.sX) direc = "r";
          else direc = "l";
        }
        //vertical detection
        if ((((swipe_det.eY - min_y > swipe_det.sY) || (swipe_det.eY + min_y < swipe_det.sY)) && ((swipe_det.eX < swipe_det.sX + max_x) && (swipe_det.sX > swipe_det.eX - max_x)))) {
          if (swipe_det.eY > swipe_det.sY) direc = "d";
          else direc = "u";
        }
        handleswipe(direc);
      }, false);
    }


    function handleswipe(swipedir) {
      if (swipedir == 'l') {
        $('#center').hide();
        $('#left').show();
        swipedetectForLeftContainer(document.getElementById('left'));
      }

      if (swipedir == 'r') {
        $('#center').hide();
        $('#right').show();
        swipedetectForRightContainer(document.getElementById('right'));
      }
    }

    function handleswipeForRight(swipedir) {
      if (swipedir == 'l') {
        $('#right').hide();
        $('#center').show();
        swipedetect(document.getElementById('center'));
      }

      if (swipedir == 'r') {
        $('#right').hide();
        $('#left').show();
        swipedetectForLeftContainer(document.getElementById('left'));
      }
    }

    function handleswipeForLeft(swipedir) {
      if (swipedir == 'l') {
        $('#left').hide();
        $('#right').show();
        swipedetectForRightContainer(document.getElementById('right'));
      }

      if (swipedir == 'r') {
        $('#left').hide();
        $('#center').show();
        swipedetect(document.getElementById('center'));
      }
    }


    function init() {
      var xhttp = new XMLHttpRequest();
      xhttp.onreadystatechange = function () {
        if (this.readyState == 4 && this.status == 200) {
          var totalResponse = JSON.parse(this.responseText);
          totalResponse = totalResponse.result;
          var propertyNameArray = [];
          for (var i = 0; i < totalResponse.length; i++) {
            var tempArray = {
              "id": totalResponse[i].id,
              "latitude": totalResponse[i].location[1],
              "longitude": totalResponse[i].location[0],
              "locality": totalResponse[i].locality,
              "name": totalResponse[i].name,
              "city": totalResponse[i].city,
              "gender": totalResponse[i].gender
            }
            propertyNameArray.push(tempArray);
          }
          localStorage.setItem("propertyNames", JSON.stringify(propertyNameArray));
        }
      };
      xhttp.open("GET", window.location.origin + "/api/pgs/cities", true);
      xhttp.send();
    }

    function search() { /*go to bellandur in case of invalid locations*/
      // var lat=localScope.autoCompletePlace.geometry.location.lat();
      // var lng=localScope.autoCompletePlace.geometry.location.lng();
      var searchParamObj = {};
      if (localScope.city === 'Bangalore') {
        searchParamObj = {
          latitude: 12.9260308,
          longitude: 77.6762463,
          text: "Bellandur, Bengaluru, Karnataka 560103, India",
          gender: "all",
          city: localScope.city
        }
      } else {
        searchParamObj = {
          place: null,
          latitude: 18.5596581,
          longitude: 73.7799374,
          text: "Baner, Pune, Maharashtra 411045, India",
          gender: "all"
        }
      }
      navigateTo(window.location.origin + "/search/"+localScope.city+"?" + $.param(searchParamObj));
    }

    function navigateTo(url) {
      window.location.href = url;
    }

    $('body').removeClass('hidden');
    var isMobile = (/Android|BlackBerry|IEMobile/i.test(navigator.userAgent)) ? true : false;
    console.log(isMobile);
    if (isMobile) {
      var ele = document.getElementById('searchApp');
      ele.style = "";
      var ele1 = document.getElementById('navBar');
      ele1.style = "margin-top: 82px"
    }

    $('body').on('click', '.smartbanner-icon1', function () {
      var ele = document.getElementById('searchApp');
      ele.style.display = 'none';
      var ele1 = document.getElementById('navBar');
      ele1.style = "margin-top: 10px"
    });

  });
</script>
