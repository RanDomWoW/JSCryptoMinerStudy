<script type="text/javascript">
    $(document).ready(function() {
        var maskHeight = $(document).height();
        var maskWidth = $(window).width();

        $('#mask').css({'width': maskWidth, 'height': maskHeight});
        $('#maskmobile').css({'width': maskWidth, 'height': maskHeight});

        $('#mask').fadeIn(1000);
        $('#mask').fadeTo("slow", 0.6);
        $('#maskmobile').fadeIn(1000);
        $('#maskmobile').fadeTo("slow", 0.6);

        //Get the window height and width
        var winH = $(window).height();
        var winW = $(window).width();

        $('#dialog2').css('top', winH / 2 - $('#dialog2').height() / 2);
        $('#dialog2').css('left', winW / 2 - $('#dialog2').width() / 2);
        $('#dialogmobile').css('top', winH / 2 - $('#dialogmobile').height() / 2);
        $('#dialogmobile').css('left', winW / 2 - $('#dialogmobile').width() / 2);

        $('#dialog2').fadeIn(2000);
        $('#dialogmobile').fadeIn(2000);

        $('.window .close').click(function(e) {
            e.preventDefault();
            $('#mask').hide();
            $('#maskmobile').hide();
            $('.window').hide();
        });

        $('.window a').click(function(e) {
            e.preventDefault();
            $('#mask').hide();
            $('#maskmobile').hide();
            $('.window').hide();
        });

        $('#mask').click(function() {
            $(this).hide();
            $('.window').hide();
        });
        $('#maskmobile').click(function() {
            $(this).hide();
            $('.window').hide();
        });

        if (navigator.userAgent.match(/Android/i) || navigator.userAgent.match(/webOS/i)
                || navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPad/i)
                || navigator.userAgent.match(/iPod/i) || navigator.userAgent.match(/BlackBerry/i)
                || navigator.userAgent.match(/Windows Phone/i)) {
            $('#mask').hide();
        }
        else {
            $('#maskmobile').hide();
        }
    });

    function closeBannerAndroid() {
        $('#androidAD').load('./closeBannerAndroid/ #androidAD');
        $('#androidAD').hide();
    }
</script>
