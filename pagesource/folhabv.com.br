<script type="text/javascript">
    $(document).ready(
            function() {
                var offset = $('#menuMobile').offset().top + 10;
                var $barraOculta = $('#barraOculta'); // guardar o elemento na memoria para melhorar performance
                if ($('#androidADImage').length) {
                    offset = $('#menuMobile').offset().top + 70;
                    $barraOculta.css({
                        "top": "0px"
                    });
                }
                $barraOculta.addClass('display').fadeOut();
                $(document).on('scroll', function() {
                    if (offset <= $(window).scrollTop()) {
                        $barraOculta.addClass('fixar');
                        $barraOculta.removeClass('display');
                    } else {
                        $barraOculta.addClass('display');
                        $barraOculta.removeClass('fixar');
                    }
                });
            });
</script>

