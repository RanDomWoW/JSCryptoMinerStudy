            $.ajax({
                url: genUrl + restUrl,
                timeout: 0,
                error: function () {
                    // will fire when timeout is reached
                    console.log("Errore:" + idobject);
                },
                success: function (data) {
                    $(idobject).html(data);
                    //Quando sto riempiendo la colonna sinistra equilibrio quella centrale.
                    if (idobject == "#divAltreNews5") {
                        $(".colSxParte2").css("min-height", $(".colSxParte1").height());
                        $(".divCx").css("min-height", $(".colSxParte1").height());
                    }
                }
            });
        }
        function caricaBanner(restUrl, idobject) {
            $.ajax({
                url: genUrl + restUrl,
                timeout: 0,
                error: function () {
                    // will fire when timeout is reached
                    console.log("Errore:" + idobject);
                },
                success: function (data) {
                    //Verifico se c'è lo sfondo, nel caso sposto i banner in alto.
                    var sfondo = $("#hfSfondo").val();
                    if (sfondo == 1) {
                        if ((idobject == "#banTop5") || (idobject == "#banTop6")) {

                        }
                        else {
                            $(idobject).html(data);
                        }

                        if (idobject == "#banTop1") {
                            $("#banTop5").html(data);
                            $("#banTop1").css("display", "none");
                        }
                        if (idobject == "#banTop2") {
                            $("#banTop6").html(data);
                            $("#banTop2").css("display", "none");
                        }
                    }
                    else {
                        $(idobject).html(data);
                    }
                    //Controlli.
                    if (idobject == "#banTop5") {
                        if ($("#banTop5").html() == "&nbsp;") {
                            $("#banTop5").css("display", "none");
                        }
                        else {
                            $("#banTop5").css("display", "block");
                        }
                    }
                    if (idobject == "#banTop6") {
                        if ($("#banTop6").html() == "&nbsp;") {
                            $("#banTop6").css("display", "none");
                        }
                        else {
                            $("#banTop6").css("display", "block");
                        }
                    }
                    if (idobject == "#banTop7") {
                        //Quando attivo/disattivo il banner 990 x 80 sotto il PP devo attuare delle modifiche.
                        if (($("#banTop7").html() == "&nbsp;") || ($("#banTop7").html() == "")) {
                            $("#banTop7").css("display", "none");
                            $("#colDx").css("padding-top", "8px");
                            $("#colSxParte1").css("padding-top", "8px");
                            $("#colSxParte2").css("margin-top", "8px");
                            $("#divColSx2").css("padding-top", "8px");
                        }
                        else {
                            $("#divDxSpecial div:first").attr("style", "margin-top:0px !important");
                        }
                    }
                }
            });
        }

        function caricaPagina() {
            //Banner.
            caricaBanner("rest/home/banner.ashx?where=top&num=1", "#banTop1");
            caricaBanner("rest/home/banner.ashx?where=top&num=2", "#banTop2");
            caricaBanner("rest/home/banner.ashx?where=top&num=3", "#banTop3");
            caricaBanner("rest/home/banner.ashx?where=top&num=4", "#banTop4");
            caricaBanner("rest/home/banner.ashx?where=top&num=5", "#banTop5");
            caricaBanner("rest/home/banner.ashx?where=top&num=6", "#banTop6");
            caricaBanner("rest/home/banner.ashx?where=top&num=7", "#banTop7");
            //Lettere.
            //caricaAsinc("rest/home/lettere.ashx", "#divLettere");
            //RistoTV video.
            caricaAsinc("rest/home/ristotv.ashx", "#ristoTVSlide");
            //Primi piano.
            caricaAsinc("rest/home/primi-piano.ashx?where=1", "#pp1");
            if ($("#hfPP2").val() == "1") {
                caricaAsinc("rest/home/primi-piano.ashx?where=2", "#pp2");
            }
            else {
                $(".boxPP2").css("display", "none");
            }
            //Gestione dei box (primi in alto).
            caricaAsinc("rest/home/box-dxs.ashx", "#divDxSpecial");
            caricaAsinc("rest/home/box-dx.ashx", "#divDx");
            caricaAsinc("rest/home/box-cx.ashx", "#divCx");
            //News principali.
            caricaAsinc("rest/home/news-3.ashx", "#divNews3");
            caricaAsinc("rest/home/news-4.ashx", "#divNews4");
            caricaAsinc("rest/home/news-5.ashx", "#divNews5");
            caricaAsinc("rest/home/news-6.ashx", "#divNews6");
            //Altre news 1° blocco.
            caricaAsinc("rest/home/altrenews.ashx?l=4", "#divAltreNews");
            //Box sotto.
            caricaAsinc("rest/home/box-sx.ashx", "#divSx");
            caricaAsinc("rest/home/box-cx-2.ashx", "#divCx2");
            caricaAsinc("rest/home/box-cx-3.ashx", "#divCx3");
            caricaAsinc("rest/home/box-cx-4.ashx", "#divCx4");
            caricaAsinc("rest/home/box-dx-2.ashx", "#divDx2");
            caricaAsinc("rest/home/box-dx-3.ashx", "#divDx3");
            caricaAsinc("rest/home/box-dx-4.ashx", "#divDx4");
            //Altre  news blocchi sottostanti fino in fondo.
            caricaAsinc("rest/home/altrenews-2.ashx?i=5&l=4", "#divAltreNews2");
            caricaAsinc("rest/home/altrenews-3.ashx?i=9&l=4", "#divAltreNews3");
            caricaAsinc("rest/home/altrenews-4.ashx?i=13&l=4", "#divAltreNews4");
            var fine = parseInt($("#hfAltreNews").val()) - 16;
            caricaAsinc("rest/home/altrenews-5.ashx?i=17&l=" + fine, "#divAltreNews5");
        }

        $(document).ready(function () {
            caricaPagina();
            var t = setInterval("caricaPagina()", 300000);
        });
    </script>
