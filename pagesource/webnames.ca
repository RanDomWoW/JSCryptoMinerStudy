<script type="text/javascript">
    // Login modal
    $(function() {
        $('#inlineLoginLink').click(function(e) {            
            $('.sign-in:not(.inline-sign-in)').toggle().appendTo('#login');
            $('#login .fa:last-child').toggleClass('fa-caret-down fa-caret-up');
            //$('#txtUsername, input[name="username"]').focus();
            // disable focus for now, accessibility/usability issue
            $('#inlineLoginLink').attr('aria-expanded', $(this).attr('aria-expanded')==='false' ? 'true' : 'false');
            e.preventDefault();
        });

        $('#hideLoginForm').click(function(e) {            
            $('.sign-in:not(.inline-sign-in)').hide();
            $('#login .fa:last-child').toggleClass('fa-caret-up fa-caret-down');
            $('#inlineLoginLink').attr('aria-expanded', 'false')
            e.preventDefault();
        });      
    });

    // Mobile alias bar
    $('.bar-toggle').click(function() {
        $('.user-bar').toggleClass('show-user-bar');
        $('.bar-toggle .fa').toggleClass('fa-caret-down fa-caret-up');
        if ($('#child-users option:selected').val() === '') {
            $('.child-account .form-select').addClass('full-w');
        }
    }); 

    $(document).ready(function () {
        // Menu active/inactive
        var removeClass = true;
        var navItem = $('.nav-wrap > li > a');
        var activeNav = 'active-nav-item';

        navItem.click(function () {
            navItem.not(this).removeClass(activeNav);
            $(this).toggleClass(activeNav);
            removeClass = false;            
        });

        // when click event reaches 'html' : remove class if needed, and reset flag
        $('html').click(function () {
            if (removeClass) {
                navItem.removeClass(activeNav);
            }
            removeClass = true;
        });

        // small device menu veil toggle
        $('.nav-toggle').click(function() {
            if ($('.main-nav').attr('aria-expanded', 'false')) {
                $('.nav-toggle .fa').toggleClass('fa-bars fa-close');
                $('.navbar-text').toggleClass('hidden');
            } else {
                $('.nav-toggle .fa').toggleClass('fa-bars fa-close');
                $('.navbar-text').toggleClass('hidden');
            }
        });

        // Append usermenu to username
        $('#userMenu').click(function() {
            //$('#userMenu .fa:last-child').toggleClass('fa-caret-down fa-caret-up');
            $('.dropdown-menu[aria-labelledby="userMenu"]').appendTo('.signed-in');
        });

        // Scroll to top footer button
        $(".take-me-higher").click(function () {
            $("html, body").animate({ scrollTop: 0 }, 750);
            return false;
        });

        // Pass email to newsletter subscription
        $("#subscribe-submit").click(function(e) {
            e.preventDefault();
            var email = $("#newsletter").val();
            var uri = "newsletter/subscribe.aspx";
            if(!!email) uri += "?email=" + email;
            window.location.href = _SmartURL + uri;           
        });

        $(".fb-toggle").click(function() {
            $(".fb-form").toggle();
        });

        $("#feedback-submit").click(function(e) {
            e.preventDefault();
            var feedback = $("textarea[name='feedback']").val();             
            if(!feedback) return;
            var promise = $.ajax({
                type: "POST",
                url: _SmartURL + "feedback",
                data: JSON.stringify({sFeedback:feedback}),
                success: function(data){
                    $(".fb-form").toggle();
                    $(".fb-received").show();                    
                },                
                contentType: "application/json",
                dataType: "json"
            });
        })
    });   

