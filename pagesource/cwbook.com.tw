  <script type="text/javascript" language="javascript">
  function reChkSaleType() {
	  var searchField = $("#searchForm_searchField").val();
	  if (searchField) {
		  changeSearch1(searchField);
	  }
	  var serachProductSerno = function(hrefStr) {
		//get fisrt parameter
		var s = hrefStr;
		var n1 = s.indexOf("'");
		if (n1==-1) return null;
		++n1;
		var n2 = s.indexOf("'",n1);
		if (n2==-1) return null;
		return s.substring(n1, n2);
	  };
	  var arr = [];
	  $('a[href^="javascript:addTo"]').each(function(i) {
		  arr[i] = serachProductSerno(this.href);
	  });
	  $.post(getContextPath() + "/ProdInventoryAction.shtml", {serNos : arr.unique().join(",")}, function(data) {
		  data = eval(data);
		  var url = location.href;
		  var productPageYN = "N";
		  if (url.indexOf("product/ProductAction") > -1 || url.indexOf("subscription/SubscriptionAction") > -1) {//判斷是否為商品頁、頻道頁
			  productPageYN = "Y";
		  }
		  $('a[href^="javascript:addTo"]').each(function(i) {
			  var serNo = serachProductSerno(this.href);
			  if (!serNo) return;
			  for (var i=0; i<data.length; i++) {
				  if (serNo == data[i].serNo && !data[i].status) {
					  var a = $(this);
					  var x = $(this).parent(".book_data").children(".book_name").children("img");
					  a.attr("href","javascript:void(0)");
					  a.css("background","none");
					  if ($(this).hasClass("track_btn") || $(this).hasClass("track_btn2")) {
						  a.html("<img src=\"\" />");
					  } else {
						  if ("R"==data[i].status_value) {
							  a.html("<img src=\"/images/icon-74.png;jsessionid=57C7889A903622A2F87B8266424AAF31\" />");
							  x.attr("src", "/images/icon-74.png;jsessionid=57C7889A903622A2F87B8266424AAF31");
						  } else {
							  if (productPageYN == "Y") {//商品頁、訂閱頁沒庫存
								  $('#buyBtn button').addClass("disabled");
								  $('#buyBtn2 button').addClass("disabled");
							  } else {//非商品頁、非訂閱頁沒庫存
								  a.html("<img src=\"/images/icon-77.png;jsessionid=57C7889A903622A2F87B8266424AAF31\" />");
								  x.attr("src", "/images/icon-77.png;jsessionid=57C7889A903622A2F87B8266424AAF31");
							  }
						  }
					  }
					  if ($(".flag-status span").length) {
						  if ("R"==data[i].status_value) {
							  $(".flag-status span").removeAttr('class');
							  $(".flag-status span").attr("class", "status-coming-soon").text("即將銷售");
						  } else {
							  $(".flag-status span").removeAttr('class');
							  $(".flag-status span").attr("class", "status-soldout").text("已售完");
						  }
					  }
				  }
			  }
		  });
	  }, 'html');
      var keyWord = "";
      var searchBottomAge = "";
      var searchUpAge = "";
	  if (keyWord) {
		  $("#searchForm_searchKeyWord").val(keyWord);
	  }
	  if (searchBottomAge) {
		  $("#searchForm_searchBottomAge").val(searchBottomAge);
	  }
	  if (searchUpAge) {
		  $("#searchForm_searchUpAge").val(searchUpAge);
	  }
  }
 $(document).ready(function() {
	  reChkSaleType();
 });
</script>
