<script type="text/javascript">
	function submitFormEditus() {
		var result = editusClearAccentsAndSpecialCars($("#search-editus-input").val());
		$("#search-editus-input").val(result);
	}
	// Fonction permettant de parser les chaines pour editus
	function editusClearAccentsAndSpecialCars(string) {
		temp = string.replace(/[àâä]/gi, "a");
		temp = temp.replace(/[éèêë]/gi, "e");
		temp = temp.replace(/[îï]/gi, "i");
		temp = temp.replace(/[ôö]/gi, "o");
		temp = temp.replace(/[ùûü]/gi, "u");
		temp = temp.replace(/[&%*]/gi, "");
		temp = temp.replace(/[-/']/gi, "+");
		return temp;
	}

</script>
