<script type="text/javascript" language="javascript">
    $(document).ready(function () {

        $.ajax({
            url: "/Ajax/AjaxHandler.aspx?fn=GetCropData&edate=2018/03/19&epg=01&type=art",
            type: "POST",
            data: '',
            success: function (msg) {
                if (msg) {
                    showArticles(msg);
                }
            },
            error: function (response) {
                alert("Failed : " + response.status + "..." + response.statusText);
            }
        });
    });

    function showArticles(data) {
        var arts = data.split('|');
        for (var i = 0; i < arts.length; i++) {
            markArticle(arts[i]);
        }

    }

    function markArticle(art) {
        var id, rel, l, t, h, w, imgsrc;
        id = art.split(',')[0];
        rel = art.split(',')[1];
        l = art.split(',')[2];
        t = art.split(',')[3];
        h = art.split(',')[4];
        w = art.split(',')[5];
        imgsrc = art.split(',')[6];

        var hlink = $('<a></a>');
        hlink.attr('id', 'a' + id);
        hlink.attr('rel', 'artgroup');
        hlink.attr('href', imgsrc);
        //hlink.attr('title', 'Article ' + rel.split('-')[0]);

        var article = $('<div>&nbsp;</div>');
        article.attr('id', 'art' + id);
        article.attr('rel', rel);

        article.addClass("croparticle");
        article.css('left', l + 'px');
        article.css('top', t + 'px');
        article.css('height', h + 'px');
        article.css('width', w + 'px');
        article.appendTo(hlink);

        hlink.appendTo('div#pagepreview');


        $(article).bind('mouseenter', function (e) {
            $('div.croparticle').each(function (index) {
                if ($(this).attr('rel').split('-')[0] == rel.split('-')[0])
                    $(this).addClass("croparticlehover");
            });

        });

        $(article).bind('mouseleave', function (e) {
            $('div.croparticle').each(function (index) {
                if ($(this).attr('rel').split('-')[0] == rel.split('-')[0])
                    $(this).removeClass("croparticlehover");
            });

        });

        $('#a' + id).colorbox({ rel: 'artgroup',
            opacity: 0.7,
            slideshow: false,
            slideshowAuto: false,

            onComplete: function () {
                //restrict user to save image
                $('img.cboxPhoto').bind('contextmenu', function (e) {
                    return false;
                });

                //add share button
                jQuery.getScript('http://s7.addthis.com/js/250/addthis_widget.js#pubid=ra-4f28237b516b620e&domready=1', function () {

                    var tbx = $('<div id="cboxShare" class="addthis_toolbox addthis_default_style" addthis:url="' + imgsrc + '"></div>');
                    $("#cboxLoadedContent").prepend(tbx);

                    svcs = { facebook: 'Facebook', twitter: 'twitter', email: 'Email', print: 'Print', compact: 'compact' };

                    for (var s in svcs) {
                        tbx.html(tbx.html() + '<a class="addthis_button_' + s + '"></a>');
                    }
                    tbx.html(tbx.html() + '<a id="atcounter" class="addthis_counter addthis_bubble_style"></a>');
                    addthis.counter("#atcounter");
                    addthis.update('share', 'url', imgsrc);
                    addthis.init();
                    addthis.toolbox('#cboxShare');
                    
                    //resize colorbox
                    $.colorbox.resize();

                });

                //change pager caption
                var pagerText = $('#cboxCurrent').html().replace("image", "Article");
                $('#cboxCurrent').html(pagerText);


                //fix height
                var window_height = $(window).height();
                var colorbox_height = $('#colorbox').height();
                var top_position = 0;

                if (window_height > colorbox_height) {
                    top_position = (window_height - colorbox_height) / 2;
                }

                $('#colorbox').css({ 'top': top_position });
                scroll(0, 0);
            }
        });
    }        
</script>


