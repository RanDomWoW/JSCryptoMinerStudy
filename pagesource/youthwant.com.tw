    <script type="text/javascript">
    setInterval(function(){
        var date_1 = new Date().getTime();    
        var date_2 = new Date(, , , 0, 0, 0).getTime();    
        var minus = (date_2 - date_1) / 1000;
        var days  = parseInt(minus / 86400);
        var d = minus - (days * 86400);
        var h = parseInt( d / 3600 );
        var m = parseInt((d - (h * 3600)) /  60 );
        var s = parseInt((d - (h * 3600)) - (m * 60) );
        h += days * 24;
		if(h < 0) {
			h = 0;
		}
		if(m < 0) {
			m = 0;
		}
		if(s < 0) {
			s = 0;
		}
        document.getElementById("g_hour").innerHTML = h;
        document.getElementById("g_min").innerHTML  = m;
        document.getElementById("g_sec").innerHTML  = s;
    }, 1000);
    </script>
	  
	  
