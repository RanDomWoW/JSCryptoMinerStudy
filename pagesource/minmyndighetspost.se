         if (!supressedMessages) {
         	supressedMessages = nodeId;
         } else {
         	supressedMessages = supressedMessages + ";" + nodeId;
         }

         $.cookie("suppressed-messages", supressedMessages, { expires: 7 });
      });

      var supressedMessages = $.cookie("suppressed-messages");

      if (supressedMessages) {
         // hide these messages
         var msgs = supressedMessages.split(";");

         $.each(msgs, function(index, msg) {
         	$("div[nodeid='" + msg + "']").hide();
         });
      }
   });
</script></div>
