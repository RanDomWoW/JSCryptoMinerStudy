<script type="text/template" id="onair-li-tmpl">
  <li{{if current}} class="current"{{/if}}>{{if siteUri}}<a href="${siteUri}"{{if newWin === "1"}} target="_blank"{{/if}}>{{else}}<a class="nolink">{{/if}}<dl class="item-program">
    <dt>${startTime.slice(8,10)}時${startTime.slice(10,12)}分～</dt>
    <dd>${title}</dd>
  </dl></a></li>
</script>
<script type="text/template" id="onair-contents-tmpl">
  {{if list !== ''}}
    <ul data-now-index="${now}">
      {{each list}}
      <li{{if current == $index}} class="current"{{/if}}>{{if siteUri}}<a href="${siteUri}"{{if newWin === "1"}} target="_blank"{{/if}}>{{else}}<a class="nolink">{{/if}}<dl class="item-program">
        <dt>${startTime.slice(8,10)}時${startTime.slice(10,12)}分～</dt>
        <dd>${title}</dd>
      </dl></a></li>
      {{/each}}
    </ul>
    {{/if}}
</script>

<script type="text/template" id="recommend-contents-tmpl">
  {{if recommendData != ''}}
  <ul>
  {{each recommendData}}
  <li class="item-recommend">{{if newWin}}<a href="${url}" target="_blank">{{else url}}<a href="${url}">{{else}}<a class="nolink">{{/if}}
    <div class="thumb thumb-recommend"><img src="${thumb}" alt="${title}"></div>
    <p class="description-recommend">
      <span class="description">${title}{{if description}}<br />${description}{{/if}}</span>
    </p>
  </a></li>
  {{/each}}
  </ul>
  {{/if}}
</script>
<script type="text/template" id="contents-news-tmpl">
  {{if newsData != ''}}
  <h2>コンテンツニュース</h2>
  <ul class="list-contents-news">
  {{each newsData}}
  <li class="item-contents-news">{{if newWin}}<a href="${linkUrl}" target="_blank">{{else linkUrl}}<a href="${linkUrl}">{{else}}<a class="nolink">{{/if}}
    <div class="thumb thumb-contents-news"><img src="${image}" alt="${title}"></div>
    <p class="description-contents-news">
      <span class="day-contents-news">{{if isNew}}<i class="ico-new">NEW</i>{{/if}}${upateMonth}月${updateDate}日(${updateDay})</span>
      <span class="description">${title}{{if description}}<br>${description}{{/if}}</span>
    </p>
  </a></li>
  {{/each}}
  </ul>
  {{/if}}
</script>
<script type="text/template" id="ranking-tmpl">
    <h2 class="title-first-contents">番組アクセスランキング</h2>
    <ol class="list-ranking">
      {{each list}}
      <li class="item-ranking">
        <i class="rank {{if $index == 0}}first{{else $index == 1}}second{{else $index == 2}}third{{else $index == 3}}fourth{{else $index == 4}}fifth{{/if}}"></i>
        <a href="${program_site_url}"{{if new_window_st == '1'}} target="_blank"{{/if}}>
          <div class="box-photo">
            <img src="{{if program_pr_image}}${program_pr_image}{{else program_logo_image}}${program_logo_image}{{else}}img/logo_abc.png{{/if}}" alt="${program_name}">
            <div class="text">${program_name}</div>
          </div>
        </a>
      </li>
      {{/each}}
    </ol>
</script>


