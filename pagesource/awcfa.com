    <script type="text/javascript">
        function clearerror() {
            $("#erroraccount").html("");
            $("#errorplatformtype").html("");
            $("#errorapplytype").html("");
            $("#errormoney").html("");
        }

        $("#account").focus(function () {
            clearerror();
        });
        $("#money").focus(function () {
            clearerror();
        });
        $("#SearchAccount").focus(function () {
            $("#serror").html("");
            $("#ResAccount").val("");
            $("#ResMoney").val("");
            $("#ResReason").val("");
        });
        var checkInfo = function () {
            var account = $.trim($("#account").val());
            if (account == "") {
                $("#erroraccount").html("&nbsp;会员账户不能为空");
                return false;
            }
            var platformtype = $.trim($("#ddlplatformtype").val());
            if (platformtype == "") {
                $("#errorplatformtype").html("&nbsp;请选择平台类型");
                return false;
            }
            var applytype = $.trim($("#ddlapplytype").val());
            if (applytype == "") {
                $("#errorapplytype").html("&nbsp;请选择申请类型");
                return false;
            }
            var money = $.trim($("#money").val());
            if (money == "") {
                $("#erroraccount").html("&nbsp;金额不能为空");
                return false;
            }
            return true;
        };
        var checksearch = function () {
            var p = $.trim($("#SearchAccount").val());
            if (p == "") {
                $("#serror").html("请输入账号!");
                return false;
            }
            return true;
