<script>
$(function () {
	//初期設定
	var mainImg = $('#mainImg p');
	var mainList = $('#mainList a');
	$(mainImg).css({
		opacity: 0
	});
	$(mainImg).first().css({
		zIndex: 2,
		opacity: 1
	});
	$(mainList).first().css({
		opacity: 0.8
	});
	$(mainList).hover(function () {
		//ボタン制御
		$(mainList).stop().animate({
			opacity: 1
		}, 500);
		$(this).stop().animate({
			opacity: 0.8
		}, 500);
		//メインイメージ制御
		$(mainImg).stop().css({
			zIndex: 1
		}).animate({
			opacity: 0
		}, 1000);
		$(mainImg).eq($(mainList).index(this)).css({
			zIndex: 2
		}).stop().animate({
			opacity: 1
		}, 1000);
	});
});
</script>
