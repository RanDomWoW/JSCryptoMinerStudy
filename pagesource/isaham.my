		<input class="form-control mr-sm-2" type="search" style="width: 365px;" placeholder="Search" aria-label="Search" id="main-search-input" name="q" spellcheck="false" autocomplete="off">
		<button id="btn-search" class="btn btn-light btn-sm my-2 my-sm-0" type="button"><img src="/svg/search.svg" alt="Search" height="24px"></button>

<script type="text/javascript">
	$(function () {
		var choices_data = []

		$.ajax({
					type: "GET",
					url: "/search-data",
					success: function(data) {
						$.each(data.stocks, function (i,d) {
							choices_data.push([d[0], d[1], d[2]]);
						});
					},
					dataType: 'json'
		});

		$("#btn-search").click(function() {
			 	var val3 = $("#main-search-input").val();
				var symb = val3.toLowerCase();
				window.location.href = "/search?q=" + symb;
		});

		$('input[name="q"]').keydown(function(e) {
			if (e.which == 13) {
				e.preventDefault();
				var val4 = $("#main-search-input").val();
				var symb = val4.toLowerCase();
				window.location.href = "/search?q=" + symb;
			}
		});

		$('input[name="q"]').autoComplete({
		    minChars: 2,
		    source: function(term, suggest){
		        term = term.toLowerCase();
		        var choices = choices_data;
		        var matches = [];
		        for (i=0; i<choices.length; i++)
		            if (~(choices[i][0] + ' ' + choices[i][1] + ' ' + choices[i][2]).toLowerCase().indexOf(term)) matches.push(choices[i][0]);
		        suggest(matches);
		    },
		    onSelect: function(e, term, item){
		    		var val2 = item.data('val');
		    	 	var val = val2.split(' (')[0];

		    		var symb = val.toLowerCase();

		    		fbq('track', 'Search', {
	    				search_string: symb
	    			});

		    	  window.location.href = "/stock/" + symb;
		    }
		});

	});

	</script>
