html,body{width:100%;height:100%;margin:0}
body{background:#404040;overflow-y:scroll}
.wrap{max-width:1240px;margin:0 auto;}
a{text-decoration:none}
*{box-sizing:border-box;-webkit-backface-visibility: hidden;font-family: Oswald,Arial !important;font-weight:300;outline:none;-webkit-tap-highlight-color: rgba(0,0,0,0);}
.pad{padding:0.5em;}
.pad span{color:#bbb;font-weight:400}
.pad>div{padding:0.5em;overflow:hidden;}
.pad.events>div>div,#search.pad>div>div{background:#fff;overflow:hidden;box-shadow:0 2px 6px rgba(0,0,0,0.35);border-radius2:0 0 6px 6px;}
h1,h2,h3,b{margin:0;font-weight:400}
h1,h2,.center{text-align:center}
h1,h2{color:#fff;text-shadow:0 2px 6px rgba(0,0,0,0.75)}
h2{font-size:16pt}
.dat{padding:0.5em;}
h3,p{padding:0;margin:0;}
h3{font-size:16pt;height:26px;overflow:hidden;line-height:24px;margin-bottom:1em}
p{font-size:14pt;line-height:24px;color:#888;overflow:hidden;height:170px}
.channel p{height:60px;}
.channel a{text-decoration:none !important}
.channel a h3{color:#444}
.events .more{color:#999;text-decoration:none !important;font-size:90%}
#header .button{border-radius:4px;padding:0.5em;background-color:rgba(255,0,0,0.8);z-index:9999999999;position:fixed;top:1em;left:1em;color:#fff;font-weight:400;font-size:12pt}
#tickets{padding:1em;}
#tickets .button{border-radius:4px;padding:0.5em;background-color:rgba(255,0,0,0.8);color:#fff;font-weight:400;font-size:12pt;}
.ad{text-align:center;padding-top:0em}
.ad2{text-align:center;padding:5em 0}
.adfix{padding:0 1em;}
.adfix div{background-color:#404040;text-align:center;padding:.5em 0;margin:1em 0;}
.opacity div {transition: .8s opacity;}
.opacity:hover>div { opacity: 0.5; }
.opacity:hover>div:hover { opacity: 1; }
.cities {background-color:rgba(255,255,255,0.25)}
.cities ul{padding	:1em;}
.cities li{list-style:none;white-space:nowrap;color:#234;}
#header{display:table;background-position:center center;}
.cities a{text-decoration:none;color:#234;font-size:13pt}
.cities a:hover{text-decoration:underline;}
.events a{text-decoration:none;font-weight:400;white-space:nowrap;}
.events a:hover{text-decoration:underline;}
.events b a{color:#888;}
.events b span a{color:#ccc;}
#search{max-width:100%;width:640px;margin:0 auto;opacity:0.97;background-color:#0188a8;margin-top:-160px;margin-bottom:32px;}
#search input{border:0;width:100%;padding:0.5em;font-size:17pt;-webkit-appearance: none;border-radius: 0;}
#search input[type=submit]{background-color:#a88801;color:#fff}
#search .button{width:100%;display:block;background-color:rgba(255,255,255,0.1);color:#fff;padding:0.5em;text-align:center}
#search .button:hover,#search .button.sel{background-color:rgba(255,255,255,0.2);}
#searcsh{margin-top:-64px}
.shref{font-size:14pt !important;color:#789 !important;}
.fbut{z-index:9999999999;position:absolute;left:1em;}
.fbut2{min-height:50px;}

#citylist a{width:100%;padding:0.5em;text-decoration:none;font-size:16pt;color:#000;display:block}
#citylist a.sel,#citylist a:hover{color:#fff;background:#a88801;}
#header>div{display:table-cell;vertical-align:top}
.pad>div>div.oneevent{background-color:rgba(255,255,255,0.75);max-height:none}
.pad>div>div#citylist{max-height:none}
.oneevent p,.oneevent h3{height:auto;overflow:visible}
.oneevent p a{color:#666;font-size:16pt}
.pastevents{text-align:center}
.pastevents h3{color:#fff;margin-top:1em;font-size:30pt;height:50px;line-height:40px;}
form.contact{width:100%}
.contact input,.contact textarea{width:100%;box-shadow: 0 2px 3px rgba(0, 0, 0, 0.3) inset;border:0;padding:0.75em;border-radius:3px;font-size:16pt;}
.contact input[type=submit]{background-color:#007898;color:#fff;box-shadow: none;}
.contact input[type=submit]:hover{background-color:#1198b8;}
.contact textarea{height:240px}
.contact img{border-radius:3px;width:100%;}
@media screen and (min-width:768px){
	.dhide{display:none}
	.img{width:100%;height:140px;overflow:hidden;}
	.img img{margin:0 auto;transition: 1s transform ease-in-out;-webkit-transition: 1s -webkit-transform ease-in-out;margin-top:140px;transform: translate(0,-140px);-ms-transform: translate(0,-140px);-webkit-transform: translate(0,-140px); }
	.pad>div:hover .img img{transform: translate(0,-100%);-ms-transform: translate(0,-100%);-webkit-transform: translate(0,-100%);}
	#header{background-image:url('/events/weeevent/default.jpg');background-size:cover;background-repeat:no-repeat;height:200px;width:100%}
	h1{font-size:32pt;}
	h2{font-size:18pt;}
	.column6{-webkit-column-count:4;-moz-column-count:4;column-count:4;-webkit-column-gap: 2em;-moz-column-gap: 2em; column-gap: 2em;}
}
@media screen and (min-width:1024px){
	#header{height:250px;width:100%}
	h1{font-size:40pt;}
	h2{font-size:20pt;}
	.column6{-webkit-column-count:5;-moz-column-count:5;column-count:5;-webkit-column-gap: 2em;-moz-column-gap: 2em; column-gap: 2em;}
}
@media screen and (min-width:1280px){
	#header{height:300px;width:100%}
	h1{font-size:48pt;}
	h2{font-size:22pt;}
	.column6{-webkit-column-count:6;-moz-column-count:6;column-count:6;-webkit-column-gap: 2em;-moz-column-gap: 2em; column-gap: 2em;}
}
@media screen and (min-width:1600px){
	#header{height:350px;width:100%}
	h1{font-size:64pt;}
	h2{font-size:24pt;}
	.column6{-webkit-column-count:7;-moz-column-count:7;column-count:7;-webkit-column-gap: 2em;-moz-column-gap: 2em; column-gap: 2em;}
}
@media screen and (max-width:767px){
	#search{max-width:100%;width:640px;height2:48px;opacity:0.97;margin-top:-52px;margin-bottom:0px;}
	#search{margin-top:0px;}
	.pastevents h3{color:#666;font-size:16pt}
	.adfix,.mhide,.events .more,.fbut{display:none}
	body{background:#fff;}
	.ad,.ad2{text-align:center;padding:0}
	#header{background-image:url('/events/weeevent/default_mobile.jpg');background-size:cover;background-repeat:no-repeat;height:150px;width:100%}
	#header{height:100px;}
	#search{margin-top:0;}
	#header>div{vertical-align:middle}
	.column3{-webkit-column-count:3;-moz-column-count:3;column-count:3;-webkit-column-gap: 1em;-moz-column-gap: 1em; column-gap: 1em;}
	.pad{padding:0;}
	.dat{padding:0.25em;}
	.pad>div{padding:0;border-bottom:1px solid #eee;}
	.img{height:100%;}
	.img,.dat{overflow:hidden;}
	.img img{
		width:320%;
		max-width:none !important;
		transform: translateX(-50%) translateY(-50%);
		-ms-transform: translateX(-50%) translateY(-50%);
		-webkit-transform: translateX(-50%) translateY(-50%);
		margin-top:50%;
		margin-left:50%;
		}
	h1{font-size:16pt;}
	h2{font-size:12pt;}
	h3,p{padding:0;padding-bottom:0em;margin:0;}
	h3{font-size:11pt;height:26px;white-space:nowrap;}
	p{font-size:12px;line-height:14px;height:44px;}
	.oneevent h3{font-size:14pt;}
	.oneevent h3,.oneevent p{padding:0.5em;margin:0em;overflow:auto}
	.fbut2{padding-left:0.5em;}
	.oneevent h3{height:auto;white-space:normal;}
	.oneevent p,.oneevent p a{font-size:13pt;line-height:22px;}
	.events.pad>div>div{max-height:80px;min-height:0;overflow:hidden}
	.contact{background-color:#404040;}
	.contact input{padding:0.5em;}
	.contact.pad>div{padding:0 0.5em;border:0 !important}
}
@media all and (device-width: 768px) and (device-height: 1024px) and (orientation:portrait) {
	.img{height:80px;}
	.img img,.pad>div:hover .img img{margin:0 auto;transition: 0 transform ease-in-out;-webkit-transition: 0 -webkit-transform ease-in-out;margin-top:15%;transform: translate(0,-45%);-ms-transform: translate(0,-45%);-webkit-transform: translate(0,-45%); }
}
@media all and (device-width: 768px) and (device-height: 1024px) and (orientation:landscape) {
	.img{height:120px;}
	.img img,.pad>div:hover .img img{margin:0 auto;transition: 0 transform ease-in-out;-webkit-transition: 0 -webkit-transform ease-in-out;margin-top:15%;transform: translate(0,-45%);-ms-transform: translate(0,-45%);-webkit-transform: translate(0,-45%); }
}
@media screen and (max-height:1440px){
	#header{height:440px;}
}
@media screen and (max-height:1280px){
	#header{height:400px;}
}
@media screen and (max-height:960px){
	#header{height:360px;}
}
@media screen and (max-height:800px){
	#header{height:340px;}
}
@media screen and (max-height:600px) and (max-width:640px){
	#header{height:160px;}
}
@media screen and (max-width:640px){
	#header{height:80px;}
}



