		var allcities={};
	var maxcity=5;
	var citypos=0;
	var showcities=[];
	$(function() {
		$("img").unveil(600);
		//FastClick.attach(document.body);
		get_cities();
//		$("#citysearch").on("input",function(e){
		$("#citysearch").keydown(function(e){
			switch(e.which) {
				case 38: // up
				console.log("up");
					citypos--;
					if(citypos<1) citypos=maxcity;
					e.preventDefault();
				break;
				case 40: // down
					citypos++;
					e.preventDefault();
				break;
				case 13:
					if(citypos){
						location.href=showcities[citypos-1][2];
					}else if(showcities){
						location.href=showcities[0][2];
					}
					e.preventDefault();
					return;
				break;
				default:
					citypos=0;
				return;
			}
			search_cities($(this).val());
		});
		$("#citysearch").on("input",function(){
			search_cities($(this).val());
		});
		 $(".cities ul").hide(); 		
	});
	function search_cities(val){
		showcities=[];
		val = val.replace(/[ ]+$/g,"");
		var vallen=val.length;
		if(vallen){
			for(var city in allcities){
				citylow=city.toLowerCase();
				seocity=allcities[city];
				val=val.toLowerCase();
				val2=val.replace(/[ ]/g,"-");
				var index=citylow.indexOf(val);
				var indexseo=seocity.indexOf(val2);
				if(index!==-1||indexseo!==-1){
					if(vallen==1){
						if(index==0) showcities.push([index,city,allcities[city]]);
					}else{
						showcities.push([index,city,allcities[city]]);
					}
//					console.log(city+" "+allcities[city])
				}
			}
		}
		showcities.sort(function(a, b) {if(a[0]==b[0]){return a[1].localeCompare(b[1])-a[0];}else{return a[0]-b[0]}});
//		console.log(JSON.stringify(showcities));
		var html="";
		if(citypos>maxcity) citypos=1;
		for(var i=0;i<maxcity&&i<showcities.length;i++) html+="<a href=\""+showcities[i][2]+"\" class=\""+(citypos==(i+1)?"sel":"")+"\">"+showcities[i][1]+"</a>";
		$("#citylist").html(html);
	}
	function get_cities(){
		$.getJSON('/jsoncitylist', function(data){
			allcities=data;
		})
	}
	function is_touch_device() { return (('ontouchstart' in window) || (navigator.MaxTouchPoints > 0) || (navigator.msMaxTouchPoints > 0));}
