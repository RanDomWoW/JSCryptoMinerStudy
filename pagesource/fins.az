<script>
$(document).ready(function(){
    var animateFinish = true;
    $(window).scroll(function(){
        if($(window).scrollTop() > ($(document).height()/2) ){
            //$('.share-bottom').position().top-$(window).height() == $(window).scrollTop()
            if(animateFinish){
                animateFinish=false;
                $("#facebook-popup").animate({
                      bottom: "10px"
                  }, 500, function(){
                      animateFinish=true;
                  } );
            }
        }else if($(window).scrollTop() < ($(document).height()-1000) ){
            if(animateFinish){
                animateFinish=false;
                $("#facebook-popup").animate({
                      bottom: "-300px"
                }, 500, function(){
                      animateFinish=true;
                } );
            }
        }
    });
    
    if($.cookie('fpopup') != '1'){
        $('#facebook-popup').show();
        if($(window).scrollTop() > ($(document).height()/2) ){
            $("#facebook-popup").animate({
                  bottom: "10px"
              }, 500, function(){
                  animateFinish=true;
              } );
        }
    }
    $('.js-fixed-like-box-close').on('click', function(){
        $.cookie('fpopup', '1', { expires: 2 });
        $("#facebook-popup").animate({
              bottom: "-300px"
        }, 500, function(){
              animateFinish=true;
              $('#facebook-popup').hide();
        } );
    });
    $('#js-fixed-like-box-hide').on('click', function(){
        $.cookie('fpopup', '1', { expires: 10 });
        $("#facebook-popup").animate({
              bottom: "-300px"
        }, 500, function(){
              animateFinish=true;
              $('#facebook-popup').hide();
        } );
        return false;
    });
});
</script>
