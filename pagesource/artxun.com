			if(nowleft== long){	
				$(".hl_list ul").css("left",0);	 
				nowleft = 0;					 
			}else{
				nowleft = nowleft - 1;
				$(".hl_list ul").css("left",nowleft);
			}
		},18);
	}
	$(".hl_list li").mouseenter(function(){
		$(this).children("div").fadeIn(200);
		window.clearInterval(mytimer);
	});
	$(".hl_list li").mouseleave(function(){
		$(this).children(".hl_name").fadeOut(10);
		shebiao();
	})
});
