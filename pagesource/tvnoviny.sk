<script type="text/javascript">
	// detect mobile os
	var isBannerOpen = false;
	if(isMobile.iOS()){
		isBannerOpen = true;
		$('#api_download_ios').show();
		$('#api_download_android').hide();
	}
	if(isMobile.Android()){
		isBannerOpen = true;
		$('#api_download_ios').hide();
		$('#api_download_android').show();
	}
	if(isBannerOpen){
		$( "#tvn_mobile_banner" ).show();	
	}
	
	// cookies sett
	if((getCookie("tvn_app_1_close") == 1) || (getCookie("tvn_app_2_close") == 1)){
		$( "#tvn_app_cont" ).hide();
		$( ".api_cont" ).css('background', 'transparent');
	}else{
		$( "#tvn_app_cont" ).show();
	}
	// hide month 
	$( "#api_close1" ).click(function() {
		$.cookie('tvn_app_1_close', '1', { expires: 30 });
		$( "#tvn_app_cont" ).hide();
		$( ".api_cont" ).css('background', 'transparent');
	});
	// hide week
	$( "#api_close2" ).click(function() {
		$.cookie('tvn_app_2_close', '1', { expires: 7 });
		$( "#tvn_app_cont" ).hide();
		$( ".api_cont" ).css('background', 'transparent');
	});	
</script>
	
	
	
	
