    <script type="text/javascript">
        $(document).ready(function () {
            $rtl = $("body").attr("dir") === "rtl";

            if ($.fn.swiper !== undefined) {
                var swiper = new Swiper('#swiper', {
                    nextButton: '#swiper-next',
                    prevButton: '#swiper-prev',
                    spaceBetween: 30,
                    onSlideChangeStart: function (swiper) {
                        $("#slide-nav .css-selected").removeClass("css-selected");
                        $("#slide-nav li:nth-child(" + (swiper.activeIndex + 1) + ")").addClass("css-selected");
                    }
                });

                $("#slide-nav li").click(function () {
                    swiper.slideTo($(this).index());
                });
            }

            if ($.fn.hoverIntent !== undefined) {
                $("#tabs > li").hoverIntent(function (e) {
                    var o = $("#tabs .css-selected").index() + 1;
                    var n = $(this).index() + 1;

                    if (o == n) return;

                    $("#tabs li.css-selected").removeClass("css-selected");
                    $(this).addClass("css-selected");

                    $("#news > li:nth-child(" + o + ")").hide();
                    $("#news > li:nth-child(" + n + ")").animate({ height: "toggle" }, 250);
                });
            }

            $("#logo-list").width($("#logo-list > a").length * 222).show();

            $mx = $("#logo-list").outerWidth();
            $fx = 880;
            $px = $rtl ? $mx - $fx : 0;

            $("#logo-next, #logo-prev").click(function () {
                if ($px >= $mx - $fx) {
                    $px -= $fx;
                    if ($mx - $fx < $px * -1) $px = 0;
                } else {
                    $px += $fx;
                    if ($mx - $fx < $px) $px = $mx - $fx;
                }

                $("#logo-slider").animate({ scrollLeft: $px }, 500);
            });
        });
    </script>


