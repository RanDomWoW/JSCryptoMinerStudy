  function sliceArray(customers, amount) {
    var slice = [];
    var start = 0;
    var end = amount;
    var count = (customers.length);
    count = ((count - (count % amount)) / 10);
    for (var i = 0; i <= count; i++) {
      slice[i] = customers.slice(start, end);
      slice[i].sort(); 
      start += amount;
      end = start + amount;
    }
    return slice;
  }
  function sortArray(customers) {
    for (var i = 0; i < customers.length; i++) {
      customers[i] = customers[i].sort(compare);
    }
    return customers;
  }
  function compare(a,b) {
    if (a.name < b.name)
      return -1;
    if (a.name > b.name)
      return 1;
    return 0;
  }

  customers = sliceArray(customers, 10);
  customers = sortArray(customers);
  var html = '';
  for (var i = 0; i < customers.length; i++) {
    html += '<li>';
    for (var j = 0; j < customers[i].length; j++) {
      html += '<img src="dist/images/customers_logo/'+customers[i][j]['img']+'" title="'+customers[i][j]['title']+'">';
    }
    html += '</li>';
  }
  document.getElementById("resultHtml").innerHTML = html;
