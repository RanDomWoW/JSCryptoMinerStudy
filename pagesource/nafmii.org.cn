<script type="text/javascript">
    var hostname=location.host;
   // var app_web_site="http://www.nafmii.org.cn/cmsauth";
    var app_web_site="http://"+hostname+"/cmsauth";
    var threadTime = 1000*30;
    var start;

    function getCommDate() {
        var today = new Date();
        var year_ = today.getFullYear();
        var month_ = today.getMonth() + 1;
        var date_ = today.getDate();
        var day = today.getDay();
        var day_;
        switch (day) {
            case 0:day_ = "日";break;
            case 1:day_ = "一";break;
            case 2:day_ = "二";break;
            case 3:day_ = "三";break;
            case 4:day_ = "四";break;
            case 5:day_ = "五";break;
            case 6:day_ = "六";break;
        }
        var str = "<font color=\"#FF0000\">"+ year_ + "</font>年<font color=\"#FF0000\">" + month_ + "</font>月<font color=\"#FF0000\">" + date_ + "</font>日";
//        var str = "<font color=\"#FF0000\">"+ year_ + "</font>年<font color=\"#FF0000\">" + month_ + "</font>月<font color=\"#FF0000\">" + date_ + "</font>日&nbsp;&nbsp;星期" + day_ + "";
        return str;
    }


    function rep_passowrd() {
        $('#password_rep').skygqbox({title:'修改密码'});
    }
    function hiddenRep_passowrd() {
        $.skygqbox.hide();
    }


    function doPasswordRep() {
        var pass_old = $("#pass_old").attr("value");
        var pass_new = $("#pass_new").attr("value");
        var pass_new_rep = $("#pass_new_rep").attr("value");
        if(pass_old==''){
            alert("请输入原密码！");return;
        }
        if(pass_new==''){
            alert("请输入新密码！");return;
        }
        if(pass_new_rep==''){
            alert("请输入确认密码！");return;
        }
        if(pass_new!=pass_new_rep){
            alert("两次输入新密码不一样！");return;
        }
        if(pass_new.length<6){
            alert("密码至少需要6位！");return;
        }

        $.ajax({
            type:'post',
            data:{'pass_old':pass_old,'password':pass_new},
            dataType:'json',
            url:app_web_site+'/cmsAuth/xhhy/webUser/doPasswordRep.action?date='+(new Date()).getTime(),

            success:function (responseText, textStatus) {
                $("#pass_old").val("");
                $("#pass_new").val("");
                $("#pass_new_rep").val("");
                if (responseText.success == 'true') {
                    hiddenRep_passowrd();
                    alert("密码修改成功");
                } else {
                    alert(responseText.loginError);

                }
            },
            error:function () {
                alert("服务器出错，请联系管理员！");
            }
        });
    }

    function loginQtSystem() {
        var account = $("#account").attr("value");
        var password = $("#password").attr("value");
        if(account==''){
            alert("请输入账号！");return;
        }
        if(password==''){
            alert("请输入密码！");return;
        }
        $.ajax({
            type:'post',
            data:{'account':account, 'password':password},
            dataType:'json',
            url:app_web_site+'/cmsAuth/xhhy/webUser/loginAjax.action?date='+(new Date()).getTime(),

            success:function (responseText, textStatus) {
                if (responseText.success == 'true') {
                    document.getElementById("loginFromA").style.display="none";
                    document.getElementById("loginFromB").style.display="";
                    var str="的会员代表";
                    var sname=responseText.name;
                    if(responseText.xhyh) {
                        str="";
                        sname=responseText.account;
                    }
                    $("#loginUserMessage").html("<strong>"+sname+"</strong>"+str+"，您好！<br>欢迎使用协会网站会员之家！<br>今天是"+getCommDate()+"，祝您工作顺利！");
                    document.getElementById("hidden_YanjiuFenxi").style.display="block" ;
                    $("#account").val("");
                    $("#password").val("");
                    window.location.href="./hyzj/";
                } else {

                    document.getElementById("loginFromA").style.display="";
                    document.getElementById("loginFromB").style.display="none";
                    $("#loginUserMessage").html("");
                    document.getElementById("hidden_YanjiuFenxi").style.display="none" ;
                    alert(responseText.loginError);

                }
            },
            error:function () {
                alert("服务器出错，请联系管理员！");
                checkLogin();
            }
        });
    }


    function checkLogin() {
        $.ajax({
            type:'get',
            dataType:'json',
            url:app_web_site+'/cmsAuth/xhhy/webUser/checkLogin.action?date='+(new Date()).getTime(),
            success:function (responseText, textStatus) {

                if (responseText.success == 'true') {
                    if (document.getElementById("loginFromA") != null) {
                        document.getElementById("loginFromA").style.display = "none";
                    }
                    if (document.getElementById("loginFromB") != null) {
                        document.getElementById("loginFromB").style.display = "";
                    }
                    var str="的会员代表";
                    var sname=responseText.name;
                    if(responseText.xhyh) {
                        str="";
                        sname=responseText.account;
                    }

                    $("#loginUserMessage").html("<strong>"+sname+"</strong>"+str+"，您好！<br>欢迎使用协会网站会员之家！<br>今天是"+getCommDate()+"，祝您工作顺利！");                    document.getElementById("hidden_YanjiuFenxi").style.display="block"
                } else {//无登陆
                    if (document.getElementById("loginFromA") != null) {
                        document.getElementById("loginFromA").style.display = "";
                    }
                    if (document.getElementById("loginFromB") != null) {
                        document.getElementById("loginFromB").style.display = "none";
                    }
                    $("#loginUserMessage").html("");
                    document.getElementById("hidden_YanjiuFenxi").style.display = "none";
                }
            },
            error:function () {


            }
        });

    }

    function logout() {
        $.ajax({
            type:'get',
            dataType:'json',
            url:app_web_site+'/cmsAuth/xhhy/webUser/loginOut.action',
            beforeSend:function (XMLHttpRequest) {
            },
            success:function (responseText, textStatus) {
                if (responseText.success) {
                    document.getElementById("loginFromA").style.display="";
                    document.getElementById("loginFromB").style.display="none";
                    $("#loginUserMessage").html("");
                    document.getElementById("hidden_YanjiuFenxi").style.display = "none";
                 } else {
                    alert('退出失败:' + responseText.msg);
                }
            },
            complete:function (XMLHttpRequest, textStatus) {
                //HideLoading();
            },
            error:function () {
                //请求出错处理
            }
        });
    }

    $(document).ready(function(){
        checkLogin();
//                    start = setInterval("checkLogin()", threadTime);
    });
</script>
