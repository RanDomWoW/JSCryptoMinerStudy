        requestCall = function () {

            var phone = jQuery('#enter-phone').val();
            var name = jQuery('#enter-name').val();
            var comment = jQuery('#enter-comment').val();




            if (name.length == 0) {

                alert('Пожалуйста, укажите ваше имя и телефон.');
                return false;
            }


            phone = phone.replace("+", "");
            phone = phone.replace("(", "");
            phone = phone.replace(")", "");
            phone = phone.replace(" ", "");
            phone = phone.replace(" ", "");


            if (parseInt(phone).toString().length < 12) {

                alert('Пожалуйста, укажите правильный номер телефона.');
                return false;
            }

            var data = "name=" + name + "&phone=" + parseInt(phone) + "&comment=" + comment;

            $.ajax({

                type: 'post',
                url: '/rus/ajax/request-call',
                data: data,
                beforeSend: function () {
                    $('input,select,textarea', '#request-call-form').attr('disabled', 'disabled');
                    overlay_block('request-call-form');
                    jQuery('#send-review').hide();

                },
                success: function (data) {

