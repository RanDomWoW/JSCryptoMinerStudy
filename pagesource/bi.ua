							}
						products_html='';
						if(data.subgroups.length){
							products_html += '<div class="category-result">';
								$.map( data.subgroups, function( item ) {
									products_html += '<div class="flex-block">';
										products_html += '<p class="word">'+searchKeyword+' в</p>';
										products_html += '<p class="category"><a href="'+item.url+'?text-filter='+searchKeyword+'">'+item.label+'</a></p>';
									products_html += '</div>';
								})

							products_html +='</div>';

						}						

						if(data.prods.length){
					
							products_html +='<div class="search-box">';
							$.map( data.prods, function( item ) {
								products_html += '<a href="'+item.url+'" class="goods-in-cart flex-block">';
									products_html += '<div class="goods-item-img">';
										products_html+='<img src="'+item.img+'" alt="">'
									products_html += '</div>';
								

								    products_html += '<div class="goods-item-description">';
									products_html += '<div class="pci-title"><p>'+item.label+'</p></div>';
									products_html += '<div class="price price-horisontal flex-block">';
									products_html += '<p class="high-price">'+item.price+'</p>';
									if(item.priceOld){
										products_html += '<p class="old-price">'+item.priceOld+'</p>';
									}else{

									}
									
									products_html += '</div>';
								products_html += '</div>';
							products_html += '</a>';
								




							})
							products_html += '</div>';
							
							products_html += '<div class="btn-center"><a href="/rus/search/?search='+searchKeyword+'" class="btn-default-fill btn-bi" onclick>Все результаты поиска</a></div>';
						
							

							
								$('.serch-result').append(products_html);
								SearchActiveWidth() ;
						}
							
						
					}
			});
		 }
	});
});

