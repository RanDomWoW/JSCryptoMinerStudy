    $(document).on('click', '#forgot-password-save', function (e) {


        var self = $(this);
        self.attr('disabled', 'disabled');

        var password1 = $("#new-password").val();
        var password2 = $("#new-password-2").val();

        password1 = $.trim(password1);
        password2 = $.trim(password2);

        var hash = getRememberPasswordHash();

        if (hash) {
            $.ajax({
                type: "POST",
                timeout: 5000,
                url: "/" + lang + "/ajax/change-forgotten-password/",
                data: {
                    lang: lang,
                    hash: hash,
                    password1: password1,
                    password2: password2
                },
                success: function (data) {

                    console.log(data);

                    self.removeAttr('disabled');

                    if (data.success == 1) {
                        $("#forgot-password-input-form").hide();
                        $("#forgot-password-input-success").text(data.message);
                    } else {
                        $("#forgot-password-input-error").text(data.message);

                    }

                }
            }); // end ajax
        } else {

            //location.reload();
            //go2Page('/');
        }

        e.preventDefault();
    });


    function getRememberPasswordHash() {

        var teg = location.hash;
        if (teg != '') {
            tegArr = teg.split('/');

            if (tegArr.length > 1 && tegArr[0] == '#forgot-password') {

                return tegArr[1];
            }
        }
        return false;
    }


</script>


