        <script type="text/javascript">

            (function () {

                $('#itemslider').carousel({interval: 3000});
            }());

            (function () {
                $('.carousel-showmanymoveone .item').each(function () {
                    var itemToClone = $(this);

                    for (var i = 1; i < 6; i++) {
                        itemToClone = itemToClone.next();


                        if (!itemToClone.length) {
                            itemToClone = $(this).siblings(':first');
                        }


                        itemToClone.children(':first-child').clone()
                            .addClass("cloneditem-" + (i))
                            .appendTo($(this));
                    }
                });
            }());



            function fanBlockReload(product_id) {

                
                var data = {
                    product_id:product_id
                };

                $.ajax({
                    url: '/rus/ajax/reload-fan-block/',
                    data: data,
                    type: 'POST',
                    dataType: 'json',
                    success: function (data) {
                        if (data.success) {

                            $('DIV.fan-slider_content').html(data.content);
                            $('DIV.fan-slider_h').html(data.photos);
                            $('P.fan-descr.hidden.visible-xs').html(data.mob_desc);
                            $('DIV.read-more-block.flex-block').html( '<a href="' + data.brand_url +'" class="read-more">' + data.brand_name + '</a>');

                            var $frame = $('.fan-slider_h-block'),
                                $wrap  = $frame.parent();

                            $frame.sly(false);

                            $frame.sly({
                                horizontal: 1,
                                itemNav: 'basic',
                                smart: 1,
                                activateMiddle: 1,
                                mouseDragging: 1,
                                touchDragging: 1,
                                releaseSwing: 1,
                                startAt: 0,
                                scrollBar: $wrap.find('.scrollbar'),
                                scrollBy: 1,
                                speed: 300,
                                elasticBounds: 1,
                                easing: 'easeOutExpo',
                                dragHandle: 1,
                                dynamicHandle: 1,
                                clickBar: 1,

                                // Buttons
                                prev: $wrap.find('.prev'),
                                next: $wrap.find('.next')
                            });


                        }

                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        console.log(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);

                    }
                })



            }

        </script>

