                    type: 'inline'
                },
                fixedContentPos: false,
                fixedBgPos: true,
                overflowY: 'auto',
                closeBtnInside: true,
                preloader: false,
                midClick: true,
                removalDelay: 300,
                mainClass: 'notification',
                closeMarkup: '<button title="%title%" class="mfp-close"></button>',

            });
        }
        addProductToBasket = function (g, shop_id) {
            $.ajax({
                type: 'get',
                url: '/rus/ajax/add-product-to-basket',
                data: 'g=' + g + '&shop_id=' + shop_id,
                success: function (data) {

                    if (data.success) {

                        if (data.redirect) {
                            //seo
                            if (data.product_js) {
                                dataLayer.push({
                                    'event': 'addToCart',
                                    'ecommerce': {
                                        'currencyCode': 'UAH',
                                        'add': {                                // 'add' actionFieldObject measures.
                                            'products': data.product_js
                                        }
                                    },
                                    'eventCallback': function () {
                                        window.location = '/rus/basket/cart/';
                                    }

                                });
                            } else {
                                window.location = '/rus/basket/cart/';
                            }
                        } else {
                            if (data.messages) {
                                if ($.magnificPopup.instance && $.magnificPopup.instance.isOpen) {
                                    $.magnificPopup.close();
                                    setTimeout(function () {
                                        popuupAddProduct(data.messages)
                                    }, 500);
                                } else {
                                    popuupAddProduct(data.messages)
                                }
                            }
                        }


                        if (data.esputnik_js) {
                            var esputnik = data.esputnik_js;
                        }
                    }
                }

            });
        }

