	function getLength(typeName,title,codeNum){ 
		var type_len = 0;  
		 for(var i=0;i<typeName.length;i++){ 
			if(typeName.charCodeAt(i) > 127 || typeName.charCodeAt(i)==94){
				type_len +=2;
			}else{
				type_len ++;	
			}
		} 
		var len = 0; 
		var subsize = 0;  //实际截取的位数  因为一个汉字是算2位 
		var s = codeNum-type_len ;  //所需截取的位数   
		
		for(var j=0;j<title.length;j++){
			if(title.charCodeAt(j) > 127 || title.charCodeAt(j)==94){
				len +=2;
			}else{
				len +=2;	
			}
			
			if(len <= s ){ 
				subsize ++;
			}
		}
		if(len>s){
			title =  title.substring(0,subsize)+"...";
		}
		return title;
	}
	
