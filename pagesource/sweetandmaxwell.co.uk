             //Save the current typed term in the hidden field
		    //$("[id$=HiddenSearchTerms]").val(keyword)
          (cssClassHiddenST!="")? $("input.ST_SearchBox").val(keyword):$("[id$=HiddenSearchTerms]").val(keyword);
        }

        $('input#ctl00_MainHeaderContent_TopNav1_SuggestSearchBox1endecaSuggestBox').val(keyword);
    }

        
    function callback_ctl00_MainHeaderContent_TopNav1_SuggestSearchBox1(item) {

        var doSearch = $("[id$=HiddenSearchSelectedWord]").val();
        //alert("in callback_ " + "ctl00_MainHeaderContent_TopNav1_SuggestSearchBox1");
        if (doSearch == "True") {

            window.location.href = "../Catalogue/Results.aspx?pagesize=20&type=KEYWORD-SEARCH&keyword=" + item.text;    
        }
        else{
        var cssClassHiddenST="ST_SearchBox";
            //Save the current typed term in the hidden field
		    //$("[id$=HiddenSearchTerms]").val(item.text)
            (cssClassHiddenST!="")? $("input.ST_SearchBox").val(item.text):$("[id$=HiddenSearchTerms]").val(item.text);
            //$("input.ST_SearchBox").val(item.text);
        }
    }
    
    //we use the onchange event in the endecaSuggestBox text field, so we make sure we alwayas have the correct value stored in the HiddenSearchTerms hidden field
