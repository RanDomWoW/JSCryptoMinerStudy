    function setKeywordInputBox_ctl00_MainHeaderContent_TopNav1_SuggestSearchBox1() {

    var cssClassHiddenST="ST_SearchBox";
       //var keyword = $("[id$=HiddenSearchTerms]").val();
     
       var keyword=cssClassHiddenST!=""? $("input.ST_SearchBox").val():$("[id$=HiddenSearchTerms]").val();

