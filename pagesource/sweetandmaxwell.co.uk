        if ((currentUrl == "")
            /* || ((currentUrl == "default.aspx") && path.indexOf("about-us/default.aspx") < 0 && path.indexOf("contact-us/default.aspx") < 0 )*/
            ) {
            updateSelectionStyles($("[id$=ctl00_MainHeaderContent_TopNav1_divTopMenuHome]"), true);
        }
        else {
            updateSelectionStyles($("[id$=ctl00_MainHeaderContent_TopNav1_divTopMenuHome]"), false);
        }

        if (path.indexOf("about-us/default.aspx") > 0) {
            updateSelectionStyles($("[id$=ctl00_MainHeaderContent_TopNav1_divTopMenuAboutUs]"), true);
        }
        else {
            updateSelectionStyles($("[id$=ctl00_MainHeaderContent_TopNav1_divTopMenuAboutUs]"), false);
        }

        if (path.indexOf("contact-us/default.aspx") > 0) {
            updateSelectionStyles($("[id$=ctl00_MainHeaderContent_TopNav1_divTopMenuContactUs]"), true);
        }
        else {
            updateSelectionStyles($("[id$=ctl00_MainHeaderContent_TopNav1_divTopMenuContactUs]"), false);
        }

    });

    function updateSelectionStyles(itemMenu, isSelected) {

        var $menuToUpdate = itemMenu;

        if (isSelected) {
            $menuToUpdate.addClass("TopRightMenuSelected");
            $menuToUpdate.removeClass("TopRightMenuNotSelected");
        }
        else {
            $menuToUpdate.removeClass("TopRightMenuSelected");
            $menuToUpdate.addClass("TopRightMenuNotSelected");
        }
    }

    function redirect() {
        window.location.href = "/Catalogue/advncdsearch.aspx";
    }


</script>

