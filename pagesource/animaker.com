<script>
$(document).ready(function() {
  /*******Multimenu hover*******/
  $(".menu_box.desktop").hide();
  $(document).on('mouseenter',".main-menu .multimenu",function(e) {
    $(".menu_box.desktop").show();
  });
  $(document).on('mouseleave',".main-menu .multimenu",function(e) {
    $(".menu_box.desktop").hide();
  });

  $(document).on('click','.multimenu',function(e) {
    var parentTarget,target;
    parentTarget = $(e.target).parent().prop('class');
    target = $(e.target).prop('class');
    // console.log('parentTarget '+parentTarget);
    // console.log('target '+target);
    if(target == 'product_text') {
      $(".cat_list.category1,.cat_list.category2,.cat_list.category3").removeClass('open');
      $(".menu_box.mobile").toggleClass('open');
      $(".mob-menu").removeClass('mob-menu-overflow');
    } else if((parentTarget == 'categories product_list') || (parentTarget == 'category_head video')) {
        $(".cat_list.category2,.cat_list.category3").removeClass('open');
        $(".cat_list.category1").toggleClass('open');
        $(".mob-menu").addClass('mob-menu-overflow');
    } else if((parentTarget == 'categories other_list') || (parentTarget == 'category_head products')) {
        $(".cat_list.category1,.cat_list.category3").removeClass('open');
        $(".cat_list.category2").toggleClass('open');
        $(".mob-menu").addClass('mob-menu-overflow');
    } else if((parentTarget == 'categories blog_list') || (parentTarget == 'category_head article')) {
        $(".cat_list.category1,.cat_list.category2").removeClass('open');
        $(".cat_list.category3").toggleClass('open');
        $(".mob-menu").addClass('mob-menu-overflow');
    }
  });
});
