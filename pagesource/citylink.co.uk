		           }
		       }
		       else {
		   		document.getElementById("rtnDateDIV").style.display = "none";

		   		document.getElementById("rtnTimeDiv").style.display = "none";

		           if(applState == "2" || applState == "3" || applState == "4"){
		               document.forms[0].appState.value = "1";
