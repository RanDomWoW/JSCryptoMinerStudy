<script text="text/javascript">
	$(function (){
		//加载 极致透明区域的两个数据 (平台累计交易额,为投资人带来收益)
		$.get(
			"/home/getNoticePageInfo",
			function(data,status){
				if(status){
					if(typeof aformatReadableCurrency === "function") {
						$("#totalProfit").html(aformatReadableCurrency(data.totalProfit) + " 元");
					} else {
						window.location.reload();
					}
				}else{
					$("#totalProfit").html("--");
				}
		});
		
		$.get(
			"/operate/async/overview",
			function(data,status){
				if(status){
					if(typeof aformatReadableCurrency === "function") {
						$("#totalBorrowAmount").html(aformatReadableCurrency(data.totalBorrowAmount) + " 元");
						$("#investerCount").html(aformatCurrency(data.totalInvesterCount));
						$("#totalServeDays").html(data.totalServeDays + "天");
						$("#totalInvesterCount").html(data.totalInvesterCount + "人");
					}else {
						window.location.reload();
					}
				}else{
					$("#totalBorrowAmount").html("--");
					$("#investerCount").html("--");
					$("#totalServeDays").html("--");
					$("#totalInvesterCount").html("--");
				}
			}
		);
		
		function aformatCurrency(num) {
			if (num==undefined)num="0";
		    num = num.toString().replace(/$|\,/g,'');
		    if(isNaN(num))
		    num = "0";
		    sign = (num == (num = Math.abs(num)));
		    num = Math.floor(num*100+0.50000000001);
		    cents = num%100;
		    num = Math.floor(num/100).toString();
		    if(cents<10)
		    cents = "0" + cents;
		    for (var i = 0; i < Math.floor((num.length-(1+i))/3); i++)
		    num = num.substring(0,num.length-(4*i+3))+','+ num.substring(num.length-(4*i+3));
		    if (cents.toString()=='00'){
		    	return (((sign)?'':'-') + num );
		    }else if(cents.toString().substring(2,1)=='0'){
		    	return (((sign)?'':'-') + num + '.' + cents.toString().substring(0,1));
		    }else{
			    return (((sign)?'':'-') + num + '.' + cents);
		    }
		}

		/**
		 * 将数值四舍五入(保留2位小数)后格式化成带'亿'、'万'金额形式
		 *
		 * @param num 数值(Number或者String)
		 * @return 金额格式的字符串,如'26亿1123万4567.45'
		 * @type String
		 */
		function aformatReadableCurrency(num) {
			if (num==undefined)num="0";
		    num = num.toString().replace(/$|\,/g,'');
		    if(isNaN(num))
		    num = "0";
		    sign = (num == (num = Math.abs(num)));
		    num = Math.floor(num*100+0.50000000001);
		    cents = num%100;
		    num = Math.floor(num/100).toString();
		    if(cents<10)
		    cents = "0" + cents;
		    var countn = 0;
		    for (var i = 0; i < Math.floor((num.length-(1+i))/4); i++) {
		    	var j = num.length-(5*i+4);
		    	if(countn == 0) {
		    		num = num.substring(0,j) + '万' + num.substring(j);
		    	} else if(countn == 1) {
		    		num = num.substring(0,j) + '亿' + num.substring(j);
		    	} else {
		    		num = num.substring(0,j) + ',' + num.substring(j);
		    	}
		    	countn = countn + 1;
		    }
		    num = num.replace(/万/, " 万 ");
		    num = num.replace(/亿/, " 亿 ");
		    if (cents.toString()=='00') {
		    	return (((sign)?'':'-') + num );
		    }else if(cents.toString().substring(2,1)=='0'){
		    	return (((sign)?'':'-') + num + '.' + cents.toString().substring(0,1));
		    }else{
			    return (((sign)?'':'-') + num + '.' + cents);
		    }
		}
	});

</script>
