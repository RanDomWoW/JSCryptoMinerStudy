		var fixStatus = function() {
			var scrollTop = $(document).scrollTop();
			if($(".main-left").height()>$(".main-right").height())
			{	
			if (scrollTop >= lefttop) {
				$(".scoll").css({
					position : 'fixed',
					top : '0'
				});
				if (scrollTop - lefttop >= lfheight) {
					$(".scoll").css({
						position : 'absolute',
						top : 'auto',
						bottom : '0'
					});
				}
			}
			else {
				$(".scoll").css({
					position : 'static',
					top : '0'
				});
			}
		}
		};
		$(window).scroll(function() {
