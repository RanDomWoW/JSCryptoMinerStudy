<script type="text/javascript">
function goGSASearchTop() 
{
  var top = document.getElementById("txtGSASearchTop").value.trim();
  var footer = document.getElementById("txtGSASearchFooter").value.trim();
  var search = "";  
  var element = document.getElementById('txtGSASearch');
  
  if (element != null) 
    search=element.value.trim();

  if (top.length > 1 && top.indexOf("Search TDSB Here")==-1) 
    location.href = "/Search.aspx?Search=" + top;
  else if (footer.length > 1 && footer.indexOf("Search TDSB Here")==-1) 
    location.href = "/Search.aspx?Search=" + footer;
  else if (search !="")
    location.href = "/Search.aspx?Search=" + search;
  else 
    return false;

}
  
$('form').keyup(function(e) {
  if (e.which === 13){
    return goGSASearchTop();
}
}); 
</script>
