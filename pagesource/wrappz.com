aElem.append(children);
});
jQuery('#qs_product').on('click', "#qs_product > li", function() {
reset_brand();
reset_model();
var catid = jQuery( this ).attr('value');
jQuery('#qs_product').attr('catid',catid);
changecategory('product');
var aElem = jQuery('#qs_product').parent();
var children = aElem.children();
aElem.children('span').html(jQuery( this ).html());
aElem.append(children);
});
jQuery('#qs_brand').on('click', "#qs_brand > li", function() {
reset_model();
var catid = jQuery( this ).attr('value');
jQuery('#qs_brand').attr('catid',catid);
changecategory('brand');
var aElem = jQuery('#qs_brand').parent();
var children = aElem.children();
aElem.children('span').html(jQuery( this ).html());
aElem.append(children);
});
jQuery('#qs_model').on('click', "#qs_model > li", function() {
var catid = jQuery( this ).attr('value');
jQuery('#qs_model').attr('catid',catid);
changecategory('model');
var aElem = jQuery('#qs_model').parent();
var children = aElem.children();
aElem.children('span').html(jQuery( this ).html());
aElem.append(children);
});
function changecategory(select_type){
var device_type = jQuery("#device_type").val();
if(device_type == 'mobile'){
var qs_product_type = jQuery("#qs_product_type").val();
var qs_product = jQuery("#qs_product").val();
var qs_brand = jQuery("#qs_brand").val();
var qs_model = jQuery("#qs_model").val();
}else{
var qs_product_type = jQuery("#qs_product_type").attr('catid');
var qs_product = jQuery("#qs_product").attr('catid');
//var qs_brand = jQuery("#qs_brand").find("[selected='selected']").attr('value');
var qs_brand = jQuery("#qs_brand").attr('catid');
var qs_model = jQuery("#qs_model").attr('catid');
}
if(qs_product_type === undefined) qs_product_type = '';
if(qs_product === undefined) qs_product = '';
if(qs_brand === undefined) qs_brand = '';
if(qs_model === undefined) qs_model = '';
var params = new Array();
params.push('qs_product_type='+qs_product_type);
params.push('qs_product='+qs_product);
params.push('qs_brand='+qs_brand);
params.push('qs_model='+qs_model);
params.push('select_type='+select_type);
//console.log(params);
//var url = 'quicksearch/index/mobile';
var url = 'quicksearch/index/mobile';
var finalurl = url + '?' + params.join('&');
//console.log(params);
jQuery.getJSON( finalurl, function( data ) {
//console.log(data);
if(device_type == 'mobile'){
jQuery.each( data, function( el_id, el_html ){
if(el_id == 'redirect' && el_html !== ''){
window.location = el_html;
return false;
}else if(el_id == 'finalurl'){
jQuery('#'+el_id).val(el_html);
}else{
jQuery('#'+el_id).empty();
jQuery.each( el_html, function( catid, label ){
//console.log(label);
jQuery('#'+el_id).append('<option value="'+label.catid+'" '+label.selected+' >'+label.label+'</option>');
});
if(el_html.length > 1){ //('#'+el_id + ' option').length
jQuery('#'+el_id).attr('class','qs_active_select');
jQuery('#'+el_id).attr('disabled', false);
jQuery('#'+el_id).css('cursor', 'pointer');
jQuery('#qs_div_select select').prev().removeProp("class");
} else{
jQuery('#'+el_id).removeClass('qs_active_select');
jQuery('#'+el_id).attr('disabled', true);
jQuery('#'+el_id).css('cursor', 'url(images/no-entry.png), not-allowed;');
}
}
});
}else{
jQuery.each( data, function( el_id, el_html ){
if(el_id == 'redirect' && el_html !== ''){
window.location = el_html;
return false;
}else if(el_id == 'finalurl'){
jQuery('#'+el_id).val(el_html);
}else{
jQuery('#'+el_id).empty();
jQuery.each( el_html, function( catid, label ){
//console.log(label);
if((label.selected) && (label.selected == 'selected')){
jQuery('#'+el_id).attr('selected',catid);
}
jQuery('#'+el_id).append('<li value="'+label.catid+'" >'+label.label+'</li>');
});
if(el_html.length > 1){ //('#'+el_id + ' option').length
jQuery('#'+el_id).addClass('class','qs_active_list');
jQuery('#'+el_id+'_parent').addClass('qs_active_list');
jQuery('#'+el_id).attr('disabled', false);
//jQuery('#'+el_id+'_parent').attr('disabled', false);
jQuery('#'+el_id).css('cursor', 'pointer');
jQuery('#qs_div_select select').prev().removeProp("class");
} else{
jQuery('#'+el_id).removeClass('qs_active_list');
jQuery('#'+el_id+'_parent').removeClass('qs_active_list');
jQuery('#'+el_id).attr('disabled', true);
jQuery('#'+el_id).css('cursor', 'url(images/no-entry.png), not-allowed;');
}
}
});
}
});
}
function reset_product(){
jQuery('#qs_product').empty();
jQuery('#qs_product_parent > li >span').html('Product');
// alert("data reset");
}
function reset_brand(){
jQuery('#qs_brand').empty();
