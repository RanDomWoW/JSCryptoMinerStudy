jQuery('#qs_brand').change(function() {
changecategory('brand');
});
jQuery('#qs_model').change(function() {
changecategory('model');
});
jQuery('#qs_btn_go').click(function() {
window.location = jQuery("#finalurl").val();
return false;
});
jQuery('#qs_product_type').on('click', "#qs_product_type > li", function() {
reset_product();
reset_brand();
reset_model();
