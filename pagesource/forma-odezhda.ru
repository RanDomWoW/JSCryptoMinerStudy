<script type="text/javascript"><!--
  $(document).ready(function(){

    var scroll_top_class = '.scroll-top-button-left, .scroll-top-button-right';
    $(scroll_top_class).hide();

    function checkOffset() {
        var a = $(document).scrollTop()+window.innerHeight;
        var b = $('.footer').offset().top;
        if (a < b) {
          $(scroll_top_class).css('bottom', '20px');
        } else {
          $(scroll_top_class).css('bottom', (20+(a-b))+'px');
        }
      }

      $(document).ready(checkOffset);
      $(document).scroll(checkOffset);

    $(window).scroll(function(){

      if ($(this).scrollTop() > 100) {
        $(scroll_top_class).fadeIn();
      } else {
        $(scroll_top_class).fadeOut();
      }
    });

    $(scroll_top_class).click(function(){
      $('html, body').animate({scrollTop : 0},500);
      return false;
    });

  });
//--></script>
