<style id='fp-style-inline-css' type='text/css'>
/*mstm buy form*/

.page-id-401360 .mstm-billboard-undermenu {display:none !important;}

.page-id-400990 .mstm-billboard-undermenu {display:none !important;}

.mstm-buyform {
    margin-top: 40px;
}

.mstm-one-half {
    float: left;
    margin-right: 30px;
}


.mstm-one-half input, .mstm-one-half-right input {
    padding: 7px;
    margin-bottom: 15px;
    border: 1px solid #ddd;
}


.mstm-one-half-right .tsantes select {
    padding: 7px;
    margin-top: 12px;
    border-radius: 0px;
    border: 1px solid #ddd;
    min-width: 305px;
}


.mstm-one-half-right .endiaferomaigia select {
    padding: 7px;
    margin-top: 12px;
    border-radius: 0px;
    border: 1px solid #ddd;
    min-width: 305px;
}


.mstm-buyform-clear {
    clear: both;
    margin-bottom:25px;
}


.mstm-buyform .wpcf7-submit {
    padding: 10px;
    min-width: 110px;
    margin-top: 12px;
    background: #295680;
    border: 0px;
    color: #fff;
    font-size: 13px;
}


.mstm-buyform .wpcf7-submit:hover {
background: #FAA838;
cursor:pointer;
}
/*mstm buy form ends*/



.section-title h4, .section-title h4 a {padding-bottom:15px;
}

.category-sexy .adsbygoogle {display: none !important;}

.home .entry-meta {
    display: none;
}

.section-title h4  {
border-bottom: 3px solid #111111;
}

.section-title {border-bottom:0px solid #fff !important;}

.likeboxclosed {display:none !important;}

.fixedlikebox-close {
padding:2px 8px;
background: black;
color: #fff;
text-align:right;
cursor:pointer;
}

#cookie-notice .button {
   margin-left: 10px;
   background: #FACB00
!important;
   color: #333333
!important;
   text-transform: inherit;
}

.homeReadMore a {
    color: #ea9738 ! important;
   }

.section-title h4, .section-title h4 a {
    color: #ea9738 ! important;
   }

.views {
    display: none;
}

.section-title-mstm {
    background-image: url(https://www.daynight.com.gr/wp-content/themes/daynight/images/sep.png);
    background-repeat: repeat-x;
    background-position: bottom center;
    padding-bottom: 28px;
}

.section-title-mstm-2 {
    background: none !important;
}

@media only screen and (min-width: 460px) and (max-width: 569px) {
#header .banner {display:block!important;}
}

@media only screen and (min-width: 320px) and (max-width: 459px) {
#header .banner {display:block!important;}
}


.slider-right-a,
.slider-right-b {
    float: right;
    width: 330px;
    height: 190px;
}


.slider-right .kentriko-mini .title a {
    background: rgba(0,0,0,0.6);
    color: white !important;
    position: absolute;
    top: 120px;
    font-size: 13px !important;
    min-height: 65px;
    text-align: center;
    padding-top: 12px !important;
    font-weight: bold !important;
    width:100%;
}

.fixedlikebox-mstm {
display:none;
position:fixed;
right:10px;
bottom:10px;
}

@media only screen and (min-width: 300px) and (max-width: 650px) {
.mstm-billboard-undermenu {display: none !important;}
}

@media (max-width: 1130px){
.fixedlikebox-mstm{
display: none !important;
}
}

@media (max-width: 1129px){
    .slider-right {
        margin-left: 0;
        width: auto;
        display: inline-block;
    }
    .slider-right .item-post:first-child {
        margin-right: 0;
    }
    html .section > div:last-child {
        margin-left: 20px;
    }
}

@media (max-width: 767px){
    html .section > .slider-right{
        margin-left: 0;
        width: auto;
        display: block;
        float: none;
        clear: both;
        margin: 0 auto;
        width: 254px !important;
    }
    html .section > div:last-child {
        margin-left: unset;
        margin: 0 auto;
        padding-top: 10px !important;
    }
    .section-title.section-title-mstm-2 > div > a {
        text-align: center;
    }
}

html #main > .content-wrap > #content  > aside.section.widget_text{
text-align: center;
}

.homeReadMore {
    clear: both;
}
</style>
