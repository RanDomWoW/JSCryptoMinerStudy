          <script language="javascript">
				function submitSearchForm() {
					document.forms['quickSearchForm'].inDesc.value=encodeURI(document.forms['quickSearchForm'].userText.value);
					document.forms['quickSearchForm'].submit();
				}
				
				function getLanguagePath(lang, lang2) {
					var currentPath = location.href;
					var fromIndex = currentPath.length - 4;
					if ('BG'==lang) {
						if (currentPath.indexOf('_BG', fromIndex) > 0) {
							return currentPath.substring(0,currentPath.length-3) + '_EN';
						}
					}
					if ('EN'==lang) {
						if (currentPath.indexOf('_EN', fromIndex) > 0) {
							return currentPath.substring(0,currentPath.length-3) + '_BG';
						}
					}
					if (currentPath.indexOf("?")>0) {
						return currentPath + "&toLang=_" + lang2;
					} else {
						return currentPath + "?toLang=_" + lang2;
					}
				}
			</script>
