sport.hoverBanner();
}

$('.veja-mais').on("click", sport.verMais);

},
configSliderResponsive: function(){
$('#slider-homepage').flexslider({
    animation: "slide",
    start: function(){
            $('<div>').attr('class','flex-overlay').appendTo('.flex-viewport');
        },
        smoothHeight: true
  });
