   //  });
},
verMais: function(event){
event.preventDefault();
var $this = $(this).parent();

$this.find('.hidden').slideToggle("show", function(){
var $button = $this.find('.veja-mais')
$button.toggleClass('veja-menos').text("Veja Menos");
if(!$button.hasClass('veja-menos')){
$button.text("Veja mais esportes");
}
});
},
slider: function(){
var $carouselWrapper = $('#carouselWrapper');
            var $t;
            $carouselWrapper.cycle({
                fx: 'fade',
                speed: 1000,
                timeout: 4000,
                width: '100%',
                slideResize: true,
                next: '#nextSlide',
                prev: '#prevSlide',
                containerResize: true,
                fit: 1,
                pager: '#controls ul',
                pause: true,
                pagerAnchorBuilder: function(idx, slide) {},
            })
            // $(window).resize(function() {
            //     $('#carouselWrapper').css('height', $(".item-slider-index").height());
            // })
            $("#controls li a").click(function() {
                $('#carouselWrapper').cycle(parseInt($(this).attr("ind")));
            })
            $("#horizontalCarousel").hover(function() {
                $(this).find(".hover_banner.slideUp").animate({
                    bottom: '35px'
                })
            }, function() {
                $(this).find(".hover_banner.slideUp").animate({
                    bottom: '-100px'
                })
            })
},
hoverBanner: function(){

$('.hover-ligth').mouseenter(function(){
$(this).find('.ligthbox-interno').fadeIn(500);
}).mouseleave(function(){
$(this).find('.ligthbox-interno').fadeOut(150);
})
}
}
sport.init();
})
