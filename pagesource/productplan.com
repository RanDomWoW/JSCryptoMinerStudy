            <script>
        function readCookie(name) {
          var nameEQ = name + "=";
          var ca = document.cookie.split(';');
          for(var i=0;i < ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
          }
          return null;
        }

        //Loop through all buttons
        var url = "https://app.productplan.com/users/sign_up";
        var links = document.links;
        var utm_source = readCookie('utm_source');
        var utm_medium = readCookie('utm_medium');
        var utm_term = readCookie('utm_term');
        var utm_campaign = readCookie('utm_campaign');
        for(var i=0; i<links.length; i++) {
          if ( links.item(i).href == url && (utm_source || utm_medium|| utm_term || utm_campaign) ) {
            links.item(i).href = url + '?' + 'utm_source='+utm_source+'&'+'utm_medium='+utm_medium+'&'+'utm_term='+utm_term+'&'+'utm_campaign='+utm_campaign;
          }
        }
        </script>
