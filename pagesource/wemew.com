<script>
var setHeadNav = function(){
	$('.headNav').bind('mouseover',function(){
		var left = this.offsetLeft,width = this.offsetWidth;
		if($('.headNavs .hover')[0].offsetWidth==0)
			return $('.headNavs .hover').css({width:width,left:left});
		$('.headNavs .hover').stop().animate({left:left,width:width},'fast');
	});
	$('.headNavs').bind('mouseleave',function(){
		$('.hover').stop().css({width:0,left:0});	
	});
}

var setFixed = {
	init:function(){
		setFixed.set();
		$(window).bind('scroll',function(){
			setFixed.set();
		});		
	},
	set:function(){
		var top = $(window).scrollTop();
		if(top>=70)
			$('.fixLogo').addClass('fixed');		
		else
			$('.fixLogo').removeClass('fixed');		
	}
}

var initNews = function(delay){
	if($('.info li').length<=1)
		return;
	delay = delay||4000;
	setTimeout(function(){
		$('.info ul').animate({
			top:-50	
		},function(){
			var first = $('.info li')[0];
			$('.info ul')[0].appendChild(first);
			$('.info ul').css({top:0});	
		});	
		setTimeout(arguments.callee,delay);					
	},delay);
}

$(function(){
	setHeadNav();
	setFixed.init();
	initNews(4000);
	$('#videoCtrl').click(function(){
	    $('#videoCover').show();
    });
	$('#videoCover').click(function(){
	    $(this).hide();
	    var video = document.getElementById('wemewVCR');
        video.currentTime = 0;
        video.pause();
    })
})
</script>
