	});
	//close interest point description
	$(".cd-close-info-red").on("click mousehover", function(event){
		event.preventDefault();
		$(this).parents(".cd-single-point-red").eq(0).removeClass("is-open").addClass("visited");
	});
}
jQuery(document).ready(function($){
 cdSinglePoint();
});
