		$("[data-cAddCartBtn='cloneAddtoCartbtn']").on( "click", function() {
			$(addToCart).trigger("click");
		});
		
		if($(addToCart).hasClass('btns-disable')){
			$("[data-cAddCartBtn='cloneAddtoCartbtn']").addClass('btns-disable');
