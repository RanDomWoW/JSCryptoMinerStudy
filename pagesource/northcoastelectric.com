		$("[data-cloneqty='cloneQty']").val($("[data-originalqty='originalQty']").val());
	});


$(document).ready(function(){
	  
	var respondViewPort = function() { 

	if (window.matchMedia('(max-width: 980px)').matches) {
		    $('#stickyHeader').removeClass("sticky");
		    $("#detailClone").hide();
		    $('#dummyHeight').css('height','0');
	}else {
		$(window).scroll(function() {
			if ($(this).scrollTop() > 100){
				if (window.matchMedia('(min-width: 981px)').matches) {
				var headerHeight = $('#stickyHeader').outerHeight(true);
			    $('#stickyHeader').addClass("sticky");
			    detail = $("#detailClone").detach();
			    detail.appendTo("#stickyHeader");
			    $("#detailClone").slideDown(200);
			    $('#dummyHeight').css('height',headerHeight);
				}
			  }
			  else if ($(this).scrollTop() < 70){
				if (window.matchMedia('(min-width: 981px)').matches) {
			    $('#stickyHeader').removeClass("sticky");
			    detail = $("#detailClone").detach();
				detail.appendTo(".detailCloneWrap");
				$('#dummyHeight').css('height','0');
				$("#detailClone").slideUp(200);
				}
			  }
			});
	}
		
	};
	  
	  $(window).resize(respondViewPort);
	  respondViewPort();  
	});

$(function() {
	if($(".formatPrice").length>0){
		$(".formatPrice").each(function(){
			var e=$(this).text();
			if(e.length > 0) {
				e=e.replace("$","");
				e="$"+commaSeparateNumber(e);
				$(this).text(e);
			}
		});
	}
	$(".cimm_loginContent #mainUserName").focus();
	$('#headerLogin').click(function () {
			if ($('.loginDropdown').is(":visible")) {
				$('.loginDropdown').slideUp(200);
			} else {
				$('.loginDropdown').slideDown(200);
				$('.loginDropdown input[name=loginId]').focus();
			}
			$(".needHelpWrap").slideUp(200);
	        $(".cimm_dropdownMenu").slideUp(200);
		return false;
		});
		$(document).mouseup(function() {
			$('.loginDropdown').slideUp(200);
		});
		$(".loginDropdown").mouseup(function() {
			return false;
		});

		$('.loginDropdown [tabindex= 6]').on('keydown', function (e) {
			   if (e.keyCode == 9 && e.shiftKey == false) {
			    e.preventDefault();
			    $('.loginDropdown [tabindex=1]').focus();
			   }
			  });
     
	$('.cimm_dropdownMenu > dl').on('click', 'dt', function() {
		$(this).next().slideToggle(200).siblings('.cimm_dropdownMenu dl dd');
		$(this).find('i').toggleClass('fa-rotate-90');
	});

	$("#dropDownMenuToggle").click(function(e){
	    $(".cimm_dropdownMenu").slideToggle(200);
		e.stopPropagation();
		if($('.needHelpWrap').is(':visible')) {
			$(".needHelpWrap").slideUp(200);
		}
	});

	$(".cimm_dropdownMenu").click(function(e){
	    e.stopPropagation();
	});

	$('#dropDownCloseBtn').click(function(){
	    $(".cimm_dropdownMenu").slideUp(200);
	});

	$(".needHelpLink").click(function(e){
	    $(".needHelpWrap").slideToggle(200);
	     e.stopPropagation();
	     if($('.cimm_dropdownMenu').is(':visible')) {
				$(".cimm_dropdownMenu").slideUp(200);
			}
	     if($('.loginDropdown').is(':visible')) {
	    	 	$('.loginDropdown').slideUp(200);
			}
	});
	
    if (!console) console = {log: function() {}};
	if ($(".formatPhoneNumber").length > 0) {
		
		$(".formatPhoneNumber").each(function(){
			$(this).attr("maxlength","12");
		});
		
		
		$(".formatPhoneNumber").keypress(function(e) {
			if (e.keyCode != 8) {
				var curchr = this.value.length;
				var curval = $(this).val();
				if (curchr == 3) {
					$(this).val( curval + "-");
				} else if (curchr == 7) {
					$(this).val(curval + "-");
				}
			}
		});
	}
	
	if ($(".formatZipCode").length > 0) {
			
			$(".formatZipCode").each(function(){
				$(this).attr("maxlength","10");
			});
			
			
			$(".formatZipCode").keypress(function(e) {
				if (e.keyCode != 8) {
					var curchr = this.value.length;
					var curval = $(this).val();
					if (curchr == 5) {
						$(this).val( curval + "-");
					}
				}
			});
		}
		
	$(".formatPhoneText").text(function(i, text) {
	    text = text.replace(/(\d\d\d)(\d\d\d)(\d\d\d\d)/, "$1-$2-$3");
	    return text;
	});
	$(".formatPhoneText").val(function(i, text) {
	    text = text.replace(/(\d\d\d)(\d\d\d)(\d\d\d\d)/, "$1-$2-$3");
	    return text;
	});
	
	$(".formatZipCodeText").text(function(i, text) {
	    text = text.replace(/(\d\d\d\d\d)(\d\d\d\d)/, "$1-$2");
	    return text;
	});
	
	$(".formatZipCode").val(function(i, val) {
		val = val.replace(/(\d\d\d\d\d)(\d\d\d\d)/, "$1-$2");
	    return val;
	});
});

</script>
