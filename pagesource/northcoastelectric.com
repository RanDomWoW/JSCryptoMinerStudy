<script>
var footer_icon_plus = 'fa-plus';
var footer_icon_minus = 'fa-minus';
var responsiveflagFooter = false;
function accordionFooter(status){
		if(status == 'enable'){
			$('.footerCol h4 i').on('click', function(){
				$(this).parent().parent().find('.toggle_content').stop().slideToggle();
				$(this).toggleClass('fa-minus');
			});
		}else{
			$('.footerCol h4').removeClass('active').off().parent().find('.toggle_content').removeAttr('style').slideDown('fast');
			$('.footerCol h4 i').removeClass(footer_icon_minus).addClass(footer_icon_plus);
			$('.footerCol').removeClass('accordion');
		}
	}		
function toDoFooter(){
	   if ($(document).width() < 980 && responsiveflagFooter == false){
		    accordionFooter('enable');
			responsiveflagFooter = true;		
		}
		else if ($(document).width() >= 980){
			accordionFooter('disable');
	        responsiveflagFooter = false;
		}
}

$(window).resize(function(){
	toDoFooter();
});
$(document).ready(function(){
	toDoFooter();
});

</script>

