function startVoice(){
	if (annyang) {
		  // Let's define our first command. First the text we expect, and then the function it should call
		  var commands = {
		    'search for *tag': function(tag) {
		      /* alert("searching for "+tag); */
		      //tag = tag.replace(/\s/g, '');
		      window.location.href = $("base").attr("href")+'/searchPage.action?keyWord='+tag;
		    },
		    'add to cart': function() {
				$('.addToCart').click();
			}
		  };
		  // Add our commands to annyang
		  annyang.addCommands(commands);

		  // Start listening. You can call this here, or attach this call to an event, button, etc.
		  annyang.start();
		}
}

	$(function(){
		$(window).resize(function(){
			var width = $(window).innerWidth();
			if(width < 980){
				$('#normalHead').height("");
			}
		});
		
		var clonePrice = $("#yourPrices").html();
		$("#clonePrice").html(clonePrice);
		
		$("[data-cloneqty='cloneQty']").keyup(function(){
			$("[data-originalqty='originalQty']").val($(this).val());
		});
		$("[data-originalqty='originalQty']").keyup(function(){
			$("[data-cloneqty='cloneQty']").val($(this).val());
