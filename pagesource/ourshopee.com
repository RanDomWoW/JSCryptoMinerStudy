			{
				$.ajax({
					url:'https://www.ourshopee.com/fetch.php',
					type:'POST',
					data:'keyword='+key,
					beforeSend:function () {
						$("#results").slideUp('fast');
					},
					success:function (data) {
																					
						$("#results").html(data);
						$("#results").slideDown('fast');
					}
				});
			}
			else
			{
				document.getElementById('results').innerHTML = "";
				$("#results").slideUp('fast');
			}
        });
    });
</script>


