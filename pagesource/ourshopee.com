<script>
function doSubmit()
{
//alert('str');
	document.getElementById("image1").style.display="block";
	document.getElementById("reportsection").innerHTML ="";
	var email = document.getElementById("subscribe_email").value;
	var msg_email = "";
	var atpos = email.indexOf("@");
    var dotpos =email.lastIndexOf(".");
	
	
	if(email == '')
	{
	msg_email = "<span style='color:#FF0000'>This field is required</span>";
	document.getElementById("image1").style.display="none";
	document.getElementById("reportsection").innerHTML =msg_email;
	//alert();
	}
	 else if (atpos<1 || dotpos<atpos+2 || dotpos+2>=email.length)  
	{
	  
   	msg_email = "<span style='color:#FF0000'>Please Enter Valid Email address</span>";
	document.getElementById("image1").style.display="none";
	document.getElementById("reportsection").innerHTML =msg_email;
	//alert();
  }
  else
  {
	
	$.ajax({
	
	type: "POST",
	url: "https://www.ourshopee.com/set_newsletter.php",
	data:"email="+email,
	success: function(data){
		document.getElementById("image1").style.display="none";
		 document.getElementById("subscribe_email").value = "";
		$("#reportsection").html(data);
		
	}
	});
	}

	
	
	
}
</script>
