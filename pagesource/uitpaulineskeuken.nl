			</div><script type="text/javascript">
	jQuery( document ).ready(function() {
		jQuery( ".post_content a" ).each(function( index ) {
		  jQuery(this).attr('rel', 'noskim');
		});
	});
	
	function updateFavorite(pid, wtd){
		var xhttp = new XMLHttpRequest();
		xhttp.onreadystatechange = function() {
			if (xhttp.readyState == 4 && xhttp.status == 200) {
				if (xhttp.responseText !== "failed"){
					if (wtd == "add"){
						jQuery("#favorite" + pid).attr('src', "https://uitpaulineskeuken.nl/wp-content/themes/stockholm-child-twee/img/favorite.png");
						jQuery("#favorite" + pid).attr('onclick', "updateFavorite("+pid+", 'off')");
					}else{
						jQuery("#favorite" + pid).attr('src', "https://uitpaulineskeuken.nl/wp-content/themes/stockholm-child-twee/img/favoriteoff.png");
						jQuery("#favorite" + pid).attr('onclick', "updateFavorite("+pid+", 'add')");
					}				
				}		  
			}
		};
		xhttp.open("POST", "https://uitpaulineskeuken.nl/wp-content/themes/stockholm-child-twee/includes/addFavorite.php", true);
		xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
		xhttp.send("u=0&p="+pid+"&w="+wtd);
	}
	
	function allowDrop(ev) {
		ev.preventDefault();
		if (ev.target.getAttribute("draggable") == "true" || ev.target.getAttribute("drop") == "false")
			ev.dataTransfer.dropEffect = "none"; // dropping is not allowed
		else
			ev.dataTransfer.dropEffect = "all"; // drop it like it's hot
	}

	function drag(ev) {
		ev.dataTransfer.setData("text/html", ev.target.id);
	}

	function drop(ev, day) {
		ev.preventDefault();
		var data = ev.dataTransfer.getData("text/html", ev.target.id);
		ev.target.appendChild(document.getElementById(data));
		var blog = data.replace("blog",'');
		addFavoriteToDay(blog, day, 'update');
	}
	
	function addFavoriteToDay(pid, day, wtd){
		if (day != ""){	
			var xhttp = new XMLHttpRequest();
			xhttp.onreadystatechange = function() {
				if (xhttp.readyState == 4 && xhttp.status == 200) {		
					if (wtd != 'update'){
						var txt = xhttp.responseText;
						if (txt.indexOf('update--') !== -1){
							document.getElementById("blog" + pid).remove();
							jQuery('#day' + day).html(jQuery('#day' + day).html() + txt.replace('update--',''));
						}else{
							jQuery('#day' + day).html(jQuery('#day' + day).html() + txt);
						}						
					}					
				}
			};
			xhttp.open("POST", "https://uitpaulineskeuken.nl/wp-content/themes/stockholm-child-twee/includes/addToWeek.php", true);
			xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
			xhttp.send("u=0&p="+pid+"&d="+day);
		}
	}
	function removeFavoriteToDay(pid, day){
		if (day != ""){	
			var xhttp = new XMLHttpRequest();
			xhttp.onreadystatechange = function() {
				if (xhttp.readyState == 4 && xhttp.status == 200) {					
					document.getElementById("blog" + pid).remove();
				}
			};
			xhttp.open("POST", "https://uitpaulineskeuken.nl/wp-content/themes/stockholm-child-twee/includes/removeToWeek.php", true);
			xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
			xhttp.send("u=0&p="+pid+"&d="+day);
		}
	}
	
	Element.prototype.remove = function() {
		this.parentElement.removeChild(this);
	}
	NodeList.prototype.remove = HTMLCollection.prototype.remove = function() {
		for(var i = this.length - 1; i >= 0; i--) {
			if(this[i] && this[i].parentElement) {
				this[i].parentElement.removeChild(this[i]);
			}
		}
	}
	function printDiv(divName) {
		var printContents = document.getElementById(divName).innerHTML;
		printContents = printContents.replace(/class="delete"/g,'class="delete" style="display:none;"');
		var originalContents = document.body.innerHTML;
		document.body.innerHTML = printContents;
		window.print();
		setTimeout(function () { document.body.innerHTML = originalContents; }, 10000);		
	}
	function PrintElem(elem)
    {
		var content = jQuery(elem).html();
		content = content.replace(/<img[^>]*>/g,"");
		content = content.replace(new RegExp('<span title="Verwijder">X</span>', 'g'),"");		
		Popup(content);
    }

    function Popup(data) 
    {
        var mywindow = window.open('', 'UIT PAULINE’S KEUKEN', 'height=400,width=600');
        mywindow.document.write('<html><head><title>UIT PAULINE’S KEUKEN</title>');
		mywindow.document.write('<style>.widget h4 {color: #7a7a7a;text-transform: uppercase;border: 1px solid #efefef;background-color: #efefef;margin-bottom: 18px;padding: 12px 18px;} a{color: #7a7a7a;text-decoration:none;}.dayOfTheWeek{padding-left: 1em;}</style>');
        /*optional stylesheet*/ //mywindow.document.write('<link rel="stylesheet" href="https://uitpaulineskeuken.nl/wp-content/themes/stockholm-child-twee/css/print.css" type="text/css" />');
        mywindow.document.write('</head><body data-rsssl=1 >');
        mywindow.document.write(data);
        mywindow.document.write('</body></html>');

        mywindow.document.close(); // necessary for IE >= 10
        mywindow.focus(); // necessary for IE >= 10

        mywindow.print();
        mywindow.close();

        return true;
    }
</script>
