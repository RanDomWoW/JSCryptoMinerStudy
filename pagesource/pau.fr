<script>
$(document).ready(function(){
    function mycarousel_initCallback(carousel)
    {
        // Pause autoscrolling if the user moves with the cursor over the clip.
        $('.carousel').hover(function() {
            carousel.stopAuto();
        }, function() {
            carousel.options.auto = 6;
            carousel.startAuto();
        });

        $('.carousel ul.liste.mini li.item a').click(function(e) {
            e.preventDefault();

            carousel.stopAuto();
            carousel.scroll($(e.target).parents('ul.liste').find('li').index($(e.target).parents('li.item')) + 1);
        });
    };

    function mycarousel_loadCallback(carousel)
    {
        carousel.options.auto = 0;
        $('.carousel ul.liste.mini li.item').removeClass('selected').eq(carousel.first - 1).addClass('selected');
    };

    $('.carousel').jcarousel({
        scroll: 1,
        initCallback: mycarousel_initCallback,
        itemLoadCallback: mycarousel_loadCallback,
        visible: 1,
        auto: 6
    });

