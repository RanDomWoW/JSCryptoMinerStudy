<script id="notificationFavoriteTemplate" type="text/x-jsrender">
		{{if isRead == true}}<li class="finish" >{{else}}<li>{{/if}}
		    {{if target =="WEBTOON"}}
			<a href="/titles/{{:id}}" class="N=a:noti.favorite">
