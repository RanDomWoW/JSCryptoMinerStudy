			{{/if}}
				<dl>
					<dt>
						<span class="thmb">
							<img src="{{:thumbnailUrl.titleMediumUrl}}" alt="{{:name}}" width="121px" height="83px"/>
							<span class="icon i-cover-new"></span>
							<span class="line"></span>
						</span>
						
					</dt>
					<dd>
						<h3>{{:name}}</h3>
						<p>{{:author}}</p>
						<span>
							<span class="label label-up">UP</span>
							{{dateTime:updatedAt}}
						</span>
					</dd>
					
					
				</dl>
			</a>
		</li>	
	</script>
	<script id="notificationRentTemplate" type="text/x-jsrender">
