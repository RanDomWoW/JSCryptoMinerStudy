$(document).ready(function(){
	var pacha = 0;
// Increase Font Size
	$('.increaseFont').click(function(){
		pacha++;
		if(pacha < 3){
		$('a , span , h3 , td , p').css('font-size', function() {
            return parseInt($(this).css('font-size')) + 1 + 'px';
        });
		$('a , span , h3 , td , p').css('word-spacing', function() {
            return parseInt($(this).css('word-spacing')) - 1 + 'px';
        	});
		}else{
		    pacha--;
		}
		$('.SM li a').css('font-size','16px');
		$('.MM li a').css('font-size','16px');
	});
// Decrease Font Size
	$('.decreaseFont').click(function(){
		pacha--;
		if(pacha > -1){
		 $('a , span , h3 ,td , p').css('font-size', function() {
		 return parseInt($(this).css('font-size')) - 1 + 'px';
			        });
		$('a , span , h3 , td , p').css('word-spacing', function() {
            	return parseInt($(this).css('word-spacing')) + 1 + 'px';
        	});
		}else{
		    pacha++;
		}
		$('.SM li a').css('font-size','16px');
		$('.MM li a').css('font-size','16px');
        });
});
