jQuery.validator.addMethod("validCaptcha", function(value, element) 
{
     if(value == '') 
        return true;
    var cap = $('#captcha_value').val();
    var temp1 = true;;
    //console.log(value);
    
   if( cap === value) 
       {
           return temp1;
       }
    else{
        return false;
    }
}, "Invalid email address");   
    
    
	
	jQuery.validator.addMethod("validName", function(value, element) {	if(value == '')         return true;    var temp1;    temp1 = true;	var str = value;	if(/^[a-zA-Z0-9_ ]*$/.test(str) == false)     return false;		temp1 = /^[a-zA-Z0-9_ ]*$/.test(str);	return temp1;}, "Invalid Name");
jQuery.validator.addMethod("validEmail", function(value, element) 
{
    if(value == '') 
        return true;
    var temp1;
    temp1 = true;
    var ind = value.indexOf('@');
    var str2=value.substr(ind+1);
    var str3=str2.substr(0,str2.indexOf('.'));
    if(str3.lastIndexOf('-')==(str3.length-1)||(str3.indexOf('-')!=str3.lastIndexOf('-')))
        return false;
    var str1=value.substr(0,ind);
    if((str1.lastIndexOf('_')==(str1.length-1))||(str1.lastIndexOf('.')==(str1.length-1))||(str1.lastIndexOf('-')==(str1.length-1)))
        return false;
    str = /(^[a-zA-Z0-9]+[\._-]{0,1})+([a-zA-Z0-9]+[_]{0,1})*@([a-zA-Z0-9]+[-]{0,1})+(\.[a-zA-Z0-9]+)*(\.[a-zA-Z]{2,3})$/;
    temp1 = str.test(value);
    return temp1;
}, "Invalid email address");
</script>



