<script>
	$(function(){
		var index=0;
		var tNext;
		var setSlideActive=function(index){
			var li=$('.da-dots li.number:eq('+ index +')');
			li.parent().find('.da-dots-current').removeClass('da-dots-current');
			li.addClass('da-dots-current');
			$('.box-us-dc .user-1').hide('fast');
			$('.box-us-dc .user-1:eq(' + index + ')').show('slow');
			tNext=setTimeout(function(){
				var length = $('.da-dots li.number').length;
				if(index<length-1) index++;
				else index = 0;
				setSlideActive(index);
			},10000);
		}
		$('.da-dots li.number').click(function(){
			if(tNext){
				clearTimeout(tNext);
				tNext=null;
			}
			var li = $(this);
			var index = li.index();
			setSlideActive(index);
		});
		setSlideActive(index);
	});
