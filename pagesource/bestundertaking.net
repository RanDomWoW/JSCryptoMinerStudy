    <script type="text/javascript">

        Sys.Application.add_load(GlobalApplicationLoadHandler);

        function GlobalApplicationLoadHandler(sender, args) {
            var prm = Sys.WebForms.PageRequestManager.getInstance();
            if (!prm.get_isInAsyncPostBack()) {
                prm.add_initializeRequest(initRequest);
                prm.add_endRequest(endRequest);
            }
        }
        function initRequest(sender, args) {
            var pop = $find("ctl00_GlobalModalPopupExtender");
            pop.show();
        }

        function endRequest(sender, args) {
            var pop = $find("ctl00_GlobalModalPopupExtender");
            pop.hide();
        }
    </script>

    <script language="javascript" type="text/javascript">
        function FnReqvalid(evt) {
            if (document.getElementById('ctl00_ContentPlaceHolder2_TxtAmt').value != '') {
                FnChkvalid(evt)
                document.getElementById('ctl00_ContentPlaceHolder2_VTxtAmt').innerText = '';
                return true;
            }
            else {
                document.getElementById('ctl00_ContentPlaceHolder2_VTxtAmt').innerText = '*';
                //alert('Enter Amount');
                return false;

            }
        }


        function FnChkvalid(evt) {
            evt = (evt) ? evt : window.event;
            var charCode = (evt.which) ? evt.which : evt.keyCode;

            if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                //alert("Numeric values only Allowed");
                return false;
            }

            return true;
        }

    </script>

    

