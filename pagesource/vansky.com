<script type="text/javascript">
  function carouselActive(ads) {
    for (var i = 0; i < ads.length; i++) {
      var item = $(ads[i]);
      if (item.hasClass('active')) {
        item.removeClass('active');
        var nextActive = 0;
        if (i < ads.length - 1) {
          nextActive = i + 1;
        }
        $(ads[nextActive]).addClass('active');
        break;
      }
    }
  }




  setInterval(function() {
    carouselActive($('#big_ad_banner'));
    carouselActive($('#small_ad_banner'));
  },3500);

