    jQuery(document).ready(function($) {
        jQuery.fn.getKey = function() {
           var o = $(this[0]); return parseInt(o.attr('id').match(/\d+/));
        };
        
        //bind click handlers to radio buttons
        $('#planTemporalitySet > .planTemporalityRadio').bind('click', function() {
            changeTemporality($(this).getKey());
        });
        
        //bind click handlers create shop button
        $(".pricing-table .create-shop-button").click(function(event){ 
            $("#express-registration-form input[name=planProfileId]").val($(this).data('planprofileid'));
            $("#express-registration-form input[name=planTemporality]").val($('#planTemporalitySet > .active').data('plantemporality'));
            $("#express-registration-form input[name=registerType]").val('reHome - Precios');
                        
        });
        
        //triggering the first plan settings for current or default plan
        changePrices(currentOrDefaultTemporality);
        
        //disable current plan column
        if (0) {
            var newTemporality = (true) ? $('#planTemporalitySet > .active').getKey() : 0;
            disableCurrentPlan(0, false, 0, newTemporality, 0, 0);
        }
    });
    
    
    function changeTemporality (newTemporality) {
        changePrices(newTemporality);
        if (0) {
            disableCurrentPlan(0, false, 0, newTemporality, 0, 0);
        }
    }
    
    function disableCurrentPlan(plan_profile, isActiveAccount, currentTemporality, newTemporality, currentPlanType, newPlanType) {
        if (plan_profile != 0 && isActiveAccount) {
            if ((currentTemporality == newTemporality) && (currentPlanType == newPlanType)) {
                $('.pricing-table-form-' + plan_profile).hide().addClass('currentPlan');
            } else {
                $('.pricing-table-form-' + plan_profile + ' .create-shop-button').html('Actualizar Plan');
                $('.pricing-table-form-' + plan_profile).show().removeClass('currentPlan');
            }
        }
    }

    function getPriceHtml (prices, key) {
        if (prices[key] != undefined) {
            priceLabel = prices[key]  + '€' + '<span class="per-month-text">/mes</span>';
        } else {
            priceLabel = "-";
        }
        return '<strong class="number">' + priceLabel + '</strong>'
    }

    function getPriceCompleteHtml (prices, temporality, key) {
        if (prices[key] != undefined) {
            priceLabel =  Number((prices[key].replace(',','.') * key).toFixed(2))  + '€' + ' /' + temporality[key];
        } else {
            priceLabel = "No Disponible";
        }
        return '<span class="plan-price-complete">' + '(' + priceLabel + ')' + '</span>';
    }
    
    function changePrices(newTemporality) {
        var element = $('#radio-'+ newTemporality);
        if (newTemporality == 0){
            return false;
        }
        var key = newTemporality;
        var basic_plan = 1, profesional_plan = 2, premium_plan = 3;
        var monthly_plan = 1, trimestral_plan = 3, anual_plan = 12;
        
        var temporality_text = [];
        temporality_text [monthly_plan] = 'mes';
        temporality_text [trimestral_plan] = 'trimestre';
        temporality_text [anual_plan] = 'año';
        

        for (var i = prices.length - 1; i >= 1; i--) {
            $('#' + i + '-plan-price').html(getPriceHtml(prices[i], key));
            $('#' + i + '-plan-price').append(getPriceCompleteHtml(prices[i], temporality_text, key));
            $('.pricing-table-form-' + i +  ' .create-shop-button').css('visibility', prices[i][key] == undefined ? 'hidden' : 'visible');
        };


        //change temporality input forms
        $('input.planTemporality').val(key);
        
        element.blur(); return true;
    }
    
    function showFeatures(evt){
        if (evt != undefined)
            evt.preventDefault();
        if ($('.plan-extra-features').is(":visible")) {
            $('.plan-extra-features').hide();
            $('.plan-extra-features-btn').html("¿Qué más incluye?");
        } else {
            $('.plan-extra-features').show();
            $('.plan-extra-features-btn').html("Mostrar menos");
            $('html, body').animate({scrollTop:$('.plan-extra-features').offset().top});
        }
    }
    $(document).on('click', '.plan-extra-features-btn', showFeatures);
    
</script> </div>
