				$('#block-views-home-block-4-linkestan .view-content').height($('.region-vitrin-3-3').height()-148);
				$('#block-views-home-block-4-weblogestan-front .view-content').height($('.region-vitrin-3-3').height()-148);
				equalHeight($(".region-vitrin-3-3 .block:not(.last)"));
				$('.region-vitrin-3-3 .block:not(.last)').width('294');
			}
			if (winwidth < 980 && winwidth >= 480) {
				//$('.header-menu').css('display','inline-block');
				$('body').addClass('tablet');
				$('.region-vitrin-1-2').width(winwidth-$('.region-vitrin-1-1').width()-42);
				$('.region-vitrin-2-2').width(winwidth-$('.region-vitrin-2-1').width()-34);
				$('.region-vitrin-3-2').width(winwidth-$('.region-vitrin-3-1').width()-11);
				$('body.front .vitrin_content').width(winwidth-3);
				$('.not-front .region-vitrin-3-2').width(winwidth-$('body.tablet .vitrin_content').width()-11);
				$('#block-views-home-block-4-newest .view-content').height($('.vitrin_3_inner').height()-153);
				$('#block-views-home-block-4-linkestan .view-content').height($('.region-vitrin-3-3').height()-114);
				$('#block-views-home-block-4-weblogestan-front .view-content').height($('.region-vitrin-3-3').height()-114);
				$('body.front .region-vitrin-3-4').width(winwidth-$('.region-vitrin-3-3').width()-11);
				//$('body.node-type-story #block-views-home-block-4-linkestan').width($('.node_bottom').width()-$('.comment-wrapper').width()-24);
				$('.region-vitrin-3-3 .block').width($('.region-vitrin-3-3').width()-16);
				$('.region-vitrin-3-3 .block.last').width($('.region-vitrin-3-3').width()-10);
				$('body.tablet .view-archives .views-row').width($('body.tablet .vitrin_content').width()-33);
				equalHeight($(".vitrin_2 .region"));
			} else {
				$('body').removeClass('tablet');
				$('.region-vitrin-1-2').attr( "style", "" );
				$('.region-vitrin-2-2').attr( "style", "" );
				$('.region-vitrin-3-2').attr( "style", "" );
				$('.region-vitrin-3-4').attr( "style", "" );
				$(".vitrin_2 .region").attr( "style", "" );
				$(".vitrin_content").attr( "style", "" );
				$('.region-vitrin-3-3 .block.last').attr( "style", "" );
				$('body.node-type-story #block-views-home-block-4-linkestan').attr( "style", "" );
				$('body.node-type-story #block-views-home-block-4-weblogestan-front').attr( "style", "" );
			}
			
			if (winwidth <= 480 ) {
				$('body').addClass('mobile');
				$(".region-vitrin-3-3 .block:last").attr( "style", "" );
				$('.region-vitrin-3-3 .block').width(winwidth-16);
				$('body.node-type-story #block-views-home-block-4-linkestan').width(winwidth-13);
				$('body.node-type-story #block-views-home-block-4-weblogestan-front').width(winwidth-13);
			}
			if (winwidth > 480 ) {
				$('body').removeClass('mobile');
			}
		}
		
	})(jQuery);
