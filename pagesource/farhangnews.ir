		var checksize = function() {
			//var pagemargin = 42;
			var winwidth = $(window).width();
			var winheight = $(window).height();
			if (winwidth >= 980) {
				$('.header-menu').show();
				$('#block-views-home-block-4-newest .view-content').height($('.region-vitrin-3-1').height()-$('#block-views-home-block-4-pishnahad').height()-129);
