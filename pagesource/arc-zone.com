				snippetId = "#" + $(this).attr("id");
				snippetHeight = $(snippetId).children(".blogSnippetContent").height();
				snippetPadding = (127 - snippetHeight)/2;
				$(snippetId + " p.blogTitle").css("padding-top", snippetPadding);		
			});	
		};

		var azImgSwaps = function() { // setup image swaps for featured images
			if (!Modernizr.touch){
				$(".azRollover").each(function (){ // preload images
					azFPImageOnSrc = this.src.replace("_off", "_on");
					$('<img/>')[0].src = azFPImageOnSrc ; //trick method for preloading
