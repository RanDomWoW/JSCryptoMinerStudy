	<script type="text/javascript">
		$(document).ready(
			function(){
				showBanner();
				
				$("#signup-banner span").click( //click close button
					function(){
						hideBanner();
						setCookie("showBanner", 0, 30);
					}
				);
			}
		);
		
		$(document).scroll(function() { 
			if($(window).scrollTop() === 0) {
				showBanner();
			}
		});
		
		function showBanner() { $( "#signup-banner" ).show( 'blind', 500, callback ); }
		
		function callback() {
			var id = setTimeout(hideBanner, 4000 );
			$("#signup-banner").click(
				function(){
					$("#pin-banner").html("&#10006;");
					clearTimeout(id);
				}
			);
		};
		
		function hideBanner() {
			$( "#signup-banner:visible" ).hide( 'blind', 500 );
		}
		
		function getCookie(cname) {
			var name = cname + "=";
			var ca = document.cookie.split(';');
			for(var i = 0; i < ca.length; i++) {
				var c = ca[i];
				while (c.charAt(0) == ' ') {
					c = c.substring(1);
				}
				if (c.indexOf(name) == 0) {
					return c.substring(name.length, c.length);
				}
			}
			return "";
		}
		
		function setCookie(cname, cvalue, expdays) {
			var d = new Date();
			d.setTime(d.getTime() + (expdays * 24 * 60 * 60 * 1000));
			var expires = "expires="+d.toUTCString();
			document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
		}
	</script>
