function check_email_mm_universal(data,$this) {
	if (data.status=='ok') {
		$this.closest('.massmail-div').html(data.message);
		(window["rrApiOnReady"] = window["rrApiOnReady"] || []).push(function() { rrApi.setEmail(data.email);	});
	} else {
		alert(data.message);
	}
}

