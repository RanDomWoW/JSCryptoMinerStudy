        <script>
        //<![CDATA[
        function manipulationOfCart(product, type, list) {
            product = JSON.parse(product);

            if (list == undefined){
                list='Category - '+ product.category
            }

            var data = [{
                'name'        : product.name,
                'id'          : product.id,
                'price'       : product.price,
                'brand'       : product.brand,
                'category'    : product.category,
                'quantity'    : product.qty,
                'list'        : list
            }];

            if(product.priceTag == 'Promotion'){
                data[0]['price']   = product.specialPrice;
                data[0]['metric1'] = product.regularPrice;
            }else{
                data[0]['price']   = product.regularPrice;
                data[0]['metric1'] = product.specialPrice;
            }
            data[0]['dimension7'] = product.priceTag;

            if (type == 'add'){
                dataLayer.push({
                    'event': 'addToCart',
                    'ecommerce': {
                        'currencyCode': 'CAD',
                        'add': {
                            'products': data
                        }
                    }
                });
            }
            else if (type == 'remove'){
                dataLayer.push({
                    'event': 'removeFromCart',
                    'ecommerce': {
                        'currencyCode': 'CAD',
                        'remove': {
                            'products': data
                        }
                    }
                });
            }
        }

        jQuery(document).ready(function () {
            //$j.cookie.json = true;
                        var productToBasket = $j.cookie("productToBasket");
            var productlist     = $j.cookie("productlist");

            if (productToBasket != undefined) {
                manipulationOfCart(productToBasket, 'add', productlist);
                $j.removeCookie("productToBasket", {
                    path: '/',
                    domain: '.www.sail.ca'
                });
                $j.removeCookie("productlist", {
                    path: '/',
                    domain: '.www.sail.ca'
                });
            }

            var productOutBasket = $j.cookie("productOutBasket");

            if (productOutBasket != undefined) {
                manipulationOfCart(productOutBasket, 'remove', '');
                $j.removeCookie("productOutBasket", {
                    path: '/',
                    domain: '.www.sail.ca'
                });
            }
        });
        //]]>
    </script>
