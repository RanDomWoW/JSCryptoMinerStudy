            // jQuery("#section-video-banner .vimeoWrapper").css("display", "block");
            // jQuery("#section-video-banner").css("height","auto");
            resizeVimeoBanner();
        });

        jQuery(window).resize(function(){
            resizeVimeoBanner();
        });

    });
    </script>
