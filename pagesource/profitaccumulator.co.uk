<script type="text/javascript">

    function resizeVimeoBanner(){

        if(jQuery(window).width() <= 767){

            if(jQuery("#section-video-banner .vimeoIframeWrapper iframe").attr("src") == "https://player.vimeo.com/video/226724497?background=1"){
                jQuery("#section-video-banner .vimeoIframeWrapper iframe").attr("src","https://player.vimeo.com/video/226720495?title=0&byline=0&portrait=0");
                jQuery("#section-video-banner span").css("display","none");
            }

            var iframeHeight = jQuery("#section-video-banner .vimeoIframeWrapper iframe").height();
            jQuery("#section-video-banner").css("max-height", iframeHeight);

        } else {

            if(jQuery("#section-video-banner .vimeoIframeWrapper iframe").attr("src") == "https://player.vimeo.com/video/226720495?title=0&byline=0&portrait=0"){
                jQuery("#section-video-banner .vimeoIframeWrapper iframe").attr("src","https://player.vimeo.com/video/226724497?background=1");
                jQuery("#section-video-banner span").css("display","block");
            }

            var iframeHeight = jQuery("#section-video-banner .vimeoIframeWrapper iframe").height();
            jQuery("#section-video-banner").css("max-height", iframeHeight-55);

        }

    }


    jQuery(document).ready(function(){

        resizeVimeoBanner();

        jQuery("#section-video-banner span").click(function(){
            jQuery("#section-video-banner span, #section-video-banner > iframe").remove();
            jQuery("#section-video-banner .vimeoIframeWrapper iframe")
