<script type="text/javascript">
		//Do time stuff
		function currentTime(){var e=new Date;e=new Date(e.getUTCFullYear(),e.getUTCMonth(),e.getUTCDate(),e.getUTCHours(),e.getUTCMinutes(),e.getUTCSeconds()),document.getElementById("d_time").innerHTML=format_time(e);setTimeout(currentTime,500)}function format_time(e){var t=e.getHours(),n=e.getMinutes(),r=(e.getSeconds(),t>11?"pm":"am");return t>12?t-=12:0==t&&(t="12"),10>n&&(n="0"+n),t+":"+n+r}
		
		currentTime();
		
		var TotalMessages = 0;
		var CurrentServers = 0;
		var CurrentUsers = 0;
	
        $(function () {
        //Set the hubs URL for the connection
            $.connection.hub.url = "https://www.carbonitex.net/dynamic/signalr";
            
            // Declare a proxy to reference the hub.
            var chat = $.connection.carbonHub;
            
            chat.client.initData = function (tm, cs) {
				TotalMessages = tm;
				CurrentServers = cs;
				$("#d_CurrentServers").html(CurrentServers);
				$("#d_TotalMessages").html(TotalMessages);
            };
			
            chat.client.tick = function (uc, vc) {
				CurrentUsers = uc;
				$("#d_CurrentUsers").text(uc);
				$("#d_VoiceUsers").text(vc);
            };			
		
			chat.client.newMessage = function (todayMessageCount) {
				
				TotalMessages++;
				$("#d_TodayMessages").text(todayMessageCount);
				$("#d_TotalMessages").text(TotalMessages);
            };
			
            $.connection.hub.start().done(function () {
				chat.server.getInitData();
            });
        });
		
	
		var datax = [], totalPoints = 300;
		function setRealtimeData() {
			if (datax.length > 0)
				datax = datax.slice(1);

			// do a random walk
			while (datax.length < totalPoints) {
				var prev = datax.length > 0 ? datax[datax.length - 1] : 50;
				var y = CurrentUsers;
				datax.push(y);
			}

			// zip the generated y values with the x values
			var res = [];
			for (var i = 0; i < datax.length; ++i)
				res.push([i, datax[i]])
			return res;
		}

		function update() {	
			var options = {
				series: { shadowSize: 1 },
				lines: { fill: true, fillColor: { colors: [ { opacity: 1 }, { opacity: 0.1 } ] }},
				yaxis: { min: CurrentUsers-50, max: CurrentUsers+50 },
				xaxis: { show: false },
				colors: ["#43B581"],
				grid: {	tickColor: "#dddddd",
						borderWidth: 0 
				},
			};
			var plot = $.plot($("#realtimechart"), [  ], options);
		
			plot.setData([ setRealtimeData() ]);
			plot.setupGrid();
			// since the axes don't change, we don't need to call plot.setupGrid()
			plot.draw();
			plot.setupGrid();
			setTimeout(update, 400);
		}

		update();
		
		

		
		
    </script>
