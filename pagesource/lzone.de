<script>var markdown = 0;</script><script>var multistatus = 0;</script><script>var multistatus = 1;</script><div id='main'>
	<div class='content'>
		<h2>About this site</h2>
		<p>Hi! I'm <a href='https://github.com/lwindolf'>Lars Windolf</a> and working as system engineer I use this site as a resource for my daily work. To the left you can find an ever expanding collection of cheat-sheets.</p>

		<p>If you are interested in how Web frontend technologies are changing have a look at the <a href='/webscan'>WebScan</a> results to get insights on past and recent Web technology changes.</p>

		<br/>
		<hr/>
		<br/>
		<h2>Status Page Aggregator</h2>

		<div id='multistatus'><i>Loading...</i></div>
	</div></div><script>
var sheets = undefined;

function changeCategory(oldCat, newCat) {
	$('.category').removeClass('current');
	$('div.category[category="'+newCat+'"]').addClass('current');
	$('div.topic').hide();
	$('div.topic[category="'+newCat+'"]').show();
}

function changeTopic(topic, oldCat, newCat) {
	$('.topic[topic="'+topic+'"]').addClass('current');
	changeCategory(oldCat, newCat);
}

$(document).ready(function() {
	var category = 'DevOps Linux';
	var topic = unescape(document.location.href.split(/\//).pop());

	if(multistatus === 1) {
		$.getJSON('/multi_status.json', function(data) {
			$('#multistatus').html('<p><small>Below you find services whose status pages indicate current problems. Check the table below for more infos.</small></p> <div class="statuscloud"/> <h'+'3>Service Outage Details</h'+'3> <p><small>Last update '+new Date(data.time*1000).toLocaleString()+'</small></p><table class="multistatus"><tbody/></table>');
			$.each(data.aggregators.sort(function(a,b) {
				return a.name.toLowerCase() > b.name.toLowerCase();
			}), function(i, a) {
				if(a.results.length === 0)
					$('#multistatus div.statuscloud').append('<span style="background:#afa">'+a.name+'</span> ');
				else
					$('#multistatus div.statuscloud').append('<span style="background:#faa">'+a.name+' ('+a.results.length+')</span> ');
			});
			$.each(data.aggregators.sort(function(a,b) {
				var rlena = a.results.length;
				var rlenb = b.results.length;
				return rlena < rlenb;
			}), function(i, a) {
				if(a.results.length > 0) {
					var name = '<td class="name" rowspan="'+(a.results.length)+'"><a href="'+a.url+'">'+a.name+'</a></td>';
					$('#multistatus table.multistatus').append(
						a.results.map(function(obj) {
							var tmp = name;
							if(name != "")
								name = "";
							return "<tr>"+tmp+"<td>"+obj.time+"</td><td>"+obj.title+"</td><td>"+(obj.description !== undefined?obj.description:'')+"</td></tr>";
						}).join('')
					);
				}
			});
		})
		.fail(function() {
			$('#multistatus').html('Failed to fetch status JSON :-(');
		});
	}

	if(markdown === 1) {
 		var base = 'cheat-sheet';
 		if(document.location.href.match(/visual-ops/))
 			base = 'visual-ops';
 		$.get('/'+base+'/'+topic+'.md', function(data) {
			var converter = new showdown.Converter({ tables: true });
			var html = converter.makeHtml(data);
			html = addMedia(html);
			$('#main .content').html(html);
			$('#main h1').append("<a class='edit' href='https://github.com/lwindolf/lzone-cheat-sheets/edit/master/cheat-sheets/"+topic+".md' target='new'>Edit Cheat Sheet</a>");
			addToc();
		})
		.fail(function(j,t,e) {
			console.log(t);
			console.log(e);
			$('h1').html('Sorry cheat sheet '+topic+' not found!');
		});
	}

	// First resolve category this sheet belongs too
	var t = $('#topics div[topic="'+topic+'"]');
	if(t.length) {
		category = t.attr('category');
		console.log("found category: "+category);
	};
	changeTopic(topic, undefined, category);

	$('.category').click(function() {
		var newCategory = $(this).attr('category');
		changeCategory(category, newCategory);
		return false;
	});
});

function addSlide(id, text) {
	if ($('#slides').is(':empty')){
		$('#slides').append('<b>Slides</b><br/><br/>');
	}
	var html = '<div><div>'+text+'</div><iframe src="//www.slideshare.net/slideshow/embed_code/key/'+id+'" width="340" height="290" frameborder="0" marginwidth="0" marginheight="0" scrolling="no" style="border:1px solid #CCC; border-width:1px; margin-bottom:5px; max-width: 100%;" allowfullscreen> </iframe></div>';
	$('#slides').append(html);
}

function addJSFiddle(id) {
	return '<iframe width="100%" height="1000" src="//jsfiddle.net/lwindolf/'+id+'/embedded/result,js" allowpaymentrequest allowfullscreen="allowfullscreen" frameborder="0"></iframe>';
}

function addMedia(data) {
	var interesting = [];

	// Handle sheet cheat references
	data = data.replace(/<\?add topic=['\"]([^'\"]+)['\"]\?>/g, function(a, b) {
		interesting.push(b);
		return "<span class='related'><a href='/cheat-sheet/"+b+"'>"+b+"</a></span>";
	});

	// Handle example references
	data = data.replace(/<\?add example=['\"]([^'\"]+)['\"]\?>/g, function(a, b) {
		interesting.push(b);
		return "<span class='related'><a href='/examples/"+b+"'>"+b+"</a></span>";
	});


	// Handle slides
	data = data.replace(/<\?slideshare,([^,]+),([^>]+)\?>/g, function(a, id, text) {
		addSlide(id, text);
		return "";
	});

	// Handle jsfiddle
	data = data.replace(/\{jsfiddle:([^}]+)}/g, function(a, id) {
		return addJSFiddle(id);
	});

	$.each(interesting, function(i, b) {
		$('.topic[topic="'+b+'"]').addClass('interesting');
	});
	return data;
}

function addToc() {
	var sections = $("h2, h3, h4, h5, h6");
	if(sections.length < 5)
		return;
	sections.each(function(i, h) {
		//console.log(h);
		//console.log("<a href='"+$(h)+"'>"+$(h).text()+"</a>");
	});
}

function load_category(c) {
	$('#topics').empty();
	$('.category[category="'+c+'"]').addClass('current');
	$.each(sheets[c].sort(), function(i, name) {
		var base = 'cheat-sheet';
		var rel = '';
		if(-1 !== c.indexOf('Examples'))
			base = 'examples';
		if(-1 !== c.indexOf('visual-ops'))
			base = 'visual-ops';
		var link = "/"+base+'/'+name;
		// External links have format <name>,<link>
		if(-1 !== name.indexOf(",")) {
			var tmp = name.split(/,/);
			name = tmp[0];
			link = tmp[1];
			rel = ' rel="nofollow"';
		}
		$('#topics').append('<div topic="'+name+'" class="topic"><a '+rel+'href="'+link+'">'+name+'</a></div>');
	});
}

var defaultText = "Search Cheat Sheets...";
var searchBox = document.getElementById("query");

//default text after load
searchBox.value = defaultText;
searchBox.style.color = '#ccc';

//on focus behaviour
searchBox.onfocus = function() {
    if (this.value == defaultText) {//clear text field
        this.value = '';
	this.style.color = 'white';
    }
}

//on blur behaviour
searchBox.onblur = function() {
    if (this.value == "") {//restore default text
        this.value = defaultText;
	this.style.color = '#ccc';
    }
}

</script>
</body>
