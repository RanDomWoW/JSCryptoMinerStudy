        function CheckPhone(obj)
        {
     
            if (document.getElementById(obj.id).value != "") {
                if (!$.isNumeric(document.getElementById(obj.id).value)) {
                    $("#lblmsg").html("Mobile Number must be Number only!");
                    $("#lblmsg").css("color", "red");
                }
                else {
                    $("#lblmsg").html("");
                    $("#lblmsg").css("color", "black");
                }

                var x = $("#txtEmail").val();
                var atpos = x.indexOf("@");
                var dotpos = x.lastIndexOf(".");
                var y = $("#txtPhoneNumber").val();
                if (x != "") {
                    if ((atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= x.length)) {

                        $("#lblmsg").html("Not a valid e-mail address !");
                        $("#lblmsg").css("color", "red");
                    }
                }
                if (y != "") {
                    if ((atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= x.length) && !$.isNumeric(y)) {

                        $("#lblmsg").html("Not a valid e-mail address & Mobile number must be Number only!");
                        $("#lblmsg").css("color", "red");
                    }

                }
            }
            
        }
        function getLogin()
        {           
            if (document.getElementById("ctl00_ContentPlaceHolder1_hdnUserName").value != "") {
                document.getElementById("userinbox").style.display="block";
            }
            else {
                document.getElementById("userinbox").style.display = "none";
            }
        }
        function GetCount(obj, len) {
            //alert($(obj).val().length);
            if ($(obj).val().length <= 500) {
                $("#lblCount").html($(obj).val().length + " out of " + len);
                $("#lblCount").css("color", "black");
            }
            else {
                $("#lblCount").html("Please Enter characters " + len + " below");
                $("#lblCount").css("color", "red");
                document.getElementById(obj.id).value = document.getElementById(obj.id).value.substring(0, 500);
                // alert($(obj).text());
            }
        }
        function CheckEmail(obj)
        {
            var x = $("#txtEmail").val();
            var atpos = x.indexOf("@");
            var dotpos = x.lastIndexOf(".");
            if (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= x.length) {
                $("#lblmsg").html("Not a valid e-mail address");
                $("#lblmsg").css("color", "red");  
            }
            else
            {
                $("#lblmsg").html("");
