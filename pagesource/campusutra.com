                    if (typeof ($('#lblEmail').html()) != "undefined") {

                        if ($('#lblEmail').html() == "")
                            $("#txtEmail").after("&nbsp;<label style='color:red' id='lblEmail'>*</label>");
                    }
                    else {
                        $("#txtEmail").after("&nbsp;<label style='color:red' id='lblEmail'>*</label>");
                    }
                }
                else {
