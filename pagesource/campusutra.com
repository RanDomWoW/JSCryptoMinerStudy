                    }
                }
                else {
                    $('#ClgName').html("");
                }
            }
            if ($("#txtPhoneNumber").val() != "") {
                if (!$.isNumeric(PhoneNumber)) {

                    $("#lblmsg").html("Mobile Number must be Number only!");
                    $("#lblmsg").css("color", "red");
                }
            }
            var x = $("#txtEmail").val();
            var atpos = x.indexOf("@");
            var dotpos = x.lastIndexOf(".");
            if (x != "" && PhoneNumber != "") {
                if ($.isNumeric(PhoneNumber) && (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= x.length)) {

                    $("#lblmsg").html("Not a valid e-mail address & Mobile number must be Number only! ");
                    $("#lblmsg").css("color", "red");
                }
            }
            
            return false;
        }
