<script>

    var scrolledToPost = false;
    var goToPost = function(){

        if(scrolledToPost)
            return;

        var id;
        var p = location.search;

        scrolledToPost = true;

        if(p && p.indexOf("post=") >= 0)
            id = p.split("post=")[1];
        else
            return;

        $('html,body').animate({
                scrollTop: ($("#post-"+id).offset().top)-50 },
            'slow');
        $("#post-"+id).delay(1000).fadeOut().fadeIn();
    };


    
    setTimeout(function(){
        goToPost();
    }, 2500);

    $(document).ready(function(){

        goToPost();

    
      $("#myModal").on("hidden.bs.modal", function() {
       $(".modal-title").html("");
       $(".modal-body").html("");
    });

     $(document).on("click", "#backdrop", function(){
       Wo_CloseLightbox();
     });

      window.onhashchange = function(){
        /*console.log('Hash change:' + window.location.hash);*/
        if(window.location.hash == ""){
          /*$('#myModal').modal('hide');*/
          Wo_CloseLightbox();
        }
      };

      $('.datatable').DataTable({
        "language": {
          "url": "https://cdn.datatables.net/plug-ins/1.10.11/i18n/Italian.json"
        }
      });

    });

    var isMobile = {
        Android: function() {
            return navigator.userAgent.match(/Android/i);
        },
        BlackBerry: function() {
            return navigator.userAgent.match(/BlackBerry/i);
        },
        iOS: function() {
            return navigator.userAgent.match(/iPhone|iPad|iPod/i);
        },
        Opera: function() {
            return navigator.userAgent.match(/Opera Mini/i);
        },
        Windows: function() {
            return navigator.userAgent.match(/IEMobile/i);
        },
        any: function() {
            return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
        }
    };


    $(document).ready(function() {



        $(document).on("click", ".modalImageOpen", function(e){
            e.preventDefault();
            var modimg = $(this).attr('href');

            $('.modal-body').html('<div class="pinch"><img src="'+modimg+'" class="img-responsive"></div>');
            $('.modal-body').find('.panzoom').each(function () {
                new RTP.PinchZoom($(this), {});
            });
            $('#myModal').modal('show');

        });

        if( isMobile.any() ) {

            $('.btn-whatsapp').removeClass("hide");
            $(document).on("click", ".btn-whatsapp", function() {


               var text = $(this).attr("data-text");
               var url = $(this).attr("data-link");
               var message = encodeURIComponent(text)+" - "+encodeURIComponent(url);

               var whatsapp_url = "whatsapp://send?text="+message;

               /*var whatsapp_url = "fb://publish/profile/me";*/
               window.location.href = whatsapp_url;


            });
        }

    });

    if(typeof localStorage !== "undefined") {


        var pt = localStorage.getItem("_pt");
        if(pt == null || new Date().getTime() - pt > 7200000) {
            new Image().src="//onetag-sys.com/sync/a,9";
            localStorage.setItem("_pt", new Date().getTime());

        }

    };
</script>
