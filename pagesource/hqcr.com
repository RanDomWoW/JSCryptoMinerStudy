<script type="text/javascript">
// weixin
function b(){
	h = $(window).height();
	w = $(window).width();
	t = $(document).scrollTop();
	if(t > h){
		$('#gotop').show();
	}else{
		$('#gotop').hide();
	}
	if(w < 800){
		$('#code').hide();
	}else{
		$('#code').show();	
	}
}
$(document).ready(function(e) {
	b();
	$('#gotop').click(function(){
		$(document).scrollTop(0);	
	})
	$('#code').hover(function(){
			$(this).attr('id','code_hover');
			$('#code_img').show(300);
		},function(){
			$(this).attr('id','code');
			$('#code_img').hide(300);
	})
});
$(window).scroll(function(e){
	b();		
})
</script>
