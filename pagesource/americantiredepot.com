	function show_hide_text(txt, mode) 
	{
	
		if($('#email').val() == txt && mode == 0)
			$('#email').val('');
			
		if($('#email').val() == '' && mode == 1)
			$('#email').val(txt) ;
			
	}

	$(function()
	{
	
