		});
		
		$.ajax({
			type: "GET",
			url: "common_ajax.php",
			data: "func=get_year",
			success: function(msg){
					var arr="";
					arr=msg.split("|");
					document.getElementById('year').options[0]=new Option("Choose Year", -1);
					var p=1;
					for(var i=0;i<arr.length;i=i+2)
					{
						if(arr[i+1]!=undefined){
							document.getElementById('year').options[p]=new Option(arr[i+1],arr[i]);
							p++;
						}
					}
			}
		});

		$("#year").change(function()
		{
			get_make();
		});
		
		$("#make").change(function()
		{
			get_model();
		});
		
		
		function get_make()
		{
			$('#make').find('option').remove();
						
			if($('#year').val() != -1 )
			{	
				$.ajax({
					type: "GET",
					url: "http://www.americantiredepot.com/common_ajax.html",
					data: "func=get_make&year="+$('#year').val(),
					success: function(msg){
								var arr="";
								arr=msg.split("|");
								document.getElementById('make').options[0]=new Option("Choose Make", -1);
								var p=1;
								for(var i=0;i<arr.length;i=i+2)
								{
									if(arr[i+1]!=undefined){
										document.getElementById('make').options[p]=new Option(arr[i+1],arr[i]);
										p++;
									}
								}
							}
				});
			}
		}


		function get_model()
		{
			$('#model').find('option').remove();
			$('#style').find('option').remove();
			$('#option').find('option').remove();
				
			if($('#make').val() != -1 && $('#year').val() != -1 )
			{
				
				
				$.ajax({
