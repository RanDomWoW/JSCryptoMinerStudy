		sweetAlert("Error .... ", "Please Select the State", "error");
		return null ; 
	}
	
	if($.trim($("#cont-city").val()) == '-1')
	{
		sweetAlert("Error .... ", "Please Select the City", "error");
		return null ; 
	}
	
	if($.trim($("#cont-comments").val()) == '')
	{
		sweetAlert("Error .... ", "Please enter your Enquiry", "error");
		return null ; 
	}
	
	if($.trim($("#g-recaptcha-response").val()) == '')
	{
		sweetAlert("Error .... ", "Please enter the Verification Code", "error");
		return null ; 
	}
	
			
	$.post('http://www.americantiredepot.com/contact-send.html', $("#cont-revfrm").serialize(), function(data) {
					if(data == 1)
					{
						swal("Thank you for your Enquiry!", "Our team will contact you as soon as possible!", "success"); 
						$('#fixed-contact').hide(); $('#fixedbutton').show();
						$("#cont-name").val(''); 
						$("#cont-email").val(''); 
						$("#cont-phone").val(''); 
						$("#cont-zip").val(''); 
						$("#cont-comments").val(''); 
						$("#recaptcha_response_field").val(''); 
					}
					else if(data == -1){
						sweetAlert("Error .... ", "Verification code is wrong..", "error");
					}
					else {
						sweetAlert("Error .... ", "Something went wrong. Please try again later.", "error");
					}
				});
	
}

function validateEmail(sEmail) {
    var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
    if (filter.test(sEmail)) {
        return true;
    }
    else {
        return false;
    }
}
