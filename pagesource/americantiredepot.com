					data: "func=get_model&make="+$('#make').val()+"&year="+$('#year').val(),
					success: function(msg){
								var arr="";
								arr=msg.split("|");
								document.getElementById('model').options[0]=new Option("Choose Model", -1);
								var p=1;
								for(var i=0;i<arr.length;i=i+2)
								{
									if(arr[i+1]!=undefined){
										document.getElementById('model').options[p]=new Option(arr[i+1],arr[i]);
										p++;
									}
								}
							}
				});
			}
		
		}
		
		
		$("#model").change(function()
		{
			$('#style').find('option').remove();
			$('#option').find('option').remove();
			
			if($('#make').val() != -1 && $('#year').val() != -1 && $('#model').val() != -1)
			{
				$.ajax({
