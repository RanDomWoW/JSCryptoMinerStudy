<script type="text/javascript">
var gxheader = jQuery("#gx-menu");
var distance = 50;

jQuery(window).scroll(function() {
	if (jQuery(window).scrollTop() > (distance)) {
		jQuery(gxheader).addClass("small-nav");
	} else if (jQuery(window).scrollTop() <= distance) {
		jQuery(gxheader).removeClass("small-nav");
	}
});

var cookie_name_sort = "gxhk_home";
var cookie_name_column = "gxhk_columns";
var cookie_expire = 365;

function restoreOrder() {
	var list = jQuery(".global-sort");
	if (list == null) return
 
	var cookie = jQuery.cookie(cookie_name_sort);
	if (!cookie) return;
 
	var stored_columns = cookie.split("|");
	for(var i=0; i<stored_columns.length; i++) {
		stored_columns[i] = stored_columns[i].split(",");
	}

	var current_columns = [];
	jQuery(".global-sort").each(function() {
		current_columns.push(jQuery(this).sortable("toArray"));
	});
	
	var rebuild = {};
	for (var i=0; i<current_columns.length; i++) {
		for(var j=0; j<current_columns[i].length; j++) {
			rebuild[current_columns[i][j]] = current_columns[i][j];
		}
	}

	for (var i=0; i<stored_columns.length; i++) {
		var stored_blocks = stored_columns[i];

		for(var j=0; j<stored_blocks.length; j++) {
			var stored_block = stored_blocks[j];

			if (stored_block in rebuild) {
				var current_block = rebuild[stored_block];
				var current_block_object = jQuery("div.ui-sortable").children("#" + current_block);
				var saved_block_object = jQuery("div.ui-sortable").children("#" + stored_block);

				current_block_object.remove();

				jQuery("div.ui-sortable:eq(" + i + ")").filter(":first").append(saved_block_object);
			}
		}
	}
}

jQuery(document).ready(function() {
	jQuery("#gx-loginmenu").load("/forum/mylogin.php");

	var topHeight = jQuery("#gx-menu").height();

	jQuery("#gx-menu").headroom({
		offset : topHeight,
		tolerance : 10
	});

	jQuery('#placenewadmenu').on('click', function() {
		ga('send', 'event', 'newad', 'placeadbegin', 'gxmenu');
	});

	var sorttop = jQuery(".t3-navhelper").height() + jQuery("#gx-menu").height();

	jQuery("#global-quick-post-clsf").on('changed.bs.select', function(e) {
		window.location.href = jQuery("option:selected", "#global-quick-post-clsf").val();
	});
	jQuery("#global-quick-post-frm").on('changed.bs.select', function(e) {
		window.location.href = jQuery("option:selected", "#global-quick-post-frm").val();
	});

	if(jQuery(window).width() < 400) {
		var moveHandle = ".fa-move";
		jQuery("div.panel-heading").removeClass('global-move');
		jQuery("h3.module-title > .fa-move").addClass('global-move');
		jQuery(".global-move").css({
			'margin':'-8px',
			'padding':'8px'
		});
	} else {
		var moveHandle = ".global-move";
	}

	jQuery(".global-sort").sortable({
		connectWith: ".global-sort",
		handle: moveHandle,
		items: ".global-pane",
		placeholder: "global-sort-placeholder global-pane",
		revert: 200,
		cursor: "move",
		start: function(e, ui) {
			ui.item.addClass("global-dragging");
			if(jQuery(window).width() < 989) {
				jQuery("div.global-sort.ui-sortable").css('min-height', 40);
			} else {
				jQuery("div.global-sort.ui-sortable").css('min-height', 600);
			}
			ui.placeholder.height(40);
			jQuery(".global-pane").addClass("global-shrink");
			if (jQuery(window).scrollTop() > sorttop) {
				jQuery("html, body").animate({ scrollTop: sorttop }, 300);
			}
			ui.placeholder.addClass("col-lg-12 col-md-12 col-sm-12 col-xs-12");
			ga('send', 'event', 'Homepage', 'modules_sort');
		},
		stop: function(event, ui) {
			ui.item.removeClass("global-dragging");
			jQuery("div.global-sort.ui-sortable").css('min-height','0');
			jQuery(".global-pane").removeClass("global-shrink");
			var sorting = [];
			jQuery(".global-sort").each(function() {
				sorting.push(jQuery(this).sortable("toArray"));
			});

			cookie_string_column = sorting.join("|");

			if (jQuery.cookie(cookie_name_sort) != cookie_string_column) {
				jQuery.cookie(cookie_name_sort, cookie_string_column, { 
					expires: cookie_expire,
					path: "/"
				});
			}
		}
	}).bind("mousedown", function(){
		jQuery(".global-move").addClass("grabbing");
	}).bind("mouseup", function(){
		jQuery(".global-move").removeClass("grabbing");
	});

	if (jQuery.cookie(cookie_name_column) == null || jQuery.cookie(cookie_name_column) != jQuery(".global-sort").length) {
		jQuery.cookie(cookie_name_sort, null, {
			expires: cookie_expire * -1,
			path: "/"
		});
		jQuery.cookie(cookie_name_column, jQuery(".global-sort").length, { 
			expires: cookie_expire,
			path: "/"
		});
	}

	restoreOrder();

	jQuery('.global-reset').click(function(e){
		e.preventDefault();
		jQuery.cookie("gxhk_home", null, { path: '/' });
		window.setTimeout('location.reload()', 500);
	});

	jQuery(".gxttip").tooltip({container: 'body'});
});

jQuery.fn.scrollTo = function(target){
	return this.click(function(e){
		e.preventDefault();
		jQuery('html, body').animate({ scrollTop: jQuery(target).offset().top }, 350);
	});
};

jQuery('.gx-top').scrollTo('#top');
jQuery('.hkforums').scrollTo('#hkforums');
jQuery('.hkclsf').scrollTo('#hkclsf');
jQuery('.hkjobs').scrollTo('#hkjobs');
jQuery('.hkresources').scrollTo('#hkresources');
jQuery('.hkapartments').scrollTo('#hkcontent');
jQuery('.hkevents').scrollTo('#hkevents');
