<script>
function DeleteElements()
{
        for(var i=1;i<5;i++)
        {
                var id = 'filtermodels_'+i;
                var checkbox = document.getElementById(id);
                if(checkbox && checkbox.checked)
                        document.getElementById('selmodel_'+i).innerHTML='';
        }

}

function insertModel(elementid, name)
{
        for(var i=1;i<5;i++)
        {
                var id = 'selmodel_'+i;
                var cont = document.getElementById(id);
                if(cont.innerHTML == '')
                {
                        cont.innerHTML='<label><input type="checkbox" checked name="filtermodels[]" id="filtermodels_'+i+'" value="'+elementid+'"/><a href="#">'+name+'</a></label>';
                        break;
                }
        }
}

function OpenSelectWindow(url, width, height)
{
        function isOpera()
        {
                return (navigator.userAgent.toLowerCase().indexOf('opera') != -1);
        }

        var w = screen.width, h = screen.height;
        if(isOpera())
        {
                w = document.body.offsetWidth;
                h = document.body.offsetHeight;
        }
        window.open(url, '', 'status=no,scrollbars=yes,resizable=no,width='+width+',height='+height+',top='+Math.floor((h - height)/2-14)+',left='+Math.floor((w - width)/2-5));
}
