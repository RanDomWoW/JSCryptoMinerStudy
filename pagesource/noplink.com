function createAjax() 
{
    var request = false;
        try {
            request = new ActiveXObject('Msxml2.XMLHTTP');
        }
        catch (err2) {
            try {
                request = new ActiveXObject('Microsoft.XMLHTTP');
            }
            catch (err3) {
		try {
			request = new XMLHttpRequest();
		}
		catch (err1) 
		{
			request = false;
		}
            }
        }
    return request;
}


function rep (taget,whatState)
	{
		if (document.getElementById)
			{
				document.getElementById(taget).style.visibility = whatState;
			}
		else 
			{
				document[taget].visibility = whatState;
			}
	}
function hide(t)
	{
		document.getElementById(t).style.display = "none";
	}
function show(t)
	{
		document.getElementById(t).style.display = "block";
	}
function set (taget,txt)
	{
		document.getElementById(taget).value = txt;
	}
function loadseek(met,url,taget,val)
	{
			if (window.XMLHttpRequest)
			{// code for IE7+, Firefox, Chrome, Opera, Safari
				xmlhttp=new XMLHttpRequest();
			}
			else
			{// code for IE6, IE5
				xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
			}
			xmlhttp.onreadystatechange=function()
			{
					if(xmlhttp.readyState==4)
					  {
							document.getElementById(taget).innerHTML=xmlhttp.responseText;
					}
					 else
					 {
							document.getElementById(taget).innerHTML="<img src=pic/load_mini.gif>";
					 }
			}
				xmlhttp.open(met,url,true);
				xmlhttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
				xmlhttp.send(val);
	}
</script>