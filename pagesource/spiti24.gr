    <script>
        var autoSuggestUrl = 'https://www.spiti24.gr/geo/autosuggest';

        //todo function below can be moved to homepage .js file
        $(document).ready(function () {

            //prefill autosuggest by cookie
            var selectedGeos = eval(phpjs.geographies);
            var areaSelect = $('#area');
            $.each(selectedGeos, function (index, item) {
                areaSelect.append($('<option>', {
                    value: item.id,
                    text: item.fullName,
                    title: item.n,
                    selected: true
                }));
            });

            //search form submit handler
            $('#propertySearch').submit(function () {
                var msg = 'Παρακαλούμε επιλέξτε περιοχή για την αναζήτηση!';
                if ($("#area").select2('val') === null || $("#area").select2('val').length == 0) {
                    swal({
                        allowOutsideClick: true,
                        title: '',
                        text: msg,
                        confirmButtonColor: '#FF545C',
                        type: "error",
                        confirmButtonText: "OK",
                        timer: 4000,
//                        html: true
                    });
                    return false;
                } else {
                    // remove the . from priceLow
                    if ($('#propertySearch').find('input[name="priceLow"]').val() != null) {
                        var priceLow = $('#propertySearch').find('input[name="priceLow"]').val();
                        var newPriceLow = priceLow.replace(/\./g, '');
                        $('#propertySearch').find('input[name="priceLow"]').val(newPriceLow);
                    }
                    // remove the . from priceHigh
                    if ($('#propertySearch').find('input[name="priceHigh"]').val() != null) {
                        var priceHigh = $('#propertySearch').find('input[name="priceHigh"]').val();
                        var newPriceHigh = priceHigh.replace(/\./g, '');
                        $('#propertySearch').find('input[name="priceHigh"]').val(newPriceHigh);
                    }
                    return true;
                }
            });

            //on country change we clear autosuggest
            $('#country').change(function () {
                $('#area').val(null).empty().trigger('change');
            });

            // Populate Fake Input on Mobile
            $('#area,#listingType').change(function () {
                updateFakeAreas();
            });

            function updateFakeAreas() {
                var val = '';
                $("select#area option:selected").each(function () {
                    val += $(this).attr('title') + ', ';
                });
                val = val.slice(0, -2);
                if(val != '') {
                    $('.js-fake-area').text(val);
                }

                if($('#listingType').val() != null){
                    $('.js-fake-area2').text($('select#listingType option:selected').text());
                }

            }

            updateFakeAreas();

        });
