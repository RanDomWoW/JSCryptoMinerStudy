<script type="text/javascript">
/* Search */
$('#search input[name=\'search\']').parent().find('button').on('click', function() {
    url = $('base').attr('href') + 'search';
    var search = $('.header-search input[name=\'search\']').val();

    if (search) {
        url += '?search=' + encodeURIComponent(search);
    }

    var search = $('.header-search input[name=\'search\']').prop('value');

    if (search) {
        location = url;
    }
});

$('#search input[name=\'search\']').on('keydown', function(e) {
    if (e.keyCode == 13) {
        $('.header-search input[name=\'search\']').parent().find('button').trigger('click');
    }
});

$(document).ready(function() {
	//$('#search input[name="filter_name"]').attr("x-webkit-speech", "x-webkit-speech")
	$('#search input[name="search"]').autocomplete({
                minLength: 2,
		appendTo: $("#searchbox"),
		source: function(request, response) {
                        var $this = $(this);
                        var $element = $(this.element);
                        var jqXHR = $element.data('jqXHR');
                        if(jqXHR)
                            jqXHR.abort();                    
                    
                     $element.data('jqXHR', $.ajax({
				url: 'index.php?route=common/search/suggest',
				dataType: 'json',
				data: {
					keyword: request.term
				},
				success: function(json) {
					response($.map(json, function(item) {
                                            item.value = item.href;
						return item;
					}));
				},
                                complete: function() {
                                    $this.removeData('jqXHR');
                                }                                
			}));
		},
                messages: {
                    noResults: '',
                    results: function() {}
                },                
		select: function(event, ui) {
			if (ui.item.value == "") {
				return false;
			} else {
				location.href = ui.item.value;
				return false;
			}
		},
		open: function() {
                    var dw = $(document).width();
                    var ww = $(window).width();
                    if(dw > 662){
                        $('.ui-widget-content').css('width', '99%'); 
                    }                    
                    
                    if(dw > 462 && dw <= 662){
                        $('.ui-widget-content').css('width', '90.5%'); 
                    }

                    if(dw <= 462){
                        $('.ui-widget-content').css({'width':'82%', 'left':'12px'}); 
                    }                    
		},
		close: function() {
			return false;
		},
		focus: function(event, ui) {
			return false;
		}
	}).data("ui-autocomplete")._renderItem = function(ul, item) {                            
                var item_html = '<div class="' + item.css_class + '">';	
                if(item.image) {
                    item_html += '<span class="image">';
                    item_html += '<img class="img-thumbnail" src="' + item.image + '" alt="' + item.name + '" title="' + item.name + '" />';
                    item_html += '</span>';
                }

                if(item.href){
                    item_html += '<span class="name"><a href="' + item.href + '">' + item.name + '</a></span>';
                } else {
                    item_html += '<span class="name">' + item.name + '</span>';
                }    

                  if (item.special) {
                        item_html += '<span class="price-old">' + item.price + '</span><span class="price-new">' + item.special + '</span>';
                  } else if(item.price) {
                        item_html += '<span class="price">' + item.price + '</span>';
                  }	

                item_html += '</div>';
        
        return $("<li></li>")
                .data("item.autocomplete", item)
                .append(item_html)
                .appendTo(ul);
    };
});
