<aside class="basket-panel flapout flapout--custom-hide ng-hide"
dd-class-if-flapout="basket-panel"
dd-class-if-flapout-classname="basket-panel--open"
dd-class-if-flapout-delayed-hide="300"
dd-basket
ng-cloak>

    <dd-preloader show="basket.updating" cover></dd-preloader>

    <div class="basket-panel__header" dd-basket-count>
        <strong class="headline">{{basketCount.amount}} Artikel im Warenkorb</strong>
        <button type="button"
        class="basket-panel__close"
        title="Warenkorb ausblenden"
        dd-close-flapouts
        dd-close-flapouts-focus=".js-basket-panel-trigger">
            <i class="fa fa-times" aria-hidden="true"></i>
            <span class="show-for-sr">Warenkorb ausblenden</span>
        </button>
    </div>

    <div class="basket-panel__body" ng-class="{'basket-panel__body--has-alert': basket.missingAmount > 0, 'basket-panel__body--has-discount': basket.basket.price.discount !== 0}">

        <div class="basket-panel__warnings" ng-if="basket.warnings.length">
            <div class="alert alert--warning basket-panel__warning" ng-repeat="warning in basket.warnings">
                <strong><span class="fa fa-exclamation-circle"></span></strong>
                {{warning}}
            </div>
        </div>

        <ul class="basket-panel__list list--unstyled">
            <li class="basket-panel__item basket-panel__item--unavailable" ng-repeat="item in basket.unavailable track by $index">
                <img class="basket-panel__item__image"
                dd-lazy-load-image="{{item.image | productImage:'basket'}}"
                dd-lazy-load-intro="scale-in"
                dd-fallback-product-image="'basket'"
                alt="Produktbild - {{item.name}}">
                <div class="basket-panel__item__description">
                    <a ng-href="/shop/{{item.sku}}/{{item.slug}}" title="{{item.product.name}}">
                        <span>{{item.name}}</span>
                    </a>
                    <button type="button"
                    class="basket-panel__item__remove js-toggle-flapout-ignore"
                    title="Aus Warenkorb entfernen"
                    ng-click="basket.removeArticle(item, '.basket-panel__close')">
                        <i class="fa fa-times-circle" aria-hidden="true"></i>
                        <span class="show-for-sr">Aus Warenkorb entfernen</span>
                    </button>
                </div>
            </li>
            <li class="basket-panel__item" ng-repeat="item in basket.noPredeliveryProducts track by $index">
                <img class="basket-panel__item__image"
                dd-lazy-load-image="{{item.product.image | productImage:'basket'}}"
                dd-lazy-load-intro="scale-in"
                dd-fallback-product-image="'basket'"
                alt="Produktbild - {{item.product.name}}">
                <div class="basket-panel__item__description">
                    <a ng-href="/shop/{{item.product.sku}}/{{item.product.slug}}" title="{{item.product.name}}">
                        <span>
                            {{item.product.name}}
                        </span>
                    </a>
                    <button type="button"
                    class="basket-panel__item__remove js-toggle-flapout-ignore"
                    title="Aus Warenkorb entfernen"
                    ng-click="basket.removeArticle(item, '.basket-panel__close')">
                        <i class="fa fa-times-circle" aria-hidden="true"></i>
                        <span class="show-for-sr">Aus Warenkorb entfernen</span>
                    </button>

                    <div class="basket-panel__item__quantity">
                        <dd-quantity class="quantity"
                        ng-if="item.product"
                        amount="item.quantity"
                        minimum="item.product.minQuantity"
                        maximum="item.product.maxQuantity"
                        step-size="item.product.quantityStepSize"
                        unit="item.product.unit"
                        lock-max="basket.willExceedMaxPrice(item.price)"
                        event-data="item"></dd-quantity>
                    </div>

                    <span class="basket-panel__item__price">{{item.price | currency}}</span>
                </div>
            </li>
        </ul>

        <div class="basket-panel__predelivery" ng-repeat-start="(time, products) in basket.predeliveryProducts">
            <a href class="basket-panel__header basket-panel__header--predelivery js-basket-predelivery-tooltip--{{time}}">
                <strong>
                    <svg ng-if="basket.isValidPredeliveryTime(time)"
                    class="icon icon--green icon--medium sprite--predelivery-time_{{time}}"></svg>
                    <svg ng-if="!basket.isValidPredeliveryTime(time)"
                    class="icon icon--green icon--small sprite--predelivery-time"></svg>
                    <span>Produkte mit {{time}} h Vorbestellzeit</span>
                </strong>
                <span class="show-for-sr">Diese Produkte unterliegen einer Vorbestellzeit und beeinflussen die Lieferzeiten.</span>
            </a>
            <div class="tooltip tooltip--predelivery"
            dd-tooltip=".js-basket-predelivery-tooltip--{{time}}">
                <svg class="icon icon--green icon--small sprite--predelivery-time" aria-hidden="true"></svg>
                <span>Diese Produkte unterliegen einer Vorbestellzeit und beeinflussen die Lieferzeiten.</span>
            </div>
        </div>

        <ul class="basket-panel__list basket-panel__list--predelivery list--unstyled"
        ng-repeat-end>
            <li class="basket-panel__item ng-scope" ng-repeat="item in products track by $index">
                <img class="basket-panel__item__image"
                dd-lazy-load-image="{{item.product.image | productImage:'basket'}}"
                dd-lazy-load-intro="scale-in"
                dd-fallback-product-image="'basket'"
                alt="{{item.product.name}}">
                <div class="basket-panel__item__description">
                    <a ng-href="/shop/{{item.product.sku}}/{{item.product.slug}}" title="{{item.product.name}}">
                        <span ng-bind-html="item.product.name| trustHtml"></span>
                    </a>
                    <button type="button"
                    class="basket-panel__item__remove"
                    title="Aus Warenkorb entfernen"
                    ng-click="basket.removeArticle(item, '.basket-panel__close');">
                        <i class="fa fa-times-circle" aria-hidden="true"></i>
                        <span class="show-for-sr">Aus Warenkorb entfernen</span>
                    </button>

                    <div class="basket-panel__item__quantity">
                        <dd-quantity class="quantity"
                        ng-if="item.product"
                        amount="item.quantity"
                        minimum="item.product.minQuantity"
                        maximum="item.product.maxQuantity"
                        step-size="item.product.quantityStepSize"
                        unit="item.product.unit"
                        lock-max="basket.willExceedMaxPrice(item.price)"
                        event-data="item"></dd-quantity>
                    </div>
                    <span class="basket-panel__item__price">{{item.price | currency}}</span>
                </div>
            </li>
        </ul>

        <div class="basket-panel__info">
            <strong>Wichtiger Hinweis:</strong> Eventuelle weitere Rabatte werden an der Kasse abgezogen.
            <br />
            <br />
            Die Lieferkosten errechnen sich aus dem gewählten Zeitfenster im Checkout.
        </div>
    </div>

    <div class="basket-panel__footer">
        <div class="basket-panel__minalert alert alert--warning alert--border no-margin"
        ng-if="basket.missingAmount > 0"
        role="alert">
            <strong>Mindestbestellwert:</strong>
            Es fehlen {{basket.missingAmount | currency}} zum minimalen Bestellwert.
        </div>
        <div class="basket-panel__footer__content">
            <span>Warenwert:</span>
            <span class="basket-panel__footer__price">{{basket.basket.price.total | currency}}</span>
            <div class="basket-panel__footer__discount" ng-if="basket.hasDiscount && !basket.isEmpty">
                <span>- Rabatt: </span>
                <span class="basket-panel__footer__price--discount">{{basket.basket.price.discount | currency}}</span>
            </div>
            <div class="basket-panel__footer__discount" ng-if="basket.hasCoupons && !basket.isEmpty">
                <span>- Gutscheine:</span>
                <span class="basket-panel__footer__price--discount">{{basket.basket.price.coupons | currency}}</span>
            </div>
            <div class="basket-panel__footer__buttons">
                <button type="button" class="button button--big button--secondary"
                ng-disabled="basket.isEmpty"
                ng-click="basket.showClearBasketDialog()">
                    <span class="show-for-sr">Warenkorb leeren</span>
                    Leeren
                    <i class="fa fa-trash" aria-hidden="true"></i>
                </button>
                <button type="button" class="button button--big button--primary"
                ng-disabled="basket.isEmpty"
                ng-click="basket.goToCart()">
                    <span class="show-for-sr">Zur Kassa</span>
                    Kassa
                    <i class="fa fa-angle-right" aria-hidden="true"></i>
                </button>
            </div>
            <div class="basket-notice" dd-basket-notification ng-show="basketNotification.visible && basketNotification.message" ng-cloak>
                <div class="tooltip tooltip--on-top"
                ng-class="{
                    'tooltip--info': basketNotification.type === 'info',
                    'tooltip--error': basketNotification.type === 'error',
                    'tooltip--success': basketNotification.type === 'success',
                    'tooltip--warning': basketNotification.type === 'warning'
                 }">

                    <span ng-bind-html="basketNotification.message | trustHtml"></span>
                </div>
            </div>
        </div>
    </div>

</aside>


    





