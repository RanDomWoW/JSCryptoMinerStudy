     dd-if-flapout-opened="login"
    >
    <div ng-if="ifFlapoutOpened.flapoutOpened" dd-tabs="login-form">
        

<div dd-tab-content="login-form">
    <form class="clearfix" method="POST" name="loginForm"
    dd-login-flapout dd-login-flapout-last-email="" ng-submit="loginFlapout.submit()" novalidate>
        <dd-preloader show="loginFlapout.loading"></dd-preloader>
        <div ng-show="!loginFlapout.loading">
            <input type="hidden" name="csrfToken" value="3a6265126e6412c595e6ccaa1eb16d75f9ce5fca-1521588723704-d3e6f9d9f6b74d7ae72e41fa"/>
            <strong class="headline" ng-if="!loginFlapout.functionName">Loggen Sie sich hier ein:</strong>
            <div class="headline" ng-if="loginFlapout.functionName">
                <strong>Loggen Sie sich hier ein um</strong><br/>
                <strong class="function-name" ng-bind="loginFlapout.functionName"></strong> <strong>zu verwenden:</strong>
            </div>
            
            
            <div ng-if="loginFlapout.serverError" class="alert alert--error">
                <strong><span class="fa fa-exclamation-triangle" aria-hidden="true"></span>Achtung
                    :</strong>
                Es ist ein unerwarteter Fehler aufgetreten
            </div>
            <div ng-if="loginFlapout.userNotFound" class="alert alert--error">
                <strong><span class="fa fa-exclamation-triangle" aria-hidden="true"></span>Achtung
                    :</strong>
                Ihre Benutzerdaten wurden nicht gefunden. Haben Sie sich eventuell vertippt?
            </div>

            <div class="row collapse">
                <div class="small-2 columns">
                    <span class="prefix fa fa-envelope" aria-hidden="true"></span>
                    <label for="loginform-email" class="show-for-sr">E-Mail</label>
                </div>
                <div class="small-10 columns">
                    <input id="loginform-email" type="email" name="email" required="required" ng-required="true" ng-model="loginFlapout.email"
                    ng-pattern="/^(?!.*\{\{.*\}\})[a-zA-Z0-9.!#$%&amp;’*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)+$/"
                    placeholder="E-Mail Adresse" />
                </div>
                <div class="small-12 columns">
                    <small class="error" ng-if="(loginFlapout.submitFailed || loginForm.email.$touched) && loginForm.email.$error.required" >
                    Bitte geben Sie Ihre E-Mail Adresse an.
                    </small>
                    <small class="error" ng-if="(loginFlapout.submitFailed || loginForm.email.$touched) && loginForm.email.$error.pattern" >
                    Die angegebene E-Mail Adresse ist ungültig.
                    </small>
                </div>
            </div>

            <div class="row collapse">
                <div class="small-2 columns">
                    <span class="prefix fa fa-lock" aria-hidden="true"></span>
                    <label for="loginform-password" class="show-for-sr">Passwort:</label>
                </div>
                <div class="small-10 columns">
                    <input id="loginform-password" type="password" name="password" ng-model="loginFlapout.password"
                    placeholder="Passwort"
                    maxlength="50"
                    required="required" ng-required="true" />
                </div>
                <div class="small-12 columns">
                    <small class="error" ng-if="(loginFlapout.submitFailed || loginForm.password.$touched) && loginForm.password.$error.required" >
                    Bitte geben Sie Ihr Passwort an.
                    </small>
                </div>
            </div>

            <button type="submit" class="button button--primary flapout--login__submit">
                <span>Login</span>
            </button>
        </div>
        <div class="flapout__links flapout__links--login margin-top-l" ng-show="!loginFlapout.loading">
            <ul class="list--unstyled">
                <li>
                    <i class="fa fa-angle-right" aria-hidden="true"></i>
                    <a href="#" dd-tab="login-forgot-password">Passwort vergessen</a>
                </li>
            </ul>
            

 
    
<ul class="list--unstyled">
    
        <li>
        
            <i class="fa fa-angle-right" aria-hidden="true"></i>
            <a href="/shop/registration">Registrieren</a>
        
        </li>
    
</ul>

 


        </div>
    </form>
</div>

        

