			if (typeof window.Promise != "function") {
				fontsLoaded();
				return;
			}

			if (!$("html").hasClass("fonts-loaded")) {
				var fontObservers = [];
				var loadFonts = {
					"Open Sans": {
						normal: [300, 400, 600, 700, 900],
						italic: [300, 400, 600, 700]
					},
					"Open Sans Condensed": {
						normal: [300, 700],
						italic: [300]
					}
				};

				$.each(loadFonts, function(name, weights) {
					$.each(weights, function(style, weightArray) {
						$.each(weightArray, function(index, weight) {
							fontObservers.push(new FontFaceObserver(name, {
								weight: weight,
								style: style
							}).check());
						});
					});
				});

				window.Promise.all(fontObservers).then(function() {
					// All fonts downloaded:
					fontsLoaded();
				}, function () {
					// Something went and died:
					fontsLoaded();
				});
			}
		});
