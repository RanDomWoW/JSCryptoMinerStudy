        function newsletterSignUp(){
            var email = $('#newsletter_email').val();
            if(validateNewsletterEmail(email)){
                $.ajax({
                    url : '/admin/newsletter_signup.php',
                    type : 'POST',
                    data : { 
                        email:email, 
