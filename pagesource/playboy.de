        $vItem.data('shown', 1).show();
        $vGroup.data('visibleItems', $vGroup.data('visibleItems') + 1);
        currentlyVisibleItems++;
        return false;
      }
    }
  });

  // Are enough items visible?
  if (currentlyVisibleItems < num) {
    determineVisibleItemsRandom($vGroup, $vItems, num);
  }
}
</script>


  </div>

</body>
</html>