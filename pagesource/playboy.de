function determineVisibleItemsChain($vGroup, $vItems, num) {
  $vItems.each(function() {
    var $vItem = jQuery(this);

    if ($vItem.data('visibility')) {
      if (num >= 0)  {
        $vItem.show();
        $vGroup.data('visibleItems', $vGroup.data('visibleItems') + 1);
        num--;
      } else {
        return false;
      }
    }
  });
}

function determineVisibleItemsRandom($vGroup, $vItems, num) {
  var possibleVisibleItems = 0;
  var currentlyVisibleItems = 0;
  var weightAll = 0;

