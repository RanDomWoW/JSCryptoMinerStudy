var adLog = function(message) {
  if ((typeof console !== 'undefined') && (typeof console.debug === 'function')) {
    console.debug(message);
  }
};

//create a method to deal with the adReceived event (first event)
var onAdReceived = function (e) {
  console.debug('onAdReceived', e.detail);
  /*
  if ((typeof Playboy !== 'undefined') && (typeof ads !== 'undefined') && ads[e.detail.container]) {
    Playboy.gaEvent('advertising', ads[e.detail.container], 'received', 1);
  }
  */

  // Reset body
  var body = document.getElementsByTagName('body')[0];
  body.clasname = body.className.replace(/adblocker\-active/g, '').replace(/  /g, ' ');

  // Reset classes
  var classes = null;
  var element = document.getElementById(e.detail.container);
  if (element !== null) {
    classes = element.parentNode.className.replace(/ is\-empty/g, '');
  }

  if (e.detail.isEmpty) {
    // no ad code was loaded so the slot is empty, do something
    classes += ' is-empty';
  } else {
    // some ad code was loaded, do nothing
  }

  if ((element !== null) && (element.parentNode.className !== classes)) {
    element.parentNode.className = classes;
  }
};


//create a method to deal with the adRendered event (second event)
var onAdRendered = function (e) {
  console.debug('onAdRendered', e.detail);
  /*
  if ((typeof Playboy !== 'undefined') && (typeof ads !== 'undefined') && ads[e.detail.container]) {
    Playboy.gaEvent('advertising', ads[e.detail.container], 'rendered', 1);
  }
  */

  // Reset body
  var body = document.getElementsByTagName('body')[0];
  body.clasname = body.className.replace(/adblocker\-active/g, '').replace(/  /g, ' ');

  // Reset classes
  var classes = null;
  var element = document.getElementById(e.detail.container);
  if (element !== null) {
    classes = element.parentNode.className.replace(/ contains\-ad/g, '').replace(/ is\-filler/g, '');
  }

  if (e.detail.isFiller) {
    // the ad code that was loaded is a filler or blocker, do something
    classes += ' is-filler';
  } else {
    // a proper ad was loaded
    if (element !== null) {
      classes += ' contains-ad';
      body.dispatchEvent(new Event('sticky_kit:recalc'));
    }

    if (e.detail.container == 'forag-content1') {
      var elementHeader = document.getElementById('header');
      if (elementHeader !== null) {
        elementHeader.className = elementHeader.className.replace(/ with\-ad/g, '') + ' with-ad';
      }

      var elementMain = document.getElementById('main');
      if (elementMain !== null) {
        elementMain.className = elementMain.className.replace(/ with\-ad/g, '') + ' with-ad';
      }
    }
  }

  if ((element !== null) && (element.parentNode.className !== classes)) {
    element.parentNode.className = classes;
  }
};

//add event listeners for both events
