$('#hbnp_skip').click(function(){
sethbnpcookie();
$.magnificPopup.close();
});

$('#cpwd').keyup(function(){
var cpwd = $('#cpwd').val();
var pwd = $('#pwd').val();
if (pwd == cpwd){
	$('#match').html("<span class='hbnp-span-success'>Şifrə oyunları!</span>");
} else {
	$('#match').html("<span class='hbnp-span-error'>Şifrə uyğun deyil!</span>");
}

});


function sethbnpcookie(){
$.cookie('hbnp_one_time', 'subscrcoockie');
}

function autoclose(){
setTimeout(function(){
	$.magnificPopup.close();
}, 7000);
}


$(document).ready(function () {
setTimeout(function(){
if (($('#hb-nl-popup').length) && ($.cookie('hbnp_one_time') != 'subscrcoockie') && (1== 1 )){
	$.magnificPopup.open({
		items: {src: '#hb-nl-popup'},
		type: 'inline',
		
				
		removalDelay: 800,
		callbacks: {
			beforeOpen: function(){
				this.st.mainClass = 'mfp-3d-unfold';
			},
			beforeClose: function() {
			    sethbnpcookie();
			}
		}
	});
}
}, 15000);  
}); 

$('#hbnp_subscribe').click(function(){
	var email = $('#hbnp_email').val();
	var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
	  if((!emailReg.test(email)) || ($.trim(email) == '') ) {
	  	alert ('Invalid email');
	    return false;
	  }
	 $('#loadgif_hbnp').show();
	$.ajax({
		  type: 'post',
		  url: 'index.php?route=module/hbnp_core',
