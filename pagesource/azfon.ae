<script type="text/javascript">
	$(document).click(function(event) {
		if ($(event.target).closest("#search dropdown-menu").length) return;
		$('#search .dropdown-menu').css('display','none');
		event.stopPropagation();
	});
	function il_search() {
		send_search('header');
		//$('header input[name=\'search\']').parent().find('button').trigger('click');
	}
	function go_link(this_){
		location.href = $(this_).attr('href');
		return false;
	}
	function clear_search(){
		$('#search .dropdown-menu').css('display','none');
		$('#search .dropdown-menu li, #search .dropdown-menu .see-all').remove();
		//$('#search .dropdown-menu').append('<a href="javascript:void(0)" class="see-all">Heç bir nəticə axtar.</a>');
	}
	$('#search input[name=\'search\']').autocomplete({
		'source': function(request, response) {
			if(request.length >= 2){
				$.ajax({
					url: 'index.php?route=product/product/autocomplete&filter_name=' +  encodeURIComponent(request),
					dataType: 'json',
					success: function(json) {
						if(json['total'] > 0){
							$('#search .dropdown-menu').css('display','block');
							$('#search .dropdown-menu li, #search .dropdown-menu .see-all').remove();
							var index = 0;
							if(json['total'] > 0){
								for(index = 0;index<=json['total'];index++){
									if(json['products'][index] && json['products'][index].product_id != 'undefined'){
										html = '<li><a onclick="go_link($(this))" href="'+json['products'][index].href+'">';
										if(json['products'][index].image){
											html += '<img src="'+json['products'][index].image+'"/>';
										}
										if(json['products'][index].special){
											html += '&nbsp;'+json['products'][index].name+'&nbsp;<span class="price">'+json['products'][index].special+'</span></a></li>';
										}else{
											html += '&nbsp;'+json['products'][index].name+'&nbsp;<span class="price">'+json['products'][index].price+'</span></a></li>';
										}
										$('#search .dropdown-menu').append(html);
									}
								}
								if(json['total'] > 5){
									$('#search .dropdown-menu').append('<a onclick="il_search()" href="javascript:void(0)" class="see-all">Bütün məhsullar ('+json['total']+')</a>');
								}
							}
						}else{
							clear_search();
						}
						
						return false;
					}
				});
			}else{
				clear_search();
			}
		},
		'select': function(item) {
		}
	});
