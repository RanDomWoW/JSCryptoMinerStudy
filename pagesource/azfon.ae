                }
                
              }
                
            }
          }
          $('#modal_product .similar_table').html(similar_table);
          
          $('#modal_product .product_features').html(attribute_groups);
          $('#modal_product .product-more a').attr('href',json['product']['href']);

          var price_ = '';
