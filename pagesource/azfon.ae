        if(json['product']){
          $('#modal_product .modal_inner').attr('id','product-'+old_product_id);
          $('#modal_product .product-thumb').html('<a href="'+json['product']['href']+'"><img src="'+json['product']['image']+'" width="200" height="200" alt="'+json['product']['name']+'"></a>');
          $('#modal_product .product-name').html('<a href="'+json['product']['href']+'">'+json['product']['name']+'</a>');
          var attribute_groups = '';
          if(json['product']['attribute_groups']){
            var ia=0;
            for (i in json['product']['attribute_groups']) {
              for (j in json['product']['attribute_groups'][i]['attribute']) {
                if(ia<10){
                  attribute_groups += '<p><b>'+json['product']['attribute_groups'][i]['attribute'][j]['name']+':</b> '+json['product']['attribute_groups'][i]['attribute'][j]['text']+'</p>';
                }
                ia++;
              }
            }
          }

          var product_olours = '';
          if(json['product']['mas_products']){
            for (i in json['product']['mas_products']) {
              var selecte = '';
              var class_color = '';
              if(json['product']['mas_products'][i]['value'] == 'd60404'){
                class_color = ' black-color';
              }
              if(json['product']['mas_products'][i]['selected']){
                selecte = ' current';
              }
              product_olours +='<span onclick="color($(this));" old-product-id="'+old_product_id+'" data-product-id="'+json['product']['mas_products'][i]['product_id']+'" class="colour-circle'+selecte+class_color+'" style="border-color:#'+json['product']['mas_products'][i]['value']+';"><span class="circle-inner" style="background:#'+json['product']['mas_products'][i]['value']+';"></span></span>';
                
            }
          }
          $('#modal_product .product-colours').html(product_olours);

          var similar_table = '';
          if(json['product']['options_links']){
            for (i in json['product']['options_links']) {
              for (j in json['product']['options_links'][i]['product_option_link_value']) {
                var selecte = '';
                if(json['product']['options_links'][i]['product_option_link_value'][j]['selected']){
                  
