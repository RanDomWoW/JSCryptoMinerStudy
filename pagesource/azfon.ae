<script type="text/javascript">
  /* Modal */
  function change_popup(old_product_id,product_id){
    $.ajax({
      url: 'index.php?route=product/product/option_link',
      type: 'post',
      data: 'product_id='+product_id,
      dataType: 'json',
      beforeSend: function() {},
      complete: function() {      
        $('#modal_product').modal('show');
