            	price_ = json['product']['price'];
            }else{
            	price_ = '<span class="price-old">'+json['product']['price']+'</span>'+json['product']['special'];
            }

          $('#modal_product .product-price').html(price_);

          if(parseInt(json['product']['quantity']) > 0){
            $('#modal_product .product-actions').html('<button type="button" class="btn add-to-cart" onclick="cart.add('+product_id+');"><span>Səbətə əlavə et</span></button><button type="button" class="btn white-btn fast-btn cartoneclic" id="order_one_click_'+product_id+'" onclick="order_one_click('+product_id+');" data-toggle="modal" data-dismiss="modal"><span>Sürətli alış</span></button>');
