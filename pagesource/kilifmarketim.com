        function getOCwizardModal_smca() {
          $.magnificPopup.open({
            <!-- tLoading: '<img src="catalog/view/theme/default/stylesheet/ocdevwizard/smart_cart/loading.svg" alt="" />', -->
            items: ocdev_modules,
            showCloseBtn: false,
            mainClass: 'mfp-smca',
            gallery: {
              enabled: true
            },
            callbacks: {
              close: function() {
                $.ajax({
                  type: 'get',
                  url:  'index.php?route=ocdevwizard/ocdevwizard_setting/modules',
                  dataType: 'json',
                  success: function(json) {
                    ocdev_modules.length = 0;

                    if (json['smca_status'] == '1') {
                      ocdev_modules.push({
                        src:  'index.php?route=ocdevwizard/smart_cart',
                        type:'ajax'
                      });
                    }

                    if (json['smac_status'] == '1') {
                      if (json['smart_abandoned_cart'] == '1') {
                        ocdev_modules.push({
                          src:  'index.php?route=ocdevwizard/smart_abandoned_cart',
                          type:'ajax'
                        });
                      }
                    }
                  }
                });
              }
            }
          });
          $('.mfp-bg').css({
            'background': 'url(image/ocdevwizard/smart_cart/background/bg_8.png)',
            'opacity': '0.5'
          });
        }
      </script>
