<script id="card-1-template" type="text/x-handlebars-template">
        <div class='card card-{{cardNum}}'>
          <div class="details">
            <div class="cardplaybtn"></div>
            {{#if is_data}}
            <div class="dataonly" datacast="{{eventKey}}" publisher="{{publisher_id}}" sport="{{sport_id}}">
              <div class="left">
                <div class="score">\{{getScore('{{team1}}')}}</div>
                <div class="logo" style="background-color: \{{getPrimaryColor('{{team1}}')}}; background-image: url(\{{getTeamLogo('{{team1}}')}})">
                </div>
                <p class="name">\{{getTeamName('{{team1}}')}}</p>
              </div>
              <div class="right">
                <div class="score">\{{getScore('{{team2}}')}}</div>
                <div class="logo" style="background-color: \{{getPrimaryColor('{{team2}}')}}; background-image: url(\{{getTeamLogo('{{team2}}')}})">
                </div>
                <p class="name">\{{getTeamName('{{team2}}')}}</p>
              </div>  
            </div>
            {{else}}
            <img src="{{logo}}" alt="">
            {{/if}}
            <div class="cardContent">
              <span class="type">{{mediatype}}</span>
              <h3>{{header}}</h3>
              <h4>{{headline}}</h4>
              <span class="description">{{subheadline}}</span>
              <span class="date" id="foo">{{start_time}} {{#if city }}| {{city}}, {{state}}{{else}}| Location TBA{{/if}}</span>
            </div>
          </div>
          <ul class="actions {{eventid}}" data-eid="{{eventid}}">
            <li onclick="nfhs.nfhs_app.members.card_clickEvent('reminder', '{{type}}', '{{unity_id}}' );" class="reminder"><a>Watch Later</a></li>
            <li onclick="nfhs.nfhs_app.members.card_clickEvent('favorite', '{{type}}', '{{unity_id}}' );" class="favorite"><a>Favorite</a></li>
            <li class="share"><a>Share</a></li>  
            <div class="cardSocial" style="display: none;">
              <div class="sharethis-wrapper"></div>
               
            </div>
          </ul>    
        </div>
</script>
<script id="card-profile-template" type="text/x-handlebars-template">
