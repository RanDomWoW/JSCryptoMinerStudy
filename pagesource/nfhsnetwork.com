<script type="text/javascript">
yepnope([{
  load: [
    'https://sdk.amazonaws.com/js/aws-sdk-2.1.16.min.js',
'https://s3.amazonaws.com/assets.nfhsnetwork.com/libs/player/2.0.1/nfhs_player.js',
'https://js.maxmind.com/js/apis/geoip2/v2.1/geoip2.js ',
'https://ws.sharethis.com/button/buttons.js',
'https://cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.min.js',
'https://cdnjs.cloudflare.com/ajax/libs/jquery-ajaxtransport-xdomainrequest/1.0.2/jquery.xdomainrequest.min.js',
'https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js',
'https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/0.9.0/jquery.mask.min.js',
'/sites/all/themes/nfhs/js/jquery.customSelect.min.js',
'/sites/all/themes/nfhs/js/jquery.paging.min.js',
'/sites/all/themes/nfhs/js/nfhs.js',
'/sites/all/themes/nfhs/js/nfhs_app.js'  ],
  complete: function() {
    $ = jQuery;
    console.log("complete called");
    var env = Drupal.settings.nfhs_panels.config.environment;
    nfhs.utils.init(env);
    nfhs.search.init(env);
    nfhs.auth.init(env);
    nfhs.analytics.init(env);
    nfhs.analytics.segmentCalls();
    nfhs.mostPopular.init(env);
    nfhs.member.init(env);
    nfhs.eventTemplater.init(env);
    nfhs.clip.init(env);
    nfhs.unity.init(env);
    nfhs.features.init(env);

    nfhs.nfhs_app.ga.gaSetup();

    nfhs.nfhs_app.ga.gaExtra();
    console.log("About to analytics for home");
    nfhs.analytics.sendPageView( 'page', true );
    var redirecting = nfhs.nfhs_app.members.updateProfile(); //has an interesting side effect, redirects users to /home
    if (redirecting){
      return;
    }

    if (stLight !== undefined) {
        var switchTo5x=true;
        stLight.options({publisher:"dr-ac255e28-a1b4-49a3-1886-ab617a793fa9", doNotHash: true, doNotCopy: true, hashAddressBar: false});
    }
    if ('nfhs_panels' in Drupal.settings) {
      if ('contexts' in Drupal.settings.nfhs_panels) {  // Sometimes contexts is not presents - use default values.
        var contexts = Drupal.settings.nfhs_panels.contexts;
      } else {
        var contexts = {
          page_type: 'generic',
          cards: false,
        };
      }
      //we pre-fetch unity data because dfp ads are instantiated
      //in various places depending on the page and we want to load ads
      //as efficiently as possible -- ideally we should consolidate
      //the unity data call into one place, but until then this will suffice
      nfhs.nfhs_app.dfp_ads.prefetchUnityData(contexts);
      if (contexts.page_type != 'index') {
         if (nfhs.comcast.authenticated()) {
           document.getElementById("xfinity").style.display = 'block';
         }
      }
      if (contexts.page_type == 'search') { // search results
        nfhs.nfhs_app.querySearch.buildSearchResults();
        nfhs.nfhs_app.querySearch.getSearchCounts();
      }
      if (contexts.page_type == 'home') {
        if (nfhs.member.authenticated()) {
            nfhs.auth.userEntitlements(function (entitlements) {
              console.log("entitlements: " + entitlements);
              if(entitlements.indexOf("video_subscription") !== -1) {
                $(".rewards").attr("href", "http://watch.nfhsnetwork.com/rewards");
              }
              else {
                $(".rewards").attr("href", "http://watch.nfhsnetwork.com/rewards-signup");
              }
            });
            nfhs.nfhs_app.members.addToQueue(function() {
              nfhs.nfhs_app.cards.buildCardPanels(Drupal.settings.nfhs_panels);
              nfhs.nfhs_app.dfp_ads.init(contexts);
            });
            nfhs.nfhs_app.members.addToQueue(function() { nfhs.nfhs_app.profile.buildUpcomingSlider(Drupal.settings.nfhs_panels) });
            nfhs.nfhs_app.members.addToQueue(function() { nfhs.nfhs_app.members.hasPersonalized(true) });
        } else {
            nfhs.nfhs_app.members.memberModal('authenticate');
        }

        //HERE
        return;
      }
      if (contexts.page_type == 'index') {
        nfhs.nfhs_app.dfp_ads.init(contexts);
      }
      if (contexts.page_type == 'clips') { // search results
        nfhs.nfhs_app.events.displayClipInfo(contexts);
      }
      if (contexts.page_type == 'highlights') {
        nfhs.nfhs_app.events.displayHighlightInfo(contexts);
      }
      if (contexts.page_type == 'tournament') {
        nfhs.nfhs_app.tournament.buildTournamentCards();
      }
      if (contexts.cards) { // page with no special detail information. Build the cards.
        nfhs.nfhs_app.cards.buildCardPanels(Drupal.settings.nfhs_panels);
      }
      if (contexts.unity_type == 'voting_page') {
        nfhs.nfhs_app.voting.buildVotingPage(Drupal.settings.nfhs_panels);
      }
      if ('event_id' in contexts && contexts.page_type != 'clips') { // specific 'event' (aka game) page .. go build it.
        nfhs.nfhs_app.events.displayEventInfo(contexts);
      } else {
        if ('unity_type' in contexts) { // specific page for school/producer/etc. ...
          nfhs.nfhs_app.pages.displayPageInfo(contexts);
        }
      }
    }

    if (Drupal.settings.state_sports_page) {
      var cards = $('[class^="nfhs-cards-"]');
      $.each(cards, function(index, card) {
        var cardKey = card.className;
        nfhs.nfhs_app.members.processMemberCardData(cardKey);
      });
    }

    if (nfhs.auth.authenticated()) {
      console.log("Authentication Status");
      console.log(nfhs.auth.authenticated());
      $(".signin").hide();
      $(".sso-auth").show();

      nfhs.auth.userEntitlements(function (entitlements) {
        console.log("entitlements: " + entitlements);
        if(entitlements.indexOf("video_subscription") !== -1) {
          $(".rewards").attr("href", "http://watch.nfhsnetwork.com/rewards");
        }
        else {
          $(".rewards").attr("href", "http://watch.nfhsnetwork.com/rewards-signup");
        }
      });
    }

    nfhs.nfhs_app.carousel.ensureLinksOpenInNewTab();
    nfhs.nfhs_app.carousel.hideCarousel(contexts);

    //there should only be one pass when there is a pass_key so select it by default
    if (getQueryVariable("pass_key")){
      $( "button#blue-button-select" ).trigger( "click" );
    };

  }
},
{
  test: window.JSON,
  nope: '//cdnjs.cloudflare.com/ajax/libs/json2/20130526/json2.min.js'
}]);



</script>


