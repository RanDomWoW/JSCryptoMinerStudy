<script type="text/javascript">
    function loginUser() {
        var param = {
            username: $('#username').val(),
            password: $('#password').val(),
            submit: 'true',
            is_remember: document.getElementById('is_remember').checked
        };
        $('#login_msg').css('color', 'green').html('من فضلكم بالانتظار يجرى تسجيل الدخول الآن');
        $.ajax({
            type: 'POST',
            url: 'http://user.arabmmo.com/?controller=public&action=login',
            data: param,
            dataType: 'jsonp',
            success: function(oJson) {
                $('#login_msg').css('color', 'red');
                var resultFlag = oJson.resultFlag;
                switch (resultFlag) {
                    case 'success':amo.cookie.set({name: 'siderlogged',value: 'true'});window.location.reload();break;
                    case 'already_logged':amo.cookie.set({name: 'siderlogged',value: 'true'});window.location.reload();break;
                    case 'username_empty':
                        $('#login_msg').css('display', 'block').html('تفضل ادخال اسم الدخول');
                        break;
                    case 'passwd_empty':
                        $('#login_msg').css('display', 'block').html('تفضل ادخال كلمة السر');
                        break;
                    case 'user_not_exist':
                        $('#login_msg').css('display', 'block').html('اسم الدخول أو كلمة السر خاطئة، نرجو المحاولة مجددا');
                        break;
                    case 'passwd_error':
                        $('#login_msg').css('display', 'block').html('اسم الدخول أو كلمة السر خاطئة، نرجو المحاولة مجددا');
                        break;
                    default:
                        $('#login_msg').css('display', 'block').html('unknown error!');
                        break;
                }

            }
        });
        return false;
    }
function checklogStatus(){
    if(amo.cookie.get('siderlogged')=='true'){
        $('#siderlogin').hide()
    }
}
$(function(){checklogStatus();})
</script>

