<script>
(function($){

    //スクロール禁止用関数
    function no_scroll(){
        //PC用
        var scroll_event = 'onwheel' in document ? 'wheel' : 'onmousewheel' in document ? 'mousewheel' : 'DOMMouseScroll';
        $(document).on(scroll_event,function(e){e.preventDefault();});
        //SP用
        $(document).on('touchmove.noScroll', function(e) {e.preventDefault();});
    }

    //スクロール復活用関数
    function return_scroll(){
        //PC用
        var scroll_event = 'onwheel' in document ? 'wheel' : 'onmousewheel' in document ? 'mousewheel' : 'DOMMouseScroll';
        $(document).off(scroll_event);
        //SP用
        $(document).off('.noScroll');
    }

    var thisW = $(window).width()
    $(function(){
        $( "#dialog-telephone" ).dialog({
            modal: true,
            resizable: false,
            draggable: false,
            autoOpen: false,
            width: thisW * 0.95,
            height: 100,
            close: function(){
                return_scroll();
            }
        });

        $('a.dialogopen').on('click', function(){
            no_scroll();
            $( "#dialog-telephone" ).dialog("open");
            return false;
        });

        $('a.dlg-tel-close').on('click', function(){
            $( "#dialog-telephone" ).dialog("close");
            return false;
        });

    });
})(jQuery);
