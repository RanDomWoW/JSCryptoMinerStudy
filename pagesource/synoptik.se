	function divMaker(propz){
		
		var mrDiv = document.createElement("div");
		propz.position = propz.position || "absolute";

		propz.container.appendChild(mrDiv);
		delete propz.container;
		
		if(propz.imgClass) mrDiv.className = "sprite " + propz.imgClass;
		delete propz.imgClass;

		TweenMax.set(mrDiv, propz);
		return  mrDiv;
	}

	function makeStarRain(starCount, zInd) {
        var stars = [];

        var Star = function() { };

        Star.prototype.moveMe = function() {

        this.topz += this.speed;
        this.rotz += this.rotSpeed;
        this.sidez = Math.cos(this.topz / 20) * -10;

        TweenMax.set(this.img, { y: this.topz, x: this.sidez, rotation: this.topz, rotationX: this.rotz });
            if (this.topz > bHeight) {
                this.topz = -20;
            }
        };

        for (var i = 0; i < starCount; i++) {
            var star = new Star();
            var flowerType;
            if (zInd > 1) {
                flowerType = Math.floor(Math.random() * 7 + 5);
            } else {
                flowerType = Math.floor(Math.random() * 4 + 1);
            }
            star.img = divMaker({ container: mrBanner, zIndex: zInd, left: Math.random() * bWidth + 1,
                top: 0, imgClass: "slutspurt_snow" + flowerType
            });

            star.rotz = 0;
            star.topz = Math.random() * bHeight + 1;
            star.rotSpeed = Math.random() * 2 + 1.4;

            star.speed = Math.random() * 1 + 0.4;
            mrBanner.appendChild(star.img);
            stars.push(star);
        }

        return stars;

    }

    function aniStars() {
        for (var i = 0; i < starsBg.length; i++) {
            starsBg[i].moveMe();
        }

        for (var a = 0; a < starsFg.length; a++) {
            starsFg[a].moveMe();
        }

        requestAnimationFrame(aniStars);

    }

