function RedirectToCorrectPlatformHead() {
	if (window.location.href.indexOf('checkout') != -1) {
		return false;
	}
	
	sHref = window.location.href;
	
	//get current platform
	var sPlatformCurrent = 'desktop';
	var sUAList = 'iPhone|iPod|BlackBerry|Palm|Googlebot-Mobile|Windows Mobile|Android|Opera Mini|Phone|phone';
	var aUAList = sUAList.split('|');
	
	for (p=0; p<aUAList.length; p++) {
		sUAVal = aUAList[p];
	
		if (window.navigator.userAgent.indexOf(sUAVal) != -1) {
			sPlatformCurrent = 'mobile';
		}
	}
	
	//get requested platform
	var sPlatformRequested = 'desktop';
	if (window.location.href.indexOf('http://mdev') != -1 || window.location.href.indexOf('http://mpreprod') != -1 || window.location.href.indexOf('http://m') != -1) {
		sPlatformRequested = 'mobile';
	}
	
	if (window.location.href.indexOf('showua=yes') != -1)
			alert(window.navigator.userAgent);
	
	if (sPlatformCurrent == 'desktop' && sPlatformRequested == 'mobile') {
		//redirect to desktop
		if (sHref.indexOf('http://mdev') != -1)
			sHref = sHref.replace('http://mdev', 'http://dev');
		else if (sHref.indexOf('http://mpreprod') != -1)
			sHref = sHref.replace('http://mpreprod', 'http://preprod');
		else if (sHref.indexOf('http://m.') != -1)
			sHref = sHref.replace('http://m.', 'http://www.');
		
		if (window.location.href != sHref) {
			if (window.location.href.indexOf('showua=yes') != -1)
				alert('redirect: ' + sHref);
			window.location = sHref;
		}
	}
	
	if (sPlatformCurrent == 'mobile' && sPlatformRequested == 'mobile' && sHref.indexOf('rma/return/create') != -1) {
		//for mobile rma create, redirect to desktop url
		if (sHref.indexOf('http://mdev') != -1)
			sHref = sHref.replace('http://mdev', 'http://dev');
		else if (sHref.indexOf('http://mpreprod') != -1)
			sHref = sHref.replace('http://mpreprod', 'http://preprod');
		else if (sHref.indexOf('http://m.') != -1)
			sHref = sHref.replace('http://m.', 'http://www.');
		
		if (window.location.href != sHref) {
			if (window.location.href.indexOf('showua=yes') != -1)
				alert('redirect: ' + sHref);
			window.location = sHref;
		}
	}
	
	if (sPlatformCurrent == 'mobile' && sPlatformRequested == 'desktop') {
		//show rma create in desktop theme only
		if (sHref.indexOf('rma/return/create') == -1) {
			//redirect to mobile
			if (sHref.indexOf('http://dev') != -1)
				sHref = sHref.replace('http://dev', 'http://mdev');
			else if (sHref.indexOf('http://preprod') != -1)
				sHref = sHref.replace('http://preprod', 'http://mpreprod');
			else if (sHref.indexOf('http://www') != -1)
				sHref = sHref.replace('http://www', 'http://m');
			
			if (window.location.href != sHref) {
				if (window.location.href.indexOf('showua=yes') != -1)
					alert('redirect: ' + sHref);
				window.location = sHref;
			}
		}
		
	}
	
}

if (window.location.hostname.indexOf('dev') == -1 && window.location.hostname.indexOf('preprod') == -1) {
//if (((window.location.hostname.indexOf('dev') == -1 && window.location.hostname.indexOf('preprod') == -1) && (window.location.hostname.indexOf('.com') > -1 || window.location.hostname.indexOf('.eu') > -1)) || window.location.hostname.indexOf('dev') > -1 || window.location.hostname.indexOf('preprod') > -1) {
	RedirectToCorrectPlatformHead();
}


