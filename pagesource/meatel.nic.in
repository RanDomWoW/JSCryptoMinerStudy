<script type='text/javascript' language='javascript'>
function string_validate(){
	var str=document.search.strg.value;
	if (str==null || str.length<3) {	
		alert("At least three alphanumeric characters required");
		document.search.strg.focus();
		return false;
	}

	if(!(str.match(/^[a-zA-Z0-9 ]+$/))) {	
		alert("Special character not allowed");
		document.search.strg.focus();
		return false;
	}

	return true; }
</script>
