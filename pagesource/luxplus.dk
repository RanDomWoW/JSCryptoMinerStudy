    function getHashFilter() {
        var hash = location.hash;
        var matches = location.hash.match( /filter=([^&]+)/i );
        var hashFilter = matches && matches[1];
        return hashFilter && decodeURIComponent( hashFilter );
    }

    $(function(){
        $("img.lazy").unveil(200, function() {
            $(this).load(function() {
                this.style.opacity = 1;
            });
        });
        $("img.transparent").unveil(200, function() {
            $(this).load(function() {
                this.style.opacity = .2;
            });
        });
        var $filters = $('#filters').on( 'click', 'button', function() {
            var filterAttr = $( this ).attr('data-filter');
            // set filter in hash
            location.hash = 'filter=' + encodeURIComponent( filterAttr );
        });
        var isIsotopeInit = false;
        var $container = $('.isotope');
        function onHashchange() {
            var hashFilter = getHashFilter();
            if ( !hashFilter && isIsotopeInit ) {
                return;
            }
            isIsotopeInit = true;
            $container.isotope({
                // options
                itemSelector: '.item_wrap',
                layoutMode: 'fitRows',
                filter: hashFilter,
                getSortData: {
                    pricedesc: '[data-pricedesc] parseInt',
                    priceasc: '[data-priceasc] parseInt',
                    name: '[data-name]',
                    brand: '[data-brand]',
                    type: '[data-type]',
                    relevance: '[data-relevance] parseInt',
                    savingsflat: '[data-savings] parseInt',
                    savingspercentage: '[data-savings-percentage] parseInt'
                }
            });
        }
        $(window).on( 'hashchange', onHashchange );
        // trigger event handler to init Isotope
        onHashchange();
        $('.iso-sort-btn').on( 'click', function(e) {
            e.preventDefault();
            var sortByValue = $(this).attr('data-sort-by');
            $('.page_sort_btn').html($(this).text() + '<i class="fa fa-angle-down"></i>');
            if(sortByValue == 'pricedesc' || sortByValue == 'relevance' || sortByValue == 'savingsflat' || sortByValue == 'savingspercentage'){
                $container.isotope({ sortBy: sortByValue, sortAscending: false });
            }else{
                $container.isotope({ sortBy: sortByValue, sortAscending: true });
            }
            $("img.lazy").trigger("unveil");
            $("img.transparent").trigger("unveil");
        });
        if ($('.back_to_top').length) {
            var scrollTrigger = 500, // px
                    backToTop = function () {
                        var scrollTop = $(window).scrollTop();
                        if (scrollTop > scrollTrigger) {
                            $('.back_to_top').addClass('show');
                        } else {
                            $('.back_to_top').removeClass('show');
                        }
                    };
            backToTop();
            $(window).on('scroll', function () {
                backToTop();
            });
            $('.back_to_top').on('click', function (e) {
                e.preventDefault();
                $('html,body').animate({
                    scrollTop: 0
                }, 700);
            });
        }
    });

    $('.top-menu-dropdown').hover(function() {
        if(!$(this).hasClass('my-hover')) {
            $(this).children('ul').stop(true).delay(200).show(0);
        }
        $(this).children('ul').show(0);
    }, function() {
        if(!$(this).hasClass('my-hover')) {
            $(this).children('ul').stop().hide();
            $(this).children('ul').delay(0).removeClass('my-hover');
        }
        $(this).children('ul').stop().hide();
        $(this).children('ul').delay(0).removeClass('my-hover');
    });

    var timeout;
    $(".top-navigation").mouseleave(function() {
        clearTimeout(timeout);
        $('.top-menu-dropdown').stop(true).removeClass('my-hover');
    });
    $(".top-navigation").mouseenter(function() {
        timeout = setTimeout(function(){
            $('.top-menu-dropdown').addClass('my-hover');
        }, 500);
    });
</script>

