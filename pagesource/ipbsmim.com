<script type="text/javascript">
	jQuery(document).ready(function(){
		jQuery('input[type="radio"]').on('change', function(){
			$form = jQuery(this).closest('.degree');
			console.log($form);
			jQuery('.degree').not($form).trigger('reset');
			$form.find('button[type="submit"]').text('Select destination');	
		});
		jQuery('#degree-responsive-select').on('change', function(){
			jQuery('.toggle-select-container').removeClass('active');
			jQuery('.toggle-select').removeAttr('name');
			$toggle_select = jQuery('#toggle-select-'+jQuery(this).val());
			$toggle_select.find('select').attr('name', 'partner');
			$toggle_select.addClass('active');
		});
	}); 
</script>


