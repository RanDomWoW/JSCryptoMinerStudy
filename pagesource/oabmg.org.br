<script type="text/javascript">
    function OnSuccessInformativo(response) {
        if (response == "3") {
            $("#msgInforInformativo").empty();
            $("#msgInforInformativo").append("Favor preecher os campos obrigatório!");
            $("#alertaInformativo").removeClass("alert alert-danger alert alert-success").addClass("alert alert-danger");
            $("#myModalInformativo").modal('show');
        } else if (response == "2") {
            $("#msgInforInformativo").empty();
            $("#msgInforInformativo").append("Favor selecionar uma das opções!");
            $("#alertaInformativo").removeClass("alert alert-danger alert alert-success").addClass("alert alert-info");
            $("#myModalInformativo").modal('show');
        } else if (response == "1") {
            $("#msgInforInformativo").empty();
            $("#msgInforInformativo").append("Enviado com sucesso!");
            $("#alertaInformativo").removeClass("alert alert-danger alert alert-info").addClass("alert alert-success");
            limparInformativo();
            $("#myModalInformativo").modal('show');
        }
    }

    function limparInformativo() {
        $("#dropAreaInformativo").val('');
        $("#dropEstadoInformativo").val('');
        $("#txtNomeInformativo").val('');
        $("#txtEmailInformativo").val('');
        $("#CkProcessoCivil").prop("checked", false);
        $("#CkProcessoPenal").prop("checked", false);
        $("#CkPublico").prop("checked", false);
        $("#CkNotInst").prop("checked", false);
        $("#ckTrabalho").prop("checked", false);
    }

    function OnFailureInformativo(response) {
        alert(JSON.stringify(response));
    }
