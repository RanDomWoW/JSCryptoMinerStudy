<script type="text/javascript">

    var q = null;
    $("document").ready(function() {
        changeIndicator();
        $("div.container div.scroll").on("click", function() {
            if($(this).hasClass("top") == true) {
                $("html").animate({scrollTop : 0}, 'slow');
            } else {
                $('html').animate({scrollTop : 820}, 'slow');
            }
        });
        $(".btn_show_video").on("click", function() {
            var movie = '<iframe src="//player.vimeo.com/video/55712533?byline=0&amp;portrait=0&amp;color=ffffff&amp;autoplay=1" width="1024" height="576" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen>123123</iframe><a class="btn_close">×</a>';
            $("section.show_movie").show();
            $('section.show_movie div.content_wrap').html(movie);
            $(".btn_close").off('click').on("click", function() {
                $('section.show_movie div.content_wrap').html('');
                $("section.show_movie").hide();
            })
        });

        $(document).on("scroll", function() {
            changeIndicator();
        });

        $("div.indicator div").on("click", function() {
            switch($(this).index()) {
                case 0 : $("html").animate({scrollTop : 0}, 'slow'); break;
                case 1 : $("html").animate({scrollTop : 820}, 'slow'); break;
                case 2 : $("html").animate({scrollTop : 1566}, 'slow'); break;
            }
        })


        $("section.somcloud div.animation div.content_wrap ul li").click(function() {
            if($(this).hasClass('active') == true || $(this).hasClass("btn_blog") == true) {
                return 0;
            }
            var target = $("section.somcloud div.animation div.content_wrap div.animation_earth div#balloon");
            target.html("")
            $("section.somcloud div.animation div.content_wrap ul li.active").removeClass('active');
            $("section.somcloud div.description ul li.desc").fadeOut(500);
            $(this).addClass("active");

            if($(this).hasClass("btn_press") == true) {
                $("section.somcloud div.animation .animation_earth .deco").fadeOut(200);
                $(".animation_earth").removeClass("deactive").addClass("active");
                $("section.somcloud div.description ul li#comment").fadeIn(500);

            } else {
                if($(".animation_earth").hasClass("active") == true) {
                    $(".animation_earth").removeClass("active").addClass("deactive");
                }
                if($(this).hasClass("btn_sync") == true) {
                    $("section.somcloud div.description ul li#item").fadeIn(500);
                    $("section.somcloud div.animation .animation_earth .deco").fadeIn(200);
                } else if($(this).hasClass("btn_world") == true) {
                    $("section.somcloud div.animation .animation_earth .deco").fadeOut(200);
                    $("section.somcloud div.description ul li#city").fadeIn(500);
                    var _locate = makeBalloon();
                    target.html("")
                    for(var idx = 0; idx < _locate.length; idx++) {
                        target.append("<div class='balloon" + _locate[idx][2] + "' style='top : " + _locate[idx][0] + "px; left : " + _locate[idx][1] + "px;'>&nbsp;</div>");
                    }
                    var _target = target.find("div");
                    for(var idx = _target.length; idx > 0; idx--) {
                        _target.eq(idx).fadeIn(idx * 150);
                    }
                } else {

                }

            }
        })
    });
    function makeBalloon() {
        var balloon = 20 + Math.floor(Math.random() * 10);
        var balloonXY = [];
        for(var idx = 0; idx < balloon; idx++) {
            var _tmpx = Math.floor(Math.random() * 184);
            var _tmpy = Math.floor(Math.random() * 184);
            var _tmpSize = Math.floor(Math.random() * 7);
            balloonXY.push([_tmpx, _tmpy, _tmpSize]);
        }
        return balloonXY;
    }
    function changeIndicator() {
        var _now = $(this).scrollTop();
        var _indicator = $("div.indicator");
        if(_now >= 820 && _now < 1566) {
            _indicator.find(".on").removeClass("on").end().find(".page2").addClass("on");
        } else if (_now >= 1566) {
            _indicator.find(".on").removeClass("on").end().find(".page3").addClass("on");
        } else {
            _indicator.find(".on").removeClass("on").end().find(".page1").addClass("on");
        }
    }
</script>
