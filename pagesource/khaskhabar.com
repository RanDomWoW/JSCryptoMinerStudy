                iAuto +='<li class="catNws-home"><a href="http://www.iautoindia.com/articles/'+data[i].year+'/'+category+'/'+data[i].short_url+'.html" title="'+data[i].article_head+'" rel="nofollow" target="_blank"><img src="http://www.iautoindia.com/images/autoindia/small376/'+data[i].article_image+'" width="120" height="90" class="bdrGry"/>'+data[i].article_head+'</a></li>';
            }
       
            if(i=2){
               iAuto +='<li class="readmore clr"><a href="http://www.iautoindia.com" class="morenews" title="iAutoIndia" target="_blank">More...</a></li>';
           }
        }
		   document.getElementById("iAutoIndia").innerHTML = iAuto;
		 //  $('#datatoshow').html(dd);
     }
})
</script>
