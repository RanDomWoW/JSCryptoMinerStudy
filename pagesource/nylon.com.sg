// if(jQuery(window).scrollTop() > 620) {		
// jQuery('.ads-type2, .ads-type1').addClass( "fixed" ); 
//  }    
//  else { 	
//  jQuery('.ads-type2, .ads-type1').removeClass("fixed");  
