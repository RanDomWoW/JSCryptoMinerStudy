function getWindowHeight() // viewport, not document
{
    var windowHeight = 0;
    if (typeof(window.innerHeight) == 'number')
    {
        // DOM compliant, IE9+
        windowHeight = window.innerHeight;
    }
    else
    {
        // IE6-8 workaround, Note: document can be smaller than window
        var ieStrict = document.documentElement.clientHeight; // w/out DTD gives 0
        var ieQuirks = document.body.clientHeight; // w/DTD gives document height
        windowHeight = (ieStrict > 0) ? ieStrict : ieQuirks;
    }
    return windowHeight;
}
 
function getScrollTop()
{
    var scrollTop;
    if(typeof(window.pageYOffset) == 'number')
    {
        // DOM compliant, IE9+
        scrollTop = window.pageYOffset;
    } else {
        // IE6-8 workaround
        if(document.body && document.body.scrollTop)
        {
            // IE quirks mode
            scrollTop = document.body.scrollTop;
        }
        else if(document.documentElement && document.documentElement.scrollTop)
        {
            // IE6+ standards compliant mode
            scrollTop = document.documentElement.scrollTop;
        }
    }
    return scrollTop;
}

jQuery(document).ready(function () {
    var positionMenuInPageBis = $('#haut').offset().top;
    var windowHeight = $(window).height() - $('#header').height() - $('#pied').height() - 85;
    var windowHeightOrigine = $('#conteneur').height();
    var windowHeightCentre = $('#centre').height();
    var windowHeightDroite = $('#droite').height();
    var positionVertiInPage = 180 + $('#conteneur').offset().top;
    var heightfooter = $('#pied').height()+20;
    $('#background-footer').css('height',heightfooter+'px')
    $('#verti').css("top", positionVertiInPage + "px");

    if (windowHeightOrigine < windowHeightDroite) {
        $('#contenu').height(windowHeightDroite);
        windowHeightOrigine = windowHeightDroite;
    }

    if (windowHeightOrigine < windowHeight) {
        $('#contenu').height(windowHeight);
    }

    var positioncentre = $('#centre').offset().top;
    if ($('#haut').height()>0){
        var positionbackmenu = $('#haut').offset().top;
    } else{
        var positionbackmenu = $('#sites_pkbis').offset().top;
    }

    $('#background-menu').css("top", positionbackmenu + "px");

    if ($("#droite .par_fixe").length > 0) {
        var positionAddDroite = $("#droite .par_fixe").offset().top;
        var positionDroite = $("#droite").offset().top;
        var positionAddPage = $('#droite .par_fixe').offset().top;
        var heightpub = windowHeightDroite + positionDroite - positionAddPage;
    } else {
        var positionAddDroite = 0;
        var positionDroite = $("#droite").offset().top;
        var positionAddPage = 0;
        var heightpub = windowHeightDroite + positionDroite - positionAddPage;
    }
    var bottomcentre = positioncentre + windowHeightCentre;
    var decalage = 85;
    var paddingaddMax = bottomcentre - heightpub - positionAddPage;

    $(window).scroll(
        function () {
            var paddingadd = getScrollTop() - positionAddPage + decalage;

            if ($(window).scrollTop() >= positionVertiInPage- 60) {
                // fixed
                $('#verti').css("top", "75px");
                $('#verti').css("position", "fixed");
            } else {
                // relative
                $('#verti').css("top", "180px");
                $('#verti').css("position", "absolute");
            }
            if ($(window).scrollTop() >=  positionbackmenu){
                $('#background-menu').css("top", "0px");
                $('#haut').css("top", "0px");
                $('#pk_banniere').css('padding-top','30px');
                $('#haut').css("position", "fixed");
                $('#background-menu').css("position", "fixed");
                $('#background-menu').css("z-index", "150");
                $('#sites_pkbis').css("top", "0px");
                $('#sites_pkbis').css("position", "fixed");
                $('#sites_pkbis').css("z-index", "151");
                $('#recherche').css("top", "26px");
                $('#recherche').css("position", "fixed");
                $('#recherche').css("z-index", "152");
            } else {
                // relative
                $('#pk_banniere').css('padding-top','5px');
                $('#background-menu').css("top", positionbackmenu + "px");
                $('#background-menu').css("position", "absolute");
                $('#sites_pkbis').css("top", "105px");
                $('#sites_pkbis').css("position", "absolute");
                $('#recherche').css("top", "0px");
                $('#recherche').css("position", "relative");
            }
            if (paddingadd > paddingaddMax) {
                paddingadd = paddingaddMax;
            }

            if (getScrollTop() < positionAddPage - decalage) {
                $("#droite .par_fixe").css("margin-top", "0px");
            } else if (getScrollTop() >= (positionAddPage - decalage) && getScrollTop() <= (bottomcentre - heightpub)) {
                $("#droite .par_fixe").css("margin-top", paddingadd + "px");
            } else if (getWindowHeight() + getScrollTop() >= $(document).height() && windowHeightCentre > windowHeightDroite) {
                $("#droite .par_fixe").css("margin-top", paddingaddMax + "px");
            }

            if ($(window).scrollTop() >= positionMenuInPageBis) {
                // fixed
                $('#haut').css("top", "0px");
                $('#haut').css("position", "fixed");
                $('#haut').css("z-index", "155");
            } else {
                // relative
                $('#haut').css("top", "105px");
                $('#haut').css("position", "absolute");
            }

            if ($(window).scrollTop() > ($(window).height() / 2)) {
                // fixed
                $('#boutontotop').fadeIn();
            } else {
                // relative
                $('#boutontotop').fadeOut();
            }
        }
    );
    /*totopbutton*/
    document.getElementsByTagName('button')[0].onclick = function () {
        $('html,body').animate({
            scrollTop: 0
        }, 'slow');
    }
})
