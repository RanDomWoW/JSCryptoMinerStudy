	<script>
		$(function(){
			var navfixed = $('.nav-offer-fixed');
			$(window).scroll(function(){
				if($(window).scrollTop() <= 140){
					navfixed.removeClass('fixed-state');
				} else {
					navfixed.addClass('fixed-state');
				}
			});
			var navbar = $('.nav-offer-container');
			$(window).scroll(function(){
				if($(window).scrollTop() <= 200){
					navbar.addClass('default-state');
					navbar.removeClass('active-state');
				} else {
					navbar.removeClass('default-state');
					navbar.addClass('active-state');
				}
			});
		})
	</script>
	<script>
		$('a[href^=#]').on("click",function(){
			var t= $(this.hash);
				var t=t.length&&t||$('[name='+this.hash.slice(1)+']');
				if(t.length){
				var tOffset=t.offset().top;
				$('html,body').animate({scrollTop:tOffset-100},'slow');
				return false;
			}
		});
	</script>

