<script>
var scrolling = false;
var eachHeight = 0;
$(function () {

	//reorganize The Latest posts
	var latestItems = $('.latestListIn li');
	latestItems.sort(function(a,b){
	  var keyA = $(a).data('date');
	  var keyB = $(b).data('date');

	  if (keyA < keyB) return 1;
	  if (keyA > keyB) return -1;
	  return 0;
	});
	var ul = $('.latestListIn ul').empty();
	$.each(latestItems, function(i, li){ul.append(li);});

	//make the scroll for more content triggers work
	//this scrolls everytime a user clicks
	$('.btn-more').on('click',function(){
		//var mtop = parseInt($(this).closest('.scrollTrigWrap').parent().find('ul').css('marginTop'));
		//if the first list item doesn't have the class .showing, then add it so we know how much we've scrolled
		if(!$(this).closest('.scrollTrigWrap').parent().find('ul li').hasClass('showing')){
			$(this).closest('.scrollTrigWrap').parent().find('ul li:first').addClass('showing');
		}
		if($(this).closest('.scrollTrigWrap').parent().find('li.showing').next().next().is('li')){
			//then theres more in the list to scroll to
			$(this).closest('.scrollTrigWrap').parent().find('li.showing').next().next().addClass('showing').siblings().removeClass('showing');
			var toScroll = parseInt($(this).closest('.scrollTrigWrap').parent().find('li.showing').offset().top) - parseInt($(this).closest('.scrollTrigWrap').parent().find('ul').offset().top);
			$(this).closest('.scrollTrigWrap').parent().find('ul').stop().animate({marginTop:(-toScroll)+'px'},150);
		}
		else{
			//dont scroll because its end of list
		}
	});
	$('.btn-back').on('click',function(){
		//var mtop = parseInt($(this).closest('.scrollTrigWrap').parent().find('ul').css('marginTop'));
		//var eachHeight = parseInt($(this).closest('.scrollTrigWrap').parent().find('li:first-child').outerHeight(true)) + parseInt($(this).closest('.scrollTrigWrap').parent().find('li:first-child').css('border-top-width')) + parseInt($(this).closest('.scrollTrigWrap').parent().find('li:first-child').css('border-bottom-width'));
		if(parseInt($(this).closest('.scrollTrigWrap').parent().find('ul').css('marginTop')) != 0){
			if($(this).closest('.scrollTrigWrap').parent().find('li.showing').prev().prev().is('li')){
				//then theres more to scroll up to
				$(this).closest('.scrollTrigWrap').parent().find('li.showing').prev().prev().addClass('showing').siblings().removeClass('showing');
				var toScroll = parseInt($(this).closest('.scrollTrigWrap').parent().find('li.showing').offset().top) - parseInt($(this).closest('.scrollTrigWrap').parent().find('ul').offset().top);
				$(this).closest('.scrollTrigWrap').parent().find('ul').stop().animate({marginTop:(-toScroll)+'px'},150);
			}
		}
	});

	$(document).on('click','#prevIssues',function(e){
		e.preventDefault();
		$('.list-products ul').animate({marginLeft:'-=138px'},500);
	});
	
    $('.btn-more,.btn-back').bind('click',false);
    
    
    
    

    
});
</script>