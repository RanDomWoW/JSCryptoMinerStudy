                        function changeIndex(){
				var currLink; 
				
				jQuery("#new_cars_ul li a").each(function() {
   					if(jQuery(this).hasClass('activeSlide')){
						jQuery(this).removeClass('activeSlide'); 					

						var $curActiveItem;
						if(jQuery(this).parent().is(':last-child')){
							$curActiveItem = jQuery(this).parent().parent().find('li').first().find('a');
						}
						else{
							$curActiveItem = jQuery(this).parent().next().find('a');
						}
						
						$curActiveItem.addClass('activeSlide');
					
						var currNavItems = jQuery('.pagination a', jQuery(this).parents('.featured_pag'))
        					currLink = currNavItems.index( $curActiveItem );
						
        					var curFeatSlides = jQuery('div[id^="featured_switch"] > div', jQuery(this).parents('.featured_wrap'));
						
						curFeatSlides.each(function(){
        						if ( jQuery(this).is(':visible')){
        							jQuery(this).fadeOut('normal', function(){
	       								curFeatSlides.eq(currLink).fadeIn('normal');
								});	
        						}
        					});	
						
						return(false);
					}
				}); 

				jQuery("#used_cars_ul li a").each(function(){
					if(jQuery(this).hasClass('activeSlide')){
						jQuery(this).removeClass('activeSlide');						
						jQuery("#used_cars_ul li").eq(currLink).find('a').addClass('activeSlide');
        					
						var curFeatSlides = jQuery('div[id^="featured_switch"] > div', jQuery(this).parents('.featured_wrap'));
						curFeatSlides.each(function(){
        						if ( jQuery(this).is(':visible')){
        							jQuery(this).fadeOut('normal', function(){
	       								curFeatSlides.eq(currLink).fadeIn('normal');
								});	
        						}
        					});	
						
						return(false);
					}
				});
			}

        			// dealing with actions
        			jQuery('.featured_pag > .pagination a').click(function(e){
					
					clearInterval(timer);
					timer = window.setInterval(changeIndex,5000);	
				
        				e.preventDefault();
					

        				var $this = jQuery(this),

        					currNavItems = jQuery('.pagination a', $this.parents('.featured_pag')),
        					currLink = currNavItems.index( $this );

        				if ( ! $this.hasClass('activeSlide'))
        				{

        					$this.parents('.featured_pag').find('.pagination > li >a').removeClass('activeSlide');
        					$this.addClass('activeSlide');
        					var curFeatSlides = jQuery('div[id^="featured_switch"] > div', $this.parents('.featured_wrap'));
						curFeatSlides.each(function(){

        						if ( jQuery(this).is(':visible') )
        						{
        							jQuery(this).fadeOut('fast', function(){
        								curFeatSlides.eq(currLink).fadeIn('fast');
        							});
        						}
        					});
            				}
        			})

            	})

            </script>
