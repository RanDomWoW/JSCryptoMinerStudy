<script>

function checkMessageType() {
    var dropdown = document.getElementById("message_type");
    var current_value = dropdown.options[dropdown.selectedIndex].value;
//if message type is a suggestion hide organizational unit fiels from form
    if (current_value == "1") {
        document.getElementById("div_organizational_unit").style.display = "none";
        document.getElementById("div_custom_organizational_unit").style.display = "none";
		
    } //if is a complaint, show the organization unit options list
	else if(current_value == "2") {
		document.getElementById("div_organizational_unit").style.display = "block";
	}
    else {
    }
}

//check organization unit

function checkOrganizationalUnit() {
    var dropdown = document.getElementById("organizational_unit");
    var current_value = dropdown.options[dropdown.selectedIndex].value;
// if the choosen organization unit is "other" show the input text field
    if (current_value == "15") {
        document.getElementById("div_custom_organizational_unit").style.display = "block";
      //  document.getElementById("div_organizational_unit").style.display = "none";		
    } // else show normal list and hide the input text field
	else{		
		document.getElementById("div_organizational_unit").style.display = "block";
        document.getElementById("div_custom_organizational_unit").style.display = "none";
	}
}

</script>

