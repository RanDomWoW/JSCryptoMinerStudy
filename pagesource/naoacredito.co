        var addedPostLinks = ["http://www.osagaz.com.br/lencinhos-secadora/","http://www.areceitaria.com.br/7-receitas-macarrao/","http://www.naoacredito.co/pequena-dancarina/","http://www.naoacredito.co/esculturas/","http://www.naoacredito.co/camisa-certa/","http://www.naoacredito.co/buddy/","http://www.areceitaria.com.br/empadao-frito/","http://www.areceitaria.com.br/enroladinho-de-canela/","http://www.osagaz.com.br/dicas-pro-banho/","http://www.naoacredito.co/joe-noonan/","http://www.naoacredito.co/seguranca-insegura/","http://www.naoacredito.co/cachorros-malandros/","http://www.cuide-se.me/depressao/","http://www.naoacredito.co/waitimo-cavernas/","http://www.areceitaria.com.br/cheesecake-galatico/","http://www.naoacredito.co/josie-canta-classico/","http://www.naoacredito.co/avaliacao-animais/","http://www.naoacredito.co/fotos-medo/","http://www.osagaz.com.br/10-habitos-do-amor/","http://www.areceitaria.com.br/espaguete-voador/","http://www.naoacredito.co/peter-maddox/","http://www.naoacredito.co/lapides-engracadas/","http://www.naoacredito.co/gambiarras-bizarras/","http://www.naoacredito.co/golden-retriever-2/","http://www.cuide-se.me/centenarios/","http://www.areceitaria.com.br/crepe-salzburgo/","http://www.naoacredito.co/cidades-perdidas/","http://www.naoacredito.co/fotos-emocionantes/","http://www.naoacredito.co/descobertas-misteriosas/"];

    	// Load new content 2x post before end but limit to [33px;333px] range
    	var loadOffset = Math.min(
    		Math.max(
    			33,
    			$( '.latest-block a:last-of-type' ).height() * 2 ),
    		333
    	);

    	var $win = $(window);
        var $csLoader = $(".js-cs-loader");
        var scrollBound = true;

    	function fetchNextFeed() {
            scrollBound = false;
            $win.off( "scroll",contScroll );
    		if ( feed.next == 0 ) {
    			return;
    		}
            $csLoader.show();

    		$.ajax({
                url: '/_positivechannel_json/' + feed.next + '.json',
    			dataType: 'json',
    			success: handleFeedUpdate,
                error: function(){
                    $csLoader.hide();
                }
    		});
    	}

    	function handleFeedUpdate(json) {
            $csLoader.hide();
    		feed = json;
    		oLength = feed.feed === undefined ? 0 : feed.feed.length;
	    	oI = 0;
            scrollBound = true;
	    	contScroll();
            $win.scroll( contScroll );
    	}

    	function contScroll() {
            if ( !scrollBound ) {
                return;
            }
			if ( $(document).height() - (window.innerHeight ? window.innerHeight : $win.height()) <= $win.scrollTop() + loadOffset ) {

				var lastIndex = Math.min( oI + chunkSize, oLength );

				var html = '';
                var changeCount = 0;

				for ( var i = oI; i < lastIndex; ++i ) {
    				var post = feed.feed[i];
                    if ( $.inArray( post.link, addedPostLinks ) != -1 ) {
                        continue;
                    } else {
                        addedPostLinks.push( post.link );
                    }

                    changeCount += 1;

                    var prettyDate = df.prettyDate(post.date);
                    if (post.date_line && post.date_line.length > 0){
                        prettyDate = df.stringFormat(post.date_line, prettyDate);
                    }

					html = html + '\
					<a class="left-img-block" href="' + post.link + '">\
						<div class="left-img-block__img" style="background-image:url(\'' + post.img + '\');"></div>\
						<div class="left-img-block__text">\
							<div class="left-img-block__text__inner">\
                                <div class="top-img-block__img__overlay js-date" title="' + post.date + '">'+ prettyDate +'</div>\
								' + post.headline + '\
							</div>\
						</div>\
					</a>';
				}


				$( '.latest-block' ).append( html );
				//console.log('added ' + (lastIndex - oI));
				oI = lastIndex;

				if ( oI >= oLength ) {
					//console.log('need new items');
					fetchNextFeed();
				} else if ( changeCount === 0 ) {
                    contScroll();
                }
			}
    	}

    	$(document).ready(function() {
    		$win.scroll( contScroll );

            //mobile navbar
            var $nav = $(".mobile-nav .nav");
            $(".navbar-button").click(function(){
                $nav.toggleClass("nav--collapsed");
            });

            $(".js-date").each(function(){
                    var $this = $(this);
                    var date_line = $this.data('date_line');
                    var title = $this.prop('title');
                    if (title && title.length > 0) {
                        if (!date_line || date_line.length === 0) {
                            $this.html(df.prettyDate(title));
                        } else {
                            $this.html(df.stringFormat(date_line, df.prettyDate(title)));
                        }
                    }
                }
            );
    	});

        var formats = {"just_now":"Agora mesmo","minutes_ago":"{0} minutos atr\u00e1s","one_hour_ago":"Uma hora atr\u00e1s","hours_ago":"{0} horas atr\u00e1s","today":"Hoje","yesterday":"Ontem","default":"{0} dias atr\u00e1s"};

        var df = new DateFormatter(formats);


        function DateFormatter(formats) {
            var stringFormat = function (format) {
                var args = Array.prototype.slice.call(arguments, 1);
                return format.replace(/{(\d+)}/g, function (match, number) {
                    return typeof args[number] != 'undefined'
                        ? args[number]
                        : match
                        ;
                });
            };

            var prettyDate = function (time) {
                var date = new Date(time);
                var diff = Math.floor(((new Date()).getTime() - date.getTime()) / 1000);

                var todayStart = new Date();
                todayStart.setHours(0);
                todayStart.setMinutes(0);
                todayStart.setSeconds(0);

                var diffTodayStart = Math.floor((date.getTime() - todayStart.getTime()) / 1000);
                var diffYesterdayStart = diffTodayStart + 60 * 60 * 24;

                return (
                diff < 120 && formats.just_now ||
                diff < 3600 && stringFormat(formats.minutes_ago, Math.floor(diff / 60)) ||
                diff < 7200 && formats.one_hour_ago ||
                diff < 3600 * 6 && stringFormat(formats.hours_ago, Math.floor(diff / 3600)) ||
                diffTodayStart > 0 && stringFormat(formats.today) ||
                diffYesterdayStart > 0 && stringFormat(formats.yesterday) ||
                stringFormat(formats.default, Math.ceil(diff/60/60/24)));
            };

            return {
                prettyDate: prettyDate,
                stringFormat: stringFormat
            };

        }
    </script>
</div>
</body>
</html>