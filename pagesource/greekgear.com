$(document).ready(function() {
      var homepage_personalization = {

        // To implement deisgn in header
        eci_implement_design: function(headerlevel, alink) {
            var headerlevel = headerlevel.toUpperCase();
            headerlevel = headerlevel.trim();
            var design_structure = '<p> Welcome Back ' + headerlevel + '! <a href = "' + alink + '">\
            SHOP NOW &#10097;&#10097;\
            </a>\
            </p>';
            if($('.eci-welcome-kappa')[0]){
                $('.eci-welcome-kappa').css({'display':'none', 'height':'0'});
                console.log('variable absent');
                    if(headerlevel && alink){
                        console.log('variable present');
                        $('.eci-welcome-kappa').append(design_structure);
                        $('.eci-welcome-kappa').css({'display':'block', 'height':'180px'});
                    }
                }
        },

        // Check Category page and define cookie on that basis.
        eci_cookie_condition: function() {           
            
            if($(".section-featured-cats > ul > li").length){
                var categoryname = $('.breadcrumbs-container > span:eq(4)').text();
                var alink = window.location.pathname
                var alink = alink.replace('/', '');
                 var cookiecontent = {
                    catname: categoryname,
                    link: alink
                    }
                    var cookiejson = JSON.stringify(cookiecontent);
                    var ob = this;
                    ob.eci_setcookiecat('eci_category', cookiejson, 15);
            }            
            
        },

        // set cookie with category name and link
        eci_setcookiecat: function(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
        },

        //extract value from cookie
        eci_getcookie: function(cname){
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for(var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        },

        //check specific cookie
        eci_checkcookiecat: function(){
            var ob = this;
            var catuser = ob.eci_getcookie('eci_category');
            var referrer = document.referrer;
            var cururl = window.location.href;

            if (catuser != ""){
                // to check whether user is from greekgeart cart page or from greekgear page itself
                // Should work only if user is not greekgear cart page or from greekgear page itself
            	if(cururl.indexOf("/#jmui=") == -1 && referrer.indexOf("greekgear.com") == -1 &&  referrer.indexOf("store.luminatestores.net") == -1){
	                value = JSON.parse(catuser);
                    var catname = value.catname;
                    var link = value.link;
                    ob.eci_implement_design(catname, link);    
                }             
            }
        },

        eci_init: function() {
            var ob = this;
            ob.eci_cookie_condition();
            // To verify Homepage and implement chnages
            if($('.organization-dropdowns').length){
                ob.eci_checkcookiecat();
            }            
        }
    }.eci_init();
});
