    <script>
    $("body").delegate("#mask_close, .window .modal-close", "click", function(event){

        event.preventDefault();
        modalCloseAll();

    });
    function validateEmail(email) {
        var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }
    function modalCloseAll(){
        //CLOSE ALL FIRST
        $('#mask_close').hide();
        $('#mask').hide();
        $('.window').hide();
    }
    function modalDefaultTemplate(modal){

        $('#modalDefault').html('');
        $("#modalDefaultTemplate").tmpl(modal).appendTo("#modalDefault");
        //CLOSE ALL FIRST
        modalCloseAll();

        var id = "#modalDefault";

        var maskHeight = $(window).height();
        var maskWidth  = $(window).width();
        var typeClose  = "#mask";

        $(typeClose).css({'width':maskWidth,'height':maskHeight});
        $(typeClose).show();  
        $(typeClose).fadeTo("slow",0.8);

        //Get the window height and width
        var winH = $(window).height();
        var winW = $(window).width();
             
        $(id).css('top',  winH/2-$(id).height()/2);
        $(id).css('left', winW/2-$(id).width()/2);
        $(id).show();

    }

    $signUpContent = '';
    $.get('/newsletter/sign-up-content', function(data){
        $signUpContent = data;
    });

    $("body").delegate("#cadastro", "click", function(event){

        msg = '<div>\
                <form id="form-cadastro">\
                  <div>'+$signUpContent+'</div>\
                  <div class="form-group">\
                    <input type="text" name="name" value="" placeholder="Nome *" class="form-control" />\
                  </div>\
                  <div class="form-group">\
                    <input type="text" name="email" value="" placeholder="E-mail *" class="form-control" />\
                  </div>\
                  <div class="form-group">\
                    <p><small>Coloque aqui a palavra chave da promoção ou deixe em branco</small></p>\
                    <input type="text" name="promocao" value="" placeholder="Código" class="form-control" />\
                  </div>\
                  <span id="status-cadastro"></span>\
                  <div class="form-group">\
                    <span id="btn-cadastro" class="btn-cadastro">Cadastrar</span>\
                  </div>\
                </form>\
               </div>';

        var modal = {'close':true,'msg':{'title':'CADASTRE-SE','text':msg}}
            modalDefaultTemplate(modal);

        $('#btn-cadastro').click(function(){

            var msg   = '';
            var name  = $('#form-cadastro [name="name"]').val();
            var email = $('#form-cadastro [name="email"]').val();

            if(name == ''){
                msg+= 'Digite seu Nome<br>';
            }

            if(email == ''){

                msg+= 'Digite seu E-mail<br>';

            }else{

                if(!validateEmail(email)){

                    $('[name="email"]').css('borderColor','#ff0000');
                    go = false;
                    msg+= 'E-mail inválido<br>';

                }

            }

            if(msg == ''){

                $('#form-cadastro #status-cadastro').text('');
                    
                    params = $('#form-cadastro').serialize();
                    $.post('/newsletter',params, function(data){

                        $('#form-cadastro #status-cadastro').text(data['msg']);

                        if(data['code'] == 200){

                            var redir = '/newsletter/response';
                            document.location = redir;

                        }

                    },'json');

            }else{

                $('#form-cadastro #status-cadastro').html(msg);

            }

        });

    });
    </script>

