                getMoreInit("#" + name + " li", "#" + name + "GM", 5);
                $(this).attr('flag', false);
            }
            $(".qczx-l .menus li a").removeClass('active');
            $(this).find('a').addClass('active');
            $(".qczx-l .list").hide().eq(ind).show();
            $(".qczx-l .list").eq(ind).length
            if ($(".qczx-l .list").eq(ind).find('li').length <= 5) {
                $(".getMore").hide().eq(ind).hide();
            } else {
                $(".getMore").hide().eq(ind).show();
            }
        });

