					self.api.bind('close:finish', function () {
						self.offResize();
						// init window resize
						clearTimeout(self.parameters.event_timeout_id);
						self.parameters.event_timeout_id = setTimeout(
							function() {
								$('#vs-support-block-position').show();
							}, 10);
					});

					self.api.bind('open:finish', function () {
						self.parameters.is_opened = true;
					});

					self.api.bind('close:finish', function () {
						if (self.parameters.is_menu_auto_close) {
							$(window).trigger('court_resize_change_view_when_menu');
						}
						self.parameters.is_menu_auto_close = false;
						self.parameters.is_opened = false;
					});

					self.api.bind('close:before', function () {
						setTimeout(function() {
							self.jq_objects.button_icon.removeClass( "is-active" );
							self.jq_objects.button.removeClass('active');
						}, 150);
					});

					self.jq_objects.button.click(function () {
						if (self.parameters.is_opened) {
							self.api.close();
						} else {
							self.api.open();
						}

						return false;
					});

					/*self.api.bind('init:before', function () {
						console.log('imf');

					});*/
				};

				this.onResize = function () {
					self.parameters.is_resized = false;
					self.parameters.is_menu_auto_close = false;
					$(window).on('resize.new', function () {
						self.parameters.is_resized = true;

						if (window.innerWidth >= 1024) {
							self.parameters.is_menu_auto_close = true;
							self.api.close();
						}
					});
				};

				this.offResize = function () {
					if (self.parameters.is_resized) {
						$(window).trigger('court_resize_when_menu');
						//$(window).trigger('court_level');
					}
					$(window).off('resize.new');
				};

				// Run init
				this.init();
			};

			// Инициализируем класс
			new menuMobile();
		});
	})();
</script>

