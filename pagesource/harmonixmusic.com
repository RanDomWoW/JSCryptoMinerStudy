<script>
			function mm() {
				if(!$('.bars').hasClass('a')) {
					opn()
				} else {
					cls()
				}
			}	
			function opn() {
				console.log('open')
				$('header nav').addClass('a')
				setTimeout(function(){
					$('header nav').addClass('o')
				},300)
				$('.bars').addClass('a')
			}
			function cls() {
				console.log('closed')
				$('header nav').removeClass('o')
				setTimeout(function(){
					$('header nav').removeClass('a')
				},300)
				$('.bars').removeClass('a')
			}
			$('.bars').on("click", function(){
				mm()
			})
			$(window).resize(function(){
				var ww = $( window ).width()
				
				if(ww > 960) {
					$('header nav').removeClass('a o')
					$('.bars').removeClass('a')
				}	
			})
		</script>
