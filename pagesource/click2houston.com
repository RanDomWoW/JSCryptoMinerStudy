  <script type="text/javascript">
    /* start: FILTER CSS/VIEW CHANGE */
    (function($, A, modId){
            
          var $modFilteredList = $('#'+modId),
              $listTrigger = $modFilteredList.find('.trigger-list'),
              $gridTrigger = $modFilteredList.find('.trigger-grid'),
              activeCls = 'view-active';

          $listTrigger.add($gridTrigger).children().click(function(e) {
              e.preventDefault();

              var isGrid = $(this).parent().attr('id') === 'view-grid';

              $modFilteredList.children(".mod-body").toggleClass('grid', isGrid);
              $gridTrigger.toggleClass(activeCls, isGrid);
              $listTrigger.toggleClass(activeCls, !isGrid);
          });

           })(jQuery, AUI(), 'mod-filtered-story-list-114443051');
    /* end: FILTER CSS/VIEW CHANGE */

    (function($, A){
      var displayDaysUI = $('.filter-story-trending-tab');
      var displayDays = displayDaysUI.html();
      displayDaysUI.remove();

      var asyncQueue;
      var modId = 'mod-filtered-story-list-114443051';
      var pageSize = 4;
      var trendingIndex = 0;
      var adFrequency = 8;

            
      var $mod=$('#'+modId), mod=$mod.get(0), $win=$(window), $doc=$(document);
      var $footerLoader = $mod.children('.mod-footer');
      var loadMoreLoading = false;

      A.use('async-queue', function () {
        asyncQueue = new A.AsyncQueue();
      });

      var addToQueue = function( task, skipClearing ){
        /*lakana.utils.log('asyncQueue.size() : '+asyncQueue.size());*/
        task.timeout = (loadMoreLoading || asyncQueue.size()>0) ? 500 : 10;
        if( !skipClearing ){
          asyncQueue.stop();
        }
        asyncQueue.add(task);
        asyncQueue.run();
      };

      var appendParam = function(url, key, val){
        if(url.indexOf('?')===-1){
          url = url + '?';
        }
        return url + '&' + key + '=' + encodeURIComponent(val);
      };

      var setTabContentLoadState = function( config ){
        var c = A.mix({
          tab: null, /* jQuery element */
          loading: false,
          firstLoad: true,
          finished: false,
          loadMore: $('#load-more-'+config.tab.attr('id'))
        }, config, true);

        if( c.loading ){
          if( !c.firstLoad ){
            $footerLoader.show();
          }
          c.loadMore.hide();
        } else {
          $footerLoader.hide();
          c.tab.find('.loading-animation').remove();
          /*c.tab.find('.hidden').attr('class', 'load-more');*/
          if( c.finished ){
            c.loadMore.hide().data('finished', true);
          }
          if(c.finished === 0) {
            c.loadMore.hide().data('finished', true);
          }
        }
        loadMoreLoading = c.loading;
      };

       
            

      var getDiffDate = function(publishedDate){
        var timeString = "";
        var currentDate = new Date();
        var remain = (currentDate - publishedDate) / 1000;
        var diffDay = Math.floor(remain / (60 * 60 * 24));
        var diffHour = Math.floor(remain % (60 * 60 * 24) / (60 * 60));
        var diffMinute = Math.floor(remain % (60 * 60 * 24) % (60 * 60) / 60);

        if (diffDay == 0) {
          if (diffHour == 0) {
            if (diffMinute > 1) {
              timeString = diffMinute + " MINS";
            } else {
              timeString = diffMinute + " MIN";
            }
          } else {
            if (diffHour > 1) {
              timeString = diffHour + " HRS";
            } else {
              timeString = diffHour + " HR";
            }

          }
        } else {
          if (displayDays > diffDay) {
            if (diffDay > 1) {
              timeString = diffDay + " DAYS";
            } else {
              timeString = diffDay + " DAY";
            }
          } else {
            timeString = "";
          }
        }
        return timeString;
      };

      var rewriteLoadMoreAjaxUrl = function(tabId) {
        var $loadMore = $('#load-more-' + tabId);
        var $alink = $loadMore.children('a');
        var ajaxUrl = $alink.attr('ajaxUrl');
        var urlPart = ajaxUrl.split('autoflowId=');
        var autoflowId = $('#lazy-load-' + tabId).val();
        ajaxUrl = urlPart[0] + 'autoflowId=' + autoflowId + '&pageNum=';
        $alink.attr('ajaxUrl', ajaxUrl);
      };

      /*for tab*/
      var lazyLoadTab = function( $el, $oldEl ){
        var tabLoaded = $el.attr('tabLoaded');
        var tabId = $el.attr('href');

        if(tabLoaded == "false" && tabId.lastIndexOf('-') != -1) {
          $el.attr('tabLoaded', 'true');
          var tab = $(tabId);
          var index = tabId.lastIndexOf('-') + 1;
          var tabIndex = Number(tabId.substring(index));
          if(!isNaN(tabIndex)) {
            if(tabIndex == trendingIndex - 1) {
              /*Trending Tab*/
              var limit = pageSize * 2;
              console.error("Trove tab is no longer functional");
            } else {
              var toggleLoad = function(bool, finished){
                setTabContentLoadState({
                  tab: tab,
                  loading: bool,
                  firstLoad: true,
                  finished: finished
                });
              };
              toggleLoad(true);

              var ajaxUrl = $('#ajaxUrl-lazy-load-' + tabIndex).val() + tabIndex;
              console.log("ajaxUrl : " + ajaxUrl);
              $.ajax({
                type: "GET",
                url: ajaxUrl,
                data: {
                  categoryLayoutMap: $("#categoryLayoutMap_FSL").text(),
                  adDesktopWidth: 1000
                }
              }).done(function(html) {
                var $newContent = $(html);
                tab.prepend($newContent);
                rewriteLoadMoreAjaxUrl(tabId.substring(1));
                toggleLoad(false);

              }).fail(function() {
                lakana.utils.log("Filtered Story AJAX error.");
                toggleLoad(false);
              });
            }
          }
        }
      };
      var $tabHeaders = $mod.find('.mod-header').find('.filter-nav-list').find('[ga-action=Sort]');
        $tabHeaders.each(function(idx, tabPanelEl) {
          var $tabPanel = $(tabPanelEl);
          if (idx != 0) {
            $tabPanel.click(function(e) {
            lazyLoadTab($(tabPanelEl));
          })
        }
      });

      var $tabPanels = $mod.find('.mod-body > .tab-content').first().children('.tab-pane[id]');

      var checkScroll = function(e) {
        if( !loadMoreLoading ){
          var bottomToTop = mod.getBoundingClientRect().top + mod.offsetHeight;
          var pageHeight = $win.height();
          var foldDistance = pageHeight;
          var atBottom = (bottomToTop-foldDistance) <= pageHeight;
          if( atBottom ){
            var $loadMore = $tabPanels.filter('.active').find('.load-more');
            if( !$loadMore.data('finished') ){
              $loadMore.children('a').trigger('click');
            }
          }
        }
      };

      /* Load More */
      (function() {
        $tabPanels.each(function(idx, tabPanelEl) {
          var $tabPanel = $(tabPanelEl);
          var tabId = $tabPanel.attr('id');
          var $loadMore = $('#load-more-'+tabId);
          var $alink = $loadMore.children('a');

          $alink.click(function(e) {
            e.preventDefault();
            var ctx = this;
            var ajaxUrl = $alink.attr("ajaxUrl");
            var pageNum = parseInt($loadMore.attr("pageNum"));
            if(isNaN(pageNum)) {
              return;
            }
            var index = tabId.lastIndexOf('-') + 1;
            var tabIndex = Number(tabId.substring(index));

            var loadMore = function(){
              pageNum = parseInt($loadMore.attr("pageNum"));

              if(tabIndex == trendingIndex - 1) {
                /* Trending */
                var limit = pageNum * pageSize;
                callTrove(tabId, limit, false);
                pageNum++;
                $loadMore.attr("pageNum", pageNum);
              } else {
                var toggleLoad = function(bool, finished){
                  setTabContentLoadState({
                    tab: $tabPanel,
                    loading: bool,
                    firstLoad: false,
                    finished: finished,
                    loadMore: $loadMore
                  });
                };
                toggleLoad(true);
                console.log( "Load More" );
                var url = ajaxUrl + pageNum + "&tabId=" + tabId;
                $.ajax({
                  type: "GET",
                  url: url,
                  data: {
                    categoryLayoutMap: $("#categoryLayoutMap_FSL").text(),
                    adDesktopWidth: 1000
                  }
                }).done(function(html) {
                  pageNum++;
                  var $newContent = $(html);
                  $loadMore.attr("pageNum", pageNum).before( $newContent );
                  var finished = ($newContent.filter("article").length && $newContent.filter("article").length < pageSize);
                  toggleLoad(false, finished);

                  checkScroll();

                }).fail(function() {
                  lakana.utils.log("Filtered Story AJAX load more error.");
                  toggleLoad(false);
                });
              }
            };

            if( asyncQueue ){
              addToQueue({
                fn: loadMore,
                context: ctx
              });
            } else {
              loadMore();
            }
          });
        });
      })();

      /* Infinite Scroll */
      $doc.ready(function () {
        $win.bind('scroll', checkScroll).bind('load', checkScroll);
        checkScroll();
      });
           })(jQuery, AUI());
  </script>
