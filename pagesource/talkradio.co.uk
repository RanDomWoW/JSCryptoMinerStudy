<script>function listenLive(url){
url1 = 'http://talksport.com/radioplayer/live/?popup=1';
url2 = 'http://talksport.com/radioplayer/live/talksport2.html?popup=1';
url3 = '/radioplayer/live/talkradio.html?popup=1';
url4 = 'http://virginradio.co.uk/radioplayer/live/virginradio.html?popup=1';

var station;

switch (url) {
    case 1:
        station = url1;
        break;
    case 2:
        station = url2;
        break;
    case 3:
        station = url3;
        break;
    case 4:
        station = url4;
        break;
}
    var windowName = "listenLiveWindow";
    var windowSize = "width=380,height=665,location=0,status=0,titlebar=0,toolbar=0";
  
if(navigator.userAgent.match(/iPhone|iPod|iPad|webOS|BlackBerry|Nokia|Android/i)==null){ window.open(station, windowName, windowSize);}
  else{ window.location.href = station; }
}
</script>
