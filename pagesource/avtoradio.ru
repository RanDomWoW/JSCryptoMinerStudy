                            onShow: function (element) {
                                var self = this;
                                setTimeout(function () {
                                    self.close();
                                    localStorage.setItem("userPoll", "true");
                                }, 2000);
                            },
                        }
                    
                };
              
            window.userpoll = new FormsPicker({
                sendDataStep : 2,
                url : "/api/interview/setAnswer",
            });
            
            userpoll.makeRequest = function () {
                var self = this;
                this.ajax = $.ajax({
                    type: "POST",
                    url: this.options.url,
                    data: {id_interview: $("#interview_form [name='id_interview']").val(), questions : this.formsData},
                    dataType: "json"
                });
            
                this.ajax.success(this.successAjax.bind(this));
                this.ajax.error(this.initError.bind(this));
            };
        }
    })
</script>
 
        
   
