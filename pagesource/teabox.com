<script>
  $(".btn-search").click(function () {
    //$('.search-form input').css("opacity": "1", "visibility": "visible");
    $('.search-done').toggleClass('search-done-active');
    $('.close_search').toggleClass('close-active');
  });
  $(".close_search").click(function () {
    //$('.search-form input').css("opacity": "1", "visibility": "visible");
    $('.search-done').removeClass('search-done-active');
    $('.close_search').removeClass('close-active');
//     $(".search").css({ fill: "#fff" });
  });

  //search functionality

  function registerSearch() {
      $(".btn-search").click(function(event) {
          event.preventDefault();
          if (!$('search-input').val()) {
              if (!$('#search-input').hasClass('active')) {
                  $('#search-input').addClass('active');
                  $('.close_btn').css('visibility', 'visible');
                  $('.float_search_icon svg').css('fill', '#333');
                  setTimeout(function() {
                      $('#search-input').focus();
                  }, 100);
              } else {
                  $('#search-input').removeClass('active');
                  $('.close_btn').css('visibility', 'hidden');
                  $('.float_search_icon svg').css('fill', '#fff');
              }
          } else {
              $('#search-en').submit();
          }
      });
      $("#search-input").bind("paste keyup", function() {
          var value = $("#search-input").val(),
              storeId = $("#search-input").data('store-id'),
              locale;
          (storeId === 1) ? locale = 'ru': locale = 'en';
          $(".search-res-wrap").remove();
          if (value.length >= 3) {
              $.ajax({
                  url: window.suggestUrl + storeId + '?q=' + value + '&l=' + locale,
                  method: 'get',
                  success: function(d) {
                      var data = d.results;
                      var autoCompHtml = '<ul class="search-res-wrap">';
                      for (var i = 0; i < data.length; i++) {
                          var dat = data[i];
                          autoCompHtml += '<a href="' + dat.link + '"><li class="search-res"><span class="search-res-title">' + dat.title + '</span><span class="search-res-count"></span></li></a>';
                      }
                      autoCompHtml += '</ul>';
                      $(".search-form").append(autoCompHtml);
                  }
              });
          }
      });
      $('.login-wrapper input').click(function(event) {
          event.stopPropagation();
      });
  }

  registerSearch();
  
  if(document.getElementById("time-left-ticker")){
        var endTime = new Date(document.getElementById("time-left-ticker").value);
        var timeLeft = (endTime.getTime() - new Date().getTime())/1000;
         if(timeLeft>0){
             setInterval(function(){
                 if(timeLeft >0){
                     timeLeft--;
                     var hoursLeft = parseInt(timeLeft/3600);
                     var minsLeft = parseInt((timeLeft%3600)/60);
                     var secsLeft = parseInt((timeLeft%3600)%60);
                     $(".header-timer span.hours").text((hoursLeft<10)?"0"+hoursLeft:hoursLeft);
                     $(".header-timer span.minutes").text((minsLeft<10)?"0"+minsLeft:minsLeft);
                     $(".header-timer span.seconds").text((secsLeft<10)?"0"+secsLeft:secsLeft);
                 }else{
                     $(".header-timer").addClass("hidden");
                 }
             },1000);
         }else{
             $(".freedom-flag").addClass("hidden");
         }
   }
</script>
