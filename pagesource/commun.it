    });

    var fourthTwitterSignIn = jQuery('#fourth-twitter-login');
    fourthTwitterSignIn.click(function () {
      if (typeof $(this).attr('no-signin') !== "undefined") {
        return;
      } else {
        UserAction.gaEvent(window.ga_avent_category, 'SignInWithTwitter4_click', 'SignInWithTwitter4_click');
        return socialSignIn(fourthTwitterSignIn);
      }
    });

    var haederTwitterSignIn = jQuery('#twitter-sign-in-haeder');
    haederTwitterSignIn.click(function () {
      if (typeof $(this).attr('no-signin') !== "undefined") {
        return;
      } else {
        UserAction.gaEvent(window.ga_avent_category, 'SignInWithTwitterHeader_click', 'SignInWithTwitterHeader_click');
        return socialSignIn(haederTwitterSignIn);
      }
    });

    var facebookSignIn = jQuery('#facebook-sign-in');
    facebookSignIn.click(function () {
      UserAction.gaEvent(window.ga_avent_category, 'SignInWithFacebook_click', 'SignInWithFacebook_click');
      return socialSignIn(facebookSignIn);
    });

    var fbPromoLink = jQuery('#fb-promo-link');
    fbPromoLink.click(function () {
      UserAction.gaEvent(window.ga_avent_category, 'SignInWithFacebook-promo_click', 'SignInWithFacebook-promo_click');
      return socialSignIn(fbPromoLink);
    });


    var InstaSignIn = jQuery('#instagram-sign-in');
    InstaSignIn.click(function () {
      UserAction.gaEvent(window.ga_avent_category, 'SignInWithInstagram_click', 'SignInWithInstagram_click');
      return socialSignIn(InstaSignIn);
    });
  });

  function socialSignIn(that) {
    if (typeof($) == 'undefined') {
      $ = jQuery
    }

    var communit_mbsy_short_code = ((typeof(mbsy_short_code) !== 'undefined') ? mbsy_short_code : '');
    var hosted_app_external_uid_val = ($('#hosted_app_external_uid').length > 0 ? $('#hosted_app_external_uid').val() : '');
    var hosted_app = '';
    if ($('#hosted_app_registration').length > 0) {
      hosted_app = $('#hosted_app_registration').val() == 'true';
    }
    href = that.attr('href');
    href += '?hosted_app_registration=' + hosted_app;
    href += '&hosted_app_external_uid=' + hosted_app_external_uid_val;
    href += '&communit_mbsy_short_code=' + communit_mbsy_short_code;
    if (Communit.Vars.optimizely_variation() != '') {
      href += '&hp_variation=' + Communit.Vars.optimizely_variation();
    }
    href += '&sign_in_origin=' + document.location.pathname.replace(/\//g, "-");
    href += '&social_network=' + that.attr('social-network');
    //check if its an extension login
    if ($('#ext_login').length > 0 && $('#ext_login').val() == 'true') {
      ext_login = true;
      href += '&ext_login=true'
    } else {
      href += '&ext_login=false'
    }
    that.attr('href', href);


    if (hosted_app || typeof(is_hootsuite_app) != 'undefined' || ext_login) {
      return popupWindow(href, 'social_authentication');
    } else {
      window.location = href;
      return false;
    }
  }

  function redirectToDashboard() {
    window.location = '/quick_actions'
  }

  function twitterAuthCompletedHootsuite() {
    window.location = '/hootsuite/dashboard';
  }

  function twitterAuthCompleted(twitterUsername) {
    redirectToDashboard();

  }
</script>

