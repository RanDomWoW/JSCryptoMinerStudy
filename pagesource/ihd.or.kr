	function getCookie(cname){
	  var cookies = document.cookie.split("; ");
	  for( i=0; i<cookies.length ; i++){
		var cookie1= cookies[i].split("=");
		if( cname == cookie1[0] && cookie1[1] != null ) {
		  return unescape(cookie1[1]);
		}
	  }
	  return "";
	}
