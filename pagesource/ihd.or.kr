<script type="text/javascript">
 
/* main calendar */
function calendar(year,month,day){
	
	var week_start = new Date(year,month-1,1).getDay();
	var day_max = get_day_max(year,month-1);
	var i = 0;
	var j = 0;
	var html = "<tr>\n"; 
	
	//console.log(month);

	$( "#nowYear" ).attr({
		  src: "/common2/examHome/image/main/"+year+".gif",
		  alt: year
	});
	$( "#nowMonth" ).attr({
		  src: "/common2/examHome/image/main/"+month*1+"month.gif",
		  alt: month
	});
	
	var list;
	
	
	$.ajax({
		async: false,
		type: "post",
		url: "examschedulelist.do",
		data: "exam_div=01&date="+year+month,
		success:function(data){
			var ospData = eval( '(' + data + ')' );
			
			if(ospData.result.code > -1){
				list = eval("("+ospData.result.etc+")");	
			}
			
		}
	});
	
	while (j < day_max){
		if (i < week_start) {
		 html += "\n<td><img src=\"/common2/examHome/image/main/dot.gif\"/></td>\n";
		} else {
	        if(i%7==0){ 
                font_color = " class='sun'"; 
	        }else if(i%7==6){
                font_color = " class='sat'";
	        }else{
                font_color = "";	
	        }
			
	        var codeData = list.list[j];
	        
            if (codeData.accept != null && codeData.accept == "Y"){ font_color = " class='today2'";}
            if (codeData.exam != null && codeData.exam == "Y"){ font_color = " class='today'";}

            if((codeData.accept != null && codeData.accept == "Y") && (codeData.exam != null && codeData.exam == "Y")){ font_color = " class='today3'";}

            html += "\n<td"+font_color+">\n";
	
            if(codeData.accept=="Y"){
	            html += "<a href=\"memaccept1.do\">\n";
            } else{
	            html += "<a href=\"#\">\n";
            }
	
            html += (j+1) +"\n";

            html += "</a>";

            html += "\n</td>";

            //html += "\n<td align=\"center\" class=\"calendar\">\n"+"	<div style ='cursor:hand;font color='"+font_color+";' onClick = \"javascript:eg_cal("+(j+1)+")\";>\n"+"		<font color='"+font_color+"'>"+ (j+1) + "\n	</div>\n</td>";

            /* result = "";
            examResult = ""; */
            font_color = "";
            j ++;
		}

		if(i%7==6){
	        if(j+1 == day_max){
                html += "\n</tr>\n";
	        }else{
	            html += "\n</tr>\n"+"<tr>\n";
	        }
		}
        i ++;
	}

	while (i%7!==0){
	    html += "\n<td><img src=\"/common2/examHome/image/main/dot.gif\" /></td>\n";
	    i ++;
	}
    html += "</tr>";
    $('#main_calendar tbody').html(html);

}

function get_day_max(year,month){
	var i = 29, cday;
	while(i<32){
	    cday = new Date(year,month,i);
	    if (cday.getFullYear()!=year || cday.getMonth()!=month) break;
	    i++;
	}
	return i-1;
}

function get_number_str(num){
	if (num<10) num = '0' + num;
	return num;
}

$(function () {
	
	var now = new Date();

	var year= now.getFullYear();
    var month = (now.getMonth()+1)>9 ? ''+(now.getMonth()+1) : '0'+(now.getMonth()+1);
	var day = now.getDate()>9 ? ''+now.getDate() : '0'+now.getDate();
	
	calendar(year,month,day);
	
	$("#eg_pre").click(function(event){
		event.preventDefault();
		
		var year = $("#nowYear").attr("alt");
	    var month = $("#nowMonth").attr("alt");
	    
		if( Number(month) == 1 ){
			year = Number(year)-1;
			month = '12';
		}else{
			month = Number(month)-1;
		}
		calendar(year, month > 9 ? ''+month : '0'+month, day);
	});

	$("#eg_next").click(function(event){
		event.preventDefault();
		
		var year = $("#nowYear").attr("alt");
	    var month = $("#nowMonth").attr("alt");
	    
		if( Number(month) == 12 ){
			year = Number(year)+1;
			month = '1';
		}else{
			month = Number(month)+1;
		} 
		calendar(year,month > 9 ? ''+month : '0'+month,day);
	});
	
});


