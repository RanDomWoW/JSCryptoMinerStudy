		this.actPOS=pos;
		var srcs=[],src=this.prefSrc;
		for(var j=0;j<this.sources.length;j++){if(this.sources[j].src==this.prefSrc){srcs.push(this.sources[j]);break}}
		for(j=0;j<this.sources.length;j++){if(this.sources[j].src==this.actSrc){srcs.push(this.sources[j]);break}}
		for(j=0;j<this.sources.length;j++){if(this.sources[j].src!=this.prefSrc&&this.sources[j].src!=this.actSrc){srcs.push(this.sources[j])}}
		if(pos!=''){
			for(j=0;j<srcs.length;j++){if(srcs[j].parts[pos]==1){src=srcs[j].src;break}}
		}
		if(src!=this.actSrc){this.showSrc(src);return}
		this.chSrcPos(pos);
		var ps=document.querySelectorAll('section[data-src="'+this.actSrc+'"] div[data-part]');
		var bls=[];
		for(var i=0;i<ps.length;i++){
			var it=ps[i],itp=it.getAttribute('data-part');
			var vis=(itp==pos||pos=='');
			it.style.display=(vis?'block':'none');
			if(vis)bls.push(it.firstElementChild||it.firstChild);
		}
		this.showBls(bls);
		for(i=0;i<tabs.length;i++){
			it=tabs[i],itp=it.getAttribute('data-part');
			it.className=lib.trim(it.className.replace('psactive',''))+(itp==pos?' psactive':'');
		}
	};
	this.showBls=function(bls){
		for(var i=0;i<bls.length;i++){
			var b=bls[i],cls=lib.trim(b.className.replace('expanded','').replace('collapsed',''));
			this.exCol(b,i<3);
		}
	};
	this.hShowSrc=function(tg){
		var clsn=tg.className||"";
		if(clsn.indexOf('srcactive')>-1||clsn.indexOf('srcdisabled')>-1)return;
		var src=tg.getAttribute('data-target-src');
		th.prefSrc=src;
		if(lib.isStor())localStorage.setItem("prefSrc",src);
		th.showSrc(src);
	};
	this.showSrc=function(src){
		var secs=document.querySelectorAll('#MainTxt section');
		for(var i=0;i<secs.length;i++){secs[i].style.display=(secs[i].getAttribute('data-src')==src?'block':'none')}
		th.actSrc=src;
		th.showPOS(this.actPOS);
	};
	this.srcTit=function(s){
		switch(s){case 'hc_thes':return'Collins';case 'wn':return'WordNet';case 'hm_thes':return'Roget\'s';case 'open_thes':return'OpenThesaurus';case 'vox_thes':return'Larousse';default:return''}
	};
	this.getParts=function(src){
		var r={};
		var tps=document.querySelectorAll('section[data-src="'+src+'"] div[data-part]');
		for(var i=0;i<tps.length;i++){var it=tps[i],itp=it.getAttribute('data-part');r[itp]=1}
		return r;
	};
	this.isSrcEx=function(src){
		for(var i=0;i<this.sources.length;i++){if(this.sources[i].src==src)return 1}
		return 0;
	};
	this.initSrc=function(){
		var sech=document.querySelectorAll('#MainTxt section h2');
		var tsrc=[],src1='';
		for(var i=0;i<sech.length;i++){
			var src=sech[i].parentNode.getAttribute('data-src');
			if(!src1)src1=src;
			tsrc.push(src);
			var p=this.getParts(src);
			this.sources.push({src:src,parts:p});
		}
		var inSrc=(sech.length>1&&lib.isStor()?localStorage.getItem("prefSrc"):"");
		if(!this.isSrcEx(inSrc))inSrc="";
		this.actSrc=src1;
		this.prefSrc=inSrc||src1;
		if(sech.length<2)return;
		for(i=0;i<sech.length;i++){
			var h=sech[i];
			var srcSw='<ul class="thsrc">';
			for(var j=0;j<tsrc.length;j++){srcSw+='<li '+(i==j?'class="srcactive"':'')+' onclick="thes.hShowSrc(this)" data-target-src="'+tsrc[j]+'">'+this.srcTit(tsrc[j])+'</li>'}
			srcSw+='</ul><div style="clear:both"></div>';
			h.outerHTML=h.outerHTML.replace('<h2>','<h2 style="float:left">')+srcSw;
		}
		this.showSrc(src1);
	};
	this.initMean=function(){
		var bls=[];
		for(var i=0;i<mean.length;i++){
			var w=mean[i];
			var f=w.parentNode.getAttribute('data-part-full');
			w.innerHTML='<span class="TPSP"'+(f?' title="'+f+'"':"")+'>'+w.parentNode.getAttribute('data-part')+'</span>'+w.innerHTML;
			bls.push(w);
			lib.event.add(w,'click',function(e){
				e=e||window.event;
				var tgt=e.target||e.srcElement;
				th.exCol(tgt,tgt.className.indexOf('collapsed')>-1);
			});
		}
		this.showBls(bls);
	};
	this.initTabs=function(){
		var tab1=null;
		for(var i=0;i<tabs.length;i++){
			var tb=tabs[i];
			if(tab1===null)tab1=tb.getAttribute('data-part');
			lib.event.add(tb,'click',function(e){
				e=e||window.event;
				var tgt=e.target||e.srcElement;
				var tpos=tgt.getAttribute('data-part');
				th.showPOS(tpos);
			});
		}
		this.showPOS(tab1);
	};
	this.initAct=function(){
		var ws=document.querySelectorAll('.TCont li .tw');
		if(ws.length==0)return;
		for(var i=0;i<ws.length;i++){
			var wsi=ws[i],wsp=wsi.parentNode,wp=encodeURIComponent(wsi.innerText);
