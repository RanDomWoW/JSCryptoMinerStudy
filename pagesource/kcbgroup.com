    <script type="text/javascript">
            jQuery(document).ready(function(){
                jQuery('.timeline-slider').bxSlider({
                    slideWidth: 150,
                    minSlides: 3,
                    maxSlides: 8,
                    slideMargin: 0,
                    moveSlides: 2,
                    pager: false,
                    nextSelector: '#timeline-next',
                    prevSelector: '#timeline-prev',
                    nextText: '<span class="icon-angle-right"></span>',
                    prevText: '<span class="icon-angle-left"></span>',
                    infiniteLoop: false
                });
    
                function theClickCallback(el) {
                    var year = el.data('filter');
                    console.log(year);
                     //alert(year);
                     jQuery('.timeline-container > div').addClass('hide');
                     jQuery('.timeline-container > div[data-filter="'+year+'"]').removeClass('hide');
                     if (year=='.year-2018'){
                        // alert('2018');
                        jQuery('.timeline-container > div').removeClass('hide');
                     }
                }
    
                jQuery('.timeline-slide > a').click( function(e) {
                    e.preventDefault();
                    var year = jQuery(this).data('filter');
                    //alert(year);
                    jQuery('.timeline-slide.active').removeClass('active');
                    jQuery(this).parent('.timeline-slide').addClass('active');
                    theClickCallback(jQuery(this));
                });
    
            });
        </script>
        
    

