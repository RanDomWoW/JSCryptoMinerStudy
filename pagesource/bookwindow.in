function check_valid_coupon(product_ids,sub_total,total_weight,shipping_cost){
    var coupon_val;
    var pay;
    coupon_val = document.getElementById("coupon").value;
    pay =  $("input:radio[name=pay]:checked").val();//document.getElementById("pay").selected;
    if(window.XMLHttpRequest)
    {// code for IE7+, Firefox, Chrome, Opera, Safari
	  xmlhttp=new XMLHttpRequest();
    }
	else
	  {// code for IE6, IE5
	  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
	  }
	xmlhttp.onreadystatechange=function()
	  {
	  if (xmlhttp.readyState==4 && xmlhttp.status==200)
	    {
	    document.getElementById("coupon_val").innerHTML=xmlhttp.responseText;
	    }
	  }
	xmlhttp.open("GET","ajaxcheckcoupon.php?coupon_val="+coupon_val+"&product_ids="+product_ids+"&sub_total="+sub_total+"&total_weight="+total_weight+"&shipping_cost="+shipping_cost+"&pay="+pay,true);
	xmlhttp.send();
}

