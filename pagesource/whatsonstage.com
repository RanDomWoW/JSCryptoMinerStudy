<script>
/* set up each template */
function tplFeatureStory(data){
    var html  = _.template('<div class=\"carousel-top-story\" style=\"background-image: url(<%=slide.background %>)\"><div><span class=\'about\'><%=slide.about %> </span><span class=\'headline\'><%=slide.headline %></span><span class=\'snipe\'><%=slide.snipe %></span></div></div>', data, {variable: "slide"});
    var link = data.href;

    if(link.indexOf('?') == -1) {
        link = link + '?cid=' + data.cid;
    }
    else {
        link = link + '&cid=' + data.cid;
    }

    return { "markup": html, "link": link};
}

function tplOutings(data){
    var html = _.template("<%_.each(slide, function(outing) { outing.class = outing.name.length > 30 ? 'outing-title outing-title-small' : 'outing-title'; %> <div class=\"club-outing\"><img alt=\"<%=outing.name %>\" src='<%=outing.src %>' /><span class=\"<%=outing.class %>\"><%=outing.name %></span><span class='outing-type'><%=outing.type %> </span><span class='outing-date'><%=outing.date %></span><span class='outing-price'><%=outing.price %></span></div><%}); %>", data, {variable: 'slide'});
    return { "markup": html, "related_link_title": "Theatre Club", "related_link": "http://www.theatreclub.co.uk"};
}

function tplHtml(data){
    var html = _.template("<div class=''><%=slide.html %></div>", data, {variable: 'slide'});
    return { "markup": html };
}

function tplSingleFeaturedShow(data){
    if(typeof data[1] != "undefined"){
        var max = data.length - 1;
        data = data[_.random(0, max)];
    }

    var html = _.template("<div class='carousel-featured-show' style='background-image: url(<%=slide.background %>)'><div><a href='<%=slide.tickets %>'><img alt='<%=slide.title %>' src='<%=slide.poster %>' /></a><span class='title'><%=slide.title %></span><span class='location'><%=slide.location %></span><span class='synopsis'><%=slide.synopsis %></span><span class='buy-tickets'><a href='<%=slide.tickets %>'><%=slide.tickets_text %></a></span></div></div><div style='display: none;'><%=slide.pixel %></div>", data, {variable: 'slide'});
    return {"markup": html };
}

function tplDoubleFeaturedShow(data){
    var html = _.template("<%_.each(slide, function(show) { %> <div class='featured-show'><a href='<%=show.tickets %>'><img alt=\"<%=show.title %>\" src=\"<%=show.poster %>\" /></a><span class='title'><%=show.title %></span><span class='location'><%=show.location %></span><span class='synopsis'><%=show.synopsis %></span><span class='buy-tickets'><a href='<%=show.tickets %>'><%=show.tickets_text %></a></span></div> <div style='display: none;'><%=show.pixel %></div><% }); %>", data, {variable: 'slide'});
    return {"markup": html};
}

function tplShowList(data){
    cid = data.cid;
    data = showsForTab(data);
    totalPosters = 6;
    $("div#carousel-tracking-pixels").html("");

    for (var i = 0; i < totalPosters; i++) {
        var index = parseInt(i) + 1,
        show = data[i],
        anchor = $("div#carousel-posters").find("a#carousel-poster-" + index + "-link"),
        container = anchor.closest(".carousel-poster"),
        image = anchor.find("img"),
        bottomText = anchor.find("div.carousel-poster-bottom-text"),
        pixels = $("div#carousel-tracking-pixels"),
        href;

        if (typeof show == "undefined") {
            container.addClass("hidden");
        }

        else {
            href = show.href;
            if (cid !== "") {
                href += "?cid=" + cid;
            }
            anchor.attr("href", href);
            image.attr({
              src: show.src,
              alt: show.text,
              title: show.text
            });

            if (typeof show.pixel != "undefined") {
                var current = show.pixel.replace(/\[timestamp\]/, new Date().getTime().toString());
                pixels.html(pixels.html() + current);
            }
            /*
            if (typeof show.bottom_text == "undefined" || show.bottom_text == "") {
              bottomText.find("span").html("");
              bottomText.addClass("hidden");
            }
            else {
              bottomText.find("span").html(show.bottom_text);
              bottomText.removeClass("hidden");
            }
            */
            container.removeClass("hidden");
        }
    }
}

function loadTab(data){

  var $topstory = $("#carousel-feature-story"),
      $cluboutings = $("#carousel-club-outings"),
      $showlist = $("#carousel-posters"),
      $rawhtml = $("#carousel-html"),
      $featured_show = $("#carousel-featured-show"),
      $featured_shows = $("#carousel-featured-shows"),
      $related = $("#carousel-related-link");

  var templateMapping = {
    "top_story": {
      "tpl": tplFeatureStory,
      "sel": $topstory
    },
    "outings":{
      "tpl": tplOutings,
      "sel": $cluboutings
    },
    "shows": {
      "tpl": tplShowList,
      "sel": $showlist
    },
    "raw_html":{
      "tpl": tplHtml,
      "sel": $rawhtml
    },
    "featured_show":{
      "tpl": tplSingleFeaturedShow,
      "sel": $featured_show
    },
    "featured_show_double":{
      "tpl": tplDoubleFeaturedShow,
      "sel": $featured_shows
    }
  };

  for(var i in templateMapping){
    if(data.hasOwnProperty(i)){
      // show the tab.
        if(i != "shows"){
            data[i].cid = data['cid'];
            data[i].slide = data['slide'];
            tpl = templateMapping[i].tpl(data[i]);
            sel = templateMapping[i].sel;

            if(tpl.hasOwnProperty('link')){
              sel.off();
              sel.on("click", function(){
                window.location = tpl.link;
              });
            }
            sel.html(tpl.markup);
            sel.removeClass("hidden");
        }

        else {
            sel = templateMapping[i].sel;
            tpl = {};
            tplShowList(data);
            sel.removeClass("hidden");
          }

        if(data.hasOwnProperty('related_link')){
            $related.html("<a href='"+data['related_link']+"?cid="+data['cid']+"'>"+data['related_link_title']+"</a>");
            $related.show();
        }
        else if(tpl.hasOwnProperty('related_link')){
            $related.html("<a href='"+tpl.related_link + "?cid="+data['cid']+"'>"+tpl.related_link_title+"</a>");
            $related.show();
        }
        else {
            $related.hide();
        }
    }

    // hide the other tabs.
    else {
        sel = templateMapping[i].sel;
        sel.addClass("hidden");
    }
  }
}
function shuffle(myArray) {
        var i = myArray.length;
        if ( i == 0 ) return false;
        while ( --i ) {
            var j = Math.floor( Math.random() * ( i + 1 ) );
            var tempi = myArray[i];
            var tempj = myArray[j];
            myArray[i] = tempj;
            myArray[j] = tempi;
        }
        return myArray;
    }


function showsForTab(tab) {


    var slideNum = tab.slideNum;

    // don't rebuild the show list for this slide if we already have it
    if(typeof(window.finalShows) != "undefined" && window.finalShows[slideNum]){
        return window.finalShows[slideNum];
    }

    var totalPosters = 6,
    showData = tab.shows,
    finalShows = showData.featured || showData.required;



    if (finalShows[0].length < totalPosters && finalShows[0].length > 0) {
        var otherShows = shuffle(showData.standard),
            neededShows = showData.featured ? totalPosters - finalShows[0].length : totalPosters,
            finalShows = showData.featured ? finalShows[0].concat(otherShows.slice(0, neededShows)) : finalShows.concat(otherShows.slice(0, neededShows));
    }

    else if (finalShows[0].length == 0){
        finalShows = shuffle(showData.standard);
    }
    else {
      finalShows = finalShows[0];
    }

    for (var i in finalShows) {
        if (finalShows[i] instanceof Array) {
            var randomIndex = Math.floor( Math.random() * ( i + 1 ) );
            finalShows[i] = shuffle(finalShows[i])[randomIndex];
        }


    }

    window.finalShows = window.finalShows || {};
    window.finalShows[slideNum] = shuffle(finalShows);

    return window.finalShows[slideNum];
}


function showCarousel(){
    $('#carousel-wrapper').removeClass("hidden");
