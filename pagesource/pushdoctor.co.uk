<script type="text/javascript">

$(document).ready(function(){
    try {
      processUTM();
      processAwinSource();
    }
    catch(e) {
        console.log("Error sending UTM information.");
        console.log(e);
    }
});

function processAwinSource(){
    var source = getParameterByName("source");
    var url = window.location.href;
    if(url.toLowerCase().indexOf("ppc") != -1){
        $.cookie("source", "ppc", { expires: 30, domain: '.pushdoctor.co.uk', path: '/' });
    }else{
        if(isSet(source)){
            $.cookie("source", source, { expires: 30, domain: '.pushdoctor.co.uk', path: '/' });
        }
    }
}

function processUTM(){
    var utm_source = getParameterByName("utm_source"),
    utm_medium = getParameterByName("utm_medium"),
	utm_campaign = getParameterByName("utm_campaign"),
	utm_term = getParameterByName("utm_term"),
	utm_content = getParameterByName("utm_content"),
	utmIdentifierCookie = $.cookie("utmIdentifier"),
	utmIdentifier = Math.random().toString(16).slice(2);
	if(isSet(utmIdentifierCookie)){
		utmIdentifier = utmIdentifierCookie;
	}
	if(
		(
			isSet(utm_source) ||
			isSet(utm_medium) ||
			isSet(utm_campaign) ||
			isSet(utm_term) ||
			isSet(utm_content)
		) &&
			isSet(utmIdentifier)
	){
		var data = {
			utmIdentifier:utmIdentifier,
			source:utm_source,
			medium:utm_medium,
			campaign:utm_campaign,
			term:utm_term,
			content:utm_content
		}
		$.ajax({
    		url: "https://svcs.pushsvcs.com/general.svc/generalW/AddUTMCampaign",
    		headers: {
    			"Content-Type": "application/json",
    			"Accept": "application/json"
    		},
    		data: JSON.stringify(data),
    		type: "POST",
    		dataType: "json",
		})
		.done(function (json) {
			$.cookie("utmIdentifier", utmIdentifier, { expires: 100, domain: '.pushdoctor.co.uk', path: '/' });
		})
	}	
}

function isSet(variable){
	if (typeof variable !== "undefined" && variable !== "") {
		return true
	}
	return false;
}

function getParameterByName(name, url) {
    if (!url) url = window.location.href;
    name = name.replace(/[\[\]]/g, "\\$&");
    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
        results = regex.exec(url);
    if (!results) return '';
    if (!results[2]) return '';
    return decodeURIComponent(results[2].replace(/\+/g, " "));
}

