    function cycleThroughStrBottomMsgs(objStrBottom, arrStrBottom){
        var arrStrBottomLength = arrStrBottom.length;
        if(arrStrBottomLength > 0){
            
             
              
              
            arrStrBottomCount = arrStrBottomCount + 1;
            if(arrStrBottomCount > arrStrBottomLength){
                arrStrBottomCount = 1;
            }
            
             objStrBottom.fadeOut(function(){
                 $(this).html(arrStrBottom[arrStrBottomCount - 1].strBottom)
             }).fadeIn("slow");

            setTimeout(function(){
                    cycleThroughStrBottomMsgs(objStrBottom, arrStrBottom)        
            },10000)
        }
    }
