    <script>

        $(document).ready(function () {
            $("#email").val("");
            $("#email").change(function () {
                $(".message").css('display', 'none');
            });
            $("#subscribe-btn").click(function () {
                $(".message").css('display', 'none');
            });

            $("#lang").change(function () {
                var id = $(this).val();

                var local = false;

                if (local) {
                    $.ajax({
                        url: "/Home/ChangeCurrentCulture",
                        type: 'GET',
                        cache: false,
                        data: { id: id },
                        success: function(result) {
                            location.reload();
                        }
                    });
                } else {
                    if (id == "fa-IR") {
                        window.location.href = "https://www.en45.com/";
                    } else {
                        $.ajax({
                            url: "/Home/ChangeCurrentCulture",
                            type: 'GET',
                            cache: false,
                            data: { id: id },
                            success: function(result) {
                                location.reload();
                            }
                        });
                    }
                }
            });

            function getLanguageCookie() {
                var cookies = document.cookie.split(';');
                for (var i = 0; i < cookies.length; i++) {
                    var cookie = cookies[i].split('=');
                    if (cookie[0].trim() === "langCookie")
                        return cookie[1].trim();
                }
                return '';
            }

            var lang = getLanguageCookie();
            switch (lang) {
                case "en-US":
                    $("select").val("en-US");
                    break;
                case "th-TH":
                    $("select").val("th-TH");
                    break;
                case "tr-TR":
                    $("select").val("tr-TR");
                    break;
                case "fa-IR":
                    $("select").val("fa-IR");
                    break;
                default:
                    $("select").val("en-US");
            }
        });

    </script>
