<script type="text/javascript">
    var nlCookieName = 'bucatars_nl_popup_facebook_new_christmass';
    var newsletterInterval = null;
    var sideBarNlOpen      = false;
    function nlIsValidEmail(email) {
        filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
        return filter.test(email);
    };

    function cookieSet(name, value,  days) {
        expires = "";
        if (days) {
            date = new Date();
            date.setTime(date.getTime()+(days*24*60*60*1000));
            expires = "; expires="+date.toGMTString();
        } else {
            expires = "";
        };
        document.cookie = name+"="+value+expires+"; path=/";
    };

    function cookieGet(name) {
        nameEQ = name + "=";
        ca = document.cookie.split(';');
        for(i=0;i < ca.length;i++) {
            c = ca[i];
            while (c.charAt(0)===' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) === 0) return c.substring(nameEQ.length,c.length);
        };
        return null;
    };

    function cookieDelete(name) {
        cookieSet(name,"",-1);
    };

    function urlParameter(URLparameter) {
        returnText   =  '';
        URLparameter = URLparameter.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
        regexS       = "[\\?&]"+URLparameter+"=([^&#]*)";
        regex        = new RegExp( regexS );
        results      = regex.exec(parent.window.location.href);
        if( results === null ) {
            returnText = '';
        } else {
            returnText = results[1];
        };
        return  returnText;
    };

    function nlShowPopup() {
        $('.nl-cover').fadeIn('fast');
        cookieSet(nlCookieName, '1');
        return false;
    };

    function nlHidePopup() {
        theMainMaster = $('.nl-cover');
        theMainMaster.fadeOut('fast');
        $('.nl-messages', theMainMaster).hide();
        $('.nl-fullname', theMainMaster).val('');
        $('.nl-email', theMainMaster).val('');
        // Set the cookie
        cookieSet(nlCookieName, '1');
        return false;
    };

    function nlDoSubmitDetails(formHolderId) {
        mainSubmitHolder = null;
        if (typeof formHolderId !== 'undefined' && formHolderId.length > 0) {
            mainSubmitHolder = $(formHolderId);
        };
        sendUserName      = $('.nl-fullname', mainSubmitHolder).val();
        sendUserEmail     = $('.nl-email', mainSubmitHolder).val();

        $('.nl-messages', mainSubmitHolder).hide();
        if (sendUserName.length > 0 && sendUserEmail.length > 0 && nlIsValidEmail(sendUserEmail) === true) {
            $.ajax({
                type: 'POST',
                url: /ska/ + '?c=newsletter&m=subscribe',
                data: {
                    email: sendUserEmail,
                    firstname: sendUserName.split(' ').slice(0, -1).join(' '),
                    lastname: sendUserName.split(' ').slice(-1).join(' '),
                    source: (formHolderId === '.nl-form-popup' ? 'nl-popup-bucataras' : 'nl-box-bucataras'),
                    confirmed: '0'
                },
                success: function(responsedata) {
                    if (typeof responsedata !== 'undefined' && responsedata.status === '1') {
                        $('.nl-success', mainSubmitHolder).show();
                        setTimeout(function() {
                            nlHidePopup();
                        }, 3000);
                    } else if (responsedata.message === 'Esti deja abonat la newsletter-ul nostru!<br />Iti multumim!') {
                        $('.nl-info', mainSubmitHolder).show();
                    } else {
                        $('.nl-error', mainSubmitHolder).show();
                    };
                },
                error: function(responsedata) {
                    $('.nl-error', mainSubmitHolder).show();
                },
                dataType: 'json'  });
        } else {
            $('.nl-error', mainSubmitHolder).show();
        };
        return false;
    };

    function nlDoSubmitDetailsPopUp(formHolderId) {
        mainSubmitHolder = null;
        if (typeof formHolderId !== 'undefined' && formHolderId.length > 0) {
            mainSubmitHolder = $(formHolderId);
        };
        sendUserEmail     = $('.nl-email', mainSubmitHolder).val();
        addNewsletterList = 1;

        $('.side-nl-text').html();
        if (sendUserEmail.length > 0 && nlIsValidEmail(sendUserEmail) === true) {
            $.ajax({
                type: 'POST',
                url: /ska/ + '?c=newsletter&m=subscribebook',
                data: {
                    email: sendUserEmail,
                    source: 'side-nl',
                    add_newsletter: addNewsletterList,
                    confirmed: '1'
                },
                success: function(responsedata) {
                    if (typeof responsedata !== 'undefined' && responsedata.status === '1') {
                        $('.side-nl-text').html('Te-ai abonat cu succes.');
                    } else if (responsedata.message === 'Nu te-ai abonat') {
                        $('.side-nl-text').html('Abonarea nu s-a realizat.');
                    } else {
                        $('.side-nl-text').html('Esti deja abonat.');
                    };
                },
                error: function(responsedata) {
                    $('.side-nl-text').html('Abonarea nu s-a realizat.');
                },
                dataType: 'json'  });
        } else {
            $('.side-nl-text').html('Emailul nu e valid.');
        };
        return false;
    };


    // Stop the shake
    function sideNewsletterShakeStop() {
        $('.side-nl-box-form').trigger('stopRumble');
    };
    // Blur the sides
    function sideDoBlurBody() {
        $('.v2-root, #ICBrandingScrollMastercmufedqlmklhxioienhqsdfo, #ICBrandingScrollMastermoqfbdttprphptkolhigsfhk').addClass('side-nl-blur');
    };
    // Unblur
    function sideDoUnBlurBody() {
        $('.v2-root, #ICBrandingScrollMastercmufedqlmklhxioienhqsdfo, #ICBrandingScrollMastermoqfbdttprphptkolhigsfhk').addClass('side-nl-unblur');
        setTimeout(function() {
            $('.v2-root, #ICBrandingScrollMastercmufedqlmklhxioienhqsdfo, #ICBrandingScrollMastermoqfbdttprphptkolhigsfhk').removeClass('side-nl-blur').removeClass('side-nl-unblur');
        }, 900);
    };

    // Start the shake
    function sideNewsletterShakeStart() {
        newsletterInterval = setInterval(function() {
            $('.sidebar-trigger').trigger('startRumble');
            setTimeout(function() {
                $('.sidebar-trigger').trigger('stopRumble');
            }, 3000);
        }, 10000);
    };

    // The opener
    function sideNlOpen() {
        // Stop the shaker
        sideNewsletterShakeStop();
        window.clearInterval(newsletterInterval);
        sideNewsletterShakeStop();
        // Display the content
        $('.sidebar-nl').animate({left: 0}, function() {
            // Put focus
            $('.side-nl-box-email').focus();
            // Show the tooltip
            $('.sidebar-trigger').tooltipster('open');
        });
        // Blur the
        sideDoBlurBody();
    };
    function sideNlClose() {
        // Close the tooltip
        $('.sidebar-trigger').tooltipster('close');
        // Animate
        $('.sidebar-nl').animate({left: -281}, function() {
            // Start the shaker
            sideNewsletterShakeStart();
        });
        // UnBlur the content
        sideDoUnBlurBody();
    }

    $(window).load(function() {
        // Shake it
        $('.sidebar-trigger').jrumble({x: -1, y: 0, rotation: -1, speed: 1});
        sideNewsletterShakeStart();
        // On the hover
        $('.sidebar-trigger').tooltipster({
            theme: 'tooltipster-borderless',
            side: 'top',
            interactive: false,
            autoClose: false,
            trackOrigin: true,
            multiple: false,
            trigger: 'custom',
            functionInit: function (origin, tooltipEl) {

                $('.sidebar-trigger').click(function() {
                    // Open
                    if (sideBarNlOpen === false) {
                        sideBarNlOpen = true;
                        sideNlOpen();
                    } else {
                        // <ark the variable
                        sideBarNlOpen = false;
                        sideNlClose();
                    };

                });
            }
        });

        $('.v2-root').click(function() {
            if (sideBarNlOpen === true) {
                sideBarNlOpen = false;
                sideNlClose();
            };
        });

        if (cookieGet('side-bar-nl') !== '1') {
            cookieSet('side-bar-nl', '1', '1');
            if ($('.sidebar-nl').is(':visible') === true) {
                $('.sidebar-nl').tooltipster({
                    theme: 'tooltipster-borderless',
                    side: 'right',
                    interactive: false,
                    autoClose: false,
                    trackOrigin: true,
                    multiple: false,
                    trigger: 'custom'
                });
                // Disaplay on
                setTimeout(function() {
                    // Open the tooltipster
                    $('.sidebar-nl').tooltipster('open');
                    setTimeout(function() {
                        // Close the tooltipster
                        $('.sidebar-nl').tooltipster('close');
                    }, 4000);
                }, 3000);
            };
        };
    });

    function Download(url) {
        document.getElementById('my_iframe').src = url;
    };

</script>

