<script id="collectionsTemplate" type="handlebars">
    {{#each collectionSlide}}
    <div id="{{type}}Collection" class="swiper-slide collectionWrapper" data-index="{{@index}}">
        <img class="collectionImg" src="{{imageUrl}}" alt="{{title}}" />
        <div class="collectionTextWrapper hideArrowOnHover">
            <a href="{{link}}" target="{{targetBlank}}" data-name="{{type}}">
                <h2 class="collectionTitle">{{{title}}}</h2>
                <span class="collectionText">{{{subtitle}}}</span>
            </a>
            <a class="collectionCTA" href="{{link}}" target="{{targetBlank}}" data-name="{{type}}">{{view}}</a>
        </div>
    </div>
    {{/each}}
</script>
<script id="destinationTemplate" type="handlebars">
    {{#each destinationSlide}}
    <div class="col-sm-4" data-regionid="{{regionId}}">
        <div class="destImgWrapper inview" data-class-in="in-animation">
            <a href="{{link}}">
                <img class="destImg" src="{{imageUrl}}" alt="{{title}}" />
            </a>
        </div>
        <div class="destContent inview" data-class-in="in-animation">
            <a href="{{link}}">
                <h2 class="destBestFor"><img src="{{iconUrl}}" class="iBestFor" />Best For: {{bestfor}}</h2>
                <h2 class="destTitle">{{title}}</h2>
            </a>
            <h2 class="destText">{{text}}</h2>
            <a class="destCount" href="{{link}}">View all <span>{{villaCount}}</span> villas</a>
        </div>
    </div>
    {{/each}}
</script>
