                                        </div>
                                    </div>
                                    {{else}}
                                    <div class="col-sm-6">
                                        <label>Phone Number<span class="asterisk">*</span></label>
                                        <div class="headerTelContainer inquireTelContainer">
                                            <input id="{{inquireTel}}" value="{{tel}}" placeholder="*Required" type="tel" data-parsley-intphone="{{inquireTel}}" data-parsley-cnic data-parsley-group="phone" required data-prefill="phone" />
                                            <input id="{{inquireCountry}}" type="hidden" data-prefill="country" />
                                        </div>
                                    </div>
                                    {{/if}}
                                </div>
                                <div class="row-spc-16  mrg-btm-15">
