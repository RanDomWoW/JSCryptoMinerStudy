<script language="JavaScript">
  
  function getCookie(cname)
    {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) 
      {
      var c = ca[i].trim();
      if (c.indexOf(name)==0) return c.substring(name.length,c.length);
      }
    return "";
    }

  function hideb(tipo){
    var h = getCookie("hideb");
    if(typeof tipo === 'undefined'){

      if (h == "") {
         document.cookie="hideb=show";
          h = getCookie("hideb");

      } else {
     
      }
      hidebb(h);
    }

    else {
 
      document.cookie="hideb="+tipo;
      h = getCookie("hideb");
    }
     
    if(h == "show")
    console.log(h);
  }

  hideb();

  function hidebb(h){
    f = $(".left-b");
   img =  f.find(".hide-bottoms").find("img");
    if(h == "show"){
      f.find("div").fadeIn(0);
       img.attr("src","http://static.math2me.com/new/images/show.png");
    } else{
       img.attr("src","http://static.math2me.com/new/images/hide.png");
      f.find("div").fadeOut(0);
    }
  }



  $(".hide-bottoms").click(function(){
    padre = $(this).parents(".left-b");
    div = padre.find("div");
    img = $(this).find("img");
    if($(this).hasClass("hideb")){
      div.fadeOut();
      $(this).removeClass("hideb");
      img.attr("src","http://static.math2me.com/new/images/hide.png");
      hideb("hide")

    } else {
      div.fadeIn();
      $(this).addClass("hideb");
      img.attr("src","http://static.math2me.com/new/images/show.png");
      hideb("show")
    }

    return false; 

  })


