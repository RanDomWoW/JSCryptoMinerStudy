            //        var strDoctoral = document.getElementById('ddDoctoral').value;

            //        if (!document.getElementById('CheckBox1').checked && strBachlors == "-1" && strMasters == "-1" && strDoctoral == "-1") {
            //            alert("Please select a course")
            //            return false;
            //        }

            return check_val();
        }
