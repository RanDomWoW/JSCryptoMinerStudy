            e.preventDefault();
        });
        
        $(document).on('click', '#btn_post_job1', function(e){
            e.preventDefault();
            
            var errors = [];
            var job_title = $.trim($('#txt_job_title').val());
            if(job_title == '' || job_title == $('#txt_job_title').attr('placeholder')){
                errors.push("Job Title is required.");
            }
            
            var job_descr = $.trim($('#txt_job_descr').val());
            if(job_descr == '' || job_descr == $('#txt_job_descr').attr('placeholder')){
                errors.push("Job Description is required.");
            }
            
            var category = '';
            if($('#job_category').length){
                category = $.trim($('#job_category').val());
                if(category == ''){
                    errors.push("Job category is required.");
                }
            }
            
            var city_state = $.trim($('#txt_job_city_state').val());
            if(city_state == '' || city_state == $('#txt_job_city_state').attr('placeholder')){
                var msg = "City, State is required.";
                errors.push(msg);
            }
            
            var email = $.trim($('#txt_job_email').val());
            if(email == '' || email == $('#txt_job_email').attr('placeholder')){
                errors.push('Email Address is required.');
            }
            else{
                if(!kit.str.isValidEmail(email)){
                    errors.push("Email Address is invalid.");
                }
            }
            
            if(errors.length > 0){
                kit.wdgt.promptErrors(errors);
            }
            else{
                $('#btn_post_job1').hide();
                $('#btn_post_job1_loader').show();
                
                $.post('/validate-new-jobpost', $.param({'city_state': city_state, 'email': email}), function(response){
                    response = $.parseJSON(response);
                    if(response.valid == 1){
                        var zipcode = response.msg;
                        var jobPostVar = {'title':job_title, 'descr':job_descr, 'job_category':category, 'zipcode':zipcode, 'email':email, 'freejobpost':'n'};
                        $.extend(jobPostVar, {GHCSRF: $('meta[name="csrf-token"]').attr("content")});

                        var loginCallback = function(){
                            $.post('/new-jobpost', $.param(jobPostVar), function(response){
                                response = $.parseJSON(response);
                                if(response.result == 1){
                                    window.location.href = GhURLHelper.url({'jobid':response.msg}, 'employer_post-job-step-two')+'?f=home';
                                }
                                else{
                                    if(response.msg != ''){
                                        kit.wdgt.prompt(response.msg);
                                        if($('#frmSignin').length > 0){
                                            $('#btn_frmSignin').show();
                                            $('#gh-modal-login_loader').hide();
                                        }
                                    }
                                }
                            });
                        };
                            
                        if(response.email_exist == 1){ //show login window
                            $('#frmSignin #username').val(email);
                            
                            Gh.showLoginDialog(loginCallback);
                        }
                        else{
                            Gh.onCreateEmployerAccountCallback = loginCallback;
                            
                            $('#home-new-account-container').find('#txt_email').val(email);                            $('#home-new-account-container').find('#txt_zipcode').val(zipcode);
                            
                            var w_height = 690;
                            if($.browser.msie && ($.browser.version < 9.0)){
                                w_height = 700;
                            }
                            
                            $('#home-new-account-container').dialog({
                                width: 510,
                                height: 'auto',
                                modal: true,
                                resizable: false
                            });
                        }
                    }
                    else{
                        kit.wdgt.promptErrors(response.msg);
                    }
                    $('#btn_post_job1').show();
                    $('#btn_post_job1_loader').hide();
                });
            }
        });
        
        $('span#create-new-account-link-span').hide();
        $('div#home-new-account-email-container').hide();        $('#btn_post_job2').text("Post Job Now");
    })(jQuery);
