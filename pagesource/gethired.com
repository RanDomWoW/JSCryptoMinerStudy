<script type="text/javascript">
    (function($){
        var GHPwd = $('#txt_password').passwordValidate($('#txt_email'));

        $('a.gh-login-link').on('click', function(e){
            e.preventDefault();
            var link = $(e.currentTarget);
            
            Gh.onCreateEmployerAccountCallback = function(){
                window.location.href = link.attr('href')
            };
            
            Gh.showLoginDialog();
        });
        
        $('a#link-have-an-account').on('click', function(e){
            e.preventDefault();
            
            $('#home-new-account-container').dialog('close');
            Gh.showLoginDialog(Gh.onCreateEmployerAccountCallback);
        });
        
        $('#frmSignin').on('click', '#btn_frmSignin', function(e){
            e.preventDefault();
            
            var errors = [];
            
            var email = $.trim($('#frmSignin #username').val());
            if(email == ''){
                errors.push("Email is required.");
            }
            
            var password = $.trim($('#frmSignin #mod_password').val());
            if(password == ''){
                errors.push("Password is required.");
            }
            
            if(errors.length > 0){
                kit.wdgt.promptErrors(errors);
            }
            else{
                if(Gh.onLoginCallback){
                    $('#btn_frmSignin').hide();
                    $('#gh-modal-login_loader').show();
                    $('#div_password_hints').hide();

                    $.post($('#frmSignin').attr('action'), $('#frmSignin').serialize(), function(response){
                        response = $.parseJSON(response);
                        if(response.status == 'succeeded'){
                            Gh.onLoginCallback.call(this);
                        }
                        else{
                            kit.wdgt.prompt(response.message, '', null, null, {width: 320, modal:true});
                            $('#btn_frmSignin').show();
                            $('#gh-modal-login_loader').hide();
                        }
                    });
                }
                else{
                    $('#frmSignin').submit();
                }
            }
        });
        
        $('#gh-modal-login').on('click', 'a.create-new-account-link', function(e){
            e.preventDefault();
            $('#gh-modal-login').dialog('close');
            
            var w_height = 740;
            if($.browser.msie && ($.browser.version < 9.0)){
                w_height = 760;
            }
            
            $('#home-new-account-container').dialog({
                                width: 500,
                                height: 'auto',
                                modal: true,
                                resizable: false
                            });
        });
        
        $(document).on('click', '#btn_post_job2', function(e){
            e.preventDefault();
            var params = new Object();
            var errors = [];
            var email = $.trim($('#txt_email').val());
            if(email == ''){
                errors.push('Email is required.');
            }
            
            var first_name = $.trim($('#txt_first_name').val());
            if(first_name == ''){
                errors.push('First Name is required.');
            }
            
            var last_name = $.trim($('#txt_last_name').val());
            if(last_name == ''){
                errors.push('Last Name is required.');
            }
            
            var company = $.trim($('#txt_company').val());
            if(company == ''){
                errors.push('Company is required.');
            }
            
            var ctype = $('#sel_company_type').val();
            if(ctype == '' && $('#sel_company_type').is(":visible")){
                errors.push("Please choose" +' '+ $('#sel_company_type').attr('label') + '.');
            }
            
            var hiring_size = $('#sel_hiring_size').val();
            if(hiring_size == '' && $('#sel_hiring_size').is(":visible")){
                errors.push("Please choose \"How many hires\".");
            }
            
            var phone = $.trim($('#txt_phone').val());
            if(phone == ''){
                errors.push('Phone is required.');
            }
            
            var user_zipcode = $.trim($('#txt_zipcode').val());
            
            var cost_reporting_entity = null;
            if($('select#sel_cost_reporting_entity').length){
                cost_reporting_entity = $('select#sel_cost_reporting_entity').val();
                if(cost_reporting_entity == ''){
                    errors.push('The answer to the question "Are you a Cost Reporting Entity?" is required');
                }
            }
            
            var medical_practice_type = null;
            if($('select#sel_medical_practice_type').length){
                medical_practice_type = $('select#sel_medical_practice_type').val();
                if(medical_practice_type == ''){
                    errors.push('Type of Medical Practice is required');
                }
            }
            
            var shipto_account = null;
            if($('input#txt_shipto_account').length){
                shipto_account = $.trim($('input#txt_shipto_account').val());
            }
            
            var password = $.trim($('#txt_password').val());

            if(password == ''){
                errors.push('Password is required.');
            } else {
                if (!GHPwd.validate(email)) {
                    errors.push('Password is not valid.');
                }
            }

            if($('#home-new-account-container #captcha').length != 0) {
                var captcha = $.trim($('#home-new-account-container #captcha').val());
                if(captcha == ''){
                    errors.push("Please enter the letters in the image.");
                }
                params.captcha = captcha;
                params.xxvalxx = $('#home-new-account-container #xxvalxx').val();
            }
            
            var referring_uuid = $('input#hidden_referring_employer_uuid').val();
            
            if(errors.length > 0){
                kit.wdgt.promptErrors(errors);
            }
            else{
                $('#btn_post_job2').parent().parent().hide();
                $('#btn_post_job2_loader').show();
                
                params.email = email;
                params.fname = first_name;
                params.lname = last_name;
                params.company = company;
                params.ctype = ctype;
                params.hiring_size = hiring_size;
                params.phone = phone;
                params.user_zipcode = user_zipcode;
                params.password = password;
                params.hearabout = $('#sel_hear_about').val();
                params.referring_uuid = referring_uuid;
                $.extend(params, {GHCSRF: $('meta[name="csrf-token"]').attr("content")});
                
                if(cost_reporting_entity) {
                    params.cost_reporting_entity = cost_reporting_entity;
                }
                if (medical_practice_type) {
                    params.medical_practice_type = medical_practice_type;
                }
                if(shipto_account) {
                    params.shipto_account = shipto_account;
                }
                
                $.post('/cea', $.param(params), function(response){
                    response = $.parseJSON(response);
                    if(response.result == 1){
                        Gh.onCreateEmployerAccountCallback.call(this);
                    }
                    else{
                        if(response.msg != ''){
                            kit.wdgt.prompt(response.msg);
                            $('#btn_post_job2').parent().parent().show();
                            $('#btn_post_job2_loader').hide();
                        }
                    }
                });
            }
        });
        
        $(document).on('click', 'a.loadModalContents', function(e){
            e.preventDefault();
            var dialogTitle = $(this).text();
            $('#privacyDlg').load($(this).attr("href"),function(){
                if($('#kidget').length > 0){
                    $('#kidget').scrollTop(0);
                }
                kit.wdgt.prompt($('#privacyDlg').html(), "GetHired " + dialogTitle, function(){}, null, {width: 680, height: 420, modal:true, dialogClass: 'modal-shadow', 
                        open: function(){
                            $('#kidget').scrollTop(0);
                        }
                    }
                );
            });
        });

    })(jQuery);

