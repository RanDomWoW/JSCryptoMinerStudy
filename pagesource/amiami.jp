	color:#ffffff;
	background-color:#000000;
	cursor: pointer;
	opacity: 0.5;
	
	border-radius: 25px;
	-moz-border-radius:25px;
	-webkit-border-radius:25px;
}
#scrolltop:hover{
	opacity: 1.0;
}
#scrolltop:before {
	padding-top:5px;
	font-size:150%;
	content: "▲";
	display: block;
	text-align: center;
	width: 100%;
	margin: auto;
}
