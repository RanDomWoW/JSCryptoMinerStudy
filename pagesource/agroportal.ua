        $(document).on("click", ".more_add_media", function () {
            var count=$('ul.media_list').data('count');
            var count_more=$('ul.media_list').data('more');
            var count_li=$('ul.media_list').data('val');
            if(count>=count_li){
               if(count<(count_li+(count_more*3))) {
                   var lang = '/';
                   $.post(lang+'ajax/type_load-media/count_' + count,
                           function (data) {
                               if (data) {
                                   $('ul.media_list').append(data);
                                   $('ul.media_list').data('count', count + count_more);
                                   if((count + count_more)>=count_li+(count_more*3)){
                                       $('.more_add_media').text('Архив мультимедиа');
                                       $('.more_add_media').attr('href','/archive/?cat_id=438');
                                   }
                               }
                           });
               }
            }

        });
        $(document).on("click", ".more_add_pubs", function () {
            var count=$('ul.publication').data('count');
            var count_more=$('ul.publication').data('more');
            var count_li=$('ul.publication').data('val');
           if(count>=count_li){
               if(count<(count_li+(count_more*3))) {
                   var lang = '/';
                   $.post(lang+'ajax/type_load-pubs/count_' + count,
                           function (data) {
                               if (data) {
                                   $('ul.publication').append(data);
                                   $('ul.publication').data('count', count + count_more);
                                   if((count + count_more)>=count_li+(count_more*3)){
                                       $('.more_add_pubs').text('Архив публикаций');
                                       $('.more_add_pubs').attr('href','/archive/?cat_id=436');
                                   }
                               }
                           });
               }
            }

        });
        $("#form-subscribe").on('submit', function(e){
            e.preventDefault();

            var data = {
                'email': $('#form-subscribe input#email').val(),
                'name': $('#form-subscribe input#name').val(),
                'company': $('#form-subscribe input#company').val()
            };
            $.ajax({
                url: '/ajax/type_subscription',
                data: data,
                type: 'POST',
                success: function (data) {
                    if (data == 1) {
                        $('#myModal1').foundation('reveal', 'close');
                    } else {
                        console.log(data);
                        $('#myModal1').foundation('reveal', 'close');

                    }
                }
            });
        });
    blog_today();
    });

    function blog_today(){
        $('.date_blogs_today, .date_today').each(function(index, element){
            var date = $(element).data('date');
            var date_today = new Date();
            var day = addZero(date_today.getDate());
            var month = addZero(date_today.getMonth()+1);
            var year = date_today.getFullYear();
            date_today = day+'-'+month+'-'+year;
            if(date_today==date){
                $(element).text('Сегодня,');
            }
        });
        function addZero(i) {
            return (i < 10)? "0" + i: i;
        }

    }
</script>
