<form action="" method="post" id="loginForm" data-showerror="none">

<div class="modal" id="loginModal" role="dialog" >
	<div class="modal-dialog" role="dialog">
	<div class="modal-content">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal"><span>&times;</span></button>
			<h4 class="modal-title">Please sign in</h4>
		</div>
		<div class="modal-body">
			<div class="row">
				<div class="col-xs-1">
				</div>
				<div class="col-xs-10 col-sm-8">
					<label for="loginUsername">Email or Username</label>
					<input type="text" class="form-control" name="loginUsername" id="loginUsername" placeholder="Email or Username" 
						autocomplete="on" required="required" />
				</div>
			</div>
			<div class="row">
				<div class="col-xs-1">
				</div>
				<div class="col-xs-10 col-sm-8">
					<label for="loginPassword">Password</label>
					<input type="password" class="form-control" name="loginPassword" id="loginPassword" placeholder="Password" autocomplete="off" required="required" />
				</div>
			</div>

			<div class="row">
				<div class="col-xs-1"></div>
				<div class="checkbox col-xs-10 col-sm-8">
					<label>
						<input name="remember" id="remember" type="checkbox" value="remember" />
						Remember login
					</label>
					<p class="help-block">(if this is a private computer)</p>
				</div>
			</div>
			<div class="row">
				<div class="col-xs-1">
				</div>
				<div id="loginError" class="col-xs-11" style="display:none;" >
					
				</div>
			</div>
			<div class="row">
				<div class="col-xs-1">
				</div>
				<div class="col-xs-10 col-sm-8">
					<span id="btnLoginSubmit" class="btn btn-success btn-block" >Sign In</span>
				</div>
			</div>
			
		</div>
		<div class="modal-footer">
			<div class="row text-left">
				<div class="col-sm-12">
					Help
				</div>
			</div>

			<div class="row">
				<div class="col-sm-6 text-left">
					<a href="/member/newLogin.aspx">Create Login</a>
				</div>
				<div class="col-sm-6">
					<a name="recover" style="cursor: pointer;">
						<span id="recoverPWLink" onclick="$('#loginModal').modal('hide'); lmb.LoadShowModal('recoverModal');">
						Recover Login
	                 </span></a>
				</div>
				<div class="col-sm-6 text-left">
					<a name="resend" onclick="ReSendConfirm();" id="btnResend" style="cursor: pointer;">Resend Confirmation email</a>
				</div>
				<div class="col-sm-6">
					<a name="contact">
						<span class="popModal" onclick="$('#loginModal').modal('hide'); lmb.LoadShowModal('contactModal'); ">Contact Us</span>
					</a>
				</div>

			</div>
		</div>
	</div>
	</div>
</div>
</form>
<script>
	$(document).ready(function () {
		var showError = $("#loginForm").data("showerror");
		if (showError != "none") {
			$('#loginModal').modal();
		}
		var loc = location.toString();
		loc = loc.replace('http:', 'https:');
		$("#loginForm").attr('action', loc);
		$("#btnLoginSubmit").click(DoLogin);
	});

	function DoLogin() {
		var username = $("#loginUsername").val();
		var pw = $("#loginPassword").val();

		if (username == null || username == "" || pw == null || pw == "") {
			alert("Please first enter a username and password.");
		} else {
			$("#loginForm").submit();
		}
	}

	function ReSendConfirm() {
		var username = $("#loginUsername").val();

		if (username == null || username == "") {
			alert("Please first enter a username.");
		} else {
			var sendData = 'action=lir' + '&un=' + username;
			var xhttp = new XMLHttpRequest();

			xhttp.onreadystatechange = function () {
				if (this.readyState == 4) {
					alert("If that username exists, an email has been sent. Please check your inbox or junkmail/spam folders.");
				}
			};
			xhttp.open("POST", $("#loginForm").attr('action'), true);
			xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
			xhttp.send(sendData);
		}
	}

</script>



