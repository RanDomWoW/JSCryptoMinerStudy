// --------------------> 设置 $('#header-user')

+function header_user_set() {
	var header_user_html  =  '<span class="grey">欢迎您，<a href="/my-profile.htm">{username}</a> </span><a href="/my-profile.htm"><span class="icon user"></span>个人资料</a>　<a href="/my-license.htm"><span class="icon icon gittip"></span>我的授权</a>　';
	    header_user_html += '<a href="/user-logout.htm"><span class="icon power-off"></span>退出</a> ';
	var header_guest_html = '<a href="user-login.htm"><span class="icon user" style="color: red;"></span>登录</a>　<a href="user-create.htm">注册</a>';

	var token = $.cookie('www_token');
	if(token) {
		// 客户端缓存
		var userdata = $.pdata('user');

		if(!userdata) {
			// 如果 pdata 没有数据，则从 ajax 获取用户数据
			$.xget('user-read.htm', function(code, message) {
				if(code == 0) {
					$('#header-user').find('div.body').html(template(header_user_html, {username:message.username}));
					$.pdata('user', message);
				} else if(code > 0) {
					$('#header-user').find('div.body').html('从服务端获取用户数据失败！');
					$.cookie('www_token', ''); // 清理 token
				} else if(code < 0) {
					$('#header-user').find('div.body').html('从服务端获取用户数据失败！');
				}
			}, 3);
		} else {
			$('#header-user').find('div.body').html(template(header_user_html, {username:userdata.username}));
		}
	} else {
		$('#header-user').find('div.body').html(header_guest_html);
	}
} ();
// ----------------------> 搜索

function request_uri() {
	var url = document.baseURI || document.URL || window.location;
	url = url.replace(/^(([a-z]+):\/\/)?([^\/\?#]+)\//i, '');
	return url + '/';
}

// 选中状态
+function header_nav_active() {
	var url = document.baseURI || document.URL || window.location;
	var parse = url.match(/^(([a-z]+):\/\/)?([^\/\?#]+)\/*([^\?#]*)\??([^#]*)#?(\w*)$/i);
	var r = {
	    'schema': parse[2],
	    'host': parse[3],
	    'path': parse[4],
	    'query': parse[5],
	    'anchor': parse[6],
	    'requesturi': parse[4] + parse[5] + parse[6]
	};
	$('#header').find('a').each(function() {
		var href = $(this).attr('href').replace(/^(\/|\.\/)/, '');
		if(r.requesturi == href) $(this).addClass('active');
	});
	window.url = r;
}();


// 调整页脚的高度，满屏效果
$(function() {
	var h1 = $(document).height();
	var h2 = $(window).height();
	if(h2 > h1) {
		var jfooter = $('#footer');
		var h3 = jfooter.height();
		jfooter.height(h3 + (h2 - h1));
	}
});

