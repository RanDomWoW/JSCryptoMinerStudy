		} else {
			jcheck_result_1.show();
			jcheck_result_2.hide();
		}
	});
}

jdownbutton.on('click', function() {
	// 显示正在检查下载权限
	jloading.show();
	check_status();
	setTimeout(function() {
		check_setInterval();
	}, 3000);
	
});

function check_setInterval() {
	// 查询订单状态，如果成功，则显示下载地址:
	check_handle = setInterval(function() {
		check_status();
	}, 3000);
}

// -----------------------> 打包下载 结束

</script>