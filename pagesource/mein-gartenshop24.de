        if (r.messages.error) {

            var messageUpdate = new Array();
            for (var i = 0; i < r.messages.error.length; i++) {

                if (r.messages.error[i] != Translator.translate('Some of the products are currently out of stock')) {
                    messageUpdate[i] = r.messages.error[i];

