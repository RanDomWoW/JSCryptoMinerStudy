			<script>
			function loadAxData(el, firstload=false){
				var type=el.attr('data-type');
				
				var p=el.attr('data-page');
				var holder=el.find('.ax-data');
				
				if(type=='complex'){
					var data={local_ajax:1, page:p, action: 'find_complex_list'};
				}else{
					var tid=el.attr('data-id');
					var data={local_ajax:1, topic_id:tid, page:p, action: 'find_list'};
				}
				
				if(!firstload){
					holder.append($('<div class="loader"><img src="/template/frontend/realia/img/loader.gif"></div>'));
				}
				
				$.ajax({
					url: estate_folder+'/js/ajax.php',
					type: 'post',
					dataType: 'json',
					data: data,
					success: function(json){
						holder.html(json.grid);
						if(!firstload){
							holder.find('.loader').remove();
						}
						el.find('.ax-pager').html(json.pager);
					}
				});
			}
			$(document).ready(function(){
				$('.ax-list').each(function(){
					loadAxData($(this), true);
				});
				$('.ax-pager').on('click', 'a', function(e){
					var page=parseInt($(this).data('page'), 10);
					if(!isNaN(page)){
						e.preventDefault();
						$(this).parents('.ax-list').eq(0).attr('data-page', page);
						loadAxData($(this).parents('.ax-list').eq(0));
					}
				})
			});
