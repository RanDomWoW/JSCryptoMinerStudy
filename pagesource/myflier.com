    	}else{
    		return '<p><i class="fa fa-star-o" aria-hidden="true"></i> ' + categorySuggestionLabel + ' - ' + data.name + '</p>';
    	}
	}
	
	function doSearch(suggestion){
	    if(suggestion && suggestion.id){
	    	var target = "/flyers/sa/riyadh";
	    		
	    	if(suggestion.type == 'C'){
