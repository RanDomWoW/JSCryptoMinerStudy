<script type="text/javascript" charset="utf-8" language="javascript" date-src="/js_sc/jq_plugin/sn-timecheck/" blzexc="1">
//<![CDATA[
(function ($) {
    var timeCheck = function (str_start, str_end) {
        var MAX = '2999/12/31 23:59:59';
        var MIN = '1970/01/01 00:00:00';
        var start, end, now;

        var time = location.search.match(/time=\d{4}\/\d{1,2}\/\d{1,2}(%20?\d{0,2}:?\d{0,2}:?\d{0,2})?/i);
        time = (time !== null) ? time[0].split("=")[1] : '';

        now = new Date(time.replace('%20', ' '));
        start = new Date(str_start);
        end = new Date(str_end);

        if (now.toString() === 'Invalid Date') now = new Date();
        if (start.toString() === 'Invalid Date') start = new Date(MAX);
        if (end.toString() === 'Invalid Date') end = new Date(MIN);

        console.log(now >= start && now < end);
        console.log(now);
        console.log(start);
        console.log(end);
        return (now >= start && now < end);
    };
    $.fn.timeCheck = function () {
        return this.each(function () {
            var $this = $(this);
            var str_start = $this.data('start');
            var str_end = $this.data('end');
            var chk = timeCheck(str_start, str_end);
            if (chk) $this.show();
        });
    };
    $.fn.timeMessage = function (options) {
        return this.each(function () {
            var $this = $(this);
            var str_start = options.start;
            var str_end = options.end;
            var message = options.message;
            if (timeCheck(str_start, str_end)) $this.html(message);
        });
    };
})(jQuery);
//]]>
</script>
