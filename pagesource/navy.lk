		if(curSize<=22)
        $('#content').css('font-size', curSize);
        });  
		
		$('#de').click(function(){	   
        curSize= parseInt($('#content').css('font-size'));
		if(curSize=16)
        $('#content').css('font-size', curSize);
        });
		
		$('#decfont').click(function(){	   
        curSize= parseInt($('#content').css('font-size')) - 2;
		if(curSize>=10)
        $('#content').css('font-size', curSize);
        }); 
	});
</script>
