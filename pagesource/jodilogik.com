submitForm = function(emailId){
	
	$(".click_confirm,.cancel_button").attr("disabled",true)
	//$(".fewMoreRow2").hide();
	emailId = emailId.trim().toLocaleLowerCase();
	$
	.ajax({
		type : 'POST',
		url : '/home/updateCommunicateEmailId',

		data : {
			"profileFor" : profileFor,
			"gender":gender,
			"emailId":emailId,
			"modelId":modelId.trim()

		},
		beforeSend: function(){
			$(".click_confirm,.cancel_button").attr("disabled",true)
			$(".showErrorMessage2").html("Please wait...").show();
			$('.change_mail').unbind('click',changeClick);
		},
		success : function(returnMsg) {
			$(".click_confirm,.cancel_button").attr("disabled",false)
		if(returnMsg == "true"){
			var url = window.location.href;
			if(url.indexOf("redirectFromFacebookAd")>=0){
				window.location = "/home/editProfileFromviewProfile?linkFrom=facebookAd";
			} else {
				window.location = "/home/editProfileFromviewProfile";
			}
		}else if(returnMsg == "mailSent"){
			$(".inviteFewHeading").html("We have sent a verification link");
			$(".inviteheadericon,.inviteheadericon1 ").remove();
			$(".fewMoreRow2 #jodiLogikImprovisedContent").remove();
			$("#fewMoreSteps #holder").css("padding-top","0px");
			$("#fewMoreSteps #holder").css("padding-bottom","28px");
			$(".showErrorMessage2").html("").hide();
			$(".change_mail").hide();
		    $(".save_mail").show();
			$(".popup_line").hide();
			$(".clsSecondPopupPartOne").hide(); 
			$(".showErrorMessage2").hide();
			$(".user_edited_mail").html(emailId.trim());
			
			
			var link = linkToInbox.getHref(emailId.trim(), {subject: 'Your request to sign up with Jodi Logik'});
			$(".locateMail").attr("href",link);
			if(link == "#close"){
				$(".locateMail").html("DONE");
				$(".locateMail").css("padding","6px 20px");
				$(".locateMail").attr("target","_self");
			}
			$(".clsSecondPopupPartTwo").show();
			$("#fewMoreSteps #Heading1").prepend('<a href="#close" title="Close" class="close"><i class="fa fa-close"></i></a>');
			$(".inviteheader").addClass("inviteheaderLast");
		}
		
			
		},
		error : function(XMLHttpRequest, textStatus, errorThrown) {
			
			$(".click_confirm,.cancel_button").attr("disabled",false)
			$('.change_mail').bind('click',changeClick);
			$(".showErrorMessage2").html("").hide();
		}

	});
}
