					$('.div3,.div2,#socialFBTW').hide();
					$(".div8").show();
				}
			}
			
		} else if (recipientStatus != '') {
			if (recipientStatus == 'hasValidPassword') {
				window.location = "#viewAuthPopup";
			} else {
				window.location = "#customRedirectPopup";
			}
		}

		$('#viewAuthPopup input[type="password"]').focus(function() {
			$('.errorMessage').hide();
		});

		if (inviteeId != '') {
			$('a[href="/home/authenticateFacebook"]').remove();
		} else {
			$('#passwordForProfileViewForm')
					.submit(
							function(e) {
								if ($('#viewAuthPopup input[type="password"]')
										.val() == "") {
									$('.errorMessage').html(
											"Please enter the password.")
											.show();
									e.preventDefault();
								}
							})
		}
		
		
		var viewerName = '';
		
		if(viewerName != ""){
			location.href="#identityConformation";
			$(".profViewerName").html(viewerName.split("||")[0]);
			$(".profViewerEmail").html(viewerName.split("||")[1]);
			var sources = '';
			var urlForEmail = "/home/userSigningInFromViewProfileToChat?sources=" + sources;
			$('.joinWithEmailConfirmingIdentity').attr('href', urlForEmail);
			var ownerName = '';
		}
		
	</script>
