<!doctype html>
<html lang="pt-BR" ng-app="app">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SMARAPD</title>

    <link rel="icon shortcut" href="/images/favicon.ico" type="image/x-icon"/>

    <link rel="stylesheet" ng-href="/themes/{{selectedThemePath}}/main.css" href="/themes/theme-default/main.css"/>
    <link rel="stylesheet" href="/libs/font-awesome/css/font-awesome.min.css"/>
    
    <script src="/scripts/api_routes.js"></script>
    <script src="/scripts/system_routes.js"></script>

    <!-- lib:js -->
    <script src="/libs/jquery/jquery.min.js"></script>
    <script src="/libs/jquery-maskedinput/dist/jquery.maskedinput.min.js"></script>
    <script src="/libs/jquery-ui/ui/minified/jquery.ui.core.min.js"></script>
    <script src="/libs/jquery-ui/ui/minified/jquery-ui.custom.min.js"></script>
    <script src="/libs/jquery-ui/ui/minified/jquery.ui.mouse.min.js"></script>
    <script src="/libs/jquery-ui/ui/minified/jquery.ui.position.min.js"></script>
    <script src="/libs/jquery-ui/ui/minified/jquery.ui.widget.min.js"></script>
    <script src="/libs/angular/angular.min.js"></script>
    <script src="/libs/angular-sanitize/angular-sanitize.min.js"></script>
    <script src="/libs/angular-route/angular-route.min.js"></script>
    <script src="/libs/angular-cookies/angular-cookies.min.js"></script>
    <script src="/libs/angular-resource/angular-resource.min.js"></script>
    <script src="/libs/moment/min/moment.min.js"></script>
    <script src="/libs/angular-i18n/angular-locale_pt-br.js"></script>
    <script src="/libs/angular-animate/angular-animate.min.js"></script>
    <script src="/libs/tinymce/tinymce.min.js"></script>
    <script src="/libs/angular-strap/dist/angular-strap.min.js"></script>
    <script src="/libs/angular-strap/dist/angular-strap.tpl.min.js"></script>
    <script src="/libs/kendo-ui/js/kendo.all.min.js"></script>
    <script src="/libs/kendo-ui/js/cultures/kendo.culture.pt-BR.js"></script>
    <script src="/libs/kendo-ui/js/messages/kendo.messages.pt-BR.js"></script>
    <script src="/libs/angular-input-masks/angular-input-masks-standalone.min.js"></script>
    <script src="/libs/signalr/jquery.signalR.min.js"></script>
    <script src="/libs/angular-ui-utils/ui-utils.min.js"></script>
    <script src="/libs/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="/libs/powerbi-client/dist/powerbi.min.js"></script>
    <script src="/libs/angular-filter/dist/angular-filter.min.js"></script>
    <script src="/libs/qrcode/lib/qrcode.min.js"></script>
    <script src="/libs/angular-qr/angular-qr.min.js"></script>
    <!-- endinject -->

    <!-- inject:js -->
    <script src="/scripts/self.min.js?v=1521038657"></script>
    <script src="/scripts/common.min.js?v=1521038657"></script>
    <!-- endinject -->

    <!-- default:js -->
    <script src="/scripts/browserDetect.js?v=1521038657"></script>
    <!-- endinject -->
    
    <script src="/scripts/config.js"></script>
</head>
<body ng-controller="ApplicationController">
    <loading-screen></loading-screen>
    <header ng-if="!onLoginScreen">
        <dx-menu></dx-menu>
    </header>
    <div class="info-message"></div>
    <div class="container">
        <div class="row">
            <alert ng-repeat="alert in alerts" type="alert.type" index="$index" lock-screen="alert.lockScreen"
                   close="closeAlert($index)" show="true">
                {{alert.msg}}
            </alert>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <breadcrumb></breadcrumb>
            </div>
        </div>
    </div>
    <div ng-class="{'container container-main': !isFullScreen()}" ng-view ng-animate="{enter: 'animate-enter'}"></div>
    <footer class="container">
        <div class="row">
            <div class="col-xs-12">
            </div>
        </div>
    </footer>
</body>
</html>