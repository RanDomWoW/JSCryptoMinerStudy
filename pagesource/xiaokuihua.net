<script type="text/javascript"> 
$(function() {
	//焦点图切换
	var slideView = function(i) {
		$(".newBig ul li").eq(i).fadeIn().siblings().hide();
		$(".newSmall ul li").eq(i).addClass("select_img").siblings().removeClass("select_img");
		
		$(".newSmall div a").eq(i).css("display",'').siblings().css("display",'none');
		
	};
	//自动切换
	var item = 0,
	els = $(".newSmall ul li").length,
	timer = setInterval(function() {
		slideView(item);
		item = (item + 1) % els;
	}, 3000);
	//鼠标经过触发器切换
	$(".newSmall ul li").each(function(i) {
		$(this).hover(function() {
			clearInterval(timer);
			slideView(i);
		}, function() {
			item = i + 1;
			timer = setInterval(function() {
				slideView(item);
				item = (item + 1) % els;
			}, 3000);
		});
	});
	$(".yao_list > ul").tabs();
	$(".Iyq_test>ul").tabs();
	$(".yiyuan_list>ul").tabs();
})
</script>



