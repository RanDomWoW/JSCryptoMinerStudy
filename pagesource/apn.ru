<script type="text/javascript">
  function rotateBlocks(){
    if( $( document ).width() <= 719 ) {
      $('.grid-4').prependTo('.grid-5');
      for(i=6; i<16; i++) $('#public div.world').eq(i).hide();
      for(i=4; i<6; i++) $('#opinio div.world').eq(i).hide();
    } else {
      $('.grid-4').appendTo('.public-back');
      for(i=6; i<16; i++) $('#public div.world').eq(i).show();
      for(i=4; i<6; i++) $('#opinio div.world').eq(i).show();
    };
  };
  rotateBlocks();
  $( window ).resize(rotateBlocks);
</script>
	
