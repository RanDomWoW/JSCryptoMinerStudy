<script>
window.onload = function aff_popup_unique(){
	if(ScanCookie("dejapopup_infos")==0){
		document.getElementById("default-popup").style.display = "block";
		document.getElementById("fond-popup-overlay-transparent").style.display = "block";
		CreationCookie("dejapopup_infos","oui",1);}
	};	

function ScanCookie(variable){
	var cook = document.cookie;
	variable += "=";
	var place = cook.indexOf(variable,0);
	if (place <= -1) {
		return("0");
	}
	else {
		var end = cook.indexOf(";",place)
		if (end <= -1) {
			return(unescape(cook.substring(place+variable.length,cook.length)));
		}
		else {
			return(unescape(cook.substring(place+variable.length,end)));
		}
	}
};

function CreationCookie(nom, valeur, duree) {
maintenant = new Date().getTime() ;
plusTard = new Date(maintenant + 1000*60*5*1*duree);
expires = plusTard.toGMTString();
if (duree) {
cookie = nom + "=" + valeur + "; expires=" + expires ;} 
else {
cookie = nom + "=" + valeur ;}
document.cookie = cookie ;};

		
function masquerdiv(){
document.getElementById("default-popup").style.display = "none";
document.getElementById("fond-popup-overlay-transparent").style.display = "none";
};
		
</script>
