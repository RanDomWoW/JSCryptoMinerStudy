<script>
function visibilite(divaffiche)	{
var navaffiche = document.getElementsByTagName('div');
    for(var no=0;no<navaffiche.length;no++){
        if(navaffiche[ no].className=='navaffiche'){ // on cible les divs dont la class est 'navaffiche'
            navaffiche[ no].style.display = "none"; // on les masque tous
			document.getElementById('fermer-rech').style.display = "none";
			document.getElementById('ouvrir-rech').style.display = "block";
        }
    }
	
    document.getElementById(divaffiche).style.display = "block"; // on affiche le div appelé
	
	
	if(document.getElementById('navcacherecherche').style.display == "block"){
	document.getElementById('ouvrir-rech').style.display = "none";
	document.getElementById('fermer-rech').style.display = "block";
	}
	
}
function fermerech(){
document.getElementById('ouvrir-rech').style.display = "block";
document.getElementById('fermer-rech').style.display = "none";
document.getElementById('navcacherecherche').style.display = "none";}

function fermeraffiche(){
var navafficheb = document.getElementsByTagName('div');
    for(var no=0;no<navafficheb.length;no++){
        if(navafficheb[ no].className=='navaffiche'){ // on cible les divs dont la class est 'navaffiche'
            navafficheb[ no].style.display = "none"; // on les masque tous
        }
    }
}


