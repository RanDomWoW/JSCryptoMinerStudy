<script>

$(document).ready(function() {
	srcVal = getUrlParameter("src");
	// checking if the src code is live or setup in an existing campaign.  // if live, reset timer
	if (srcVal.match(/^(welcome1|welcome2|welcome3|anniversary|work|welcome1retail|welcome2retail|welcome3retail|au_welcome|au_welcome2|au_welcome3|sg_welcome|sg_welcome2|sg_welcome3|kr_welcome|kr_welcome2|au_anniversary|au_anniversary_v2|sg_anniversary|kr_anniversary|kr_anniversary_v2|sg_wlcm_rtl|sg_wlcm2_rtl)$/)) {
		//START to be deleted when EDM src has been updated
		if (srcVal.match(/^(au_welcome|sg_welcome|kr_welcome)$/)) {
			srcVal = "welcome1";
		}
		if (srcVal.match(/^(au_welcome2|sg_welcome2|kr_welcome2)$/)) {
			srcVal = "welcome2";
		}
		if (srcVal.match(/^(au_welcome3|sg_welcome3)$/)) {
			srcVal = "welcome3";
		}
		if (srcVal.match(/^(au_anniversary|au_anniversary_v2|sg_anniversary|kr_anniversary|kr_anniversary_v2)$/)) {
			srcVal = "anniversary";
		}
		if (srcVal.match(/^(sg_wlcm_rtl)$/)) {
			srcVal = "welcome1retail";
		}
		if (srcVal.match(/^(sg_wlcm2_rtl)$/)) {
			srcVal = "welcome2retail";
		}
		//END to be deleted when EDM src has been updated
		
		// if live, reset timer + reset stored data: srcVal and couponVal
		//console.log("source code is found in URL."); 
		//console.log("src parameter is : " + srcVal); 
		couponVal = getUrlParameter("couponcode");
		//console.log("couponcode parameter is : " + couponVal); 
		//console.log("running popup"); 
		apacpopup(srcVal,couponVal);
		
		//console.log("resetting stored startTime and closedByUser"); 
		today = new Date();
		startTime = today.getTime();
		localStorage.setItem("startTimeStored",startTime);
		localStorage.setItem("closedByUser",null);
		
		//console.log("storing values of srcVal and couponVal"); 
		localStorage.setItem("srcVal", srcVal);
		localStorage.setItem("couponVal", couponVal);
	} else if(srcVal == "" ) {
		// if NOT live, continue with checking the stored startTime
		//console.log("source code is empty");
		// run function storageCheck()
		storageCheck();
		// if something is stored, run popup (stored)
		// else do nothing
	} else { 
		//console.log("source code is unknown"); 
		//localStorage.setItem("srcVal", null);
		$(".apac-couponpopup").addClass("closed"); 
		return; 
	}
});

function apacpopup(promo,code) { 
                $('.apac-couponpopup').removeClass('welcome1 welcome2 welcome3 anniversary work welcome1retail welcome2retail welcome3retail'); $('.apac-couponpopup-title').removeClass('image');
                if (promo === "welcome1") {
                                //AU & SG: Welcome offer!
								var title = 'Welcome offer!'; 
                                //KR: 회원가입시할인!
								//var title = '회원가입시할인!'; 
                                var image = 'no';
								//AU & SG: 20% OFF all full-priced items
								var discount = "<span class='XX'>20</span> <span class='pct'>%&nbsp;</span> <span class='off'>OFF&nbsp;</span> <span class='txt'>all full-priced items</span>";
                                //KR: 20% 할인 모든 정상가 상품
								//var discount = "<span class='XX'>20</span> <span class='pct'>%&nbsp;</span> <span class='off'>할인&nbsp;</span> <span class='txt'>모든 정상가 상품</span>";
                }
                else if (promo === "welcome2") {
                                var title = 'Expires soon!';
								// KR: 쿠폰이 곧 만기됩니다!
                                //var title = '쿠폰이 곧 만기됩니다!';
                                var image = 'yes';
                                var discount = "<span class='XX'>20</span> <span class='pct'>%&nbsp;</span> <span class='off'>OFF&nbsp;</span> <span class='txt'>all full-priced items</span>";
								// KR: 20% 할인 모든 정상가 상품
								//var discount = "<span class='XX'>20</span> <span class='pct'>%&nbsp;</span> <span class='off'>할인&nbsp;</span> <span class='txt'>모든 정상가 상품</span>";
                }
                else if (promo === "welcome3") {
                                var title = 'Welcome offer!';
                                var image = 'no';
                                var discount = "<span class='txt' style='top: 5px; color:#1483c6'>Free shipping! • All orders</span>";
                }
                else if (promo === "anniversary") {
                                var title = 'Happy Anniversary';
                                var image = 'yes';
                                // AU: 15% OFF all full-priced items
								//var discount = "<span class='XX'>15</span> <span class='pct'>%&nbsp;</span> <span class='off'>OFF&nbsp;</span> <span class='txt'>all full-priced items</span>";
								// SG: 15% OFF min 2 pairs, full price
								var discount = "<span class='XX'>15</span> <span class='pct'>%&nbsp;</span> <span class='off'>OFF&nbsp;</span> <span class='txt'>min 2 pairs, full price</span>";
								// KR: 20% 할인 모든 정상가 상품
								//var discount = "<span class='XX'>20</span> <span class='pct'>%&nbsp;</span> <span class='off'>할인&nbsp;</span> <span class='txt'>모든 정상가 상품</span>";
                }
                else if (promo === "work") {
                                var title = 'Work special offer'; 
                                var image = 'yes';
                                var discount = "<span class='XX'>25</span> <span class='pct'>%&nbsp;</span> <span class='off'>OFF&nbsp;</span> <span class='txt' style='color:#ff681d'> professional footwear</span>";
                }
                else if (promo === "welcome1retail") {
                                var title = 'Welcome offer!'; 
                                var image = 'no';
                                var discount = "<span class='XX'>10</span> <span class='pct'>$&nbsp;</span> <span class='off'>OFF&nbsp;</span> <span class='txt'>min. $30 purchase</span>";
                }
                else if (promo === "welcome2retail") {
                                var title = 'Expires soon!'; 
                                var image = 'yes';
                                var discount = "<span class='XX'>10</span> <span class='pct'>$&nbsp;</span> <span class='off'>OFF&nbsp;</span> <span class='txt'>min. $30 purchase</span>";
                }				
                else if (promo === "welcome3retail") {
                                var title = 'Welcome offer!';
                                var image = 'no';
                                var discount = "<span class='txt' style='top: 5px; color:#1483c6'>Free shipping! • All orders</span>";
                }
                else { $(".apac-couponpopup").addClass("closed"); return; }

                $('.apac-couponpopup').css('visibility','visible'); 
                var discount = discount + "<span class='apac-couponpopup-atcheckout'>Use this code at checkout</span>";
				// KR: &nbsp;&nbsp;할인 쿠폰 코드 : 
				
                $('.apac-couponpopup-title').html(title); 
                if (image === "yes") { $('.apac-couponpopup-title').addClass('image'); }
                $('.apac-couponpopup-discount').html(discount);
                $('.apac-couponpopup').addClass(promo); 
                $('.apac-couponpopup-code').html(code); 
                $('.apac-couponpopup').removeClass('closed');
}

$(".apac-couponpopup-close").click(function() { $(".apac-couponpopup").addClass("closed"); localStorage.setItem("closedByUser","yes"); });

function storageCheck() {
	//console.log("running function storageCheck()");
	//localStorage.clear();
	srcStored = localStorage.getItem("srcVal");
	couponStored = localStorage.getItem("couponVal");
	if(srcStored) {
		//console.log("srcStored found : " + srcStored);
		//console.log("couponStored found : " + couponStored);
		
		// if stored data found, check expiry
		today = new Date();
		currentTime = today.getTime();
		startTimeStored = localStorage.getItem("startTimeStored");
		timeDifference = currentTime - startTimeStored;
		secondsUsed = (timeDifference / 1000 );
		//console.log("popup will expire if beyond 120 seconds");
		//console.log("running seconds difference. seconds used is : " + secondsUsed);
		
		if( secondsUsed < 3600 ) { // 3600 seconds is 1 hour
			//console.log("not yet expired...");
			var closedByUser = localStorage.getItem("closedByUser");
			if(closedByUser !== "yes") {
				//console.log("closedByUser was not triggered so display popup");
				apacpopup(srcStored,couponStored);
			} else if (window.location.href.indexOf("Cart-Show") > -1) {
				//console.log("you're in the Cart Page, so always display popup");
				apacpopup(srcStored,couponStored);
			} else {
				//console.log("closedByUser was triggered so DO NOT display popup");
			}
		} else {
			//console.log("already expired so DO NOT display popup even in Cart Page");
		}
	} else {
		//console.log("srcStored NOT found!");
	}

}

