	function checkResizeAssure() {
		ccwAssure = window.innerWidth;
		if( ccwAssure < 959 && ccwAssure > 750) {
			$("#desktop_riskFreeContainer").addClass("desktop_riskFreeContainer_resize");
			$(".desktop_riskFreeContainer_resize").css({
				"-webkit-transform" : "scale("+ccwAssure/960+")",
				"-moz-transform" : "scale("+ccwAssure/960+")",
				"-ms-transform" : "scale("+ccwAssure/960+")",
				"transform" : "scale("+ccwAssure/960+")",
			});
			$("#aboveFooterSlot").addClass("aboveFooterSlot_resize");
			$("#aboveFooterSlot").css({
				"height" : " "+ (((ccwAssure/960)*125))  +"px",
			});
