<script type="text/javascript">
    //<![CDATA[

    //***********************************************************************************
    // Here is how you need to handle the departure and arrival date validations. This
    // code is only provided as an example and has not been completed.  You will need to
    // add code in to validate the departure and arrival hour if the departure and
    // arrival dates are equal.
    //***********************************************************************************

    function getDateControls() {

        return {
            departureDateControl: $find('ctl00_body_search_dateDepart'),
            returnDateControl: $find('ctl00_body_search_dateReturn')
        };
    }

    function OnDepartureDateSelected() {

        var controls = getDateControls();
        var departureDate = controls.departureDateControl.get_selectedDate();

        if (departureDate)
            controls.returnDateControl.set_minDate(departureDate);
        else
            controls.returnDateControl.set_minDate(new Date());
    }

    function OnReturnDateSelected() {

        var controls = getDateControls();
        var returnDate = controls.returnDateControl.get_selectedDate();

        if (returnDate)
            controls.departureDateControl.set_maxDate(returnDate);
        else {
            var oneYearFromNow = new Date();
            oneYearFromNow.setDate(oneYearFromNow.getDate() + 365);
            controls.departureDateControl.set_maxDate(oneYearFromNow);
        }
    }

    //]]>
</script>


