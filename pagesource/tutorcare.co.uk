    <script type="text/javascript">
    function SetDropDownValue() {
        var hid_Id = document.getElementById('ctl00_ContentPlaceHolder1_hid_hProduct_value');
        var hid_Text = document.getElementById('ctl00_ContentPlaceHolder1_hid_hProduct_text');
        var ddl_productopen = document.getElementById('ddl_productopen');
        hid_Id.value = ddl_productopen.options[ddl_productopen.selectedIndex].value;
        hid_Text.value = ddl_productopen.options[ddl_productopen.selectedIndex].text;
    }
    function SearchValidate() {
        var PostCode = document.getElementById('ctl00_ContentPlaceHolder1_txtPostCode');
        var Fromdate = document.getElementById('ctl00_ContentPlaceHolder1_txtStartDate');
        var Todate = document.getElementById('ctl00_ContentPlaceHolder1_txtEndDate');
        var postCodeValue = PostCode.value.replace(/(^\s+|\s+$)/g, '');
        var ddl_productopen = document.getElementById('ddl_productopen');
        debugger;

        if (ddl_productopen.value == "0") {
            alert('Please select product.');
            return false;
        }
        else if (ddl_productopen.value.indexOf("_category") > -1) {
            alert('Please select valid product.');
            return false;
        }
        if (Fromdate.value == '') {
            alert('Start Date Required.');
            return false;
        }
        if (Todate.value == '') {
            alert('End Date Required.');
            return false;
        }
        var from = $('#ctl00_ContentPlaceHolder1_txtStartDate').datepicker("getDate");
        var to = $('#ctl00_ContentPlaceHolder1_txtEndDate').datepicker("getDate");
        var today = new Date();
        today.setHours(0);
        today.setMinutes(0);
        today.setSeconds(0);
        if (Date.parse(from) < Date.parse(today)) {
            alert('Cannot book dates prior to today.');
            $('#ctl00_ContentPlaceHolder1_txtStartDate').datepicker('setDate', new Date());
            return false;
        }
        if (Date.parse(to) < Date.parse(today)) {
            alert('Cannot book dates prior to today.');
            return false;
        }
        if (from > to) {
            alert('Invalid Date Range');
            return false;
        }
        if (PostCode.value == "RH6 or RH6 0LG") {
            alert('Please fill in the Postcode.');
            return false;
        }
        var path = '/open-course-search.aspx?pr=' + ddl_productopen.value + '&townpost=' + postCodeValue + '&fdate=' + Fromdate.value + '&tdate=' + Todate.value + ''
        window.location.href = path;
        return false;
    }
 </script>
<script type="text/javascript">
    function ContactValidate() {
        document.getElementById('ctl00_ContentPlaceHolder1_hdn_url').value = window.location.href;
        var Name = document.getElementById('ctl00_ContentPlaceHolder1_txtName').value;
        var TelNo = document.getElementById('ctl00_ContentPlaceHolder1_txtTel').value;
        var Email = document.getElementById('ctl00_ContentPlaceHolder1_txtEmail').value;
        var Comment = document.getElementById('ctl00_ContentPlaceHolder1_txtComment').value;
        var strMessage = '';
        if (Name == 'NAME:')
            strMessage += "Please Enter Name \n";
        if (TelNo == 'TEL NO:')
            strMessage += "Tel. No Required\n";
        if (Email == 'EMAIL:') {
            strMessage += "Please Enter Email \n";
        }
        if (Email != 'EMAIL:' && !validateEmail(Email)) {
            strMessage += "Please Enter Valid Email \n";
        }
        if (Comment == 'COMMENTS/TRAINING REQUIRED:') {
            strMessage += "Please Enter Comment\n";
        }
        if (strMessage != '') {
            alert(strMessage);
            return false;
        }
        else {

            return true;
        }
    }
    function validateEmail(elementValue) {
        var emailPattern = /^\s*[\w\-\+_]+(\.[\w\-\+_]+)*\@[\w\-\+_]+\.[\w\-\+_]+(\.[\w\-\+_]+)*\s*$/;
        return emailPattern.test(elementValue);
    }
