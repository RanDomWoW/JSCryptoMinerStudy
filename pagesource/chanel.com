    <script>
        var locale = "en_WW";
        var baseurl = "//www.chanel.com/";

        var docWidth = window.innerWidth || document.documentElement.clientWidth;
        var docHeight = window.innerHeight || document.documentElement.clientHeight;
        var screenWidth = screen.width;
        var screenHeight = screen.height;
        var isPortrait = (docHeight > docWidth);
        var isMobile = ((docWidth <= 640 || docHeight <= 640) && Modernizr.touch);
        var isTablet = (screenWidth > 640 && Modernizr.touch);
        var device = (isMobile) ? "mobile" : (isTablet) ? "tablet" : "desktop";
        var iphone = (navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPod/i)) ? "iphone" : "";

        /** ADD CLASS DEVICE TO HTML **/
        var html = document.getElementsByTagName("html")[0];
        html.className = html.className + " " + device + " " + iphone;

        /** SCREEN RESOLUTION **/
        var screenformat = "portrait_mobile";
        if (isMobile || isTablet) {
            if (docWidth <= 560) {
                screenformat = (isPortrait) ? "portrait_mobile" : "landscape_mobile"; //360
            } else if (docWidth > 560 && docWidth <= 640) {
                screenformat = "landscape_mobile"; //480
            } else if (docWidth > 640 && docWidth <= 800) {
                screenformat = (isPortrait) ? "portrait" : "landscape_ipad"; //768
            } else if (docWidth > 800) {
                screenformat = "landscape_ipad"; //1024
            }
            if (window.devicePixelRatio > 1) {
                screenformat += "_2X";
            }
        } else {
            if (screenWidth <= 560) {
                screenformat = (isPortrait) ? "portrait_mobile" : "landscape_mobile"; //360
            } else if (screenWidth > 560 && screenWidth <= 640) {
                screenformat = "landscape_mobile"; //480
            } else if (screenWidth > 640 && screenWidth <= 800) {
                screenformat = (isPortrait) ? "portrait" : "landscape_ipad"; //768
            } else if (screenWidth > 800 && screenWidth <= 1152) {
                screenformat = "landscape_ipad"; //1024
            } else if (screenWidth > 1152 && screenWidth <= 1440) {
                screenformat = "1280";
            } else if (screenWidth > 1440 && screenWidth <= 1760) {
                screenformat = "1600";
            } else if (screenWidth > 1760) {
                screenformat = "1920";
            }
        }
        window.originalscreenformat = screenformat;
        window.screenformat = screenformat;

        /** COOKIE FUNCTION **/
        function createCookie(name, value, days) {
            if (days) {
                var date = new Date();
                date.setTime(date.getTime()+(days*24*60*60*1000));
                var expires = "; expires="+date.toGMTString();
            }
            else var expires = "";
            document.cookie = name+"="+value+expires+";domain="+window.location.hostname+";path=/";
        }
        function readCookie(name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for(var i=0;i < ca.length;i++) {
                var c = ca[i];
                while (c.charAt(0)==' ') c = c.substring(1,c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
            }
            return null;
        }
        function eraseCookie(name) {
            createCookie(name, "", -1);
        }

        /** GEOLOCATION **/
        var oldCookieLangAssoc = {"ar":"ar_ME","ar-me":"ar_ME","de":"de_DE","de-de":"de_DE","en":"en_US","en-as":"en_AS","en-as2":"en_AU","en-au":"en_AU","en-ca":"en_CA","en-gb":"en_GB","en-us":"en_US","es":"es_ES","es-lx":"es_LX","fr":"fr_FR","fr-fr":"fr_FR","fr-be":"fr_BE","nl-be":"nl_BE","fr-ca":"fr_CA","it":"it_IT","jp":"ja_JP","ko":"ko_KR","kr":"ko_KR","nl":"nl_NL","nl-nl":"nl_NL","pt":"pt_BR","pt-br":"pt_BR","ru":"ru_RU","ru-ru":"ru_RU","zh":"zh_CN","zh-cn":"zh_CN","zh-hk":"zh_HK","zh-tw":"zh_TW","en-sg":"en_SG"};

        var uriSegments = window.location.href.split("/");
        var uriLang = (typeof(uriSegments[3]) != "undefined") ? uriSegments[3] : null;
        if (uriLang == null || uriLang == "") {
            var cookieLang = readCookie('hp_lang');
            if (cookieLang != null && cookieLang != '') {
                if (typeof(oldCookieLangAssoc[cookieLang]) != "undefined") {
                    cookieLang = oldCookieLangAssoc[cookieLang];
                }
                if (cookieLang.localeCompare(locale) != 0) {
                    window.location.replace(baseurl + cookieLang + '/');
                } else {
                    createCookie('hp_lang', cookieLang, 362 / 2);
                }
            } else {
                window.location.href = "#languages";
            }
        } else if (typeof(uriSegments[4]) == "undefined" && uriLang.charAt(0) != "#") {
            window.location.replace(baseurl + uriLang + '/');
        } else if (typeof(uriSegments[4]) != "undefined" && uriLang.charAt(0) == "#") {
            window.location.replace(baseurl + uriLang);
        }

        /** REFERER **/
        var referer = document.referrer;
        if (referer != "") {
            var parser = document.createElement('a');
            parser.href = referer;
            if (parser.hostname.search("chanel.") == -1) {
                createCookie('initialreferer', parser.hostname);
            }
            parser = null;
        } else {
            eraseCookie('initialreferer');
        }
