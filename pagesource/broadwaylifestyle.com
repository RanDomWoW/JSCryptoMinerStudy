<script type="text/javascript">
    require( [ 'jquery', 'slick' ], function( $ ) {
        $( document ).ready( function() {
            $( '#home-slider' ).removeClass( 'hidden' );
            $( '#home-slider' ).slick( {
                dots: true,
                infinite: true,
                autoplay: true,
                autoplaySpeed: 4000,
                speed: 2000,
                slidesToShow: 1,
                adaptiveHeight: true,
                arrows: false,
                fade: false,
                pauseOnHover: false
            } );
            $( '#mobile-slider' ).removeClass( 'hidden' );
            $( '#mobile-slider' ).slick( {
                dots: true,
                infinite: true,
                autoplay: true,
                autoplaySpeed: 4000,
                speed: 2000,
                slidesToShow: 1,
                adaptiveHeight: true,
                arrows: false,
                fade: false,
                pauseOnHover: false
            } );

            function startAutoplay() {
                $( '#home-slider,#mobile-slider' ).slick( 'slickSetOption', {
                    'dots': true,
                    'autoplay': true
                }, true );
            }
            function stopAutoplay() {
                $( '#home-slider,#mobile-slider' ).slick( 'slickSetOption', {
                    'dots': false,
                    'autoplay': false
                }, true );
            }

            $( 'body' ).on( 'click touchstart', '.vid_trigger', function( e ) {
                e.preventDefault();
                var htm = $( this ).data( 'video' );
                var videoId = $( this ).attr( 'href' );
                var url = $( this ).data( 'url' );
                var player_id = $( this ).parent().find( '.video_container > div' ).attr( 'id' );

                if ( url.indexOf( 'youtube.com' ) !== -1 ) {
                    stopAutoplay();
                    // 2. This code loads the IFrame Player API code asynchronously.
                    var tag = document.createElement( 'script' );

                    tag.src = "https://www.youtube.com/player_api";
                    var firstScriptTag = document.getElementsByTagName( 'script' )[0];
                    firstScriptTag.parentNode.insertBefore( tag, firstScriptTag );

                    // 3. This function creates an <iframe> (and YouTube player)
                    //    after the API code downloads.
                    var player;

                    player = new YT.Player( player_id, {
                        height: '100%',
                        width: '100%',
                        playerVars: {
                            autoplay: 0,
                            controls: 0,
                            rel: 0,
                            playsinline: 1,
                            showinfo: 0,
                            modestbranding: 1
                        },
                        videoId: videoId,
                        events: {
                            'onReady': onPlayerReady,
                            'onStateChange': onPlayerStateChange
                        }
                    } );

                    function onPlayerReady( event ) {
                        //player.playVideo();
                    }

                    function onPlayerStateChange( event ) {
                        if ( (event.data === YT.PlayerState.ENDED) || (event.data === YT.PlayerState.PAUSED) ) {
                            startAutoplay();
                        }
                        if ( event.data === YT.PlayerState.PLAYING ) {
                            stopAutoplay();
                        }
                    }
                } else if ( url.indexOf( 'vimeo.com' ) !== -1 ) {
                    stopAutoplay();
                    var htm1 = '<iframe src="//player.vimeo.com/video/' + videoId + '?api=1&player_id=video&title=0&amp;byline=0&amp;portrait=0&amp;autoplay=0" width="100%" height="100%"  id="video" class="' + player_id + '" frameborder="0"></iframe>';

                    $( this ).parent().find( '.video_container' ).html( htm1 );
                    $( this ).parent().find( '.video_container' ).show();

                    var iframe = $( '.' + player_id )[0], player = $f( iframe );

                    player.addEvent( 'ready', function() {
                        //player.api('play');
                        player.addEvent( 'playProgress', onPlayProgress );
                        player.addEvent( 'pause', onPause );
                        player.addEvent( 'finish', onFinish );
                    } );

                    function onPlayProgress( id ) {
                        stopAutoplay();
                    }
                    function onPause( id ) {
                        startAutoplay();
                    }
                    function onFinish( id ) {
                        startAutoplay();
                    }
                }
                $( this ).closest( 'div' ).find( '.video_container' ).show();
            } );
        } );
    } );
</script>	

