            SEGMENT_MIXPANEL.setEmailOrPhoneToCookie('email',email);
            SEGMENT_MIXPANEL.triggerEvent('SW_SignUpCaratlaneOffer', {
              SW_CurrentPageURL: window.location.href,
              SW_EmailID: email
            });
