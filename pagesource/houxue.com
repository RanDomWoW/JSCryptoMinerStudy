<script type="text/javascript">
    $(function(){
        //登录弹窗
        $('#login').click(function () {
            $.ajax({
                url:'/user/login',
                type:'post',
                success:function(data){
                    $.dialog({
                        id: 'from_login',
                        title: '',
                        lock: true,
                        width: '300px',
                        height: '200px',
                        content: data
                    });
                }
            });
        });
    });

    showCalendar('');
    var i;
    var lengthnum;
    function showCalendar(type){

        i = $("#index").val();
        lengthnum = $("#lengthnum").val();
        var length = $('#length').val();
        if(type=='next'){
            i++;
        }else if(type=='prev'){
            i--;
        }
        if(i<0){
            i=0;
            if(lengthnum>0){
                lengthnum--;
        }
        }
        if(i>=length){
            i= 0;
            lengthnum++;
        }
        $('#lengthnum').val(lengthnum);
        $("#index").val(i);
        var result;
        $.ajax({
            url : '/newajax-calendar.html',
            data : {lengthnum:lengthnum},
            dataType : 'json',
            type : "post",
            async: false,
            success:function(e){
                result = e;

            },
            timeout: 30000,
            error: function (e) {
                alert("网络超时");
            }
        });
        var data = result;
        var $html = '';
        if(data=='  '){return false;}
        $html+= '<div class="calendar-head">'+
            '<ul class="calendar-head-inner">';
        if(data[i].title){
            $html+= '<li class="calendar-name title-show">距今'+data[i].title+'还有'+data[i].nowdays+'天</li>' ;
        }else{
            $html+= '<li class="calendar-name title-show">暂无考试信息！</li>' ;
        }
        $html+='</ul>' +
            '</div>' +
            '<div class="calendar-wrap">' +
            '<div class="calendar-con">' +
            '<div>' +
            '<span class="weekday inlineB">S</span><span class="weekday inlineB">M</span><span class="weekday inlineB">T</span><span class="weekday inlineB">W</span><span class="weekday inlineB">T</span><span class="weekday inlineB">F</span><span class="weekday inlineB">S</span>' +
            '</div>' +
            '<ul id="calendar-date" class="clear">';

        var firstday = data[i].week;//当月第一天是星期几
        switch(firstday)
        {
            case '1':
                $html+='<li class="eachday day-dark fl"></li>';
                break;
            case '2':
                $html+='<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>';
                break;
            case '3':
                $html+='<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>';
                break;
            case '4':
                $html+='<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>';
                break;
            case '5':
                $html+='<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>';
                break;
            case '6':
                $html+='<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>'+
                    '<li class="eachday day-dark fl"></li>';
                break;
        }
        for(var j =1;j<=data[i].days;j++){
            if(data[i].day==j){
                $html +='<li class="eachday eachday-active fl">'+j+'</li>';
            }else{
                $html+= '<li class="eachday fl">'+j+'</li>';
            }
        }

        $html+= '</ul>' +
            '</div>' +
            '<div class="calendar-bottom">' +
            '<em class="prev-time" onclick="showCalendar(\'prev\')">&lt;</em>' +
            '<em class="next-time" onclick="showCalendar(\'next\')">&gt;</em>' +
            '<div class="calendar-title title-show">'+data[i].examdate+'</div>' +
            '</div>' +
            '</div>';

        $html+='<div class="calendar-left">'+
            '<div class="calendar-left-head">'+
            '<em class="prev-time" onclick="showCalendar(\'prev\')">&lt;</em>' +
            '<em class="next-time" onclick="showCalendar(\'next\')">&gt;</em>' +
            '<div class="calendar-left-title title-show">'+data[i].examdate+'</div>' +
            '</div>'+
            '<div class="calendar-left-content">'+
            '<ul></ul>'+
            '<p></p>'+
            '</div>'+
            '<div class="calendar-left-bottom">'+
            '<span class="testUp ion-chevron-up"></span>'+
            '<span class="testDown ion-chevron-down"></span>'+
            '</div>'+
            '</div>';

        $(".calendar").html('').html($html);
        $("#length").val(data.length);
        txtmove();
        testMove();
        fadeOut();
        fadeIn(lengthnum)

    }

    function txtmove(){
        $('.calendar-name').mouseover(function (){
            var len = $(this).outerWidth(true);
            var move_width = len - $(this).closest('.calendar-head-inner').width();
            if(move_width>0){
                $(this).stop().animate({
                    'left': -move_width
                },2500);
            }
        }).mouseout(function (){
            $(this).stop().animate({
                'left':'0'
            },800);
        });
    }
    function testMove(){
        var ul=$('.calendar-left-content ul');
        var li=$('.calendar-left-content ul li');
        var len=li.length;
        if(len>5){
            var i=0;
            $('.testDown').click(function(){
                i+=-30;
                ul.animate({
                    'top':i
                },250);
                if(i<(-30*(len-6))){
                    i=(-30*(len-6))
                }
            });
            $('.testUp').click(function(){
                if(i!=0){
                    i+=30;
                    ul.animate({
                        'top':i
                    },250);
                    if(i>-30){
                        i=-30
                    }
                }
            })
        }
    }
    function fadeOut(){
        var clickArea=$('.calendar-left-content');
        var area=$('.calendar-left');
        clickArea.click(function(){
            area.fadeOut();
        })
    }
    function fadeIn(lengthnum){
        var eachDay=$('.eachday');
        var area=$('.calendar-left');
        var areaul=$('.calendar-left-content ul');
        eachDay.click(function () {
            if($(this).html()!==''){
                var date=$(this).html();
                $(this).addClass('eachday-active').siblings().removeClass('eachday-active');
                $.ajax({
                    url : '/newajax-test.html',
                    data : {lengthnum:lengthnum,date:date},
                    dataType : 'json',
                    type : "post",
//                    async: false,
                    success:function(res){
                        var temp='';
                        if(trim(res.info)){
                            for(var i=0;i< res.info.length;i++){
                                    for(var l=0;l< res.info[i].titleArr.length;l++){
                                        temp+='<li>'+res.info[i].titleArr[l]+'</li>';
                                    }

                            }
                        }else{
                            temp='<li>今天没有考试！</li>';
                        }
                        $('.calendar-bottom .title-show').html('').html(res.examdate);
                        $('.calendar-left .title-show').html('').html(res.examdate);
                        areaul.html('').html(temp);
                        area.fadeIn();
                    },
                    timeout: 30000,
                    error: function (res) {
                        alert("网络超时");
                    }
                });
            }
        }).mouseover(function(){
            if($(this).html()!==''){
                $(this).css('cursor','pointer')
            }
        })
    }


</script>
</body>
</html>