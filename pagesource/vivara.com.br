									//Comparison modal - iPhone 6P
									var windowWidth = $(window).width();
									if (windowWidth <= 414) {
										var finalHeight = 0;
										$('.td-comparision, .th-comparision').each(function() {
											var lineHeight = $(this).css('height');
											lineHeight = parseInt(lineHeight);
											
											if (lineHeight > finalHeight) {
												finalHeight = lineHeight;
											}
										});
										
										$('.th-comparision').css('height', finalHeight + 'px');
										$('.td-comparision').css('height', finalHeight + 'px');
									}
									
									$('.loading-return').hide();
								});
							}
						});
					}
					
					$(document).ready(function() {
						// Product Comparison Modal
						$('.show-compare-modal').on('click', function() {
							
							if (CompareProducts.getProductComparisonQuantity() > 1) {
								$('.viv-compare-modal').stop(true).fadeToggle();
								
								// ajusta o scroll para o modal de comparação de produtos
								$('html').addClass("modal-compare");
								
