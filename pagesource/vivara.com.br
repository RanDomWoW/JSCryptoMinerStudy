<script id="shopsocial-template" type="text/x-handlebars-template">
    <div class='col-md-2 col-sm-6 col-xs-6 sizing-class'>
      <div class='product-div' style='background-image: url("{{image_high.url}}");'>
        {{#if looks}}
          <a href="javascript:void(0);" class='IBY-hover-link toggle-modal'>
            <p style='margin-bottom:0.3em !important;'><i  class='fa fa-heart'></i> {{source.likes.count}}<span class="vivara-buy-button"><span>comprar </span></span></p>
          </a>
          <div class='IBY-modal-background' id='modal-{{id}}' style='display:none;'>
            <div class='IBY-modal-container'>
              <div class='IBY-left-pagination previous' data-go='{{id}}'></div>
              <div class='IBY-modal'>
                <div class='powered-by'>Powered By <img src='https://drive.google.com/uc?export=download&id=0B_yb8JM-b4XrSlhRdzgwWUEzZWc'></div>
                <div class='IBY-close'>&times;</div>
                <div class='IBY-modal-image'>
                  <div style='position:relative; max-width:400px;'>
                    <img src='{{image_high.url}}'>
                    {{#each looks as |look|}}
                      <a href='{{look.link}}' class='IBY-look-index' style='left: calc({{look.x}}%); top: calc({{look.y}}%);'>{{look.idx}}</a>
                    {{/each}}
                  </div>
                  <div class='IBY-user'>
                    <img src='{{source.user.profile_picture}}'>
                    <a href='https://instagram.com/{{source.user.username}}'>@{{source.user.username}}</a>
                  </div>
                  <p style="font-family: 'Open Sans', sans-serif !important;" class='IBY-product-caption'>{{description}}</p>
                </div>
                <div class='IBY-modal-products'>
                  <h4 style='text-align:center; margin-bottom:1em; color:#333;'>SHOP THE LOOK</h4>
                    {{#each looks as |look|}}
                      <div class='IBY-modal-product' style='position:relative;'>
                        <a class='IBY-product-details-image' href='{{look.link}}'><img src='{{lookfix look}}'></a>
                        <div class='IBY-product-details'>
                          <p><a class='IBY-product-title' href='{{look.link}}'>{{look.name}}</a></p>
                          <a class='IBY-product-button' href='{{look.link}}'>shop now</a>
                        </div>
                        <span class='IBY-look-index' style='top:2px; left:2px;'>{{look.idx}}</span>
                      </div>
                    {{/each}}
                </div>
              </div>
              <div class='IBY-right-pagination next' data-go='{{id}}'></div>
            </div>
          </div>
        {{else if alert}}
          <div class='hover-alert' style='cursor:pointer;'>
            <p style='margin-bottom:0.3em !important;'><i  class='fa fa-heart'></i> {{source.likes.count}}<span class="vivara-buy-button"><span>comprar </span></span></p>
          </div>
          <div class='IBY-modal-background' id='modal-{{id}}' style='display:none;'>
            <div class='alert-success-message' style='display:none; width:100%; text-align:center; padding:20px; color:white; font-weight:bold; background:limegreen; position:absolute; top:0; left:0;'>Email cadastrado com sucesso!</div>
            <div class='IBY-modal-container'>
              <div class='IBY-left-pagination previous' data-go='{{id}}'></div>
              <div class='IBY-modal'>
                <div class='powered-by'>Powered By <img src='https://drive.google.com/uc?export=download&id=0B_yb8JM-b4XrSlhRdzgwWUEzZWc'></div>
                <div class='IBY-close'>&times;</div>
                <div class='IBY-modal-image'>
                  <img src='{{image_high.url}}'>
                  <div class='IBY-user'>
                    <img src='{{source.user.profile_picture}}'>
                    <a href='https://instagram.com/{{source.user.username}}'>@{{source.user.username}}</a>
                  </div>
                  <p style="font-family: 'Open Sans', sans-serif !important;" class='IBY-product-caption'>{{description}}</p>
                </div>
                <div class='IBY-modal-products'>
                  <h4 style='text-align:center; margin-bottom:1em; color:#333;'>{{alert.title}}</h4>
                  <p style='text-align:center;'>{{alert.description}}</p>
                  <input type='text' name='email' id='email' placeholder="Seu email pessoal">
                  <button style='width:100%; margin-top:1em;' class="IBY-product-button avisar">
                      CADASTRAR
                  </button>
                </div>
              </div>
              <div class='IBY-right-pagination next' data-go='{{id}}'></div>
            </div>
          </div>
        {{else}}
          <a href="{{link}}" class='IBY-hover-link'>
            <p style='margin-bottom:0.3em !important;'><i  class='fa fa-heart'></i> {{source.likes.count}}<span class="vivara-buy-button"><span>comprar </span></span></p>
          </a>
          <div class='IBY-modal-background' id='modal-{{id}}' style='display:none;'>
            <div class='IBY-modal-container'>
              <div class='IBY-left-pagination previous' data-go='{{id}}'></div>
              <div class='IBY-modal'>
                <div class='powered-by'>Powered By <img src='https://drive.google.com/uc?export=download&id=0B_yb8JM-b4XrSlhRdzgwWUEzZWc'></div>
                <div class='IBY-close'>&times;</div>
                <div class='IBY-modal-image'>
                  <img src='{{image_high.url}}'>
                  <div class='IBY-user'>
                    <img src='{{source.user.profile_picture}}'>
                    <a href='https://instagram.com/{{source.user.username}}'>@{{source.user.username}}</a>
                  </div>
                  <p style="font-family: 'Open Sans', sans-serif !important;" class='IBY-product-caption'>{{description}}</p>
                </div>
                <div class='IBY-modal-products'>
                  <h4 style='text-align:center; margin-bottom:1em; color:#333;'>SHOP THE LOOK</h4>
                    <div class='IBY-modal-product'>
                      <a class='IBY-product-details-image' href='{{link}}'><img src='{{image_high.url}}'></a>
                      <div class='IBY-product-details-link'>
                        <p><a class='IBY-product-title' href='{{link}}'>{{name}}</a></p>
                        <a class='IBY-product-button' href='{{link}}'>comprar</a>
                      </div>
                    </div>
                </div>
              </div>
              <div class='IBY-right-pagination next' data-go='{{id}}'></div>
            </div>
          </div>
        {{/if}}
      </div>
    </div>
  </script>
