            if (key.keyCode == 13) {//키가 13이면 실행 (엔터는 13)
                searchList(txt);
            }
        };

		searchList = function (txt) {

		    location.href = "/job/list/?keyword=" + encodeURI(txt);

		};

		$('#mainHiro').owlCarousel({
			loop: true,
			autoplay: true,
			margin: 0,
			nav: false,
			autoplayHoverPause: true,
			animateOut: 'fadeOut',
			items:1
		});

		var jobOwl = $('#jobList');

		jobOwl.owlCarousel({
			loop: true,
			autoplay: true,
			margin: 0,
			nav: false,
			autoplayHoverPause: true,			
			items:1
		});

		$(".button-prev").click(function () {
			jobOwl.trigger('prev.owl.carousel');
		});


		$(".button-next").click(function () {
			jobOwl.trigger('next.owl.carousel');
		});


		// 로고가 없는 것들 기본이미지로.
		$('.logoimg').each(function (n) {

			$(this).error(function () {
				$(this).parent().html("").css("background", "url('/images/common/nologo.png') #fff right top no-repeat");
			});

		});

		jQuery(function ($) {
		    $('.count').each(function () {
		        $(this).prop('Counter', 0).animate({
		            Counter: $(this).text()
		        }, {
		            duration: 1000,
		            easing: 'swing',
		            step: function (now) {
		            	$(this).text(Math.ceil(now).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ","));
		            }
		        });
		    });
		});


	</script>


