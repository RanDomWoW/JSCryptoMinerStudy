  <script src="/assets/masonry/dist/masonry.pkgd.min.js"></script>

  <script>
    // Prep for masonry right away:
    // if (Modernizr.mq('only all and (min-width: 50em)')) {
    //  $('.tile-list').addClass('will-masonry');
    // };

    $(document).ready(function($)
    {
      // Do Masonry layout only if on Medium+ screen sizes
      if (Modernizr.mq('only all and (min-width: 50em)')) {

        // Reorder our list:
        var ul = $('.tile-list'),
          li = ul.children('li');

        li.detach().sort(function(a,b) {
            return $(a).data('sortby') - $(b).data('sortby');
        });

        ul.append(li);

        // Wait till all images have successfully loaded:
        $('.tile-list').imagesLoaded().done( function( instance ) {
          // Tell css masonry images are ready to show
          $('html').addClass('js-masonry-ready').removeClass('js-will-masonry');

          $('.tile-list').addClass('masonry-layout', function() {
            var $container = $('.masonry-layout');
            // initialize
            $container.masonry({
              columnWidth: '.grid-sizer',
              itemSelector: '.tile',
              transitionDuration: ''
            });
          });
        });
      };
    });
  </script>

