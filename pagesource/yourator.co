<script>
  $('.flash-message-close').click(function(e){
    e.preventDefault();
    $(this).parent().parent().remove();
  });

  function flashMessageIcon(type) {
    switch(type){
      case 'success':
        return 'fa fa-check';
        break;
      case 'notice':
        return 'fa fa-check';
        break;
      case 'info':
        return 'fa fa-info-circle';
        break;
      case 'warning':
        return 'fa fa-exclamation-triangle';
        break;
      case 'error':
        return 'fa fa-exclamation-circle';
        break;
      case 'alert':
        return 'fa fa-exclamation-triangle';
        break;
    }
  }
</script>


