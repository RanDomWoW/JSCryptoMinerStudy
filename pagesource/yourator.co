<script>
  // reset after every time modal is closed
  $(document).ready(function(){
    $('#auth-modal').on('hidden.bs.modal', function(){
      resetAuthModal();
    });
  });

  function resetAuthModal() {
    $('#auth-modal .modal-dialog').removeAttr('style');
    $('#auth-modal .modal-body').html('').removeAttr('style');
    $('#auth-modal .modal-content').removeAttr('style');
    $('#auth-modal .auth-modal-title').removeAttr('style');
    $('#auth-modal .line').remove();

    modalRightAreaOpen = false;
  }

  /**
   * set up flash message html template and active
   * @param { Array of Objects } flashMessages
   */
  function flashAuth(flashMessages) {
    var htmlText = '';

    for(var i = 0; i < flashMessages.length; i++){
      htmlText += [
        '<div class="flash-message flash-message-' + flashMessages[i].type + ' flash-message-modal">',
          '<div class="container-fluid">',
            '<a href="#" class="flash-message-close">',
              '<span class="fa fa-times" aria-hidden="true"></span>',
            '</a>',
            '<span class="' + flashMessageIcon(flashMessages[i].type) + '" aria-hidden="true"></span> ',
            flashMessages[i].message,
          '</div>',
        '</div>'
      ].join('');
    }

    $('.modal-body-middle').prepend(htmlText);

    // set up events
    $('.flash-message-close').off().click(function(e){
      e.preventDefault();

      $(this).parent().parent().remove();
    });
  }
</script>
