      $('.popular-jobs-top-list li:nth(1)').addClass('active');
      setTopBtn();

      // $('.popular-jobs-top').scrollLeft(65);

      $('.popular-jobs-top-list li a').click(function(e){
        e.preventDefault();

        $('.popular-jobs-top-list li').removeClass('active');
        $(this).parent().addClass('active');

        setTopBtn();
      });

      function setTopBtn(){
        var targetId = $('.popular-jobs-top-list li.active a').attr('target-id');

        var targetSlideIndex;

        $('.popular-job').each(function(){
          if (targetId == $(this).attr('category-id'))
            targetSlideIndex = $(this).attr('data-slick-index');
        });

        $('.popular-jobs-left').slick('slickGoTo', targetSlideIndex);
      }
    }
  });
</script>
