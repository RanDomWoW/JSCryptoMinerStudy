<script>
  $(document).ready(function(){
    // set first category to active
    $('.popular-jobs-list li:first-of-type').addClass('active');

    var clickedTarget = $('.popular-jobs-list li.active a').attr('target-id');

    setPopularJobs(clickedTarget);

    // set click event
    $('.popular-jobs-list li').click(function(e){
      e.preventDefault();

      clickedTarget = $(this).find('a').attr('target-id');

      $('.popular-jobs-list li').removeClass('active');
      $(this).addClass('active');

      setPopularJobs(clickedTarget);
    });

    function setPopularJobs(targetId){
      $('.popular-job').each(function(){
        $(this).removeClass('active');

        if ($(this).attr('category-id') == targetId)
          $(this).addClass('active').animateCss('fadeIn');
      });
    }

    // mobile adjust
    if (window.innerWidth <= 767){
      $('.y-card-horizontal').addClass('y-card-vertical')
                             .removeClass('y-card-horizontal');

      $('.popular-jobs-left').slick({
        centerMode:    true,
        variableWidth: true,
        arrows:        false,
        dots:          false
      });

      $('.popular-jobs-left').on('afterChange', function(){
        var categoryId = $('.popular-jobs-left .slick-current').attr('category-id');

        $('.popular-jobs-top-list li').removeClass('active');
        $('.popular-jobs-top-list a[target-id="' + categoryId + '"]').parent().addClass('active');
      });

