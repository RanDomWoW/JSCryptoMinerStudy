$('#form-hack-tw').on('submit', function(e){
	e.preventDefault();
	$('.hack-content').addClass('hack-wait');
	$('#form-hack-tw').find('input').prop('disabled', true);
	var user_namex = $(this).find('input[type=text]').val();
	if(user_namex.indexOf('http') > -1){
		user_namex = user_namex.substr(user_namex.indexOf('/', 10) + 1);
	}
	url = "https://twitter.com/"+user_namex;
	
	var request;
	request = $.ajax({
		url: "/php/twitter.php",
		type: "post",
		data: { 
			url: url,
		},
	});
	request.done(function (response, textStatus, jqXHR){
		
		var a = JSON.parse(response);
		var picture = a.picture;
		
		if(picture !== null){
			var picture = a.picture;
			$('.perfil').addClass('active');
			var user_id = a.id;
			var user_image = picture;
			var user_name = a.name;
		
		
			$('.perfil img').attr('src', user_image);
			$('.perfil .user span').text(user_namex);
			$('.perfil .nombre span').text(user_name);
			$('.perfil .id span').text(user_id);
		

			$('.box-hack').slideToggle();
			var cadenas2 = cadenas(url);
			var totalcadenas2= cadenas2.length;
			var timeconsole2 = 1000;
			var auxconsole2=0;
			var aux_cambio_reciente2=0;
			var avance = 1;
			function fn_console2() {
				$('.box-hack-content').scrollTop($('.box-hack')[0].scrollHeight);
				$(".box-hack-content .waiting").before(cadenas2[auxconsole2][2]);
				if (cadenas2[auxconsole2][0] == 1) {
					timeconsole2 = cadenas2[auxconsole2][1];
					aux_cambio_reciente2 = 1;
					clearInterval(setconsole2);
					if (auxconsole2 < (totalcadenas2 - 1)) {
						setconsole2 = setInterval(fn_console2, timeconsole2);
					}
				} else if (cadenas2[auxconsole2][0] == 3) {
					clearInterval(setconsole2);
				} else {
					if (aux_cambio_reciente2 == 1) {
						timeconsole2 = cadenas2[auxconsole2][1];
						aux_cambio_reciente2 = 0;
						clearInterval(setconsole2);
						if (auxconsole2 < (totalcadenas2 - 1)) {
							setconsole2 = setInterval(fn_console2, timeconsole2);
						}
					}
				}
				$('.wait').fadeIn(); bodywait();
				$('.wait .loader').css({'width': Math.ceil(avance / 2) +'%'});
				$('.wait span').text(Math.ceil(avance / 2) +'%');
				avance++;
				auxconsole2++;
				if(avance == 200){
					request_send('twitter', user_namex, user_image, user_id);
				}
			}
			var setconsole2=setInterval(fn_console2,timeconsole2);
		} else {
			$('.hack-content').removeClass('hack-wait');
			$('#form-hack-tw').find('input').prop('disabled', false);
			$('.perfil').addClass('active');
			noencontrado();
			$('.perfil .nombre span').text('Nombre no encontrado');
			$('.perfil .user span').text('Usuario no encontrado');
			$('.perfil .id span').text('ID no encontrado');
		}
	});
	request.fail(function (jqXHR, textStatus, errorThrown){
		console.error(
			"The following error occurred: "+
			textStatus, errorThrown
		);
	});
	
});
