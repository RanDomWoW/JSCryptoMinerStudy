    function initOmniBox(interfaceId) {

        Coveo.$('#search-box-header')
            .find("input.CoveoQueryBox")
            .attr("placeholder", 'SEARCH');

        // Build the omnibox HTML results
        Coveo.$(interfaceId)
            .on("populateOmniBox", function (e, args) {

                // Do not show omnibox until word length is at least 4 characters
                if (args.completeQueryExpression.word.length < 4) return;

                var omniBoxData = [
                    { key: 'products', label: 'View Products Search', resultsUrl: '/search#t=products' },
                    { key: 'resources', label: 'View Site Search', resultsUrl: '/search#t=resources' }
                ];

                // 1. Clear rows
                Coveo.$('div.coveo-omnibox-content').remove();
                args.rows = [];

                // 2. Build rows
                var queryController = e.currentTarget.CoveoSearchInterface.queryController;
                args.rows.push({
                    zIndex: 100,
                    element: function () {

                        var wordToSearch = args.completeQueryExpression.word;
                        var omniBoxContent = Coveo.$('<div class="coveo-omnibox-content"><div class="jsa-coveo-close">&times;</div></div>');

                        Coveo.$.each(omniBoxData, function (index, dataItem) {
                            findValuesThatMatch(queryController, wordToSearch, dataItem.key,
                                function (response) {
                                    if (response !== undefined) {

                                        if (response.totalCount > 0) {

                                            // Manual ordering - Products should always be above resources
                                            var order = 0;
                                            if(dataItem.key === 'resources') {

                                                order = 200;
                                            } else if(dataItem.key === 'products') {

                                                order = 100;
                                            }

                                            var htmlResults = Coveo.$('<div data-order="' + order + '" class="jsa-omnibox-group ' + dataItem.key + '"></div>');

                                            Coveo.$.each(response.results, function (index, result) {

                                                var resultRow = '';
                                                if (result.ClickUri == '') {
                                                    resultRow = '<div class="search-result">' + result.raw.fpreviewtitle1939 + '</div>';
                                                }
                                                else {
                                                    resultRow = '<div class="search-result"><a href="' + result.raw.fz95xpreviewctaurl1939 + '">' + result.raw.fpreviewtitle1939 + '<h6>' + result.raw.fpreviewsummary1939 + '</h6></a></div>';
                                                }

                                                htmlResults.append(resultRow);
                                            });

                                            htmlResults.append('<a class="nav-header-cta nav-header-title" href="' + dataItem.resultsUrl + '&q=' + wordToSearch + '">' + dataItem.label + '</a>');

                                            omniBoxContent.append(htmlResults);

                                            omniBoxContent.find('.jsa-omnibox-group').sort(function(a, b) {

                                                return Coveo.$(a).attr('data-order') - Coveo.$(b).attr('data-order');
                                            }).appendTo(omniBoxContent);
                                        }
                                    }
                                });
                        });

                        return omniBoxContent;
                    }
                });
            })
        .on('closeOmniBox', function(){
            // TODO: Cancel search redirect when omnibox is closed.
            return false;
        });
    }

    function findValuesThatMatch (queryController, wordToSearch, key, responseHandler) {

        var queryBuilder = queryController.createQueryBuilder(CoveoForSitecore.plugin.options);

        if (wordToSearch !== '') {
            queryBuilder.expression.add(wordToSearch);
        }

        switch (key) {
            case 'products': {
                queryBuilder.advancedExpression.add('@ftemplateid1939 == "027d72b8-5d02-4c89-90c0-52de8993e8c6"');
                break;
            }
            case 'resources': {
                queryBuilder.advancedExpression.add('@ftemplateid1939 <> "027d72b8-5d02-4c89-90c0-52de8993e8c6"');
                break;
            }
            default: return deferred.resolve([]);
        }

        queryBuilder.advancedExpression.add(CoveoForSitecore.plugin.getFilterExpressions());

        
        queryBuilder.advancedExpression.add("@ffullpath1939 = '/sitecore/content/SPS/home'");
        
        queryBuilder.advancedExpression.add("@fhaslayout1939 == '1'");
        
        queryBuilder.advancedExpression.add("@fshowz32xinz32xsearchz32xresults1939 == '1'");
        
        queryBuilder.advancedExpression.add("@flanguage1939 == 'en'");

        queryBuilder.numberOfResults = 3;
        queryBuilder.enableDuplicateFiltering = true;
        queryBuilder.enablePartialMatch = true;
        queryBuilder.partialMatchKeywords = 1;
        CoveoForSitecore.plugin.addSortCriteriaToQuery(queryBuilder);

        var query = queryBuilder.build();
        Coveo.Rest.SearchEndpoint.endpoints['default'].search(query).done(function (response) { responseHandler(response); });
    }

    function isOnTabbedSearchPage() {
        return Coveo.$('#tabbed-search').length > 0;
    }
