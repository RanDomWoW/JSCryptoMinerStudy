					function inputFieldOk(objectFormField, validateEmailField = false){
						var fieldOk = false;
				
						if (validateEmailField) {
							//START - CAMPO RELLENADO CORRECTAMENTE
		                	if($.trim(objectFormField.val()).length>0 && validateEmail(objectFormField.val()) ){
		                		objectFormField.removeClass('error');
				                objectFormField.addClass("input-ok");
				                objectFormField.prev().removeClass('error');
				                objectFormField.prev().addClass("input-ok");
				                objectFormField.next().removeClass("nok");
				                objectFormField.next().addClass("ok");  
				                fieldOk = true;                  		
		                    }
		                	//END - CAMPO RELLENADO CORRECTAMENTE
						}
						else {
							//START - CAMPO RELLENADO CORRECTAMENTE
		                	if($.trim(objectFormField.val()).length>0){
