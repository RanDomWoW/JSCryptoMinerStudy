<script>

		function docheckin() {
			// If longer than 6 chars, assume an eTicket number
			if ($('#checkinform #bookref').val().length > 6) {
				$('#checkinsearchoption').val('VCR');
				$('#checkinform #bookref').attr('name', 'vcrNumber');
			}
			return true;
		}


	function updateReturnCities() {

		$('#origin1').val($('#selacity1').val());
		$('#destination1').val($('#seldcity1').val());
	
	}

	function do_book_flight() {
		
		if ($('#tid').val() == 'SB') { // Return flight
			$('#submit-book-flight').attr('action', "https://dx-flights.airniugini.com.pg/dx/PXDX/#/flight-selection?direction=0&journeyType=round-trip&origin="+$('#seldcity1').val()+"&destination="+$('#selacity1').val()+"&class="+$('#seat-class').val()+"&ADT="+$('#number-adults').val()+"&CHD="+$('#number-children').val()+"&INF="+$('#number-infants').val()+"&date="+$('#fdate').val()+"&origin1="+$('#origin1').val()+"&destination1="+$('#destination1').val()+"&date1="+$('#fdate1').val());
		} else { // One way
			$('#submit-book-flight').attr('action', "https://dx-flights.airniugini.com.pg/dx/PXDX/#/flight-selection?journeyType=one-way&activeMonth="+$('#fdate').val()+"&origin="+$('#seldcity1').val()+"&destination="+$('#selacity1').val()+"&class="+$('#seat-class').val()+"&ADT="+$('#number-adults').val()+"&CHD="+$('#number-children').val()+"&INF="+$('#number-infants').val()+"&date="+$('#fdate').val());
		}

		$('#submit-book-flight').submit();
		return false;	
	}

	function do_manage_booking() {
		
		$('#submit-book-flight').attr('action', "https://dx-flights.airniugini.com.pg/dx/PXDX/#/myb?pnr="+$('#bookref2').val()+"&lastName="+$('#lastname2').val());
		$('#submit-book-flight').submit();
		return false;	
	}


	$(function() {

		$('#seldcity1, #selacity1').chosen();

		$('.hero-booking-tool').animate({opacity:1});

        // Change available destinations in booking widget whenever origin is changed

        $('#seldcity1').chosen().change(function() {
        	var dests = booking_routes[$(this).val()];
			$('#selacity1').empty();
			var sortdests = [];
        	for (var i in dests) {
      			sortdests.push($('#seldcity1 option[value="'+dests[i]+'"]').text());
        	}
        	sortdests.sort();
        	console.log(sortdests);
        	for (var key in sortdests) {
        		var dest = sortdests[key];
	       		$('#selacity1').append('<option value="'+dest.substr(dest.length-4, 3)+'">'+dest+'</option>');
        	}

        	$('#selacity1').trigger("chosen:updated");
        	updateReturnCities();
        });

		$('#selacity1').chosen().change(updateReturnCities);


	});

</script>    </div>
