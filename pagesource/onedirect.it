<script type="text/javascript">
    var miniLoginForm = new VarienForm('minilogin-form', true);
    var miniForgotPasswordForm = new VarienForm('form-validate-fp', true);
    jQuery("#email, #pass").bind('keypress', function(event) {
        if(event.keyCode == 13){
            callAjaxLogin();
        }
    });
    jQuery("#f_email_address").bind('keypress', function(event) {
        if(event.keyCode == 13){
            callAjaxForgotPassword();
            event.preventDefault();
        }
    });
    jQuery(".miniLogin-button").click(function() {
        callAjaxLogin();
    });
    jQuery(".forgotPasswordButton").click(function() {
        callAjaxForgotPassword();
    });

    function callAjaxForgotPassword() {
        var miniForgotPasswordFormParams = jQuery("#form-validate-fp").serialize();
        if (miniForgotPasswordForm.validator && miniForgotPasswordForm.validator.validate()) {
            jQuery(".forgotPasswordButton").prop("disabled",true);
            jQuery('.display_error').html('<span class="text-success">Attendere prego, caricamento...</span>').show('slow');
            jQuery.ajax({
                type: 'POST',
                url: 'https://www.onedirect.it/customer/account/forgotPasswordPostAjax/',
                data: miniForgotPasswordFormParams,
                success: function (result) {
                    jQuery(".forgotPasswordButton").prop("disabled",false);
                    result = jQuery.parseJSON(result);
                    if (result.message) {
                        if (result.success === 'true') {
                            hideForgotPassword('login');
                            jQuery("#f_email_address").val('');
                        }
                        jQuery('.display_error').html('<span class="text-primary">'+result.message+'</span>').show('slow');
                    }
                },
                always: function(){
                    jQuery(".forgotPasswordButton").prop("disabled",false);
                    setTimeout(function () {
                        jQuery.colorbox.resize();
                    }, 1);
                }
            });
        }
    }

    function callAjaxLogin() {
        var miniLoginFormParams = jQuery("#minilogin-form").serialize();
        if (miniLoginForm.validator && miniLoginForm.validator.validate()) {
            jQuery('.display_error').html('<span class="text-success">Attendere prego, caricamento...</span>').show('slow');
            jQuery.ajax({
                type: 'POST',
                url: 'https://www.onedirect.it/customer/account/loginPostAjax/',
                data: miniLoginFormParams,
                success: function (result) {
                    if (result.error) {
                        jQuery('.display_error').html('<span class="text-primary">'+result.error+'</span>').show('slow');
                    } else {
                        if (result.success){
                            if (result.success != 'no_redirect') {
                                window.location.replace(result.success);
                            }
                        } else {
                                if ( result.charAt(0) == '<' ) {
                                    window.location.replace('https://www.onedirect.it/checkout/cart/');
                                } else {
                                    result = jQuery.parseJSON(result);
                                    if (result.success) {
                                        window.location.replace(result.success);
                                    }
                                }
                            }
                        }
                },
                error: function (result) {
                    if (result.error) {
                        jQuery('.display_error').html('<span class="text-primary">'+result.error+'</span>').show('slow');
                    }
                },
                always: function(){
                    setTimeout(function () {
                        jQuery.colorbox.resize();
                    }, 1);
                }
            });
        }
    }

    function hideForgotPassword(form) {
        jQuery('.display_error').html('');
        if (form === 'login') {
            jQuery('.forgot_password').addClass('no-display');
            jQuery('.registered-users').removeClass('no-display');
        } else {
            jQuery('.registered-users').addClass('no-display');
            jQuery('.forgot_password').removeClass('no-display');
        }
    }
</script></div></div>
