<script>
  (function ($) {
    $( document ).ready(function() {

        // exit link modal for non gov websites.
        var destination = '';
        $('a').click(function(e){

            destination = $(this).attr('href');
            var PATTERN_FOR_EXTERNAL_URLS = /^(\w+:)?\/\//;

            if(destination.indexOf("amazonaws") == -1 && destination.indexOf("addthis") == -1 && destination.indexOf(".gov") == -1 && destination.indexOf(".ctacdev.com") == -1 && destination.search(PATTERN_FOR_EXTERNAL_URLS) !== -1){
              e.preventDefault();
              $('#exitLink').modal('show');
            }
        });
        $('#exitLink .btn-ok').click(function(){
            window.location.href = destination;
        });

        //calling owlcarousel
        $('.owl-carousel').owlCarousel({
            navigation : false, // Show next and prev buttons
            slideSpeed : 300,
            paginationSpeed : 400,
            singleItem : true,
            afterInit : afterOWLinit
            // "singleItem:true" is a shortcut for:
            // items : 1,
            // itemsDesktop : false,
            // itemsDesktopSmall : false,
            // itemsTablet: false,
            // itemsMobile : false
        });
        function afterOWLinit() {
            // adding A to div.owl-page
            $('.owl-controls .owl-page').append('');

            adjustSizeOwl();

            // add Custom PREV NEXT controls
            $('.owl-pagination').prepend('<a href="#prev" class="prev-owl" tabindex="-1" aria-label="previous image left arrow"><i class="fa fa-chevron-left" aria-hidden="true" title="Previous Slide"></i></a>');
            $('.owl-pagination').append('<a href="#next" tabindex="-1" class="next-owl" aria-label="next image right arrow"><i class="fa fa-chevron-right" aria-hidden="true" title="Next Slide"></i></a>');

            // set Custom event for NEXT custom control
            $(".next-owl").click(function () {
                $('.owl-carousel').trigger('owl.next');
            });

            // set Custom event for PREV custom control
            $(".prev-owl").click(function () {
                $('.owl-carousel').trigger('owl.prev');
            });
        }

        function adjustSizeOwl() {
            var paginatorsLink = $('.owl-controls .owl-page');

            $(".owl-item span.field-content div.item").each(function (i) {

                $(paginatorsLink[i])
                    // i - counter
                    // Give some styles and set background image for pagination item
                    .css({
                    })
                    // set Custom Event for pagination item
                    .click(function () {
                        $('.owl-carousel').trigger('owl.goTo', i);
                    });

                $(paginatorsLink[i]).find("span").css({
                    'background': 'url(' + $(this).attr('data-thumb') + ') center center no-repeat',
                    'background-color': 'transparent',
                    '-webkit-background-size': 'cover',
                    '-moz-background-size': 'cover',
                    '-o-background-size': 'cover',
                    'background-size': 'cover',
                    '-webkit-border-radius': '0px',
