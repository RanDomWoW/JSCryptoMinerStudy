        function screen_resize() {
            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) === false && $(window).width() > 768) {
                $('#menu .level_0 > li').each(function() {
                    var $this = $(this);

                    $this.children('ul').width($this.width());
                });
                
            } else {
                $('#menu .level_0 > li').each(function() {
                    $(this).children('ul').removeAttr('style');
                });
            }
        }
        screen_resize();

        $(window).on('resize', screen_resize);
    });
    
