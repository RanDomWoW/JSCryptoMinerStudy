        </div>
        <!--login box end-->
        
        <!--register box start-->
            <div class="register-box-wrap view-item" id='register-box-wrap'>
                
            </div>
        <!--register box end-->
        <!-- forget box start -->
                <div class="forget-box-wrap view-item" id='forget-box-wrap'>
