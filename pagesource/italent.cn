                                    <div class="error error-forget">
                                        <span class="login-icon-error"></span>
                                        <span class="error-msg"></span>
                                    </div>
                                    <div class="account-forget-mobile" data-name="forget-mobile">
                                        
                                        <div class="form-part">
                                            <!-- account login -->
                                            <div class="account-forget">
                                                <div class="validate-code">
                                                    <div class="input-part">
                                                        <div class="area-code-box clearfix">
                                                            <label class="lg-icon login-icon-phone"></label>
                                                            <div class="area-code">+86</div>
                                                        </div>
                                                        <input class="mobile-number form-item" type="text" placeholder="手机号码" autocomplete="off" />
                                                        <span class="botton-line"></span>
                                                    </div>
                                                    <div class="valid-code code-stop">
                                                        <span class="click-event-ban"></span>
                                                        <span class="get-code">获取验证码</span>
                                                    </div>
                                                </div>
                                                <div class="input-part">
                                                    <label class="lg-icon login-icon-valid"></label>
                                                    <input class="mobile-code form-item" type="text" placeholder="验证码" />
                                                    <span class="botton-line"></span>
                                                </div>
