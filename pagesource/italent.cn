                <div class="login-switch">
                    <div class="login-type-box"></div>
                </div>

                <!-- register log in -->
                <div class="register-in">
                    <span class="switch-way" data-type="register">
                        <span>还没账号？立即注册</span>
                        <span class="arraw-right"></span>
                    </span>
                </div>
