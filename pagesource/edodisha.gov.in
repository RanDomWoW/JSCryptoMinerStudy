    <script type="text/javascript" language="javascript">
            $(document).ready(function() {
            //debugger;
               updateTime();
			   ShoweDSummary();
               //setInterval(updateTime,10000);

        });
 function updateTime() 
  { 
   
    PageMethods.ShowTotalLetterCount(0,onSucess, onError);

    function onSucess(result) {
                //debugger;
            var n=result.split("|");
            $('.TotalDes').odoTicker
                ({                
                    number: n[0]                
                });
            $('.TodayDes').odoTicker
                ({                
                    number: n[1]
                });
            $('.TotalRec').odoTicker
            ({
                number: n[2]
            });
            $('.TodayRec').odoTicker
            ({
                number: n[3]
            });
            $('.TotalCitz').odoTicker
            ({
                number: n[4]
            });
            $('.TodayCitz').odoTicker
            ({
                number: n[5]
            });            
            ltrNumWid();
        }
        function onError(result) {
                //alert('Something wrong.');
            }
        }
		function ShoweDSummary() {

            PageMethods.ShoweDSummaryDetails(0, onSucess, onError);

            function onSucess(result) {
                var m = result.split("|");
                document.getElementById('UsersCount').innerHTML = numberWithCommas(m[0]);
                document.getElementById('OfficesCount').innerHTML = numberWithCommas(m[1]);
                document.getElementById('TrainedUserCount').innerHTML = numberWithCommas(m[2]);
                document.getElementById('TotalLetterCount').innerHTML = m[3];
                document.getElementById('TotalFilesCount').innerHTML = m[4];
                document.getElementById('TranDailyCount').innerHTML = numberWithCommas(m[5]) + '+';
                document.getElementById('UploadDailySize').innerHTML = m[6] + ' GB';
                document.getElementById('DownloadDailySize').innerHTML = m[7] + ' GB';
                document.getElementById('LetterSize').innerHTML = m[8] + ' TB';

            }
            function onError(result) {
                //alert('Something wrong.');
            }
        }
        function numberWithCommas(x) {
            var parts = x.toString().split(".");
            parts[0] = parts[0].replace(/\B(?=(\d{3})+(?!\d))/g, ",");
            return parts.join(".");
        }
    </script>

