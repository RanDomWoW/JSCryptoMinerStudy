<script>
function CheckIfAlphaNumeric(objtext)
{

	for (i=0;i<(objtext.value.length);i++)
	{
		if (((objtext.value.charCodeAt(i) >= 65) && (objtext.value.charCodeAt(i) <= 90))  ||
		    ((objtext.value.charCodeAt(i) >= 97) && (objtext.value.charCodeAt(i) <= 122)) ||
		    ((objtext.value.charCodeAt(i) >= 48) && (objtext.value.charCodeAt(i) <= 57)))
		{
		}
		else
		{
		alert('订位代码仅限英文或数字！');
		objtext.value='';
		objtext.focus
		return false;
		}
	}
}
function CheckIfNumeric(objtext)
{

	for (i=0;i<(objtext.value.length);i++)
	{
		if (((objtext.value.charCodeAt(i) >= 48) && (objtext.value.charCodeAt(i) <= 57))){
		}else{
			alert('机票号码仅限输入数字！');
			objtext.value='';
			objtext.focus
			return false;
		}
	}
}
function CheckIfAlpha(objtext)
{

	for (i=0;i<(objtext.value.length);i++)
	{
		if (((objtext.value.charCodeAt(i) >= 65) && (objtext.value.charCodeAt(i) <= 90)) ||
		    ((objtext.value.charCodeAt(i) >= 97) && (objtext.value.charCodeAt(i) <= 122)))
		{}
		else
		{
		alert('请勿输入空格或其他非英文字母的符号，谢谢！');
		objtext.value='';
		objtext.focus;
		return false;
		}
	}
}

function ck_pnr(){
	var f = document.pnrquery ;
	if(f.pnr.value != ""){
		if(f.pnr.value.length != 6){
			alert("订位代码为六码的数字或英文，请确认是否填写正确，谢谢！")
			f.pnr.focus();
			return false;
		}
	}
	if(f.tktno.value != ""){
		if(f.tktno.value.length != 13){
			alert("机票号码为十三码的数字,请确认是否填写正确,谢谢！")
			f.pnr.focus();
			return false;
		}
	}
	if((f.efname.value.length == 0)||(f.elname.value.length == 0)){
		alert("请确实填写您的英文姓名，谢谢！")
		f.efname.focus();
		return false;
	}
}
</script>
