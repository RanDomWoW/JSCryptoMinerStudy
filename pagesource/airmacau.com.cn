	//   IBE Booking日期套件
	var currdate = new Date();
	var tmp = new Date((currdate.getFullYear()+1)+"/"+(currdate.getMonth()+1)+"/01");
	tmp.setDate(tmp.getDate()-1);
	var mday=(Date.parse(tmp.toString()) - Date.parse(currdate.toString()))/(1000*60*60*24);
	$("#CheckinDate").datepicker({
		dateFormat:'yy-mm-dd',
		minDate: 1,
		maxDate: mday+0,
		dayNames:["星期日","星期一","星期二","星期三","星期四","星期五","星期六"],
		dayNamesMin:["日","一","二","三","四","五","六"],
		monthNames:["一月","二月","三月","四月","五月","六月","七月","八月","九月","十月","十一月","十二月"],
		monthNamesShort:["一月","二月","三月","四月","五月","六月","七月","八月","九月","十月","十一月","十二月"],
		prevText:"上月",
		nextText:"次月",
		weekHeader:"週",
		showMonthAfterYear: true,
		changeYear: true,
		changeMonth: true,
		numberOfMonths: 1,
		onSelect: gotvalue_s
	});

	tmp = new Date((currdate.getFullYear()+1)+"/"+(currdate.getMonth()+1)+"/"+currdate.getDate());
	mday=(Date.parse(tmp.toString()) - Date.parse(currdate.toString()))/(1000*60*60*24);
	$("#CheckoutDate").datepicker({
		dateFormat:'yy-mm-dd',
		minDate: 2,
		maxDate: mday-1,
		dayNames:["星期日","星期一","星期二","星期三","星期四","星期五","星期六"],
		dayNamesMin:["日","一","二","三","四","五","六"],
		monthNames:["一月","二月","三月","四月","五月","六月","七月","八月","九月","十月","十一月","十二月"],
		monthNamesShort:["一月","二月","三月","四月","五月","六月","七月","八月","九月","十月","十一月","十二月"],
		prevText:"上月",
		nextText:"次月",
		weekHeader:"週",
		showMonthAfterYear:true,
		changeYear: true,
		changeMonth: true,
		numberOfMonths: 1,
	});
	function gotvalue_s()	{
		var tmp1=$("#CheckinDate").datepicker("getDate");
		$("#CheckoutDate").val(tmp1.getFullYear()+"-"+(tmp1.getMonth()+1)+"-"+(tmp1.getDate()+2));
		$("#CheckoutDate") .datepicker("option", "minDate", new Date(tmp1.getTime()));
	}

	});
})(jQuery);

</script>

