				jQuery(document).ready(function(){
					jQuery('.qtyplus').click(function(e){
						e.preventDefault();
						fieldName = jQuery(this).attr('field');
						var currentVal = parseFloat(jQuery('input[name='+fieldName+']').val());
						if (!isNaN(currentVal)) {
							if(currentVal % 0.5) {
								
								currentVal = (Math.ceil(currentVal * 2) / 2).toFixed(1)
								jQuery('input[name='+fieldName+']').val(parseFloat(currentVal));
								
								//jQuery('input[name='+fieldName+']').val((Math.ceil(currentVal * 2) / 2).toFixed(1));
							} else {
								jQuery('input[name='+fieldName+']').val(currentVal + 0.5);	
							}
						} else {
							jQuery('input[name='+fieldName+']').val(0);
						}
					});
					jQuery(".qtyminus").click(function(e) {
						e.preventDefault();
						fieldName = jQuery(this).attr('field');
						var currentVal = parseFloat(jQuery('input[name='+fieldName+']').val());
						if (!isNaN(currentVal) && currentVal > 0) {
							if(currentVal % 0.5) {
								
								currentVal = (Math.floor(currentVal * 2) / 2).toFixed(1)
								jQuery('input[name='+fieldName+']').val(parseFloat(currentVal));
								
								//jQuery('input[name='+fieldName+']').val((Math.floor(currentVal * 2) / 2).toFixed(1));

							} else {
								jQuery('input[name='+fieldName+']').val(currentVal - 0.5);	
							}
						} else {
							jQuery('input[name='+fieldName+']').val(0);
						}
					});
					jQuery( "#reCalcForm" ).click(function() {
						jQuery( "#calcs" ).submit();
					});
					jQuery("#calcs").submit(function( event ) {
						var temp = jQuery('#wheel_width').val().replace(".", "-");
						jQuery('#wheel_width').val(temp);
					});
				});
