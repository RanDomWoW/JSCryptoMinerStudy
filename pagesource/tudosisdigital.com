    $("#load").attr("start", 0)
    start = $("#load").attr("start"), end = $("#load").attr("end");
    // $("#load").remove();
    setTimeout(function(){
    $("#load").css("display","inline-block");   
            $("#load").attr("type","populares")
    getPost(start, end, $("#mas_popular").attr("type"), false);  
  },1000);
  });
  
  // cargar mas al scroll
  win.scroll(function(){
    //console.log("scroll");
