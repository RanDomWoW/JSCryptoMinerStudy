                //When creapolis contains submenus.
            } else {
                showCoopernia();
            }

            executeCallToAction();

            //Trigger Init for Partial View
            $(document).trigger('initPartial');

            if (navigator.appVersion.match(/MSIE [\d.]+/)) {
                var placeholderText = 'Some Placeholder Text';
                $(".required").each(function () {
                    $(this).removeClass("required");
                });

                $("[placeholder]").each(function (index, value) {
                    $(this).val($(this).attr("placeholder"));
                    $(this).blur(function () {
                        $(this).val() == '' ? $(this).val($(this).attr("placeholder")) : false;
                    });
                    $(this).focus(function () {
                        $(this).val() == $(this).attr("placeholder") ? $(this).val('') : false;
                    });
                });
            }
        });

        //TODO DB: move to separate file
        function executeCallToAction() {
            var action = getQuerystring('action', null);

            if (action) {
                switch (action) {
                    case 'tdp-upgrade': CalloutsManager.svaPeruUpgrade.show(); break;
                    case 'callout-premium-activated': CalloutsManager.premiumActivated.show(false); break;
                    case 'callout-to-be-verified': CalloutsManager.premiumActivatedAndUserDataIsGoingToBeVerified.show(false); break;
                }
            }
        }

