<script>
  function countup(targetElmId, countSpeed) {
    var target = $(targetElmId);
    var countMax = target.attr('data-countup-max');
    var counter = parseInt(target.text(), 10);

    if (counter >= countMax) { return; }

    var interval = setInterval(function() {
      target.text(++counter);
      if(counter < countMax) { return; }
      clearInterval(interval);
    }, countSpeed);
  }

  $('#count-old').on('inview', function(e, isInView) {
    if (!isInView) { return; }
    countup('#count-old', 10);
    $(this).off('inview'); // don't need to call countup twice.
  });

  $('#inoue').on('inview', function(e, isInView) {
    if (!isInView) { return; }
    countup('#count-lang1', 50);
    countup('#count-lang2', 100);
    countup('#count-year', 200);
    $(this).off('inview'); // don't need to call countup twice.
  });
</script>

