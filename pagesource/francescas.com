	jQuery("#global-cart-popup #amount").html('$'+ (basketSubtotal + removeSubtotal).toFixed(2)); 

}

function toRemove(productCode) {
	var removeQty = 0; 
	var removeSubtotal = 0;

	jQuery('#global-cart-popup .product-to-undo').each(function(idx){
		if (this.id == productCode){
			this.style.display = 'block';
		}
	});

	jQuery('[name=removed]').each(function(idx){
		if (this.id == productCode){
			this.value = '1';
		}
	});
	jQuery('#global-cart-popup .globalCartItemInfo').each(function(idx){
		if (this.id == productCode){
			this.style.display = 'none';
			removeQty = removeQty + parseInt(jQuery('#qty_'+productCode).attr("value"));
			removeSubtotal = removeSubtotal + parseFloat(jQuery('#price_'+productCode).attr("value").replace('$', ''));
		}
	});

	var basketQty = parseInt(jQuery("#globalBasket .ml-header-global-cart-count b").html());
	basketQty = basketQty - removeQty;

	jQuery("#globalBasket .ml-header-global-cart-count b").html(basketQty);

    var basketSubtotalValue = jQuery("#globalBasket .ml-header-global-cart-price").html();
	var basketSubtotal = parseFloat(basketSubtotalValue.replace(/\(|\)/g,'').replace("$",''));
	
	jQuery("#globalBasket .ml-header-global-cart-price").html('$'+ (basketSubtotal - removeSubtotal).toFixed(2));
