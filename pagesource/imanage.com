			function prevent_non_business_mail(form) {
			//This script prevents Marketo form submission if a user enters non-business email (Gmail, Hotmail, Yahoo, etc.)
			// Please include the email domains you would like to block in this list
			var invalidDomains = ["@gmail.","@yahoo.","@hotmail.","@live.","@aol.","@outlook.","@icloud.","@comcast.net"];
			var invalidFirstName = ["test","example","12345"];
			var invalidLastName = ["test","example","12345"];

			form.onValidate(function(){
			  //Validate Email
			  var email = form.vals().Email;
			  if(email){
				if(!isEmailGood(email)) {
				  form.submittable(false);
				  var emailElem = form.getFormElem().find("#Email");
				  form.showErrorMessage("Must be a business email.", emailElem);
				  form.submittable(false);
				  return false;
				}else{
				  form.submittable(true);
				}
			  }
			});

			//Validate Email from list
			function isEmailGood(email) {
				for(var i=0; i < invalidDomains.length; i++) {
				  var domain = invalidDomains[i];
				  if (email.indexOf(domain) != -1) {
					return false;
				  }
				}
				return true;
			}
		}
		
		function validateFirstLastName(form) {
			
			var invalidFirstName = ["test","example","12345"];
			var invalidLastName = ["test","example","12345"];

			form.onValidate(function(){
			  //Validate FirstName
			  var firstname = form.vals().FirstName;
			  if(firstname){
				if(!isFirstNameGood(firstname)) {
				  var firstnameElem = form.getFormElem().find("#FirstName");
				  form.showErrorMessage("Invalid First Name.", firstnameElem);
				  form.submittable(false);
				  return false;
				}else{
				  form.submittable(true);
				}
			  }
			  
			  //Validate LastName
			  var lastname = form.vals().LastName;
			  if(lastname){
				if(!isLastNameGood(lastname)) {
				  var lastnameElem = form.getFormElem().find("#LastName");
				  form.showErrorMessage("Invalid Last Name.", lastnameElem);
				  form.submittable(false);
				  return false;
				}else{
				  form.submittable(true);
				}
			  }
			  
			});
			
			//Validate FirstName from list
			function isFirstNameGood(firstname) {
				for(var i=0; i < invalidFirstName.length; i++) {
				  var firstname_list = invalidFirstName[i];
				  if (firstname == firstname_list) {
					return false;
				  }
				}
				return true;
			}
			
			//Validate LastNameName from list
			function isLastNameGood(lastname) {
				for(var i=0; i < invalidLastName.length; i++) {
				  var lastname_list = invalidLastName[i];
				  if (lastname == lastname_list) {
					return false;
				  }
				}
				return true;
			}
			
		}
		
		</script>
