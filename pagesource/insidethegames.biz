        $(function()
        {
            // Hide empty slides
            var $carousels = $('.ad-carousel');

            $carousels.each(function()
            {
                var $this = $(this);
                $this.data('length', $this.find('.item').length);
                $this.data('empty', 0);
            });

            $carousels.carousel({
                interval: 15000,
                keyboard: false
            });

            $carousels.on('slid.bs.carousel', function(e)
            {
                var $this = $(this);
                var $active = $this.find('.active');
                var height = $active.height();

                if (height < 1)
                {
                    var consecutive = $this.data('empty') + 1;
                    var length = $this.data('length');

                    $this.data('empty', consecutive);

                    if (consecutive < length)
                    {
                        $this.carousel('next');
                    }
                }
                else
                {
                    $this.data('empty', 0)
                }

            });
        });
    </script>

    

