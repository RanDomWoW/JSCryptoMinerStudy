		function slide(){
		    if(cnt >= texts.length)
			cnt = 0;
		    if(cnt == 1){
			$('#textMessage').css('color', 'red');
		    }else{
			$('#textMessage').css('color', 'white');
		    }
		    $('#textMessage').html(texts[cnt++]);
		    $('#textMessage')
			    .fadeIn('slow').animate({opacity: 1.0}, 1000).fadeOut('fast',
			    function(){
				return slide()
			    }
		    );
		}
		$(document).ready(function(){
		    slide();
		});
