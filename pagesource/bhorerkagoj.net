    jQuery(document).ready(function (e) {
        var getdata=function (id,sele) {
            var html=' <option dat-url=""> লোড হচ্ছে... </option>';
            jQuery(sele).html(html).prop('disabled', 'disabled');
            var data = {
                'action': 'location',
                'id': id
            };
            jQuery.get("http://www.bhorerkagoj.net/wp-admin/admin-ajax.php", data, function(response) {
                if(sele=="#jala"){
                    var html=' <option dat-url="">--জেলা--</option>';
