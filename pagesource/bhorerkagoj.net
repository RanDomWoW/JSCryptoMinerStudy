                else {
                    var html=' <option dat-url="">--উপজেলা--</option>';
                }
                jQuery(sele).html(html+response).prop('disabled', false);;
            });
        };

        jQuery(document).on("change","#bibag",function () {
            var val=jQuery(this).find(":selected").val();
            getdata(val,"#jala");
        });

        jQuery(document).on("change","#jala",function () {
            var val=jQuery(this).find(":selected").val();
            getdata(val,"#upojala");
        });

        jQuery("#anusondan").click(function () {
            var upojala=jQuery("#upojala").find(":selected").data("url");
            console.log(upojala);
            var jala=jQuery("#jala").find(":selected").data("url");
            console.log(jala);
            var bibag=jQuery("#bibag").find(":selected").data("url");
            if(upojala!==undefined){
                window.location=upojala;
            }
            if(jala!==undefined && upojala===undefined){
                window.location=jala;
            }
            if(bibag!==undefined && jala===undefined){
                window.location=bibag;
            }
        })
    })
