    <script type="text/javascript">
        var _postsStreamLoading = false;
        var _postsStreamTimeStamp = "2018-03-17T00:00:00.0000000Z";

        $(document).ready(function() {
            var postsStreamBtn = $('div.posts-stream button.bt-load-more');
            var postsStreamAdd = $('div.posts-stream div.add');
            var postsStreamLoader = $('div.posts-stream div.loader ');

            var fPostStreamLoadNext = function() {
                var that = this;

                if (_postsStreamLoading == false) {
                    _postsStreamLoading = true;
                    postsStreamLoader.addClass('active');
                    var url = '/Api/Post';
                    setTimeout(function() {
                        $.ajax({
                            url: url,
                            data: { from: _postsStreamTimeStamp},
                            success: function(data) {
                                _postsStreamLoading = false;
                                _postsStreamTimeStamp = data.timeStamp;
                                postsStreamAdd.before(data.html);
                                postsStreamLoader.removeClass('active');
                            }
                        });
                    }, 500);
                }
            };

            postsStreamBtn.click(fPostStreamLoadNext);

            $(window).scroll(function() {
                if ($('#body-wrapper').height() <= ($(window).height() + $(window).scrollTop()) +500) {
                    fPostStreamLoadNext();
                }
            });

            $('#tbSearch').focus();
