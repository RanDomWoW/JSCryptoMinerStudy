<script>
$(document).ready(function(){

    // wysiwyg内のリンクファイルにアイコンをつける
    var reg = /(.*)(?:\.([^.]+$))/;
    var extensions = {
        'jpg': 'jpg',
        'jpeg': 'jpg',
        'gif': 'gif',
        'png': 'png',
        'pdf': 'pdf',
        'zip': 'zip',
        'xls': 'excel',
        'xlsx': 'excel',
        'doc': 'word',
        'docx': 'word',
        'ai': 'ai'
    };

    $('.wysiwyg_content a:not(.no-icon)').each(function(){
        var match = this.href.match(reg);

        if (match && extensions[match[2]]){
            var ext = extensions[match[2]];
            $(this).after('<span class="c-label-'+ext+'">'+ext+'</span>');
        }
    });
});
</script>
