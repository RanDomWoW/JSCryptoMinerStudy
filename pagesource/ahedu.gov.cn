    function changetabover(cur, num) {
        for (i = 1; i <= num; i++) {
            if (i == 1)
                document.getElementById("ttopmenu" + i).className = "topmenuleftout topmenu";
            else if (i == num)
                document.getElementById("ttopmenu" + i).className = "topmenurightout topmenu";
            else
                document.getElementById("ttopmenu" + i).className = "topmenumidout topmenu";

        }
        if (cur == 1)
            document.getElementById("ttopmenu" + cur).className = "topmenuleftover topmenu";
        else if (cur == num)
            document.getElementById("ttopmenu" + cur).className = "topmenurightover topmenu";
        else
            document.getElementById("ttopmenu" + cur).className = "topmenumidover topmenu";
        if (cur + 1 <= num)
            document.getElementById("ttopmenu" + (cur + 1)).className = document.getElementById("ttopmenu" + (cur + 1)).className + " menuleftline";
    }

</script>
