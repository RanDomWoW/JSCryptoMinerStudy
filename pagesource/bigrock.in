    function doABTest()
    {
        var v2;
        var params = window.location.search;
        var surl = "http://abtester.eigstorefront.net/abTest/determine?id=13";
        var domain_name = $("#field_domains-input").val();
        domain_name = trim(domain_name);
        domain_name = domain_name.replace(/s/, '');

        if((domain_name.length == 0) || (domain_name.match(/[^a-zA-Z0-9-.]+/)))
        {
            /*$("#error_msg").show();*/
            $(".br-dca-wrapper").addClass("br-error");
            $("#field_domains-input").addClass("input-error");
            $("#field_domains-input").val('');
            $("#field_domains-input").attr("placeholder", "Please enter a valid Domain Name");
            return false;
        }

        if (params !== "") {
            v2 = 'http://www.bigrock.in/domains-dca-page-v3' + params + '&domainname=' + domain_name;
        } else {
            v2 = 'http://www.bigrock.in/domains-dca-page-v3?domainname=' + domain_name;
        }

        if (window.localStorage && window.localStorage.abtestVersion13) {
            if (window.localStorage.abtestVersion13 === 'v2') {
                window.location.href = v2;
            } else {
                $('#homepageDomainSearch').submit();
            }
        } else {
            $.ajax({
                type: "GET",
                url: surl,
                contentType: "text",
                success: function(data) {
                    if (data) {
                        window.localStorage.abtestVersion13 = "v2";
                        window.location.href = v2;
                    } else {
                        window.localStorage.abtestVersion13 = "v1";
                        $('#homepageDomainSearch').submit();
                    }
                },
                error: function(xhr, status, error) { console.log(error) },
                async: false
            });
        }

    }
    function setPromoBanner(index){
        // make this banner inactive
        $('ul#banner-slider li.current').removeClass("current");
        // make next banner active
        $('ul#banner-slider li#img_banner'+index).addClass("current");
        // make this pagination inactive
        $('ul#banner-pagination a.current').removeClass("current");
        // make next pagination active
        $('ul#banner-pagination a#banner'+index).addClass("current");
    }
