        $("#hostingplan_with_domain input[name='location']").val(location);
        $("#hostingplan_with_domain input[name='type']").val(type);
        $("#hostingplan_with_domain input[name='planid']").val(planid);
        $("#hostingplan_with_domain input[name='duration']").val(duration);
        $("#hostingplan_with_domain #product_add").attr('name','hosting_add');
        $('#domainname_opt_id, #other_domain_name').remove();
        // $('.sitelock-box .upsell-bot-box.sitelock-wrap #sitelock-price-homepage').html('948');
        // $('.sitelock-box .upsell-bot-box.codeguard-wrap #codeguard-price-homepage').html('39');

        if(plan_id == "starter_us"){
            try{
                ga('send', 'pageview', 'homepage_add_starter_plan');
            }catch(e){}
        }
        if(plan_id == "business_us"){
            try{
                ga('send', 'pageview', 'homepage_add_business_plan');
            }catch(e){}
        }
        $('#new-domain-user, #existing-domain-user, .sitelock-box, .modal-footer').hide();
        $('.domain-options').show();
        $('#modal_div').modal('show');
    }
    function redirect_to_add_order(plan_id,type) {
        var form_name = 'hostingplan_'+plan_id;
        $('#'+form_name).attr('action', 'hosting.php');
        $('#'+form_name+" input[name='action']").val('add_order');
        $('#'+form_name+" input[name='type']").val(type);
        $('#'+form_name+" input[name='location']").val('us');
        $("#"+form_name).submit();
    }


    function analytics_custom_tracking()
    {
        var $input, hasTld, checkAndFire, sent = false, filteredOnce = false;
        $input = $('#field_domains-input');

        hasTld = function () {
            return ($input.val()).indexOf('.') > -1;
        };

        checkAndFire = function () {
            ga('set', 'dimension2', 1);
            gaPageView('homepage_domain_searched');
            if (hasTld() === true) {
                gaPageView('homepage_search_input_domain_tld');
            } else {
                gaPageView('homepage_search_input_domain_only');
            }
        };

        $(document).ready(function(){
            $(document).keydown(function (event) {
                if ($input.val() === "") {
                    return;
                }
                if (event.which === 13) {
                    checkAndFire();
                }
            });

            $('#go_btn').on('click', function () {
                if ($input.val() === "") {
                    return;
                }
                checkAndFire();
            });

            $('.domains-select').on('click', function () {
                if (filteredOnce === true) {
                    return;
                }
                gaPageView('homepage_search_tldfilter_viewed');
                filteredOnce = true;
            });

            $('input[name="tld[]"]').on('change', function () {
                if(sent === false) {
                    gaPageView('homepage_search_tldfilter_changed');
                }
                sent = true;
            });
        });
    }
    function gaPageView(str)
    {
        console.log(str);
        ga('send', 'pageview', str);
    }
    analytics_custom_tracking();
</script>
