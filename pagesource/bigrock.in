<script type="text/javascript">
    var step = 1;
    var timeout_id;
    $(document).keydown(function (event) {
        if (event.which == 27) {
            clearTimeout(timeout_id);
            $(".goBtnClass").attr("disabled", "");
            $(".goBtnClass").removeClass("domain-submit-disabled");
            $(".goBtnClass").addClass("domain-submit");
            $(".goBtnClass").html('Go');
