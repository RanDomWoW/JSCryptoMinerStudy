    });
    $(document).ready(function (){
        //dca input default text handler
        $('#field_domains-input').focus(function(){
            if($(this).val() == $(this).attr('data-default')){
                $(this).val('');
                $(this).addClass('active');
            }
        });
        $('#field_domains-input').blur(function(){
            if($(this).val() == ""){
                $(this).val($(this).attr('data-default'));
                $(this).removeClass('active');
            }
        });
        //by default select the first two tld
        $('#com').attr('checked', 'checked');
        $('#net').attr('checked', 'checked');
        //hide extra checkboxes
        $('#otherTlds').find('tr:gt(4)').hide();
        // toggle open and close of dropdown and change arrow to OK btn
        $('.domains-select').click(function () {
            $('.tld-dropdown').toggleClass('active');
            $('.tlds-dd').toggleClass('btn-ok');
            var action_url = $('#homepageDomainSearch').attr('action');
            if (action_url.indexOf('#opened') == -1) {
                $('#homepageDomainSearch').attr('action', action_url + "#opened");
            }
        });
//Select all toggle script
        $('.tld-dropdown input#searchall').click(function () {
            if ($(this).is(':checked') == true) {
                $('.tld-dropdown .popular-extensions input, .tld-dropdown .more-extensions input').each(function () {
                    $(this).attr('checked', true);
                });
                $('#tldDisplay').html("All");
            }
            else {
                $('.tld-dropdown .popular-extensions input, .tld-dropdown .more-extensions input').each(function () {
                    $(this).attr('checked', false);
                });
                $('#com').click();
            }
        });

// Sets Tld display name based on selection
        function changeTld()
        {
            if($('.tld-dropdown .popular-extensions input:checked, .tld-dropdown .more-extensions input:checked').length == 1 )
            {
                $('#tldDisplay').html("." + $('.popular-extensions input:checked, .more-extensions input:checked').val()+" <i style='font-size:10px; vertical-align: middle;' class='fa fa-chevron-down' aria-hidden='true'></i>");
            }
            else if($('.tld-dropdown .popular-extensions input:checked, .tld-dropdown .more-extensions input:checked').length > 1)
            {
                $('#tldDisplay').html("<div style=' width:100px; left:-10px; position:absolute'><small>Selected</small> <i style='font-size:10px; vertical-align: middle; position:absolute; top:7px; left:75px' class='fa fa-chevron-down' aria-hidden='true'></i></div>");
            }
            else if($('.tld-dropdown .popular-extensions input:checked, .tld-dropdown .more-extensions input:checked').length == 0)
            {
                $('#tldDisplay').html(".com <i style='font-size:10px; vertical-align: middle;' class='fa fa-chevron-down' aria-hidden='true'></i>");
            }
        }
        $('.tld-dropdown .popular-extensions :checkbox, .tld-dropdown .more-extensions :checkbox').click(changeTld);
        // event listener to close dropdown when clicked outside
        $(document).click(function(event)
        {
            if((!$(event.target).parents().hasClass('tld-dropdown') && !$(event.target).parents().hasClass('domains-select')) && $('.tld-dropdown').css('display') == "block" && event.target.className != "domains-select")
            {
                $('.tld-dropdown').removeClass('active');
                $('.tlds-dd').removeClass('btn-ok');
            }
        });

        //$('#field_domains-input').focus(); // Focus the cursor on the search box as soon as the page loads
        //$('#field_domains-input').val('www.'); // Focus the cursor on the search box as soon as the page loads


        $('#img_banner0 a').click(function(){
            try{
                ga('send', 'event', 'br-homepage-carousel', 'com-at-99-2nd-march-2016');
            }catch(e){}
        });


    });




    function animLoading()
    {
        step++;
        if (step == 7) step = 1;
        if (step == 1)
        {
            window.status = 'Processing. ';
            butt.html('. ');
        }
        if (step == 2)
        {
            window.status = 'Processing. ';
            butt.html('. ');
        }
        if (step == 3)
        {
            window.status = 'Processing.. ';
            butt.html('.. ');
        }
        if (step == 4)
        {
            window.status = 'Processing.. ';
            butt.html('.. ');
        }
        if (step == 5)
        {
            window.status = 'Processing... ';
            butt.html('... ');
        }
        if (step == 6)
        {
            window.status = 'Processing... ';
            butt.html('... ');
        }
        timeout_id = setTimeout("animLoading()", 200);
    }


    $('ul#banner-pagination a').click(function(){
        clearTimeout(t);
