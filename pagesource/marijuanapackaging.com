<script>
jQuery(document).ready(function($){
$('.trigger').toggleClass('js no-js');
$('ul.footer-menu ul.sub-menu').hide();

$plus = '<i class="icon-plus-circle"></i>';
$minus = '<i class="icon-minus-circle"></i>';

$('li.menu-item-has-children > a').after('<span>' + $plus + '</span>');

$('.trigger.js a').click(function (event) {
    $(this).siblings('ul.footer-menu').slideToggle(800).toggleClass('open').find('ul.sub-menu').slideUp(500);
    if ($('ul.footer-menu').not().hasClass('open')) {
        $(this).siblings('ul.footer-menu').find('li.menu-item-has-children span').addClass('plus').removeClass('minus').html($plus);
    }
    event.stopPropagation();
});
$('.trigger.js li span').click(function (event) {
    event.stopPropagation();
    $(this).siblings('ul.sub-menu').slideToggle(500).closest('li.menu-item-has-children').children('span').toggleClass('plus minus');
    if ($(this).hasClass('minus')) {
        $(this).html($minus);
    } else {
        $(this).html($plus);
        if ($(this).parent('li').find('ul.sub-menu ul.sub-menu').css('display') === 'block') {
            $(this).parent('li').find('ul.sub-menu ul.sub-menu').hide().prev('span').addClass('plus').removeClass('minus').html($plus);
        }
    }
});


});
