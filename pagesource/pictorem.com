<script type="text/javascript">
var xmlHttp;

function getajaxsearch2(str)
{
    var url="http://www.pictorem.com/ajaxsearch3.html?expand=1&keyword=" + str
    
    xmlHttp=GetXmlHttpObjectajax(stateChangedajax2)
    xmlHttp.open("GET", url , true)
    xmlHttp.send(null)
}
 
function stateChangedajax2()
{
    if (xmlHttp.readyState==4 || xmlHttp.readyState=="complete")
    {
    
        document.getElementById("displaysearchajax2").innerHTML=xmlHttp.responseText
    
    }
}
 
function GetXmlHttpObjectajax(handler)
{
    var objXmlHttp=null
 
    if (navigator.userAgent.indexOf("Opera")>=0)
    {
        alert("Opera not supported...")
        return;
    }
    if (navigator.userAgent.indexOf("MSIE")>=0)
    {
        var strName="Msxml2.XMLHTTP"
        if (navigator.appVersion.indexOf("MSIE 5.5")>=0)
        {
            strName="Microsoft.XMLHTTP"
        }
        try
        {
            objXmlHttp=new ActiveXObject(strName)
            objXmlHttp.onreadystatechange=handler
            return objXmlHttp
        }
        catch(e)
        {
            alert("Error. Scripting for ActiveX might be disabled")
            return
        }
    }
    if (navigator.userAgent.indexOf("Mozilla")>=0)
    {
        objXmlHttp=new XMLHttpRequest()
        objXmlHttp.onload=handler
        objXmlHttp.onerror=handler
        return objXmlHttp
    }
} 



</script> 

