<script>
jQuery(function($) {
	
var showTop = 500;
$('body').append('<div id="pgtop"><a href="javascript:void(0);">TOP</a></div>');
var fixedTop = $('#pgtop');
fixedTop.on('click',function(){
	$('html,body').animate({scrollTop:'0'},500);
});
$(window).on('load scroll resize',function(){
	if($(window).scrollTop() >= showTop){
		fixedTop.fadeIn('normal');
	} else if($(window).scrollTop() < showTop){
		fixedTop.fadeOut('normal');
	}
});
	
//アコーディオンスクロール本体
$window = $(window);	
var toggleArea_mj = $('.nav-toggle_mj');
toggleArea_mj.on('click','.showmore',function() {
	var dd = $(this).prev();
	if(!dd.is(':animated')){
		dd.slideDown();
		$(this).addClass('nav-toggle-active_mj');
	}
	$(this).text('▲ 目次を一部隠す ▲');
});
toggleArea_mj.on('click','.nav-toggle-active_mj',function() {
	var dd = $(this).prev(),
		$thisElm = $(this),
		currentScrollTop = $window.scrollTop(),
		posTop = $thisElm.offset().top,
		currentPosTop = posTop;
	if(!dd.is(':animated')){
		dd.slideUp({
			step: function() {
				currentPosTop = $thisElm.offset().top;
				$('html,body').animate({
				"scrollTop": currentScrollTop - (posTop - currentPosTop)
				}, 0);
			}
		});
		$(this).removeClass('nav-toggle-active_mj');
	}
	$(this).text('▼ 目次を全部開く ▼');
});
	
$('a[href^="#"]:not(.noscr)').on('click', function(event){
	event.preventDefault();
	var targetID = $(this).attr('href');
	var targetST = $(targetID).offset().top;
	$("html, body").animate({
		scrollTop: targetST + 'px'
	}, 800);
});
	
});
</script>

