<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Frameset//EN" "http://www.w3.org/TR/html4/frameset.dtd">
<html>
<head>
<title>KCI 국내 학술지 인용색인 정보 포털입니다.</title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<meta name="google-site-verification" content="nWFTZ0eXnpF0DSl6Uvmqr9vPucO73Wq4GZebWJPz46E" />
<meta name="naver-site-verification" content="df427147fc47d6cef596b26fb6ef2348dbbf4025"/>
<script type="text/javascript">
  var loc = window.location.href;
  var tmpUser = navigator.userAgent;

  if (loc.indexOf("http://") == 0 && loc.indexOf("local") == -1 ) {
    window.location.replace("https://www.kci.go.kr?referrer=" + encodeURIComponent(document.referrer));
    if (tmpUser.indexOf("iPhone") > 0 || tmpUser.indexOf("iPod") > 0 || tmpUser.indexOf("Android ") > 0) {
      document.location("https://www.kci.go.kr/kciportal/mindex.kci");
    }
  }else{
    if (tmpUser.indexOf("iPhone") > 0 || tmpUser.indexOf("iPod") > 0 || tmpUser.indexOf("Android ") > 0) {
      location.href = "/kciportal/mindex.kci";
    }else{
      var getParameter = function(param) {
        var returnValue;
        var url = location.href;
        var parameters = (url.slice(url.indexOf('?') + 1, url.length)).split('&');
        for (var i = 0; i < parameters.length; i++) {
          var varName = parameters[i].split('=')[0];
          if (varName.toUpperCase() == param.toUpperCase()) {
            returnValue = parameters[i].split('=')[1];
            return decodeURIComponent(returnValue);
          }
        }
      };
      referrer = getParameter('referrer');
      if(typeof(referrer) == "undefined"){
        referrer = document.referrer;
      }
      window.location.replace("/kciportal/index2.jsp?referrer=" + encodeURIComponent(referrer));
    }
    
  }
</script>
</head>
</html>