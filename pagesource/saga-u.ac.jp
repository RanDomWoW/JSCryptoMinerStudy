    $(document).ready(function(){
      $('.picMonthHeadTitle').clone().prependTo('.picMonthImage').removeClass('picMonthHeadTitle').addClass('spPicMonthHeadTitle');

      $(".inline").colorbox({
        inline: true,
        width:"100%",
        maxWidth: "972px"
      });

      var timer = false;
      var windowWidth = $(window).width();
      var windowWidthResized;
      $(window).on('resize', function(){
        if (timer !== false) {
          clearTimeout(timer);
        }
        timer = setTimeout(function() {
          windowWidthResized = $(window).width();
          if ( windowWidth != windowWidthResized ) {
            $(".inline").colorbox.resize({
              width:"90%",
              maxWidth: "972px"
            });
            windowWidth = $(window).width();
          }
        }, 200);
      });
    });
    $(document).ready(function(){
      $('.bxslider1').bxSlider({
        // スライドを無限ループにする
