<style>
#myVideo{position: fixed;top: 0; left: 0; display: none; width: 100%; height: 100%; z-index: 1000; background: #000000;}
.closeVideo{display: none; width: 80px; height: 80px; font-size: 0; position: fixed; right: 0; top: 0; z-index: 1001; cursor: pointer;}
.ztBtn{display: none; width: 128px; height: 126px; font-size: 0; position: fixed; top: 50%; margin-top: -64px; left: 50%; margin-top: -63px; z-index: 1001; cursor: pointer; opacity: 0.5;filter:alpha(opacity=50);}
.ztBtn:hover{opacity: 1;filter:alpha(opacity=100);}
</style>
<video id="myVideo" width="100%" height="100%" controls="controls">
	<source src="dbwave3.mp4"></source>
</video>
<div class="closeVideo"><img src="/statics/images/closeVideo.png"></div>
<div class="ztBtn"><img src="/statics/images/playBtn.png"></div>
