					}else if(domain == 'https://booking.sistic.stixcloud.com/SisticWebApp') {
						checkCart('https://ticketing.sistic.com.sg', false);				
					}else {
						if(!loginStatus) {
							//
						}
					}
				}
				else {
					beURL = domain;
					//website revamp phase 1 b
					var patronName = data.patronName;
					var patronEmail = data.patronEmail;
					
					
					//if(typeof(patronEmail)!='undefined' && patronEmail != null && patronEmail!=""){
						emar.patronEmail = patronEmail;
					//}else{
						//
					//}
				
					if(patronName.length > 13) {
						patronName = patronName.substring(0,13) + '...';
					}
					jQuery('#patronName').html('Hello ' + patronName +
					'&nbsp;&nbsp;&nbsp;<a class="user-meta" href="' + beURL + '/manageUser.do">my account</a>&nbsp;&nbsp;<span class="user-meta">|</span>'+
					'&nbsp;&nbsp;<a class="user-meta" href="' + beURL + '/SignOut.do">log out</a>&nbsp;&nbsp;&nbsp;');
					
					jQuery('.confirmorder').attr('href', beURL + '/shoppingCart.do');
					//end website revamp phase 1 b
					var productIdList = "";
					if (data.cartItemList != null && data.cartItemList.length != 0) {
						// ZW 20161026 for emarsys
						ScarabQueue.push(['setEmail', emar.patronEmail]); //patron had signed in

						//20161221 ZW - 20170630 shifted code 
						var valuesIDsArray = triggerScarabQueue(); //request parameters
						var cartItems = new Array();
						var checkCartItems = new Array(data.cartItemList.length);
						for(var i=0; i<checkCartItems.length; i++){
							cartItems.push({item: data.cartItemList[i].contentCode, price: (data.cartItemList[i].unitPrice / 100) , quantity: data.cartItemList[i].numOfSeat});
						}
						
						ScarabQueue.push(['cart', cartItems]);
						for (var i = 0; i < valuesIDsArray.length; i++) {
							ScarabQueue.push(valuesIDsArray[i]);
						}//end
					
							for (var i = 0; i < data.cartItemList.length; i++) {
								if (productIdList != ""){
										productIdList += "-" + data.cartItemList[i].productId;
								} else {
									productIdList += data.cartItemList[i].productId;
								}
								jQuery('#verticalslider ul').append('<li id="'+ data.cartItemList[i].productId + '" style="height:100px; width:210px">' +
								'<img src="assets/images/cart/sampleimagethumb1.jpg" width="75px" height="75px" />' +
								//'<img src="thumbnailURL" />' +
								data.cartItemList[i].productDesc + '</li>');
							}
						ajaxGetThumbnail(productIdList);
						timeLeft = data.timeLeft;
						jQuery("#verticalslider").carousel({
							direction: "vertical",
							dispItems: data.cartItemList.length>1?2:1
						});
						timerStart();
							//jQuery('#cartpopup').css('height',data.cartItemList.length>1?'397px':'300px');
						jQuery('#cartpopup').addClass('extend');
						jQuery('#cartpopup').css('height',data.cartItemList.length>1?'397px':'300px');
						jQuery('.cartcount').text(data.cartItemList.length);
						jQuery('.cartSection').show();
						jQuery('#emptycart').hide();
