						checkCart('https://booking.sistic.stixcloud.com/SisticWebApp', true);
					}else if(domain == 'https://booking.sistic.stixcloud.com/SisticWebApp'){
						checkCart('https://ticketing.sistic.com.sg', true);
					}
				}
			},
			error: function(e) {
				//jQuery('#patronName').html('<a href="https://booking.sistic.com.sg/SisticWebApp/LoginRedirect.do">Login / Register</a>');
				
				console.log("99");
			}
		});
	//domain check BE 
	}else if(domain === 'https://ticketing.sistic.com.sg'){
	
		jQuery.ajax({
			url: domain + '/sistic/patron/checkcart/portal',
			type: 'get',
			data: {  },
			cache: false,
			jsonpCallback: 'sistic',
			crossDomain: true,
			dataType: 'jsonp',
			success: function(data) {
			
			if (data.loginStatus == 1){
				
				if ("patronEmail" in emar) {
					ScarabQueue.push(['setEmail', emar.patronEmail]);
				}
				
				var cartItems = new Array();
				ScarabQueue.push(['cart', cartItems]);
				var valuesIDsArray = triggerScarabQueue(); //request parameters
				for (var i = 0; i < valuesIDsArray.length; i++) {
					ScarabQueue.push(valuesIDsArray[i]);
				}
			}
			else if (data.loginStatus == 0) {	
				beURL = domain;
				var patronName =  data.patron.firstName +" "+ data.patron.lastName;
	
				if(patronName.length > 13) {
					patronName = patronName.substring(0,13) + '...';
				}
						
