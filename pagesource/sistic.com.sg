function simplify() {
	$('div.info > .title').each(function (index) {
		//console.log($(this).text());
		$(this).dotdotdot( {
			ellipsis : '...',
			height : 28
		});
		var $this = $(this);
		var isLargeTruncated = $(this).triggerHandler("isTruncated");

		if(isLargeTruncated) {
			$this.siblings('.desc').dotdotdot({height: 60});
			//console.log('isLargeTruncated');
		}else {
			$this.dotdotdot( {
				height : 15
			});
			var isSmallTruncated = $this.triggerHandler("isTruncated");
			//console.log("isSmallTruncated: " + isSmallTruncated);
			if(isSmallTruncated) {
				$this.dotdotdot( {
				height : 28
			});
			$this.siblings('.desc').dotdotdot({height: 60});
			//console.log('60');
			} else {
				$this.siblings('.desc').dotdotdot({height: 75});
				//console.log('75');
			}
		}
	});

	$('#topView .entry .title').dotdotdot();

	$('.event_tb > .con').addClass('not-hover');
}
