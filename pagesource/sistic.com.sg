	playNews(); //phase 1b
	
    /*if(newsArr.length >0) {
		var interval = setInterval(function() {
			playNews();
		},timer);
		jQuery(".tbox-news").show();
    }phase 1b*/

    /*function displayNews() {
    alert(newsArrHtml[counter].replace(/(<([^>]+)>)/ig,""));
    } */

    function playNews() {
    	//phase 1b
		for(var i = 0; i<newsArr.length; i++) {
			var list = html_substr(newsArr[i], 70);
			var strippedString = newsArr[i].replace(/(<([^>]+)>)/ig,"");
			var maxLength = 70;
			if(strippedString.length > maxLength) {
				list = list + '...';
				jQuery('#news_scroll').append('<li class = "expandable" num = "' + i + '">' + list + '</li>');
			}else {
				jQuery('#news_scroll').append('<li num = "' + i + '">' + list + '</li>');
			}
		}//end phase 1b
        /*if(counter <= newsArr.length - 1) {
		jQuery('#news-content').html(html_substr(newsArr[counter], 70));
    }else {
		counter = 0;
		jQuery('#news-content').html(html_substr(newsArr[counter], 70));
        }phase 1b*/

    //get truncated string without html tags
        /*var strippedString = newsArr[counter].replace(/(<([^>]+)>)/ig,"");
    var maxLength = 70;
    if(strippedString.length > maxLength) {
		jQuery('#news-content').append("...");
		jQuery('a.news-more').attr("num", counter);
		jQuery('a.news-more').show();
    }else {
		jQuery('a.news-more').hide();
    }
		fullNews = newsArrHtml[counter];
		counter++;phase 1b*/

	//phase 1b
		if (newsArr.length > 0) {
			jQuery("ul#news_scroll").webTicker({
				speed: 50, //pixels per second
				direction: "left" //if to move left or right
			});
			jQuery("#news_content").css("display","block");
		}

    }

    function html_substr( str, count ) {

		var div = document.createElement('div');
		div.innerHTML = str;

		walk( div, track );

		function track( el ) {
			if( count > 0 ) {
				var len = el.data.length;
				count -= len;
				if( count <= 0 ) {
					el.data = el.substringData( 0, el.data.length + count );
				}
			} else {
				el.data = '';
			}
		}

		function walk( el, fn ) {
			var node = el.firstChild;
			do {
				if( node.nodeType === 3 ) {
					fn(node);
				} else if( node.nodeType === 1 && node.childNodes && node.childNodes[0] ) {
					walk( node, fn );
				}
			} while( node = node.nextSibling );
		}
		return div.innerHTML;
    }

	//phase 1b
	jQuery("li.expandable").click(function() {
		var content = $(this).attr('num');
		jQuery("li.expandable").fancybox({ //end phase 1b
			openEffect  : 'elastic',
			closeEffect : 'fade',
			autoSize : false,
			beforeLoad : function() {
				this.width  = 500;
				this.height = 150;
			},
			afterLoad   : function() {

				//this.content = fullNews
				this.content = newsArrHtml[content]; //phase 1b
			}
		});

    });


</script>





