<script type="text/template" class="list-item-template">
    <article class="row list-item">
        <div class="col-xs-12 col-sm-6 photo photo-mobile text-center">
            <a href="<%- url %>">
                <% if (isEditorPick == true) { %>
                    <span class="editors-pick">
                        Editor's pick
                    </span>
                <% } %>
                <% if (type == 'galleries') { %>
                    <span class="fa-stack fa-lg badges">
                        <i class="fa fa-stack-2x icon-badge-background"></i>
                        <i class="fa fa-stack-2x icon-badge-gallery"></i>
                    </span>
                <% } else if (type == 'collections') { %>
                    <span class="fa-stack fa-lg badges">
                        <i class="fa fa-stack-2x icon-badge-background"></i>
                        <i class="fa fa-stack-2x icon-badge-collection"></i>
                    </span>
                <% } %>
                <img src="<%- imageUrl %>" class="img-responsive lead-image" alt="<%- title %>" title="<%- title %>">
            </a>
        </div>
        <div class="col-xs-12 col-sm-6 intro intro-mobile">
                        <% if (isPromotion == true) { %>
                <h3><span class="section-title">Promotion</span></h3>
            <% } else if (sectionName != '' && showSection == true) { %>
                <h3><a href="<%- sectionUrl %>" class="section-title"><%- sectionName %></a></h3>
            <% } %>
            <h2 class="title"><a href="<%- url %>"> <%- title %> </a></h2>

            <% if (authors) { %>
                <div class="authors">
                    <% _.each(authors, function(author, index) { %><% if (author.isPublished != '') { %><a class="author-name" href="<%- author.link %>"><%- author.name %></a><% } else { %><span class="author-name"><%- author.name %></span><% } %><% if (index == authors.length - 2 && index != authors.length) { %><span class="author-name"> and </span><% } else if (index < authors.length - 1) { %><span class="author-name">, </span><% } %><% }) %>                </div>
            <% } %>

            <% if (intro) { %>
                <a class="hidden-xs intro-text" href="<%- url %>">
                    <span><%- _(intro).prune(90) %></span>
                </a>
            <% } %>

            <% if (rating && rating > 0) { %>
                <div class="rating-container">
                    <div class="gray-stars-container">
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                    </div>
                    <div class="color-stars-restrict" style="width: <%- (rating / 5 * 100) %>%">
                        <div class="color-stars-container">
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                        </div>
                    </div>
                </div>
            <% } %>

            <% if (type == 'collections' && collectionType == 'collection' && itemCount > 29) { %>
                <div class="items-count">
                    <span class="fa-stack fa-lg">
                      <i class="fa fa-circle fa-stack-2x count-circle"></i>
                      <i class="fa fa-stack-1x count-number"><%- itemCount %></i>
                    </span>
                    <p>items in this collection</p>
                </div>
            <% } %>
        </div>
    </article>
</script>
        </section>
