<script src="http://cfront.isoi.co.kr/renewal_2016/js_v2/underscore-min.js"></script>
<script>

$().ready(function() {

    $(".start_step").on("click", function(e){
        e.preventDefault();
        $(".question > li").removeClass("on");
        $(".question > li").eq(1).addClass("on");
        ajax_question(0);
    });

    $(".popup_type .next_step").on("click",function(e){
        e.preventDefault();
        var selected_no = $('input:radio[name=question0]:checked').val();
        if (typeof selected_no == "undefined") {
            alert("알맞는 문항을 선택해주세요.");
            return false;
        }
        ajax_question(selected_no);

    });

    function ajax_question(no) {
        var params = {
            url:        '/api/skin_test/test1?_=1&no=' + no ,
            type:       'GET' ,
            dataType:   'json',
            success: function(data) {
                    if (data.result == 'Y') 
                        make_result(data);
                    else
                        make_question(data);
                }
            }; 
        $.ajax(params);
    }


    function make_question(data) {
        var items = data.items;
        var step = data.step;

        $("#d_step_img").attr("src", "http://cfront.isoi.co.kr/renewal_2016/img/pc/common/type_test_step" + (step-1) +".png");
        $("#d_title").text("문항 " + step);
        $("#d_selection").children().remove();

        $.each(items, function(idx, item) {
            $("#d_selection").append(
            "<li><input type=\"radio\" value=\"" + item['no'] +"\" name=\"question0\" id=\"question0_" + (idx+1) 
            +"\" class=\"question\" ><label for=\"question0_"+ (idx+1) +"\"> " 
            + item['content'] + " </label> </li>");
        });
    }
    function make_result(data) {
        $(".question").removeClass("on");
        $(".popup_type").css({"width":'870px', "height":'712px', "marginLeft":'-435px', "marginTop":'-356px'});
        $(".result").addClass("on");
        $(".popup_type .result > li").addClass("on");

        $("#skin_result_content").append(data.result_content);
        var template = _.template(goods_list);
        $.each(data.items, function(idx, item) {
            $('#skin_result_goods').append(template(item)); 
        });
        
    }

    var goods_list  = '     <li class="check_goods" >'
				    + '     <input type="checkbox" name="goods" value="<%= goods_no %>" id="resultProduct<%= no %>">'
					+ '			<label for="resultProduct<%= no %>">'
                    + '     		<p><img src="<%= goods_list_image %>" /></p>'
					+ '				<dl>'
					+ '					<dt><%= title %></dt>'
					+ '					<dd>'
                    + '                     <% if (good_percent > 0 ) { %> '
                    + '                     <em> <%= f_good_price %>원</em> '
                    + '                     <% } %>'

                    + '                     <strong><%= f_good_saleprice %></strong>원</dd>'
					+ '				</dl>'
					+ '		    </label>'
					+ '		</li>';


    $('.goods-checkbox').click(function() {
        if ($(this).attr('isOption') == 'true') {
            alert("해당 상품은 현재 페이지에서 선택이 불가능 합니다.\n상세 페이지에서 옵션 선택 후 구매해 주세요");
            return false;
        }
    });

    $('#pc-skin-order').click(function() {
        var goods_str = $.map($('.check_goods input[type=checkbox]:checked'), function(item) {  return 'goods[]=' + $(item).val(); });
        if(goods_str == "") {
            alert("상품을 한 개 이상 선택해주세요. ");
            return; 
        }
                var returnValue = confirm("현재 고객님은 로그인 되어 있지 않은 상태입니다. 비회원으로 구매하시겠습니까?");
        if (returnValue)
        {
            location.href='/order/cart_insert?mode=order&' + goods_str.join('&');
            return false;
        }
        else
        {
            location.href='/member/login?return_url='+encodeURIComponent('/order/cart_insert?ct=&mode=order&' + goods_str.join('&'));
            return false;
        }
            });

    $('#pc-skin-cart').click(function() {
        var goods_str = $.map($('.check_goods input[type=checkbox]:checked'), function(item) { return 'goods[]=' + $(item).val(); });
        if(goods_str == "") {
            alert("상품을 한 개 이상 선택해주세요. ");
            return; 
        }
        location.href='/order/cart_insert?' + goods_str.join('&');
        return false;
    });

});
</script>

