<script>
var isSend = false;
$('form[name=fm_email]').submit(function() {

    if (!$('input[id=consult_agreement]:checked').length) {
        alert('개인정보취급방침에 동의해주세요');
        return false;
    }

    if (isSend) {
        alert('메일을 보내는 중입니다. 잠시만 기다려주세요');
        return false;
    }

    var fm = $(this)[0];

    if (fm.fromName.value == "")
    {
        alert("보내는 사람 이름을 입력하세요");
        fm.fromName.focus();
        return false;
    }
    if (fm.fromEmail.value == "" )
    {
        alert("보내는사람 이메일을 입력하세요");
        fm.fromEmail.focus();
        return false;
    }
    if (fm.comment.value == "")
    {
        alert("내용을 입력하세요.");
        fm.comment.focus();
        return false;
    }

    isSend = true;

    var params = {
        url: $(this).attr('action'),
        data: $(this).serialize(),
        type: 'POST',
        beforeSend: function() {
            hideLayer();
        }
    };

    $.ajax(params);
    return false;
});
</script>
