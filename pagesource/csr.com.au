    <script type="text/javascript">
        $(document).foundation();


        function doSearch() {
            showProgressRing();

            var dText = $('#txtDTSearchText').val();
            var mobText = $('#txtMobSearchText').val();
            var searchText = "";
            if (dText != "")
                searchText = dText;
            if (mobText != "")
                searchText = mobText;

            if (searchText != "") {
                var url = "/search-results?searchString=" + searchText + "&pageIndex=0";
                location.href = url;
            } else {
                alert('Please enter search text');
            }
        }
        $(function () {
            
            $("#divContactUs").bind("click", function () {
                window.location.href = "/About-Us/Contact-Us";
            });
            $(".right-off-canvas-toggle").bind("click", function () {
                window.scrollTo(0, 0);
                $('body').css('overflow', 'hidden');
            });
            $(".exit-off-canvas").bind("click", function () {
                $('body').css('overflow', 'auto');
            });


            $("#searchToggle").bind("click", function () {
                $('.dt-search-text-outer-wrap').slideToggle();
                $("#txtDTSearchText").focus();
            });
            $("#mobSearchToggle").bind("click", function () {
                $('.mob-search-text-outer-wrap').slideToggle();
                $("#txtMobSearchText").focus();
            });
            $('#txtDTSearchText').keypress(function (e) {
                if (e.which === 13) {
                    e.preventDefault();
                    e.stopPropagation();
                    doSearch();
                }
            });
            $('#txtMobSearchText').keypress(function (e) {
                if (e.which === 13) {
                    e.preventDefault();
                    e.stopPropagation();
                    doSearch();
                }
            });
            $("#btnDTSearch").bind("click", function () {
                doSearch();
            });
            $("#btnMobSearch").bind("click", function () {
                doSearch();
            });
            

            //$('a[data-dropdown^="mega-menu-"]').hover(function () {
            //    var str = $(this).attr('data-dropdown');
            //    var last = str.length;
            //    var order = str.substring(last-1, last);
            //    positionSubmenu(order);
            //});

            // Create simple menu dropdown with levels
            $(".levels-menu").menu();

            $(".levels-menu").each(function () {
                var maxWidth = $(this).width();
                $(this).find(".ui-menu").each(function () {
                    if ($(this).outerWidth(true) > maxWidth) {
                        maxWidth = $(this).outerWidth(true);
                    }
                });
                $(this).css("min-width", maxWidth + 10 + "px");
            });


            // Fix emphasized links
            $(".emphasized-link a").addClass("button");


            // Position dropdowns on right or left
            function positionDropdowns() {
                var windowWidth = $(window).width();

                $(".levels-menu").each(function () {

                    var yPos = $(this).offset().left;
                    if (yPos / windowWidth > 0.5) {
                        $(this).removeClass("left");
                        $(this).addClass("right");

                        $(this).menu("option", "position", { my: "right top", at: "left top" });

                    } else {
                        $(this).removeClass("right");
                        $(this).addClass("left");

                        $(this).menu("option", "position", { my: "left top", at: "right top" });
                    }

                });
            }

            $(window).bind("resize", positionDropdowns);
            $(window).bind("load", positionDropdowns);


            // Hide by default
            $("#progressRing").hide();
            $("#searchBar").hide();


        });

        function triggerCsrConnectGoal() {
            $.get("/SitecoreGoals/TriggerGoal?goalId={7938DDD4-5710-4EE3-9D45-56E93F4F8857}");
            return false;
        }

   
        $(document).ready(function () {

          

            $("div.text-image-menu .master-item").hover(function () {
                var masterItemId = this.id;
                var detailItemId = masterItemId.replace("master", "detail");

                var detailPrefix = removeAfterLastdash(detailItemId);

                $("div[id^='" + detailPrefix + "']").addClass("hide");
                $("#" + detailItemId).removeClass("hide");
            });


            $(".text-menu div.master-item span").click(function (e) {

                var masterItem = this.parentNode;
                var masterItemId = masterItem.id;
                var detailItemId = masterItemId.replace("master", "detail");

                if ($(masterItem).hasClass('arrow-down')) {
                    $('#' + detailItemId).removeClass('hide');
                    $(masterItem).removeClass('arrow-down');
                    $(masterItem).addClass('arrow-up');
                }
                else {
                    $('#' + detailItemId).addClass('hide');
                    $(masterItem).removeClass('arrow-up');
                    $(masterItem).addClass('arrow-down');
                }
            });

        });


        function removeAfterLastdash(str) {
            var lastIndexOfDash = str.lastIndexOf("-");
            return str.substring(0, lastIndexOfDash);
        }


        function positionSubmenu() {
            var order = "6";
            //last menu top item
            var navItem6 = $('a[data-dropdown="mega-menu-6"]')[0];
            var navItem6LeftPos = navItem6.offsetLeft;
            var navItem6RightPos = navItem6.offsetLeft + $(navItem6).width();
            var navItem6Width = $(navItem6).width();


            var nav = $('nav.main')[0];
            var navItem = $('a[data-dropdown="mega-menu-' + order + '"]')[0];
            var submenu = $('#mega-menu-' + order)[0];

            var navLeftPos = nav.offsetLeft;
            var navItemLeftPos = navItem.offsetLeft;
            var submenuLeftPos = submenu.offsetLeft;

            var navRightPos = navItem6RightPos;
            var navItemRightPos = navItem.offsetLeft + $(navItem).width();
            var submenuRightPos = submenu.offsetLeft + $(submenu).width();

            var navWidth = navItem6RightPos - navLeftPos;
            var navItemWidth = $(navItem).width();
            var submenuWidth = $(submenu).width();

            if ((navRightPos - navItemLeftPos) > submenuWidth)
                $(submenu).css('margin-left', navItemLeftPos + 'px');
            else
                $(submenu).css('margin-left', (navRightPos - submenuRightPos) + 'px');


        }
    </script>

