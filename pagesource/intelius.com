      $('#premier-learn-more').on('click', function(e) {
         e.preventDefault();
         $(this).closest('#premier-learn-more').find('.hidden-content').slideToggle();
         $(this).toggleClass('open');
      });

      $(document).ready(function() {
         var isDSPEnabled     = typeof disableDSP === 'undefined' || !disableDSP;
         var hasSeenDSP       = !!getCookie('dspShown');
         var shouldExitEarly  = !isDSPEnabled || hasSeenDSP;
         if (shouldExitEarly) return false;

         var DSPData          = decodeDSPCookie();

         var shouldShowPreiemerContent = DSPData.isPremierOffer;
         var hasDSPData       = !!DSPData;
         var shouldOpenDSP    = hasDSPData && validate(DSPData, 'searchType') && validate(DSPData, 'purchasePath') && validate(DSPData, 'offeringId');
         if (shouldOpenDSP) openDSP(DSPData.searchType, DSPData.offeringId, DSPData.purchasePath);

         if (shouldShowPreiemerContent) {
           $('#preiemerContent').show();
           $('#notPreiemerContent').hide();
         } else {
           $('#preiemerContent').hide();
           $('#notPreiemerContent').show();
         }
      });
