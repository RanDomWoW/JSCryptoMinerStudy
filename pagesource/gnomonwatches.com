	$(document).ready(function() {
		ww = $(window).width();
		setInterval( function() {
			var seconds = new Date().getSeconds();
			var sdegree = seconds * 6;
			var srotate = "rotate(" + sdegree + "deg)";
			$("#index-packery-sec, .second").css({"-moz-transform" : srotate, "-webkit-transform" : srotate});

		}, 1000 );


		setInterval( function() {
			var hours = new Date().getHours();
			var mins = new Date().getMinutes();
			var hdegree = hours * 30 + (mins / 2);
			var hrotate = "rotate(" + hdegree + "deg)";

			$("#index-packery-hour").css({"-moz-transform" : hrotate, "-webkit-transform" : hrotate});

		}, 1000 );


		setInterval( function() {
			var mins = new Date().getMinutes();
			var mdegree = mins * 6;
			var mrotate = "rotate(" + mdegree + "deg)";

			$("#index-packery-min").css({"-moz-transform" : mrotate, "-webkit-transform" : mrotate});

		}, 1000 );

		$(".global-popup").magnificPopup({
			type: 'iframe',
