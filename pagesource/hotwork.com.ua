<script type="text/javascript">jQuery(document).ready(function () {
    $('#header_fixx_mobile').show();
     $('#mobile-hamburger').click(function() {
         if ($('.head-menu ul, .head-menu-wide ul').is(':hidden')) {
             $('.head-menu ul, .head-menu-wide ul').slideDown();
         } else {
             $('.head-menu ul, .head-menu-wide ul').slideUp();
         }
         return false;
     });

     $(window).scroll(function(){
        if( $(window).scrollTop() >= $('#header_v2').height() ){
            $('#header_fixx_v2').show();
        } else {
            $('#header_fixx_v2').hide();
        }
    });

    $('.main-search-input, .tt_mobile_query').focus(function() {
        $(this).closest('.header_form_v2').addClass('header_form_open');

    });


    $('#header_fixx_mobile').on('submit', function(){
       var query = $('input[name=query]', $(this)).val();
       var region = $('input[name=region]', $(this)).val();
       if (query.length || region.length) {
           try {
                ga('send', 'event', "Top Panel v2", "Click", "Job search show sticky");
                console.log('"Top Panel v2", "Click", "Job search show sticky"');
            } catch (e) {}
        }

    });

    $('#default_form_v2').on('submit', function(){
       var query = $('input[name=query]', $(this)).val();
       var region = $('input[name=region]', $(this)).val();

       if (query.length || region.length) {
           try {
                ga('send', 'event', "Top Panel v2", "Click", "Job search no sticky");
                console.log('"Top Panel v2", "Click", "Job search no sticky"');
            } catch (e) {}
        }
    });


jQuery('#w0').typeahead({"name":"query","remote":{"url":"/job/autocomplete/query/?q=-QUERY-","wildcard":"-QUERY-"}});
jQuery('#w1').typeahead({"name":"regions","remote":{"url":"/job/autocomplete/region/?q=-QUERY-","wildcard":"-QUERY-"}});
jQuery('#w2').typeahead({"name":"query","remote":{"url":"/job/autocomplete/query/?q=-QUERY-","wildcard":"-QUERY-"}});
jQuery('#w3').typeahead({"name":"regions","remote":{"url":"/job/autocomplete/region/?q=-QUERY-","wildcard":"-QUERY-"}});
    $('.job-post-link').on('click', function(e){
        e.preventDefault();

    });
    $('#post-job-info').on('shown.bs.modal', function () {
        ga('send', 'event', 'Employer', 'Show', $('#post-job-info').data('track'));
        
    }).on('click', '.action-partner', function() {
        ga('send', 'event', 'Employer', 'Click', $('#post-job-info').data('track'));
        
        $('#post-job-info').modal('hide');
    }).on('click', '.action-close', function() {       
        $('#post-job-info').modal('hide');
    }).on('hidden.bs.modal', function() {
        ga('send', 'event', 'Employer', 'Close', $('#post-job-info').data('track'));
    });
});</script></body>
