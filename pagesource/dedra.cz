    <script>
        subscriberFormOnSuccess = function (data) {

            if (data.result) {
                $('#email-newsletter').val('');
                $('#email-newsletter').attr('placeholder', data.message);
            } else {
                processError(data.message);
            }
        }

        var scrollTimerDodani = null;
        $(window).scroll(function () {
            if (scrollTimerDodani) {
                clearTimeout(scrollTimerDodani);
            }
            scrollTimerDodani = setTimeout(handleScrollDodani, 1);
        });

        function handleScrollDodani() {
            var $bannerDelivery = $(".bottom-banner");

            var bannerPosition = $bannerDelivery.offset().top;
            var bannerWidth = $bannerDelivery.width();

            var windowsPosition = $(window).scrollTop();

            var availableRelativeBannerWidth = (bannerWidth - 140 /* do not hit family */) / bannerWidth;

            var carRelativePosition = 0;
            if (windowsPosition > 0) {
                carRelativePosition = windowsPosition / (bannerPosition - 80 /* Fixed header height */);
            }

            var carPosition = Math.min(availableRelativeBannerWidth, carRelativePosition) * 100;
            $bannerDelivery.css('background-position', carPosition + '% 50%');
        }

        $(function () {
            /*Owl carousel for main-banner*/
            $("#homepage-banner-carousel").owlCarousel({
                items: 1,
                nav: true, // Show next and prev buttons
                slideSpeed: 300,
                paginationSpeed: 400,
                lazyLoad: true,
                loop: true,
            });

            var stagePadding = 80;

            if (window.innerWidth < 1330) {
                stagePadding = 0;
            }

            /*Owl carousel for main-banner*/
            $(".homepage-product-carousel").owlCarousel({
                items: 3,
                slideBy: 3,
                dots: false,
                nav: true, // Show next and prev buttons
                slideSpeed: 300,
                paginationSpeed: 400,
                loop: true,
                margin: 15,
                stagePadding: stagePadding,
                responsive: {
                    0: {
                        items: 1,
                        nav: true
                    },
                    600: {
                        items: 2,
                        nav: false
                    },
                    1000: {
                        items: 3,
                        nav: true,
                        loop: false
                    }
                }
            });

            if (typeof (tourPublishDates) !== 'undefined') {
                var unreadCount = tourPublishDates.length;
                var lastVisit = getCookie('ToursLastAccess');

                for (var i = 0; i < tourPublishDates.length; i++) {
                    if (tourPublishDates[i] < lastVisit) {
                        unreadCount--;
                    }
                }
                if (unreadCount > 0) {
                    $('.section-hp-tour .newbies-count').text(unreadCount);
                    $('.section-hp-tour .newbies-count').show();
                }
            }

            if (typeof (newsPublishDates) !== 'undefined') {
                var unreadCount = newsPublishDates.length;
                var lastVisit = getCookie('NewsLastAccess');

                for (var i = 0; i < newsPublishDates.length; i++) {
                    if (newsPublishDates[i] < lastVisit) {
                        unreadCount--;
                    }
                }
                if (unreadCount > 0) {
                    $('.section-hp-hot-news .newbies-count').text(unreadCount);
                    $('.section-hp-hot-news .newbies-count').show();
                }
            }

        });

        function getCookie(name) {
            var value = "; " + document.cookie;
            var parts = value.split("; " + name + "=");
            if (parts.length == 2) return parts.pop().split(";").shift();
        }
    </script>

