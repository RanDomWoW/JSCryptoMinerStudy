$(document).ready(function() {
  $('.fastnav p a').click(function(){
    to = $($(this).attr("href")).offset().top - 20 + 'px';
    $("html, body").animate({
        scrollTop: to
      },
      {
        duration: 1000,
        easing: "swing"
      });
    return false;
  });

//  links = $(".main_news .nav-tabs a[href^=#ma_]");
//  $(links[Math.floor(Math.random() * 3)]).tab('show');

});

var h_hght = 0;
var h_mrg = 0;
				
$(function(){
	var elem = $('.fastnav');
	var top = $(this).scrollTop();
	if(top > h_hght){
		elem.css('top', h_mrg);
	}			
	$(window).scroll(function(){
		top = $(this).scrollTop();
		
		if (top+h_mrg < h_hght) {
			elem.css('top', (h_hght-top));
		} else {
			elem.css('top', h_mrg);
		}
	});
});
</script>


