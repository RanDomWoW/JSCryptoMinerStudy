<div id="booking-options" role="booking" class="hidden-xs content-panel booking-widget-panel">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <form id="bookingNowForm" name="bookingNowForm" method="GET" action="">
                    <input type="hidden" id="target" name="target" value="/tour-options/tmpl"/>
                    <label class="purchase-tickets-label" for="booking-ticket-type">purchase tickets</label>
                    <div class="collapsedContent">
                        <select id="booking-ticket-type" name="booking-ticket-type" class="bookingpicker"
                                title="Select a Ticket Type">
                            <option selected value="">
                                Select a Ticket Type
                            </option>
                            <option value="ticket-early-bird-link">
                                Early Bird Tour Ticket
                            </option>
                            <option value="ticket-day-tour-link">
                                Day Tour (AM &amp; PM) Ticket
                            </option>
                            <option value="ticket-night-tour-link">
                                Night Tour Ticket
                            </option>
                            <option value="ticket-island-tour-link">
                                Alcatraz &amp; Angel Island Tour Ticket
                            </option>
                        </select>
                        <div class="datepicker-wrapper">
                            <input type="text" name="booking-date" class="datepicker" title="Select a date" readonly placeholder="date"/>
