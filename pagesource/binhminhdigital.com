<script type="text/javascript">
    document.getElementById("textCbInput")
        .addEventListener("keyup", function (event) {
            event.preventDefault();
            if (event.keyCode == 13) {
                SendChatCus();
            }
        });
    function setCookie(key, value) {
        var expires = new Date();
        expires.setTime(expires.getTime() + (1 * 24 * 60 * 60 * 1000));
        document.cookie = key + '=' + value + ';expires=' + expires.toUTCString();
    }
    function toggleCb() {
        if ($("#chatBox").attr('class') == "cBmin") {
            document.getElementById("chatBox").className = "";
            setCookie('ChatShow', 'True');
            GetChat();
        }
        else {
            document.getElementById("chatBox").className = "cBmin";
            setCookie('ChatShow', 'False');
        }
    }
    function HiddenCb() {
        document.getElementById("chatBox").className = "cBmin";
    }
    function ScrollChatBox() {
        var myDiv = $(".cbContent");
        var myMess = $("#cbMessList");
        myDiv.scrollTop(myMess.height() + 230 - myDiv.height());
    }
    function ShowHelloMess() {
        var helloMess = $('#helloMess').val();
        var str = "<li class=\"messadName\">Hỗ trợ viên</li>";
        str += "<li class=\"messad\">" + helloMess + "</li>";
        setTimeout(function () {
            $('#cbMessList').html($('#cbMessList').html() + str);
            ScrollChatBox();
        }, 1000);
    }
    function SaveInfo() {
        var cusGuid = $('#cusGuid').val();
        var cusEmail = $('#inputEmail').val();
        var cusPhone = $('#inputPhone').val();
        $.ajax({
            type: "POST",
            url: "/ChatBox/SaveCusInfo",
            data: { guid: cusGuid, email: cusEmail, phone: cusPhone },
            dataType: "json",
            success: function (response) {
                if (response.status != "0") {
                    $('.inputInfo-title').html('Đã cập nhật thông tin!');
                    if (cusGuid != response.status) {
                        $('#cusGuid').val(response.status);
                        setCookie('CusGuid', response.status);
                    }
                } else {
                    $('.inputInfo-title').html('Không thể cập nhật thông tin!');
                }
            }
        });
    }
    function SendMessOff() {
        var cusGuid = $('#cusGuid').val();
        var cusName = $('#inputName').val();
        var cusEmail = $('#inputEmail').val();
        var cusPhone = $('#inputPhone').val();
        var mess = $('#inputMessOff').val();
        var sttsSend = $('#sttsSend').val();
        $('#btnSendMessOff').attr('class', 'hidden');
        $('#messBoxOff').html('Đang gửi tin nhắn!');
        if (sttsSend == 1) {
            $('#sttsSend').val("0");
            $.ajax({
                type: "POST",
                url: "/ChatBox/SendMessOff",
                data: { guid: cusGuid, name: cusName, email: cusEmail, phone: cusPhone , mess: mess },
                dataType: "json",
                success: function (response) {
                    if (response.status != "0") {
                        $('#messBoxOff').html('Đã gửi tin nhắn!');
                        $('#sttsSend').val("1");
                        if (cusGuid != items) {
                            $('#cusGuid').val(response.status);
                            setCookie('CusGuid', response.status);
                        }
                    } else {
                        $('#messBoxOff').html('Không thể gửi!');
                        $('#sttsSend').val("1");
                    }
                }
            });
        }
    }
    function SendChatCus() {
        var cusGuid = $('#cusGuid').val();
        var mess = $('#textCbInput').val().replace(/'/gi, "\"");
        var isCus = "true";
        $.ajax({
            type: "POST",
            url: "/ChatBox/SendChat",
            data: { guid: cusGuid, mess: mess, isCus: isCus, userName: '' },
            dataType: "json",
            success: function (response) {
                if (response.status != "0") {
                    $('#textCbInput').val('');
                    $('#statuscb').val("1");
                    if (cusGuid != response.status) {
                        $('#cusGuid').val(response.status);
                        setCookie('CusGuid', response.status);
                    }
                    GetChat();
                } else {
                    var msg = "<li class=\"messcus\">Không thể gửi tin nhắn, vui lòng thử lại!</li>";
                    $('#cbMessList').html($('#cbMessList').html() + msg);
                }
            }
        });
    }
    function GetChat() {
        var cusGuid = $('#cusGuid').val();
        var lassId = $('#lassId').val();
        var statuscb = $('#statuscb').val();
        var cBmin = document.getElementById("chatBox").className;
        if (statuscb == 1 && cBmin != "cBmin" && cusGuid != "") {
            $('#statuscb').val("0");
            $.ajax({
                type: "POST",
                url: "/ChatBox/GetChatContent",
                data: { guid: cusGuid, lassId: lassId },
                dataType: "json",
                success: function (response) {
                    if (response.length > 0) {
                        var str = "";
                        $.each(response, function (i, item) {
                            if (item.ChatUser != "")
                                str += "<li class=\"messadName\">" + item.ChatUser + "</li>";
                            str += "<li id=\"" + item.ChatId + "\" class=\"" + item.ChatCss + "\" title=\"" + item.ChatDate + "\">" + item.ChatMess + "</li>";
                            lassId = item.ChatId;
                        });
                        var check = $("#cbMessList").find('li#' + lassId);
                        if (check.html() == null) {
                            $('#lassId').val(lassId);
                            $('#cbMessList').html($('#cbMessList').html() + str);
                            ScrollChatBox();
                        } else if ($('#lassId').val() == 0) {
                            $('#lassId').val(lassId);
                            $('#cbMessList').html($('#cbMessList').html() + str);
                            ScrollChatBox();
                        }
                        setTimeout(function () {
                            $('#statuscb').val("1");
                            GetChat();
                        }, 500);
                    } else {
                        setTimeout(function () {
                            $('#statuscb').val("1");
                            GetChat();
                        }, 500);
                    }
                }
            });
        }
    }
</script>
