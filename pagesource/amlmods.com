(function($) {
	$(document).ready(function(){
		var show = "show",
		hidden = "hide",
		replace = "primary_nav";
		if(replace == "primary_nav"){
			$("#primary_nav").after($("#primary_responsive_js"));
		}
		if(replace == "header_menu"){
			$("#header_menu").after($("#primary_responsive_js"));
		}
		
		$("#responsiveMenuprimary_responsive").click(function(){
			if(!$("#primary_responsive_menu").data("isOpen")){
				$(this).addClass("open");
				if(show == "show"){
					$("#primary_responsive_menu").data("isOpen",1).show(200);
				}
				else if(show == "fade"){
					$("#primary_responsive_menu").data("isOpen",1).fadeIn(200);
				}
				else if(show == "slide"){
					$("#primary_responsive_menu").data("isOpen",1).slideDown(200);
				}
			}
			else{
				$(this).removeClass("open");
				if(hidden == "hide"){
					$("#primary_responsive_menu").removeData("isOpen").hide(200);
				}
				else if(hidden == "fade"){
					$("#primary_responsive_menu").removeData("isOpen").fadeOut(200);
				}
				else if(hidden == "slide"){
					$("#primary_responsive_menu").removeData("isOpen").slideUp(200);
				}
				$("#primary_responsive").find(".primary_responsive-button2").each(function() {
					if($(this).hasClass('open')){
						$(this).removeClass('open').prev().removeData("isOpen").hide();
					}
				});
			}
		});
		statMenuprimary_responsive = function(e){
			wi = $(window).width();
			replace = "primary_nav";
            if($("#"+replace).length == 0){
                replace = "super_menu";
            }
			if(!$.isNumeric()){
				if(wi <= 1000){
					$("#primary_responsive_js").show();
					$("#"+replace).hide();
					if(replace == "header_menu"){
						h = $("#header_responsive_js").outerHeight();
						$(".head_line").css("height",h);
					}
				}
				else{
					$("#primary_responsive_js").hide();
					if($("#primary_responsive_menu").data("isOpen")){
						$("#responsiveMenuprimary_responsive").removeClass("open");
						$("#primary_responsive_menu").removeData("isOpen").hide();
					}
					$("#primary_responsive").find("div#"+replace+"_chi").each(function() {
						if($(this).hasClass('open')){
							$(this).removeClass('open').prev().removeData("isOpen").hide();
						}
					});
					$("#"+replace).show();
				}
			}
			if($.isNumeric()){
				$("#primary_responsive_js").show();
				$("#primary_nav").hide();
				if(replace == "header_menu"){
					h = $("#header_responsive_js").outerHeight();
					$(".head_line").css("height",h);
				}
			}
		}
		$(window).resize(statMenuprimary_responsive);
		statMenuprimary_responsive();
