			<script>
	var lastScrollTop = 0;
	var enableScrollEvents = false;
	var screensize = '';
	var slider = null;

	$(document).ready(function() {
		$('.lifestyle-nav-inner-wrapper a.more-to-explore').click(function(event) {
			$('.lifestyle-nav-wrapper').toggleClass('closed').toggleClass('open');
			$('.lifestyle-nav-inner-wrapper').toggleClass('closed').toggleClass('open');
			ga('send', 'event', 'theloopnav', 'more-to-explore', 'more-to-explore-click');
			return false;
		});

		$('.lifestyle-nav-inner-wrapper a.minimize').click(function(event) {
			$('.lifestyle-nav-wrapper').toggleClass('closed').toggleClass('open');
			$('.lifestyle-nav-inner-wrapper').toggleClass('closed').toggleClass('open');
			ga('send', 'event', 'theloopnav', 'minimize', 'minimize-click');
			return false;
		});

		$('.lifestyle-nav-inner-wrapper a.close-button').click(function(event) {
			$('.lifestyle-nav-wrapper .protruding-box').hide();
			$('.lifestyle-nav-wrapper').height(0);
			ga('send', 'event', 'theloopnav', 'close', 'close-click');

			//set a cookie
			createCookie('closedcookie','closedcookie',1);
			return false;
		});

		$(document).on('click', ".lifestyle-nav-inner-wrapper a.disabled", function() {
    	return false;
		});

		//wait for nav to finish doing any scroll events
		setTimeout(function(){
			enableScrollEvents=true;
    }, 3000);

		//set the slider based on window size
		if($(window).width() >= 1200){
			//large breakpoint
			slider = $('.lifestyle-articles ul.slides').bxSlider({
				infiniteLoop: false,
				hideControlOnEnd: true,
	  		maxSlides: 4,
	  		slideWidth: 315,
	  		slideMargin: 15,
	  		moveSlides: 3,
	  		pager: false,
	  		touchEnabled: false,
	  		nextSelector: '#slider-next',
	  		prevSelector: '#slider-prev',
			  onSlideAfter: function(){
			  	bindDisabledClicks();
			  }
			});
			screensize='lg';
		} else {
			//medium or smaller breakpoint
			slider = $('.lifestyle-articles ul.slides').bxSlider({
				infiniteLoop: false,
				hideControlOnEnd: true,
	  		maxSlides: 4,
	  		slideWidth: 275,
	  		slideMargin: 15,
	  		moveSlides: 3,
	  		pager: false,
	  		touchEnabled: false,
	  		nextSelector: '#slider-next',
	  		prevSelector: '#slider-prev',
			  onSlideAfter: function(){
			    bindDisabledClicks();
			  }
			});
			screensize='md';
		}
	});

	$(window).scroll(function(){
		if(!readCookie('closedcookie')){
			var st = $(this).scrollTop();
			if(enableScrollEvents){
				
		   	if (st < lastScrollTop && !$('.lifestyle-nav-wrapper').hasClass('on')){
		   		//scrolling up
					$('.lifestyle-nav-wrapper').addClass('on');
					$('.lifestyle-nav-wrapper').addClass('open');
					$('.lifestyle-nav-inner-wrapper').addClass('open');
					ga('send', 'event', 'theloopnav', 'open', 'open-auto-scroll');
		   	}
			}

			lastScrollTop = st;	
		}
	});
	
	$(window).resize(function(){

		if($(window).width() >= 1200 && screensize=='md'){
			//large breakpoint
			slider.reloadSlider({
	  		infiniteLoop: false,
				hideControlOnEnd: true,
	  		maxSlides: 4,
	  		slideWidth: 315,
	  		slideMargin: 15,
	  		moveSlides: 3,
	  		pager: false,
	  		touchEnabled: false,
	  		nextSelector: '#slider-next',
	  		prevSelector: '#slider-prev',
			  onSlideAfter: function(){
			    bindDisabledClicks();
			  }
			});
			bindDisabledClicks();
			screensize='lg';
		} else if($(window).width() >= 992 && $(window).width() < 1200 && screensize=='lg') {
			//medium or smaller breakpoint
			slider.reloadSlider({
	  		infiniteLoop: false,
				hideControlOnEnd: true,
	  		maxSlides: 4,
	  		slideWidth: 275,
	  		slideMargin: 15,
	  		moveSlides: 3,
	  		pager: false,
	  		touchEnabled: false,
	  		nextSelector: '#slider-next',
	  		prevSelector: '#slider-prev',
			  onSlideAfter: function(){
			    bindDisabledClicks();
			  }
			});
			bindDisabledClicks();	
			screensize='md';
		}
	});

	function bindDisabledClicks(){
		$('.lifestyle-articles ul.slides li a').removeClass('disabled');
  	if( $('.bx-prev').hasClass('disabled')  ){
  		//next button is showing. disable the 4th li
  		$('.lifestyle-articles ul.slides li:nth-child(4) a').addClass('disabled');
  	} else {
  		//previous button is showing disable 3rd li
  		$('.lifestyle-articles ul.slides li:nth-child(3) a').addClass('disabled');
  	}

  	//show/hide gradient for next/prev
  	if( $('#slider-prev a.bx-prev').hasClass('disabled') ){
  		//previous is disabled
  		$('#slider-prev').hide();
  		$('#slider-next').show();
  	} else {
  		//next is disabled
  		$('#slider-prev').show();
  		$('#slider-next').hide();
  	}
	}

	/* cookie functions */
	function createCookie(name,value,days) {
    var expires = "";
    if (days) {
      var date = new Date();
      date.setTime(date.getTime() + (days*24*60*60*1000));
      expires = "; expires=" + date.toUTCString();
    }
    document.cookie = name + "=" + value + expires + "; path=/";
	}

	function readCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
      var c = ca[i];
      while (c.charAt(0)==' ') c = c.substring(1,c.length);
      if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
	}

	function eraseCookie(name) {
  	createCookie(name,"",-1);
	}
</script>

