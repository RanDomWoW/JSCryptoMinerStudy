<script>

var slot = '';
var slotArray = new Array();
slotCount = 0;
function dynLoadAd(slotName,adType, adPosition) {
    // creates slots for ads. Once all slots are created, fires displayAllAds
    if(slotName.length){
        googletag.cmd.push(function() {
            if(adType == 'bigBox'){
                // console.log('setting slot to bigBox');
                var slot = googletag.defineSlot('/5479/ctv.theloop/'+adzone, [[300, 1050],[300, 250],[300, 600]], slotName).addService(googletag.pubads()).addService(googletag.companionAds()).setCollapseEmptyDiv(true,true);
                // console.log('bigbox slot set');

            }            
            if(adType == 'leaderBoard') {
                // console.log('setting slot to leaderBoard');
                var slot = googletag.defineSlot('/5479/ctv.theloop/'+adzone, [728, 90], slotName).addService(googletag.pubads()).addService(googletag.companionAds()).setCollapseEmptyDiv();
                // console.log('leaderBoard slot set');
            }
            if(adType == 'homeSponsor') {
                // console.log('setting slot to leaderBoard');
                var slot = googletag.defineSlot('/5479/ctv.theloop/homesponsor', [728, 90], slotName).addService(googletag.pubads()).addService(googletag.companionAds()).setCollapseEmptyDiv(true,true);
                // console.log('homeSponsor slot set');
            }
            if(adType == 'masthead') {
                // console.log('setting slot to leaderBoard');
                var slot = googletag.defineSlot('/5479/ctv.theloop/'+adzone, [[970,250]], 'div-gpt-ad-970_250-1').addService(googletag.pubads()).setCollapseEmptyDiv(true,true);
                // console.log('masthead slot set');
            }
            if(adType == 'oop') {
                // console.log('setting slot to oop');
                var slot = googletag.defineOutOfPageSlot('/5479/ctv.theloop/'+adzone, slotName).addService(googletag.pubads()).setCollapseEmptyDiv(true,true);
                // console.log('oop slot set');
            }
            if(adType == 'slimCut') {
                var slot = googletag.defineSlot('/5479/ctv.theloop/'+adzone, [30, 1], slotName).addService(googletag.pubads()).addService(googletag.companionAds()).setCollapseEmptyDiv();
            }
            if(adPosition != ''){
                    // console.log('setting ad position to ' + adPosition);
                    slot.setTargeting("pos", [adPosition]);
                }
            
            googletag.pubads().setTargeting("ksgmnt", Krux.segments);
            googletag.pubads().setTargeting("u", Krux.user);
            // googletag.pubads().setTargeting('envr',['dev']);
            slotArray.push(slotName);
            // console.log(slotArray);
            slotCount++;
            // console.log('done creating slot');
            // console.log('checking count - ' + slotCount + ' - ' + adCount);
            if(slotCount==adCount){
                googletag.pubads().enableSingleRequest();
                // googletag.pubads().disableInitialLoad();
                googletag.enableServices();
                displayAllAds();
            } else {
                // console.log('ads not ready');
            }
        });
    }
    
         
}

var adArray = new Array();



function setAds(){
    // finds all visible ads on the page, determines type
        // console.log('calling setAds');
    $('.lazyAd').each(function(e){
        adDomId = $(this).attr('id');        
        if($('#' + adDomId).is(':visible')){
            if(adArray.indexOf(adDomId) == -1) {
                adArray.push(adDomId);
                if($(this).attr('data-pos')){
                        adPosition = $(this).attr('data-pos');
                    } else {
                        adPosition = '';
                    }
                if($('#' + adDomId).hasClass('bigBox')){
                    dynLoadAd(adDomId,'bigBox', adPosition);
                }
                if($('#' + adDomId).hasClass('aliantBigBox')){
                    dynLoadAd(adDomId,'aliantBigBox', adPosition);
                }
                if($('#' + adDomId).hasClass('leaderBoard')){
                    dynLoadAd(adDomId,'leaderBoard', adPosition);
                }
                if($('#' + adDomId).hasClass('masthead')){
                    dynLoadAd(adDomId,'masthead', adPosition);
                }

                if($('#' + adDomId).hasClass('homeSponsor')){
                    dynLoadAd(adDomId,'homeSponsor', adPosition);
                }

