<script type="text/javascript">
	function chooseStyle(param,val){
		if(param == 'yellow'){
			//$("body").css('background','#FFFF81');
			$("#headerid").css('background','rgb(71, 122, 24)');
			$("#footerid").css('background','rgb(71, 122, 24)');
			$("body").css('background-size','cover');
		}else if(param == 'pink'){
			//$("body").css('background','#FFD3DB');
			$("#headerid").css('background','rgb(164, 158, 193)');
			$("#footerid").css('background','rgb(164, 158, 193)');
			$("body").css('background-size','cover');
		}else if(param == 'blue'){
			//$("body").css('background','#B2E8FF');
			$("#headerid").css('background','rgb(66, 128, 192)');
			$("#footerid").css('background','rgb(66, 128, 192)');
			$("body").css('background-size','cover');
		}else if(param == 'default'){
			$("body").css('font','14px Helvetica, Arial, sans-serif');
		}else if(param == 'small'){
			$("body").css('font','12px Helvetica, Arial, sans-serif');
		}else if(param == 'bigger'){
			$("body").css('font','14.5px Helvetica, Arial, sans-serif');
			$(".navbar-brand").css('margin-left','-52px');
		}else{
			$("body").css('background','#FFF');
			$("#headerid").css('background','');
			$("#footerid").css('background','');
			$("body").css('background-size','cover');
		}
	}
</script>
