	$("#div4Img2").css({"top":$("#overray_middle").width()*overray_top_ratio_dv4Img_box_margin_w,"left":$("#overray_middle").width()*3+$(".div4Img").width()*2+$("#overray_middle").width()*overray_top_ratio_dv4Img_box_margin_w*3});
	
