<script>
var goto_url_r18 = "";
var data_box="";
$(window).ready(function(e) {
    $.getJSON("/data/r18.json", function(i_data){
		data_box = i_data;
		
		});
});
function adult_chk(genre_id,url,type) {
	goto_url_r18 = url;
	var flg = "";
	var ps_flg = 0;
	if(type == "ps"){
		
        for(var i in data_box){
			if(url.indexOf(data_box[i].id) != -1){
				ps_flg++;
				}		
            }
		}else{
		for(var i in data_box){
			if(genre_id.indexOf(data_box[i].id) != -1){
				ps_flg++;
				}
            }	
		}
		
		
		if(ps_flg !=0 && flg !="1"){
		$("#over_bg_r18,#over_bg_r18_box").css({"display":"block"});
		r18_size();
		$("#over_bg_r18_box").css({"opacity":"1"});
	} else {
		location.href=url;
	}
		
}
function goto_next_r18(){
	document.cookie = "r_flg=1;";
	location.href=goto_url_r18;
	}
function r18_size(){
	if($("#over_bg_r18_box").css("display") == "block"){
		$("#over_bg_r18_box").css({"left":($(window).width()-870)/2,
												"top":($(window).height()-405)/2});
	}
}
$(window).resize(function(e) {
    r18_size();
});


