function overray_size(){
	
	if($("#overray_bg").css("display") != "none"){
	var w_width = $(window).width();
	var w_height = $(window).height();
	
	if(w_width >=1060 && w_height >= 690){
		
	$("#overray_top").css({
							"top":(w_height - 690)/2+$(window).scrollTop(),
							"left":(w_width - 1060)/2+$(window).scrollLeft(),
							"width":"1060px",
							"height":"690px"
	});	
		
		
	}else if((w_width>w_height) && w_height>520){
		var left = (w_width -w_height*overray_top_ratio_h)/2+$(window).scrollLeft();
		
		if(w_width -(w_height*overray_top_ratio_h)<0){
					if($(window).scrollLeft()>(w_height*overray_top_ratio_h)-w_width){left = $(window).scrollLeft()-(w_height*overray_top_ratio_h-w_width);}else{
					left = 0;
		}
		}
	$("#overray_top").css({
							"top":$(window).scrollTop(),
							"left":left,
							"width":w_height*overray_top_ratio_h,
							"height":w_height
	});	
	}else if((w_width<w_height) && w_width>800){
	var top = (w_height -w_width*overray_top_ratio_w)/2+$(window).scrollTop();
	if(w_height -w_width*overray_top_ratio_w<0){
					if($(window).scrollTop()>(w_width*overray_top_ratio_w)-w_height){top = $(window).scrollTop()-(w_width*overray_top_ratio_w-w_height);}else{
					top = 0;
		}
		}
	
	$("#overray_top").css({
							"top":top,
							"left":$(window).scrollLeft(),
							"width":w_width,
							"height":w_width*overray_top_ratio_w
	});	
	}else{
		
		var top = (w_height -520)/2+$(window).scrollTop();
	if(w_height -w_width*overray_top_ratio_w<0){
					if($(window).scrollTop()>520-w_height){top = $(window).scrollTop()-(520-w_height);}else{
					top = 0;
		}
		}
		var left = (w_width -800)/2+$(window).scrollLeft();
		
		if(w_width -800<0){
					if($(window).scrollLeft()>800-w_width){left = $(window).scrollLeft()-(800-w_width);}else{
					left = 0;
		}
		}
	$("#overray_top").css({
							"top":top,
							"left":left,
							"width":"800px",
							"height":"520px"
	});	
	}
	
