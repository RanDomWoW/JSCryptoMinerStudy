    methods = {
        init : function( options ) {
            if( this.length ) {
                var settings = {
                    autoStart: true,
                    sliderSpeed: 1100, // speed for the sliding animation
                    sliderEasing: 'easeOutExpo',// easing for the sliding animation
                    itemSpeed: 1100, // speed for the item animation (open / close)
                    itemEasing: 'easeOutExpo',// easing for the item animation (open / close)
                    scroll: 1, // number of items to scroll at a time,
                    dir:-1
                };
                return this.each(function() {
                    // if options exist, lets merge them with our default settings
                    if ( options ) {
                        $.extend( settings, options );
                    }
                    // control the scroll value
                    settings.scroll = Math.max(Math.min(settings.scroll, 3), 1);
                    var $el = $(this),
                        $wrapper = $el.find('div.ca-wrapper74212'),
                        $items = $wrapper.children('div.ca-item74212'),
                        cache = {
                            autoscroll: null,
                            itemW: $items.width(),// save the width of one item
                            totalItems: $items.length// save the number of total items
                        };
