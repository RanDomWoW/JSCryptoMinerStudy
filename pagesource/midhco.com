		$('div').css('font-size', originalFontSize);
		$('li').css('font-size', originalFontSizeLi);
	});
// Increase Font Size
	$('.increaseFont').click(function(){
		var currentFontSize = $('div').css('font-size');
		var currentFontSizeLi = $('li').css('font-size');
		var currentFontSizeNum = parseFloat(currentFontSize, 10);
		var currentFontSizeNumLi = parseFloat(currentFontSizeLi, 10);
		var newFontSize = currentFontSizeNum*1.2;
		var newFontSizeLi = currentFontSizeNumLi*1.2;
		if(newFontSize < 14) {
			$('div').css('font-size', newFontSize);
			$('li').css('font-size', newFontSizeLi);
		}
		return false;
	});
// Decrease Font Size
	$('.decreaseFont').click(function(){
		var currentFontSize = $('div').css('font-size');
		var currentFontSizeLi = $('li').css('font-size');
		var currentFontSizeNum = parseFloat(currentFontSize, 10);
		var currentFontSizeNumLi = parseFloat(currentFontSizeLi, 10);
