                var $item = $(this),
                    idx = $item.index();
                $item.find('div.ca-item-main74212').show();
                if( idx !== openedIdx ) {
                    $item.find('div.ca-content-wrapper74212').css({
                        width: '0px'
                    }).end().css( 'left', ( ( cache.winpos - 1 ) - ( openedIdx - idx ) ) * cache.itemW + 'px' ).show();
                }
            });
        },
