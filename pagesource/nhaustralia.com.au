<script type="text/javascript">

    //This function will be used to toggle the tpnoverlay element, this element is used to grey out the screen and make it inactive while
    // the user needs to choose a location.
    function overlay() {
        //We get the element we want to toggle by it's ID value
        el = document.getElementById("tpnavoverlay");
        //Then just toggle it's visibility value
        el.style.visibility = (el.style.visibility == "visible") ? "hidden" : "visible";
    }
    //This method will fire off when the user visits each of the pages, when the cookie is not yet set we will show the overlay to make the page inactive
    //and then show the locationDiv element on top of the overlay (this is the menu with locations).
    //This relies on the the z-index that was set in the style sheets, the menu has a higher z-index than the overlay, making it active while the
    //rest are inactive.. Please make sure that all elements that you create is lower than the overlays z-index of (999);
    //We also check to see if the cookie has been set already, if it has an value we will use that value instead of showing the overlay.
    function checkcookie() {
        if (typeof $.cookie('locationCookie') === 'undefined') {
           /* overlay();
            el = document.getElementById("locationDiv");
            el.style.visibility = "visible";
            */
            $.cookie("locationCookie", 'NSW', { expires: 30 });
            $("#lblcurrentlocation").text($.cookie("locationCookie"));
            $("#lblPhoneNumber").text("1300 794 006");
            $("#lblPhoneNumber").attr("href", "tel:1300794006");
            $("#sub-header-home").addClass("sub-header-home-nsw");
        } else {
            $("#lblcurrentlocation").text($.cookie("locationCookie"));


            switch ($.cookie("locationCookie")) {
                case 'NSW':
                   $("#lblPhoneNumber").text("1300 794 006");
            $("#lblPhoneNumber").attr("href", "tel:1300794006");
                    $("#sub-header-home").addClass("sub-header-home-nsw");
                    break;
                case 'VIC':
                   $("#lblPhoneNumber").text("1300 794 006");
            $("#lblPhoneNumber").attr("href", "tel:1300794006");
                    $("#sub-header-home").addClass("sub-header-home-vic");
                    console.log('changing background');
                    break;
                case 'QLD':
                   $("#lblPhoneNumber").text("1300 794 006");
            $("#lblPhoneNumber").attr("href", "tel:1300794006");
                    $("#sub-header-home").addClass("sub-header-home-qld");
                    break;
                case 'WA':
                    $("#lblPhoneNumber").text("1300 794 006");
            $("#lblPhoneNumber").attr("href", "tel:1300794006");
                    $("#sub-header-home").addClass("sub-header-home-wa");
                    break;
                case 'ACT':
                    $("#sub-header-home").addClass("sub-header-home-act");
                    break;
                case 'TAS':
                    $("#sub-header-home").addClass("sub-header-home-tas");
                    break;
                case 'SA':
                    $("#sub-header-home").addClass("sub-header-home-sa");
                    break;
                case 'NT':

                    $("#sub-header-home").addClass("sub-header-home-nt");
                    break;
                default:
                    $("#lblPhoneNumber").text("1300 794 006");
                    $("#lblPhoneNumber").attr("href", "tel:1300794006");
                    break;

            }

            // check if user is already logged in

            if (typeof $.cookie('NHUserInfo') != 'undefined') {
                console.log('userExists');


                var arr = $.cookie('NHUserInfo').split('&');
                var username = arr[1].split('=');
                console.log(arr[1]);
                $("#login").hide();
                $("#logout").show();
                $("#loginstatus").html(username[1]);

            }

        }
    }
    //This function set the click event on the list items in the locationdiv, we will call this logic to set the value of the cookie
    //First we get the text of the clicked anchor element
    //The IF statement is just to make sure that we hide the overlay if it is the first time we set the cookie's value
    //Next we will set the cookie's value equal to the text of the clicked anchor element, but also show the selected location on the menu
    function setevents() {

        $(document).on('click', '.popup-modal-dismiss', function (e) {
            var chosenlocation = $(this).text();
            if (typeof $.cookie('locationCookie') === 'undefined') {
                overlay();
            }
            $.cookie("locationCookie", chosenlocation, { expires: 30 });
            $("#lblcurrentlocation").text(chosenlocation);

            //state cookie

            switch (chosenlocation) {
                case 'Sydney':
                    $.cookie("locationState", "NSW", { expires: 30 });
                    $("#sub-header-home").addClass("sub-header-home-nsw");
                    break;
                case 'Brisbane':
                    $.cookie("locationState", "QLD", { expires: 30 });
                    break;
                case 'Melbourne':
                    $.cookie("locationState", "VIC", { expires: 30 });
                    $("#sub-header-home").addClass("sub-header-home-vic");
                    break;
            }
            //e.preventDefault();


            //This part is just a temporary part, this is used to refresh the current page..
            //If we are going to implement calls to a controller's actions we can remove this.
            location.reload();
        });
    }
</script>

