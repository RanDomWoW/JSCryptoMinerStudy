    <script>
        function Search() {
            if ($('.search_input').val().length > 2)
                window.location = 'arabic/searchAll.aspx?MID=39&k=' + $('.search_input').val().replace("+", "plussign");
            else {
                alert("Please enter more than 2 characters for search.");
                $('.search_input').focus();
            }
            return false;

        }

        $("#txtSearch").keyup(function (event) {
            if (event.keyCode == 13) {
                Search();
            }
        });

    </script>
