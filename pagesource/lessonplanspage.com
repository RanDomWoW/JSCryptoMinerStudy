			$(form).find('.step'+closedPanel+' .field-box').removeClass('hc-opened');
			$(form).find('.form-step.step'+closedPanel).slideUp(200);
			
			if (closedPanel == (topPanel+1)) {
			
				sendZipCategories($(form).find('input[name="zipcode"]').get(0));
				disableInput = false;
				return;
			}
			
			var nextPanel = closedPanel - 1;
			
			$(form).find('.step'+nextPanel+' .field-box').slideUp(300, 
					function() { closeNextPanel(form, nextPanel, topPanel) } );
		
		}
		
		function closeEarlierPanels(form, topPanel, lastPanel) {
		
			$(form).find('.step'+lastPanel+' .field-box').slideUp(300, function() { closeNextPanel(form, parseInt(lastPanel), parseInt(topPanel) ) } );
			
		}
		
		
		function setHasWalked(form,panel) {
		
			if ( $(form).find('.step'+panel+' .step-headline').hasClass('hc-walked') == false ) {
				  $(form).find('.step'+panel+' .step-headline').addClass('hc-walked');
			}
		
		}
		
		function setPanelFocus(form,step) {
			
			 $(form).find('.step'+step).focus();
		}
		
		function scrollToStep(form,step) {
			
		   var fromTop = $(form).find('div.form-step.step'+step).offset().top;
		   $('body').animate({ scrollTop: fromTop });
		}
		
		function setFirstInputFocus(form,step) {
		
		   if (!singleStep()) {
			
				 $(form).find('.step'+step+' :input[type="text"]:enabled:visible:first').focus();
				 
			} else {
			
				// jump to inline anchor unless this step contains firstname input
				if (  $(form).find('div.form-step.step'+step+' div.firstname').length ) {
					// learn more button of program copy does this
					
				} else {
				
				  scrollToStep(form, step);
				
				}
			
			}
			
		}
		
		function bindCategoryInput(form) {
		
			 $(form).find('input[name="categoryradio"]').change(function() {
			 
			  if (disableInput) return false;
			 
			  var thisStep = getStepNum(this);
			  
			  
			  window['validateAccordionStep'+getFormNum(form)](form,thisStep);
			  currPanel = thisStep;
			  $(this.form).find('.processuilite').css('display','block');
			  sendCategoryChange(this);
			  return false;
			});
		}
		
		// called when program panel finishes opening
		function bindDegreeInput(obj,form) {
			if (disableInput) return false;
			// here, obj is the accordion div.programradio - get input with object
			  var prObj = $(obj).find('input[name="programradio"]').get(0);
			  currPanel = getStepNum(prObj);
			  validatePanelStep(prObj);
			  return false;
		}
		
		var disableInput = false;
		var lastZipValue;
		
		function getFormNum(form) {
			var p = $(form).parents('[id^="form-container-"]').attr('id');
			var formNum = p.substring(p.lastIndexOf('-')+1);
			return formNum;
		}
		
		function bindZipInput(form) {
		
			$(form).find('input[name="continue"]').click(function() {
			
				if ($(form).find('input[name="zipcode"]').val() == ''  ) {
				
					$(form).find('input[name="zipcode"]').trigger('change');
				
				}
			
			});
		
		
			$(form).find('input[name="zipcode"]').keyup(function (e) {
			
				var zValue = $(this).val();
			
				if (zValue != lastZipValue) {
					disableInput = true;
				} else {
					disableInput = false;
				}
			
			
			});
		
			$(form).find('input[name="zipcode"]').change(function() {
			
			  lastZipValue = $(this).val();
				
			  currPanel = getStepNum(this);
			  
			  val = window['validateAccordionStep'+getFormNum(form)](form,currPanel);
			  
			  if (val) {
			  
				  var lastPanel = getStepNum($(form).find('.field-box.last'));
				  
				  closeEarlierPanels(form, parseInt(currPanel), parseInt(lastPanel));
			  }
		  
			  
			  return false;
				
			});
		}
		
		</script>
		
