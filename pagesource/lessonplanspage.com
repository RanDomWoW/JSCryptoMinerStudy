	function setCategories(form,html) {
	  var obj = jQuery(form).find('select[name="category"]');
	  if (html.indexOf('error') != -1) {
		var args = {};
		var department = jQuery(form).find('select.department').val(); 
		var school = 'cu-portland';
		args['department'] = department;
		args['school'] = school;
		doAjaxRequest(obj.form,'getAllCategories',args,setAllCategories);
