	function isUSorCanada(form) {
		
		var foreign = jQuery(form).find('input[name="hc_foreign_parm"]').val();
		
		if (foreign) {
			
			return false;
		}
	
		var country = geoip_country_code();
		
		if (country != '' && country != 'US' && country != 'CA') {
		
			return false;
		}
		
		return true;
	}
	
