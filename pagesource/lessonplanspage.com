	function setRadioCategories(form,html) {
		
	  var jsonData = html.substr(0,html.indexOf('|||'));
	  var data = JSON.parse(jsonData);
	  partnerObj = data;
	  
	  var state = data.rif_state;
	  var city = data.rif_city;
	  
	  var cHtml = html.substr(html.indexOf('|||')+3); // the radio buttons
	  
	  setStateValue(form,state);
	  setCityValue(form,city);
	  
	  var obj = jQuery(form).find('div.categoryradio');
	  jQuery(obj).html(cHtml);
	  bindCategoryInput(form);
	  openNextSliderPanel(form);
	  jQuery(form).find('.processuilite').css('display','none');
	  rebindGAtracking();
	}
	
	function setSelectCategories(form,html) {
		
	  var city = html.substr(html.indexOf('<city>')+6,html.indexOf('</city>')-html.indexOf('<city>')-6); // the city looked up from zip code
		
	  var state = html.substr(html.indexOf('<state>')+7,2); // the state looked up from zip code
	  var count = parseInt(html.substr(html.indexOf('<count>')+7,3)); // the category count
	  var cHtml = html.substr(html.indexOf('</count>')+8); // the select options
	  
	  setStateValue(form,state);
	  setCityValue(form,city);
	  
	  // see if category already selected and valid
	  var cat = jQuery(form).find('select[name="category"]').val();
	  jQuery(form).find('select[name="category"]').html(cHtml);
	  
	  if (cat != '_none_') {
		  jQuery(form).find('select[name="category"] option[value="'+cat+'"]').attr('selected', 'selected');
		  var newCat = jQuery(form).find('select[name="category"]').val();
		  if (newCat != '_none_') {
			jQuery(form).find('select[name="category"]').trigger("change");
			return;  
		  }
	  }
	  //jQuery(form).find('select[name="program"]').prop('disabled',true);
	  
	  if (count == 1) {
		  jQuery(form).find('select[name="category"] option:eq(1)').attr('selected', 'selected');
		  jQuery(form).find('select[name="category"]').css('display','none');
		  jQuery(form).find('select[name="category"]').trigger("change");
	  }

	}
	
	function isCategoryDropdown(form) {
		if (jQuery(form).find('select[name="category"]').length > 0) return true;
		return false;
	}
	
	function isCategoryRadio(form) {
		if (jQuery(form).find('input:radio[name="categoryradio"]').length > 0) return true;
		return false;
	}
	
	function doAjaxZipCategories(form) {
		
		var args = {};
		
		var state = getStateValue(form);
		var zip = jQuery(form).find('input[name="zipcode"]').val();
		var partner = jQuery(form).find('input[name="hc_partner"]').val();
		var select_label = jQuery(form).find('input[name="hc_category_select_label"]').val();
		
		args['state'] = state;
		args['zip'] = zip;
		args['partner'] = partner;
		args['select_label'] = select_label;
		args['zip_filter'] = zipFilter;
		
		var obj = jQuery(form).find('select[name="category"]');
		
		if (obj.length) {
			
			doAjaxRequest(form,'getSelectCategories',args,setSelectCategories);
			
		} else {
			
			jQuery(form).find('.processuilite').css('display','block');
			
			doAjaxRequest(form,'getRadioCategories',args,setRadioCategories);
		}
	}
	
	function getZipCategories(obj) { // obsolete?
		
		if (!isCityOrState()) return;
		if (!inInputPrepare) { // skip these triggers
			if (jQuery(obj).val().toLowerCase().indexOf('zip') > -1) {			
				return;
			} else if (obj.value == '') {
				return;
			}
			
			doAjaxZipCategories(obj.form);
		}
	}
	
	function setCategoryOptions(form, state, data) {
		
		var cats = data['categories'];
		
		if (state == '') state = 'XX';
		
		var catLabel = data['cats_with_labels'];
		
		for (var states in cats) {
			
			if (states.indexOf(state) != -1) {
				
				var key = states;
				
				break;
			}
		}
		
		// key contains state key
		
		var categories = cats[key];
		
		if (categories == null) {
			// if no categories for this location, then show all categories
			categories = data['categoriesSortFlip'];
		}
		
		var categorySelect = jQuery(form).find('select[name="category"]');
		
		// grab any existing selection
		var selected = jQuery(categorySelect).val();
		
		jQuery(categorySelect).find('option:gt(0)').remove();
		
		var catSort = data['categoriesSort']; // display categories in this order
		
		for (var catSortKey in catSort) {
			
			if (categories[catSort[catSortKey]] !== undefined) {
				
			  jQuery(categorySelect).append(jQuery('<option>', { value : catLabel[catSort[catSortKey]] }).text(catSort[catSortKey]));
			}
			
		}
		
		// set selection
		jQuery(categorySelect).val(selected);
		
	}
	
		
	function setCategoriesRif6(form, html) {
		
	  var cityObj = jQuery(form).find('input[name="city"]');
	  var zipcode = jQuery(form).find('input[name="zipcode"]');
	  
	  var res = html.trim().split(',');
	  var city = res[0];
	  var state = res[1];
	  
	  if (city == 'error') {
		  setStateValue(form,'');
		  setCityValue(form,'');
		  setGeoValues();
	  } else {
		  clearGeoValues();
		  setStateValue(form,state);
		  setCityValue(form,city);
	  }
	  
	  // if no zipFilter, were done
	  if (!zipFilter) return;
		
	  // if show all categories, then were done
	  var data = getPartnerData(form);
	  
	  if (data['show_all_categories'] == 'Y') return;
	  
	  setCategoryOptions(form, state, data);
		
	}

	
	function getCategories(obj) {
		
		if (!isCityOrState()) return;
		if (!inInputPrepare) { // skip these triggers
			if (jQuery(obj).val().toLowerCase().indexOf('zip') > -1) {			
				return;
			} else if (obj.value == '') {
				return;
			}
			
			var valZip = '';
			jQuery(obj).val(jQuery.trim(jQuery(obj).val()));
			// validate zip first before looking up
			valZip = validate_zipcode(obj.form);
			if (valZip == '') {
				var zip = new Array(obj.value);
				doAjaxRequest(obj.form,'lookupZip',zip,setCategoriesRif6);
			} else {
				removeInvalid(obj.form,'zipcode');
			}
			
		}
	}
	
	function departmentChange(obj) {
		// assemble args
		var args = {};
		
		if (inInputPrepare) return; // ignore events triggered from inputPrepare() placeholders
		
		if (isLocalized()) {
				if (!isCategoryDropdown(obj.form)) {
				var valZip = validate_zipcode(obj.form);
				if (valZip != '') {
					disableProgram(obj.form);
					return;
				}
			}
		}
		
		var department = jQuery(obj).val();
		var state = getStateValue(obj.form);
		var zip = jQuery(obj.form).find('input[name="zipcode"]').val();
		var school = 'cu-portland';
		
		args['department'] = department;
		args['state'] = state;
		args['zip'] = zip;
		args['school'] = school;

		if (!isCategoryDropdown(obj.form)) {
			doAjaxRequest(obj.form,'getDepartmentDegrees',args,setDepartmentDegrees);
		} else {
			doAjaxRequest(obj.form,'getCategories',args,setCategories);
		}

	}
	
	function categoryChange(obj) {
		// assemble args
		var args = {};
		
		if (inInputPrepare) return; // ignore events triggered from inputPrepare() placeholders
		
		var category = jQuery(obj).val();
		var department = jQuery(obj.form).find('select.department').val();
		var state = getStateValue(obj.form);
		var school = jQuery(obj.form).find('input[name="hc_client"]').val();
		
		args['department'] = department;
		args['state'] = state;
		args['school'] = school;
		args['category'] = category;
		
		doAjaxRequest(obj.form,'getCategoryDegrees',args,setCategoryDegrees);

	}
	
