                 function toggleArrows(){ 
                    if($a(elm).find(".owl-item").last().hasClass('active') && 
                       $a(elm).find(".owl-item.active").index() == $a(elm).find(".owl-item").first().index()){                       
                        $a(elm).find('.owl-nav .owl-next').addClass("off");
