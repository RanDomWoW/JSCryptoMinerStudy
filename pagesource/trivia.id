<script>
var isPaused = false;

function change_video(id){
  isPaused = false;
  $('.player-video').empty().css('display','none');
  $('.link-video').removeClass('active');
  $('.link-'+id).addClass('active');
  $('.display-video').removeClass('active');
  $('.video-'+id).addClass('active');
}

function play_video(video){
  isPaused = true;
  $('<iframe id="video-youtube" width="100%" class="embed-responsive-item" frameborder="0" allowfullscreen></iframe>').attr('src','https://www.youtube.com/embed/'+video+'?autoplay=1').appendTo('.player-video');
  $('.display-video').removeClass('active');
  $('.player-video').css('display','block');
}

function slideshow() {
  if(!isPaused) {
    var max = $('#list-video > div').length;
    var flag = 0;
    $($('#list-video > div')).each(function (index) {
      if ($(this).hasClass('active') == true && flag==0) {
        var post = index+1;
        if(index==max-1){
          post = 0; 
        }
        $('.link-video').removeClass('active');
        $('.link-'+post).addClass('active');
        $('.display-video').removeClass('active');
        $('.video-'+post).addClass('active');
        flag = 1;
      }
    });
  }
}

var timer = window.setInterval(slideshow, 3000);
</script>
