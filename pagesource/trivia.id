                }
            }
        });
        return false;
    });
    function loadcomment(post, limit, position, placeresult, url){
        var link = url+'/'+post+'/'+limit+'/'+position;
        $.ajax({
            type: "GET",
            url: link,
            data: "",
            success: function(response){ 
                if (response!="") {
                    $("#"+placeresult).append(response);
                    position++;
                    $("#loadmore-button").html('<a href="javascript:;" onClick="loadcomment(\''+post+'\',\''+limit+'\',\''+position+'\',\''+placeresult+'\',\''+url+'\');">Loadmore Comment</a>');
                }
                else {
                    $("#loadmore-button").html('');
                }
            }
        });
        return false;
    }
