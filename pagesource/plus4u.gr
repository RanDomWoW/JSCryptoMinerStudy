	<script>
		var canvas, stage, exportRoot;
		
		function init() {
			canvas = document.getElementById("canvas");
			images = images||{};
		
			var loader = new createjs.LoadQueue(false);
			loader.addEventListener("fileload", handleFileLoad);
			loader.addEventListener("complete", handleComplete);
			
			
			$.each(lib.properties.manifest, function(key, value) {
				value.src = 'https://www.plus4u.gr/images/banners/bn-106/'+value.src;
			});
			
			loader.loadManifest(lib.properties.manifest);
		}
		
		function handleFileLoad(evt) {
			if (evt.item.type == "image") { images[evt.item.id] = evt.result; }
		}
		
		function handleComplete(evt) {
			exportRoot = new lib.Serton_kentriko();
			
			console.log(exportRoot);
			
			stage = new createjs.Stage(canvas);
			stage.addChild(exportRoot);
			stage.update();
			stage.enableMouseOver();
		
			createjs.Ticker.setFPS(lib.properties.fps);
			createjs.Ticker.addEventListener("tick", stage);
		}
		
		function banner_open() {
			var state = document.getElementById("banner-thumbs-content").getAttribute("data-state");
			if(state == "opened" || document.getElementById("banner-thumbs-content").classList.contains("slideDown") || document.getElementById("banner-thumbs-content").classList.contains("slideUp")) { return; }

			slideUp(document.getElementById("banner-thumbs-content"), 120, function() {
				document.getElementById("banner-thumbs-content").setAttribute("data-state", "opened");
			});
		}
		
		function banner_close() {
			var state = document.getElementById("banner-thumbs-content").getAttribute("data-state");
			if(state == "closed") { return; }
			
			slideDown(document.getElementById("banner-thumbs-content"), 120, function() {
				document.getElementById("banner-thumbs-content").setAttribute("data-state", "closed");
			});
		}
		
		function slideUp(element, maxHeight, callback) {
		 	if(element.classList.contains("slideDown")) { 
				//setTimeout(function() { slideUp(element, maxHeight); }, 40);
				return;
			}
			if(element.classList.contains("slideUp")) { 
				return;
			}
		 	
		 	element.classList.add("slideUp");
		    
		    jQuery("#banner-thumbs-content").show().animate({height :maxHeight+'px' }, 1000, 0, function() {
			     element.classList.remove("slideUp");
		            if(callback) { callback(); }
		    });
 
		}
		
		function slideDown(element, maxHeight, callback) {
			if(element.classList.contains("slideUp")) { 
				//console.log("timeoutfadeout");
				//setTimeout(function() { slideDown(element, maxHeight); }, 40);
				return;
			}
			
			if(element.classList.add("slideDown")) {
				return ;
			}
			
			element.classList.add("slideDown");
			
		    jQuery("#banner-thumbs-content").show().animate({height :0+'px' }, 1000, 0, function() {
			    
			    jQuery(this).hide();
			    
			     element.classList.remove("slideDown");
		            if(callback) { callback(); }
		    });
		}
		
		function canvas_fix_size() {
			var width = 1920; //jQuery("#canvas").attr("width");
			var height = 650; //jQuery("#canvas").attr("height");
			var ratio = width/height;
			
			var windowWidth = jQuery(window).width();
			var calHeight = windowWidth/ratio;
			
			jQuery("#canvas").attr('width', windowWidth);
			jQuery("#canvas").attr('height', calHeight);
		}

		
	</script>

	
