<script>
function del(sh_id) { 
	// Создание нового объекта XMLHttpRequest для общения с Web-сервером 
	if(window.XMLHttpRequest) { 
		req = new XMLHttpRequest();  
	} else if (window.ActiveXObject) { 
	req = new ActiveXObject("Microsoft.XMLHTTP"); }
	//objavim funkciu obrabotki otveta
	function processReqChange() 
	{ 
	   if(req.readyState == 4) { 
		if(req.status == 200) 
		{ 
			var response = req.responseText;
			if (response == 'deleted') {
				document.getElementById(sh_id).className='btn btn-success';
				document.getElementById(sh_id).innerHTML='удалено';
			}
			if (response == 'undeleted') {
				document.getElementById(sh_id).className='btn';
				document.getElementById(sh_id).innerHTML='удалить';
			}			
		} else { alert("There was a problem retrieving the XML data:\n" + req.statusText); 	}
	  }
	}
	// otpravka
	req.onreadystatechange = processReqChange; 
	req.open('GET', './del_shutka_ajax.php?sh_id=' + sh_id , true);  
	req.send(null);	
}	
</script>

