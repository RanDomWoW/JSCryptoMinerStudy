                if( typeof(popupZone.rollingTimer) != "undefined"){
                    popupZone.rollingTimer = clearInterval(popupZone.rollingTimer);
                }
                popupZone.rollingTimer = setInterval(function(){
                    $btn_next.trigger("click");
                },3000);
            },
            event : function() {
                $("div.eventbanner a.tab, div.eventbanner a.img_tab").on('click', popupZone.rolling);
                $("a.img_tab").on("click", function() {
                    var $this = $(this);
