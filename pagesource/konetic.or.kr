                        $(".eventbanner li:eq(3) a.tab").css("right", "23px");
                        $(".eventbanner li:eq(4) a.tab").css("right", "5px");
                    } else if ($(".eventbanner li:eq(4)").hasClass("on")) {
                        $(".eventbanner li:eq(0) a.tab").css("right", "102px");
                        $(".eventbanner li:eq(1) a.tab").css("right", "83px");
