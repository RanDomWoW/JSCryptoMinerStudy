//이미지 관련함수
function setImage(imageId){
	//Jquery Selector Setting (init)
	var jQImageId = "#"+imageId;
	var imageInfo_width		= $(jQImageId).width();
	var imageInfo_height	= $(jQImageId).height();
	var imageInfo_maxWidth	= 735;
	var imageInfo_rltHeight = 0;

	//Origin Info Set hidden form
	$("#org_imgInfo").val(imageInfo_width+"x"+imageInfo_height);

	//Max ImageSize Setting
	imageInfo_rltHeight = imageInfo_maxWidth * imageInfo_height / imageInfo_width;

	//toFiexed(0): 123.11 -> 123, 123.50->124
	imageInfo_rltHeight = imageInfo_rltHeight.toFixed(0);

	//Set Image Width,Height
	$(jQImageId).css({
		width : imageInfo_maxWidth+"px",
		height: imageInfo_rltHeight+"px"
	});
}
function setWidthImage(imageId,maxWidth){
	//Jquery Selector Setting (init)
	var jQImageId = "#"+imageId;
	var imageInfo_width		= $(jQImageId).width();
	var imageInfo_height	= $(jQImageId).height();
	var imageInfo_maxWidth	= maxWidth;
	var imageInfo_rltHeight = 0;

	//Origin Info Set hidden form
	$("#org_imgInfo").val(imageInfo_width+"x"+imageInfo_height);

	//Max ImageSize Setting
	imageInfo_rltHeight = imageInfo_maxWidth * imageInfo_height / imageInfo_width;

	//toFiexed(0): 123.11 -> 123, 123.50->124
	imageInfo_rltHeight = imageInfo_rltHeight.toFixed(0);

	//Set Image Width,Height
	if (imageInfo_width < imageInfo_maxWidth){
		$(jQImageId).css({
			width : imageInfo_maxWidth+"px",
			height: imageInfo_rltHeight+"px"
		});
	}
}

function popimage(imageId){	
	//Jquery Selector Setting (init)
	var jQImageId = "#"+imageId;
	var OriginImageInfo = $("#org_imgInfo").val().split("x");
	var imageInfo_width		= OriginImageInfo[0];
	var imageInfo_height	= OriginImageInfo[1];
	var imagesrc = $(jQImageId).attr("src");
	var look='width='+imageInfo_width+',height='+imageInfo_height+',';
	var popupTitle = $(jQImageId).attr("alt");
	
	popwin = window.open("","",look);
	popwin.document.open();
	popwin.document.write('<title>인포그래픽스 - '+popupTitle+'</title><body topmargin=0 leftmargin=0><img style=cursor:hand; onclick="self.close()" src="'+imagesrc+'"></body>');
	popwin.document.close();
}	  


function setCookie(name, value, expiredays) {
	var today = new Date();
	today.setDate(today.getDate() + expiredays);
	document.cookie = name + '=' + escape(value) + '; path=/; expires=' + today.toGMTString() + ';'
}
