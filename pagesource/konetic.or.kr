                        //가운데 play, stop 버튼 이미지 및 이벤트 토글
                        if ($this.hasClass("btn_ctrl_stop")) {
                            $this.empty();
                            $this.removeClass("btn_ctrl_stop").addClass("btn_ctrl_play");
                            $this.append($("<img>").attr("src", "/renewal/images/main/btn_control_play.gif").attr("alt", "팝업존 시작").attr("title", "팝업존 시작"));
                            
                            //롤링 stop
                            if( typeof(popupZone.rollingTimer) != "undefined"){
                                popupZone.rollingTimer = clearInterval(popupZone.rollingTimer);
                            }
                        } else {
                            $this.empty();
                            $this.removeClass("btn_ctrl_play").addClass("btn_ctrl_stop");
                            $this.append($("<img>").attr("src", "/renewal/images/main/btn_control_stop.gif").attr("alt", "팝업존 정지").attr("title", "팝업존 정지"));
                            
                            //롤링 start
                            popupZone.rolling();
                        }
                    } else if ($this.hasClass("tab01")) {
                        //prev
                        var $prevon = $("div.eventbanner ul li.on").prev();
                        if($prevon.length == 0){
                            $("div.eventbanner ul li").eq(2).find("a").trigger("click");
                        } else {
                            $prevon.find("a").trigger("click");
                        }
                    } else {
                        //next
                        var $nexton = $("div.eventbanner ul li.on").next();
                        if($nexton.hasClass("evt_ctrl") || $nexton.length == 0){
                            $("div.eventbanner ul li").eq(0).find("a").trigger("click");
                        } else {
                            $nexton.find("a").trigger("click");
                        }
                    }
                });
                
