                        $(".eventbanner li:eq(0) a.tab").css("right", "132px");
                        $(".eventbanner li:eq(1) a.tab").css("right", "113px");
                        $(".eventbanner li:eq(2) a.tab").css("right", "71px");

                        $(".eventbanner li:eq(3) a.tab").css("right", "23px");
                        $(".eventbanner li:eq(4) a.tab").css("right", "5px");
                    } else if ($(".eventbanner li:eq(3)").hasClass("on")) {
