<script type="text/javascript">
$(function(){
	//cokkieのaccessの値で判定
	if($.cookie("access")){//accessの値がある場合
		$('#layer_board_area').jqm();
		$('#layer_board_area').jqmHide();//modalウインドウを隠す
	}else{//accessの値が無い場合
		$('#layer_board_area').jqm();
		$('#layer_board_area').jqmShow();//modalウインドウを表示する
	}
	$(window).load(function(){
		//accessというキーで適当な値をcookieに保存。7日間有効
		$.cookie("access","foo", { expires: 7 });
	})
});
</script>
