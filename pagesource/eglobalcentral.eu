ul.ban_images_wp li { position:absolute; top:0px; left:0px; list-style-type: none !important;}

ul.triggers_wp {display: none; list-style-type: none;}

.control_wp {position:relative; color:#fff; cursor:pointer;}

.prev_wp, .next_wp{
	position: relative;
	top: 100px;
	font-size: 30px;
	text-decoration: none;
	color:#fff;
	background: rgba(0,0,0,0.5);
	padding: 5px;
	z-index:2;
}

.prev_wp{
	left:-128px;
	border-left: 3px solid #fff;
}

.next_wp{
	left:38px;
	border-right: 3px solid #fff;
}</style>
<script>

$( "ul.ban_images_wp li img" ).each(function(index) {
  $( "ul.triggers_wp" ).append( "<li></li>" );
});

var triggers_wp = $('ul.triggers_wp li');
var images_wp = $('ul.ban_images_wp li');
var lastElem_wp = triggers_wp.length-1;
var target_wp;

$('ul.triggers_wp li').first().addClass('active');
$('ul.ban_images_wp li').hide().first().show();

function sliderResponse_wp(target_wp) {
    $('ul.ban_images_wp li').fadeOut(300).eq(target_wp).fadeIn(300);
    $('ul.triggers_wp li').removeClass('active').eq(target_wp).addClass('active');
}

$('ul.triggers_wp li').click(function() {
	
    if ( !$(this).hasClass('active') ) {
        target_wp = $(this).index();
        sliderResponse_wp(target_wp);
        resetTiming_wp();
    }
});

$('.next_wp').click(function() { 
    target_wp = $('ul.triggers_wp li.active').index();
    target_wp === lastElem_wp ? target_wp = 0 : target_wp = target_wp+1;
    sliderResponse_wp(target_wp);
    resetTiming_wp();
});

$('.prev_wp').click(function() {	
    target_wp = $('ul.triggers_wp li.active').index();
    lastElem_wp = $('ul.triggers_wp li').length-1;
    target_wp === 0 ? target_wp = lastElem_wp : target_wp = target_wp-1;
    sliderResponse_wp(target_wp);
    resetTiming_wp();
});

function sliderTiming_wp( lastElem_wp ) {
    target_wp = $('ul.triggers_wp li.active').index();
    target_wp == lastElem_wp ? target_wp = 0 : target_wp = target_wp+1;
    sliderResponse_wp(target_wp);
}

var timingRun_wp = setInterval(function() { sliderTiming_wp(lastElem_wp); },5000);
function resetTiming_wp() {
    clearInterval(timingRun_wp);
    timingRun_wp = setInterval(function() { sliderTiming_wp(lastElem_wp); },5000);
}


</script></div>
