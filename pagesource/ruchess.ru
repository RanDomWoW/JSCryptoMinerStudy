<script>
function slide_next(elemlem, lemlem)
{   
    if(elemlem.next().data("time"))
    {
        lemlem.find(".elemsl").hide();
        elemlem.next().show(); 
        setTimeout(function() {slide_next(elemlem.next(), lemlem);}, elemlem.next().data("time")*1000); 
    }
    
    //alert(elemlem.next());  
}

function slide(lemlem)
{
    count_elems= lemlem.data("count");
    lemlem.find(".elemsl").hide();
    lemlem.find(".elemsl_first").show();
    lemlem.find(".elemsl").each(function(index){
        if(index==0)
        {               
              var this_lem =$(this);
              setTimeout(function() {slide_next(this_lem, lemlem);}, this_lem.data("time")*1000);
          
        }
    }); 

}
$(document).ready(function(){
  $(".slideelems").each(function(){
      var time=$(this).data("time")*1000;  
      var this_lem=$(this);  
      slide(this_lem);
      setInterval(function() {slide(this_lem);}, time);       
  });  
});
</script>
