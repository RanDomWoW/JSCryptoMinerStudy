		createEmospro: function() {
			window.emospro = {
					content: tracking.subUrlParts[0],
					siteid:  tracking.siteid,
					langid:  tracking.language
				};
		},

		trackECDownload: function() {
			createEmospro();
			window.emospro.download = event.currentTarget.href.split("/").pop();
			window.emosPropertiesEvent(window.emospro);
		},

		closestClass: function(element, className) {
			while (element) {
				if (element.className.indexOf(className) > -1) return element; 
				element = element.parentNode;
			}
		},

		trackECRecommendationEvent: function(src) {
			window.emcs0 = ACC.econda.closestClass(src.currentTarget, "econda-recommendation-container").dataset.widgetId;
			window.emcs1 = tracking.pageType;
			if (tracking.pageType == 'PRODUCT')
			{
				window.emcs2 = tracking.product.pzn;
			}
			url = src.currentTarget.querySelector("a").href;
			window.emcs3 = getPZNFromUrl(url);
		
			ACC.econda.createEmospro();
		
			window.emosPropertiesEvent(window.emospro);
		},

		bindPdpTracking: function() {
			tracking.forEach(document.querySelectorAll(".recommendation-entry .productGridItem"),
				function(index,element){
					element.addEventListener("click", function(event){
						ACC.econda.trackECRecommendationEvent(event);
					});
				}
			);
		}
	};

	if (typeof window.emospro === 'undefined') {
		ACC.econda.createEmospro();
	} else {
		ACC.econda.createEmospro();
		window.emospro.register = [CryptoJS.MD5(tracking.user).toString(), 0];
	}

	if ((tracking.pageType == 'PRODUCTSEARCH') && (tracking.currentSubUrl.indexOf('/seite/') == -1))
	{
		window.emospro.search = [tracking.search.text, tracking.search.results];
	}
	else if (tracking.pageType == 'PRODUCT')
	{
		window.emospro.ec_Event = [[
			'view',
			tracking.product.pzn,
			tracking.product.name,
			tracking.product.price,
			tracking.product.categoryPath,
			'1',
			'NULL',
			'NULL',
			'NULL'
		]];
	}
	else if (tracking.pageType == 'ORDERCONFIRMATION')
	{
		window.emospro.ec_Event = [];

		tracking.forEach(tracking.order.entries,
			function(index,element){
				window.emospro.ec_Event = emospro.ec_Event.concat([[
					'buy',
					element.pzn,
					element.name,
					element.basePrice,
					element.categoryPath,
					element.quantity,
					'NULL',
					'NULL',
					'NULL'
				]]);
			}
		);

		window.emospro.billing = [[
			tracking.order.code,
			CryptoJS.MD5(tracking.order.user).toString(),
			tracking.order.location,
			tracking.order.totalPrice
		]];
	}

	// login tracking
	if (!(typeof tracking.login === "undefined"))
	{
		window.emospro.login = [CryptoJS.MD5(tracking.login.id).toString(), tracking.login.success]
	}

	// Order process tracking
	if (tracking.currentSubUrl.indexOf('/cart') > -1)
	{
		window.emospro.orderProcess = '1_Warenkorb';
	}
	else if (tracking.currentSubUrl.indexOf('/login/checkout') > -1)
	{
		window.emospro.orderProcess = '2_Login-Zahladresse';
	}
	else if (tracking.currentSubUrl.indexOf('/checkout/multi/add-payment-shipping') > -1)
	{
		window.emospro.orderProcess = '3_Zahlmethode-Lieferadresse';
	}
	else if (tracking.currentSubUrl.indexOf('/checkout/multi/summary') > -1)
	{
		window.emospro.orderProcess = '4_Bestelluebersicht';
	}
	else if (tracking.currentSubUrl.indexOf('/checkout/bestellbestaetigung') > -1)
	{
