    function submitForm_new(type,num){
        var money = $('#money'+num).val();
        var dkqx = $('#dkqx'+num).val();
        var linkageid = $('#linkageid'+num).val();
        var shoufu = $('#shoufu').val();
        if(num!=4 && type!='daikuan-mc'){
            if(linkageid){
                var url = '/'+type+ '-' +linkageid+ '-' +money+ '-' +dkqx+'-0-0-0-0/';
            }else{
                var url = '/'+type+ '-0-' +money+ '-' +dkqx+'-0-0-0-0/';
            }
        }else{
            if(linkageid){
                var url = '/'+type+ '-' +linkageid+ '-' +money+ '-' +dkqx+'-0-0-0-0-'+shoufu+'/';
            }else{
                var url = '/'+type+ '-0-' +money+ '-' +dkqx+'-0-0-0-0-'+shoufu+'/';
            }
        }
        if(num==6){     //列表页
            document.form6.action = url;
            document.form6.submit();
        }else if(num==1){   //消费贷款
            document.form1.action = url;
            document.form1.submit();
        }else if(num==2){   //企业贷款
            document.form2.action = url;
            document.form2.submit();
        }else if(num==3){   //买房贷款
            document.form3.action = url;
            document.form3.submit();
        }else if(num==4){   //买车贷款
            document.form4.action = url;
            document.form4.submit();
        }
        return true;
    }

