    function openModal(obj_) {
        obj_.style.display = "block";
        $("body").css("overflow-y", "hidden");
        $("body").css("overflow-x", "hidden");
    }
    function closeModal(obj_my) {
        obj_my.style.display = "none";
        $("body").css("overflow-y", "scroll");
        $("body").css("overflow-x", "hidden");
       
    }

    var slideIndex = 1;
    showSlides(slideIndex, v);

    function plusSlides(v) {
        showSlides(slideIndex += 1, v);
    }
    function subSlides(v) {
        showSlides(slideIndex += -1, v);
    }
    function currentSlide(n, v) {
        showSlides(slideIndex = n, v);
    }

    function showSlides(n, v) {
        var xyz = '#showdata' + v + ' .mySlides';
        var value = document.querySelectorAll(xyz);
        var dots = document.getElementsByClassName("demo");
        var captionText = document.getElementById("caption");
        if (n > value.length) { slideIndex = 1 }
        if (n < 1) { slideIndex = value.length }
        for (i = 0; i < value.length; i++) {
            value[i].style.display = "none";
        }
        for (i = 0; i < dots.length; i++) {
            dots[i].className = dots[i].className.replace("active", "");
        }
        value[slideIndex - 1].style.display = "block";
        dots[slideIndex - 1].className += " active";
        captionText.innerHTML = dots[slideIndex - 1].alt;
    }
    function destroyVideo(video_id) {
       // var url = $('#youtubeplayer').attr('src');
       // $('#youtubeplayer').attr('src', '');
        // $('#youtubeplayer').attr('src', url);

        for (i = 1; video_id >= i; i++) {
            var data = "youtubeplayer" + i;
            var youtubeIframeId = document.getElementById(data).src; 
            document.getElementById(data).src = youtubeIframeId;
        }
       
    }

    function allprevious(video_id) {

        destroyVideo(video_id);
        subSlides(0);
        
    }
    function allnext(video_id) {
        destroyVideo(video_id);
        plusSlides(0);

    }
