<body class="login">
    <div id="ProgressBox"></div>
    <!-- BEGIN LOGO -->
    <div class="logo"></div>
    <!-- END LOGO -->
    <!-- BEGIN LOGIN -->
    <div class="content CustShadow ">
        <!-- BEGIN LOGIN FORM -->
        <div class="login-form" id="DivLogin">
            
            <a href="/">
                <img src="/Contents/images/LoginKingdomLogo.png?Ver=1.0302" alt="Kingdom" class="ImgResp" width="150" height="150" />
            </a>
            
            <h3 class="form-title">Login to your account</h3>
            <div class="col-md-12 col-xs-12 NoPadding">
                <div id="DivDisplayMsg" style="display: none;">
                    <button class="close" data-close="alert"></button>
                    <span id="SpnMsgHeader"></span>
                    <span id="SpnErrorMsg"></span>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label">Username</label>
                <div class="input-icon">
                    <i class="fa fa-user"></i>
                    <input class="form-control placeholder-no-fix" type="text" id="txtUserName" placeholder="Username" name="username" />
                </div>
            </div>
            <div class="form-group">
                <label class="control-label">Password</label>
                <div class="input-icon">
                    <i class="fa fa-lock"></i>
                    <input class="form-control placeholder-no-fix" type="password" id="txtPassword" placeholder="Password" name="password" />
                </div>
            </div>
            <div class="form-actions">
                <label class="rememberme mt-checkbox mt-checkbox-outline">
                    <input type="checkbox" name="remember" id="chkRememberMe" />
                    Remember me
                    <span></span>
                </label>
                <button type="submit" class="btn green pull-right" id="btnLogin">Login</button>
            </div>
            <div class="forget-password">
                <h4>Forgot your password ?</h4>
                <p>
                    no worries, click
                    <a href="javascript:void(0);" id="LinkForgetPassword">here </a>to reset your password.
                </p>
            </div>
        </div>
        <!-- END LOGIN FORM -->
        <!-- BEGIN FORGOT PASSWORD FORM -->
        <div class="hide" id="DivForgotPass">
            <div class="col-md-12 col-xs-12 NoPadding">
                <div id="DivDisplayMsg1" style="display: none;">
                    <button class="close" data-close="alert"></button>
                    <span id="SpnMsgHeader1"></span>
                    <span id="SpnErrorMsg1"></span>
                </div>
            </div>
            <h3>Forget Password ?</h3>
            <p>Enter your Username below to reset your password. </p>
            <div class="form-group">
                <div class="input-icon">
                    <i class="fa fa-envelope"></i>
                    <input class="form-control placeholder-no-fix" type="text" id="txtForgotPassEmail" placeholder="Enter Username" name="email" />
                </div>
            </div>
            <div class="form-actions BorderBttom0">
                <button type="button" id="btnForgotBack" class="btn grey-salsa btn-outline">Back</button>
                <button type="submit" class="btn green pull-right" id="btnForgotPass">Submit</button>
            </div>
        </div>
        <!-- END FORGOT PASSWORD FORM -->
        <!-- BEGIN SWITCH COMPANY FORM -->
        <div class="hide" id="DivCouncil">
            <div class="col-md-12 col-xs-12 NoPadding">
                <div id="DivDisplayMsg2" style="display: none;">
                    <button class="close" data-close="alert"></button>
                    <span id="SpnMsgHeader2"></span>
                    <span id="SpnErrorMsg2"></span>
                </div>
            </div>
            <h3 class="MarginTop0">Select Council</h3>
            <div class="portlet light ">
                <div class="portlet-body form">
                    <div class="form-horizontal">
                        <div class="form-body NoPadding">
                            <div class="form-group">
                                <select class="bs-select form-control drpCouncil" data-live-search="true" data-size="8" name="drpCouncil" id="drpCouncil">
                                    <option value="0">-- Select Council --</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="form-actions BorderBttom0">
                <button type="button" id="btnCouncilBack" class="btn grey-salsa btn-outline">Back</button>
                <button type="submit" class="btn green pull-right" id="btnCouncil" onclick="SwitchCompany();">Submit</button>
            </div>
        </div>
        <!-- END SWITCH COMPANY FORM -->
    </div>
    <!-- END LOGIN -->
    
    
    


