<script type="text/javascript">

function main_plan(get_gubun){

	for(var main_cnt=1; main_cnt < 3; main_cnt++) {

		if (get_gubun == main_cnt) {
			document.getElementById('on'+main_cnt).src = "http://image.nhmarket.kr/data/rental/2014_main/main_ban_on.png";
			document.getElementById('main_plan'+main_cnt).style.display = "block";
		} else {
			document.getElementById('on'+main_cnt).src = "http://image.nhmarket.kr/data/rental/2014_main/main_ban_off.png";
			document.getElementById('main_plan'+main_cnt).style.display = "none";
		}

	}

}

function btnWeeklyChange(get_area_best,get_ranking) {

	for(var weekly_cnt=1; weekly_cnt < 4; weekly_cnt++) {

		if (get_ranking == weekly_cnt) {

			document.getElementById(get_area_best+weekly_cnt).style.display = "block";

		} else {

			document.getElementById(get_area_best+weekly_cnt).style.display = "none";

		}

	}

}

function WeeklyBestDisplay(get_roof_cnt, gubun, over) {
	
	for(var weekly_cnt=1; weekly_cnt < 46; weekly_cnt++) {

		if (get_roof_cnt == weekly_cnt) {

			if (over == "over") {

				document.getElementById("WeeklyBest"+gubun+weekly_cnt).style.display = "block";

			} else {

				document.getElementById("WeeklyBest"+gubun+weekly_cnt).style.display = "none";

			}

		} 

	}

}

</script>
