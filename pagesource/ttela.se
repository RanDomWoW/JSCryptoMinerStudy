              Tulo.logout(window.location.href+"?logout=true");
              return false;
            });
            window.slm.user.logged_in = true;
            window.slm.user.id = data.id;
            window.slm.user.name = data.display_name;
            window.slm.user.email = data.contact_email;
            window.slm.user.products = data.products;
            /* For issues when buying */
            var onlineaccess = getCookie("p_onlineaccess");
            if(onlineaccess != null && onlineaccess.match(/[0-9a-z]/i) == null){
              /* No package! Do we have one in tulo-cookie? */
              var tuloproducts = window.slm.user.products;
              substring = window.slm.premiumConfig.tuloProductCode;
