              }
            }
          }
        });
      } else {
        Tulo.session(function (data) {
          if (data && data.active) {
            /* inlogg gammal, inloggad tulo */
            var loginAttempted = getCookie("p_loginAttempted");
            if (loginAttempted && loginAttempted.length > 0){
              /* Inloggningen misslyckades */
            } else {
              /* inlogg gammal, INLOGGAD TULO, loginAttempted finns ej, REDIRECT SKA SKE */
              clearSLMCookies();
              document.cookie = 'p_loginAttempted=true; Path=/; Expires=/';
