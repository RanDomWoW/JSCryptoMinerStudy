            'price': 1,
            'currency': 'SEK',
            'quantity': 1,
            'author' : author
          }],
          'event': 'slm-ecommerce'
        });
          
        window.slm.DY.sendEvent('BoughtPremium', {
          contentId: window.slm.article.articleid,
          name: stripUnwantedChars(document.title),
          category: window.slm.article.category.main,
          currency: 'SEK',
          quantity: 1
        });
          
        setTimeout(function(){ window.location = login_url; }, 1000);
      }
    }
    var eventMethod = window.addEventListener ? "addEventListener" : "attachEvent";
