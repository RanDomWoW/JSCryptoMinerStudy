          if( __article["id"] == window.slm.article.articleid ){
            author = __article["author"];
            category = __article["main-category"]
            if(typeof __article["sub-category"] !== "undefiend" && __article["sub-category"] != ""){
              category += "/" + __article["sub-category"];
            }
          }
        }
        });
        dataLayer.push({
          'transactionId': data.payway_order_id,
          'transactionAffiliation': window.slm.premiumConfig.klarnaSite,
          'transactionTotal': 1,
          'transactionProducts': [{
            'sku': window.slm.article.articleid,
