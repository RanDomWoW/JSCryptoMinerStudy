.Book {
    -webkit-perspective: 1800px;
            perspective: 1800px;
}
.Book-container {
    -webkit-transform-style: preserve-3d;
            transform-style: preserve-3d;
    height: 300px;
    width: 300px;
    -webkit-transition: -webkit-transform .5s;
            transition: transform .5s;
    -webkit-transform: rotate3d(0,1,0,-10deg) rotate3d(1,0,0,10deg);
            transform: rotate3d(0,1,0,-10deg) rotate3d(1,0,0,10deg);
}
.Book-small .Book-container {
    height: 150px;
    width: 150px;
}
.Book-container.is-hovered,
.Book-container:hover {
    -webkit-transform: rotate3d(0,1,0,0deg);
            transform: rotate3d(0,1,0,0deg);
}
.no-preserve3d .Book-container {
    -webkit-transform: none;
        -ms-transform: none;
            transform: none;
}

.Book-front {
    position: absolute;
    background-color: #14b8c0;
    height: 300px;
    width: 100%;
    -webkit-transform-style: preserve-3d;
            transform-style: preserve-3d;
    -webkit-transform-origin: 0 50%;
        -ms-transform-origin: 0 50%;
            transform-origin: 0 50%;
