	<script type="text/javascript">	

(function($){

		jQuery(function($){
			$(document).ready(function($){
				init_carousel();
			});
			$(window).on('resize', function($){
				//init_carousel();
			});	
		});

		var slides = 
			[
				{
					'image'	: 'url(http://www.universitystudy.ca/wp-content/themes/universitystudy/images/slides/stairs.jpg)',
					'title' : 'EXPLORE CANADA&rsquo;S UNIVERSITIES',
					'blurb' : 'University profiles, tuition information, and more!',
					'url'	: 'canadian-universities'
				},
			
				{
					'image'	: 'url(http://www.universitystudy.ca/wp-content/themes/universitystudy/images/slides/computing.jpg)',
					'title' : 'SEARCH STUDY PROGRAMS',
					'blurb' : 'Our large database makes searching for certificate, diploma and degree programs easy.',
					'url'	: 'search-programs'
				},
				{
					'image'	: 'url(http://www.universitystudy.ca/wp-content/themes/universitystudy/images/slides/mac.jpg)',
					'title' : 'GET PLANNING ADVICE',
					'blurb' : 'Articles on scholarships, university selection, applications and many other topics!',
					'url'	: 'plan-for-university'
				}
			];

		var slide_timer = null;
		var current_slide = 0;

		var effect_speed = 200;

		function init_carousel (){
			
			current_slide = 0;

			// setup events and triggers
			$('.carousel-image').live('click', function(){
				document.location.href = slides[current_slide].url;
			});

			$('.carousel-title a').live('click', function(){
				clearInterval(slide_timer);
			});
			$('.carousel-title a').live('focus', function(){
				clearInterval(slide_timer);
			});
			$("#carousel").on({
			    mouseenter: function () {
			    	clearInterval(slide_timer); 
			    },
			    focus: function(){
					clearInterval(slide_timer); 
			    },
			    mouseleave: function () {
			    	slide_timer = setInterval(play_slides, 4000);
			    },
			    focusout: function(){
					clearInterval(slide_timer); 
			    }
			});

			$('.carousel-nav li a').on('click', function(e){
				e.stopPropagation;
				clearInterval(slide_timer);
				nav_slide($(this).parent().index());
				//return false;
			});
			$('.carousel-nav li a').on('focus', function(e){
				clearInterval(slide_timer);
			});


			// reset and run !
			clearInterval(slide_timer);
			slide_timer = setInterval(play_slides, 4000);

		}

		function play_slides() {
		    nav_slide();
  		}

		function nav_slide(slide_index){

			if(slide_index > -1){
				current_slide = slide_index;
			}else if ( current_slide+1 < slides.length ){
				current_slide++;
			}else{
				current_slide = 0;
			}

			var slide = slides[current_slide];
			$('.carousel-image').fadeOut('effect_speed',function(){
				$('.carousel-image').css('background-image', slide.image );	
				var nav_index = current_slide+1;
				$('.carousel-nav li').removeClass('carousel-active');
				$('.carousel-nav li:nth-child('+nav_index+')').addClass('carousel-active');
				$('.carousel-dots li').removeClass('carousel-active');
				$('.carousel-dots li:nth-child('+nav_index+')').addClass('carousel-active');

				$(this).fadeIn(effect_speed);
				
			});

			$('.carousel-title').fadeOut(effect_speed, function(){
					$(this).html('<a href="'+slide.url+'">'+slide.title+'&nbsp;&raquo;</a>').fadeIn(effect_speed);
				});
			$('.carousel-blurb').fadeOut(effect_speed, function(){
				$(this).html(slide.blurb).fadeIn(effect_speed);
			});
		}
		function debug(s){
			console.log(s);
		}
})(jQuery)
	</script>

