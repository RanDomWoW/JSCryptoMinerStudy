	<div class="search-overlay">
		<div class="flex">
			<form action="/zoeken" method="get" class="content relative">
				<button type="button" class="search-overlay-close close-search-overlay img-replace">Sluiten</button>
				<img class="search-overlay-logo" src="//cdn.vtwonen.nl/app/assets/img/vtwonen-logo.png" alt="vtwonen">
				<div class="search-overlay-text-outer">
					<input type="search" class="search-overlay-text" name="q" placeholder="vul hier je zoekterm in..." autocomplete="off">
					<button type="submit" class="search-overlay-submit img-replace">Zoek</button>
				</div>
				<div class="row columns search-suggestions">
					<div class="col-2 mb-20">
						<div class="col-inner">
							<h2 class="search-suggestions-title">Shop suggesties</h2>
							<div class="search-suggestion search-suggestion-shop list">
								
							</div>
							<div class="row">
								<button type="button" class="button button-submit shop-all-result pull-right">Toon alle shop resultaten</button>
							</div>
						</div>
					</div>
					<div class="col-2">
						<div class="col-inner">
							<h2 class="search-suggestions-title">Inspiratie suggesties</h2>
							<div class="search-suggestion search-suggestion-inspiration list">
								
							</div>
							<div class="row">
								<button type="submit" name="sectie" value="inspiratie" class="button button-submit pull-right">Toon alle inspiratie resultaten</button>
							</div>
						</div>
					</div>
				</div>
			</form>
		</div>
	</div>
	

	
	
	<script id="search-suggestion-template" type="text/x-handlebars-template">
		<div class="search-suggestion-item">
			<a href="{{permalink}}" class="search-suggestion-link">
				<img class="search-suggestion-img" src="{{image}}" alt="Afbeelding bij: {{title}}">
				{{#if category}}<p class="search-suggestion-category">{{category}}</p>{{/if}}
				<h3 class="search-suggestion-title">{{title}}{{#if price}} || {{price}}{{/if}}</h3>
			</a>
		</div>
	</script>
	
	<script id="basket-product-template" type="text/x-handlebars-template">
		<div class="basket-product-item row" data-quantity="{{quantity}}">
			<a class="basket-product-link" href="{{link}}">
				<img class="basket-product-image" src="{{image}}" alt="Afbeelding bij: {{title}}">
				<div class="basket-product-content">
					<div class="basket-product-content-inner">
						<h3 class="basket-product-title">{{quantity}} &times; {{title}} | {{price}}</h3>
					</div>
				</div>
			</a>
			<button type="button" data-url="{{removeLink}}" class="basket-product-remove img-replace transition3s">Artikel verwijderen</button>
		</div>
	</script>
	
	
		
	<div id="enriched-overlay" class="overlay">
		<div class="overlay-outer">
			<div class="overlay-inner">
				<div class="overlay-box">
					
				</div>
			</div>
		</div>
	</div>
	
	<script id="enrichment-overlay" type="text/x-handlebars-template">
		<div class="enrichment-overlay-outer{{#if recommended}} recommended{{/if}}">
			<button type="button" class="enrichment-overlay-close close-enrichment img-replace">Sluiten</button>
			<div class="enrichment-overlay-inner row">		
				<a class="enrichment-image" href="{{link}}"><img src="{{image}}" alt=""></a>
				<div class="enrichment-content">
					<div class="enrichment-content-text">
						<h2>{{title}}</h2>
						{{{text}}}
					</div>
					<div class="enrichment-content-meta">
						
						<div class="row enrichment-meta-bar">
							{{#if price}}
							<p class="enrichment-old-price">{{priceBefore}}</p>
							<p class="enrichment-price">{{price}}</p>
							<div class="pull-right">
								<a href="{{shopLink}}" class="button pull-right button-pink in-shopping-cart" data-id="{{shopId}}" data-sku="{{sku}}" data-title="{{title}}">In winkelwagen</a>
							</div>
							{{else}}
							<a href="{{link}}" class="button pull-right button-pink">Lees verder</a>
							{{/if}}
						</div>
						
						{{#if recommended}}
						<div class="enrichment-meta-recommended">
							<h3>meer suggesties</h3>
							<div class="cols row">
								{{#each recommended}}
									
									<article class="article article-normal">
										<a href="{{permalink}}" class="article-inner-alt catalogus-recommended" data-label="{{title}}">
											<img src="{{image}}" alt="{{title}} - {{price_formatted}}" title="{{title}} - {{price_formatted}}">
										</a>
									</article>
									
								{{/each}}
							</div>
						</div>
						{{/if}}
						
						<ul class="article-meta-list list">
							<li class="article-meta-item article-meta-share">
								<span class="article-meta-link share-button">
									<span class="share-button-text">delen</span>
									<ul class="share-list list">
										<li class="share-item share-item-facebook">
											<a class="share-link" data-type="facebook" data-text="vtwonen" data-url="{{link}}">facebook</a>
										</li>
										<li class="share-item share-item-pinterest">
											<a class="share-link" data-type="pinterest" data-text="vtwonen" data-media="{{image}}" data-url="{{link}}">pinterest</a>
										</li>
										<li class="share-item share-item-twitter">
											<a class="share-link" data-type="twitter" data-text="{{title}} - vtwonen" data-url="{{link}}">twitter</a>
										</li>
										<li class="share-item share-item-google">
											<a class="share-link" data-type="google" data-text="vtwonen" data-url="{{link}}">google</a>
										</li>
										<li class="share-item share-item-newsletter">
											<a class="share-link" data-type="email" data-text="{{title}} - vtwonen" data-url="{{link}}">mail</a>
										</li>
									</ul>
								</span>
							</li>
						</ul>
						
						<a class="becommerce pull-right" target="_blank" href="https://www.becommerce.be/nl/leden/zoek-een-lid/d/certificate/1413">
							<img src="//cdn.vtwonen.nl/app/assets/img/becommerce.png" alt="BeCommerce">
						</a>
					</div>
				</div>
			</div>
		</div>
	</script>
	
		
	
	
