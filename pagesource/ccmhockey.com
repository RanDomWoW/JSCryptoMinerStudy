<script>
	queueTask = typeof queueTask != 'undefined' ? queueTask : new Array();
	(function($) {
		/** START NEWSLETTER OPT IN **/
		newsletterManagement = new function(){

			var self = this;

			self.manageOptIn = function (pData) {
				var newsletter_checkbox = jQuery('#dropdown-opt-in');
				var checkbox = jQuery('#newsletter-opt-in');
				if(pData.length > 0){
					newsletter_checkbox.addClass('active');
				}else{
					newsletter_checkbox.removeClass('active');
					checkbox.attr('checked', false);
				}
			}

			self.checklegal = function(applyError){
				if(!$('.js-newsletter-checkbox').prop('checked') ){
					if(applyError){
						$('.js-newsletter-checkbox').addClass("error");
					}
					return false;
				}
				$('.js-newsletter-checkbox').removeClass("error");	
				return true;
			}

			self.checkformat = function(applyError){
				if(!(/^[A-Z0-9\.\_\%\+\-]+@[A-Z0-9\.\_\%\+\-]+\.[A-Z]{2,4}$/img).test($('.js-email-field').val()) ){
					if(applyError){
						$('.js-error-text').html("Invalid email format");
					}
					return false;
				}
				$('.js-error-text').html("");
				return true;
			}

			queueTask.push(function(){
				$(".js-newsletter-checkbox").bind("click",function(e){
					self.checklegal();
				});
				$(".js-newsletter-form").bind("submit",function(e){
					if(!self.checkformat(true) || !self.checklegal(true)){	
						e.stopPropagation();
						e.preventDefault();
						return false;
					}
				});
				$('#newsletter-subscribe input[type="text"]').bind('input', function(){
					self.manageOptIn(this.value);
				});

				if($("#newsletter-text-success").text()){
					dataLayer.push({'newsletter-text-success': 'success'});
					$("#newsletter-text-success").trigger( "newsletter-text-success" );
				}

				if($("#newsletter-text-error").text()){
					dataLayer.push({'newsletter-text-error': 'error'});
					$("#newsletter-text-error").trigger( "newsletter-text-error" );
				}

			});

		}
		/** END NEWSLETTER OPT IN **/
	})(jQuery);
