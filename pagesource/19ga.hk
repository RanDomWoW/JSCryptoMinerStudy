			}
		});
	});
	function scroll_load(){
		
		if(next && enable){
			$('.btn-load-more').stop().fadeIn();
			enable = false;
			$.ajax({
				method : "GET",
				url:"https://19ga.hk/page/"+page_no+"/",
				data:{}
			}).done(function(data){
				$('#posts-list-container .posts-list').append(data);
				if($('#posts-list-container .posts-list a:contains("nextpage")').length>0)
					next = true;
				else{
					next = false;
				}
				$('#posts-list-container .posts-list a:contains("nextpage")').remove();
				$(':not(.feature-post-others-container) .top-feature .feature-post-description p').trunk8({
					lines: 3,
				});
