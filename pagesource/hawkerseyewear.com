            var actual_action = $('#cartform').attr('action');
            var new_action = actual_action + '&' + ga_client_id;

            $('#cartform').attr('action', new_action);

            clearInterval(ga_interval);
        }
        else
        {
          ga_interval = setInterval(function(){ get_ga_client_id(); }, 1000);
        }
    }

