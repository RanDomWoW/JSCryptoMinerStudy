    <script>
        $(document).ready(function () {

    var defaultId = 4;
    var defaultCity = "اصفهان";


    $(".weather select option[id='" + defaultId + "']").attr('selected', 'selected');
    $(".prayer select option[id='" + defaultId + "']").attr('selected', 'selected');

    weatherGetData(defaultId);
    prayerGetData(defaultId);

    $('.weather select').on('change', function () {
        var id = $(this).find("option:selected").attr("id");
        $(".weather .frame .header").append("<img class=\"pull-right\" src=\"/Content/images/loading.gif\" />");
        weatherGetData(id);
    });

    function weatherGetData(id) {
        $.ajax({
            type: "GET",
            url: "http://api.farsnews.com/news/GetWeather",
            dataType: "json",
            data: "id=" + id,
            success: function (data) {
                weatherShowData(data);
            }
        });
    }

    function weatherShowData(data) {
        var weather = data.cityw;
        var finalData = "<div class='header'>" +
            "<div class='pull-right'><span>آب و هوا - </span><span class='brown'>" + weather["CenterProvince"] + "</span></div>" +
            "</div>" +
            "<div class='body'>" +
            "<div class='pull-right'>" +
            "<span>" + data["cond"] + "</span>" +
            "<div><span>رطوبت:</span>&nbsp;<span class='num blue'>%" + weather["humidity"] + "</span></div>" +
            "<div><span>دما:</span> <span class='num red'>" + weather["TodayHighTemp"] + "</span> ، <span class='num blue'>" + weather["TodayLowTemp"] + "</span></div>" +
            "<div><span>فردا:</span> <span class='num red'>" + weather["TomorrowHighTemp"] + "</span> ، <span class='num blue'>" + weather["TomorrowLowTemp"] + "</span></div>" +
            "</div>" +
            "<div class='today pull-right'>" +
            "<span>" + weather["TodayTemp"] + "˚</span>" +
            "</div>" +
            "</div>";
        $(".weather .frame").empty().append(finalData);
    };


    // ************************** Prayer Time **************************

    $('.prayer select').on('change', function () {
        var id = $(this).find("option:selected").attr("id");
        defaultCity = $(this).find("option:selected").text();
        $(".prayer .brown").append("<img class=\"pull-left\" src=\"/Content/images/loading.gif\" />");
        prayerGetData(id);
    });

    function prayerGetData(id) {
        $.ajax({
            type: "GET",
            url: "http://api.farsnews.com/news/GetOqat",
            dataType: "json",
            data: "id=" + id + "&date=td",
            success: function (data) {
                prayerShowData(data);
            }
        });
    }

    function prayerShowData(data) {
        var finalData = "<div class=\"header\">" +
        "<div class=\"pull-right wide\">" +
        "<div>" +
        "<span>اوقات شرعی - </span>" +
        "<span class=\"brown\">" + defaultCity + "</span>" +
        "</div>" +
        "<span class=\"pull-left\">" + data["persianDate"] + "</span>" +
        "</div>" +
       "</div>" +
        "<div class=\"body\">" +
        "<div><span>اذان صبح</span><span class=\"num\">" + data["azaneSobh"] + "</span></div>" +
        "<div><span>طلوع آفتاب</span><span class=\"num\">" + data["tolooeAftab"] + "</span></div>" +
        "<div><span>اذان ظهر</span><span class=\"num\">" + data["azaneZohr"] + "</span></div>" +
        "<div><span>غروب آفتاب</span><span class=\"num\">" + data["qoroobeAftab"] + "</span></div>" +
        "<div><span>اذان مغرب</span><span class=\"num\">" + data["azaneMaqreb"] + "</span></div>" +
        "<div><span>نیمه شب شرعی</span><span class=\"num\">" + data["nimeshabeSharei"] + "</span></div>" +
        "</div>";
        $(".prayer .frame").empty().append(finalData);

        toPersianNum();
    };

    function toPersianNum() {
        var persian = { 0: '۰', 1: '۱', 2: '۲', 3: '۳', 4: '۴', 5: '۵', 6: '۶', 7: '۷', 8: '۸', 9: '۹' };
        $(".prayer .header").each(function () {
            for (var i = 0; i <= 9; i++) {
                var re = new RegExp(i, "g");
                $(this).html($(this).html().replace(re, persian[i]));
            }
        });

    }

});
    </script>


