            document.getElementById('vps_like_box').style.display = 'block';
            document.getElementById('vps_like_fade').style.display = 'block';
            VpsPopupActive = "ok";
        } else if (VpsType == "4" && VpsCookieLike == null) {
            VpsCloseActionNext = "share";
            document.getElementById('vps_like_box').style.display = 'block';
            document.getElementById('vps_like_fade').style.display = 'block';
