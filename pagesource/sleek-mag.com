		<script>
			
			String.prototype.trunc = 
			function(n){
				return this.substr(0,n-1)+(this.length>n?'…':'');
			};
			
			$(document).ready(function(){
				//if($('.entry-content').length){
				//	var entryTop = '200px';
				//}
				var stickyTop = 1;
				var masthead = $('#masthead');
				var nextpost = $('#sleek-next-post');
				var next_title = nextpost.find('h5');
				//var shareBox = $('#share-box-fixed');
				var searchToggle = $('#search-toggle');
				var burgerToggle = $('#burger-toggle');
				var searchInput = $('#s');
				var searchOverlay = $('#search-overlay');
				var dropDown = $('#drop-menu');
				var socialFooter = $('#social-footer');
				
				//if(typeof shareBox !== 'undefined' && typeof entryTop !== 'undefined' ){
				//	shareBox.css('top',entryTop);
				//}
				
				if(nextpost.length > 0){
					setTimeout(function(){
						nextpost.addClass('reveal');
						if(next_title.text().length > 40){
							next_title.text(next_title.text().trunc(40));
						}
						
					}, 5000);					
				}
				
				var doSticky = function(){
				    if ($(this).scrollTop() > stickyTop && $(window).width() > 768) {
						masthead.addClass("sticky");
						burgerToggle.attr('aria-expanded', 'false').removeClass('is-active');
						dropDown.removeClass('toggled');
						searchToggle.attr('aria-expanded', 'false').removeClass('is-active');
						searchOverlay.removeClass('toggled');
						
						nextpost.addClass("sticky");
						
				    }
				    else {
						masthead.removeClass("sticky");
						nextpost.removeClass("sticky");
				    }
				}
				
				$(window).scroll(function() {  
					doSticky();
				});
			
				$(document).on('touchmove', function() {
					doSticky();
				});
				
				doSticky();
			
				masthead.hover(
					function(){
						$(this).removeClass("sticky");
						nextpost.removeClass("sticky");
					}
					,
					function(){
						if($(this).scrollTop() > stickyTop){
							$(this).addClass("sticky");
							nextpost.addClass("sticky");
						}
					}
				);
			
				$('#scrolltop').click(function(){
					$('html, body').animate({scrollTop : 0}, 800);
					return false;
				});
				
				// Search
				searchToggle.on('touchstart click', function(e){
					e.preventDefault();
					if(!$(this).hasClass('is-active')){
						burgerToggle.removeClass('is-active');
						dropDown.removeClass('toggled');
						$(this).attr('aria-expanded', 'true');
						setTimeout(function(){
							searchInput.focus();	
						},250);
					} else {
						$(this).attr('aria-expanded', 'false');
						searchInput.blur();
					}
					$(this).toggleClass('is-active');
					searchOverlay.toggleClass('toggled');
					masthead.removeClass('full-height-absolute');
					socialFooter.removeClass('toggled');
				});
				
				burgerToggle.on('touchstart click', function(e){
					e.preventDefault();
					searchInput.blur();
					if(!$(this).hasClass('is-active')){
						searchToggle.removeClass('is-active');
						searchOverlay.removeClass('toggled');
						$(this).attr('aria-expanded', 'true');
						socialFooter.addClass('toggled');
						masthead.addClass('full-height-absolute');
					} else {
						$(this).attr('aria-expanded', 'false');
						masthead.removeClass('full-height-absolute');
						socialFooter.removeClass('toggled');
						$(this).blur();
					}
					$(this).toggleClass('is-active');
					dropDown.toggleClass('toggled');
				});
				
				$(document).keyup(function(e) {
					switch(e.keyCode){
						case 27:  // ESC
							$('#search-toggle, #burger-toggle').removeClass('is-active');
							$('#search-overlay, #drop-menu').removeClass('toggled'); // 27
							masthead.removeClass('full-height-absolute');
							searchInput.blur();
						break;
						case 13:  // ENTER
							if(searchOverlay.hasClass('toggled')){
								$('#search-submit').submit();
							}
						break;
					}
				});
			});			
		</script>		
