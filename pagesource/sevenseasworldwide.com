                controls: true,
                pager: false,
                auto: true,
                pause: 5000,
                nextSelector: '#offer-next',
                prevSelector: '#offer-prev',
                nextText: '<img src="../images/arrow-next.png">',
                prevText: '<img src="/images/arrow-prev.png">',
            });
        });
    </script>



</body>
