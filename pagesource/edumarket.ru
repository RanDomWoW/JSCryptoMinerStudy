            hiddenObj: "zpcalc_region_h",
            helpBlock: "help_zpcalc_region",
            onItemSelect: selectItemRZpCalcLayer
        });
        
        //почистим поля от тегов
        $('#layer_zpcalc_fio').on('blur',function(){
            $(this).val($(this).val().trim());
            //вычистим теги
            $(this).val(clearScriptTagsZpcalc($(this).val())); 
        }); 
        $('#layer_zpcalc_email').on('blur',function(){
            $(this).val($(this).val().trim());
            //вычистим теги
            $(this).val(clearScriptTagsZpcalc($(this).val())); 
        });

        //нажали выполнение 
        $('#zpcalc_btn_next').on('click', function() {
            if (checkZpCalcData()) {
                // alert ('ok');
                //собираем данные
                var fio = $('#layer_zpcalc_fio').val(),
                        email = $('#layer_zpcalc_email').val(),
                        prof = $('#zpcalc_prof_h').val(),
                        city = $('#zpcalc_region_h').val();

                //сохраняем пользователя
                var userid = $('#layer_zpcalc_user').val();
                //alert(userid);

                var data = new FormData();
                data.append('email', email);
                //новый пользователь?
                $.ajax({
                    url: '/modules/check_layers.php?mode=user_chck',
                    data: data,
                    dataType: 'json',
                    cache: false,
                    contentType: false,
                    processData: false,
                    type: 'POST',
                    // все ок, надо вывести строку
                    success: function(msg) {
                        var res = msg.res;
                        var userId = msg.user_id;
                        //проверяем ученик или учитель
                        if (res == -1) {
                            //учитель
                            //alert('учитель');
                            document.getElementById('zpcalc_msg_err').innerHTML = 'На данный e-mail уже зарегистрирован пользователь в статусе <br/>"Менеджер компании-организатора мероприятий". <br/>Вам необходимо зарегистрироваться на другой e-mail, чтобы создать аккаунт в статусе "Карьерист".';
                            $('#zpcalc_msg_err').show();
                        } else if (res == 1) { // активный или новый ученик, даем зеленый свет, но нужно ввести пароль
                            //alert('активный ученик');
                            document.getElementById('zpcalc_msg_err').innerHTML = 'На указанный e-mail на сайте edumarket.ru уже зарегистрирован пользователь. <br/>Для входа введите свой пароль.';
                            $('#zpcalc_msg_err').show();
                            //блокируем изменение данных, открываем ввод пароля
                            ViewZpCalcPassw();
                            // нажали войти (ввели пароль при этом)
                            $('#enter_zpcalc_passwd').bind('click', function() {
                                var password, res_passwd;
                                password = $('#layer_zpcalc_password').val();
                                $.ajax({
                                    url: '/modules/check_layers.php?mode=enter_passwd&password=' + password + '&user_id=' + userId + '&email=' + email,
                                    dataType: 'json',
                                    success: function(data_passwd) {
                                        res_passwd = data_passwd.res;
                                        // пошли проверки пароля
                                        if (res_passwd == 1) {
                                            //залогинились, скрываем сообщение
                                            $('#zpcalc_msg_err').hide();
                                            $('#layer_zpcalc_user').val(userId);
                                            //updateLayerProf(userId,prof);
                                            //выполняем заказанные действия
                                            // alert('выполняем заказанные действия '); 
                                            executeZpcalcActions(userId);
                                            // alert('yes');
                                        } else {
                                            //неверный пароль
                                            document.getElementById('zpcalc_msg_err').innerHTML = 'Пароль введен неверно. Вам необходимо <a href="/?get_page=032" target="_blank">восстановить пароль</a> либо зарегистрировать аккаунт на другой e-mail';
                                            $('#zpcalc_msg_err').show();
                                        }
                                    }
                                });//аякс
                            });//бинд
                        } else if (res == -2) { // ученик, заблокирован, говорим ему об этом
                            //alert('ученик, заблокирован -2');
                            document.getElementById('zpcalc_msg_err').innerHTML = 'На данный e-mail уже зарегистрирован пользователь. Статус пользователя: заблокирован. Вам необходимо обратиться в службу поддержки либо зарегистрировать аккаунт на другой e-mail';
                            $('#zpcalc_msg_err').show();
                        } else if (res == -3) { // ученик, заблокирован (не подтвердил регистрацию, можно активироваться), говорим ему об этом
                            //alert('ученик, заблокирован -3');
                            document.getElementById('zpcalc_msg_err').innerHTML = 'На данный e-mail уже зарегистрирован пользователь. Статус пользователя: заблокирован (не подтвердил регистрацию). Вам необходимо <a href="/?get_page=032" target="_blank">подтвердить регистрацию из письма</a> либо зарегистрировать аккаунт на другой e-mail';
                            $('#zpcalc_msg_err').show();
                        } else if (res == 2) { // пользователь уже залогинен и просто хочет изменить данные
                            //alert('пользователь уже залогинен');
                            //залогинились, скрываем сообщение
                            $('#zpcalc_msg_err').hide();
                            $('#layer_zpcalc_user').val(userId);
                            //выполняем заказанные действия
                            //  alert('выполняем заказанные действия '); 
                            executeZpcalcActions(userId);
                            //  alert('yes');
                        } else if (res == 3) { // человека такого еще нет, спокойно регистрируем нового
                            //alert('новый ученик');
                            //создаем нового пользователя
                            data.append('fio', fio);
                            // data.append('phone', phone);
                            data.append('prof_id', prof);
                            data.append('reg_id', city);
                            $.ajax({
                                url: '/modules/check_layers.php?mode=new_student',
                                data: data,
                                dataType: 'json',
                                cache: false,
                                contentType: false,
                                processData: false,
                                type: 'POST',
                                // все ок, надо вывести строку
                                success: function(new_student) {
                                    //alert(new_student.id);
                                    //залогинились, скрываем сообщение
                                    $('#zpcalc_msg_err').hide();
                                    $('#layer_zpcalc_user').val(new_student.id);
                                    //выполняем заказанные действия
                                    //  alert('выполняем заказанные действия '); 
                                    executeZpcalcActions(new_student.id);
                                    //         alert('yes'); 

                                }
                            });
                        } else { // непонятно кто
                            //alert('непонятно кто');           
                        }
                    }
                });
            }
        });
    });
</script>
