    $('#zpcalc_dialog_window').ready(function() {
        //автозаполнение профессии
        function selectItemPZpCalcLayer(li) {
            if (li == null)
                var sValue = "";
            if (!!li.extra)
                var sValue = li.extra;
            else
                var sValue = "";
            $('#zpcalc_prof_h').val(sValue);
        }
        $("#layer_zpcalc_prof").autocomplete("/autocomplete.php", {
            delay: 10,
            minChars: 1,
            matchSubset: 1,
            autoFill: true,
            maxItemsToShow: 20,
