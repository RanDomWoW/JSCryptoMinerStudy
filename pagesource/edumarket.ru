            if (!reg.test(email))
                $('#layer_zpcalc_email').addClass('error_border');
            if (password.length <= 0)
                $('#layer_zpcalc_password').addClass('error_border');
            document.getElementById('zpcalc_msg_err').innerHTML = 'Некоторые поля заполнены некорректно! Пожалуйста, проверьте правильность ввода.';
            $('#zpcalc_msg_err').show();
