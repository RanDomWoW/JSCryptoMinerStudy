.image_thumb:hover,.featured-bylabels #featured-home li img:hover{-webkit-filter:brightness(1);}
#featured_below #featured-home li img:hover,.featured-bylabels #featured-home li img:hover{-webkit-filter:brightness(1);}
/* Subscribe wrapper */
#subscribe-wrapper{background:#6bb7e2;margin:0 auto;padding:10px 0;width:100%;height:auto;color:#aaa;display:block;overflow:hidden}
#subscribe-limit{position:relative;width:100%;max-width:900px;padding:0;margin:0 auto;display:block;overflow:hidden;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;box-sizing:border-box;}
.subscribe-wrapper{color:#444;font-size:14px;line-height:normal;margin:0 10px;text-align:center;text-transform:none;font-weight:400;}
.subs-form{clear:both;display:block;margin:0 auto 5px;width:100%;overflow:hidden;position:relative;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;box-sizing:border-box;}
input.subs-field[type=&quot;text&quot;]{color:#555!important}
.subs-field{background:#fff;color:#555!important;margin:0;padding:11px 15px;width:100%;outline:none;border:none;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;box-sizing:border-box}
.subs-button{background:#446CB3;color:#fff;cursor:pointer;font-weight:700;padding:11px 50px;text-transform:none;font-size:14px;border:none;transition:all .3s;position:absolute;right:0;top:0}
.subs-button:hover{background:#000}
#subscribe-limit p.subscribe-text{color:#444;margin:0 10px 10px;text-align:center;font-size:120%;font-weight:700;line-height:1.3em}
/***Countries***/
#countries {width:100%;margin-bottom:20px;}
.column-1, .column-2, column-3 {width:33%; float:left;min-width:255px;margin-bottom:20px;}
