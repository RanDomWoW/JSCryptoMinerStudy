<script>
    $(function() {
        $(".floorClick").click(function() {
            $(".floorList").slideToggle();
            $(".floorFoot").toggleClass("floorFoot01")
        });
        $(".floorBox").css({ "right": ($(window).width() - 990) / 2 - 103 + "px" });
        //start
        var timer = null;
        var maxNum = 14;
        var H = 30;   //预留高度
        var $floorOneA = $(".floorOne a");
        // 获取所有link元素的top，存进数组
        var linkTop = [];

        for (var i = 0; i < maxNum; i++) {
            linkTop[i] = $("#link" + i).offset().top - H;
        }

        function getNowIndex(winScrollTop) {
            // 从后往前循环，当linkTop[i]小于winScrollTop的时候，返回i
            for (var i = maxNum - 1; i >= 0; i--) {
                if (winScrollTop >= linkTop[i]) {
                    return i;
                }
            }
        }

        function setscroll() {
            var h_num = $(window).scrollTop();
            if (h_num > 164) {
                $('.floorBox').addClass('fixer');
            } else {
                $('.floorBox').removeClass('fixer');
                $(".floorOne a").removeClass("cur");
            };

            var $winScrollTop = $(this).scrollTop();
            $floorOneA.removeClass("cur").eq(getNowIndex($winScrollTop)).addClass("cur");
        }

        $(window).scroll(function () {
            clearTimeout(timer);
            timer = setTimeout(function () {
                setscroll();
            }, 100);
        });
    });
    
