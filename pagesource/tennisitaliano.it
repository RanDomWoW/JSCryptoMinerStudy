    function header_showMenu(menuId) {
        //alert(menuId);

        $(".menu-current").removeClass("menu-current");
        var subMenu = $("#headerSubMenu_" + menuId);

        if (subMenu.text().trim() !== "") {

            $(".headerSubMenuContent").hide();
