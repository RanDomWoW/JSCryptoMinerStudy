<script type="text/javascript">
	jQuery(function(){
		/*jQuery('#mainNav li a').bind('mouseenter mouseleave', function() {
			$(this).toggleClass('entered');
		});*/
		
		jQuery('#faqsList .itemList h3 a').bind('click', function() {
			var parent = jQuery(this).parent().parent().attr('id');
			jQuery('#'+parent+' .itemContent').slideToggle('fast');
			jQuery('#'+parent).toggleClass('opened');
		});

		jQuery('#newsList .itemList h3 a').bind('click', function() {
			var parent = jQuery(this).parent().parent().attr('id');
			jQuery('.itemList .itemContent').slideToggle('fast');
			jQuery('.itemList').removeClass('opened');
			jQuery('#'+parent).toggleClass('opened');
			
			//jQuery('#'+parent+' .itemContent').slideToggle('fast');
			//jQuery('#'+parent).toggleClass('opened');
		});
		
		jQuery('#navWrapper').bind('mouseenter', function() {
			/*jQuery('.subNav').css('display','none');
			jQuery('#subNav1').css('display','block');*/
		});
		jQuery('#navWrapper').bind('mouseleave', function() {
			jQuery('.subNav').css('display','none');
			if (subNav!=0) {
				jQuery('#subNav'+subNav).css('display','block');
			}
			
			jQuery.each(jQuery('#mainNav li a'), function(key, value) {
				if (key+1==subNav)
					$(this).parent().addClass('selected');
			});
		});
		
				jQuery('#headerWrapper').bind('mouseenter', function() {
			var headerClass = jQuery('#headerWrapper').attr('class');
			if (headerClass=='closed') {
				$("#headerWrapper").animate({ 
					top: "0px"
				}, 500 );
				jQuery('#headerWrapper').removeClass('closed');
			}
		});
		jQuery('#headerWrapper').bind('mouseleave', function() {
			var headerClass = jQuery('#headerWrapper').attr('class');
			if (headerClass!='closed' && window.pageYOffset>80) {
				$("#headerWrapper").animate({ 
					top: "-125px"
				}, 500 );
				jQuery('#headerWrapper').addClass('closed');
			}
		});
				jQuery('#mainNav li a').bind('mouseenter', function() {
			var id= $(this).attr('id');
			jQuery('#mainNav li').removeClass('selected');
			if (id=='navCMT') {
				jQuery('.subNav').css('display','none');
				jQuery('#subNav1').css('display','block');				
			}else if (id=='navFormations') {
				jQuery('.subNav').css('display','none');
				jQuery('#subNav2').css('display','block');				
			}else if (id=='navTecnicas') {
				jQuery('.subNav').css('display','none');
				jQuery('#subNav5').css('display','block');
			}else{
				jQuery('.subNav').css('display','none');
			}
		});
		
		jQuery('.subNav li a').bind('mouseenter', function() {
			//jQuery('.subNav li').removeClass('selected');
		});
		
		var windowHeight = jQuery(window).height();
		var bodyHeight = jQuery('body').height();
		if (bodyHeight<windowHeight) {
			jQuery('#footerWrapper').addClass('fixed');
		}
		
				
		$.backstretch([
		"http://www.centromaosterra.com/image/cnt/2012/12/13/201212131645214.jpg"
		], {duration: 5000, fade: 2000});
		subNav = 0;
			
	
					jQuery(document).scroll(function(){
				var headerClass = jQuery('#headerWrapper').attr('class');
				if (window.pageYOffset>80 && headerClass!='closed') {
					$("#headerWrapper").animate({ 
						top: "-125px"
					}, 500 );
					jQuery('#headerWrapper').addClass('closed');
					}else if (window.pageYOffset<=80 && headerClass=='closed') {
					$("#headerWrapper").animate({ 
						top: "0px"
					}, 500 );
					jQuery('#headerWrapper').removeClass('closed');
				}
			});
			
				
	
	});
</script>
