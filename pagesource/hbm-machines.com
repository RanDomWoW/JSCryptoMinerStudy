        <script>
            $(function(){

                $('.transportinfo-block .btn-readmore').unbind('click.transportinfo').bind('click.transportinfo', function(e){

                    e.preventDefault();

                    var transportNumber = $(this).parents('.transportForm').find('input[name="transport[nr]"]').val();
                    var transportZipcode = $(this).parents('.transportForm').find('input[name="transport[zipcode]"]').val();
                    var error = true;

                    if (transportNumber == "") {
                        $(this).parents('.transportForm').find('.nr-error').show();
                    } else {
                        $(this).parents('.transportForm').find('.nr-error').hide();
                        error = false;
                    }
                    if (transportZipcode == "") {
                        $(this).parents('.transportForm').find('.zipcode-error').show();
                    } else {
                        $(this).parents('.transportForm').find('.zipcode-error').hide();
                        error = false;
                    }

                    if (error) {
                        return false;
                    }

                    window.open('http://www.steenbergentransport.com/tracktrace/1/90010/'+transportNumber+'/'+transportZipcode, '_blank');

                    return false;
                });

            });
        </script>
