  display: none;
}

#featured-books .control {
  position: absolute;
  z-index: 3;
  left: 0;
  top: 0;
}

img.enlarged {
  position: absolute;
  z-index: 5;
  cursor: pointer;
}

img.enlarged-control {
  position: absolute;
  z-index: 6;
  cursor: pointer;
}

.enlarged-price {
  background-color: #333;
  color: #fff;
  padding: 5px;
  font-size: 12px;
  font-weight: normal;
  position: absolute;
  z-index: 6;
}

.enlarged-price .link:hover {
 cursor: pointer;
 text-decoration:underline;
}
-->
