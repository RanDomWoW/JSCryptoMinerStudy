						]
					};
					$('#fujianMap').SVGMap({
						mapName: 'fujian', /*名字*/
						mapWidth:600,
						mapHeight: 600,
						stateColorList: ['fcd7b9', 'fcd7b9', 'fcd7b9', 'fcd7b9', 'fcd7b9', 'fcd7b9', 'fcd7b9', 'fcd7b9', 'fcd7b9', 'fcd7b9'],
