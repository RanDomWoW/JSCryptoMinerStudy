						},
						clickCallback: function (stateData, obj) {
							var strOptions = str.opt;
							for(var k in strOptions ){
								if(obj.id==strOptions[k].id){
//							console.log(strOptions[k].url);
									window.open(strOptions[k].url)
								}
							}

