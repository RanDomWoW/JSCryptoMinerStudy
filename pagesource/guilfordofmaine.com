  <script type="text/javascript">
    $(window).load(function() {
      if ($(window).width() > 768) {
        var hgt = $(document).height() - $('footer').outerHeight() - $('header.global').outerHeight();
        var contentHgt = $('#container').outerHeight();
        if (hgt > contentHgt) {
          $('#container').height(hgt);
        }
      }
    });
    $(window).resize(function() {
      if ($(window).width() < 768) {
        if ($('#container').attr('style')) {
          $('#container').css('height', 'auto');
        }
      }
      else {
        $('#container').removeAttr('style');
        hgt = $(document).height() - $('footer').outerHeight() - $('header.global').outerHeight();
        var contentHgt = $('#container').outerHeight();
        if (hgt > contentHgt) {
          $('#container').height(hgt);
        }
      }
    });
