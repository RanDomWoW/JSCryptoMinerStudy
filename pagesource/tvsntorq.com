        <script type="text/javascript">
    $(document).ready(function () {
        $('#state').change(function () {
            // var variant = $("#varient").val();
            var state = $('#state').val();

            //if (variant == "" || variant == null || typeof (variant) === 'undefined') {
            //    swal("Please select variant");
            //    return false;
            //}

            if (state == "" || state == null || typeof (state) === 'undefined') {
                swal("Please select state");
                return false;
            }
            $("#overlay").show().fadeIn(1000);
            $.ajax({
                type: "post",
                url: "Home/GetSelectedCity",
                data: { stateId: $('#state').val() },
                datatype: "json",
                traditional: true,
                success: function (data) {
                    var city = "<select id='city'>";
                    //city = city + '<option value="">Select City</option>';
                    for (var i = 0; i < data.length; i++) {
                        city = city + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
                    }
                    city = city + '</select>';
                    $('#city').html(city);

                    $("#overlay").hide().fadeOut(1000);

                    getprice($('#city').val());
                }
            });
        });
    });

    function getprice(cityid)
    {
        $("#overlay").show().fadeIn(1000);
        $.ajax({
            type: "POST",
            url: "Home/GetSelectedPrice",
            data: { cityid: cityid },
            datatype: "json",
            traditional: true,
            success: function (data) {
                $('#price').html(data.responseText);
                $("#overlay").hide().fadeOut(1000);
            },
            error: function (data) {
                $("#overlay").hide().fadeOut(1000);
            }
        });
    }
