        <script>
            $(function () {
                $("#CallBackForm").submit(function (event) {
                    event.preventDefault();

                    var userName = $('#userName').val().trim();
                    var phone = $('#phone').val().trim();
                    var email = $('#email').val().trim();
                    var m_tbCaptcha = $('#m_tbCaptcha').val().trim();
                    var comm;

                    if ($("#communication").is(":checked")) {
                        comm = "1";
                    } else { comm = "0"; }

                    if (userName == '' || userName == null || userName == "" || typeof (userName) === 'undefined')
                    { $('#userName').focus(); swal("Please enter your name"); return false; }
                    //else
                    //{
                    //    var regex = new RegExp("^[a-zA-Z ]+$");
                    //    var str = String.fromCharCode(!e.charCode ? e.which : e.charCode);
                    //    if (regex.test(str)) {
                    //        return true;
                    //    }
                    //    else {
                    //        $('#userName').focus();
                    //        swal('Please enter valid name');
                    //        return false;
                    //    }
                    //}

                    if (phone == '' || phone == null || phone == "" || typeof (phone) === 'undefined')
                    { $('#phone').focus(); swal("Please enter your phone no."); return false; }
                    else if (phone.length < 10 || phone.length > 11)
                    { $('#phone').focus(); swal("Phone no. must be 10 or 11 digits."); return false; }
                    else if (phone == "0000000000" || phone == "00000000000")
                    { $('#phone').focus(); swal("Please enter valid phone no."); return false; }

                    if (!email.trim() == '') {
                        var reg = /^[_a-z0-9-]+(\.[_a-z0-9-]+)*@[a-z0-9-]+(\.[a-z0-9-]+)*(\.[a-z]{2,4})$/;
                        if (!reg.test(email)) {
                            $('#email').focus(); swal("Please enter valid email address"); return false;
                        }
                    }

                    if (m_tbCaptcha.trim() == '' || m_tbCaptcha.trim() == null || m_tbCaptcha.trim() == "" || typeof (m_tbCaptcha) === 'undefined')
                    { $('#m_tbCaptcha').focus(); swal("Please enter captcha code"); return false; }


                    $.ajax({
                        type: "POST",
                        url: "Home/PUTCallback",
                        data: { userName: $('#userName').val(), phone: $('#phone').val(), email: $('#email').val(), communication: comm, m_tbCaptcha: $('#m_tbCaptcha').val() },
                        beforeSend: function () {
                            $("#overlay").show().fadeIn(1000);
                        },
                        datatype: "json",
                        traditional: true,
                        success: function (data) {
                            if (data.message == "Y") {
                                $('#userName').val('');
                                $('#phone').val('');
                                $('#email').val('');
                                $('#m_tbCaptcha').val('');

                                //loadCaptcha();
                                $('#callBack').modal('hide');
                                $("#overlay").hide().fadeOut(1000);

                                swal("Thank You!", "We will be in touch with you soon");
                            }
                            else {
                                $("#overlay").hide().fadeOut(1000);
                                swal(data.message);
                            }
                        },
                        error: function (data) {
                            $("#overlay").hide().fadeOut(1000);
                        }
                    });
                });
            });
        </script>

        <script type="text/javascript" language="javascript">
