<script type="text/javascript">
	$(function(){
	   
		//enable parallax - except on mobile devices
		if((!navigator.userAgent.match(/iPhone|iPad|iPod/i) && !navigator.userAgent.match(/Android/i)) && (!$("#nav-mobile").is(':visible'))){
			$.stellar({horizontalScrolling: false,hideDistantElements:false});
		}
		
		//add active class to current link in sub menu
		$('#sub-nav a').filter(function() {
		    var text = $(this).text().toLowerCase();
		    return text == 'home' || text == 'home home';
		}).addClass('active');
		
		//device toggle tabs
					$('#mac-tab').show();
			$('#mac-tabC').show();
			$('#mac-tab').removeClass('inactive');
			$('#mac-tab').addClass('active');
			$('#pc-tabC').hide();
			$('#pc-tab').addClass('inactive');
				    
		$('#device-tabs li a').click(function(){
		  var t = $(this).attr('id');
		  if($(this).hasClass('inactive')){
		    $('#device-tabs li a').addClass('inactive');           
		    $(this).removeClass('inactive');
		    
		    $('.tab-container').hide();
		    $('#'+ t + 'C').fadeIn('slow');
		    if(t=='mac-tab'){
				ga('send', 'event', 'OS Tabs', 'Mac Change', 'Change Toggle to Mac');
		    }else if(t=='pc-tab'){
				ga('send', 'event', 'OS Tabs', 'PC Change', 'Change Toggle to PC');
		    }
		 }
		});
		$('#toggle-tabs li a').click(function(){
		  var t = $(this).attr('id');
		  if($(this).hasClass('inactive')){
		    $('#toggle-tabs li a').addClass('inactive');           
		    $(this).removeClass('inactive');
		    
		    $('.tab-container').hide();
		    $('#'+ t + 'C').fadeIn('slow');
		    if(t=='mac-tab'){
				ga('send', 'event', 'OS Tabs', 'Mac Change', 'Change Toggle to Mac');
		    }else if(t=='pc-tab'){
				ga('send', 'event', 'OS Tabs', 'PC Change', 'Change Toggle to PC');
		    }
		 }
		});
		if($(".navigation").length > 0) {
			if( !$('#mobile-nav').is(":visible") ){
				var header = $('.product-nav');
				$(window).scroll(function(e){
					if($(window).scrollTop() > 55){
						header.addClass("fix-header");
					}else{
						header.removeClass("fix-header");
					}
				});
			}
		}
		$('#view-all-videos').click(function(){
			$('#more-videos').slideDown();
			$('#view-all-videos').hide();
		});
		$('#view-all-case-studies').click(function(){
			$('#more-case-studies').slideDown();
			$('#view-all-case-studies').hide();
		});
	});
</script>
