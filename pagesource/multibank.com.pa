	<script type="text/javascript">
		function popup(mylink, windowname) {
			if (!window.focus) return true;
			var href;
			if (typeof mylink == "string") href = mylink;
			else href = mylink.href;
			if (windowname == "BannerTasaTarjetas") {
				newwindow = window.open(
					href,
					windowname,
					"width=605,height=1018,scrollbars=yes"
				);
				newwindow.onload = changeBGc;
			} else {
				newwindow = window.open(
					href,
					windowname,
					"width=800,height=700,scrollbars=yes"
				);
				newwindow.onload = changeBGc;
			}
			return false;
		}

		function changeBGc() {
			newwindow.document.getElementsByTagName("body")[0].style.backgroundImage =
				"none";
			newwindow.document.getElementsByTagName("body")[0].style.backgroundColor =
				"white";
		}
	</SCRIPT>
