	<script type="text/javascript">
		function createGoogTransCookie() {
			var cookieGoogTransPermanent = readCookie("googtransp");
			if (cookieGoogTransPermanent != null) {
				createCookie("googtrans", cookieGoogTransPermanent);
			}
		}

		function googleTranslateElementInit() {
			new google.translate.TranslateElement({pageLanguage: 'en',
				layout: google.translate.TranslateElement.InlineLayout.SIMPLE,
				includedLanguages: 'en,fr,zh-CN,es,pt'
			}, 'hiddenGoogleTranslate');

			$(document).ready(function () {
				$(document).click(function (event) {
					var ev = event || window.event;
					var target = ev.target || ev.srcElement;

					if ($(target).parents('#google_translate_element').length == 0) {
						$('.choiceLanguages').hide();
					}
				});

				if (readCookie("languageChangeFlag")) {
					deleteCookie("languageChangeFlag");

					if (readCookie("googtransp") != "/en/en") {
						$('#dialog-change-language').showDialog();
					}
				}

				$(".goog-te-gadget-simple,.goog-te-gadget > ul").click(function () {
					$(".choiceLanguages").toggle();
				});

				$(".choiceLanguages > li").click(function () {
					var languageCode = $("a", this).attr("id").substring("languageCode".length);

					location.hash = "#googtrans/en/" + languageCode;

					createCookie("googtrans", "/en/" + languageCode);
					createCookie("googtransp", "/en/" + languageCode, 365);
					createCookie("languageChangeFlag", "true");

					location.reload();
				});
			});
		}

		createGoogTransCookie();
	</script>
