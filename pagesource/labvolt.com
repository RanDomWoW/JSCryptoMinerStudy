					$("div.home-slider-wrapper div#navigation-slide div#previous-slide").bind("click touchstart", function (e) {
						$('#lvSlider').trigger('previousSlide');
						e.preventDefault();
					});
					$("div.home-slider-wrapper div#navigation-slide div#next-slide").bind("click touchstart", function (e) {
						$('#lvSlider').trigger('nextSlide');
						e.preventDefault();
					});
				});

				$(document).ready(function () {
					listenForHtmlValueChange(".lvSliderLearnMoreInnerAnchor", false, function () {
						$('.lvSliderLearnMore').css("right", parseFloat($(".lvSliderBarreDroite").css("width")) + 20 + $('.lvSliderLearnMoreInner').width());
					});

					fixLeftBorderOfProductCategories();
				});

				$(window).resize(function() {
					fixLeftBorderOfProductCategories();
				});

				function fixLeftBorderOfProductCategories() {
					if ($(window).width() >= 943) {
						$("div.product-categories").height($("div.whatwedo-intro").height() + parseInt($("div.whatwedo-intro").css("margin-bottom")));
					} else {
						$("div.product-categories").css("height", "auto");
					}

					if ($("div.product-categories").height() < 580) {
						$("div.product-categories").height(580);
					}
				}
			</script></body>
</html>