		<script type="text/javascript">

		jQuery('form.dealer-locator').submit( function( event ) {

			var url, mfg, zip, miles;

			event.preventDefault();
			mfg = jQuery(this).find("select[name='manufacturer']").val();
			zip = jQuery(this).find("input[name='zipcode']").val();
			miles = jQuery(this).find("select[name='miles']").val();

			if( zip && ( isNaN(zip) || zip.length != 5 ) ) {
				alert('Use a zipcode with only five digits');
				return;
			}

			if(mfg && zip) {
				url = '/dealer-locator/'+zip+'-'+mfg+'-dealers/';
			} else if( mfg ) {
				url = '/dealer-locator/'+mfg+'-dealers/';
			} else if( zip ) {
				url = '/dealer-locator/'+zip+'/';
			}

			if( url && miles ) {
				url = url + '?miles='+miles;
			}

			if( url ) {
				location.href = url;
			} else {
				alert('Enter a zipcode or select a brand.')
			}
		});

		</script>
