<script type="text/javascript">
    function SetNotify(){
        title = $('title').html().replace(/\(*([^<]*)\) /, '');
        if($('.notify').length > 0){
            $('title').html(
                '(' + $('.notify').html() + ') ' + title
            );
        }else{
            $('title').html(title);
        }
    }

    SetNotify();

    function MarkRead(e){
        $(e).parent().removeClass('unread');
        window.location = $(e).find('a').attr('href');
        // $(e).remove();
        // SetNotify();
    }

    function MarkAllRead(e){
        $(e).nextAll("div").removeClass('unread');
        $(e).nextAll("div").find(".MarkRead").remove();
        $(e).remove();
        SetNotify();
    }

    function notifyByClick(e) {
        clearInterval(interval);
        var notifyNextLink = $(e).attr('href');
        CallSignal(notifyNextLink,false,SetNotify);
        interval = setInterval(function(){
            CallSignal(notifyNextLink,false,SetNotify);
        },10000);
        return false;
    }

    function notifyMessagesByClick(e) {
        clearInterval(messInterval);
        var messageNextLink = $(e).attr('href');
        CallSignal(messageNextLink,false,SetNotify);
        messInterval = setInterval(function(){
            CallSignal(messageNextLink,false,SetNotify);
        },10000);
        return false;
    }
</script>
