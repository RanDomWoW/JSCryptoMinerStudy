    <script type="text/javascript">
		$(document).ready(function(){
			var toggler = $('#pop'),
				togglerOriginalPosition = toggler.css('position'),
				togglerOriginalBottom = toggler.css('bottom'),
				footer = $('#prolinkftr'),
				scrollBreakPoint;

			//scroll
			var fixPosition = function() {
				scrollBreakPoint = $(document).height() - $(window).height() - footer.innerHeight();

				if ($(window).scrollTop() > scrollBreakPoint) {
					toggler.css({
						position: 'absolute',
						top: 'auto',
						bottom: -26
					});
				} else {
					toggler.css({
						position: togglerOriginalPosition,
						bottom: togglerOriginalBottom,
						top: 'auto'
					});
				}
			}

		fixPosition();
		$(window).bind("scroll", fixPosition);
        nette.load = function () {
            for (var i = 0; i < document.forms.length; i++) {
                nette.initForm(document.forms[i]);
            }
        };
        });
        $(document).load(function() {
            // fancybox
            jQuery.extend(jQuery.fn.fancybox.defaults, {
                centerOnScroll: true,
                overlayColor: '#000',
                overlayOpacity: 0.65,
                autoScale: false,
                showCloseButton: false,
                width:800,
                height:600,
                titleShow: false,
                type:'iframe',
                onStart: function(){
                    $("#pop").addClass("out");
                },
                onClosed: function(){
                    $("#pop").removeClass("out");
                    // rebind scroll here 2
                    $(window).bind("scroll", fixPosition);
                }
            });

            $("a#pop").fancybox({
                onComplete: function(){
                    $("#fancybox-wrap").addClass("fancy-soutez");
                    $("#pop").unbind("click").attr("id", "popout");
                    $("#popout").click(function() {
                        $.fancybox.close();
                        return false;
                    }).attr("id", "pop");
                },
                onClosed:function(){
                    $("#pop").removeClass("out").fancybox();
                    // rebind scroll here 1
                    $(window).bind("scroll", fixPosition);
                }
            });
        });
    </script>
