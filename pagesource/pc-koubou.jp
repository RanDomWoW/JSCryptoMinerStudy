$(function(){
    var sessioncheck;
    setTimeout(function() {
        return sessioncheck();
    },200);
    return sessioncheck = function() {
        var url = '/ajax/init.php';
        var token;
        $.ajax({
                type: 'GET',
                url: url,
                success: function(data) {
                       if (data == null) {
                           data = {token: '', name: ''};
                       }
                       getUserStatus(data.token, data.name);
                    },
                dataType:'json'
            });
    }
});

function getUserStatus(token, tname) {
    if (token.length > 0) {
        var url = '/ajax/user.php';
        var params = {};
        params[tname] = token;
        $.ajax({
                type: 'POST',
                url: url,
                data: params,
                success: function(data) {
                        setUserStatus(data);
                    },
                dataType:'json'
                });
    }
}
function setUserStatus(data) {
    
    if (data.islogin == 0 || data.islogin == null) {
        $('#navi-header-list-menu').hide();

    } else {
        $('#navi-header-login').hide();
        $('#navi-header-entry').hide();
        $('#navi-header-list-menu').show();
        var name;
        name = data.name + 'さん ';
        if (data.iscorp) {
            name = name + '<span class="badge-x">法人</span>';
        }
        $('#navi-header-name').html(name);
    }
    if (data.cartquantity > 0) {    
        $('#navi-header-cart-count').text(data.cartquantity);
        $('#navi-header-cart-num').text(data.cartquantity);
        $('#navi-header-cart-total').html('&yen;' + data.carttotal + '(税別)');
    } else {
        $('#navi-header-cart-count').text('0');
        $('#navi-header-cart-num').text('0');
        $('#navi-header-cart-total').html('&yen;0(税別)');
    }
}
