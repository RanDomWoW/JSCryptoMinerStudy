 else if (top > fixedBannerOffsetTop && $('#tiser').data('fixed')=='fixed') {
 $('#tiser').css({'position':'fixed', 'top':'0'});
 } else {
 fixedBannerOffsetTop = $('#tiser').offset().top;
 $('#tiser').css({'position':'relative'});
 }
 });
