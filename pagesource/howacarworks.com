<script>
  var current_frame = 0;
  var total_frames = 90;
  var paths = document.querySelectorAll('.__ca_draw path');



  document.querySelector('.car-drawing').classList.add('is-drawing');

  var lengths = new Array();

  for(var i=0; i<paths.length; i++) {
    var l = paths[i].getTotalLength();
    lengths[i] = l;
    paths[i].style.strokeDasharray = l + ' ' + l;
    paths[i].style.strokeDashoffset = l;
  }
var handle = 0;

var draw = function() {
  var progress = current_frame/total_frames;
  if (progress > 1) {
    window.cancelAnimationFrame(handle);
    document.querySelector('.car-drawing').classList.add('is-drawn');
  } else {
    current_frame++;
    for(var j=0; j<paths.length;j++){
      paths[j].style.strokeDashoffset = Math.floor(lengths[j] * (1 - progress));
    }
    handle = window.requestAnimationFrame(draw);
  }
};
draw();
</script>


