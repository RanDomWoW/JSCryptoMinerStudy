h2 {font-size: 60px; margin: 0px 0px 0px 0px ; padding: 0px 0px 0px 0px;}
h3 {font-size: 48px; margin: 0px 0px 0px 0px ; padding: 0px 0px 0px 0px;}
h4 {font-size: 36px; margin: 0px 0px 0px 0px ; padding: 0px 0px 0px 0px;}
h5 {font-size: 31px; margin: 0px 0px 0px 0px ; padding: 0px 0px 0px 0px;}
