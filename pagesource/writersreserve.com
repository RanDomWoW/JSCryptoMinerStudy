/* */

/* no fonts ligatures */
body {
  font-variant-ligatures: none;
  -moz-font-feature-settings: "liga" 0, "clig" 0;
  -webkit-font-feature-settings: "liga" 0, "clig" 0;
  -ms-font-feature-settings: "liga" 0, "clig" 0;
}

.x-colophon.top, .x-colophon.bottom {
  border-top: rgba(255, 255, 255, 0.5);
  background-color: rgba(255, 255, 255, 0.5);
  box-shadow: inset 0px 8px 0px 0px rgba(255,255,255,0.8);
}
.x-colophon.bottom {
    margin-top: 800px;
}
.entry-wrap {
  padding-top: 3% !important;
  padding-bottom: 3% !important;
  padding-left: 4% !important;
  padding-right: 4% !important;
  margin-top: 5px !important;
  text-align: center;
  background-color: rgba(255, 255, 255, 0.9)!important;
}
.masthead {
  min-height: 0px;
  background-color: rgba(255, 255, 255, 0);
}
.x-topbar {
  display: none;
}
.x-topbar-inner {
}
.x-logobar {
  background-color: rgba(255, 255, 255, 0.8);
}
.x-logobar-inner {
}
body .x-entry-share { border: 0px !important; }
/* MAPS */
a[href^="http://maps.google.com/maps"]{display: none !important}
a[href^="https://maps.google.com/maps"]{display: none !important}
.gmnoprint a, .gmnoprint span, .gm-style-cc {
    display: none;
}
.gmnoprint div {
    background: none !important;
}
/*Submit Button*/
.x-btn, .button, [type='submit'] {
  color: rgba(0, 0, 0, 1) !important;
  padding: 0.6em 1em 0.6em 1em;
  margin: 2px 2px 2px 2px;
  border-radius: 5px;
  letter-spacing: 1px;
  background: rgba(255, 255, 255, 1) !important;
  border: 1px solid rgba(142, 98, 21, 1);
  opacity: 0.4;
  transition: all 500ms ease-in-out 0s !important;
  width: 80px;
}
.x-btn:hover, .button:hover, [type='submit']:hover {
  color: rgba(242, 98, 21, 1) !important;
  opacity: 0.95;
}
.x-hr {
  border-top-width: 1px;
  border-top-style: solid;
  border-top-color: rgba(39,39,39,0.2);
}
/*media all*/
table th, table td {
    border-top: none;
}
/* FOOTER */
.tooltip-arrow {
  background-color: transparent !important;
  border-color: transparent !important;
}
.tooltip-inner {
  background-color: rgba(142, 98, 21, 0.9) !important;
  color: rgba(255, 255, 255, 1) !important;
}
.x-entry-share {
  margin: 0;
  padding: 0;
}
.x-entry-share > p {
  display: none;
}
