    function entersearch(){

        var event = window.event || arguments.callee.caller.arguments[0];
        if (event.keyCode == 13)
        {
            var search =  $("input[name='all_search']").val();
            if(search != ""){
                location.href = "http://p.uthing.cn/search?q="+search;
            }
        }
    }
 function init(){
     if ($.trim($('#login_info').val()) == '') {
         $('#login_info').val($('#login_info').attr('defvalue'));
     }
     if ($.trim($('#password').val()) == '') {
         $('#password').val($('#password').attr('defvalue'));
     }
 }
 function checkLogin() {
     var isTrue = true;
     var login_info = $.trim($('#login_info').val());
     if (login_info == '' || login_info == $('#login_info').attr('defvalue')) {
         $("#user_msg").show();
         $("#user_msg").text("请填写手机号或邮箱");
         isTrue = false;
         return isTrue;
