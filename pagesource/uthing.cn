<script type="text/javascript">
    function float_nav(dom) {
        var right_nav = $(dom);
        var nav_height = right_nav.height();

        function right_nav_position(bool) {
            var window_height = $(window).height();
            var nav_top = (window_height - nav_height);
            if (bool) {
                right_nav.stop(true, false).animate({top: nav_top + $(window).scrollTop()}, 400);
            } else {
                right_nav.stop(true, false).animate({top: nav_top}, 300);
            }
            right_nav.show();
        }

        if (!+'\v1' && !window.XMLHttpRequest) {
            $(window).bind('scroll resize', function () {
                if ($(window).scrollTop() > 480) {
                    right_nav_position(true);
                } else {
                    right_nav.hide();
                }
            })
        } else {
            $(window).bind('scroll resize', function () {
                if ($(window).scrollTop() > 480) {
                    right_nav_position();
                } else {
                    right_nav.hide();
                }
            })
        }
    }

    float_nav('.newBackbox');

    //验证码
    $("._jsrefucaptcha").on("click", function () {
        update_verify();
    });

    update_verify();
    //更新验证码图片
    function update_verify() {
        //更换图片验证码
        jQuery("#refucaptcha").attr("src", function () {
            var url = "./ajax.php?action=captcha";
            var t = Math.round(new Date().getTime());
            return url + "&t=" + t;
        });
    }

    //咨询
    var z = 0;
    $(".zixun").on("click", function () {
        if (z == 1) {
            window.clearInterval(interval_tel);
            clearTimeout(interval_tel);
            $("#djs_tel").attr("disabled", false);
            $('#djs_tel').html('发送');
            $("#djs_tel").bind("click", function () {
                telSecond = 60;
                send_tel();
            });
        }
        $("input[name='verify']").val("");
        $(".zixun_name").parents(".clearfix").find(".jy").hide();
        $(".zixun_content").parents(".clearfix").find(".jy").hide();
        $(".zixun_telephone").parents(".clearfix").find(".jy").hide();
        //openMaster('.new_zixun');
    });

    $(".zx_offbtn").on("click", function () {
        //openMaster('.new_zixun');
    });
    $(".close_productfrom").on("click", function () {
        openMaster('.product_from');
    });
    //咨询提交
    $(".zixun_submit").on("click", function () {
        var name = $("input[name='name']").val();
        var content = $("textarea[name='content']").val();
        var telephone = $("input[name='telephone']").val();
        var code = $("input[name='code']").val();
        var hash = $("input[name='hash']").val();

        if (add_check(name, content, telephone) && check_code()) {
            $.ajax({
                type: 'post',
                url: './index.php?action=newconsult',
                data: {"name": name, "content": content, "telephone": telephone, "code": code, "hash": hash},
                dataType: 'json',
                success: function (data) {
                    if (data.status == 'success') {
                        openMaster('.new_zixun');
                        openMaster('.zxtcbox_cg');
                        $("input[name='name']").val('');
                        $("textarea[name='content']").val('');
                        $("input[name='telephone']").val('');
                        $("input[name='code']").val('');
                        $("input[name='hash']").val('');
                    } else {
                        $(".zixun_error").html(data.msg).show();
                    }
                }
            });
        }
    });
    //保存成功后的提示
    $(".success_save").on("click", function () {
        openMaster('.zxtcbox_cg');
    });
    //短信发送
    function send_tel() {

        //发送验证码前验证
        var verify = $("[name='verify']").val();
        var return_status = 0;
        if (verify == '') {
            $(".zixun_error").show();
            $(".zixun_error").html("请输入图片验证码");
            return false;
        }
        $.ajax({
            url: './ajax.php?action=checkverify',
            type: 'GET',
            data: {verify: verify},
            async: false,
            success: function (data) {
                if (data == 'true') {
                    $(".zixun_error").hide();
                    $(".zixun_error").html("");
                } else {
                    $(".zixun_error").show();
                    $(".zixun_error").html("图片验证码不正确");
                    update_verify();
                    return_status = 1;
                }
            }
        });
        if (return_status == 1) {
            return false;
        }

        var re = /^1\d{10}$/;
        var phone = $("input[name='telephone']").val();
        $("input[name='hash']").val('');
        phone = phone.replace(/(^\s*)|(\s*$)/g, "");
        if (phone == "") {
            $(".zixun_error").html('请输入正确的手机号！').show();
        } else if (re.test(phone)) {
            $(".zixun_error").html('').hide();
            $.ajax({
                url: './ajax.php?action=pcSendsms',
                type: 'POST',
                data: {phone: phone, type: 1, verify: verify},
                success: function (ret) {
                    //$("input[name='hash']").val(ret);
                    var data = eval("(" + ret + ")");
                    if (data.error_code >= 500) {
                        $(".zixun_error").show();
                        $(".zixun_error").html(data.msg);
                    } else {
                        run_tel();
                        $("input[name='hash']").val(data.hash);
                        $(".zixun_error").hide();
                        $(".zixun_error").html('');
                        update_verify();
                    }
                }
            });
        } else {
            $(".zixun_error").html('请输入正确的手机号！').show();
        }
    }
    var interval_tel;

    function run_tel() {
        interval_tel = setInterval(chat_tel, "1000");
    }
    function chat_tel() {
        if (telSecond == 0) {
            $("#djs_tel").attr("disabled", false);
            $('#djs_tel').html('发送');
            $("#djs_tel").bind("click", function () {
                telSecond = 60;
                send_tel();
            });
            clearTimeout(interval_tel);
        } else {
            $('#djs_tel').html(telSecond + 's');
            $("#djs_tel").attr("disabled", true);
            $("#djs_tel").unbind("click");
            telSecond--;
        }
    }

    var v = false;
    //验证码校验
    var check_code = function () {

        var phone = $("input[name='telephone']").val();
        phone = phone.replace(/(^\s*)|(\s*$)/g, "");
        var code = $("input[name='code']").val();
        var hash = $("input[name='hash']").val();
        $.ajax({
            url: './ajax.php?action=ckcode',
            type: 'POST',
            data: {telephone: phone, hash: hash, code: code},
            async: false,
            dataType: "json",
            success: function (data) {
                if (data.result == true) {
                    //验证码正确
                    v = true;
                } else {
                    //验证码错误
                    $(".zixun_error").html('验证码错误！').show();
                    v = false;
                }
            }
        });

        return v;

    }
    var telSecond = 60;
    //验证码发送
    if (z == 0) {
        $(".yzm_btn_blue").on("click", function () {
            z = 1;
            telSecond = 60;
            send_tel();
        });

        //定制弹框
        $(".my_ding").on("click", function () {
            openMaster('.product_from');
        })
    }

    // 检测表单是否合法
    function add_check(name, content, telephone) {
        $(".zixun_error").html('').hide();
        var flag = true;
        if (!name) {
            $(".zixun_name").parents(".clearfix").find(".jy").show();
            flag = false;
        } else {
            $(".zixun_name").parents(".clearfix").find(".jy").hide();
        }
        if (!content) {
            $(".zixun_content").parents(".clearfix").find(".jy").show();
            flag = false;
        } else {
            $(".zixun_content").parents(".clearfix").find(".jy").hide();
        }
        if (!telephone) {
            $(".zixun_telephone").parents(".clearfix").find(".jy").show();
            flag = false;
        } else {
            $(".zixun_telephone").parents(".clearfix").find(".jy").hide();
//            var reg = /^(((13[0-9]{1})|(15[0-9]{1})|(18[0-9]{1}))+\d{8})$/;
//            if (!reg.test(telephone)) {
//                $(".zixun_error").html('请输入有效的手机号码！').show();
//                flag = false;
//
//            } else {
//                $(".zixun_error").html('请输入有效的手机号码！').hide();
//            }
        }

        return flag;
    }
</script>


