                <div class="statusjson booking-status-json parbase">













    
    
        <script type="text/javascript">
            bookingStatusJson='{}';
            var dateBookingStatus = new Date();
            var urlBookingStatus = '/content/fourseasons/en/status/_jcr_content/statusjson.json-out.html' + '?c=t&ts=' + dateBookingStatus.getTime().toString();
            $.ajax({
                dataType: 'text',
                async: true,
                url: urlBookingStatus,
                success: function(data){
                	bookingStatusJson=data;
                    var booker = $('.mod-booker');
                    if (typeof(booker) !== 'undefined') {
                        booker.each(function() {
                            if (typeof($(this).data('Booker')) !== 'undefined') {
                                $(this).data('Booker').adjustBookingEngineStatus();
                            }
                        });
                    }
                }
            });
        </script>
    


</div>

            
        
    



    















    
    
        
            
            
                
                <div class="cawjson caw-availability-json parbase">















    
    
        <script type="text/javascript">
            availabilityJson='{}';
            var dateCawAvail = new Date();
            var urlCawAvail = '/content/fourseasons/en/calendar_availability/_jcr_content/cawjson.json-out.html' + '?c=t&ts=' + dateCawAvail.getTime().toString();
            $.ajax({
                dataType: 'text',
                async: true,
                url: urlCawAvail,
                success: function(data){
                    availabilityJson=data;
                    var booker = $('.mod-booker');
                    if (typeof(booker) !== 'undefined') {
                        booker.each(function() {
                            if (typeof($(this).data('Booker')) !== 'undefined') {
                                $(this).data('Booker').adjustAvailability();
                            }
                        });
                    }
                    var offerLandingList = $('.mod-expander');
                    if (typeof(offerLandingList) !== 'undefined') {
                        offerLandingList.each(function() {
                            if (typeof($(this).data('OfferLandingList')) !== 'undefined') {
                                $(this).data('OfferLandingList').hideDates();
                            }
                        });
                    }
                    var offerDatesOffered = $('.mod-offer-dates-offered');
                    if (typeof(offerDatesOffered) !== 'undefined') {
                        offerDatesOffered.each(function() {
                            $(this).trigger('hideDates');
                        });
                    }
                }
            });
        </script>
    


</div>

            
        
    




    


