<script type="text/javascript">
function SubmitSearch(searchfor, cityid, cityname) {
	$('#loading_proc').show();
	$("#loading_proc").html('<img src="https://static.realestateindia.com/rei/images/loading.gif" alt="Please Wait..." title="Please Wait..."/>');
	var cityid_str;
	var cityname_str;
	if(cityid!='') {
		cityid_str = cityid;
	}
	else {
		cityid_str = '';
	}
	if(cityname!='') {
		cityname_str = cityname;
	}
	else {
		cityname_str = '';
	}
	$.ajax({
		type : 'POST',
		url  : "https://www.realestateindia.com/search_property_project_service.php",
		dataType: "html",
		cache : false,
		data: { action_type:"search_from", searchfor:searchfor, ref_cityid:cityid_str, ref_cityname:cityname_str},
		success :  function(msg) {
			$("#loading_proc").html('');
			if(msg!='') {
				$('#searchtype').html('');
				$('#searchtype').html(msg);
			}
		}
	});
	//return true;
}
</script>
