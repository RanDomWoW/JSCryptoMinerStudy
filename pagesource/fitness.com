<script>
    $(function () {
        $('#featuredPoll').on('submit', function (e) {
            e.preventDefault();
            $.ajax({
                type: 'post',
                url: 'https://www.fitness.com/poll/vote',
                data: $('#featuredPoll').serialize(),
                success: function (data) {
                    if(data.status == false) {
                        $('.poll-error').html(data.message);
                        $('.poll-error').removeClass('hidden');
                    } else {
                        $('#poll-details-featured').html(data.html);
                    }
                }
            });
        });
    });

    $(function () {
        $('#featuredPoll-result').on('click', function (e) {
            e.preventDefault();
            $.ajax({
