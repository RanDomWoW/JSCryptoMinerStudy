            <script type="text/javascript">
                var amp = String.fromCharCode(38);

                // Lädt weitere Suchergebnisse per Ajax
                function getNewListElements() {

                    //Loading Gif einblenden
                    $("#footer_loading_gif").show();

                    var oldConfUrl = phnservice.getCurrentUrlConfig();
                    var newConfUrl = '';

                    // Entfernt vor dem Nachladen der nächsten Ergebnisse eine
                    // evtl. vorhandene Anzahl nachzuladender Items
                    newConfUrl = getUrlWithoutParam(oldConfUrl, "count");
                    var url = phnservice.getCurrentUrlBase("listajax") + phnservice.getCurrentUrlLocation() + newConfUrl + phnservice.getCurrentUrlSort() + "start=" + searchIterator;
                    // Die neue ajaxCallbackAppendSync Funktion wird aufgerufen für einen synchronen Ajax Call, da die Anzahl ab welcher Zahl die nächsten PEs nachgeladen werden,
                    // berechnet wird im Ajax Call und wenn vorherige Aufruf nicht beendet ist, hat der nächste den falschen Wert.
                    // Dies hat vorher dazu geführt, dass immer die selben PEs auf der nächsten Seite geladen wurden.
                    phnservice.ajaxCallbackAppendSync(url, afterAjaxNextResults);
                }


                // Fügt an die Ergebnislisten per Ajax weitere Suchergebnisse hinzu und zeichnet diese auf die Karte
                function appendNewListElements(resultList, mapList) {
                    // Der Seitentrenner sollte ganzzahlig sein. Vorher wurde alles geladen da hat es funktioniert, aber der Wert des Endes wird jetzt berechnet und das kann dazu führen das die Division keine Integer ergibt.
                    $("#searchresult_list_content").append("<" + "div class='searchresult_list_item'><" + "div class='searchresult_list_seperator'><" + "h4>Seite " + ( Math.ceil(searchIterator / resultsPerPage) + 1) + "<" + "/h4><" + "/div><" + "/div>");
                    
                    // Neue Ergbenisse
                    $("#searchresult_list_content").append(resultList);
                    $("#searchresult_map_items_wrapper").append(mapList);

                    // Einzeichnen in Karte
                    document.getElementById("iframe_map").contentWindow.addCareInstitutionsToMap(searchresultArray, true);

                    // Darstellung der angezeigten Ergebnisse ändern
                    searchIterator += resultsPerPage;
                    if (searchIterator < totalResults) {
                        $("#pflegeheim_counter_shown").html(searchIterator);
                    } else {
                        $("#pflegeheim_counter_shown").html(totalResults);
                    }
                }

                // Funktionen für die Popup-Box der Sortierung nach Einzelnoten
                var defaultPopupMaxHeight = 700;
                var defaultPopupMaxWidth = 1000;
                var popupMaxHeight = defaultPopupMaxHeight;
                var popupMaxWidth = defaultPopupMaxWidth;

                function calcPopupDimensions() {
                    var currHeight = $(window).height();
                    var currWidth = $(window).width();

                    if (currHeight < 790) {
                        popupMaxHeight = currHeight - 90;
                    } else {
                        popupMaxHeight = defaultPopupMaxHeight;
                    }

                    if (currWidth < 1230) {
                        popupMaxWidth = currWidth - 230;
                    } else {
                        popupMaxWidth = defaultPopupMaxWidth;
                    }
                }

                function resizeSortboxSmallSort() {
                    $("#sort").css("height", "300px");
                    TINY.box.size(570,300,1,0);

                    $(".row_qualitaetsbereich").css("width", "522px");
                    $(".row_qualitaetsbereich label").css("width", "488px");
                    $(".subsorts label").css("width", "488px");
                }

                function resizeSortboxSmallSelect() {
                    $("#select").css("height", "420px");
                    TINY.box.size(570,420,1,0);

                    $(".row_qualitaetsbereich").css("width", "522px");
                    $(".row_qualitaetsbereich label").css("width", "488px");
                    $(".subsorts label").css("width", "488px");
                    $("#preselection_inner_right").css("width", "405px");
                }

                function resizeSortboxBig() {
                    TINY.box.size(popupMaxWidth,popupMaxHeight,1,0);
                    $(".row_qualitaetsbereich").css("width", (popupMaxWidth*0.9) + "px");
                    $(".row_qualitaetsbereich label").css("width", ((popupMaxWidth*0.9) - 34) + "px");
                    $(".subsorts label").css("width", (popupMaxWidth*0.902) + "px");
                    $(".subsorts label:last-child").css("border-bottom", "1px solid #E3E3E3");
                    $(".subselects label").css("width", (popupMaxWidth*0.902) + "px");
                    $(".subselects label:last-child").css("border-bottom", "1px solid #E3E3E3");
                    $("#preselection_inner_right").css("width", (820 - (defaultPopupMaxWidth - popupMaxWidth)) + "px");
                    $("#sort").css("height", popupMaxHeight + "px");
                    $("#select").css("height", popupMaxHeight + "px");
                }

                function openPreselectedSort() {
                    // Oberkategorien der vorausgewählten Sortierungen öffnen und ggfl. Popup vergrößern
                    var subContainerOpen = false;
                    var sortId = $("input[name='extSearchSortierungSort']:checked").attr('id');
                    if (sortId.indexOf("sub") != -1) {
                        $("#" + sortId).parent().show();
                        subContainerOpen = true;
                    }

                    if (subContainerOpen) {
                        resizeSortboxBig();
                    } else {
                        resizeSortboxSmallSort();
                    }
                }

                function openPreselectedSelect() {
                    // Oberkategorien der vorausgewählten Auswahlen öffnen und ggfl. Popup vergrößern
                    var subContainerOpen = false;
                    $("input[name='extSearchSortierungAuswahl']:checked").each(function() {
                        var selectId = $(this).attr('id');
                        if (selectId.indexOf("sub") != -1) {
                            $("#" + selectId).parent().show();
                            subContainerOpen = true;
                        }
                    });

                    if (subContainerOpen) {
                        resizeSortboxBig();
                    } else {
                        resizeSortboxSmallSelect();
                    }
                }

                // Öffnet die Popup-Box
                function openSortbox(module, version) {
                    //Berechnung der Max.-Größen für das Einzelnoten-Sort-Popup
                    calcPopupDimensions();

                    if ($(window).height() < 500 || $(window).width() < 900) {
                        alert("Ihr Browser-Fenster ist leider zu klein, um die Sortierung nach " +
                              "Qualitätsbereichen und / oder Einzelkriterien korrekt darzustellen.\n\n" +
                              "Um diese Funktion nutzen zu können, maximieren Sie ihr Browser-Fenster " +
                              "oder stellen Ihre Bildschirm-Auflösung höher ein.");
                        $("#distance").attr('checked', true);
                        return false;
                    }

                    /*
                        Standard ist pflegekarte_sort.php
                        Wird die Funktion "openSortbox" mit dem Parameter
                        version=2009 aufgerufen so wird die Datei pflegekarte_sort_2009.php
                        für die sortbox verwendet. Kann später, wenn nicht mehr
                        in Verwendung, entfernt werden.
                    */
                    var sortboxFile = "pflegekarte_sort.php";
                    if (version == "2014") {
                        sortboxFile = "pflegekarte_sort_2014.php";
                    } else if (version == "2009") {
                        sortboxFile = "pflegekarte_sort_2009.php";
                    }

                    TINY.box.show({url: sortboxFile + '?module=' + module,width:570,height:300,openjs:function(){
                        // Letzte Sortierung vorauswählen
                        var subContainerOpen = false;
                        var lastSort = $("#extSearchSortierungSort").val();
                        var lastTbVersion = $("#extSearchTBVersion").val();
                        if (lastSort != null && lastSort != "" && version == lastTbVersion) {
                            $("#" + lastSort).attr('checked', true);
                            if (lastSort.indexOf("sub") != -1) {
                                $("#" + lastSort).parent().show();
                                subContainerOpen = true;
                            }
                        }
                        if (subContainerOpen) {
                            resizeSortboxBig();
                        } else {
                            resizeSortboxSmallSort();
                        }

                        // Letzte Notenauswahl vorauswählen
                        var lastSelect = $("#extSearchSortierungSelect").val();
                        if (lastSelect != null && lastSelect != "" && version == lastTbVersion) {
                            var lastSelectArray = lastSelect.split(",");
                            for (var i = 0; i < lastSelectArray.length; i++) {
                                $("#" + lastSelectArray[i]).attr('checked', true);
                            }
                        }

                        $('#sort').buttonset();
                        $('#select').buttonset();

                        // Erzeugt aus den Title-Attributen der jeweiligen elemente Qtips
                        if (window.isBrowserIE6 === false) {
                            $('.qtip_trigger').qtip({
                                position: {
                                    // Position relativ zum Tooltip (Spitze)
                                    my: 'bottom center',
                                    // Position relativ zum Link
                                    at: 'top center',
                                    // Tooltip immer im sichtbaren Bereiche anzeigen
                                    viewport: $(window)
                                },
                                style: 'ui-tooltip-sortbox qtip-shadow qtip-rounded ui-tooltip-wide'
                            });
                        }

                        // Button "Seite 2" anzeigen wenn Auswahl auf Seite 1 getroffen
                        showButtonSeite2();
                    }});
                }

                // Öffnet/Schließt die Fragen-Container der Qualitätsbereiche
                function toggleSubContainers(subcontainerId, subWrapper) {
                    if ($("#" + subcontainerId).is(":visible")) {
                        $("#" + subcontainerId).slideUp('fast', function() {
                            if (!$(subWrapper).is(":visible")) {
                                if (subWrapper.indexOf("subsorts") != -1) {
                                    resizeSortboxSmallSort();
                                } else {
                                    resizeSortboxSmallSelect();
                                }
                            }});

                    } else {
                        if (!$(subWrapper).is(":visible")) {
                            resizeSortboxBig();
                            setTimeout(function() {$("#" + subcontainerId).slideDown('fast');}, 280);
                        } else {
                            $("#" + subcontainerId).slideDown('fast');
                        }
                    }
                }

                // Anzeige der Seite 2 der Sortbox (Auswahl der zusätzlich angezeigten Noten)
                function showSelectsForm() {
                    // Überträgt das Label der ausgewählten Sortierung auf die zweite Seite
                    var sortId = $("input[name='extSearchSortierungSort']:checked").attr('id');
                    if (sortId.indexOf("main") != -1) {
                        $(".preselection_main_category").html($("label[for='" + sortId + "']").text());
                        // preselection_sub leeren falls vorher schon ausgewählt
                        $(".preselection_sub").html("");
                    } else {
                        $(".preselection_sub").html($("label[for='" + sortId + "']").text());
                        $(".preselection_main_category").html($("#" + sortId).parent().parent().find(".row_qualitaetsbereich > label").text());
                    }
                    var imgSrc = $("#" + sortId).parent().parent().find(".mainImage").attr("src");
                    $("#preselection_inner_center > img").attr("src", imgSrc);

                    // Disabled bei der Auswahl das Sortierkriterium von Seite 1
                    var selectId = sortId.replace("Sort", "Select");
                    $("#" + selectId).attr('checked', false);
                    $("#" + selectId).button({disabled: true });

                    // offene Untercontainer schließen
                    $(".subsorts").each(function(i) {
                        if($(this).is(":visible")) {
                            $(".row_qualitaetsbereich").css("width", "522px");
                            $(".row_qualitaetsbereich label").css("width", "488px");
                            $(".subsorts label").css("width", "488px");
                            $(this).slideUp();
                        }
                    });
                    $('#sort').fadeOut('fast',function() {
                        openPreselectedSelect();$('#select').fadeIn('fast');
                    });
                }

                // Anzeige der Seite 1 der Sortbox (Auswahl der Sortierung)
                function showSortForm() {
                    // Enabled wieder alle Checkboxen auf Seite 2
                    $("#select").find("input[name=extSearchSortierungAuswahl]").button({disabled: false });
                    // offene Untercontainer schließen
                    $(".subselects").each(function(i) {
                        if($(this).is(":visible")) {
                            $(".row_qualitaetsbereich").css("width", "522px");
                            $(".row_qualitaetsbereich label").css("width", "488px");
                            $(this).slideUp();
                        }
                    });
                    $('#select').fadeOut('fast',function() {
                        openPreselectedSort();$('#sort').fadeIn('fast');
                    });
                }

                // Speichert die ausgewählten Werte in den zugehörigen versteckten Input-Feldern
                function saveSortboxValues(version) {
                    // Speichert den Value der ausgewählten Sortierung
                    $("#extSearchSortierungSort").val($("input[name='extSearchSortierungSort']:checked").val());
                    // Überträgt das Label der ausgewählten Sortierung in ein bereitgestelltes Div
                    $("#showSortDiv").html("");
                    var sortId = $("input[name='extSearchSortierungSort']:checked").attr('id');
                    var img = $("#" + sortId).parent().parent().find(".mainImage");
                    img.clone().appendTo('#showSortDiv');
                    $("#showSortDiv").append("<p class='chosen_question'>" + $("label[for='" + sortId + "']").text() + "</p>" );

                    // Speichert alle Values der ausgewählten Selectboxen zur Anzeige von Noten
                    $("#extSearchSortierungSelect").val($("input[name='extSearchSortierungAuswahl']:checked").map(function() {return $(this).val();}).get());
                    // Überträgt die Label der ausgewählten Noten in ein bereitgestelltes Div
                    $("#showSelectDiv").html("");
                    $("input[name='extSearchSortierungAuswahl']:checked").each(function() {
                        var selectId = $(this).attr('id');
                        var img = $("#" + selectId).parent().parent().find(".mainImage");

                        img.clone().appendTo('#showSelectDiv');
                        $("#showSelectDiv").append("<span class='chosen_question'>" + $("label[for='" + selectId + "']").text() + "</span>" );

                        // vertikaler Abstandshalter
                        $("#showSelectDiv").append("<div class='question_spacer'>");
                    });

                    // Versteckt die Popup-Box
                    TINY.box.hide();

                    // Auswahl anzeigen
                    $("#sort_select_wrapper").css("display", "block");
                    if ($("#extSearchSortierungSelect").val() != "") {
                        $("#show_select_header").show();
                        $("#showSelectDiv").show();
                    } else {
                        $("#show_select_header").hide();
                        $("#showSelectDiv").hide();
                    }
                }

                // Zeige den Button "Seite 2" wenn eine Auswahl getroffen wurde
                function showButtonSeite2() {
                    // Button "Seite 2" anzeigen wenn Auswahl auf Seite 1 getroffen
                    var numChosenItems = $("input[name='extSearchSortierungSort']:checked").length;
                    if (numChosenItems > 0) {
                        $(".tnext").css("display", "block");
                        $(".tfinishsort").css("display", "block");
                    }
                }

                // Markieren von max. 3 Checkboxen auf Seite 2
                function checkChosenItems(curObj) {
                    var maxItems = 3;
                    var numItems = $("input[name='extSearchSortierungAuswahl']:checked").length;
                    if ( !(numItems <= maxItems) ) {
                        alert("Sie können max. 3 Qualitätsbereiche bzw. Einzelkriterien auswählen.");
                        curObj.attr("checked", false);
                        curObj.parent().find("label[for=" + curObj.attr("id") + "]").removeClass("ui-state-active");
                        curObj.parent().find("label[for=" + curObj.attr("id") + "]").attr("aria-pressed", false);
                    }
                }

                // Funktionen, die nach vollständiger Seiten-Ladung ausgeführt werden müssen
                $(function(){
                    // Erzeugen eines Qtips zur Anzeige der der ausgewählten Suchoptionen
                    $('#infoWeitereSuchoptionen').qtip({
                        content: {
                            title: {
                                text: '<h3 class="tooltip-header">Zusätzlich ausgewählte Suchoptionen</h3>'
                            },
                            text: ''
                        },
                        position: {
                            // Position relativ zum Tooltip (Spitze)
                            my: 'bottom center',
                            // Position relativ zum Link
                            at: 'top center',
                            // Tooltip immer im sichtbaren Bereiche anzeigen
                            viewport: $(window)
                        },
                        style: 'ui-tooltip-aok qtip-shadow qtip-rounded ui-tooltip-wide'
                    });

                    // Öffnen der Erweiterten Suchoptionen beim Click auf den Link
                    $('#infoWeitereSuchoptionen').click(function(){
                        if ($("#box_erweiterte_suche").is(':hidden')){
                            phnservice.openExtendedSearch();
                        }
                        if ($("#box_weitere_optionen").is(':hidden')){
                            phnservice.openOptionSearch();
                        }
                        phnservice.checkAfterToggle();
                        return false;
                    });

                    // Tooltip "GPS-Koordinaten bestimmen lassen"
                    $('#get_geocoords_link').qtip({
                        content: {
                            title: {
                                text: '<h3 class="tooltip-header">Aktueller Standort</h3>'
                            },
                            text: 'Sie können Ihren aktuellen Standort automatisch ermitteln lassen. Nach einem Klick auf diesen Link wird Ihr Browser Sie dazu kurz um Erlaubnis bitten.'
                        },
                        position: {
                            // Position relativ zum Tooltip (Spitze)
                            my: 'bottom center',
                            // Position relativ zum Link
                            at: 'top center',
                            // Tooltip immer im sichtbaren Bereiche anzeigen
                            viewport: $(window)
                        },
                        style: 'ui-tooltip-aok qtip-shadow qtip-rounded ui-tooltip-wide'
                    });

                    // Wenn Browser IE6 oder niedriger -> Sortierung nach Einzelnoten nicht möglich
                    if (isBrowserIE7) {
                        $("#einzelnoteSortContainer").hide();
                        $("#browser_incompatible_warning").show();
                    } else {
                        // Click-Listener für die Sortieroptionen
                        // Bei der Auswahl einer "normalen" Sortieroption -> Verstecken
                        // des Zusatz-Divs zur Anzeige der Einzelnoten-Sortierauswahl
                        $("#distance").click(function() {$("#sort_select_wrapper").hide();});
                        $("#price").click(function() {$("#sort_select_wrapper").hide();});
                        $("#tb_gesamt").click(function() {$("#sort_select_wrapper").hide();});
                        $("#bes_krit_2009").click(function() {$("#sort_select_wrapper").hide();});
                        $("#bes_krit_2014").click(function() {$("#sort_select_wrapper").hide();});
                        $("#bes_krit").click(function() {$("#sort_select_wrapper").hide();});

                        // Einzelnoten-Sortierung für 2009
                        $("#einzelnote_2009").click(function() {
                            // Wenn Hidden-Feld befüllt, dann Anzeige der letzten Werte
                            var lastSort = $("#extSearchSortierungSort").val();
                            if (lastSort != null && lastSort != "") {
                                $("#sort_select_wrapper").show();
                            }
                            // Öffnen des Einzelnoten-Sort-Popups
                            openSortbox('nursinghome', "2009");
                        });

                        // Einzelnoten-Sortierung für 2014
                        $("#einzelnote_2014").click(function() {
                            // Wenn Hidden-Feld befüllt, dann Anzeige der letzten Werte
                            var lastSort = $("#extSearchSortierungSort").val();
                            if (lastSort != null && lastSort != "") {
                                $("#sort_select_wrapper").show();
                            }
                            // Öffnen des Einzelnoten-Sort-Popups
                            openSortbox('nursinghome', "2014");
                        });

                        // Einzelnoten-Sortierung für 2017
                        $("#einzelnote").click(function() {
                            // Wenn Hidden-Feld befüllt, dann Anzeige der letzten Werte
                            var lastSort = $("#extSearchSortierungSort").val();
                            if (lastSort != null && lastSort != "") {
                                $("#sort_select_wrapper").show();
                            }
                            // Öffnen des Einzelnoten-Sort-Popups
                            openSortbox('nursinghome', "2017");
                        });

                        // Wenn Repost und Sortierung nach Einzelnote bereits ausgewählt -> Auswahl anzeigen
                        if (($("#einzelnote_2009").is(':checked') || $("#einzelnote_2014").is(':checked') || $("#einzelnote").is(':checked')) && $("#extSearchSortierungSort").val() != "") {
                            $("#sort_select_wrapper").css("display", "block");
                            if ($("#extSearchSortierungSelect").val() != "") {
                                $("#show_select_header").show();
                                $("#showSelectDiv").show();
                            }
                        }
                    }
                });
            </script>

