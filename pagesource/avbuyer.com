function getPromotionWindowControl()
{

  width = $(window).width();

  if (width <= 568)
    control = true;
  else
    control = true;

  return control;
}

function getPromotionWindowSlides()
{
  width = $(window).width();

  if (width > 1280)
    slides = 3;
  else if (width >= 800 && width <= 1280 )
    slides = 3;
  else if (width >= 640)
    slides = 3;
  else if (width >= 480)
    slides = 2;
  else if (width >= 320)
    slides = 1;

  return slides;
}

