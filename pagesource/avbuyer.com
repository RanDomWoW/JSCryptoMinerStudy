function getPromotionWindowSlideWidth()
{
  width = $(window).width();

  if (width >= 1361)
	  slide_width = 260;
  else if (width >= 768)
    slide_width = 246;
  else if (width >= 568)
    slide_width = 270;
  else if (width >= 480)
    slide_width = 233;
  else
    slide_width = 309;

  return slide_width;
}

