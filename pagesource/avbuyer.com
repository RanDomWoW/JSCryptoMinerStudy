function highlightOption(id)
{
  $("#"+id+" option").each(function () {
    str = $(this).text();
    if(str.split(" ").join("").match(/L0/i))
      $(this).addClass("level_style_0");
    str = str.replace(/L[\d]/g, "");
    $(this).text(str);
  });
}
setMaxRange('year', 'https://www.avbuyer.com/maxRange/ajax/market/year/', true, 'Year model to');
setMaxRange('mileage', 'https://www.avbuyer.com/maxRange/ajax/market/mileage/', true, 'Mileage to');
function setMaxRange(fieldType, ajaxUrl, isInt, blankLabel)
{
  $('#qsearch_'+fieldType+'_from').change(function() {
    var val = 0;
    if(this.value > 0)
    {
      if (isInt)
        val = parseInt(this.value);
      else
        val = this.value;
    }
    $('#qsearch_'+fieldType+'_to').prop('disabled', true);
    $.ajax({
      type: "POST",
      url: ajaxUrl+val,
      success: function(msg){
        $('#qsearch_'+fieldType+'_to').html("<option value=\"\">" + blankLabel + "</option>"  + msg);
        $('#qsearch_'+fieldType+'_to').prop('disabled', false);
      }
    });
    return false;
  });
}
