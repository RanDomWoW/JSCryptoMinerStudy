function saveSearchAgent()
{
  var data = $('#frmAlert').serialize()+"&"+$('#q_search').serialize();
  $.ajax({
    type: "POST",
    data: data,
    url: "https://www.avbuyer.com/save/searchagent",
    success: function(msg){
      var obj = $.parseJSON(msg);

      if (obj.error)
      {
        $('#searchagentMsgs #notice').html('<span>'+obj.msg+'</span>');
        $('#searchagentMsgs, #notice').slideDown();
      }
      else if(!obj.error && obj.redirect_url)
        window.location.href = obj.redirect_url;
    }
  });
}

