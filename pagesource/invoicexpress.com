<script type="text/javascript">
  $(document).ready(function() {
      var $cookieBar = $('#cookieBar');

      if ( $cookieBar.length > 0){
        $( '#cookieBar' ).hide();

        // Show bar if cookie not there
        if ($.cookie('ix-cookiepolicy') != '1')
        {
          $( '#cookieBar' ).show();
          $(".navbar").addClass("top");
        }
      }

      $("#cookieBar a" ).click(function(event) {
        if (event.target.classList.contains('buttonHide') || event.target.classList.contains('buttonEnable')) {
          event.preventDefault();
          $( '#cookieBar' ).hide();
          $(".navbar").removeClass("top");
          $.cookie('ix-cookiepolicy', '1', { expires: 365, path: '/' });
        }
      });

      $('.close, .modal-video').click(function () {
        $('.modal-video').hide();
        this.parentElement.getElementsByTagName("iframe")[0].src = this.parentElement.getElementsByTagName("iframe")[0].src;
      }); // STOP VIDEO on close modal

      $('input, textarea').placeholder();

  });


  // HIDE MENU FOOTER
  $(function() {
    var url = window.location.href;

    if(/\/es/.test(url) || /\/en/.test(url)) {
      $('.contentHide').hide();
    }
  });


  // SELECT LANGUAGE FLAG IN FOOTER
  $(function() {
    var urlLanguages = window.location.href;

    if(/\/es/.test(urlLanguages)) {
      $('li.languages a').addClass('not-selected');
      $('li.languages a.es').removeClass('not-selected');
    }

    if(/\/en/.test(urlLanguages)) {
      $('li.languages a').addClass('not-selected');
      $('li.languages a.en').removeClass('not-selected');
    }
  });

  // BLOG SHARE STICKY MENU
  $("#sticky").pin({
        containerSelector: ".contentShow"
  });

  // BLOG AND EBOOK HOVER EFFECT
  $(function(){
      $("#vertical div.wrap").hover(function(){
          $(this).stop().animate({top:"-300px"},{queue:false,duration:200});
      }, function() {
          $(this).stop().animate({top:"0px"},{queue:false,duration:200});
      });
  });


  // SMOOTHSCROLL
  $(function() {
    $('.smoothScroll[href*=#]:not([href=#])').click(function() {
      if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {

        var target = $(this.hash);
        target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
        if (target.length) {
          $('html,body').animate({
            scrollTop: target.offset().top - 110
          }, 500);
          return false;
        }
      }
    });
  });

  $(window).scroll(function() {
      var scroll = $(window).scrollTop();

      if (scroll >= 500) {
          $(".btn.btn-default.trial").addClass("orange");
      } else {
          $(".btn.btn-default.trial").removeClass("orange");
      }
  });

</script>



