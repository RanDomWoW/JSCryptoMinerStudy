    <!-- Affiliate & SegmentIO Token-->
    <script>
      $(document).ready(function(){
        $("a").click(function(){
          //Generate segmentio token and save it as cookie segmentio
          if (!$.cookie("segmentio")){
            number = Math.floor((Math.random() * 100000) + 1);
            $.cookie("segmentio", number, { domain: ".invoicexpress.com" });
            window.analytics.identify($.cookie("segmentio"));
          }

          // Saves the affiliate token on a cookie
          affiliate_token = "";
          if (affiliate_token != "" && affiliate_token != null ){
            $.cookie("affiliate_token", affiliate_token, { domain: ".invoicexpress.com" });
          }

          // The following 2 code blocks can be removed once we are sure the cross subdomain cookie works with segment.io

          //Inject SegmentIO & Affiliate token on the url
          href = $(this).attr("href");
          if (href.indexOf("app.invoicexpress.com") >= 0){
            segmentio_token = $.cookie("segmentio");
            if (segmentio_token != "" && segmentio_token != null ){
              $("a").each(function(){
                var anchorUri = new URI($(this).attr("href"));
                $(this).attr("href", anchorUri.addSearch({segmentio_token: segmentio_token}));
              });
            }

            affiliate_token = $.cookie("affiliate_token");
            if (affiliate_token != "" && affiliate_token != null ){
              $("a").each(function(){
                var anchorUri = new URI($(this).attr("href"));
                $(this).attr("href", anchorUri.addSearch({affiliate_token: affiliate_token}));
              });
            }
          }
        });
      });




    </script>

