<script type="text/javascript">
    $(document).ready(function () {

        window.googleDropdownMenuOn = false;

        function toggleGoogleMenu() {
            $('#google.dropdown-menu ul').toggle();
            window.googleDropdownMenuOn = !window.googleDropdownMenuOn;
        }

        $('#header #google.dropdown-menu > a').click(function (evt) {
            evt.preventDefault();
            evt.stopPropagation();
            toggleGoogleMenu();
        });

        $(document).click(function (evt) {
            if (window.googleDropdownMenuOn) {
                var g = $(this).parents('.google-dropdown-menu');
                if (g.length === 0) {
                    // Clicked outside the menu so close it
                    toggleGoogleMenu();
                }
            }
        });

    });
</script>        </div>

