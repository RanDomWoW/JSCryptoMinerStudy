<!-- If First Page and Email Local Storage Value Set, API Call to marketo -->
<script>
    if (!sessionStorage['marketoquery']) {

        if(getTrackValue("email")){

            <!-- Ajax call to get Marketo -->
            var localStorageLeadDetails = {
            email: getTrackValue('email'),
            firstname: getTrackValue('firstname'),
            lastname: getTrackValue('lastname'),
            company: getTrackValue('company'),
            crm: getTrackValue('crm'),
            phone: getTrackValue('phone'),
            title: getTrackValue('title'),
            titlefunction: getTrackValue('title_function'),
            titlelevel: getTrackValue('title_level'),
            analyticssearchreferrerurl: getTrackValue('Analytics_Search_referrer_url'),
            analyticsaffiliateid: getTrackValue('Analytics_Affiliate_id'),
            isawebsection: getTrackValue('ISA_web_section'),
            isamobile: getTrackValue('ISA_mobile'),
            isaoriginalppckeyword: getTrackValue('ISA_Original_ppc_keyword'),
            isaoriginalppckeywordid: getTrackValue('ISA_Original_ppc_keyword_id'),
            isaoriginalsource: getTrackValue('ISA_Original_source'),
            isachannel: getTrackValue('ISA_channel'),
            isappcadid: getTrackValue('ISA_ppc_ad_id'),
            isappckeyword: getTrackValue('ISA_ppc_keyword'),
            isappckeywordid: getTrackValue('ISA_ppc_keyword_id'),
            isappcmatchtype: getTrackValue('ISA_ppc_match_type'),
            isappcposition: getTrackValue('ISA_ppc_position'),
            isasource: getTrackValue('ISA_source'),
            isatouchtype: getTrackValue('ISA_touch_type'),
                        isacampaignname: getTrackValue('ISA_campaign_name'),
            isacampaigncode: getTrackValue('ISA_campaign_code'),
                        isaaffiliatecode: getTrackValue('ISA_affiliate_code'),
                        isaaffiliatepromotionname: getTrackValue('ISA_affiliate_promotion_name'),
                        isacontactid: getTrackValue('ISA_contact_id'),
                        isaorganickeyword: getTrackValue('ISA_organic_keyword'),
                        isappckeywordid: getTrackValue('ISA_ppc_keyword_id')
               };

                for (var key in localStorageLeadDetails) {
                    if (localStorageLeadDetails[key] === null) {
                    localStorageLeadDetails[key] = ""
                    }
                }

                if (window.XMLHttpRequest) {
                    // code for IE7+, Firefox, Chrome, Opera, Safari
                    xmlhttp = new XMLHttpRequest();
                } else {
                    // code for IE6, IE5
                    xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
                }
                xmlhttp.onreadystatechange = function() {
                    if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
                        marketoAPIDetail = xmlhttp.responseText;
                    }
                }
                xmlhttp.open("GET", "https://static.insidesales.com/assets/php/marketo-munchkin-api-associate-lead.php?"+
                "email="+localStorageLeadDetails['email']+
                "&firstname="+localStorageLeadDetails['firstname']+
                "&lastname="+localStorageLeadDetails['lastname']+
                "&company="+localStorageLeadDetails['company']+
                "&crm="+localStorageLeadDetails['crm']+
                "&phone="+localStorageLeadDetails['phone']+
                "&title="+localStorageLeadDetails['title']+
                "&titlefunction="+localStorageLeadDetails['titlefunction']+
                "&titlelevel="+localStorageLeadDetails['titlelevel'] +
                "&isawebsection="+localStorageLeadDetails['isawebsection'] +
                "&isamobile="+localStorageLeadDetails['isamobile'] +
                "&isaoriginalppckeyword="+localStorageLeadDetails['isaoriginalppckeyword'] +
                "&isaoriginalppckeywordid="+localStorageLeadDetails['isaoriginalppckeywordid'] +
                "&isaoriginalsource="+localStorageLeadDetails['isaoriginalsource'] +
                "&isachannel="+localStorageLeadDetails['isachannel'] +
                "&isappcadid="+localStorageLeadDetails['isappcadid'] +
                "&isappckeyword="+localStorageLeadDetails['isappckeyword'] +
                "&isappckeywordid="+localStorageLeadDetails['isappckeywordid'] +
                "&isappcmatchtype="+localStorageLeadDetails['isappcmatchtype'] +
                "&isappcposition="+localStorageLeadDetails['isappcposition'] +
                "&isasource="+localStorageLeadDetails['isasource'] +
                "&isatouchtype="+localStorageLeadDetails['isatouchtype']+
                                "&isacampaignname="+localStorageLeadDetails['isacampaignname']+
                                "&isacampaigncode="+localStorageLeadDetails['isacampaigncode']+
                                "&isaaffiliatecode="+localStorageLeadDetails['isaaffiliatecode']+
                                "&isaaffiliatepromotionname="+localStorageLeadDetails['isaaffiliatepromotionname']+
                                "&isacontactid="+localStorageLeadDetails['isacontactid']+
                                "&isaorganickeyword="+localStorageLeadDetails['isaorganickeyword']+
                                "&isappckeywordid="+localStorageLeadDetails['isappckeywordid']
                , true);
                xmlhttp.send();

                <!-- End Ajax call to marketo -->
        }
        sessionStorage.setItem("marketoquery", "yes");
    }
</script>
<!-- End If First Page and Email Local Storage Value Set, API Call to marketo -->
