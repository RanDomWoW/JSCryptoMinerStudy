<script type="text/javascript">
    var CURRENCY_SELECTED = 'CurrencySelected';

    $(function() {
        var selector = $('#currencySelector');
        var defaultCurrency = '$AUD';
        
        selector.on('change', handleCurrencySelectorChange);
        $('a.currency-option').on('click', handleCurrencyOptionClick);

        if ($.cookie(CURRENCY_SELECTED) === null) {
            $.cookie(CURRENCY_SELECTED, defaultCurrency, { path: '/' });
        }
        setCurrencySelectorFromCookie(selector);
        handleCurrencySelectorChange.call(selector[0]);
    });

    function setCurrencySelectorFromCookie(currencySelector) {
        var value = $.cookie(CURRENCY_SELECTED);

        if (value) {
            currencySelector.val(value);
        }
    };

    function handleCurrencySelectorChange() {
        $.cookie(CURRENCY_SELECTED, this.value, { path: '/' });
        $('#currencyText').html(this.value);

        
        (function setPhoneUsingCurrency(){
            var currencyMap = {  '$AUD': '1300 385 625',   '$NZD': '0800 442 095',   };
            $('#ccl_header_locale-number b').html(currencyMap[this.value]);
        }).call(this);
        
    };

    function handleCurrencyOptionClick(event) {
        event.preventDefault();
        $('#currencySelector').val($(this).data('currency')).change();
        if (Carnival.Header2013) {
            Carnival.Header2013.navTooltips('close');
        }
    }

</script>
    
