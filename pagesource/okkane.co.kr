<script>

    function fadeSlider() {
        var nowNum = 0; //초기 인덱스값 설정.
        var bannerObj = jQuery('.slideUl li'); // 자식넣기
        var bannerBth = jQuery('.s_btnUl li'); // 자식넣기

        bannerObj.eq(nowNum).addClass("sul_active").fadeTo("normal", 1);
        bannerObj.not(bannerObj.eq(nowNum)).removeClass("sul_active").fadeTo("normal", 0);
        bannerBth.eq(nowNum).addClass("s_active");
        bannerBth.not(bannerBth.eq(nowNum)).removeClass("s_active");

        var maxLength = bannerObj.length; // 배너 갯수구하기
        var btnLength = bannerBth.length; // 버튼 갯수구하기
        var timer = 3000; // 초기 자동 롤링되는 타임 설정
        var autoRun = setInterval(function () {
            rollingFn('right');
        }, timer);

        jQuery('.nextBtn').bind('click', function () {
            rollingFn('right');
        });

        jQuery(".s_btnUl li").mouseover(function () {
            if (jQuery(this).hasClass('s_active')) {
                jQuery(this).not().fadeTo("normal", 1);
            } else {
                var nowNum = jQuery(".s_btnUl li").index(this);
                bannerObj.eq(nowNum).addClass("sul_active").stop(true).fadeTo("normal", 1);
                bannerObj.not(bannerObj.eq(nowNum)).removeClass("sul_active").stop(true).fadeTo("normal", 0);
                bannerBth.eq(nowNum).addClass("s_active");
                bannerBth.not(bannerBth.eq(nowNum)).removeClass("s_active");
            };
        });

        var now = true;
        jQuery(".s_btnUl li").mouseover(function (e) {
            e.preventDefault();
            nowNum = Number(jQuery(".s_btnUl li").index(this));
            if (now) {
                now = false;
                window.clearInterval(timer);
                window.clearInterval(autoRun); //  타이머멈추기
            }
        });

        jQuery(".s_btnUl li").mouseout(function (e) {
            e.preventDefault();
            if (now == false) {
                now = true;
                setInterval(timer);
                autoRun = window.setInterval(function () {
                    rollingFn('right');
                }, timer); //  타이머
            }
        });

        function rollingFn(direction) {
            if (direction == 'right') {
                nowNum = nowNum + 1;
                if (nowNum > btnLength - 1) {
                    nowNum = 0;
                }
            } else {
                nowNum = nowNum - 1;
                if (nowNum < 0) {
                    nowNum = btnLength - 1;
                }
            }

            bannerObj.eq(nowNum).addClass("sul_active").stop(true).fadeTo("normal", 1);
            bannerObj.not(bannerObj.eq(nowNum)).removeClass("sul_active").stop(true).fadeTo("normal", 0);
            bannerBth.eq(nowNum).addClass("s_active");
            bannerBth.not(bannerBth.eq(nowNum)).removeClass("s_active");
        }
    }

    jQuery(window).load(function(){
       jQuery('.slideUl').fadeTo('normal',1);
       fadeSlider();
    });
</script>
