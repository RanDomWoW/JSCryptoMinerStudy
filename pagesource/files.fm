		$('#addcookieuploads-modal-content').css('display', 'none');

		$('#createaccountsuccessmessage-modal-content').css('display', 'none');

		$('#createaccount-fb-modal-content').css('display', 'none');

		if (bolRewriteUrlHash) {
			window.location.hash = 'sign_up';
		}
	}

	function showCreateAccountFb() {
		showModal('sign-in');

		$('#createaccount-fb-modal-content').css('display', 'block');

		$('#signin-modal-content').css('display', 'none');
		$('#signin-header').css('color', '#707070');

		$('#forgotpassword-modal-content').css('display', 'none');

		$('#addcookieuploads-modal-content').css('display', 'none');

		$('#createaccountsuccessmessage-modal-content').css('display', 'none');

		$('#sign-in header').css('display', 'none');
		$('#sign-in footer').css('display', 'none');
	}

	function showSignIn() {

		showModal('sign-in');

		$('#createaccount-modal-content').css('display', 'none');
		$('#createaccount-header').css('color', '#707070');

		$('#signin-modal-content').css('display', 'block');
		$('#signin-header').css('color', '#CA1913');

		$('#forgotpassword-modal-content').css('display', 'none');

		$('#addcookieuploads-modal-content').css('display', 'none');

		$('#createaccountsuccessmessage-modal-content').css('display', 'none');

		$('#createaccount-fb-modal-content').css('display', 'none');

		if (bolRewriteUrlHash) {
			window.location.hash = 'log_in';
		}
	}

	function showForgotPassword() {
		showModal('sign-in');

		$('#createaccount-modal-content').css('display', 'none');
		$('#createaccount-header').css('color', '#707070');

		$('#signin-modal-content').css('display', 'none');
		$('#signin-header').css('color', '#707070');

		$('#forgotpassword-modal-content').css('display', 'block');

		$('#addcookieuploads-modal-content').css('display', 'none');

		$('#createaccountsuccessmessage-modal-content').css('display', 'none');

		$('#createaccount-fb-modal-content').css('display', 'none');

		if (bolRewriteUrlHash) {
			window.location.hash = 'forgot_password';
		}
	}

	function showAddCookieUploads() {
		showModal('sign-in');

		$('#createaccount-modal-content').css('display', 'none');
		$('#createaccount-header').css('color', '#707070');

		$('#signin-modal-content').css('display', 'none');
		$('#signin-header').css('color', '#707070');

		$('#forgotpassword-modal-content').css('display', 'none');

		$('#sign-in header #modal-label').html('&nbsp;');

		$('#addcookieuploads-modal-content').css('display', 'block');

		$('#createaccountsuccessmessage-modal-content').css('display', 'none');

		$('#createaccount-fb-modal-content').css('display', 'none');
	}

	function showCreateAccountSuccessMessage()
	{
		showModal('sign-in');

		$('#createaccount-modal-content').css('display', 'none');
		$('#createaccount-header').css('color', '#707070');

		$('#signin-modal-content').css('display', 'none');
		$('#signin-header').css('color', '#707070');

		$('#forgotpassword-modal-content').css('display', 'none');

		$('#sign-in header #modal-label').html('&nbsp;');

		$('#addcookieuploads-modal-content').css('display', 'none');

		$('#createaccountsuccessmessage-modal-content').css('display', 'block');

		$('#createaccount-fb-modal-content').css('display', 'none');
	}


	var bolHasCookieUploads = false;
	var bolAddCookieUploadsAnswered = false;
	var bolAddCookieUploads = false;
	var strActiveForm = '';

	function setAddCookieUploads(val) {

		bolAddCookieUploads = val;
		bolAddCookieUploadsAnswered = true;

		if (val) {
			$('#login_facebook .add_anonym_uplaods').val('1');
			$('#login_google .add_anonym_uplaods').val('1');
			$('#login_draugiem .add_anonym_uplaods').val('1');
		}

		if (strActiveForm == 'signin') {
			checkSignInForm();
			showSignIn();
		}
		else if (strActiveForm == 'createaccount') {
			checkCreateAccountForm();
			showCreateAccount();
		}
		else if (strActiveForm == 'facebook' || strActiveForm == 'google' || strActiveForm == 'draugiem') {

			checkSocialLogin(strActiveForm);
		}
	}


