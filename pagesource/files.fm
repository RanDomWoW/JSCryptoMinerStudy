					url: "./ajax/sign_in.php?PHPSESSID=bsni2p8vvt15j1tjrobe2e0lq4",
                    crossDomain: true,
                    data: {
						'check_signin' : true ,
						'username' : $('#l_user').val() ,
						'password' : $('#l_pass').val()
					},
					success: function(data) {
