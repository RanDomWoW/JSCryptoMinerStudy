<script type="text/javascript">

	var bolRewriteUrlHash = true;

	function showCreateAccount() {
		showModal('sign-in');

		$('#createaccount-modal-content').css('display', 'block');
		$('#createaccount-header').css('color', '#CA1913');

		$('#signin-modal-content').css('display', 'none');
		$('#signin-header').css('color', '#707070');

