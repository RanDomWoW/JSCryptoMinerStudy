<script type="text/javascript">
    function getDepartmentIds() {
        var dept = $("input[type=checkbox][id*=chkDept_]:checked");
        if (!dept || dept.length == 0) return "";
        var dids = "";
        dept.each(function() {
            dids += (dids == "" ? "" : ",") + $(this).val();
        });
        return dids;
    }
    function cfc() {
        $("#btnGO").attr("disabled", true);
        $("#filterLoading").html('<div style="margin-top:15px;">Loading...</div><img src="/images/ajaxloading5.gif" style="margin-top:4px;" /><br />');

        $.getJSON(
			"/roajax.aspx?F=GetFinderTotal&SiteId=1&dids=" + getDepartmentIds(),
			function(o) {
			    if (!o.Success) {
			        return;
			    }
			    $("#FilterResults").html(o.Total);
			    $("#btnGO").attr("disabled", o.Total == 0);
			    o.Total == 0 ? $("#btnGO").attr("src", "/images/buttons/go_alt_greyed.gif") : $("#btnGO").attr("src", "/images/buttons/go_alt.gif");
			    $("#filterLoading").html('');
			}
		);
    }
    function FilterSubmit() {
        var dids = getDepartmentIds();
        if (dids == "") {
            alert("Please choose at least one filter before continuing.");
            return;
        }
        window.location = "https://www.flowershopping.com/store/finder.aspx?F_DepartmentIds=" + dids;
    }


</script>

