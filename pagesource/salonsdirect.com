<script type="text/javascript">
    function rcheckQty(strid,str,val1)
    {
        var min_qty =  parseInt(str);
        var q_val = jQuery('#'+strid).val();
        var curr_value = q_val;
        if(min_qty < curr_value)
        {  if(parseInt(val1) <= 0){
            document.getElementById("writerror-"+strid).innerHTML = "Oops! Only " +min_qty+ " available";
            jQuery('#'+strid).val(min_qty);
			}
        }
        else if(min_qty != curr_value)
        {
            document.getElementById("writerror-"+strid).innerHTML = " ";
        }
    }
</script>
<script type="text/javascript">
    function rminusqty(id,val1){
        var qty = jQuery("#"+id).val();
        if(qty!=0)
        {
            jQuery("#"+id).val(parseInt(qty)- parseInt(1));
        }
        else {
            jQuery("#"+id).val(parseInt(0));
        }
    }
    function rplusqty(id,val1){
        var qty = jQuery("#"+id).val();
        qty = parseInt(qty) + parseInt(1);
        var stock = jQuery("#"+id).attr('data-stock');
        stock = parseInt(stock);
        
         if(stock >= qty){
            jQuery("#"+id).val(parseInt(qty));
        }else{
			if(parseInt(val1) > 0){
			jQuery("#"+id).val(parseInt(stock));
			}else{
				jQuery("#"+id).val(parseInt(qty));
				}
			}
    }
function submitrelatedform(id){
