<script type="text/javascript">
function subscribe_ajax(nokey) {
	if (nokey && document.getElementById('s_dis').value != 1) {
		return false;
	}
    JsHttpRequest.query(
        '/cgi-bin/Runs/dyna_loader.pl',
        {
            'par': 'subscribe',
            'email': document.getElementById('subs_email').value.trim(),
            'name': document.getElementById('sub_name').value,
            'topic_id': '1',
            'check_url': 'http://www.greeninfo.ru/',
            'key': nokey ? '' :''
        },
        function(result,errors) {
            if ( result == 'BADDATA' ) {
				$('#subsErrPlatz').html('<span class="danger">Не заполнены важные поля</span>');
            }
            if ( result == 'BADKEY' ) {
				$('#subsErrPlatz').html('<span class="danger">Плохая ссылка для подтверждения подписки/отписки на рассылку</span>');
            }
            if ( result == 'EMAILCHECK_SUBSCRIBE' ) {
				$('#subsErrPlatz').html('<span class="success">На вашу почту отправлено письмо для подтверждения подписки на рассылку</span>');
            }
            if ( result == 'EMAILCHECK_UNSUBSCRIBE' ) {
				$('#subsErrPlatz').html('<span class="success">На вашу почту отправлено письмо для подтверждения отписки от рассылки</span>');
            }
            if ( result == 'SUBSCRIBED' ) {
				$('#subscribeAjaxForm').remove();
				$('#subsErrPlatz').html('<span class="success">Вы успешно подписаны на рассылку</span>');
            }
            if ( result == 'UNSUBSCRIBED' ) {
				$('#subscribeAjaxForm').remove();
				$('#subsErrPlatz').html('<span class="success">Вы успешно отписаны от рассылки</span>');
            }
        },
        true
    );
    return false;
}
	function validateEmail(email) {
		var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
		return re.test(email);
	}
	function checkSub(func,n) {
		var email = document.getElementById('subs_email').value.trim(),
			name = document.getElementById('sub_name').value.trim(),
			divErr = document.getElementById('subsErrPlatz');
		if (validateEmail(email)) {
			document.getElementById('s_dis').value = 1;
			divErr.innerHTML = '';
			eval(func);
				if(name){
					document.getElementById('s_dis').value = 1;
					divErr.innerHTML = '';
					if($('#SubConfidentialCheck').is(':checked') || $('#SubConfidentialCheck').val() == 4){
						document.getElementById('s_dis').value = 1;
						divErr.innerHTML = '';
						return true;
					} else {
						document.getElementById('s_dis').value = '';
						if(n) divErr.innerHTML = '<span class="danger">Нужно принять условия</span>';
						return false;
					}
					return true;
				} else {
					document.getElementById('s_dis').value = '';
					if(n) divErr.innerHTML = '<span class="danger">Не указано имя!</span>';
					return false;
				}
		} else {
			document.getElementById('s_dis').value = '';
			divErr.innerHTML = '<span class="danger">Некорректный e-mail!</span>';
			return false;
		}
	}</script>
