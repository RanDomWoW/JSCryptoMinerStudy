        //Global
        window.showPageLoading = function (){
            if (document.readyState !== "complete") {
                document.querySelector('.js-svg-loader').className += ' ' + 'svg-hidden';
            } else {
                document.querySelector('.js-svg-loader').classList.remove('svg-hidden');
            }
            document.querySelector('.js-loader').classList.remove('loader-hidden');
            var element = document.querySelector('.vv_loader-wrapper');
            var cloneElement = element.cloneNode(true);
            element.parentNode.replaceChild(cloneElement, element); 
        };
    
        window.hidePageLoading = function(){
            document.querySelector('.js-loader').className += ' ' + 'loader-hidden';
        };
    
        // Show loader faster on slow connection 
        function pageNotReady() {
            window.showPageLoading();
        }
    
        document.onreadystatechange = function () {
            if (document.readyState === "complete") {
                document.querySelectorAll('a.js-navigate').forEach(function(link){
                    link.removeEventListener("click", pageNotReady, true);
                });
            } else {
                document.querySelectorAll('a.js-navigate').forEach(function(link){
                    link.addEventListener('click', pageNotReady, true);
                });
            }
        }
    
      </script>
