<script src="/js/nodejs/ufo_socket.io.js"></script>
<script>

  	$(function() {   
  		$(document).on("keydown", "#ufo_input_reply", function(e) {
	        if ((e.keyCode == 10 || e.keyCode == 13) && e.ctrlKey) {
	        	$('#ufo_form_reply').submit();
	        } 
	    });
	     
	    $(document).on("keyup", "#ufo_input_reply", function(e) {
	    	var reply = jQuery.trim($(this).val());
    		 
    		$.base64.utf8encode = true;
    		reply_base64 = $.base64.encode(reply);
    		
			UfoChat.setCookie("message", reply_base64);
			if (reply.length > 0) {    
				UfoChat.socketEmit("chat_client_writing", {'chat_id':$("#ufo_input_chat_id").val(), 'reply':reply});
			} 
	    });
	    
		$(document).mouseup(function (e) {
			jQuery.each( new Array('attached', 'online', 'tools') , function(key, val) {
				var container = $("#ufo_"+val);
			    if (!container.is(e.target) && container.has(e.target).length === 0) {
			    	if (val == 'attached') UfoChat.attachedClose();
			    	if (val == 'online') UfoChat.onlineClose();
			    	if (val == 'tools') UfoChat.toolsClose();
			    }
			});   
		});   
	});
	    
	(UfoChat = {
		
		widget : "admtools",
		
		/* -------------------------- Работа с событиями -------------------------- */
		socket : new Object(),
		
		setSocket : function(socket) {
			this.socket = socket;
		},
		socketEmit : function(event_name, event_params) {
			this.socket.emit(event_name, event_params);	
		},
		
		sessionHandler : new Object(),
		timeout : new Object(),
		sound : 0,
		  
		/* -------------------------- Вспомогательные методы -------------------------- */
		setEnding : function(num) {
			if (num == 0) return "ов";
			if (num == 1) return "";
			if (num > 1 && num < 5) return "а";
			if (num >= 5) return "ов";
		},  
		setCookie : function(key, val) {    
		    $.cookie('ufo_'+key, val, { path: '/', domain: '.ukraine.com.ua' });
		}, 
		getCookie : function(key) { 
		    return $.cookie('ufo_'+key);
		}, 
		
		soundTurn : function(switcher) {  
			this.sound = switcher;
		},
		
		/* -------------------------- Управление интерфейсом -------------------------- */
		widgetWarning : function(message) {
			if (typeof(this.timeout['warning']) != 'undefined') clearTimeout(this.timeout['warning']);
        	
			$('#ufo_warning').stop().html("<div style='margin:10px 20px 10px 45px;'>"+message+"</div>").show().css("height", "auto").css("opacity", "1");
			this.timeout['warning'] = setTimeout(function() {
			    $('#ufo_warning').animate({height: 'hide', opacity: 'hide'}, 1000);
			}, 4000); 
			return false; 	  
		},
		widgetWarningForceClose : function() {
			$('#ufo_warning').stop().hide();
		},
        widgetEventNotify : function(message, delay_param) {
        	if (typeof(delay_param) == "undefined") delay_param = 4000; 
			if (typeof(this.timeout['event']) != 'undefined') clearTimeout(this.timeout['event']);
        	
        	$('#ufo_event').stop().html(message).show().css("height", "auto").css("opacity", "1");
			this.timeout['event'] = setTimeout(function() {
			    $('#ufo_event').animate({height: 'hide', opacity: 'hide'}, 1000);
			}, delay_param);
			return false; 
        },
        widgetEventNotifyForceClose : function() { 
        	$('#ufo_event').stop().hide(); 
        },
		widgetOpen : function() {
			this.widgetPropertiesClear();
			$("#ufo_loader").show();
		
			AjaxRequest.action("/action/support/ufo/widget/open/", "Загрузка...");
			return false;
		},
		widgetRollDown : function() {
			$(".ufo-chat").addClass("rolledup");
			$(".ufo-chat").css("top", "auto");
			$(".ufo-chat").css("left", "auto");
			$(".ufo-chat").draggable("disable");
			this.widgetPropertiesClear(); 
			this.setCookie("roll", "down");
		},
		widgetRollUp : function() {
			$(".ufo-chat").removeClass("rolledup");
			$(".ufo-chat").draggable("enable");
			this.widgetPropertiesClear();
			this.setCookie("roll", "up");
		},
		
		widgetClosureOn : function() { 
			$("#ufo_loader").show();
			AjaxRequest.action("/action/support/ufo/widget/closure/on/");
			return false;
		},
		
		widgetClosureOff : function() { 
			$("#ufo_loader").show();
			AjaxRequest.action("/action/support/ufo/widget/closure/off/");
			return false;
		},
		
		widgetClose : function() { 
			this.widgetPropertiesClear();
			var self = this;
			$("#ufo_form_login").find("input[type='text'], textarea").each(function() {
				self.setCookie($(this).attr("name"), "");
			}); 
		
			$("#ufo_loader").show();
			
			AjaxRequest.action("/action/support/ufo/widget/close/", "");
			return false;
		},
        widgetRepaint : function(action, params) {
        	$("#ufo_loader").show();
             
            AjaxRequest.action("/action/support/ufo/widget/event/"+action+"/", "", params);
            return false;
        },
        
        widgetPropertiesClear : function() {
			this.setCookie("roll", "");
			this.setCookie("chat_top", "");
			this.setCookie("chat_left", ""); 
			this.setCookie("chat_height", ""); 
			this.setCookie("scroll_height", ""); 
        },
		
		widgetBodyClear : function () {
			$("#ufo_loader").hide();
			this.widgetEventNotifyForceClose();
		},
		 
		/* -------------------------- Обработка прикрепленных файлов -------------------------- */
		attachedReload : function(length, attached) {
			$("#ufo_upload_files").html(length);  
			$("#ufo_attached").html(attached);  
		},
		attachedOpen : function() { 
			if (jQuery.trim($("#ufo_attached").html()) == "") {
				return this.widgetEventNotify("Прикрепленные файлы не обнаружены.");
			}
			$("#ufo_attached").find(".close").show();
			$("#ufo_attached").css("width", "230px").css("opacity", "1").show();
		},
		attachedClose : function() {
			$("#ufo_attached").find(".close").hide();
			$("#ufo_attached").animate({width: 'hide', opacity: 'hide'}, 700);
		},
		attachedRemove : function(base64) { 
			$("#ufo_"+base64).find(".remove").hide();
			$("#ufo_"+base64).find(".loader").show();
			 
			AjaxRequest.action("/action/support/ufo/attached/remove/", null, {'file' : base64});
			return false;
		},
		
		/* -------------------------- Обработка операторов Online -------------------------- */
		onlineOpen : function() {
			if (jQuery.trim($("#ufo_online").find(".list").html()) == "") {
				return this.widgetEventNotify("Комната чата пуста.");
			}
			
			$("#ufo_online").find(".close").show();
			$("#ufo_online").css("width", "230px").css("opacity", "1").show();
		},
		onlineClose : function() {
			$("#ufo_online").find(".close").hide();
			$("#ufo_online").animate({width: 'hide', opacity: 'hide'}, 700);
		},
		onlineUpdate : function(data) {
			var online = (typeof(data.online) != "undefined") ? data.online : new Object();
			  
			if (typeof(data.operator_id) != "undefined" && typeof(online[data.operator_id]) != "undefined") {
				var operator = online[data.operator_id];
				if (typeof(data.status) != "undefined" && data.status == "on") {
					if (!$("#ufo_online_"+data.operator_id).length) {
						this.widgetEventNotify("Оператор <b>"+operator.name+"</b> вошел в чат.");
						if (this.sound == 1) ion.sound.play('button_tiny');
					} 
				} else {   
					this.widgetEventNotify("Оператор <b>"+operator.name+"</b> покинул чат.");
					if (this.sound == 1) ion.sound.play('tap');
					delete online[data.operator_id]; 
				} 
			}
			
			var template = "";
			var count = 0;
			
			for (var user_id in online) {
				if (!online.hasOwnProperty(user_id)) continue;
				var user = online[user_id];
				var is_client = (user_id == 0) ? true : false; 
				 
				template = template + this.onlineBuildItem(user, is_client);
				count++;
			}
			
			$("#ufo_online").find(".list").html(template);
			$(".ufo-chat").find(".online-icon").find(".count").animate({ opacity : "hide"}, 300);
			 
			if (typeof(this.timeout['online']) != 'undefined') clearTimeout(this.timeout['online']);
			this.timeout['online'] = setTimeout(function () { 
				$(".ufo-chat").find(".online-icon").find(".count").html(count);
			}, 300); 
			 
			$(".ufo-chat").find(".online-icon").find(".count").animate({ opacity : "show", color : "green" }, 600);
			$(".ufo-chat").find(".online-icon").find(".count").animate({color : "#666666" }, 900);
		},
		onlineBuildItem : function(item, is_client) {
			var item_class = (is_client) ? "client" : "operator";
			var name = (is_client) ? "Вы, " + item.name : "Оператор, " + item.name;
			var avatar = (typeof(item.avatar) != "undefined" && jQuery.trim(item.avatar) != "") ? "style=\"background:url("+item.avatar+") no-repeat;\"" : "";
		      
			return "<tr id=\"ufo_online_"+item.id+"\" class=\"item "+item_class+"\" title=\""+item.name.replace(/("|')/g, '&quot;')+"\">"
				+ "<td align=\"center\" width=\"5%\" style=\"padding:8px 8px 6px 8px;\"><div class=\"avatar "+item_class+"\" "+avatar+"></div></td>"
			 	+ "<td align=\"left\" width=\"95%\" style=\"padding:11px 10px 10px 0px;\"><div class=\"name\">"+name.replace(/("|')/g, '&quot;')+"</div></td>" 
				+ "</tr>";
		},
		onlineKick : function(id) {
			$("#ufo_online_"+id).find(".kick").hide();
			$("#ufo_online_"+id).find(".loader").show();
			 
			AjaxRequest.action("/action/support/ufo/online/kick/", null, {'id' : id});
			return false;
		},
		 
		/* -------------------------- Интструменты -------------------------- */
		toolsOpen : function() {
			$("#ufo_tools").find(".close").show();
			$("#ufo_tools").css("width", "230px").css("opacity", "1").show();
		},
		
		toolsClose : function() {
			$("#ufo_tools").find(".close").hide();
			$("#ufo_tools").animate({width: 'hide', opacity: 'hide'}, 700);
		},
		 
		toolsMailOpen : function() {
			$("#ufo_loader").show();  
			AjaxRequest.action("/action/support/ufo/tools/mail/open/");
			return false;
		},
		
		toolsMailSend : function() {
			$("#ufo_loader_tools").show();  
			AjaxRequest.form("ufo_form_tools_mail");
			return false;
		},
		
		toolsSoundMute : function() {
			$("#ufo_loader").show();  
			AjaxRequest.action("/action/support/ufo/tools/sound/mute/");
			return false;
		},
		
		toolsSoundOn : function() {
			$("#ufo_loader").show();  
			AjaxRequest.action("/action/support/ufo/tools/sound/on/");
			return false;
		},
		
		/* -------------------------- Основные события -------------------------- */
		login : function() {
			UfoChat.setCookie("message", "");  
			$("#ufo_loader").show();  
			
			AjaxRequest.form("ufo_form_login");
			return false;
		},
		
		reply : function() {
			UfoChat.setCookie("message", "");
			$("#ufo_loader").show();  
			 
			AjaxRequest.form("ufo_form_reply");
			return false;
		},
		 
		writing : function(operator) {  
			var message = "Оператор";  
			if (typeof(operator.name) != "undefined") message = message + " " + operator.name;
			message = message + " пишет...";
			this.widgetEventNotify(message, 20000);
		},
		
		init : function(chat_id) {
			this.widgetWarning("Связь с сервером утеряна. Восстановление...");
			$("#ufo_loader").show();  
			
			AjaxRequest.action("/action/support/ufo/control/init/", "", {'chat_id':chat_id});
			return false;
		},
		  
		bind : function(chat_id, operators, next) {
			this.widgetEventNotify("Поиск оператора...", 10000);
			AjaxRequest.action("/action/support/ufo/control/bind/", "", {'chat_id':chat_id, 'operators':operators, 'next':next});
			return false;
		},
		     
		departmentChange : function(chat_id) {
			$("#ufo_loader").show();   
			
			AjaxRequest.action("/action/support/ufo/widget/event/department_change/send/");
			return false;
		},
		
		rate : function(message_id, value) {
			$("#ufo_loader").show();   
			 
			AjaxRequest.action("/action/support/ufo/control/rate/", "", {'message_id':message_id, 'value':value});
			return false;
		},
		
		sessionRefresh : function() {
			var intervals = this.sessionHandler;
			
			for (key in intervals) { 
				if (!intervals.hasOwnProperty(key)) continue;
				window.clearInterval(intervals[key]);	
			}
			
			var self = this;	
			var interval_id = setInterval(function() {
				self.sessionSend();
			}, 60000); 
				
