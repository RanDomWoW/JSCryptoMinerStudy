<script type="text/javascript">
  jQuery(document).ready(function(){ 
        jQuery('#angebot1').show();
        
        // Setzen Startzustand der Banner
        var AngebotCurrent = 'angebot1';
        var AngebotNext    = 'angebot2';
        
        // Klicks Prev / Next
        // toggleAngebote --> Tauscht die Banner aus je nachdem, wie der aktuelle Schaltzusatnd der Banner ist
        // clearInterval --> Bei geklicktem Button soll die Zeit zurueckgesetzt werden
        // myTimer --> Startet den Intervall wieder mit neuen 6 Sekunden
        jQuery("#button_next1").click(function ()  { jQuery(this).toggleAngebote('angebot10','angebot1','next'); clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_next2").click(function ()  { jQuery(this).toggleAngebote('angebot1','angebot2','next');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_next3").click(function ()  { jQuery(this).toggleAngebote('angebot2','angebot3','next');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_next4").click(function ()  { jQuery(this).toggleAngebote('angebot3','angebot4','next');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_next5").click(function ()  { jQuery(this).toggleAngebote('angebot4','angebot5','next');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_next6").click(function ()  { jQuery(this).toggleAngebote('angebot5','angebot6','next');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_next7").click(function ()  { jQuery(this).toggleAngebote('angebot6','angebot7','next');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_next8").click(function ()  { jQuery(this).toggleAngebote('angebot7','angebot8','next');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_next9").click(function ()  { jQuery(this).toggleAngebote('angebot8','angebot9','next');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_next10").click(function () { jQuery(this).toggleAngebote('angebot9','angebot10','next'); clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
      
        jQuery("#button_prev1").click(function ()  { jQuery(this).toggleAngebote('angebot2','angebot1','prev');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_prev2").click(function ()  { jQuery(this).toggleAngebote('angebot3','angebot2','prev');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_prev3").click(function ()  { jQuery(this).toggleAngebote('angebot4','angebot3','prev');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_prev4").click(function ()  { jQuery(this).toggleAngebote('angebot5','angebot4','prev');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_prev5").click(function ()  { jQuery(this).toggleAngebote('angebot6','angebot5','prev');  clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
