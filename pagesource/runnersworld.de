        jQuery("#button_prev9").click(function ()  { jQuery(this).toggleAngebote('angebot10','angebot9','prev'); clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });
        jQuery("#button_prev10").click(function () { jQuery(this).toggleAngebote('angebot1','angebot99','prev'); clearInterval(myTimer); myTimer = window.setInterval(startInterval, 6000); });

        // Beim Aufruf der Seite wird die Variable isActive auf 1 gesetzt. D.h. der Tab  ist eingeblendet
        var isActive = 1;
        
        // Reiter Aktiv sichtbar: Werbemittelslider aktivieren
        window.onfocus = function () { 
          isActive = 1;
          clearInterval(myTimer); 
          myTimer = window.setInterval(startInterval, 6000);
        }; 

        // Reiter nicht Aktiv sichtbar: Werbemittelslider deaktivieren
        window.onblur = function () { 
          isActive = 0; 
        };
        
        // Funktion zum Tauschen der Angebote je nach aktuellem Schaltzustand
        // Nur wenn Reiter aktiv ist wird getoggelt
        var startInterval = function() {
            if ( isActive == 1 ) {
                jQuery(this).toggleAngebote(AngebotCurrent,AngebotNext, 'next');
            }
        };
        
        // Weiterschaltung der Werbemittel alle 6 Sekunden
        var myTimer = setInterval( startInterval, 6000);
            
        jQuery.fn.toggleAngebote = function(angebotout, angebotin, direction) { 
        
            AngebotCurrent = angebotout;
            
            //console.log('AngebotCurrent' + AngebotCurrent);
            //console.log('angebotout..' + angebotout);
            //console.log('angebotin..'  + angebotin);
           
            if ( AngebotCurrent == "angebot1"  ) {
                AngebotCurrent = "angebot2";
                AngebotNext    = "angebot3";
            }
            else if ( AngebotCurrent == "angebot2"  ) {
                AngebotCurrent = "angebot3";
                AngebotNext    = "angebot4";
            }
            else if ( AngebotCurrent == "angebot3" ) {
                AngebotCurrent = "angebot4";
                AngebotNext    = "angebot5";
            }
            else if ( AngebotCurrent == "angebot4") {
                AngebotCurrent = "angebot5";
                AngebotNext    = "angebot6";
            }
            else if ( AngebotCurrent == "angebot5"  ) {
                AngebotCurrent = "angebot6";
                AngebotNext    = "angebot7";
            }
            else if ( AngebotCurrent == "angebot6"  ) {
                AngebotCurrent = "angebot7";
                AngebotNext    = "angebot8";
            }
            else if ( AngebotCurrent == "angebot7"  ) {
                AngebotCurrent = "angebot8";
                AngebotNext    = "angebot9";
            }
            else if ( AngebotCurrent == "angebot8" ) {
                AngebotCurrent = "angebot9";
                AngebotNext    = "angebot10";
            }
            else if ( AngebotCurrent == "angebot9" ) {
                AngebotCurrent = "angebot10";
                AngebotNext    = "angebot1";
            }
            else {
                if ( direction == "next" ) {
                  AngebotCurrent = "angebot1";
                  AngebotNext    = "angebot2";
                  angebotin = "angebot1";
                }
            }
            
            if ( direction == "prev" ) {
                AngebotCurrent = angebotin;
                AngebotNext    = angebotout;
            }
            
           
            
            jQuery('#' + angebotout ).fadeOut("1000", function() {
                jQuery('#angebot1' ).hide();
                jQuery('#angebot2' ).hide();
                jQuery('#angebot3' ).hide();
                jQuery('#angebot4' ).hide();
                jQuery('#angebot5' ).hide();
                jQuery('#angebot6' ).hide();
                jQuery('#angebot7' ).hide();
                jQuery('#angebot8' ).hide();
                jQuery('#angebot9' ).hide();
                jQuery('#angebot10' ).hide();
                jQuery('#' + angebotin  ).fadeIn("1000");
            });
            
            
        } 
  });
 
</script>


