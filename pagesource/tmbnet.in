<script type="text/javascript">
var timerBanner=0;
function func(){
    timerBanner = setTimeout("slideGallery_1()", 3000);
}
window.onload = func;

function slideGallery_1()
{
	var BannerContainer_1=$(".Box1").children(".bannerContainer");
	var BannerContainerMargin_1=parseInt($(BannerContainer_1).css("margin-top"));
	if(BannerContainerMargin_1!=-204)
	{
		$(BannerContainer_1).animate({"margin-top":"-=204px"});
	}
	else
	{
		$(BannerContainer_1).animate({"margin-top":"+=204px"});
	}
	setTimeout("slideGallery_2()", 3000);
}

function slideGallery_2()
{
	var BannerContainer_2=$(".Box2").children(".bannerContainer");
	var BannerContainerMargin_2=parseInt($(BannerContainer_2).css("margin-top"));
	if(BannerContainerMargin_2!=-204)
	{
		$(BannerContainer_2).animate({"margin-top":"-=204px"});
	}
	else
	{
		$(BannerContainer_2).animate({"margin-top":"+=204px"});		
	}
	setTimeout("slideGallery_1()", 3000);
}
</script>
            <script type="text/javascript">

                var c=1;
                var t;
                var timer_is_on=0;
                function timedCount()
                {
                	
                	c=c-1;

                	if(c < 1)
                	{
                		/*c=4;*/
						c=5;


                	}
					if(c==5)
                	{
						document.getElementById("testimg").src = "../img/store.png";
                		document.getElementById("testref").href = "#null";
                	}
                	else if(c==4)
                	{
						document.getElementById("testimg").src = "../img/speak.png";
                		/*document.getElementById("testimg").src = "../img/img-3" + ".png";*/
                		document.getElementById("testref").href = "#null";
                	}
                	else if(c==3)
                	{
                		document.getElementById("testimg").src = "../img/first.png";
						/*document.getElementById("testimg").src = "../img/first.png";*/
                		document.getElementById("testref").href = "#null";
                	}
                		else if(c==2)
                	{
                		document.getElementById("testimg").src = "../img/img-"+ c + ".jpg";
                		document.getElementById("testref").href = "#null";
                	}
                	else
                	{
                		/*document.getElementById("testimg").src = "../img/img-"+ c + ".jpg"; */
						/*document.getElementById("testimg").src = "../img/first.png";*/
						document.getElementById("testimg").src = "../img/img-3" + ".png";
                		document.getElementById("testref").href = "#null";
                	}	
                	
                	t=setTimeout("timedCount()",7000);
                }

                function doTimer()
                {
                if (!timer_is_on)
                  {
                	timer_is_on=1;
                	timedCount();
                  }
                }
            </script>
