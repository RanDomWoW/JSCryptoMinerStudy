<script>
$(document).ready(function() {
	handleFooter();
	function handleFooter() {
		if( $(window).width() > 768 ) {
			var theFooter = $('.footer-about').prev();
			var fullHeight = theFooter.height() + 10;
			var collapsedHeight = 220;
			theFooter.append('<div class="footer-links-showall" id="footer-links-showall">show all +</div>');
			theFooter.addClass('footer-links');
			theFooter.height(collapsedHeight);
			$('#footer-links-showall').bind('click', function() {
				var theHeight;
				var linkText;
				
				if(theFooter.hasClass('open')) {
					theHeight = collapsedHeight;
					linkText = 'show more +';
				} else {
					theHeight = fullHeight;
					linkText = 'show less -';
				}
				$(this).text(linkText);
				theFooter.animate({
					height: theHeight
				}, 200);
				theFooter.toggleClass('open');
			});
		}
	}
});
</script>

