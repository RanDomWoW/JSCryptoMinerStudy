<script type="text/template" id="property-tile-template">
    <article data-eventCategory="ejendom" data-eventAction="klik" data-eventLabel="{{= ItemId }}" data-eventValue="0" data-eventNoninteraction="true" data-brokerId="" data-brokerName="" data-propertyId="{{= ItemId }}" data-propertyType="{{= Type }}" data-propertyZipcode="{{= PostCode }}" data-propertyPrice="{{= Price }}" data-userId="" class="property-tile js-property-tile {{ if(HasBeenSold){ }} property-sold {{ } }} {{ if(IsSaleInProgress){ }} property-sale-in-progress {{ } }} {{ if((DaysInSale <= 8) || (HasNewPrice) && (OpenHouseText)){ }} has--double-splash {{ } }}" data-equalizer-watch>
        {{ if(!HasBeenSold && !IsSaleInProgress){ }}
        <a href="{{= PropertyLink}}" class="tile-link" {{ PropertyLink.search("http") == -1 ? "" :  print("target='_blank'")}}>
        {{ } }}
            <figure class="tile-figure" style="background-image: url('{{ ImageReference ? print(ImageReference.Url) : print( NoPhotoUrl ? NoPhotoUrl.Url : '') }}');">
                <img class="tile-image" src="{{ ImageReference ? print(ImageReference.Url) : print( NoPhotoUrl ? NoPhotoUrl.Url : '') }}" alt="{{ ImageReference ? print(ImageReference.Alt) : ''}}" title="{{ ImageReference ? print(ImageReference.Alt) : ''}}" style="display:none" />
                
                {{ if(HasBeenSold){ }}
                
                    {{ if(ShowSmartSale){ }}
                    <div class="tile-smartsale">
                        <div>Solgt med SalesBooster</div>
                        {{ if(SmartSaleLink && SmartSaleText){ }}
                        <a href="{{= SmartSaleLink}}" class="read-more">{{= SmartSaleText }}</a>
                        {{ } }}
                    </div>
                    {{ } else { }}
                    <div class="tile-sold">
                        {{ if(Type.toLowerCase().search("grund") > -1){ }}
                        <div>Solgt</div>
                        {{ } }}
                        {{ if(Type.toLowerCase().search("grund") === -1){ }}
                        <div>Solgt</div>
                        {{ } }}
                    </div>
                    {{ } }}

                {{ } else if(IsSaleInProgress){ }}

                <div class="tile-sale-in-progress">
                    <div>Købsaftale underskrevet</div>
                </div>

                {{ } else { }}

                <div class="tile-markers">

