            invitecode: '37a4194f-3d11-43ab-8d43-8134a707a9c0',
            title: '订阅我们的Newsletter',
            successMsg: '你已订阅成功，请留意我们（不）定期的情报推送'
        }
        ifaxin.subscribe(option);
        
        var ifaxinBtn = document.querySelector(".subscribe-submit-btn");
        
        if (ifaxinBtn) {
            document.querySelector(".subscribe-email-input").placeholder = "邮箱地址";
        
            ifaxinBtn.addEventListener("click", function() {
              var timer = setInterval(function() {
                if (!document.querySelector(".subscribe-submit-btn")) {
                  clearInterval(timer);
                  document.cookie = "ifaxin_subscribe=hidden;path=/;expires=" + new Date(Date.now() + -1*24*60*60*1000).toGMTString();
                }
              }, 500);
              ifaxinBtn.removeEventListener("click", arguments.callee);
            });
        }
    </script>
</div>

<script type="text/template" id="tmpl-more">
    <!-- {{each items as post}} --><div class="grid-item {{if post.rowad}}rowad{{/if}}">
        {{if post.ad}}
        {{#post.ad}}
        {{else}}
        <article class="entry-item {{if post.advertorial}}entry-item-advertorial{{/if}} {{post.item_type}}"
                data-id="{{post.id}}">
            <div class="entry-meta">
                {{if post.outlink}}
                <a href="{{post.url}}" class="outlink" target="_blank"><span class="icon-outlink"></span>{{post.tag.name}}</a>
                {{else}}
                <span class="topics"><a href="{{post.tag.url}}" class="topic-primary" target="_blank">{{post.tag.name}}</a></span>
                {{/if}}
                <span class="published">{{post.publish_date}}</span>
            </div>
            {{if post.featured}}
            <a href="{{post.url}}" class="entry-body" target="_blank">
                <div class="entry-image">
                    <img src="/public/assets/images/vice/blank.png" data-src="{{post.portrait}}" class="lazy" onerror="img_placeholder(this, 'www')">
                    {{if post.media_type_id == 2 && post.media_duration}}
                        <span class="video-duration">{{post.media_duration}}</span>
                    {{/if}}
                </div>
                <h3 class="entry-title">{{post.title}}</h3>
                <span class="icon-photo"></span>
            </a>
            {{else}}
            <div class="entry-body">
                <a href="{{post.url}}" class="entry-image" target="_blank">
                    <img src="/public/assets/images/vice/blank.png" data-src="{{post.mobile}}" class="lazy" onerror="img_placeholder(this, 'www')">
                    {{if post.media_type_id == 2 && post.media_duration}}
                        <span class="video-duration">{{post.media_duration}}</span>
                    {{/if}}
                </a>
                <h3 class="entry-title"><a href="{{post.url}}" target="_blank">{{post.title}}</a></h3>
                <a class="entry-summary" href="{{post.url}}" target="_blank">{{post.preview}}</a>
            </div>
            {{/if}}
        </article>
        {{/if}}
    </div><!-- {{/each}} -->
</script>

