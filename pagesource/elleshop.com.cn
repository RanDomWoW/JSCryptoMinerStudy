    $(function () {
        $(".search-control").click(function () {
            if ($(".search-wrap").hasClass("on")) {
                $(".search-wrap").removeClass("on");
                $(".search").removeClass("fadeInRight");
            } else {
                $(".search-wrap").addClass("on");
                $(".search").addClass("fadeInRight");
            }
        });

        $(".back-top").click(function () {
            $("body,html").animate({"scrollTop": "0px"});
        }, 200);

        //搜索框热词效果：
        $(".f-search").focus(function () {
            $(this).val("");
        })

        //设置弹框高度为屏幕浏览器高度;
        $('.popup-sidenav').height($(window).height());

    });

    //cart弹框:
    $(".popup-sidenav").show();

    var screenWidth = $(window).width() - 120;//浏览器屏幕的宽度;
    $("#popup-side-cart").css("left", screenWidth);
    $("#popup-side-foot").css("left", screenWidth);

    var cartTimeoutCommon = 0;
    $("#popup-side-cart,#cart-btn").mouseenter(function () {
        cartShowCommon();
        footHideCommon();
        if (cartTimeoutCommon){
            clearTimeout(cartTimeoutCommon);
        };

    });
    $("#cart-btn,#popup-side-cart").mouseleave( function () {

        cartTimeoutCommon = setTimeout(cartHideCommon,300);
    })


    //将购物车弹框展开:
    function  cartShowCommon(){
        $("#popup-side-cart").show();
        $(".side-nav .cart").addClass('active');
        var showAfter = screenWidth - 284;
        $(".side-nav .cart").find(".triangle").show();
        $("#popup-side-cart").animate({"left":showAfter + "px"}),function(){

        }
    }

    function getCartCommon(){

        $.ajax({
            url: 'index.php?route=checkout/getcart',
            success: function (html) {
                $("#popup-side-cart").html(html);
            }
        })

    }
    getCartCommon();


    //将购物车弹框收起:
    function  cartHideCommon(){
        $(".side-nav .cart").removeClass('active');
        var showAfter = screenWidth + 284;
        $(".side-nav .cart").find(".triangle").hide();
        $("#popup-side-cart").animate({"left":showAfter + "px"},200);
    }



    //foot弹框:

    $(".foot-popup-close").click(function () {
        footHideCommon();
    });

    var footTimeoutCommon = 0;
    $("#popup-side-foot,#foot-btn").mouseleave(function () {
        footTimeoutCommon = setTimeout(footHideCommon,300);
    })

    $("#popup-side-foot,#foot-btn").mouseenter(function () {
        cartHideCommon()
        footShowCommon();
        if (footTimeoutCommon){
            clearTimeout(footTimeoutCommon);
        };
    });


    //展开足迹弹框:
    function  footShowCommon() {
        $(".side-nav .foot").addClass('active');
        var showAfter = screenWidth - 284;
        $(".side-nav .foot").find(".triangle").show();
        $("#popup-side-foot").animate({"left":showAfter + "px"});
    }

    //关闭足迹弹框:
    function  footHideCommon() {
        $(".side-nav .foot").removeClass('active');


        var showAfter = screenWidth + 284;
        $(".side-nav .foot").find(".triangle").hide();
        $("#popup-side-foot").animate({"left":showAfter + "px"},200);
    }


    //当有物品加入购物车的时候:购物车滑出滑入共6s时间。
    function cartPlus(){
        cartShowCommon();
        footHideCommon();

        setTimeout(cartHideCommon,3000);

    }



    function clear_history() {
        $.ajax({
            url: 'index.php?route=common/home/clearhistory',
            success: function () {
                alert('已清空');
                window.location.href = window.location.href;
            },
            error: function (xhr, ajaxOptions, thrownError) {
                alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
            }
        });
    }

    //购物车弹框中删除商品提示:
    function removecart_z(cid) {
        open_alert('确定要删除此商品吗?', 2);
        $(".tip-sure").click(function () {
            $.ajax({
                url: 'index.php?route=common/home/clearhistory',
                type: 'post',
                data: 'cid=' + cid,
                success: function () {
                    window.location.href = window.location.href;
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
                }
            });
        })
    }

    function removecart(cid) {
        open_alert('确定要删除此商品吗?', 2);
        $(".tip-sure").click(function () {
            $.ajax({
                url: 'index.php?route=checkout/cart/remove',
                type: 'post',
                data: 'key=' + cid,
                dataType: 'json',
                success: function (json) {
                    window.location.href = window.location.href;
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
                }
            });
        })
    }
