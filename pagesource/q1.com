<script>
    $(document).ready(function() {
        $('.new, .ul-box, .job_list_wrap').removeClass('loading');
        setTimeout(LoadInfo_Part1, 100);
    });
    function LoadInfo_Part1() {
        var listLen = NewsData.length - 1;
        var i = 0;
        if (NewsData.length == 0) {
            $("#newslist_part1").append('<li>暂时没有信息</li>');
        } else {
            for (; i<=listLen; i++) {
                var css = "";
                var shortstr = '';
                if (i == 0) {
                    css = 'class="new-li-active"';
                    shortstr = '<p><a target="_blank" href="http://www.q1.com' + NewsData[i].LinkUrl + '">' + cutstr(NewsData[i].Content, 160) + '</a>&nbsp;&nbsp;<a class="detail" href="http://www.q1.com' + NewsData[i].LinkUrl + '" target="_blank">详情</a></p>';
                }
                $("#newslist_part1").append('<li ' + css + '>' + i + '<a target="_blank" href="http://www.q1.com' + NewsData[i].LinkUrl + '" class="title">' + NewsData[i].Title + '</a><div class="date">' + NewsData[i].CreateTime
                        + '</div>' + shortstr + '</li>');
            }
        }
    }
    function cutstr(str, len) {
        str = str.replace(/<[^>]+>/g, "");
        var str_length = 0;
        var str_len = 0;
        str_cut = new String();
        str_len = str.length;
        for (var i = 0; i < str_len; i++) {
            a = str.charAt(i);
            str_length++;
            if (escape(a).length > 4) {
                //中文字符的长度经编码之后大于4
                str_length++;
            }
            str_cut = str_cut.concat(a);
            if (str_length >= len) {
                str_cut = str_cut.concat("...");
                return str_cut;
            }
        }
        //如果给定字符串小于指定长度，则返回源字符串；
        if (str_length < len) {
            return str;
        }
    }
    ~function($) {
        var picItem = $('img[_src]');
        var arr = [];
        var picItemIndex = 0;
        var i = 0;

        picItem.each(function() {
            arr.push($(this).attr('_src'));
        });

        var len = arr.length;

        for(; i<len; i++) {
            var oImg = new Image();
            oImg.onload = function() {
                picItem.eq(picItemIndex)[0].src = picItem.eq(picItemIndex).attr('_src');
                picItemIndex++;
            }
            oImg.src = arr[i];
        }
    }(jQuery);
</script>
