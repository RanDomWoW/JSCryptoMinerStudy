        <script type="text/template" id="media_popup_overlay_template">
    <div class="overlay" data-active-class="overlay__opened">
        <span class="overlay-close">close</span>
        <div class="media-popup js-media-popup-content">

            <div class="slider slider__thin js-media-slider">
                <div class="slider_holder js-slider">

                    <% forEach(data.sliderItems, function(item) { %>
                    <span class="slider_item">
                        <% if (item.block_type === MEDIA_TYPE_VIDEO) { %>
                            <div class="video">
                                <%= item.player %>
                            </div>
                        <% } else { %>
                            <img class="slider_item-image" src="<%= item.original_url %>" alt="<%= item.alt %>">
                        <% } %>
                    </span>
                    <% }) %>

                </div>

                <% if (data.media_type === MEDIA_TYPE_GALLERY) { %>
                <p class="slider_counter js-paging">
                    1 / <%= data.items.length %>
                </p>
                <% } %>

            </div>

            <% if (data.title) { %>
            <h1 class="media-popup_header"><%= data.title %></h1>
            <% } %>

            <% if (data.description) { %>
            <p class="media-popup_text"><%= data.description %></p>
            <% } %>

            <div class="media-popup_action">

                <% if (data.share_url && data.show_share_buttons) { %>

                

                
                
                

                <div class="share">
                    <div class="a2a_kit a2a_kit_size_32 a2a_default_style"
                         data-a2a-url="<%= data.share_url %>"
                         data-a2a-title="<%= data.title %>"
                         data-a2a-color-main="555555"
                         data-a2a-color-link-text="FFFFFF"
                         data-a2a-icon-color="transparent">
                            <a class="a2a_button_facebook a2a_counter js-analytics-item"
                                data-gtm-event-to-listen="click"
                                data-gtm-event-action="click"
                                data-gtm-event-category="share_button"
                                data-gtm-event-label="share_facebook"
                            ></a>
                            <a class="a2a_button_twitter a2a_counter js-analytics-item"
                                data-gtm-event-to-listen="click"
                                data-gtm-event-action="click"
                                data-gtm-event-category="share_button"
                                data-gtm-event-label="share_twitter"
                            ></a>
                            <a class="a2a_button_google_plus a2a_counter js-analytics-item"
                                data-gtm-event-to-listen="click"
                                data-gtm-event-action="click"
                                data-gtm-event-category="share_button"
                                data-gtm-event-label="share_google_plus"
                            ></a><a class="a2a_dd js-analytics-item" href="https://www.addtoany.com/share"
                                data-gtm-event-to-listen="click"
                                data-gtm-event-action="click"
                                data-gtm-event-category="share_button"
                                data-gtm-event-label="share_other"
                            ></a></div>
                </div>
                <% } %>

                <% if (data.media_type === MEDIA_TYPE_WALLPAPER) { %>
                <div class="media-popup_download js-download-action">
                    <div class="select js-select select__right-indent" data-active-class="select__opened">
                        
                        <span class="select_label js-perfect" style="display: none;">Perfect for your screen</span>
                        <span class="select_button js-button">
                            <span class="js-resolution"></span>
                            <span class="select_arrow">
                                <span class="ico-arrow-down">down</span>
                            </span>
                        </span>
                        <ul class="select_list">
                            <% forEach(data.items, function(item) { %>
                            <li class="select_item js-item"><%= item.width %> x <%= item.height %></li>
                            <% }) %>
                        </ul>
                    </div>
                    
                    <a href="#" class="button button__download js-download-button" target="_blank" download>Download</a>
                </div>
                <% } %>

            </div>

            <% if (data.media_type === MEDIA_TYPE_GALLERY) { %>
            <ul class="media-gallery js-media-gallery">
                <% forEach(data.items, function(item) { %>
                <% var modifier = (item.block_type === MEDIA_TYPE_VIDEO ? MEDIA_TYPE_VIDEO : 'photo') %>
                <li class="media-gallery_item js-item" data-active-class="media-gallery_item__active">
                    <span class="media-gallery_link">
                        <div class="media-gallery_img-holder">
                            <img src="<%= item.thumbnail_url %>" alt="<%= item.alt %>" class="media-gallery_img">
                        </div>
                        <span class="entry-type entry-type__<%= modifier %>"></span>
                    </span>
                </li>
                <% }) %>
            </ul>
            <% } %>

        </div>
    </div>
</script>
        <script type="text/template" id="image_popup_overlay_template">

    <div data-active-class="">
        <div class="js-image-popup-content">
            <div class="dialog">
                <div class="dialog_holder dialog_holder__centered">
                    <div class="dialog_content-holder">
                        <span class="dialog_close"></span>
                        <img class="dialog_image" src="<%= data.original_url %>" alt="<%= data.alt %>" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>

        
