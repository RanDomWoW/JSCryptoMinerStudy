        function loadGoogleMaps(){
        var mapsScript = document.createElement('script');
        mapsScript.type = 'text/javascript';
        mapsScript.src = 'https://maps.googleapis.com/maps/api/js?key=AIzaSyDpUxj2k6VK9MgvvJwwqC8b6hqPXWa8iK8&sensor=false' + '&libraries=places&signed_in=true&callback=initialize';
        document.body.appendChild(mapsScript);
        }
        function initialize() {
            var options = {
                types: ['(regions)'], 
                componentRestrictions: {
                    country: "us"
                }
            };
            var input = document.getElementById('searchTextField');
            // this is where the error is triggering
            var autocomplete = new google.maps.places.Autocomplete(input, options);
            google.maps.event.addListener(autocomplete, 'place_changed', function() {
                var place = autocomplete.getPlace();
                var address_components = place.address_components;
                var components = {};
                $.each(address_components, function(k, v1) {
                    jQuery.each(v1.types, function(k2, v2) {
                        components[v2] = v1
                    });
                })
                var dlat = place.geometry.location.lat(),
                    dlng = place.geometry.location.lng();
                document.getElementById('lat').value = dlat;
                document.getElementById('lng').value = dlng;
            });         
        }
    </script>

