			  <script>
 $(document).ready(function(){
 
	var year = parseInt($("#current_year").val());
	var month = parseInt($("#current_month").val());
	var day = parseInt($("#current_day").val());
	var check_request = $("#check_request").val();
	var request_date = $("#request_date").val();
	
	AjaxCal(year, month, day, check_request, request_date);

    $("#prev").click(function(){

		if(month == 1){
			year = year - 1;
			month = 12;
		}else{
			month = month - 1;
		}
		 AjaxCal(year, month, day, check_request, request_date);
    });
	
	 $("#next").click(function(){

		if(month == 12){
			year = year + 1;
			month = 1;
		}else{
			month = month + 1;
		}
		  AjaxCal(year, month, day, check_request, request_date);
    });
});

function AjaxCal(year, month, day, check_request, request_date){
	var month_info = new Array(  "January", "February",  "March",  "April",  "May",  "June",  "July",  "August",   "September",  "October",  "November",  "December" ) ;
	return  $.ajax({
                 url: "http://www.thestandard.com.hk/ajax_calender.php",
                data: {year: year, month: month, day: day, check_request : check_request, request_date:request_date},
                type:"POST",
                dataType:'html',

                success: function(data){
					$("#current_year").val(year);
					$("#current_month").val(month);
					$("#curr_dateMonth").text(month_info[month-1] + ' ' +year);
				    $("#table1 > tbody").html("");
					$("#table1 > tbody").append(data);
                },
                 error:function(xhr, ajaxOptions, thrownError){ 
					console.log('ajax calender error.');
					console.log(xhr.status);
                    //alert(xhr.status); 
                    //alert(thrownError); 
                 }
            });
}
 </script>
