<script> var API_BASEURL = "https://webapi.toyotabharat.com/" </script> 
<script src="https://static.toyotabharat.com/js/forms/api.home.ob.js"></script> 
<script src="https://static.toyotabharat.com/js/common.js"></script> 
<script src="https://static.toyotabharat.com/js/jquery.easing.js"></script> 
<script src="https://static.toyotabharat.com/js/wow.min.js"></script> 
<script src="https://static.toyotabharat.com/js/jquery.lazy.min.js"></script> 
<script src="https://static.toyotabharat.com/js/owl.carousel.js"></script> 
<script src="https://smsgateway.toyotabharat.com/api/gateway/otpapiscript"></script> 
<script type="text/javascript">

        //Website Feedback
        $("#closeFeedbackTriangle").click(function () {
            $("#feedbackTriangle").hide("slow");
        });

        $("#openForm").click(function () {
            $("#feedbackTriangle").css("display", "none");
            $("#webFeedbackBox").show();
        });

        $("#closeFeedBackBox").click(function () {
            $("#feedbackTriangle").show("slow");
            $("#webFeedbackBox").css("display", "none");
        });

        $("input:radio[name ='webFeedbackRating']").click(function () {
            validateRating('#ulRadioGroup');
        });

        $(window).bind('scroll', function () {

            if ($(window).scrollTop() > 807) {

                $('#feedback-container').show("slow");

            }

            if ($(window).scrollTop() + $(window).height() == $(document).height()) {
                $('#feedback-container').hide("slow");
            }
        });

        //SUBMIT FORM DATA
        function submitForm() {

            var rating = $("input:radio[name ='webFeedbackRating']:checked").val();
            var feedback = $("#txtFeedback").val();
            var mobile = $("#txtMobile").val();
            var otp = $("#txtOtp").val();

            apiSubmitWebsiteFeedback(rating, feedback, mobile, otp);

        }
        //Form submit function ends

        var isFocus;
        validate = function () {
            isFocus = false;
            var save = true;

            if (!validateRating('#ulRadioGroup')) save = false
            if (!validateFeedback('#txtFeedback', true)) save = false
            if (!validateMobileNumber("#txtMobile", true)) save = false
            if (validateMobileNumber("#txtMobile", true)) {
                //This function will be called from the SMS API Gateway
                if (!validateOtp($('#txtMobile')[0], $('#txtOtp')[0])) save = false
            }

            if (save) {
                submitForm();
            }
            return save;
        }

        validateRating = function (obj) {
            hideError(obj);
            var Selectedvalue = $("input:radio[name ='webFeedbackRating']:checked").val();

            if (!Selectedvalue) {
                showError(obj, "Please provide rating");
                return false;
            }

            return true;
        }

        validateFeedback = function (obj, triggerFocus) {
            hideError(obj);
            var Selectedvalue = $("input:radio[name ='webFeedbackRating']:checked").val();

            if (Selectedvalue && Selectedvalue != 'Good' && Selectedvalue != 'Excellent') {
                if ($(obj).val().trim().length == 0) {
                    showError(obj, "Please let us know how can we improve your experience");
                    if (!isFocus) { if (triggerFocus) $(obj).focus(); isFocus = true }
                    return false;
                }
                return true;

            }
            return true;
        }

        function generateOtp() {
            if (validateMobileNumber(document.getElementById('txtMobile'))) {
                requestOtp(document.getElementById('txtMobile'), document.getElementById('btnVerify'));
            }
            return false;
        }

        validateMobileNumber = function (obj, triggerFocus) {
            hideError(obj);
            if ($(obj).val().trim().length == 0) {
                showError(obj, "Please enter mobile number");
                if (!isFocus) { if (triggerFocus) $(obj).focus(); isFocus = true }
                return false;
            }

            if ($(obj).val().trim().length != 10) {
                showError(obj, "Please enter Valid mobile number");
                if (!isFocus) { if (triggerFocus) $(obj).focus(); isFocus = true }
                return false;
            }
            return true;
        };

