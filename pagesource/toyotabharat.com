            if (docWidth > 767) {
                $("#videoBanner").css({ "width": "100%", "height": "766px", "display": "block", "overflow": "hidden" });
            }
            else {
                $("#videoBanner").css({ "width": "100%", "height": vidHeight, "display": "block", "overflow": "hidden" });
            }
            event.target.playVideo();
            event.target.setPlaybackQuality('default');
