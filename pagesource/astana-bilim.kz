		$navbarDropdowns.removeClass('open');
		// отключаем hover эффект у bootstrap'a
		$navbarLinks.on('hover', function(e){e.stopPropagation();});
	}
	
	function defaultBootstrapDropdown(){ // возращаемся к стандартному Bootstrap dropdown
		$navbarLinks.off();
		
