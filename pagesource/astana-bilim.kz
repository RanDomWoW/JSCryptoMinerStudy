	// functions
	function upgradeBootstrapDropdown(){ // перезапись bootstrap hover and click dropdown
		$navbarLinks.on('click', function(e){
			var $elem = $(this),
				$parentElem = $elem.parents('.dropdown'),
				$elemDropdownMnu = $parentElem.find('.dropdown-menu');
			
			if($elemDropdownMnu.length > 0){ // проверка на наличие подменю
				if($elemDropdownMnu.is(':hidden')){
					hideDropdownMnus(); // скрываем блоки
					
					$parentElem.addClass('bmc-dropdown-open');
					$elem.find('.bmc-icon-chevron i').removeClass('fa-chevron-down').addClass('fa-chevron-up');
					$elemDropdownMnu.slideDown('fast');
				}
				else {
					$parentElem.removeClass('bmc-dropdown-open');
					$elem.find('.bmc-icon-chevron i').removeClass('fa-chevron-up').addClass('fa-chevron-down');
					$elemDropdownMnu.slideUp('fast');
				}
				e.preventDefault();
				e.stopPropagation();
			}
		});
