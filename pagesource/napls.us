	function emergencyAnnouncement(){
		var shutoff = 767;
		if($('#hp-announcement .ui-widget-detail ul.ui-articles').children().length <= 0){
			$('#hp-announcement-outer').hide();
		}else{
			$('#hp-announcement-outer').show()
			
			var getWidth = $(window).width();
			if( getWidth >= shutoff ){
				var anncHeight = $('#hp-announcement-outer').outerHeight() + 3;
				
				$('#gb-sticky-header').css({
					"top": anncHeight,
					"position": "absolute"
				});
				$("#gb-page").css('padding-top', 215 + anncHeight);
				
				var anncHeight = $('#hp-announcement-outer').outerHeight() + 3;
				
				var scrollHookOn = anncHeight;
				var scrollHookOff = anncHeight - 1;
				
				$(window).off("scroll").scroll(function(e){	
					var top = $(window).scrollTop();
						//check where the window is
					if(top >= scrollHookOn  && !$('#gb-sticky-header').hasClass('on')){ //make it sticky
						$('#gb-sticky-header')
						.addClass('on')
						.css({
							"top": 0,
							"position": "fixed"
						});
					}else if( top <= scrollHookOff  && $('#gb-sticky-header').hasClass('on')){ //unstick it
						$('#gb-sticky-header')
						.removeClass('on')
						.css({
							"top": anncHeight,
							"position": "absolute"
						});
					}
				});
			}else{ 	
				$(window).off("scroll");
				$('#gb-sticky-header').removeClass('on');
				$('#gb-sticky-header').css({
					"top": 0,
					"position": "relative"
				});
				$("#gb-page").css('padding-top', 0);
			}
		}
	}
    
    function streamingVideo() {
    	if($("#hp-slideshow").length && true) {
        	$("#gb-bg-image").html('<div id="player"></div>');
            
            var streamingVideoInfo = 	'<div id="hp-streaming-video-overlay-outer">' +
                                            '<div id="hp-streaming-video-overlay">' +
												(($.trim("Home of the Eagles!") != "") ? '<h2>Home of the Eagles!</h2>' : '') +
                                                (($.trim("#NAPride") != "") ? '<p>#NAPride</p>' : '') +
                                                ((true) ? '<p><a href="https://goo.gl/E7ye1F" target="_self">Spring Break March 26 - April 2 </a></p>' : '') +
                                            	'<button id="hp-streaming-video-button" class="pause" aria-label="Pause Background Video"></button>' +
                                            '</div>' +
                                            '<button id="hp-streaming-video-scroll" tabindex="-1" aria-hidden="true">Scroll Down</button>' +
                                        '</div>';
                                        
            $("#hp-slideshow").html(streamingVideoInfo);
            
            var options = {
                id: 260292390,
                title: false,
                byline: false,
                autoplay: true,
                loop: true
            };
            
            var player = new Vimeo.Player("player", options);
			
			// VIMEO PLAYER IS READY
            player.ready().then(function() {
                // MUTE THE VIDEO
            	player.setVolume(0);
                
                // PLAY/PAUSE VIDEO
                $(document).on("click keydown", "#hp-streaming-video-button", function(e) {
                    if(allyClick(e)) {
                        e.preventDefault();
                        if($(this).hasClass("pause")) {
                            player.pause();
                            $(this).removeClass("pause").addClass("play").attr("aria-label", "Play Background Video");
                        } else {
                            player.play();
                            $(this).removeClass("play").addClass("pause").attr("aria-label", "Pause Background Video");
                        }
                    }
                });
                
                // SCROLL TO CONTENT
                $("#hp-streaming-video-scroll").click(function(e){
                    e.preventDefault();
                    $("html, body").animate({
                    	scrollTop: $(".hp-content-outer.top").offset().top - 215
                    }, "slow");
                });
            });
            
            Promise.all([player.getVideoWidth(), player.getVideoHeight()]).then(function(dimensions) {
                var videoWidth = dimensions[0];
                var videoHeight = dimensions[1];
                
                videoResize(videoWidth, videoHeight);
                $(window).resize(function() { videoResize(videoWidth, videoHeight); });
            });
        } else {
        	$("#hp-slideshow-outer").append('<button id="hp-streaming-video-scroll" tabindex="-1" aria-hidden="true">Scroll Down</button>');
            
            // SCROLL TO CONTENT
            $("#hp-streaming-video-scroll").click(function(e){
                e.preventDefault();
                $("html, body").animate({
                    scrollTop: $(".hp-content-outer.top").offset().top - 215
                }, "slow");
            });
        }
    }
    
    function videoResize(videoWidth, videoHeight) {
    	var videoRatio = videoHeight / videoWidth;
        var screenWidth = $(window).width();
        var screenHeight = $(window).height();

        var adjustedVideoWidth = screenWidth;
        var adjustedVideoHeight = adjustedVideoWidth * videoRatio;

        if(adjustedVideoHeight < screenHeight) {
            adjustedVideoHeight = screenHeight;
            adjustedVideoWidth = adjustedVideoHeight / videoRatio;
        }

        var centerWidth = (screenWidth - adjustedVideoWidth) / 2;
        var centerHeight = (screenHeight - adjustedVideoHeight) / 2;

        // APPLY THE DIMENSIONS
        $("#player iframe").css({
        	"width": adjustedVideoWidth,
            "height": adjustedVideoHeight,
            "margin-top": centerHeight,
            "margin-left": centerWidth
        });
    }
    
    function allyClick(event) {
        if(event.type == "click") {
            return true;
        } else if(event.type == "keydown" && (event.keyCode == csKeyCodes.space || event.keyCode == csKeyCodes.enter)) {
            return true;
        } else {
            return false;
        }
    }
