	function compileNav(){
		if (!$("#gb-rs-menu-dropdown #gb-rs-menu-channels li").length) {
			// GRAB ALL OF THE CHANNELS AND DROPDOWNS
			$("li.sw-channel-item").each(function (){
				$("#gb-rs-menu-dropdown #gb-rs-menu-channels").append("<li>" + $(this).html() + "</li>");
				var navDropdown = $(this).find("ul.sw-channel-dropdown");
				if ($(navDropdown).length) {
					$("#gb-rs-menu-dropdown #gb-rs-menu-channels > li").last().append("<div class='gb-rs-list-bullet'><div class='gb-rs-list-bullet-inner'></div></div>");
				}
				$("#gb-rs-menu-dropdown #gb-rs-menu-channels .sw-channel-dropdown").removeClass("sw-channel-dropdown").addClass("gb-rs-channel-sublist");
			});
			
			// EXPAND THE MENU
			$("#gb-rs-menu-btn").click(function(){
				if (!$(this).hasClass("closed")) {
					$(this).removeClass("closed").addClass("open");
					$("#gb-rs-menu-dropdown").slideToggle();
				} else {
					$("#gb-rs-menu-dropdown").slideToggle(function(){
						$(this).removeClass("open").addClass("closed");
					});
				}
			});
			
			// EXPAND AN INNER MENU ITEM 
			$(".gb-rs-list-bullet").click(function(){
				var clickedListItem = $(this);
				var subListBody = $(this).prev($(".gb-rs-channel-sublist"));
				if(!$(this).hasClass("open")){
					$(".gb-rs-list-bullet").removeClass("open").addClass("closed");
					$(".gb-rs-channel-sublist").slideUp(function(){
						clickedListItem.removeClass("closed").addClass("open");
					});
					subListBody.slideDown();
				} else {
					clickedListItem.removeClass("open").addClass("closed");
					subListBody.slideUp();	
				}
				$(".gb-rs-list-bullet").css("transform", "rotate(45 deg)");
			});
		}
	}
	
	function bgImgResize(){
		var screenWidth = $(window).width();
		var screenHeight = $(window).height();
		var bgImgWidth = 0, bgImgHeight = 0;
		
		var initW = 1500, initH = 985;
		var ratio = initH / initW;
		
		bgImgWidth = screenWidth;
		bgImgHeight = screenWidth * ratio;
		
		if(bgImgHeight < screenHeight){
			bgImgHeight = screenHeight;
			bgImgWidth = bgImgHeight / ratio;
		}
		
		$("#gb-bg-image img").css({"width": bgImgWidth + "px", "height": bgImgHeight + "px"});
	}
	
