<script>
    var selectedPollAnswer = 0;
    var otherAnswerId = 0;

    function SetPollAnswer(answerId) {

        selectedPollAnswer = answerId;

        if (answerId == otherAnswerId) {
            $('#OtherAnswerText').show();
        } else {
             $('#OtherAnswerText').hide();
        }
    }


    function commitPollAnswer() {

        if (selectedPollAnswer != 0 && selectedPollAnswer == otherAnswerId && ($('#OtherAnswerText').val() == null || $('#OtherAnswerText').val() == "")) {
            swal({
                //title: "Uyarı!",
                text: "Lütfen diğer seçeneğinizi belirtin!",  
                    //"SelectedPollAnswer: " + selectedPollAnswer + " - " + 
                    //"otherAnswerId: " + otherAnswerId,
                imageUrl: "/Content/Napolyon70/img/kazandin.png",
                confirmButtonColor: '#00B6A6',
                confirmButtonText: 'Tamam',
                closeOnConfirm: true});
            return;
        }

        $.ajax({
            url: "/AnaSayfa/GetPollResults/",
            data: { selectedAnswerId: selectedPollAnswer, pollId: 38, otherAnswerText:  $('#OtherAnswerText').val()},
            cache: false,
            type: "POST",
            success: function(data) {
                if (data == "loginRequired") {
                    window.location.href = "/Uye/Giris?ReturnUrl=/Anasayfa/Giris";
                }
                else if (data.Status == "loginForSurvey") {
                    window.location.href = "/Uye/Giris?ReturnUrl="+data.ReturnUrl;
                }
                else if (data.Status == "redirectForSurvey") {
                    window.location.href = data.ReturnUrl;
                }
                else if (data == "redirectToMiniSurvey") {
                    window.location.href = "/Uye/Giris?ReturnUrl=/Anasayfa/Giris";
                }
                else if (data == "nonMemberAnswerRecorded") {
                    showNonMemberPollCommitted();
                } else {
                    $(".rPoll").html(data);
                    $(".aPoll").hide();
                    $(".rPoll").show().addClass("animated bounceInRight");
                }
            },
            error: function(error) {
                console.log(error);
            }


        });
    }

    function showNonMemberPollCommitted() {
        $(".qPoll").hide();
        $(".aPoll").hide();
        $(".mPoll").show().addClass("animated bounceInRight");
    }

    $(document).ready(function () {




            //<Text>

        if (selectedPollAnswer > 0)
        {
            commitPollAnswer(); // ignores if member vote exists
        }
        else
        {
            $(".radio input[type='radio']").click(function() {
                //$("#summitPoll").removeClass("disabled-poll-btn");
                $("#summitPoll").removeAttr("disabled");
            });

            $("#summitPoll").click(function() {
                //alert(selectedPollAnswer);

                commitPollAnswer();

            });
        }
        //</Text>
        //}
    });

</script>
