$(".cPost .ipsComment_controls li.ipsHide:only-child").closest('.ipsItemControls').addClass('iIC-no-buttons');
$(".cPost .ipsReact .ipsReact_blurb.ipsHide:only-child").closest('.ipsItemControls').addClass('iIC-no-react');



});</script>
		
		
