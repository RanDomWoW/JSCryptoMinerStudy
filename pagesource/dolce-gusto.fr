(function($){
    $(document).ready(function(){
        function daysUntilNextYear() {
            var oneDay = 24*60*60*1000;

            var today = new Date();
            var nextYearFirstDay = new Date(new Date().getFullYear() + 1, 0, 1);

            return Math.round(Math.abs((today.getTime() - nextYearFirstDay.getTime())/(oneDay)));
        }

