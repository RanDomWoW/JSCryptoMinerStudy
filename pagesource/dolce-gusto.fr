        function (v,elm) {
            if (Validation.get('IsEmpty').test(v)) {
            return true;
        } else {
            var dateFormat = jQuery(elm).datepicker('option', 'dateFormat');
            var dateString = jQuery(elm).datepicker('getDate');
            var dateFormated = jQuery.datepicker.formatDate(dateFormat, dateString);

            return (v == dateFormated);
        }        });
     Validation.add('validate-one-required-by-name',
        'Ce champ est obligatoire',
        function (v,elm) {
            var inputs = $$('input[name="' + elm.name.replace(/([\\"])/g, '\\$1') + '"]');

                var error = 1;
                for(var i=0;i<inputs.length;i++) {
                    if((inputs[i].type == 'checkbox' || inputs[i].type == 'radio') && inputs[i].checked == true) {
                        error = 0;
                    }

                    if(Validation.isOnChange && (inputs[i].type == 'checkbox' || inputs[i].type == 'radio')) {
                        Validation.reset(inputs[i]);
                    }
                }

                if( error == 0 ) {
                    return true;
                } else {
                    return false;
                }        });
     Validation.add('validate-dob-dmy-textbox',
        'Vous n\'avez pas l\'âge minimum requis',
        function (v,elm) {
            if (Validation.get('IsEmpty').test(v)) {
        return true;
    } else {
        var dateString = jQuery(elm).datepicker('getDate');
        var dateFormated = jQuery.datepicker.formatDate('dd/mm/yy', dateString);

        var dobDay = dateFormated.match(/(\d{2})\/(\d{2})\/(\d{4})/)[1];
        var dobMonth = (dateFormated.match(/(\d{2})\/(\d{2})\/(\d{4})/)[2]) - 1;
        var dobYear = dateFormated.match(/(\d{2})\/(\d{2})\/(\d{4})/)[3];

        var dobDate = new Date(dobYear, dobMonth, dobDay);

        var todayDate = new Date();

        var years = todayDate.getFullYear() - dobDate.getFullYear();
        var months = todayDate.getMonth() - dobDate.getMonth();

        if (months < 0 || (months === 0 && todayDate.getDate() < dobDate.getDate())) {
            years--;
        }

        if (years >= 18) {
            return true;
        } else {
            return false;
        }
    }        });
