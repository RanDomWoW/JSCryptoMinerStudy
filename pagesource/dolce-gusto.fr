            (function($){
                $(document).ready(function() {
                    var $form = $('#newsletter-validate-header');
                    var isSubscribed = Mage.Cookies.get('custEmailIsSubscribed');
                    var newsletterSubscriberFormDetail = new VarienForm('newsletter-validate-header');

                    var customerEmail = null;
                    if(typeof emailGlobal !== 'undefined' && emailGlobal != ''){
                        customerEmail = emailGlobal;
                    }


                    if (customerEmail) {
                    var email = customerEmail;
                    jQuery.get( "/customer/account/isLoggedInAjax", function(data) {
                        if (data["logged_in"]) $form.find('input').attr('readonly', 'readonly');
                    }, "json");
                    $form.find('input').val(email);

                        if (isSubscribed) {
                            $form.find('button').attr('readonly','readonly');
                        }
                    } else {
                        $form.find('input')
                            .val('Adresse e-mail')
                            .attr('onfocus', "this.value=='Adresse e-mail' ? this.value='' : ''")
                            .attr('onblur', "this.value=='' ? this.value='Adresse e-mail' : ''");
                    }

                    $form
                        .on('submit', function(e) {
                            e.preventDefault();
                            processSubscribeForm() ;
                        })
                        .find('button, [type=submit]').on('click',function(e){
                            e.preventDefault();
                            processSubscribeForm() ;
                        })
                        .bind('keypress', function (e) {
                            var code = e.keyCode || e.which;
                            if (code == 13) {
                                e.preventDefault();
                                $form.find('[type=button], [type=submit]').click();
                            }
                        });

                    var processSubscribeForm = function() {
                        var formButton = $form.find('button');
                        var newsletterButton = $('#newsletter-validate-detail').find('button')
                        if (!isSubscribed &&
                            newsletterSubscriberFormDetail.validator.validate() &&
                            !formButton.hasClass('disabled') &&
                            !newsletterButton.hasClass('disabled'))
                        {
                            formButton.addClass('disabled');
                            newsletterButton.addClass('disabled');
                            $.post(
                                $form.attr('action'),
                                $form.serialize(),
                                function(data) {
                                    window.location.reload();
                                }
                            );
                        }
                    }
                });
            })(jQuery);
        </script>
