<script type="text/javascript">
    (function($){
        $(document).ready(function() {
            var $form = $('#newsletter-validate-detail');
            var isSubscribed = Mage.Cookies.get('custEmailIsSubscribed');
            var newsletterSubscriberFormDetail = new VarienForm('newsletter-validate-detail');
            var customerEmail = null;
            if(typeof emailGlobal !== 'undefined' && emailGlobal != ''){
                customerEmail = emailGlobal;
            }

        if (customerEmail != null) {
            var email = customerEmail;
            $form.find('input').val(email).attr('readonly', 'readonly');

                if (isSubscribed) {
                    $form.find('button').attr('readonly', 'readonly');
                }
            } else {
                $form.find('input')
                    .val('Adresse e-mail')
                    .attr('onfocus', "this.value=='Adresse e-mail' ? this.value='' : ''")
                    .attr('onblur', "this.value=='' ? this.value='Adresse e-mail' : ''");
            }

            $form
                .on('submit', function (e) {
                    e.preventDefault();
                    processSubscribeForm();
                })
                .find('button, [type=submit]').on('click', function (e) {
                e.preventDefault();
                processSubscribeForm();
            });

            var processSubscribeForm = function () {
                var formButton = $form.find('button');
                var newsletterButton = $('#newsletter-validate-header').find('button')
                if (!isSubscribed &&
                    newsletterSubscriberFormDetail.validator.validate() && !formButton.hasClass('disabled') && !newsletterButton.hasClass('disabled')) {
                    formButton.addClass('disabled');
                    newsletterButton.addClass('disabled');
                    $.post(
                        $form.attr('action'),
                        $form.serialize(),
                        function (data) {
                            window.location.reload();
                        }
                    );
                }
            }

            /**
             * Handles footer newsletter disclaimer display
             */
            var newsletterConsentFooter = $('#newsletter_consent_footer');
            $('[type=text],[type=email]', $form)
                .on('focus', function (event) {
                    if (newsletterConsentFooter.not(':visible')) {
                        newsletterConsentFooter.slideDown('fast');
                        $('html,body').animate({
                            scrollTop: newsletterConsentFooter.offset().top
                        }, 200);

                    }
                })
                .on('blur', function (event) {
                    if (newsletterConsentFooter.is(':visible')) {
                        newsletterConsentFooter.slideUp('fast');

                    }
                });
        });
    })(jQuery);
</script>
            
<script>
