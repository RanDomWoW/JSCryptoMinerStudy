<script type="text/javascript">
    Enterprise.TopCart.initialize('topCartContent');

    jQuery(document).ready(function(){
        var $topCart = jQuery('#top-cart');
        var $topCartLink = jQuery('#top-cart > a').first();
        var $topCartBasketLink = jQuery('#top-cart .shopping-action');
        var focusInTimer, focusOutTimer, closeOnBlur;

        $topCart
            .on('focus','>a', function() {
                focusInTimer = setTimeout(function() {
                    if (!$topCartLink.hasClass('expanded')) {
                        Enterprise.TopCart.onElementMouseClick();
                    }
                }, 300);
            })
            .on('blur','a, button', function() {
                closeOnBlur = true;
                focusOutTimer = setTimeout(function() {
                    if (closeOnBlur && $topCartLink.hasClass('expanded') && !jQuery(document.activeElement).parents('#top-cart').length) {
                        Enterprise.TopCart.onElementMouseClick();
                    }
                }, 300);
            })
            .on('click', function(event) {
                //Clicking inside the minicart, the cart button will lose the focus, but at the same time we don't
                //want to close the minicart so we set the <<closeOnBlur>> variable to false.
                closeOnBlur = false;
                event.stopPropagation();
            });

        // close the minicart when click outside it
        jQuery('body').on('click', function(event) {
            if($topCartLink.hasClass('expanded') && jQuery(event.target).closest($topCart).length < 1) {
                Enterprise.TopCart.onElementMouseClick();
            }
        });

        $topCartBasketLink.on('click', function(e) {
            $topCart.off('focusout');
            $topCart.off('focusin');
            $topCartLink.addClass('redirecting');
        });
    });

    
    </script>
