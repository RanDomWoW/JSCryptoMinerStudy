<form id="newsletter-validate-detail" action="https://www.dolce-gusto.fr/advancednewsletter/index/handleSubcribeAjax/" method="post">
    <div id="footer-input">
        <input type="text" id="newsletter-header" name="email" class="top-input-text validate-email" autocomplete="off" />
        <button type="button" class="button account-action newsletter-footer-button capitalization-control" title="Je m'inscris">Je m'inscris</button>
        <div id="newsletter_consent_footer">
                    </div>
    </div>
