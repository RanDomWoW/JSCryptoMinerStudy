            function checkPsnId() {

        var PsnId = document.getElementById("PsnId").value;
        //alert(PsnId);

        var UPPERCASE = "ABCDEFGHJKLMNPQRSTUVXYWZIO"; //請注意W的位置,I和O二字母置最未
        var first_dd = UPPERCASE.indexOf(PsnId.substr(0, 1)) + 10;
        var second_d = UPPERCASE.indexOf(PsnId.substr(1, 1)) % 10;

        if (PsnId.length != 10) {
            alert("請輸入身份證號共10碼");
            return (false);
        }
        if (PsnId.substr(0, 1) < 'A' || PsnId.substr(0, 1) > 'Z') {
            alert("身份證號首碼限英文大寫 A 到 Z");
            return (false);
        }
        //alert(PsnId.substr(1,1));
        switch (PsnId.substr(1, 1)) {
            case '1':
            case '2':
                if (isNaN(PsnId.substr(1, 9))) {
                    alert("身份證號末9碼限數字");
                    return (false);
                }
                PsnId = first_dd + PsnId.substr(1, 9); //身份證號PsnId已變11碼
                break;
            case 'A':
            case 'B':
            case 'C':
            case 'D':
                if (confirm("若您為外籍人士,請按(確定)鍵, 若否,請按(取消) ? ") == false) {
                    alert("身份證號第二碼有誤");
                    return (false);
                }
                if (isNaN(PsnId.substr(2, 8))) {
                    alert("外僑統一證號末8碼限數字");
                    return (false);
                }
                PsnId = first_dd + "" + second_d + PsnId.substr(2, 8); //外僑統一證號已變11碼,""強制型別轉換
                break;
            default:
                alert("身份證號第二碼有誤");
                return (false);
        } //switch

        //checksum
        //alert(PsnId);
        var checksum_id = parseInt(PsnId.substr(0, 1)) * 1;
        for (i = 1; i <= 9; i++) {
            checksum_id += parseInt(PsnId.substr(i, 1)) * (10 - i);
        }
        //alert(checksum_id);
        "checksum_id = checksum_id % 10;		//若除10餘0，則取0"
        if (checksum_id > 0) checksum_id = 10 - checksum_id;
        //alert(checksum_id);
        if (checksum_id != parseInt(PsnId.substr(10, 1))) { //檢查碼原在9已變10
            alert("身份證號第10碼檢查碼有誤");
            return (false);
        }
        else
        {   alert("身份證號正確!");
            return true;
        }
    } //func CheckID

function checkCompanyNo() {

    var CompanyNo = document.getElementById("CompanyNo").value;
    var tbNum = new Array(1, 2, 1, 2, 1, 2, 4, 1);
    var temp = 0;
    var total = 0;

    if (CompanyNo == "") alert("請先輸入欲檢驗的營利事業統一編號!");
    else if (!CompanyNo.match(/^\d{8}$/)) alert("營利事業統一編號長度不夠或格式有誤!請注意營利事業統一編號格式為八碼數字!");
    else{
        for(var i = 0; i < tbNum.length ;i ++){
            temp = CompanyNo.charAt(i) * tbNum[i];
            total += Math.floor(temp/10)+temp%10;
        }
        if (total % 10 == 0 || (total % 10 == 9 && CompanyNo.charAt(6) == 7)) {
            alert("營利事業統一編號正確!");
            return true;
        }
        else {
            alert("營利事業統一編號錯誤!");
            return false;
        }
    }
