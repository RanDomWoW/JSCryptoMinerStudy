<script type="text/javascript">
            $("li.mtab").click(function() {
                var id = $(this).attr('id');
                $('.ui-tabs-active').removeClass('ui-tabs-active').addClass();
                $(this).addClass('ui-tabs-active').removeClass();

                $(this).addClass("ui-tabs-active");
                $.blockUI();

                $.post(id+'.php', { action:id,'group_no':0 }, function(data) {
                    $("#mobile_content").html(data);
                    $.unblockUI();
                    infinity(1,false,4);
                });
                return false; // prevent default

            });
            var track_load = 0;
            $('#mobile_content').load("mobile-home.php", {'group_no':track_load},
                function() {
                    track_load++;
                    infinity(1,false,4);
                }); //load first group*/
      function infinity(track_load,loading,total_groups){

//          var track_load = 0; //total loaded record group(s)
//          var loading  = false; //to prevents multipal ajax loads
//          var total_groups = 8; //total record group(s)

            $(window).scroll(function() { //detect page scroll
                if($(window).scrollTop() + $(window).height() == $(document).height())  //user scrolled to bottom of the page?
                {   var url = $('li.ui-tabs-active').attr('id');
                    if(track_load <= total_groups && loading == false) //there's more data to load
                    {
                            loading = true; //prevent further ajax loading
                        $('.animation_image').show(); //show loading image
                            //load data from the server using a HTTP POST request
                            $.post(url + '.php', {'group_no': track_load}, function (data) {
                                $("#mobile_content").append(data); //append received data into the element
                                //hide loading image
                                $('.animation_image').hide(); //hide loading image once data is received
                                track_load++; //loaded group increment
                                loading = false;
                            }).fail(function (xhr, ajaxOptions, thrownError) { //any errors?
                                alert(thrownError); //alert with HTTP error
                                $('.animation_image').hide(); //hide loading image
                                loading = false;
                            });
                    }
                }
            });
        }
</script>
