            cvm.getLatestCommunications(3, function () {
                ko.applyBindings(cvm, $("#Communications")[0]);
            });
        }

        function tick() {
            $('#ticker li:first').slideUp(function () { $(this).appendTo($('#ticker')).slideDown(); });
