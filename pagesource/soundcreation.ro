 <script type="text/javascript">
    
        
        var my_Suggestion_class_products = new Bloodhound({
            datumTokenizer: Bloodhound.tokenizers.whitespace,
            queryTokenizer: Bloodhound.tokenizers.whitespace,
            
            remote: {
                url: "livesearch.html?q=%QUERY",
                filter: function(x) {
                    return x.products;
                },
                wildcard: "%QUERY"
            }
        });

        var my_Suggestion_class_brands = new Bloodhound({
            datumTokenizer: Bloodhound.tokenizers.whitespace,
            queryTokenizer: Bloodhound.tokenizers.whitespace,
            
            remote: {
                url: "livesearch.html?q=%QUERY",
                filter: function(x) {
                    return x.brands;
                },
                wildcard: "%QUERY"
            }
        });
        var my_Suggestion_class_categories = new Bloodhound({
            datumTokenizer: Bloodhound.tokenizers.whitespace,
            queryTokenizer: Bloodhound.tokenizers.whitespace,
            
            remote: {
                url: "livesearch.html?q=%QUERY",
                filter: function(x) {
                    return x.categories;
                },
                wildcard: "%QUERY"
            }
        });
        var my_Suggestion_class_bundles = new Bloodhound({
            datumTokenizer: Bloodhound.tokenizers.whitespace,
            queryTokenizer: Bloodhound.tokenizers.whitespace,
            
            remote: {
                url: "livesearch.html?q=%QUERY",
                filter: function(x) {
                    return x.bundles;
                },
                wildcard: "%QUERY"
            }
        });

        // Initialize Typeahead with Parameters
        my_Suggestion_class_products.initialize();
        my_Suggestion_class_brands.initialize();
        my_Suggestion_class_categories.initialize();
        my_Suggestion_class_bundles.initialize();

        $("#search-input").typeahead({
                    classNames: {
                        input: 'form-control',
                        hint: 'Typeahead-hint',
                        selectable: 'Typeahead-selectable'
                      },
                    hint: false,
                    minLength: 4,
                    highlight: true
                        },
                    {
                        source: my_Suggestion_class_products.ttAdapter(),
                        limit:15,
                          templates: {
                            pending: loader,
                            notFound: [
                              '<div class="m-t-1">fără rezultate</div>'
                            ].join('\n'),
                            header: '<h4 class="tt-catehead">Produse</h4>',
                            footer: '',
                            suggestion: Handlebars.compile('<div><div class="pull-right"><small>{{subtitle}}</small></div><div><span>{{{title}}}{{{stock}}}</span></div></div>')
                            
                          },
                        displayKey: "title"
                    },
                    {
                        source: my_Suggestion_class_brands.ttAdapter(),
                        limit:15,
                          templates: {
                            notFound: [
                              '<div></div>'
                            ].join('\n'),
                            header: '<h4 class="tt-catehead">Mărci</h4>',
                            footer: '',
                            suggestion: Handlebars.compile('<div><div class="pull-right"><small>{{subtitle}}</small></div><div><span>{{{title}}}</span></div></div>')
                            
                          },
                        displayKey: "title"
                    },
                    {
                        source: my_Suggestion_class_categories.ttAdapter(),
                        limit:15,
                          templates: {
                            notFound: [
                              '<div></div>'
                            ].join('\n'),
                            header: '<h4 class="tt-catehead">Categorii</h4>',
                            footer: '',
                            suggestion: Handlebars.compile('<div><div class="pull-right"><small>{{subtitle}}</small></div><div><span>{{{title}}}</span></div></div>')
                            
                          },
                        displayKey: "title"
                    }
                )
        .on('typeahead:selected', function($e, datum){
                $("#search-input").val(datum["title"]);
                location.href=datum["link"];
            })
        .on('typeahead:asyncreceive', function() {
      if ($(this).data('tt-typeahead').menu._allDatasetsEmpty()) {
        $(this).trigger('typeahead:empty')
      }
    });
    
</script>

