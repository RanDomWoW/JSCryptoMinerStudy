<script type="text/javascript">
    $(function() {
    var SliderModule = (function() {
    var pb = {};
    pb.el = $('#SliderMovistar_117995861');
    pb.items = {
    panels: pb.el.find('.slider-wrapper > li'),
    }
    // Interval del Slider
    var SliderInterval;
    var currentSlider = 0;
    var nextSlider = 1;
    var lengthSlider = pb.items.panels.length;

    // Constructor del Slider
    pb.init = function(settings) {
    this.settings = settings || {
    duration: 10000
    };
    var items = this.items,
    lengthPanels = items.panels.length,
    output = '';

    // Insertamos nuestros botones
    for (var i = 0; i < lengthPanels; i++) {
    if (i == 0) {
    output += '<li class="active"></li>';
    } else {
    output += '<li></li>';
    }
    }

    $('#SliderMovistar_117995861 #control-buttons').html(output);

    // Activamos nuestro Slider
    activateSlider();
    // Eventos para los controles
    $('#SliderMovistar_117995861 #control-buttons').on('click', 'li', function(e) {
    var $this = $(this);
    if (!(currentSlider === $this.index())) {
    changePanel($this.index());
    }
    });
    }

    // Funcion para activar el Slider
    var activateSlider = function() {
    clearInterval(SliderInterval);
    SliderInterval = setInterval(pb.startSlider, pb.settings.duration);
    }

    // Funcion para la Animacion
    pb.startSlider = function() {
    clearInterval(SliderInterval);
    var items = pb.items,
    controls = $('#SliderMovistar_117995861 #control-buttons li');
    // Comprobamos si es el ultimo panel para reiniciar el conteo
    if (nextSlider >= lengthSlider) {
    nextSlider = 0;
    currentSlider = lengthSlider - 1;
    }

    if(lengthSlider > 1){

    controls.removeClass('active').eq(nextSlider).addClass('active');
    items.panels.eq(currentSlider).fadeOut('slow');
    items.panels.eq(nextSlider).fadeIn('slow');

    // Actualizamos los datos del slider
    currentSlider = nextSlider;
    nextSlider += 1;
    var activo = $("#SliderMovistar_117995861 li")[currentSlider];
    var contiene = $(activo).find("input");

    if(contiene.length > 0){
    SliderInterval = setInterval(pb.startSlider, pb.settings.duration * 3);
    } else {
    SliderInterval = setInterval(pb.startSlider, pb.settings.duration);
    }
    } else {
    controls.removeClass('active').eq(nextSlider).addClass('active');
    // items.panels.eq(currentSlider).fadeOut('slow');
    items.panels.eq(nextSlider).fadeIn('slow');
    currentSlider = nextSlider;
    nextSlider += 1;
    }
    }

    // Funcion para Cambiar de Panel con Los Controles
    var changePanel = function(id) {
    clearInterval(SliderInterval);
    var items = pb.items,
    controls = $('#SliderMovistar_117995861 #control-buttons li');
    // Comprobamos si el ID esta disponible entre los paneles
    if (id >= lengthSlider) {
    id = 0;
    } else if (id < 0) {
    id = lengthSlider - 1;
    }

    controls.removeClass('active').eq(id).addClass('active');
    items.panels.eq(currentSlider).fadeOut('slow');
    items.panels.eq(id).fadeIn('slow');

    // Volvemos a actualizar los datos del slider
    currentSlider = id;
    nextSlider = currentSlider + 1;
    var activo = $("#SliderMovistar_117995861 .contenedor-fill > .slider-wrapper > li")[currentSlider];
    var contiene = $(activo).find(".iframe > iframe");

    console.log("changePanel");
    if(contiene.length > 0){
    console.log("if");
    SliderInterval = setInterval(pb.startSlider, pb.settings.duration * 3);
    } else {
    console.log("else");
    SliderInterval = setInterval(pb.startSlider, pb.settings.duration);
    }
    // Reactivamos nuestro slider
    }
    return pb;
    }());

    SliderModule.init({
    duration: 30000
    });
    });
</script>
