        function specialitySearchLocal(classname){
      var host = "https://www.way2college.com/";

      if(!localStorage){
        jQuery.ajax({
                url: host+'static/all_specialities.txt'
            })
            .done(function(response) {
                var specialitydata = JSON.parse(response);
                var options = "";
                jQuery.each(specialitydata, function (i, value) {
                    options += '<option value="'+i+'">'+value+'</option>';
                });
                jQuery('.'+classname).append(options);
                jQuery('.'+classname).trigger("liszt:updated");
                return false;
            });
      
      } else {

            var d = new Date();
            var n = d.getDate();
            var m = d.getMonth()+1;
            var hostcode = "w2c";

            var dataexp = localStorage.getItem(hostcode+"_local_expiry"); 
            var data = localStorage.getItem(hostcode+"_specialities");

            if(dataexp != n+m || !data){
                jQuery.ajax({
                    url: host+'static/all_specialities.txt'
                })
                .done(function(response) {
                    var specialitydata = JSON.parse(response);
                    var options = "";
                    jQuery.each(specialitydata, function (i, value) {
                        options += '<option value="'+i+'">'+value+'</option>';
                    });
                    jQuery('.'+classname).append(options);
                    jQuery('.'+classname).trigger("liszt:updated");

                    localStorage.setItem(hostcode+"_specialities", response);

                    var d = new Date();
                    var n = d.getDate();
                    var m = d.getMonth()+1;
                    localStorage.setItem(hostcode+"_local_expiry", n+m);
                    return false;                    
                });
            } else {
                data = JSON.parse(data);
                var options = "";
                jQuery.map(data, function (value, i) {
                    options += '<option value="'+i+'">'+value+'</option>';
                });
                jQuery('.'+classname).append(options);
                jQuery('.'+classname).trigger("liszt:updated");
            }
        }
    }
