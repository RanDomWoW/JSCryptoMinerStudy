function setCookieForCompare(id) {
	var userId = jQuery('#user-id').val();
	var cookieStr = getCookieForComp("compareEntity"+userId);
	var cookieValues = unescape(cookieStr);
	id = id.toString();
	var cookieArray = [];
	if(cookieValues == '') {
		cookieArray.push(id);
	} else {
		cookieArray.push(id);
		cookieArray.push(cookieValues);
	}
	var d = new Date();
    d.setTime(d.getTime() + (30*24*60*60*1000));
    var expires = "expires=" + d.toUTCString();
	document.cookie = "compareEntity"+userId+"=" + cookieArray + ";" + expires + "; path=/  ";
