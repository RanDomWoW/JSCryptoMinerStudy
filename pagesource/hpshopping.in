<script>
    require(
        [
            'jquery',
            'HPOLS_Catalog/js/popup',
            'touchSwipe'
        ],
        function(
            $,
            $p
        ) {
            $(function() {
                /**
                 * Popup add to cart and know more
                 */
                $('.popup-addtocart').click(function (e) {
                    e.preventDefault();
                    $p.addPopup($(this));
                });

                /**
                 * Swipe on mobile for Best Sellers & Featured Products
                 */
                var screenWidth = $(window).width(),
                    tabWidth = getTabWidth(),
                    tabOldLeftVal = 0;

                var limitTabMarginLeft = tabWidth - screenWidth - 8;


                $('.block-tabs .tabs').swipe({
                    allowPageScroll: "none",
                    threshold: screenWidth / 4,
                    cancelThreshold: 10,
                    preventDefaultEvents: false,
                    swipeStatus: function (event, phase, direction, distance, duration) {
                        var Ww = $(window).width(),TabW = $('.block-best-deals .block-tabs .tabs').outerWidth();
                        if(Ww > TabW){
                            return;
                        }

                        if (phase == 'start') {
                            tabOldLeftVal = parseInt($(this).css('margin-left'));
                        }

                        if (phase == 'move') {
                            if (direction =='up' || direction =='down') {
                                return;
                            }
                        }

                        if (direction == 'right') {
                            if (parseInt(tabOldLeftVal + distance) > 0) {
                                $(this).css({'margin-left': ''});
                            } else {
                                var tabRightVal = parseInt(tabOldLeftVal) + distance;
                                $(this).css({'margin-left': tabRightVal + 'px'});
                            }
                        }

                        if (direction == 'left') {
                            if (Math.abs(tabOldLeftVal - distance) >= limitTabMarginLeft) {
                                $(this).css({'margin-left': '-' + limitTabMarginLeft + 'px'});
                            } else {
                                var tabLeftVal = parseInt(tabOldLeftVal) - distance;
                                $(this).css({'margin-left': tabLeftVal + 'px'});
                            }
                        }
                    }
                });

                /**
                * Monitor the transfer event
                * */
                $(window).on("orientationchange",function(){
                    $('.block-tabs .tabs').css({'margin-left': ''});
                });

                /**
                 * Get tabs actual width
                 *
                 * @returns {number}
                 */
                function getTabWidth() {
                    var tabWidth = 0;
                    $('.block-best-deals .block-tabs .tabs label').each(function () {
                        tabWidth += $(this).outerWidth();
                    });

                    return tabWidth;
                }

                // Reload customer section for homepage in case of the FPC
                $.initNamespaceStorage('mage-cache-storage').localStorage.remove('customer');
            });
        }
    );
</script>
