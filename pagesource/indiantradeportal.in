				<script>
				
				//$(".fPassword1, .cUsername1").hide();
				$(document).on('click','.userType1', function(){
					if(parseInt($(this).val()) === 2 ){
						$(".labelEmail").text('Username:');
						//$(".fPassword1, .cUsername1").show();
					}else if(parseInt($(this).val()) === 1 ){
						$(".labelEmail").text('Username:');
						//$(".fPassword1, .cUsername1").hide();
					}else if(parseInt($(this).val()) === 3 ){
						$(".labelEmail").text('Email ID:');
						//$(".fPassword1, .cUsername1").show();
					}
				});
				
		
				$(document).on('click','.cUsername1', function(){
					if(parseInt($('.userType1:checked').val()) === 1){
						var win = window.open("https://www.fieo.org/login.php?divid=4", '_blank');
						win.focus();
					}else{		
						window.location.href = "/"+webFolder+"register/";
					}
				});
				
				var validateLogin1 = function(type){
				$('#submitType').val(type);
				var pass = $('#pupassword1').val().trim();
				if(type === 2){
					$('#pupassword1').prop('disabled', true);
				}else{
					$('#pupassword1').prop('disabled', false);
				}
				if(parseInt($('.userType1:checked').val()) === 1 && type === 2){
					var win = window.open("https://www.fieo.org/login.php?divid=1", '_blank');
					win.focus();
					return false;
				}
				var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
					var pass = $('#pupassword1').val().trim();
					if($('#puemailid1').val().trim() === '' && parseInt($('.userType1:checked').val()) === 1){
						alert('Please enter the username.');
						$('#puemailid1').focus();
						return false;
					}else if($('#puemailid1').val().trim() === '' && parseInt($('.userType1:checked').val()) === 2){
						alert('Please enter the username.');
						$('#puemailid1').focus();
						return false;
					}else if(!re.test($('#puemailid1').val()) && parseInt($('.userType1:checked').val()) === 3 && $('#puemailid1').val().indexOf("@") > -1 ){
						alert('Please enter the valid email address.');
						$('#puemailid1').focus();
						return false;
					}else if(pass === '' && pass.length < 2 && type === 1){
						alert('Please enter the valid password.');
						$('#pupassword1').select();
						return false;
					}else if($('#txtShownImg').val() === ''){
						alert('Please fill the captcha.');
						$('#txtShownImg').select();
						return false;
					}else{
						//if(parseInt($('.userType1:checked').val()) === 1){
							if(type===1){							
								$('#pupassword1').val(CryptoJS.MD5(pass));
							}
						//}		
						$('#frmlogin').submit();
					}
				};
