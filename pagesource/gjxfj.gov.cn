		$(".topNav .menuList ul li").removeClass("selected");
		$(".topNav .menuCon .subcon").hide();
		$(".menuList").attr("style","background-image:none");
	})
	$(".topNav .menuList ul li").find("a").hover(function(){
		index = $(".topNav .menuList ul li").find("a").index(this);
        $(this).parent().parent().addClass("selected").siblings().removeClass("selected");        
        $(".topNav .menuCon .subcon").eq(index).show().siblings().hide();         
        //bg
        var pos=158*index;
		$(".menuList").stop().animate({
		   backgroundPositionX: pos,
		   backgroundPositionY: "0px"
		}, "slow");
   });
   
