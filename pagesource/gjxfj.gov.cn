        slidernav.stop().show()
    },function() {
        slidernav.stop().hide()
    });
    $(".flexslider").each(function(){
        $(this).hover(function(){
            $(this).find(".flex-direction-nav").fadeIn("slow")
        },function(){
            $(this).find(".flex-direction-nav").hide()
        })
    })
/*END*/
