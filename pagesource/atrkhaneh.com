    <script>
        timer = 0;
        function mySearch() {
            $(".search-box ul").slideUp("slow").html("");
            $.ajax({
                url: "/Search/SearchAjax",
                dataType: "json",
                type: "POST",
                data: { "search": $("#searchTitleText").val().trim() },
                success: function (data) {
                    if (data.result == "success") {
                        var p = jQuery.parseJSON(data.Products);
                        $.each(p, function (index, item) {
                            $(".search-box ul").append("<li><a href='/product/" + item.Id + "/" + item.Name + "/'><h6>" + item.Title + "</h6></a></li>");
                        });
                        $(".search-box ul").slideDown("slow");

                    } else {
                        $(".search-box ul").html("<li>متاسفانه کالایی یافت نشد!</li>");
                        $(".search-box ul").slideDown("slow");
                    }
                },
                error: function (error) {
                    $(".search-box ul").html("<li>متاسفانه کالایی یافت نشد!</li>");
                    $(".search-box ul").slideDown("slow");
                }
            });
        }
        $(document).ready(function () {
            $("#searchTitleText").keyup(function (e) {
                if ($(this).val().length > 4) {
                    if (timer) {
                        clearTimeout(timer);
                    }
                    timer = setTimeout(mySearch, 1000);
                }
                //alert(e.keyCode);
                if (e.which == 13) {
                    e.preventDefault();

                    SearchWord();
                }
            });
        });
