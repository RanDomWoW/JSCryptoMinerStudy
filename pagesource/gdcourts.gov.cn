<script type="text/javascript">
function funSearch(page){
	debugger;
	page = page || 1;
	var s = $.trim($("#keyword").val());
	s = s.replace(/'/g,"").replace(/%/g,"");
	if(s==""||s==undefined){
		$("#keyword").val("");
		$("#keyword").focus();
		layer.tips("请输入关键字","#keyword",{tips:[3,'#c00']});
		return false;
	}else{
	//1.将p和q的值赋给p input page 和q input s
	$("#searchq").val(s);
	$("#searchp").val(page);	
	//2.提交方式修改为form提
	$("#searchform").submit();
	}
	//window.location.href = webBase+"/search?q="+encodeURIComponent(encodeURIComponent(s))+"&p="+page;
}

function funCheckEnter(e){
	e = event || window.event;
	var target = e.target || e.srcElement;
	var key = e.keyCode;
	if(key=='13'){
		$(target).siblings("input[type='button']").click();
	}
}
</script>
