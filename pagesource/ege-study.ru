  // Срабатывет при клике на кнопку отправки формы
  function ege_subscribe (el) {
    if(capha.rezultat != document.getElementById('rezeltat_user').value){
      return;
    }
    el=$(el);
    var data=el.serialize();
    el.find('#subRequest').attr('disabled','disabled');
    el.find('#subRequest').css('opacity',0.5);
    $.ajax({
      type: "POST",
      url: el.attr('action'),
      data: data,
      dataType: 'json',
      success: function (data) {
        if(!data.success) alert ('Ошибка!');
        else show2();
      },
      error: function (){
        alert ('Неполадки со связью');
      },
      complete: function (){
        el.find('#subRequest').removeAttr('disabled');
        el.find('#subRequest').css('opacity',1);
      }
    });
  }
