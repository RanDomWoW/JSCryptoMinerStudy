<script>
    jQuery(document).ready(function(){
        var interval;
        function _start_slide(){
            interval = setInterval(function(){
                jQuery('#_next').click();
            }, 6000);
        }

        function _stop_slide(){
            clearInterval(interval);
        }

        jQuery('#_prev').on('click', function(){
            _stop_slide();
            var stt = jQuery('._promobox_img.active').attr('stt');
            jQuery('._promobox_img').eq(stt).removeClass('active');
            jQuery('._bullet').eq(stt).removeClass('active');

            prev = --stt;
            if(prev==-1)
                prev = 4;

            jQuery('._promobox_img').eq(prev).addClass('active');
            jQuery('._bullet').eq(prev).addClass('active');
            _start_slide();
        });

        jQuery('#_next').on('click', function(){
            _stop_slide();
            var stt = jQuery('._promobox_img.active').attr('stt');
            jQuery('._promobox_img').eq(stt).removeClass('active');
            jQuery('._bullet').eq(stt).removeClass('active');

            next = ++stt;
            if(next == 4)
                next = 0;

            jQuery('._promobox_img').eq(next).addClass('active');
            jQuery('._bullet').eq(next).addClass('active');
            _start_slide();
        });

        jQuery('._promobox_img').on('mouseover',_stop_slide).on('mouseout', _start_slide);
        _start_slide();

        jQuery('._bullet').on('click', function(){
            _stop_slide();
            var stt = jQuery('._promobox_img.active').attr('stt');
            var index = jQuery(this).attr('index');
            jQuery('._promobox_img').eq(stt).removeClass('active');
            jQuery('._bullet').eq(stt).removeClass('active');

            //add class active of img & li index next
            jQuery('._promobox_img').eq(index).addClass('active');
            jQuery('._bullet').eq(index).addClass('active');
            _start_slide();
        });
    });
</script>

