    <script>
        var waitingForNumbers = true;       
        $(window).scroll(function() {
            if (waitingForNumbers && $("#run-numbers").isOnScreen()) {
                waitingForNumbers = false;
                $('.count').each(function() {
                    var number = $(this);
                    var more = $(".more[data-index='" + number.data("index") + "']");
                    more.addClass('invisible'); // built-in foundation class. fixing unwanted move after number appears
                    jQuery({ Counter: number.text() / 4 }).animate({ Counter: number.text() },
                    {
                        duration: 2000,
                        easing: 'swing',
                        step: function(i) {
                            number.text(Math.ceil(i));
                        },
                        complete: function () {
                            more.hide().removeClass('invisible').fadeIn(600);
                        }
                    });
                });
            }
        });
    </script>

