<script type="text/template" id="destination-itemview-template">
	<% var selected_theme; %>
	<div class="themes">
	<% _.each(data,function(theme,key) { %>
		<% if (theme.destination_list.length === 0) { return false; } %>
		<% if (theme.selected) { selected_theme = theme; } %>
		<% if (theme.type === 'popular') { return; } %>
		<div data-key="<%= key%>" class="theme <% if (theme.selected) { print('selected'); }%>">
			<div class="marker"><i class="icon-right-open"></i></div>
			<%= getName(theme) %>
		</div>
	<% }); %>
	</div>
	<div class="items">
		<% _.each(selected_theme.destination_list, function (dest) { %>
			<div class="item clearfix" data-destination-id="<%= dest.destination_id%>" data-destination-name="<%= dest.name %>" data-destination-theme-id="<%= selected_theme.destination_theme_id || '' %>" data-upper-right="<%= getUpperRight(dest) %>" data-lower-left="<%= getLowerLeft(dest) %>" data-destination-type-id="<%= dest.type_id %>">
				<% if (!dest.destination_id) { %>
					<div class="long name"><%= __('Alla') %> <% print(dest.name.toLowerCase());  %></div>
				<% } else if (dest.hierarchy && dest.type_id != 2) { %>
					<div class="name"><%= dest.name %></div>
					<div class="type"><%= getParentName(dest.hierarchy) %></div>
				<% } else { %>
					<div class="long name"><%= dest.name %></div>
				<% } %>
			</div>
		<% }); %>
	</div>
</script>

