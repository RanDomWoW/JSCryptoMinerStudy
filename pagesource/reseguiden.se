			// Cookies overwrite perl and the new test set.
			// Note that perl tests read the cookies as well so it
			// should not matter if we overwrite those values.
			var tests = document.cookie.match(/(?:^|\s)ab_([_\w\d]+)=(\d)(?:(?:\:|%3A)([^;]+))?/g);
			if (tests) {
				for (i = tests.length - 1; i >= 0; i--) {
					regexpMatch = /(?:^|\s)ab_([_\w\d]+)=(\d)(?:(?:\:|%3A)([^;]+))?/g.exec(tests[i]);
					if (regexpMatch && abTest[regexpMatch[1]]) {
						abTest[regexpMatch[1]].active = true; // Data from cookies counts as used
						abTest[regexpMatch[1]].value = parseInt(regexpMatch[2]);
						abTest[regexpMatch[1]].variant = unescape(regexpMatch[3] || '-');
					}
				}
			}

			// QS overwrites all
			if (/ab_test_alias/.test(window.location.search)) {
				alias = /ab_test_alias=([\w-]+)/.exec(window.location.search);
				variant = /ab_test_variant=(\d)/.exec(window.location.search);

				if (alias && variant) {
					abTest[alias[1]].active = true;
					abTest[alias[1]].value = parseInt(variant[1]);
				}
			}

			// Set cookie (7 days)
			for (alias in abTest) {
				// Only save a cookie for tests that are active.
				// Tests are active if the page has used that test. (or it came from a cookie and thus already active)
				if (abTest.hasOwnProperty(alias) && abTest[alias].active) {
					setCookie(alias);
				}
			}

			// Reload when QS force tests
			if (/ab_test_reload=1/.test(window.location.search)) {
				// Quickfix to not loop
				var cleanerUrl = window.location.href.replace(/&?ab_test_.+?=[^&#]+/g, '');
				window.location.href = cleanerUrl;
			}

			// Use this to check tests from javascript
			// It sets the test as active and sets the cookie.
			abTest.test = function (alias) {
				// Exempt bots from ab-tests
				if (/ghost inspector|googlebot/i.test(window.navigator.userAgent)) {
					return 0;
				}

				if (abTest[alias]) {
					if (!abTest[alias].disabled) {
						abTest[alias].active = true;
						setCookie(alias);
					}
					return abTest[alias].value;
				}
				return 0;
			};
		})();
	
