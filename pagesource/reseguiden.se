<script type="text/template" id="search-to-option-template">
<% if (item.id === 'placeholder') { %>
	<% if (isAirSearch) { %> 
		<div class="no-destinations"><%= __('Detta resmål har ingen flygplats. Prova med en annan närliggande ort.') %></div>
	<% } else { %>
		<div class="no-destinations"><%= __('Just nu har vi tyvärr inga resor till detta resmål') %></div>
	<% } %>
<% } else if (item.empty) { %>
	<div class="selectize-empty"><div class="spinner-wrapper"><i class="icon-spinner"></i></div></div>
<% } else if (!item.name) {  %> 
<% } else if (isPackageOrLmSearch) { %>
	<div class="item single <% if (item.class == "destinations") {print("destination");} else {print("lodging");} %>">
    	<div class="left-column">
        	<div class="name"><%= item.name %></div>
            <% if (item.class == "hotel-names" && hierarchy && !item.hide_hierarchy) { %><div class="geo"><%= hierarchy %></div> <% } %>
        </div>
        <% if (item.class != "hotel-names" && item.countryName) { %>
        	<div class="right-column"><span class="type"><%= item.countryName %></span></div>
        <% } %>
    </div>                  
<% } else { %>
	<div data-img-url="<% if(item.image) {print(item.image)} else if (item.rg_image) {print(item.rg_image);}; %>" class="item <%= (hierarchy.length > 0 && !item.hide_hierarchy) ? "double" : "single" %> <%= item.is_subitem ? "subitem" : "" %>">
    	<span class="name"><% if (item.icon) { %><i class="<%= item.icon %>"/> <% }%>
			<%= item.name %>
			<% if (additionalName) { %> <%= additionalName %><% } %>
			<% if (item.count >= 0) { %> <span class="count">(<%= item.count %>)</span><% }%>
        </span>
        <% if (hierarchy.length > 0 && !item.hide_hierarchy) { %><div class="geo"><%= hierarchy %></div> <% } %>
    	<% if (item.description) { %><div class="geo"><%= item.description %></div> <% }%>
	    <% if (item.airport_code) { %><div class="text-muted"><%= item.airport_code %></div><% } %>
    </div>    
<% } %>
</script>
