$(function(){
	var v_num=0;
	var v_auto = setInterval(_v_auto,4000);
	function _v_auto(){
		$(".visual2_right").trigger("click");
	}
	$(".visual2_right").click(function(){
		v_num++;
		if(v_num>$(".visual2 div").size()-1){v_num=0;}
		$(".visual2_num em").removeClass("on");
		$(".visual2_num em").eq(v_num).addClass("on");
		$(".visual2 div").fadeOut();
		$(".visual2 div").eq(v_num).fadeIn();
	});
	$(".visual2_left").click(function(){
		v_num--;
		if(v_num<0){v_num=$(".visual2 div").size()-1;}
		$(".visual2_num em").removeClass("on");
		$(".visual2_num em").eq(v_num).addClass("on");
		$(".visual2 div").fadeOut();
		$(".visual2 div").eq(v_num).fadeIn();
	});
	$(".visual2_num em").click(function(){
		$(".visual2_num em").removeClass("on");
		$(this).addClass("on");
		$(".visual2 div").fadeOut();
		$(".visual2 div").eq($(this).index()).fadeIn();
		v_num = $(this).index();
	});
});
