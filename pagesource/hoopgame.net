<script language="javascript" type="text/javascript">//show imgs lazyload


$(".page_0 img.lazy").show().lazyload({effect: "fadeIn", failure_limit: 10000}).removeClass("lazy");
function dolazyloadnow() {
    $("#gameListTj").find("img.lazy").show().lazyload({effect: "fadeIn", failure_limit: 10000});
}
$(document).ready(function () {
    window.onresize = function () {
        resize_gamelist();
    };
});
var bigArr = [[1, 3], [4, 1]];
var posArr = new Array();
var gameNumPreRow = 9;
var pageLanguage = 'en';
var gameWidth = 135;
var gameHeight = 135;
function init_posArr() {
    for (var i = 0; i < 100; i++) {
        posArr[i] = new Array();
        for (var j = 0; j < gameNumPreRow; j++) {
            if (i < 3 && j < 2) {
                posArr[i][j] = 1;
            }
            else {
                posArr[i][j] = 0;
            }
            posArr[i][j] = 0;
        }
    }
}
function setPosArr(row, col, isBig) {
    if (!isBig) {
        posArr[row][col] = 1;
    } else {
        posArr[row][col] = 1;
        posArr[row + 1][col] = 1;
        posArr[row][col + 1] = 1;
        posArr[row + 1][col + 1] = 1;
    }
}
function getPosArr(isBig) {
    var pos = {};
    if (isBig == 0) {
        for (var i = 0; i < 100; i++) {
            for (var j = 0; j < gameNumPreRow; j++) {
                if (posArr[i][j] == 0) {
                    pos = {row: i, col: j};
                    return pos;
                }
            }
        }
    }
    else if (bigArr.length > 0) {
        pos = {row: bigArr[0][0], col: bigArr[0][1]}
        bigArr.shift();
        return pos;
    }
    else {
        for (var i = 0; i < 70 - 1; i++) {
            for (var j = 0; j < gameNumPreRow - 1; j++) {
                if (posArr[i][j] == 0 && posArr[i + 1][j] == 0 && posArr[i][j + 1] == 0 && posArr[i + 1][j + 1] == 0) {
                    pos = {row: i, col: j};
                    return pos;
                }
            }
        }
    }
    return pos;
}
function resize_tagNav(heigth, width, leftgap, header_height) {
    var ulHeight = (heigth * 3 + 20);
    var ulWdith = width * 2 + 10;
    var liHeight = Math.round(ulHeight * 0.116);
    var liWidth = width + 4;
    var moreHeight = ulHeight - 8 * liHeight - 2;
    var topHeight = Math.round(liHeight * 11 / 57);
    var imgLeftPadding = Math.round(width * 12 / gameWidth);
    var contentLeftPadding = Math.round(width * 10 / gameWidth);
    var imageHeight = Math.round(liHeight * 12 / 19);
    var bottom_height = liHeight - topHeight - imageHeight;
    var lineHeight = Math.ceil(imageHeight / 2);
    var fontSize = Math.round(lineHeight * 5 / 6);
    var contentWidth = liWidth - imgLeftPadding - contentLeftPadding - imageHeight;
    if (pageLanguage == 'tw' || pageLanguage == 'zh') {
        fontSize = fontSize - 1;
    }
    if (fontSize > 12) {
        fontSize = 12;
    }
    var morePadding = Math.round((moreHeight - fontSize) / 2);
    $('li[id=tagInfo]').each(function () {
        $(this).css('height', liHeight + 'px');
        $(this).css('width', liWidth + 'px');
        $(this).find('.tag_container').css('padding-top', topHeight + 'px');
        $(this).find('.tag_container').css('padding-bottom', bottom_height + 'px');
        $(this).find('.tag_container').css('padding-left', imgLeftPadding + 'px');
        $(this).find('.right_content').css('padding-left', contentLeftPadding + 'px');
        $(this).find('img').css('width', imageHeight + 'px').css('height', imageHeight + 'px');
        $(this).find('.tag_name').css('line-height', (lineHeight - 2) + 'px').css('font-size', fontSize + 'px').css('height', lineHeight + 'px');
        $(this).find('.game_num').css('line-height', (lineHeight - 2) + 'px').css('font-size', fontSize + 'px').css('height', lineHeight + 'px');
        $(this).find('.right_content').css('width', contentWidth + 'px');
    });
    $('#tagMore').css('height', moreHeight + 'px');
    $('.more_content').css('font-size', fontSize + 'px').css('line-height', moreHeight + 'px');
    $('.leftTag').css('height', ulHeight + 'px');
    $('.leftTag').css('width', ulWdith + 'px');
    $('.leftTag').css('left', (leftgap + 5) + 'px');
    $('.leftTag').css('top', (header_height + 5 + 105) + 'px');
}
function resizeTitle(width) {
    if (!$('.h1container')) {
        return false;
    } else {
        var hlHeight = Math.round(width / 2.8);
        var fontSize = Math.round(hlHeight * 0.94);
        var top = Math.round(42 + hlHeight * 0.6);
        var homeTop = Math.round(hlHeight * 0.3 + 5);
        if (pageLanguage == 'tw' || pageLanguage == 'zh') {
            homeTop = homeTop + 3;
        }
        $('.h1container').css('height', hlHeight).css('top', top);
        $('.h1container').find('.h1').css('height', hlHeight);
        $('.h1container').find('.h1').css('font-size', fontSize);
        $('#home_title').css('top', homeTop);
    }
}
var resize_gamelist_valid = 1;
function resize_gamelist() {
    resize_gamelist_valid = 0;
    var bodyWidth = $('body').outerWidth();
    if ($('body').height() < $(window).height()) {
        bodyWidth = bodyWidth - 17;
    }
    if (bodyWidth > 1170) {
        gameNumPreRow = Math.floor((bodyWidth - 10) / (gameWidth + 10));
    }
    else {
        gameNumPreRow = 8;
    }
    var current_width = (bodyWidth - 20) / gameNumPreRow - 10;
    var cur_height = current_width;
    var column = 0;
    var row = 0;
    var def_column_count = gameNumPreRow;
    var tmpheight = cur_height;
    var tmpwidth = current_width;
    //修改标题高度
    resizeTitle(tmpwidth);
    var header_height = $('.game_common_header').height() + $('.h1container').height() * 1.5;
    var tmpgap = 10;
    var leftgap = (bodyWidth - (tmpwidth + tmpgap) * def_column_count) / 2;
    // resize_tagNav(cur_height, current_width, leftgap, header_height);
    init_posArr();
    bigArr = [[1, 3], [4, gameNumPreRow - 5]];
    $('#gameListTj .inList').each(function () {
        var cPos, width, height;
        var tmpisbig = $(this).attr('isbig');
        if (tmpisbig == 1) {
            cPos = getPosArr(1);
            width = 2 * current_width + tmpgap;
            height = 2 * cur_height + tmpgap;
        } else {
            cPos = getPosArr(0);
            width = current_width;
            height = cur_height;
        }
        column = cPos.col;
        row = cPos.row;
        $(this).css('left', (column * (tmpwidth + tmpgap)) + leftgap + 'px');
        $(this).css('top', (row * (tmpheight + tmpgap) + header_height + 5) + 'px');
        $(this).width(width);
        $(this).height(height);
        setPosArr(row, column, tmpisbig);
    });
    $('#gameListTj').width((tmpwidth + 10) * def_column_count);
    $('.gameListTj').height((row + 1) * (tmpheight + tmpgap) + header_height);
    $('#gameListTj .inList').hover(function () {
        $(this).find('.name').show();
    }, function () {
        $(this).find('.name').hide();
    });
    $('#gameListElxCont').show();
    resize_gamelist_valid = 1;
}
resize_gamelist();

</script>

