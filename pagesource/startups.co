  <!--get bundle file names and create tags for them -->
  <script type="text/javascript">
    (function () {
      var xmlhttp = new XMLHttpRequest();
      xmlhttp.onreadystatechange = function() {
        if (xmlhttp.readyState == XMLHttpRequest.DONE ) {
          if (xmlhttp.status == 200) {
            console.log('resp', xmlhttp.responseText);
            var files = JSON.parse(xmlhttp.responseText);
            if (files.js !== undefined && files.js !== '') {
              var s = document.createElement("script");
              s.type = "text/javascript";
              s.src = files.js;
              s.async = true;
              document.body.appendChild(s);
            }
            if (files.css !== undefined && files.css !== '') {
              var c = document.createElement("link");
              c.type = "text/css";
              c.rel = "stylesheet";
              c.href = files.css;
              document.head.appendChild(c);
            }
          }
          else {
            // Write error on to page
            document.write('<div style="font-family:sans-serif;text-align:center;padding:50px 0;line-height:1.3;"><img style="width: 80%;max-width: 160px;" src="https://s3.amazonaws.com/media.startups.co/startups-co-logo.png" /><h3>Be right back!</h3><p>We\'re updating the site.<br/>Things should be back to normal shortly.<br/>This page will try again in a few seconds.</p></div>')

            // Refresh in 5 seconds
            setTimeout(function() { window.location.reload(true); }, 5000);
          }
        }
      };
      xmlhttp.open("GET", "/node/bundle.json", true);
      xmlhttp.send();

    }());
  </script>

