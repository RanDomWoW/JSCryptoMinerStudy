        function getFirstOverlay() {
            if (!jsonForLabel || jsonForLabel == "") return 0;
            var points = eval("(" + jsonForLabel + ")");
            var length = points.length;
            if (length > 0)
                return new BMap.Point(points[0].lng, points[0].lat);
