    function MapInIt() {
        citymap = new BMap.Map("city_map_index",{enableMapClick:false});
        var point = new BMap.Point(city_G_lng, city_G_Lat);
        citymap.centerAndZoom(point, 5);
        var navControl = new BMap.NavigationControl({ type: BMAP_NAVIGATION_CONTROL_LARGE, offset: new BMap.Size(5, 45) });
        citymap.addControl(navControl); //添加导航条
        citymap.addControl(new BMap.ScaleControl()); //添加比例尺控件
        //地图惯性拖拽
        citymap.enableInertialDragging();
        //地图滚轮
        citymap.enableScrollWheelZoom();
    }
    //定义自定义覆盖物
    function ComplexCustomOverlay(point, divclass, spanclass, name, host) {
        this._point = point;
        this._name = name;
        this._divClass = divclass;
        this._spanClass = spanclass;
        this._host = host;
    }
    ComplexCustomOverlay.prototype = new BMap.Overlay();
    ComplexCustomOverlay.prototype.initialize = function (citymap) {
        this._map = citymap;
        var div = this._div = document.createElement("div"); //覆盖物外层
        div.style.position = "absolute";
        div.style.zIndex = 10; //BMap.Overlay.getZIndex(this._point.lat);

        var divTitle = this._divTitle = document.createElement("div"); //覆盖物显示title层
        divTitle.style.background = "url(http://css.city8.com/images/tck2.png) no-repeat";
        divTitle.style.color = "white";
        divTitle.style.height = "73px";
        divTitle.style.width = "88px";
        divTitle.style.padding = "2px";
        divTitle.style.lineHeight = "18px";
        divTitle.style.whiteSpace = "nowrap";
        divTitle.style.MozUserSelect = "none";
        divTitle.style.fontSize = "12px"
        divTitle.style.position = "absolute";
        divTitle.style.top = "-40px";
        divTitle.style.left = "-25px";
        if (this._name != "上海")
            divTitle.style.display = "none";
        divTitle.className = this._divClass;

        var marAs = this._marAs = document.createElement("a");
        marAs.href = "http://" + this._host + ".city8.com";
        marAs.target = "_blank";
        var span = this._span = document.createElement("span");
        span.className = this._spanClass;
        marAs.appendChild(span);
        divTitle.appendChild(marAs);

        div.appendChild(divTitle);

        var marBiao = this._marBiao = document.createElement("div");
        marBiao.style.position = "absolute";
        marBiao.style.padding = "0px";
        marBiao.style.margin = "0px";
        marBiao.style.border = "0px none";
        marBiao.style.overflow = "hidden";
        marBiao.style.width = "13px";
        marBiao.style.height = "14px";
        marBiao.style.top = "28px";
        marBiao.style.left = "-3px";

        var marA = this._marA = document.createElement("a");
        marA.href = "http://" + this._host + ".city8.com";
        marA.target = "_blank";
        var marImg = this._marImg = document.createElement("img");
        marImg.src = "http://css.city8.com/images/d.png";
        marImg.title = this._name;

        marA.appendChild(marImg);
        marBiao.appendChild(marA);
        div.appendChild(marBiao);

        div.onmouseover = function () {
            divTitle.style.display = "block";
            this.style.zIndex = 11;
        }

        div.onmouseout = function () {
            divTitle.style.display = "none";
            this.style.zIndex = 10;
        }

        div.onclick = function () {
            divTitle.style.display = "none";
        }

        citymap.getPanes().labelPane.appendChild(div);

        return div;
    }
    ComplexCustomOverlay.prototype.draw = function () {
        var citymap = this._map;
        var pixel = citymap.pointToOverlayPixel(this._point);
        this._div.style.left = pixel.x - 4 + "px";
        this._div.style.top = pixel.y - 35 + "px";
    }

    function AddCityMarker() {
        for (var i = 0; i < citylist.length; i++) {
            citymap.addOverlay(new ComplexCustomOverlay(new BMap.Point(citylist[i].lng, citylist[i].lat), citylist[i].divClass, citylist[i].spanClass, citylist[i].name, citylist[i].host));
        }
    }

    
