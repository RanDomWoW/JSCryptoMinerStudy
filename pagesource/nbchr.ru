            function makeBanner(index) {
                var item = banners[index];

                var img = jQuery("<img></img>").css("width", autoWidth ? 'auto' : bannerWidth).css("height", bannerHeight).attr("src", item.img);
                var a = jQuery('<a/>').attr('href', item.url || item.img).css('padding-right', padding + "px").attr("target", "_blank");
                a.append(img);

                a.attr(indexAttr, index);

                return a;
            }

            function getNextIndex() {
                var index = inner.children().last().attr(indexAttr);

                if (!index && index !== 0 && index !== '0')
                    index = -1;

                index++;
                if (index >= banners.length)
                    index = 0;

                return index;
            }

            function getPrevIndex() {
                var index = inner.children().first().attr(indexAttr) || 0;

                index--;
                if (index < 0)
                    index = banners.length - 1;

                return index;
            }

            function setSize() {
                var count = 1 + (ctr.width() / (bannerWidth || 120));

                while (inner.children().size() > count) {
                    inner.children().last().remove();
                }

                while (inner.children().size() < count) {
                    var index = getNextIndex();
                    var b = makeBanner(index);
                    inner.append(b);
                }
            }

            function scrollLeft() {
                clearTimeout(scrollTimer);
                dettachHandlers();

                var first = inner.children().first();

                inner.animate({ 'left': -first.outerWidth() }, 400, 'swing', function () {
                    first.remove();
                    inner.css('left', 0);

                    setSize();
                    scheduleScroll();
                    attachHandlers();
                });
            };

            function scrollRight() {
                clearTimeout(scrollTimer);
                dettachHandlers();

                var first = inner.children().first();
                inner.css('left', -first.outerWidth());

                var index = getPrevIndex();
                var b = makeBanner(index);
                inner.prepend(b);

                inner.animate({ 'left': 0 }, 400, 'swing', function () {
                    setSize();
                    scheduleScroll();
                    attachHandlers();
                });
            };

            function attachHandlers() {
                buttonRight.on('click', scrollRight);
                buttonLeft.on('click', scrollLeft);
            }

            function dettachHandlers() {
                buttonRight.off('click', scrollRight);
                buttonLeft.off('click', scrollLeft);
            }

            function scheduleScroll() {
                scrollTimer = setTimeout(scrollLeft, scrollTimeout * 1000);
            }

            jQuery(window).resize(function () {
                clearTimeout(sizeTimer);
                sizeTimer = setTimeout(setSize, 300);
            });

            setSize();
            scheduleScroll();
            attachHandlers();
        }

        jQuery(document).ready(function () {
            initBanners();
        });

    </script>

