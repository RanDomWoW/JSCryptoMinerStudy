<script>
	$(document).ready(function () {

		var inProgress = false;

		$(window).scroll(function () {

			if ($(window).scrollTop() + $(window).height() >= $(document).height() - 400 && !inProgress) {

				var activeTab = $("#all-news-tabs").find("li.active");
				var activeTabId = activeTab.attr("id");
				var offset;
				var activeTabHref = activeTab.find("a").attr("href");
				var excludeNews = 'Array';
				var limit = (parseInt(activeTab.attr('data-limit')));
				var tag = '';
				var newsOrGames = 'news';

				switch (activeTabId) {
					case "last-news-tab" :
						offset = parseInt(activeTab.attr('data-last-news-offset'));
						if (activeTab.attr("data-has-more-news") == undefined)
							getMoreLastNews(offset + limit, limit, activeTab, activeTabHref, tag, newsOrGames, excludeNews);

						break;

					case "popular-news-tab" :
						offset = parseInt(activeTab.attr('data-popular-news-offset'));
						if (activeTab.attr("data-has-more-news") == undefined)
							getMorePopularNews(offset + limit, limit, activeTab, activeTabHref, tag, excludeNews);

						break;

					case "trend-news-tab" :
						offset = parseInt(activeTab.attr('data-trend-news-offset'));
						if (activeTab.attr("data-has-more-news") == undefined)
							getMoreTrendNews(offset + limit, limit, activeTab, activeTabHref, tag, excludeNews);

						break;
				}
			}
		});

		function getMoreLastNews(offset, limit, activeTab, activeTabHref, tag, newsOrGames, excludeNews) {
			var url = tag != '' ? "/get-more-last-news/" + tag + '/' + offset + "/" + limit + '/' + newsOrGames + "/" + excludeNews : "/get-more-last-news-list/" + offset + "/" + limit + "/" + excludeNews;

			$.ajax({
				type: "GET",
				url: url,
				beforeSend: function () {
					inProgress = true;

					$("ul" + activeTabHref).find("li.hidden").each(function () {
						if ($(this).hasClass("hidden")) {
							$(this).addClass("showed").removeClass("hidden");
						}
					});
				},
				success: function (data) {
					if (data.success) {

						if (data.hasMoreNews == 'false') {
							activeTab.attr('data-has-more-news', 'false');
						}

						if (data.html != '') {
							$('ul' + activeTabHref).append(data.html);
							initMetrics();
						}

						visibleLabel();
					}
				}
			}).done(function (data) {
				activeTab.attr('data-last-news-offset', offset);
				inProgress = false;
			});

		}

		function getMorePopularNews(offset, limit, activeTab, activeTabHref, tag, excludeNews) {
			var url = tag != '' ? "/get-more-popular-news/" + tag + '/' + offset + "/" + limit + "/" + excludeNews : "/get-more-popular-news-list/" + offset + "/" + limit + "/" + excludeNews;

			$.ajax({
				type: "GET",
				url: url,
				beforeSend: function () {
					inProgress = true;

					$("ul" + activeTabHref).find("li.hidden").each(function () {
						if ($(this).hasClass("hidden")) {
							$(this).addClass("showed").removeClass("hidden");
						}
					});
				},
				success: function (data) {
					if (data.success) {

						if (data.hasMoreNews == 'false') {
							activeTab.attr('data-has-more-news', 'false');
						}

						if (data.html != '') {
							$('ul' + activeTabHref).append(data.html);
							initMetrics();
						}

						visibleLabel();
					}
				}
			}).done(function (data) {
				activeTab.attr('data-popular-news-offset', offset);
				inProgress = false;
			});
		}

		function getMoreTrendNews(offset, limit, activeTab, activeTabHref, tag, excludeNews) {
			var url = tag != '' ? "/get-more-trend-news/" + tag + '/' + offset + "/" + limit + "/" + excludeNews : "/get-more-trend-news-list/" + offset + "/" + limit + "/" + excludeNews;

			$.ajax({
				type: "GET",
				url: url,
				beforeSend: function () {
					inProgress = true;

					$("ul" + activeTabHref).find("li.hidden").each(function () {
						if ($(this).hasClass("hidden")) {
							$(this).addClass("showed").removeClass("hidden");
						}
					});
				},
				success: function (data) {
					if (data.success) {

						if (data.hasMoreNews == 'false') {
							activeTab.attr('data-has-more-news', 'false');
						}

						if (data.html != '') {
							$('ul' + activeTabHref).append(data.html);
							initMetrics();
						}

						visibleLabel();
					}
				}
			}).done(function (data) {
				activeTab.attr('data-trend-news-offset', offset);
				inProgress = false;
			});
		}

		function visibleLabel() {
			var labelStatus = $(".tabs-content").find('.rubric-label').is(":visible");


			if (labelStatus) {
				$(".tabs-content").find('.rubric-label').css("display", "block");
			}
		}
	});


</script>


