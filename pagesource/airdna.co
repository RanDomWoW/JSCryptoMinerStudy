	<script type="text/javascript">
	var div = $('.global-industry .opacity-layer');
	var range = 400;

	$(window).on('scroll', function () {
	    var scrollTop = $(this).scrollTop() + $(window).height()*.5;
	    var offset = div.offset().top;
	    var height = div.outerHeight();
	    offset = offset + height / 2;
	    var calc = 1 - (scrollTop - offset + range) / range;

	    if ( calc > '.8' ) {
	    	div.css({ 'opacity': .8 });
	    } else if ( calc < '0' ) {
	    	div.css({ 'opacity': 0 });
	    } else {
	    	div.css({ 'opacity': calc });
	    }
	});
	</script>
