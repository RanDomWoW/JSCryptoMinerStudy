<form id="homeBean" name="loginForm" class="loginForm box-form-small form-horizontal" action="/" method="post">
    










    
        

    

 









    
        
    



    
        <div class="control-group">
            
            
            
             
                    <input type="hidden" name="loginFormBean.partnerName" value="Oi WiFi">
                

            <div class="tour-tip tour-tip-1 myTipTour ">
                <span class="sprite-tour sprite-tour-arrow1-l number1-tour">&nbsp;</span>

                <p class="tour-tip-text">Selecione  Oi WiFi ou seu provedor de acesso para acessar.</p>
            </div>
        </div>
    

    <div class="control-group float-label">
        























    
        <label for="usernameInput">Usuário</label>
    







    
    
    
    
    
        <input id="usernameInput" name="loginFormBean.login" class="floatLabel" placeholder="DDD + n&uacute;mero ou usu&aacute;rio" type="text" value="" autocomplete="false"/>
    















    
        

    



        <div class="tour-tip tour-tip-2 myTipTour">
            <span class="sprite-tour sprite-tour-arrow2-l number2-tour">&nbsp;</span>

            <p class="tour-tip-text">loginForm.input.phone.labe</p>
        </div>
    </div>

    <div class="row-fluid">
        <label for="passwordInput">Senha</label>
    </div>

    <div class="row-fluid">
        

















<div class="box-password">
    
    <input id="passwordInput" name="loginFormBean.password" class="floatLabel" placeholder="Sua Senha" type="password" value="" autocomplete="false"/>
    <input type="text" class="floatLabel" id="passwordInput" placeholder="Sua Senha" style="display: none" autocomplete="off" />

    <div class="togglepassword">
        <input id="showPasswordCheckbox" type="checkbox" onclick="passwordInput_toggle()"/>
    </div>
</div>

<script>
    function passwordInput_toggle() {
        $('#passwordInput[type=password]').toggle();
        $('#passwordInput[type=text]').toggle();
    }
    ;

    $('#passwordInput[type=password]').keyup(function () {
        var elText = $(this).val();
        $('#passwordInput[type=text]').val(elText);
    });
    $('#passwordInput[type=text]').keyup(function () {
        var elText = $(this).val();
        $('#passwordInput[type=password]').val(elText);
    });
</script>












    
        

    



        <div class="forgotpass"><a href="forgot" class="show font-forgot">esqueceu?</a></div>
    </div>

    <div class="row-fluid">
        <div class="">
            <input type="submit" class="btn btn-primary btn-large span" value="Entrar"/>
        </div>
        <div class="tour-tip tour-tip-3 myTipTour">
            <span class="sprite-tour sprite-tour-arrow3-l number3-tour">&nbsp;</span>
            <p class="tour-tip-text">Digite sua senha cadastrada. Não tem senha? Digite o CPF ou CNPJ do titular da conta. </br> Não lembra a senha? Acesse “Esqueci minha senha” embaixo do campo.</p>
        </div>
    </div>

    <script type="text/javascript">
        var arrayPartners = [];

        
        
        


        $(function () {
            $(".select-provide").change(function () {
                var destino = $(this).val();
                if ($.inArray(destino, arrayPartners) >= 0) {
                    $("#homeBean").submit();
                }
            });
        });
    </script>
</form>

