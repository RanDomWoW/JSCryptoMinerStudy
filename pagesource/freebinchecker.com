<script>
      var $err = $('#err'), $srchAd = $('#srchAd'), $binCheck = $('#bin-check');
      function mkR(p) {return (p==null?'':'<tr><th>Card Type</th><td>'+p+'</td></tr>');}
      $binCheck.click(function() {
        $err.html('');      
        var gresp = grecaptcha.getResponse(), bin = $.trim($('#bin').val());
        if(gresp.length == 0) {$err.html("Enter Captcha Validation"); return false;}        
        if(bin.length != 6) {$err.html("Enter 6 digit BIN Number"); return false; }
        $binCheck.addClass('disabled');
        $.ajax({url: '/', type : "POST", cache : false, dataType: 'json', data: {'bin': bin, 'recaptcha': grecaptcha.getResponse()}}).done(function (resp, textStatus, jqXHR){
          $binCheck.removeClass('disabled');
          grecaptcha.reset();
          $('html, body').animate({scrollTop: $("#result").offset().top-180}, 1000);
          if(resp.status == 'error') {$err.html(resp.message); return false;}                           
          $('#result').html('<table class="table"><tr><th>BIN</th><td>'+bin+'</td></tr>'+
              '<tr><th>Status</th><td>'+resp.data.bin.status+'</td></tr>'+
              mkR(resp.data.bin.card_type)+mkR(resp.data.bin.card_brand)+mkR(resp.data.bin.card_category)+
              mkR(resp.data.bin.issuer_name)+mkR(resp.data.bin.issuer_website)+mkR(resp.data.bin.issuer_phone)+
              (resp.data.ctry==null?'':'<tr><th>Country</th><td>'+resp.data.ctry.country_s+'</td></tr>'+
                  '<tr><th>Country Code</th><td>'+resp.data.ctry.country_code_s_x+'</td></tr>'+
                  '<tr><th>Latitude</th><td>'+resp.data.ctry.latitude_s+'</td></tr>'+
                  '<tr><th>Longitude </th><td>'+resp.data.ctry.longtitude_s+'</td></tr>'+
                  '<tr><th>Country Image</th><td><div class="sm-img"><img class="img-fluid" src="//fbc-cdn.shooho.com/'+resp.data.bin.country_id+'-bin-list-img.jpg"></div></td></tr>')+              
              '</table><p class="text-center"><button id="cm" class="btn btn-primary btn-lg">Check another BIN</button></p>');
          $('#cm').click(function() {$('html, body').animate({scrollTop: $('#bin').offset().top-100}, 1000);});
          $srchAd.html('<ins class="adsbygoogle" style="display: block" data-ad-format="auto" data-ad-client="'+sId+'"></ins>');                              
          $('#result-ad').html('<ins class="adsbygoogle" style="display: block" data-ad-format="auto" data-ad-client="'+sId+'"></ins>');
          for (var i=0; i<2; i++)(adsbygoogle=window.adsbygoogle||[]).push({});
        });        
      });
    </script></div>
