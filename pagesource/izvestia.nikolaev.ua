<script type="text/javascript">

var current = 1;

function changeMainArticle()
{
    if($('#newsoftheday-tabs .ui-tabs-nav li').length)
    {
        if(current > $('#newsoftheday-tabs .ui-tabs-nav li').length)
            current = 1;
        
        $($('#newsoftheday-tabs .ui-tabs-nav li')[current-1]).find('a').click();
        current++;
    }
}

$(document).ready(function() {

    setInterval('changeMainArticle();', 5000);    
	$('#slider').cycle({
		fx: 'scrollHorz',
		timeout: 0,
		prev: '#slider_prev',
		next: '#slider_next'
	});

	$('.large-slider').cycle({
		fx: 'fade',
		timeout: 5000,
	});

	$("#newsoftheday-tabs").tabs();

	$("#event-tabs").tabs();

	$("#opinion-tabs").tabs();

	$("#detect-tabs").tabs();

	$('.user-nav-search').on('click', function(event) {
		event.preventDefault();

		if($('.search-block').hasClass('hidden')) {
			$('.search-block').height(0).css('display', 'block').animate({height: 100}, 200).toggleClass('hidden');
		} else {
			$('.search-block').animate({
					height: 0
				},
				200, function() {
				$(this).css('display', 'none').toggleClass('hidden');
			});
		}
	});

	$('.user-nav-register').on('click', function(event) {
		event.preventDefault();

		if($('.login-block').hasClass('hidden')) {
			$('.login-block').height(0).css('display', 'block').animate({height: 100}, 200).toggleClass('hidden');
		} else {
			$('.login-block').animate({
					height: 0
				},
				200, function() {
				$(this).css('display', 'none').toggleClass('hidden');
			});
		}
	});

    if($('#content .gallery a').length)
    {
        var images = $('#content .gallery a');
        $('#content .gallery').html('').height(400);
        
        for(i = 0; i < images.length; i++)
            $('#content .gallery').append(images[i]);
        
        Galleria.run('#content .gallery');
        Galleria.configure({
            lightbox: true
        });
    }
    
    selection = {
        nonie: function() {
            return window.getSelection ? true : false;
        },

        getText: function() {
            return window.getSelection ? window.getSelection().toString() : (document.selection.createRange ? document.selection.createRange().text : "");
        },

        deselect: function() {
            window.getSelection ? window.getSelection().removeAllRanges() : (document.selection ? document.selection.empty() : false);
        }
    };

    $(document).keypress(function (e) {
        if ((e.keyCode == 10) || (e.keyCode == 13 && e.ctrlKey)) {
            var s = selection.getText();
            var l = s.length;
            var r = window.location.href;
            var t = document.title; 
            
            if (l > 0) {

                if (l > 254) {
                    alert("Длина выделенного фрагмента не может быть больше 254 символов.");
                } else {
                    var msg = "Отправить сообщение об ошибке?";
                    msg += "\n-------------------------------------------------------\n";
                    msg += s;
                    msg += "\n-------------------------------------------------------\n";
                    if (confirm(msg)) {
                        $.post(ajaxurl, { action: 'ctrl_enter', text: s, cap: t, reff: r }, function (data) {
                            selection.deselect();
                        });
                    }
                }
            }
        }
    });
    
    if($('.wpua-edit-container h3').length){$('.wpua-edit-container h3').text('Аватар');}
    if($('.wpua-edit-container #wpua-max-upload-existing').length){$('.wpua-edit-container #wpua-max-upload-existing').text('максимальный размер файла 2 мб.');}
    if($('.wpua-edit-container #wpua-preview-existing .description').length){$('.wpua-edit-container #wpua-preview-existing .description').text('исходный размер');}
});

function addLink() {
    var body_element = document.getElementsByTagName('body')[0];
    var selection;
    selection = window.getSelection();
    //var pagelink = " Источник: <a href='"+document.location.href+"'>"+document.location.href+"</a> © izvestiya.nikolaev.ua";
    var pagelink = " Источник: <a href='http://izvestia.nikolaev.ua/'>izvestia.nikolaev.ua</a>";
    var copytext = selection + pagelink;
    var newdiv = document.createElement('div');
    newdiv.style.position='absolute';
    newdiv.style.left='-99999px';
    body_element.appendChild(newdiv);
    newdiv.innerHTML = copytext;
    selection.selectAllChildren(newdiv);
    window.setTimeout(function() { body_element.removeChild(newdiv); },0);
}
document.oncopy = addLink;

</script>

