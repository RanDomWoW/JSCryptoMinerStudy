<script>

// Grab fresh social data from GH and Twitter
function digits(n){
  return n.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
}

$.ajax({
  dataType: "jsonp",
  url: "https://api.github.com/repos/GeekyAnts/NativeBase",
  success: function(data){
    if(data) {
      if(data.data.watchers){
        $("#gh-stargazers").html(digits(data.data.watchers));
        $('#gh-stars').html(digits(Math.round(data.data.watchers / 100)*100));
      }
      if(data.data.forks){
        $("#gh-forks").html(digits(data.data.forks));
      }
    }
  }
});

$.ajax({
  dataType: "jsonp",
  url: "https://api.github.com/repos/GeekyAnts/NativeBase/tags",
