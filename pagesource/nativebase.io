        var iphoneImageVar = $("#iphone-image");
        var androidImageVar = $("#android-image");
        var iphoneRightSideBarVar = $("#iphone-right-sidebar");
        var androidRightSideBarVar = $("#android-right-sidebar");

        (function(){
            iphoneImageVar.css( "display", "block" );
            androidImageVar.css( "display", "none" );

            iphoneRightSideBarVar.css( "display", "block" );
            androidRightSideBarVar.css( "display", "none" );

            iphoneBtnVar.addClass("active");
            iphone();
        })();

        iphoneBtnVar.on('click', function() {
            androidImageVar.css( "display", "none" );
            iphoneImageVar.css( "display", "block" );

            iphoneRightSideBarVar.css( "display", "block" );
            androidRightSideBarVar.css( "display", "none" );

            iphoneBtnVar.addClass("active");
            androidBtnVar.removeClass("active");
            iphone();
        });

        androidBtnVar.on('click', function() {
            androidImageVar.css( "display", "block" );
            iphoneImageVar.css( "display", "none" );

            iphoneRightSideBarVar.css( "display", "none" );
            androidRightSideBarVar.css( "display", "block" );

            androidBtnVar.addClass("active");
            iphoneBtnVar.removeClass("active");

            android();
        });

        function iphone()
        {
            var device = 'iphone';
            $.get('change-device?device='+device);
        }
        function android()
        {
            var device = 'android';
            $.get('change-device?device='+device);
        }
    </script>
