var truncatePost = function(classname) {
	var ele = $('.' + classname);
	var str = (ele.length) ? ele.html() : '';
	var maxLength = 150;
	if (str && str.length > maxLength) {
		return str.substring(0, 150) + '...';
	}
	return str;
}



