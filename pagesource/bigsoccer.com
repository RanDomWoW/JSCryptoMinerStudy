<script>
	var th_forumListDropdown = function(id) {
		var targetEle = $('.th_openFrom_' + id);
		if (targetEle.length) {
			if (targetEle.hasClass('active')) {
				targetEle.removeClass('active');
				targetEle.css('maxHeight', '');			
			} else {
				targetEle.addClass('active');
				targetHeight = targetEle.find('.th_forumList__dropdownInner').height();
				targetEle.css('maxHeight', targetHeight + 'px');
			}
		}
	}
	
	var th_forumListEmpty = function(e) {
		if (e.target.value.length > 0) {
			$('.forumList_inner').addClass('forumList_inner--searching');		
		} else {
			$('.forumList_inner').removeClass('forumList_inner--searching');
		}
	}
	
	$('#forumListFilterSearch').on('keyup', th_forumListEmpty);
	$('#forumListFilterSearch').on('change', th_forumListEmpty);

	
	var dropdowns = $('.th_forumList__dropdown');
	for (var i = 0, len = dropdowns.length; i < len; i++) {
		var ele = dropdowns[i];
		var parentId = ele.dataset.parent;
		var parentEle = $('.forumList_inner .node_' + parentId);
		if (parentEle.length === 1) {
			parentEle.after('<i class="mdi mdi-chevron-down th_forumList__dropdownTrigger" aria-hidden="true" onclick="th_forumListDropdown(' + parentId + ')"></i>');
		}
	}
	
	var config = {
		hideEmptyCategories: true,
		searchDescriptions: true,
		containerSel: '.th_js-forumListPanel',
		searchSel: '#forumListFilterSearch',
		forumNodeSel: '.th_navItem',
		forumCategoryPrefix: 'category',
		forumNodeCategorySel: null,
		forumNodeCategoryNameSel: 'a',
		
		noResultsBoxSel: '#forumListFilterNoResults'
		
	};

	window.setTimeout(function() {
		themeHouse.forumList.register(config);
	}, 0);
</script>
