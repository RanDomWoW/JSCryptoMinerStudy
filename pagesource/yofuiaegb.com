<script>   
/* _________________________________ FUNCIONES RWD _________________________________ */	
	
	
	var btnMenu = document.getElementById("botonMenuRWD");
	var btnBuscador = document.getElementById("botonBuscadorRWD");

	//Función del botón del menú RWD
	function mostrarMenu(){		
		var menu = document.getElementById('topMenu');
		var buscador = document.getElementById('buscador');
		if(menu.style.display == "" || menu.style.display == "none") {
			menu.style.display = "block";
			buscador.style.display = "none";
		}else {
			menu.style.display = "none";
		}
		return false;
	}	

	//Función del botón del buscador RWD
	function mostrarBuscador(){		
		var menu = document.getElementById('topMenu');
		var buscador = document.getElementById('buscador');		
		if(buscador.style.display == "" || buscador.style.display == "none") {
			buscador.style.display = "block";
			menu.style.display = "none";
		}else {
			buscador.style.display = "none";
		}
		return false;
	}	

	btnMenu.onclick = mostrarMenu;
	btnBuscador.onclick = mostrarBuscador;

	
	//RWD Videos y otro contenido embebido
	jQuery( '.post iframe' ).wrap( '<div class="rwd-container"></div>' );
	
