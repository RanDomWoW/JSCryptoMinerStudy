	<script type="text/javascript" src="/lib/base64.js"></script>
	<script type="text/javascript"><!--
	var T_DAN=[
		"新人","９級","８級","７級","６級","５級","４級","３級","２級","１級",
		"初段","二段","三段","四段","五段","六段","七段","八段","九段","十段",
		"天鳳位"
	];
	var GT_CHIP  = 0x00000200; // 祝 512
	var GT_JANS  = 0x00000400; // 雀 1024
	function GT_TAKU(w){return ( (w&0x0020)>>4 | (w&0x0080)>>7 );} // 0=一般 1=上級 2=特上 3=鳳凰
	function GT_ISJANS(w){return ( ((w)&(GT_CHIP|GT_JANS))!=0 );}	// 雀荘戦
	function toSafeHTML(s){return s.replace(/&/g,"&amp;").replace(/</g,"&lt;").replace(/>/g,"&gt;");}
	function unescapeBase64SafeHTML(un){return un?toSafeHTML(decodeURIComponent(Base64.decode(un))):"";}
	function sw(a){
		for(var i=0;i<a.length;++i){
			var aa=a[i]=a[i].split(",");
			aa[1]=~~aa[1];
			for(var who=0;who<4;++who){
				var dan=aa[5+who*3]=~~aa[5+who*3];
				aa[1]+=dan*dan*100; // dan
			}
		}
		a.sort(function(a,b){return b[1]-a[1];});
		function aa2s(aa){
			var gt=aa[3];
			var wg="/0/?wg="+aa[0];
			var un=[
				unescapeBase64SafeHTML(aa[ 4]),
				unescapeBase64SafeHTML(aa[ 7]),
				unescapeBase64SafeHTML(aa[10]),
				unescapeBase64SafeHTML(aa[13])
			];
			var dan =[~~aa[5],~~aa[8],~~aa[11],~~aa[14]];
			var rate=[~~aa[6],~~aa[9],~~aa[12],~~aa[15]];
			var s="<div class=e>"+
				"| 観戦 | "+aa[2]+" | "+
				(gt&0x0010?"三":"四")+
				("般上特鳳若銀琥孔".substr((GT_ISJANS(gt)?4:0)+GT_TAKU(gt),1))+
				(gt&0x0008?"南":"東")+(GT_ISJANS(gt)?
				"祝"+(~gt&GT_CHIP?"０":gt&GT_JANS?"５":"２")
				:
				(gt&0x0004?"":"喰")+
				(gt&0x0002?"":"赤")+
				(gt&0x0040?"速":""))+
				"<span style=\"white-space:nowrap;\"> | "+wg+"</span></div>";
			for(var who=0;who<4;++who){
				if (!un[who]) continue;
				s+="<div class=b><span class=dan"+dan[who]+">"+T_DAN[dan[who]]+"R"+rate[who]+"</span>/<a href=\""+wg+"&tw="+who+"\">"+un[who]+"</a></div>";
			}
			return s;
		}
		var s="";
		var n=2;
		for(var i=0;i<a.length;++i){
			var gt=a[i][3];
			if (gt&0x0010 || GT_TAKU(gt)<3) continue; // 四鳳のみ
			s+=aa2s(a[i]);
			if (!--n) break;
		}
		var n=1;
		for(var i=0;i<a.length;++i){
			var gt=a[i][3];
			if (~gt&0x0010 || GT_TAKU(gt)<3) continue; // 三鳳のみ
			s+=aa2s(a[i]);
			if (!--n) break;
		}
		document.write(s);
	}
	//--></script>
