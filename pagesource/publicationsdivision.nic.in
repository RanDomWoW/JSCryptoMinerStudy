   <script language="javascript" type="text/javascript">
       var timerid = 0;
       var images = new Array("Bharat 2018 Cover Page.PNG", "Bharat 2018 Cover Page.PNG");
       var countimages = 0;
       function startTime() {
           if (timerid) {
               timerid = 0;
           }
           var tDate = new Date();

           if (countimages == images.length) {
               countimages = 0;
           }
           if (tDate.getSeconds() % 5 == 0) {
               document.getElementById("img1").src = images[countimages];
           }
           countimages++;

           timerid = setTimeout("startTime()", 1000);
       }
</script>
