       });
    });
    function getParameterByName(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        switch(decodeURIComponent(results[2].replace(/\+/g, " "))){
            case 'broker':
                $(".select-market").val("/broker?selected=broker");
                break;
            case 'association':
                $(".select-market").val("/association?selected=association");
                break;
            case 'merchant':
                $(".select-market").val("/merchant?selected=merchant");
                break;
            case 'employee':
                $(".select-market").val("/employee?selected=employee");
                break;
            default:
                $(".select-market").val("/?selected=employer");
        }
    }
