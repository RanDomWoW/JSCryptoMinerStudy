    function getParameterByName(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        switch(decodeURIComponent(results[2].replace(/\+/g, " "))){
            case 'broker':
                $("#bottom_selector").val("/broker?selected=broker");
                break;
            case 'association':
                $("#bottom_selector").val("/association?selected=association");
                break;
            case 'merchant':
                $("#bottom_selector").val("/merchant?selected=merchant");
                break;
            case 'employee':
                $("#bottom_selector").val("/employee?selected=employee");
                break;
            default:
                $("#bottom_selector").val("/?selected=employer");
        }
