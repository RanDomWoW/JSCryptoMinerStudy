    (function ($) {
        $(function () {
            var block = $('.branding-box');
            if (!block.length) {
                return
            }
            var offset_top = block.offset().top;
            var block_height = 1080;
            var all_block_height = offset_top + block_height;
            var window_height = $(window).height();
            var scroll_max = all_block_height - window_height;

            $(window).scroll(function () {
                if ($(window).scrollTop() > scroll_max) {
                    block.addClass('branding-fixed');
                } else {
                    block.removeClass('branding-fixed');
                }
            });

            $(window).resize(function () {
                var window_height = $(window).height();
                var scroll_max = all_block_height - window_height;
            });
        });
    })($);
</script>

