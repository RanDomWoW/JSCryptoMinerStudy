<script>
    /*
     *  Add title to node news page
     */
    (function ($) {
        $(function () {
            var links = $('a');
            var ret_all = '';
//            debugger;
            var body = $('body');
            var body_classes = body.attr('class');
            var categories_reg = /\bpage-news-([\w-]*)\b/;
            var news_reg = /\bnode-type-news\b/;
            var front_reg = /\sfront\b/;
            var domain_reg = /\bdomain-([\w-]*)\b/;

            var path;
            if (path = body_classes.match(front_reg)) {
                ret_all += path[0];
            } else if (path = body_classes.match(categories_reg)) {
                ret_all += 'ct_' + path[1];
            } else if (path = body_classes.match(news_reg)) {
                ret_all += 'article';
            } else {
                ret_all += 'default';
            }
            ret_all +='_';
            if (path = body_classes.match(domain_reg)) {
                ret_all += 'dm-' + path[1];
            } else {
                ret_all += 'dm-znaj-ua';
            }


            var lang = ($('html').attr('lang') === 'ru') ? 'ru' : 'uk';

            var views = $('.view');

            var blocks = $('*[id^="block-"]');

            blocks.each(function (item) {
                var block = $(this);
             //   debugger;
                if (block.find('[id^="block-"]').length || block.find('.view').length){
                    return;
                }
                var block_id = block.attr('id');
                var a = block.find(' a');
                a.each(function (item) {
                    var link = $(this);
                    link.addClass(ret_all + '_'+block_id + '_' + lang);
                });
            });

            views.each(function (item) {

                var view = $(this);
                var classNames = view.attr('class');
                var view_id_reg = /view-id-([\w-]*)\b/;
                var view_id, view_display_id;
                var view_display_id_reg = /view-display-id-([\w-]*)\b/;
                var a = view.find('a');
                var ret;
                if ((view_id = classNames.match(view_id_reg)) && (view_display_id = classNames.match(view_display_id_reg))) {
                    ret = '_vid_' + view_id[1] + '_vdspid_' + view_display_id[1];
                    is_founded = true;
                }
                a.each(function (item) {

                    var link = $(this);
                    link.addClass(ret_all + ret + '_' + lang);
                });
            });
        });
    })(jQuery);
</script>

