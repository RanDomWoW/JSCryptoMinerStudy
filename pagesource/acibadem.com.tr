    <script>
        
        $(function () {
            toastr.options = {
                closeButton: true,
                positionClass: "toast-bottom-full-width",
                showDuration: "1000",
                hideDuration: "1000",
                timeOut: "5000",
                extendedTimeOut: "3000",
            };

            $('#sbDegerlendir').on("click", function () {
                var txt = $('#guvenlik').val();
                var err = '';


                if ($('input[name=usability]:checked').val() == null && $('input[name=instantAccess]:checked').val() == null && $('input[name=design]:checked').val() == null && ($('#oneri').val() == null || $('#oneri').val() == 'Fikirleriniz bizim için çok önemli...')) {
                    toastr.error('Tüm alanlar boş görünüyor. Lütfen alanları doldurup tekrar deneyiniz.');
                    return false;
                }


                $.ajax({
                    type: "POST",
                    url: "/home/ValidateCaptcha",
                    data: "{CaptchaValue:'" + txt + "'}",
                    contentType: "application/json; charset=utf-8",
                    async: false,
                    dataType: "json",
                    success: function (data) {
                        if (data == '0') {
                            captcha(data);
                            err = "Güvenlik Kodunu Hatalı Girdiniz.";
                        }
                    }
                });

                if (err != '') {
                    toastr.error(err);
                } else {
                    return true;
                }

                return false;

            });

        });

        function RatingReturn(data) {
            captcha('');
            switch (data.result) {
                case 1:
                    toastr.success(data.message);
                    break;
                default:
                    toastr.success(data.message);
            }
        }

        function captcha(t) {
            $.ajax({
                url: '/Home/Show2',
                type: "POST",
                contentType: "image/png",
                success: function (data) {
                    $('#imgCaptcha').attr('src', data.imageList[0]);
                },
                error: function (xhr, textstatus, error) {
                    toastr.error(error);
                }
            });

            $('#guvenlik').val('').focus();
            if (t != "")
                toastr.error(t);
        }
    </script>
