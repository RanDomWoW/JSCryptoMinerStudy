				nextButton: '.arrow-right',
				prevButton: '.arrow-left',
				parallax: true,
			});
			$("#tj_btn").click(function() {
				var tel_reg = /^(((0\d{2,3})-?)?\d{7,8}|((\+?86)-?|(\(\+86\))-?)?1\d{10})$/;
				var email_reg = /^(\w)+(\.\w+)*@(\w)+((\.\w+)+)$/;
				var company_name = $(".company_name").val()
				var name = $(".name").val()
				var phone = $(".phone").val()
				var mailbox = $(".mailbox").val()
				var content = $(".content").val()
                if (phone == ""){
                	$(".tj_e").html("手机号不能为空")
					window.setTimeout(function () {
									 $(".tj_e").html("")
									}, 2000);
                }else if(!tel_reg.test(phone)){
					$(".tj_e").html("手机号格式不正确")
					window.setTimeout(function () {
									 $(".tj_e").html("")
									}, 2000);
				}else if(mailbox !="" && !email_reg.test(mailbox)){
					$(".tj_e").html("邮箱号格式不正确")
					window.setTimeout(function () {
									 $(".tj_e").html("")
									}, 2000);
				}else if(content ==""){
					$(".tj_e").html("内容不能为空")
					window.setTimeout(function () {
									 $(".tj_e").html("")
									}, 2000);
				}else {
					$.ajax({
							type: "POST",
							url: "/app/feedback",
							dataType: 'json',
							data: {
								"company_name":company_name,
								"name":name,
								"phone":phone,
								"mailbox":mailbox,
								"content":content,
								"site_id":"2"
							},
							success: function (data) {
								if(data == 1){
									$(".tj_e").html("提交成功")
								 window.setTimeout(function () {
								  $("input").val("")
									$("textarea").val("")
									 $(".tj_e").html("")
									}, 2000);
								}else if(data ==2){
									$(".tj_e").html("提交失败")
								}

							}
					})
				}
			})
		</script>
