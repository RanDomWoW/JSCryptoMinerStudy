<script type="text/html" id="productEntryTemplate">
    {{#productsList}}
        <div>
            <div class="content item">
                <div class="product">
                    {{^useWishlistPopup}}
                        <div class="add-to-wishlist">
                            <span class="js-add-to-wishlist icon-heart-empty" data-url="/addToWishlist/"
                                  data-product-code="{{code}}">
                                <span class="wishlist-label">Lieblingsprodukte</span>
                            </span>
                            <span class="js-wishlist-message icon-heart" style="display: none">
                                <span class="wishlist-label">Hinzugefügt!</span>
                            </span>
                        </div>
                    {{/useWishlistPopup}}

                    {{#useWishlistPopup}}
                        <div class="add-to-wishlist">
                            <span class="js-add-to-wishlist icon-heart-empty" data-url="/wishlist/addToWishlist/"
                                    data-product-code="{{code}}"
                                    useWishlistPopup="{{useWishlistPopup}}">
                            </span>
                            <span class="js-wishlist-message icon-heart" style="display: none"></span>
                        </div>
                    {{/useWishlistPopup}}

                    <div class="info-badges">
                        <ul class="list-inline">
                            {{#isOffer}}
                                <li>
                                    <div class="discount">
                                        <span class="percentage-value">{{discount}}</span>
                                    </div>
                                </li>
                            {{/isOffer}}
                        </ul>
                    </div>

                    <div class="image-wrapper">
                        <a href="{{url}}?recType={{recommendationType}}">
                            {{#sliderConfig}}
                                {{#lazyLoad}}
                                    <img alt="{{name}}" title="{{name}}"
                                         data-lazy="{{cdnRoot}}/{{#template}}{{imageFormat}}{{/template}}/{{imageLinks | getByIndex:0}}">
                                    <span class="custom-loading"></span>
                                {{/lazyLoad}}
                            {{/sliderConfig}}

                            {{#sliderConfig}}
                                {{^lazyLoad}}
                                    <img alt="{{name}}" title="{{name}}"
                                         src="{{cdnRoot}}/{{#template}}{{imageFormat}}{{/template}}/{{imageLinks | getByIndex:0}}">
                                {{/lazyLoad}}
                            {{/sliderConfig}}

                            {{^sliderConfig}}
                                <img alt="{{name}}" title="{{name}}"
                                     src="{{cdnRoot}}/{{#template}}{{imageFormat}}{{/template}}/{{imageLinks | getByIndex:0}}">
                            {{/sliderConfig}}
                        </a>
                    </div>

                    <div class="description-wrapper">
                        <div class="title-wrapper">
                            <a href="{{url}}?recType={{recommendationType}}">
                                <h4 class="truncate-text" title="{{name}}">{{name}}</h4>
                            </a>
                        </div>

                        <div class="manufacturer truncate-text">
                            <a href="/search/?q=::manufacturer:{{manufacturerCompany.manufacturerURL}}">{{manufacturerCompany.displayName}}</a>
                        </div>

                        <div class="rating-wrapper clearfix">
                            <div class="prodReview">
                                <span class="stars " style="width: {{averageRatingByCurrentStore | multiply:21}}px;"></span>
                            </div>
                            <div class="total-ratings">
                                ({{totalNoOfReviews}})
                            </div>
                        </div>

                        <div class="price">
                            <div class="old-price">
                                {{#oldPrice}}
                                    <span>statt</span>
                                    <span class="initial-price reduced">{{oldPrice.formattedValue}}</span>
                                {{/oldPrice}}
                            </div>
                            <p>{{price.formattedValue}}*</p>
                        </div>
                        <div class="quantity-details">
                            <span class="total-weight">{{contentUnit}}</span>
                                {{^isGiftVoucher}}
                                    {{#basePrice.formattedValue}}
                                        <span class="price-per-unit-measure">({{basePrice.formattedValue}})</span>
                                    {{/basePrice.formattedValue}}
                                {{/isGiftVoucher}}
                        </div>

                        <div class="stock-and-delivery">
