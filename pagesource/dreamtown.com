<script>

    var BrokerBuyModal = {
        $modal: $('[data-remodal-id="broker-buy"]'),
        $form: $('[data-remodal-id="broker-buy"] form'),
        $loggedModal: $('[data-remodal-id="request-info-confirmed"]'),
        $notLoggedModal: $('[data-remodal-id="request-info-notlogged"]'),
        isLoggendIn: false,
        Close: function() {
          BrokerBuyModal.$modal.remodal().close();
        },
        OpenLoggedModal: function () {
          BrokerBuyModal.$loggedModal.remodal().open();
        },
        OpenNotLoggedModal: function (form) {
            var registerForm = BrokerBuyModal.$notLoggedModal.find('form').get(0);
            registerForm.firstName.value = form["data['First Name']"].value;
            registerForm.lastName.value = form["data['Last Name']"].value;
            registerForm.email.value = form["data['Email']"].value;
            registerForm.phone.value = form["data['Phone']"].value;
            BrokerBuyModal.$notLoggedModal.remodal().open();
        },
        GetFormData: function (form) {
            var jsonObj = {
                agentId: form.agentId.value,
                subject: form.subject.value,
                data: {
                    'First Name': form["data['First Name']"].value,
                    'Last Name': form["data['Last Name']"].value,
                    'Email': form["data['Email']"].value,
                    'Phone': form["data['Phone']"].value,
                    'Comment': form["data['Comment']"].value,
                    'Contact Method': form["data['Contact Method']"].value,
                    'Looking For': form["data['Looking For']"].value
                }
            };

            return JSON.stringify(jsonObj);
        },
        EnableLoadingAnimation: function (form) {
          $(form).find('.js-form-loading').removeClass('hidden').parent().addClass('disabled');
        },
        DisableLoadingAnimation: function (form) {
          $(form).find('.js-form-loading').addClass('hidden').removeClass('disabled');
        }
    };

    $('[data-remodal-id="broker-buy"] form').on('submit', function(event) {
        event.preventDefault();
        var self = this;

        BrokerBuyModal.EnableLoadingAnimation(this);

        $.ajax({
            url: this.action,
            type: this.method,
            contentType: "application/json; charset=utf-8",
            dataType: 'json',
            data: BrokerBuyModal.GetFormData(this)
        })
        .done(function() {
            BrokerBuyModal.Close();
            BrokerBuyModal.DisableLoadingAnimation(self);

            if (BrokerBuyModal.isLoggendIn) { BrokerBuyModal.OpenLoggedModal(); }
            else { BrokerBuyModal.OpenNotLoggedModal(self); }
        });

    });
</script>



