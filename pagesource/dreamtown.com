<script>
    var LoginCenteredModal = {
        $modal: $('[data-remodal-id="login-centered"]'),
        $loginForm: $('[data-remodal-id="login-centered"] form'),
        isLoggendIn: false,
        OpenLoggedModal: function () {
            LoginCenteredModal.$loggedModal.remodal().open();
        },
        Close: function() {
            LoginCenteredModal.$modal.remodal().close();
        },
        EnableLoadingAnimation: function (form) {
            $(form).find('.js-form-loading').removeClass('hidden').parent().addClass('disabled');
        },
        DisableLoadingAnimation: function (form) {
            $(form).find('.js-form-loading').addClass('hidden').removeClass('disabled');
        }
    };

    // LoginCenteredModal.$modal.remodal({
    //     closeOnEscape: false,
    //     closeOnOutsideClick: false
    // });

    LoginCenteredModal.$loginForm.on('submit', function(event) {
        event.preventDefault();
        var self = this;

        LoginCenteredModal.EnableLoadingAnimation(this);

        $.ajax({
            url: this.action,
            type: this.method,
            dataType: 'html',
            data: $(this).serialize()
        })
        .done(function(result) {
            LoginCenteredModal.DisableLoadingAnimation(self);
            $.each($(result), function(index, val) {
                if(val.nodeName === 'TITLE' && val.textContent === "Haven't Activated Your Account") {
                    console.log(val.textContent);
                    if (LoginCenteredModal.$modal.find('.content>p').length === 0) {
                        LoginCenteredModal.$modal.find('.content').append("<p>Haven't activated your account. Please check your email address.</p>");
                    }
                }
                else {
                  if (typeof loginSuccess === 'function' && !self.isLoggendIn) {
                    self.isLoggendIn = true;
                    loginSuccess();
                  }
                  window.location.href = window.location.href;
                }
            });
        });

    });
</script>


