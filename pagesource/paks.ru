function updateFormField() { 
  
  $.getJSON('/rs/form_tour/', {cid: $('#sel_country').val()}, function(data) {
    
    $('#sel_tour').empty().append(data['tour']).parents('.jquery-selectbox').unselectbox();
    $('#sel_tour').selectbox();
    
    $('#sel_night').empty().append(data['night']).parents('.jquery-selectbox').unselectbox();
    $('#sel_night').selectbox();
    
    $('#sel_date').empty().append(data['date']).parents('.jquery-selectbox').unselectbox();
    $('#sel_date').selectbox();
    
  });

}

function customSubmit(tab) {
  
  var depFrom = $('#sel_depart').val();
  
  if (!depFrom) depFrom = 1;
  
  var url = '?departfrom=' + depFrom + '&country=' + $('#sel_country').val() + '&nights=' + $('#sel_night').val();
  
  url += '&dateFrom=' + $('#sel_date').val() + '&tourtype=' + $('#sel_tour').val();
  
  if (tab == 'agency') url = 'http://online.paks.ru/mw6/Extra/QuotedDynamic2.aspx' + url + '&hotelQuotaMask=5&aviaQuotaMask=5';
  else url = 'https://www.paks.ru/market/search/' + url + '&hotelQuotaMask=5&aviaQuotaMask=5';
  
  location.href = url;
  
  return false;
  
}

$(document).ready(function() {
    
    $('#date1').datepicker({
      minDate: 0,
      numberOfMonths: 2,
      showButtonPanel: true,
      onClose: function (input, inst) {
        
      	var startDate = new Date(input.replace(/(\d{2})\.(\d{2})\.(\d{4})/, '$3-$2-$1'))
      	  , finDate = new Date($('#date2').val().replace(/(\d{2})\.(\d{2})\.(\d{4})/, '$3-$2-$1'));
      	
      	if (startDate > finDate) {
      	  
      		finDate.setDate(startDate.getDate() + 7 * 0);
      		
      		var fDate = finDate.getDate();
      		
      		if (fDate < 10) fDate = '0' + fDate.toString();
      		
      		var fMonth = finDate.getMonth() + 1;
      		
      		if (fMonth < 10) fMonth = '0' + fMonth.toString();
      		
      		$('#date2').val(fDate + '.' + fMonth + '.' + finDate.getFullYear());
      		
      	}
      }
    });
    
    $("#filter_cal_but_from").click(function () {
      
      $('#date1').datepicker('show');
      
    });
  
    $('#date2').datepicker({
        
        minDate: 0,
        numberOfMonths: 2,
        showButtonPanel: true,
        beforeShow: function(input, inst) { 
          
          if ($('#date1').val().match(/^\s*\d{2}\.\d{2}\.\d{4}\s*$/)) {
            
            inst.settings.minDate = $.datepicker.parseDate($(this).datepicker("option", "dateFormat"), $('#date1').val());
            
            inst.settings.minDate.setDate(inst.settings.minDate.getDate() + 0);
            
          }
          
        }
        
    });
    
    $("#filter_cal_but_to").click(function () {
      
      $('#date2').datepicker('show');
      
    });
    

    $('#tf_market_btn').click(function() {
      
        $('form[name="tf_market"]').submit();
        
        return false;
        
    });
    
    $('#tf_market_btn').removeAttr('onlick');

})

