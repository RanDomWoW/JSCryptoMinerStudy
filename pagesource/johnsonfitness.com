  $(function() {
    var mq = window.matchMedia("(min-width: 768px)");

    // Our two megamenu thingers. Caching them here to avoid
    // re-selecting 'em every time a repositioning function
    // is invoked.
    var $navPanels = $(".nav-dropdown");

    // Magento's native nav markup
    var $mageNav = $("nav.navigation");

    // There are two .widget.block.block-static-block containers
    // as children of .top-container. This ensures nav stuff only
    // gets swapped around inside the correct one.
    var $navContainer = $(
      ".top-container > .widget.block.block-static-block"
    ).first();

    var $algContainer = $("#algolia-autocomplete-container");
    var $searchBlock = $(".block.block-search");
    var $topContainer = $(".top-container");
    var $pageHeader = $(".page-header.page-header-v3");
    var $headerRight = $(".header_right");
    var $logo = $(".logo img");
    var $logoSrcDefault = $logo.attr("src");
    // var algContainer = document.getElementById();
    // var searchBlock = document.querySelector(".block.block-search");
    // if (searchBlock !== null) {
    //   searchBlock.appendChild(algContainer);
    // }

    function positionSearchMobile() {
      $searchBlock.insertAfter($pageHeader).hide();
      $algContainer.prependTo($topContainer);
    }

    function positionSearchDesktop() {
      $searchBlock.insertAfter($headerRight).fadeIn();
      $algContainer.appendTo($searchBlock);
    }

    $(".panel.wrapper").appendTo(".header.content");

    function positionNavMobile() {
      $navPanels.show().appendTo("nav.navigation");
      $("#cat-drop-btn").addClass("active");
      $("#category-nav").show();
      $("#info-nav").hide();
    }

    function positionNavDesktop() {
      $navPanels.hide().prependTo($navContainer);
      $(".jfit_nav__main-dropdown-link").removeClass("active");
      $(".mobile-hide").show();
    }

    function swapLogoMobile() {
      $logo.attr(
        "src",
        window.location.origin +
          "/media/wysiwyg/JohnsonFitness/jfw-logo-red.svg"
      );
    }

    function swapLogoDesktop() {
      $logo.attr("src", $logoSrcDefault);
    }

    function widthChange(mq) {
      if (mq.matches) {
        positionNavDesktop();
        positionSearchDesktop();
        swapLogoDesktop();
      } else {
        positionNavMobile();
        positionSearchMobile();
        swapLogoMobile();
      }
    }

    mq.addListener(widthChange);
    widthChange(mq);

    $("#header-nav")
      .prependTo("nav.navigation")
      .delay(500)
      .fadeIn();

    $("#nav-bg-left")
      .prependTo("header.page-header")
      .addClass("loaded");

    $("header.page-header")
      .delay(500)
      .addClass("loaded");

    $("#cat-drop-btn").click(function() {
      $("#info-drop-btn").removeClass("active");
      $(this).toggleClass("active");
      $("#info-nav").hide();
      $("#category-nav").slideToggle();
      return false;
    });

    $("#info-drop-btn").click(function() {
      $("#cat-drop-btn").removeClass("active");
      $(this).toggleClass("active");
      $("#category-nav").hide();
      $("#info-nav").slideToggle();
      return false;
    });

    $("span.jfit_nav__group-head").click(function() {
      $(this)
        .next(".mobile-hide")
        .slideToggle();
    });

    if (mq.matches) {
      $("#search_mini_form")
        .delay(500)
        .fadeIn();
    }
  if ($('body').hasClass('review-product-list')) {
    var $storemenu = $(document.getElementById('store.menu'));
    $storemenu.html('<nav class="navigation" data-action="navigation"');
    $storemenu.css('display', 'block');
    $('#header-nav').prependTo($storemenu).fadeIn();
  }
  });
});
// document.addEventListener("DOMContentLoaded", function() {});

</script></div>
