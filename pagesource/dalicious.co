function arrangeServiceSection() {
  var $wrapperObject = $(".service--info--wrapper"),
          $coverObject = $(".service--info--cover");

  var height = $wrapperObject.height(),
          width = $wrapperObject.width();

  var windowWidth = $(window).width();

  if (windowWidth > 768) {
    $coverObject.width(width + 52);
  } else {
    $coverObject.width(width + 50);
  }

  $coverObject.height(height + 8);
}

$(window).resize(function () {
  arrangeEatIcon();
  arrangeServiceSection();
});

