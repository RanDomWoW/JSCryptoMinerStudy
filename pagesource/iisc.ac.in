<script>
	function showSameGroup(target1, target2, el){
    		if(target1 == el){
        		$(target2).find("option").show().not("."+el).hide();
    		}
	};


	$("#cbcat input[type='checkbox']").click(function(){
	// alert ("Test");
		var s1 = $(this).val(), s2 = $("#catne");
    
    		if(s1 == "News"){
        		showSameGroup(s1, s2, "News");
    		}
    		else if(s1 == "Events"){
        		showSameGroup(s1, s2, "Events");
    		}
	});		
</script>
