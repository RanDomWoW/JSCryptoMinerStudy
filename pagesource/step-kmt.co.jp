    <script>
        (function(){
            var $ = jQuery;
            $('#js-humberger').on('click', function() {
                var num = $('#js-menu').children('li').length;
                if($('#js-menu').hasClass('on')) {
                    $('#js-menu').attr('style', 'height: 0px');
                    $('#js-menu').removeClass('on');
                    $('#js-menu li').removeClass('on').find('ul').attr('style', 'height: 0')
                } else {
                    $('#js-menu').attr('style', 'height: ' + num * 51 + 'px');
                    $('#js-menu').addClass('on');
                }
            })
            $('#js-menu li').on('click', function() {
                var num = $(this).find('li').length;
                if(!$(this).hasClass('more')) return;
                if($(this).hasClass('on')) {
                    var parentNum = $('#js-menu').height() - (num * 51);
                    $(this).find('ul').attr('style', 'height: 0px');
                    $('#js-menu').attr('style', 'height: ' + parentNum + 'px');
                    $(this).removeClass('on');
                } else {
                    var parentNum = $('#js-menu').height() + (num * 51);
                    $(this).find('ul').attr('style', 'height: ' + num * 51 + 'px');
                    $('#js-menu').attr('style', 'height: ' + parentNum + 'px');
                    $(this).addClass('on');
                }
            });

            $(window).on('scroll', function() {
                if($(window).scrollTop() > 500) {
                    $('#js-telButton').attr({'style': 'display: block'});
                } else {
                    $('#js-telButton').attr({'style': ''});
                }
            });
        })();
    </script>


