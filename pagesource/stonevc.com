    <script type="text/javascript">
    $(document).ready(function(){
        //banner
        ;(function(){
            var winW=$(window).width();
            winW=winW<=1000?1000:winW;//设定最小宽度为1000
            $('#banner_inner .banner_ul').append($('#banner_inner .banner_ul li:eq(0)').clone());//复制第一张插入到最后
            $('#banner_inner,#banner_inner .banner_ul li').width(winW);//设置单张宽度及外层的盒子等同宽度
            $('#banner_inner .banner_ul').width(winW*$('#banner_inner .banner_ul li').length);//设置滚动盒子的宽度
            $(window).resize(function(){//浏览器窗口大小改变时重置
                winW=$(window).width();
                winW=winW<=1000?1000:winW;
                $('#banner_inner,#banner_inner .banner_ul li').width(winW);
                $('#banner_inner .banner_ul').width(winW*$('#banner_inner .banner_ul li').length+1);
            });
            $(".banner_control li").click(function(){//添加点击事件
                i=$(this).index();//记住当前显示的索引值
                clearTimeout(timer);
                timer=null;//清除定时器
                $(".banner_control li").removeClass("curr");
                $(this).addClass("curr");
                $(".banner_inner").animate({"scrollLeft":winW*$(this).index()},function(){
                    timer=setTimeout(auto,speed);//手动执行完毕启动定时器
                });
            });
            var i=0,timer=null,speed=5000;
            function auto(){
                if(!$(".banner_inner").is(":animated")){
                    if($(".banner_inner").scrollLeft()>=winW*($('#banner_inner .banner_ul li').length-2)){
                        i=0;
                        $(".banner_control li").removeClass("curr");
                        $(".banner_control li:eq(0)").addClass("curr");
                        $(".banner_inner").animate({"scrollLeft":winW*($('#banner_inner .banner_ul li').length-1)},function(){
                            $(".banner_inner").scrollLeft(0);
                        });
                    } else {
                        $(".banner_control li").removeClass("curr");
                        i++;
                        $(".banner_control li:eq("+i+")").addClass("curr");
                        $(".banner_inner").animate({"scrollLeft":winW*i});
                    }
                }
                timer=setTimeout(arguments.callee,speed);
            }
            timer=setTimeout(auto,speed);
        })();
    });
    </script>
