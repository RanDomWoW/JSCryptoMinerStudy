function changeList(obj, id, event)
{
	var obj2 = document.getElementById(id).getElementsByTagName("dl");
	event = event || window.event;
	if (event.type == "mouseover") {
		for (var i = 0; i < obj2.length; i++) {
			if (obj2[i].className == "") {
				if (i != obj2.length - 1) {
					obj2[i].className = "bg";
				} else {
					obj2[i].className = "bg nobg";
				}
			}
		}
		obj.className = "";
	}
}
</script>
<script type="text/javascript"> 
function setTab(name,cursel,n,class1,class2,class3,class4,src){
	for(i=1;i<=n;i++){
		var menu=document.getElementById(name+i);
		var con=document.getElementById("con_"+name+"_"+i);
		var td=document.getElementById("td_"+name+"_"+i);
		menu.className=i==cursel?class2:class1;
		td.className=i==cursel?class4:class3;
		con.style.display=i==cursel?"block":"none";
	}
	//document.getElementById(name+"_url").href=""+src;
}
</script>

