<script type="text/javascript">
	'use strict'
	var channelList = {};
	var weekList = {};
	var weekEvents = {};
	var epgList = {};
	var GroupColorList = {};
	var t_s0;

	function loadWeekDates2(d2) {
	    $.ajax({
	        type: "GET",
	        async: true,
	        url: '/modules/sima/sima_uws.asmx' + "/GetTVConductorData?day=" + d2 + "&chID=" + 21,
	        success: function (msg) {
	            weekList = msg.days;
	            epgList = msg.epglist
	            renderFullConductor();
	            loadNow();
	            $('[data-toggle="popover"]').popover();
	            var t3 = performance.now();
	            console.log("Load Conductor took: " + (t3 - t_s0) + " milliseconds.");
	        },
	        error: function (jqXHR, textStatus, errorThrown) {
	            //alert('Error Message: ' + textStatus);
	           // alert('HTTP Error: ' + errorThrown);
	        }
	    });
	};

    var renderFullConductor = function () {
        var t0 = performance.now();
        var $rc;
        var chID =21;
        $rc = $('<div class="requestchannel" data-channel="' + chID + '" />');
        $('.timespans').append($rc);
        $.each(weekList, function (index, value) {
        	renderSimaProgs(chID, value.Date, value.SequenceValue + 1, 300, 5, true, 7200
				, 0, 1, 1032, value.DayType, "portal", 69);
        });
        //});
        var t1 = performance.now();

        //renderDateLabels(weekList);
        //renderEvents(weekEvents);

        var t2 = performance.now();
        setSimaHeights3(35, 1, 25);
        var t3 = performance.now();
        //var z = parseInt($("#cmbCount").val());
        //z = z * -86;
        //$(".requestchannel").css("right", z + "px");
        //$(".timesplit").css("right", z + "px");
