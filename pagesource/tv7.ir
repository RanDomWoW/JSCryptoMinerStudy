        //var y14 = Math.abs(z) + 1031;
        //$("div.requestchannel").width(y14 + "px");
    };

	var loadNow = function () {
		var dt = new Date(1521482192284);
		var h = (dt.getHours() - 1);
		if (h <= 0) {
			h = 0;
		}
		var m = dt.getMinutes();

		var hourmarker = parseInt(h) * 300 + 300;
		var minutemarker = parseInt(m) * 5;

		if (h > 21) h = 21;
		var margin = h * 300;
		var maxMargin = parseInt($('.timeline').width());
		if (margin > maxMargin)
			margin = maxMargin;
		$('.timespans').animate({
			marginRight: "-" + margin
		}, 1000);

		var $timeSplit = $('<div class="cu-marker" />');

		var $rm = hourmarker + minutemarker;

		$timeSplit.css('right', $rm);
		//$timeSplit.css("font-size", "9px");
		$('.timespans').append($timeSplit);
	};

	var init = function () {
		$('.channeldata,.channels,.timespans').empty();
		loadTimespan();
		loadWeekDates2('1396/12/28');
		//loadWeekEvents2('http://77.36.165.69/Modules/Conductor/ConductorWS.asmx'); 
		//loadDateInfo();
		$('.timespans').on('mousewheel', function (event) {
			if ($('.timespans').is(':animated')) {
				event.preventDefault();
				return false;
			}
			var moveMargin = event.deltaY * 8 * event.deltaFactor * -1;
			var maxMargin = parseInt($('.timeline').width()) - parseInt($('.timelinewrapper').width());
			var curmargin = parseInt($('.timespans').css('margin-right'));
			if (curmargin + moveMargin >= 0) {
				$('.timespans').animate({
