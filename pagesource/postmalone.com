    aeJS.events.onLogin.addHandler(function(user, type){ require(["IGA.events"], function(events){ events.trigger("ae_login", [user, type]); }); });
    aeJS.events.onLogout.addHandler(function(){ require(["IGA.events"], function(events){ events.trigger("ae_logout"); }); });
    aeJS.events.onWindow.addHandler(function(state){ require(["IGA.events"], function(events){ events.trigger("ae_window", [state]); }); });
    aeJS.events.onFlow.addHandler(function(step){ require(["IGA.events"], function(events){ events.trigger("ae_flow", [step]); }); });
    aeJS.events.onMobileDetect.addHandler(function(){ require(["IGA.events"], function(events){ events.trigger("ae_mobile_detect"); }); });
    if(typeof require !== "undefined" && require.specified("IGA.events")){
      require(["IGA.events"], function(events){
        events.trigger("ae_ready", [aeJS]);
      });
    }
  }
</script>
