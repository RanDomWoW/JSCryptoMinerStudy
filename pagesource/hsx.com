                    .each(function () {
                        var $this_html = $(this).html();
                        for (var x = 0; x < number_of_loops; x++) {
                            $(this).append($this_html);
                        }
                    })
                    .marquee('pointer')
                    .each(function () {
                        marquee_list.push(this);
                    })
                    .mouseover(function () {
                        $(this).trigger('stop');
                    })
                    .mouseout(function () {
                        $(this).trigger('start');
                    })
                    .bind('textfieldfocus', function () {
                        $(this).trigger('stop');
                    })
                    .bind('textfieldblur', function () {
                        $(this).trigger('start');
