<script type="text/javascript">
var ibf = new IBfortune('0,1');
var cck = new CreateCookie();

cck.GetSegment();

birthday_year = '';
birthday_month = '';
birthday_day = '';
if(cck.login == '1'){
  birthday_year = cck.birthday_year;
  birthday_month = cck.birthday_month;
  birthday_day = cck.birthday_day;
  date_birth = new Date(birthday_year, birthday_month - 1, birthday_day);
  personfortune = ibf.getConstellation(date_birth);
}else{
  personfortune = '01';
}

ibf.disp();

thatdayofsun = Array_fortune_name[0];
thatdayofmoon = Array_fortune_name[1];
thatdayofmoonangle = Array_fortune_anglenum[1];

var key_str = thatdayofsun+';'+thatdayofmoon+';'+thatdayofmoonangle+';'+personfortune;
var hash = CryptoJS.SHA256(key_str);

//占い結果表示
function fortune_dayluck(data){

	var ranking = data['ranking'];
	var personfortune = ibf.Sgnt[data['personfortune']];
	var allaround_evaluation = data['allaround_evaluation'];

	$('.ranking').append(ranking);
	$('.personfortune').append(personfortune);
	$('.allaround_evaluation').append(allaround_evaluation);

	//エラー非表示
	$('.fortune_top_v2').find('.err').css({'display': 'none'});

	//JS有効時のみ表示
	$('.fortune_top_v2').find('.one').css({'display': 'block'});

	//ランキング二桁版
	var rank_adj = ranking;

	//一桁なら二桁化
	if (String(rank_adj).length === 1){

		//十の位に0
		var rank_adj = '0' + rank_adj;

	}

	//ランキング用Class追加
	$('.rank_icon').addClass('rank_' + rank_adj);

	//リンク出力
	$('.constellation_day').html('<a href="/fortune/detail.html?key='+hash+'&n='+thatdayofsun+'">今日の運勢</a>');
	$('.constellation_time').html('<a href="/fortune/detail_time.html?key='+hash+'&n='+thatdayofsun+'">今の運勢</a>');

}

var loadurl = '/fortune/data/?key='+hash+'&n='+thatdayofsun+'&callback=fortune_dayluck';
document.write('<scr' + 'ipt type="text/javascript" src="' + loadurl + '"></scr' + 'ipt>');
</script>



