function infoseek_ui(genre_name){

	//変数宣言
	var now_number = 0;
	var html_photo = '';
	var html_new = '';
	var html_result = '';
	var len = 0;
	var genre_name_j = '';

	//引数が空の場合は「総合」扱い
	if (!genre_name){

		genre_name = 'all';

	}

	//第壱次元の要素数を取得
	/*
	for (var i in data[genre_name]) {

		len++;
	}
	*/

	//和名
	genre_name_j = infoseek_genre_trans(genre_name);

	//表示数指定
	len = 7;

	//ソース生成
	for ( i=0; i<len; i++ ){

		//データが完全な場合は値を取得
		if (data[genre_name][i] && data[genre_name][i]['image_flag'] && data[genre_name][i]['new_flag'] && data[genre_name][i]['title'] && data[genre_name][i]['url']){

			//取得済み記事数カウント
			now_number++;

			//写真フラグ判別
			if (data[genre_name][i]['image_flag'] == 0){

				html_photo = '';

			} else {

				html_photo = '<div class="photo"></div>';

			}

			//新着フラグ判別
			if (data[genre_name][i]['new_flag'] == 0){

				html_new = '';

			} else {

				html_new = '<div class="new"></div>';

			}

			//出力html連結
			html_result += '<li><dl class="cf"><dt>・<a href="/register/news/?genre=' + genre_name + '&url=' + data[genre_name][i]['url'] + '" target="_blank">' + data[genre_name][i]['title'] + '</a></dt><dd>' + html_photo + html_new + '</dd></dl></li>\n';

		}

	}

	//記事を1件も取得できなかった場合
	if (now_number == 0){

		html_result = '<li>現在、このカテゴリに該当する記事はありません。</li>';

	}

	//出力
	$('#infoseek_result').html('<ul>' + html_result + '</ul>');

	//選択解除
	$('#infoseek_menu li').removeClass('selected');

	//当該IDを選択
	$('#genre_' + genre_name).addClass('selected');

	//記事一覧
	$('#infoseek_list').html('<a href="http://www.fruitmail.net/newslist/?genre=' + genre_name + '">' + genre_name_j +'記事一覧</a>');

}



//--------------------------------------------------ジャンル和変換
function infoseek_genre_trans(genre_name){

	//和名に変換
	switch (genre_name){

		case 'all':		genre_name = 'ニュース'; break;
		case 'entertainment':	genre_name = '芸能'; break;
		case 'sports':		genre_name = 'スポーツ'; break;
		case 'busi-econ':	genre_name = '経済'; break;
		case 'world':		genre_name = '国際'; break;
		case 'life':		genre_name = 'ライフ'; break;
		case 'poli-soci':	genre_name = '社会'; break;
		case 'it':		genre_name = 'IT'; break;

	}

	//和名を返す
	return genre_name;

}



//--------------------------------------------------デフォルト実行
infoseek_ui();



