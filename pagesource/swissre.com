    var gatedData = function(){
    var dataString = $('#signupForm').serialize();
	//set the form data into cookie.
	if($('#remember').is(':checked')){
		dataString = 'emailAddress='+$('#signupForm #email').val()+'&firstName='+$('#signupForm #firstName').val()+'&lastName='+ $('#signupForm #lastname').val() +'&company='+ $('#signupForm #company').val();
