                 if(responseStr == "201" || responseStr == "500"){
                 console.log(responseStr);
                 console.log("response " + responseStr)
	         console.log($('#signupForm #fileURL').val());
                 window.open($('#signupForm #fileURL').val());
                 closeMe();
                }

           }

      });

}


