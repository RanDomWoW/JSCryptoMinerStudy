<script type="text/javascript">
	(function(jQ) {
		function applyFilters() {
			var searchterm = jQ("#searchterm").val();

			var searchByType =
				jQ(":checkbox[checked][data-categoryset='Publication Type']")
					.map(function(index, item) {
						return $(item).attr("id");
					})
					.toArray()
					.toString();

			var searchByCategory =
				jQ(":checkbox[checked][data-categoryset='Publication categories']")
					.map(function(index, item) {
						return $(item).attr("id");
					})
					.toArray()
					.toString();

			var searchBySection =
				jQ(":checkbox[checked][data-categoryset='Section']")
					.map(function(index, item) {
						return $(item).attr("id");
					})
					.toArray()
					.toString();

			var searchByLanguage = jQ("select[data-categoryset='Language']").val();

			var searchByDate = jQ("select[data-categoryset='Date']").val();

			var searchByYear = jQ("select[data-categoryset='Year']").val();

			var searchByStartDate = jQ("select[data-categoryset='Start Date']").val();
			var searchByEndDate = jQ("select[data-categoryset='End Date']").val();

			var activeTab = window.location.search.match(/activeTab=([^&]*)/);
			if(activeTab) {
				activeTab = activeTab[1];
			} else {
				activeTab = "";
			}

			var url = "?search=yes&searchterm=" + searchterm + (searchByType ? "&searchByType=" + searchByType : "")
	                + (searchByLanguage ? "&searchByLanguage=" + searchByLanguage : "") + (searchByDate ? "&searchByDate=" + searchByDate : "") + (searchByYear ? "&searchByYear=" + searchByYear : "")
	                + (searchByStartDate ? "&searchByStartDate=" + searchByStartDate : "") + (searchByEndDate ? "&searchByEndDate=" + searchByEndDate : "")
	                + (searchByCategory ? "&searchByCategory=" + searchByCategory : "") + (searchBySection ? "&searchBySection=" + searchBySection : "")
	                + (activeTab ? "&activeTab=" + activeTab : "");

	            return url;
		}

		jQ(function() {
			jQ("#search-filter-container")
				.on("change", "select[data-categoryset='Language'], select[data-categoryset='Year'], select[data-categoryset='Date'], select[data-categoryset='Start Date'], select[data-categoryset='End Date'], :checkbox:not(:not([data-categoryset='Publication Type'], [data-categoryset='Publication categories'], [data-categoryset='Section']))", {}, function(event, extraParameter) {
					event.stopPropagation();
					applyFilters();
				});


		        jQ("#btnFilter").on("click", function () {
		            window.location.href = applyFilters();
		        });

			jQ("#searchterm")
				.closest("form")
				.on("submit", function(event, extraParameter) {
					event.preventDefault();
					applyFilters();
				});

		});
	})(jQuery);
</script>

<script type="text/javascript">

if (typeof jQuery != 'undefined') {
	jQuery(document).ready(function($) {
		var filetypes = /\.(zip|exe|dmg|pdf|doc.*|xls.*|ppt.*|mp3|txt|rar|wma|mov|avi|wmv|flv|wav|png|jpg|jpeg|gif)(?:$|\?|&)/i;
		var doctypes = /\.(zip|exe|dmg|doc.*|xls.*|ppt.*|mp3|txt|rar|wma|mov|avi|wmv|flv|wav|)(?:$|\?|&)/i;
		var baseHref = '';
		if (jQuery('base').attr('href') != undefined) baseHref = jQuery('base').attr('href');
		jQuery('a').on('click', function(event) {
			var el = jQuery(this);
			var track = true;
			var href = (el.attr('data-url')) ? el.data('url') : (typeof(el.attr('href')) != 'undefined' ) ? el.attr('href') : "";
			var isThisDomain = href.match(document.domain.split('.').reverse()[1] + '.' + document.domain.split('.').reverse()[0]);
			if (!href.match(/^javascript:/i)) {
				var elEv = { "value" : 0, "non_i" : false },
						customCategory = el.attr("data-eventcategory") ? el.data("eventcategory") : null,
						customAction = el.attr("data-eventaction") ? el.data("eventaction") : null,
						customLabel = el.attr("data-eventlabel") ? el.data("eventlabel") : null;

				if (href.match(/^mailto\:/i)) {
					elEv.category = customCategory || "email";
					elEv.action = customAction || "click";
					elEv.label = customLabel || href.replace(/^mailto\:/i, '');
					elEv.loc = href;
				}
				else if (href.match(filetypes)) {
					var match = /(?:40=(?:[^&/]+\/{1,2})*([^&/.]+(?:\.([^&/]+))?))|(?:(?:^|\/)([^/.?]+(?:\.([^/]+))?)(?=$))/.exec(decodeURIComponent(href));
					var file = match[1] || match[3];
					var extension = match[2] || match[4];
					elEv.category = customCategory || "download";
					elEv.action = customAction || "click-" + (extension ? extension : "file");
					elEv.label = customLabel || file.replace(/ /g,"-");
					elEv.loc = baseHref + href;

				}
				else if (href.match(/^https?\:/i) && !isThisDomain) {
					elEv.category = customCategory || "external";
					elEv.action = customAction || "click";
					elEv.label = customLabel || href.replace(/^https?\:\/\//i, '');
					elEv.non_i = true;
					elEv.loc = href;
				}
				else if (href.match(/^tel\:/i)) {
					elEv.category = customCategory || "telephone";
					elEv.action = customAction || "click";
					elEv.label = customLabel || href.replace(/^tel\:/i, '');
					elEv.loc = href;
				}
				else track = false;

				console.log("GA: Url processing is done!");
				if (track) {
					console.log("GA: Data => Category: '" + elEv.category + "', Action: '" + elEv.action + "', Label: '" + elEv.label + "', Location: '" + elEv.loc + "', Value: '" + elEv.value + "', Non-Internal: '" + elEv.non_i + "'");
					console.log("GA: Send => " + ['_trackEvent', elEv.category, elEv.action, elEv.label, elEv.value, elEv.non_i]);
					_gaq.push(['_trackEvent', elEv.category, elEv.action, elEv.label, elEv.value, elEv.non_i]);
					if ((el.attr('target') == undefined || el.attr('target') != '_blank') && !(el.is('.modalbox') || el.is(".fancybox"))) {
						// hanling oping the file in new of the same page.
						if (href.match(doctypes)){
							setTimeout(function() { location.href = elEv.loc; }, 400);
						}else{
							setTimeout(function() { window.open(elEv.loc); }, 400);
						}

						return false;
					}
				} else {
					console.log("GA: '" + href + "' is not a supported url!");
				}
			}
		});
	});
}

</script>

