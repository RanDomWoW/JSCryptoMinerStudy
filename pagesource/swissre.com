function submitFormRedirect(dataString) {
	var fileURL = $('#signupForm #fileURL').val();

	var form = document.createElement('FORM');
	form.method = 'POST';
	form.action = '/templates/http-post-data-capture-caller';
	form.target = '_blank';

	var addField = function(name, value) {
		console.log(name + " = " + value);
		var input = document.createElement('INPUT');
		input.type = 'hidden';
		input.name = name;
		input.value = value;
		form.appendChild(input);
	};

	var params = dataString.split("&");
	for (var i = 0; i < params.length; i++) {
		var param = params[i].split("=");
		addField(param[0], param[1]);
	}
	addField("fileURL", fileURL);

	document.body.appendChild(form);
	form.submit();
	document.body.removeChild(form);
	closeMe();
}

function submitFormAjax(dataString){
