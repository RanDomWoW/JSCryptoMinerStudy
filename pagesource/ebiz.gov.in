<script  type="text/javascript">
$( document ).ready(function() {
	var feedURL = "http://www.thehindu.com/business/?service=rss";
    var nooffeed = 0, feedData="<ul>";
	$.ajax({
	  url      : document.location.protocol + '//query.yahooapis.com/v1/public/yql?q=select%20*%20from%20xml%20where%20url%20%3D%20%27' + encodeURIComponent(feedURL)+ '%27&format=json&env=store%3A%2F%2Fdatatables.org%2Falltableswithkeys',
	  dataType : 'json',
	  success  : function (data) {
		//alert(JSON.stringify(data.responseData.feed));
		if (data.query.results.rss.channel && data.query.results.rss.channel.item) {
		  $.each(data.query.results.rss.channel.item, function (i, e) {
			feedData=feedData + '<li><a href="' + e.link + '" target="_blank">' + e.title + '</a></li>';
			nooffeed=nooffeed+1;
			if (nooffeed>=5) {
				return false;
		  	}
		  });
		  document.all.divRssFeed.innerHTML = feedData + "</ul>";
		}
	  }
	});
});
</script>