            &lt;/a&gt;
            &lt;a href=&quot;https://twitter.com/share?url=/blog/7147/uncategorized/launch-close-surveys-automatically/&amp;amp;text=Launch &amp;amp; Close Surveys Automatically&quot; class=&quot;tweeter&quot; target=&quot;blank&quot; rel=&quot;nofollow&quot;&gt;tweet&lt;/a&gt;
            &lt;a href=&quot;https://www.facebook.com/sharer.php?u=/blog/7147/uncategorized/launch-close-surveys-automatically/&amp;amp;t=Launch &amp;amp; Close Surveys Automatically&quot; class=&quot;facebook&quot; target=&quot;blank&quot; rel=&quot;nofollow&quot;&gt;share&lt;/a&gt;
            &lt;a href=&quot;https://www.linkedin.com/shareArticle?mini=true&amp;amp;url=/blog/7147/uncategorized/launch-close-surveys-automatically/&amp;amp;title=Launch &amp;amp; Close Surveys Automatically&quot; class=&quot;linkedin&quot; target=&quot;blank&quot;rel=&quot;nofollow&quot; &gt;linkedin&lt;/a&gt;
            &lt;a href=&quot;https://digg.com/submit?phase=2&amp;amp;url=/blog/7147/uncategorized/launch-close-surveys-automatically/&amp;amp;title=Launch &amp;amp; Close Surveys Automatically&quot; class=&quot;digg&quot; target=&quot;blank&quot; rel=&quot;nofollow&quot;&gt;digg it&lt;/a&gt;
