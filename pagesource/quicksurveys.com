    &lt;/div&gt;
    &lt;div class=&quot;blog&quot;&gt;
        &lt;span class=&quot;name&quot;&gt;&lt;a href=&quot;/blog/7104/uncategorized/organizing-your-survey/&quot; target=&quot;_blank&quot;&gt;Organizing Your Survey&lt;/a&gt;&lt;/span&gt;
        &lt;span class=&quot;date&quot;&gt;2/22/2018&lt;/span&gt;
        &lt;span class=&quot;author&quot;&gt;&lt;/span&gt;
