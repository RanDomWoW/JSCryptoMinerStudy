    <script type="text/javascript" src="http://bongda24h.vn/websites/js/jquery-ui-1.11.0.min.js"></script>
    <script type="text/javascript" src="http://bongda24h.vn/websites/js/jquery.autofix_anything.js"></script>
    <script type="text/javascript" src="https://apis.google.com/js/platform.js" async defer></script>
    <script type="text/javascript">
        var cdialog;
        var delay = 200, setTimeoutConst, delay2 = 200, delay3 = 200, setTimeoutConst2;
        var headcurentid;
        var menucurentid;
        var menucurentid2;
        $(document).ready(function () {
            $("#nav").show();
            $(".tab-content").hide();
            $("ul#main-nav li:first").addClass("active").show();
            $(".tab-content:first").show();

            $("ul#main-nav li").click(function () {
                $("ul#main-nav li").removeClass("active");
                $(this).addClass("active");
                $(".tab-content").hide();
                var activeTab = $(this).find("a").attr("data-href");
                $(activeTab).fadeIn();
                return false;
            });

            $("#nav2").show();
            $(".tab-content2").hide();
            $("ul#main-nav2 li:first").addClass("active").show();
            $(".tab-content2:first").show();

            $('li[id*="nav2head"]').mouseover(function () {
                headcurentid = $(this).attr("id");
                setTimeoutConst = setTimeout(function () {
                    var isHover = $("#" + headcurentid + ":hover").length;
                    var activeTab = $("#" + headcurentid).find("a").attr("data-href");
                    if (isHover > 0 && $(activeTab).css('display') == 'none') {
                        $("ul#main-nav2 li").removeClass("active");
                        $("#" + headcurentid).addClass("active");
                        $(".tab-content2").hide();
                        $(activeTab).show();
                        return false;
                    }
                }, delay);
            });
            //menu top

            $('li[id*="fulldropmenu"]').mouseover(function () {
                clearTimeout(setTimeoutConst2);
                menucurentid = $(this).attr("id");
                setTimeoutConst = setTimeout(function () {
                    var isHover = $("#" + menucurentid + ":hover").length;
                    if (isHover > 0 && $("#" + menucurentid).children(".submenu").css("display") == "none") {
                        $("#" + menucurentid).children(".submenu").show();
                        $('div.submenu').not($("#" + menucurentid).children()).hide();
                        return false;
                    }
                }, delay2);
            });
            $('li[id*="fulldropmenu"]').mouseout(function () {
                clearTimeout(setTimeoutConst);
                menucurentid2 = $(this).attr("id");
                setTimeoutConst2 = setTimeout(function () {
                    var isHover = $("#" + menucurentid2 + ":hover").length;
                    if (isHover == 0) {
                        $("#" + menucurentid2).children(".submenu").hide();
                    }
                    if ($("#" + menucurentid + ":hover").length == 0)
                        $('div.submenu').hide();
                }, delay3);
                return false;
            });
            $(".menutop").autofix_anything({ customOffset: 120 });
            //search
            $('#txtSearchkey').keypress(function (event) {
                if (event.which == 13) {
                    $('#btnSearch').click();
                    return false;
                }
            });
            showads();
            $('a.popup').on('click', function (e) {
                var page = $(this).attr("href")
                cdialog = $('<div id="VoteBox"></div>')
                .html('<iframe id="ifEdit" style="border: 0px; " src="' + page + '" width="98%" height="98%"></iframe>')
                .dialog({
                    autoOpen: false,
                    modal: true,
                    height: 420,
                    width: 580,
                    title: $(this).attr("title"),
                    close: function (event, ui) {
                        $(this).remove();
                    }
                });
                cdialog.dialog('open');
                e.preventDefault();
            });
            // facebook
            (function (d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/vi_VN/all.js#xfbml=1";
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
            // twitter
            !function (d, s, id) { var js, fjs = d.getElementsByTagName(s)[0], p = /^http:/.test(d.location) ? 'http' : 'https'; if (!d.getElementById(id)) { js = d.createElement(s); js.id = id; js.src = 'http://platform.twitter.com/widgets.js'; fjs.parentNode.insertBefore(js, fjs); } }(document, 'script', 'twitter-wjs');

        });
        $(window).resize(function () {
            showads();
            $(".menutop").autofix_anything({ customOffset: 120 });
        });
        function showads() {
            var windowWidth = $(window).width() - 240;
            var adPadding = ($(window).width() - 1000) / 2;
            adPadding = adPadding - 165;
            if (windowWidth <= 1000) {
                $('#divAdLeft').hide();
                $('#divAdRight').hide();
            }
            else {
                $('#divAdLeft').show();
                $('#divAdRight').show();
                $('#divAdLeft').css('left', adPadding);
                $('#divAdRight').css('right', adPadding);
            }
        }
        function doSearch() {
            var SearchUrl = $('#txtSearchkey').attr("data-href");
            var Keyword = $('#txtSearchkey').val();
            SearchUrl = SearchUrl + "?Keyword=" + encodeURIComponent(Keyword);
            window.location = SearchUrl;
        }
        function vote(url) {
            var VoteSelect = $('input[name=radiopolls]:checked');
            var VoteResult = '';
            VoteSelect.each(function () {
                VoteResult = this.value;
            });
            if (VoteResult == '') {
                alert('Bạn cần chọn ít nhất một phương án');
                return;
            }
            url = url + "&paid=" + VoteResult;
            cdialog = $('<div id="VoteBox"></div>')
                .html('<iframe id="ifVote" style="border: 0px; " src="' + url + '" width="98%" height="98%"></iframe>')
                .dialog({
                    autoOpen: false,
                    modal: true,
                    height: 420,
                    width: 580,
                    title: 'Kết quả bình chọn',
                    close: function (event, ui) {
                        $(this).remove();
                    }
                });
            cdialog.dialog('open');
        }
        function SwitchToWeb() {
            var d = new Date();
            var n = d.getTime();
            var logpannel = $('<div id="logpanel" style="display:none"></div>');
            var url = "/changeapptype.aspx?t=" + n;
            logpannel.load(url, function (response, status, xhr) {
                if (status == "error") {
                    alert("Rất tiếc! Không thể chuyển đổi phiên bản.");
                }
                document.location.reload(true);
            });

        }
        //test
        $(function () {
            var host = location.host;
            if (host == 'bongda24h.icsoft.vn') {
                $('a').each(function () {
                    var seft = $(this);
                    var href = seft.attr('href');
                    var newhref;
                    if (typeof href !== 'undefined') {
                        newhref = href.replace(/bongda24h.vn/i, 'bongda24h.icsoft.vn');
                        //newhref = newhref.replace(/tdp.bongda24h.icsoft.vn/i, 'bongda24h.icsoft.vn');
                        //newhref = newhref.replace(/tdp.bongda24h.vn/i, 'bongda24h.icsoft.vn');
                        seft.attr('href', newhref);
                    }
                });
            }
        });
    </script>

    
