			function CheckLength(str) {
				var len;
				var i = 0;
				var l = 0;
				var temp, lastl;

				while(i < str.length) {
					temp = str.charAt(i);

					if (escape(temp).length > 4) {
						l += 2;
					} else if (temp != '\r') {
						l++;
					}

					lastl = l;
					i++;
				}

				return lastl;
			}

