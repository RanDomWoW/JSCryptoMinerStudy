		var componentsFactory = function(args) {
			this.param = args;
			this.new_back_button = null;
			this.left_source_panel = null;
			this.about_panel = null;
			this.right_panel = null;
			this.button_panel = null;
			this.control_buttons = null;
		}
		
		componentsFactory.prototype.getNewBackButton = function() {
			if (this.new_back_button == null) {
				var curr_page = this.param['curr_page'];
				var domain = this.param['domain'];
				var subdomain = this.param['subdomain'];
				var cvalue = this.param['cvalue'];
				var page = this.param['page'];
				this.new_back_button = new newBackButton(
					curr_page, domain, subdomain, cvalue, page);
			}
			return this.new_back_button;
		}
		
		componentsFactory.prototype.getLeftSourcePanel = function() {
			if (this.left_source_panel == null) {
				this.left_source_panel = new leftPanel($("#topSourcePanel"));
			}
			return this.left_source_panel;
		}
		
		///////new version of vertical animation for left panel////////////
		componentsFactory.prototype.getVerticalAboutPanel = function() {
			if (this.vert_about_panel == null) {
				this.vert_about_panel = new aboutPanel($("#aboutSourcePanel"));
			}
			return this.vert_about_panel;
		}
		
		///////new version of vertical animation for left panel////////////
		
		componentsFactory.prototype.rightPanel = function() {
			if (this.right_panel == null) {
				this.right_panel = new rightPanel($("#sources_panel"));
			}
			return this.right_panel;
		}
		
		componentsFactory.prototype.buttonPanel = function() {
			if (this.button_panel == null) {
				this.button_panel = new buttonPanel();
			}
			return this.button_panel;
		}

		
		componentsFactory.prototype.controlButtons = function() {
			if (this.control_buttons == null) {
				this.control_buttons = new controlButtons();
			}
			return this.control_buttons;
		}

	
		componentsFactory.prototype.LeftPanelCloseButton = function() {
