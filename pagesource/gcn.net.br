      $("#your-name").keyup(function(){
          $("#msgYourname").empty();
      });
      $("#sender-email").keyup(function(){

          $("#msgSendemail").empty();
      });
      $("#to-name").keyup(function(){
          $("#msgToname").empty();
      });
      $("#recipient").keyup(function(){
          $("#msgRecipient").empty();
      });

      var captchaContainer = null;
      var dados = null;
      var captchaContainerMail = null;
      var dadosMail = null;
      var widgetId1;
      var widgetId2;
    
        // Renders the HTML element with id 'example1' as a reCAPTCHA widget.
        // The id of the reCAPTCHA widget is assigned to 'widgetId1'.
        $(document).ready(function(){
            var serverName = 'gcn.net.br';
            var endereco  = '/';
            $('#myModal11').on('shown.bs.modal', function (e) {
                widgetId2 = grecaptcha.render(document.getElementById('captcha_mail'), {
                    'sitekey' : '6LfHJP0SAAAAADR-iuNjqHexfNJ8wlaXxEefWdOe',
                    'callback' : function(response) {
                        dadosMail = response;
                    }
                });
            });
        });
        
  function shareEmail(noticia_id, tipo){
      
      $("#myModal11").modal('show');
      if(noticia_id == 'pagina'){
          var serverName = 'gcn.net.br';
          $("#link-news").val("http://"+serverName);
      }
      else{
          $.ajax({
              url:"/sys-php/getNoticia.php",
              type:"post",
              dataType:"json",
              data:{noticia_id:noticia_id, tipo:tipo},
              success:function(data){
                  $("#title-news").html(data[0].titulo);
                  $("#resume-news").val(data[0].resumo);
                  $("#image-news").val(data[0].link_image);
                  $("#link-news").val(data[0].link_noticia);
                  $("#input-title-news").val(data[0].titulo);
              }
          });
      }
  }
    
  function sendEmail(){
      
      var image = $("#image-news").val();
      var title = $("#input-title-news").val();
      var resume = $("#resume-news").val();
      var yourname = $("#your-name").val();
      var senderemail = $("#sender-email").val();
      var linknews = $("#link-news").val();
      var toname = $("#to-name").val();
      var recipient = $("#recipient").val();
      var message = $("#message").val();
      
      if(yourname == ""){ 
          $("#msgYourname").html('<p class="text-danger">*Preencha este campo!</p>');
      }
      else if(senderemail == ""){
          $("#msgSendemail").html('<p class="text-danger">*Preencha este campo!</p>');
      }
      else if(toname == ""){
          $("#msgToname").html('<p class="text-danger">*Preencha este campo!</p>');
      }
      else if(recipient == ""){
          $("#msgRecipient").html('<p class="text-danger">*Preencha este campo!</p>');
      }
      else{
        $.ajax({
            url:"/head/novoCaptcha.php",
            type:"post",
            data:"g-recaptcha-response="+dadosMail,
            dataType:"html",
            success:function(response){
                if(response == 'successful'){    
                  $.ajax({
                      url:"/sys-php/sendEmail.php",
                      type:"post",
                      dataType:"json",
                      data:{image:image,
                            title:title,
                            resume:resume,
                            yourname:yourname,
                            senderemail:senderemail,
                            linknews:linknews,
                            toname:toname,
                            recipient:recipient,
                            message:message},
                      success:function(data){
                          if(data == 'enviou'){
                              alert("Email enviado com sucesso!");
                              grecaptcha.reset();
                              $("#myModal11").modal("hide");
                          }
                          else{
                              alert("Falha ao enviar email! Verifique o endereço de envio!");
                              grecaptcha.reset();
                          }
                      }
                  });
              }
              else{
                  alert('Preencha a validação corretamente!');
                  grecaptcha.reset();
              }
            }    
        });
      }
