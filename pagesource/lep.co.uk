<script>
    //metering

    EV.Em.init({
        url: "https://jps.evolok.net/acd/api/3.0",
        sidCookieDomain: 'lep.co.uk'
    });

    function getUrlParameters() {
        var urlParameters = [], hash;
        var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
        for(var i = 0; i < hashes.length; i++)
        {
            hash = hashes[i].split('=');
            urlParameters.push(hash[0]);
            urlParameters[hash[0]] = hash[1];
        }
        return urlParameters;
    }

    function hasWidgetParameter(urlParameters) {
        if(urlParameters != null) {
            return urlParameters['registration']
            || urlParameters['loginform'] || urlParameters['forgotpassword'];
        } else {
            return false;
        }
    }

    function getAuthorization() {
        var params = {"section":"Section","referrer":"https://www.lep.co.uk","site":"pres"};
        if (document.referrer.indexOf('facebook.com') >= 0) {
            params["referrer"] = "facebook.com";
        }
        params["adblock"] = EV.Dab.isAdblockDetected();

        var urlParameters = getUrlParameters();
        if(EV.util.getEVSession() && hasWidgetParameter(urlParameters)) {
            window.location.href = "https://www.lep.co.uk" + "/profile";
        } else {
            if(urlParameters['registration']) {
                EV.Event.publish("ev.open.modal.registration-widget");
                params["adblock"] = false;
            } else if(urlParameters['loginform']) {
                EV.Event.publish("ev.open.modal.login-widget");
                params["adblock"] = false;
            } else if(urlParameters['forgotpassword']) {
                EV.Event.publish("ev.open.modal.forgot-password-widget");
                params["adblock"] = false;
            }
        }

        params = JSON.stringify(params);

        EV.Em.authorize(params, function (res) {
            console.log("Metering response:", res)
            if (res.result !== "ALLOW_ACCESS") {
                //reload page on login to get the new state
                EV.Event.on(EV.Event.SOCIAL_LOGIN_SUCCESS, function () { window.location.reload() })
                EV.Event.on(EV.Event.SOCIAL_REGISTER_SUCCESS, function () { window.location.reload() })
                EV.Event.on(EV.Event.REGISTRATION_SUCCESS, function () { window.location.reload() })
                EV.Event.on(EV.Event.LOGIN_SUCCESS, function () { window.location.reload() })

                //hide article content
                $(".article-content").hide();
                $(".comments-container").hide();
            }
        }, function (error) {

        })
    }

    $( document ).ready(function() {
        getAuthorization()
    });
</script>

<script>
    // social register
    EV.Event.on('social-register.success', function (response) {console.log("social-register.success = ", response);
        EV.Event.publish("ev.close.modal.socialRegistration");
    });

    EV.Event.on('social-register.failed', function (error) {
        console.log("social-register.failed = ", error);
        if (error.error) {
            if (error.error.code === 'ATTRIBUTE_REQUIRED' ||
                error.error.code === 'REQUIRED_ATTRIBUTE_MISSING' ||
                error.error.code === 'USER_NOT_REGISTERED' ||
                error.error.code === 'USER_PROFILE_NOT_FOUND') {

                EV.Event.publish("ev.close.modal.registration-widget");
                EV.Event.publish("ev.open.modal.socialRegistration", error.error);
            }
        }
    });

    EV.Event.on("social-register.cancelled", function (error) {
        console.log("social-register.cancelled");
        EV.Event.publish("ev.close.modal.socialRegistration");
        EV.Event.publish("ev.open.modal.login-widget");
    });

    EV.Event.on('social-login.success', function (response) {
        console.log("social-login.success = ", response);
        EV.Event.publish("ev.close.modal.socialRegistration");
    });

    EV.Event.on('social-login.failed', function (e) {
        console.log("social-login.failed = ", e);
        if (e.error) {
            if (e.error.code === 'ATTRIBUTE_REQUIRED' ||
                e.error.code === 'REQUIRED_ATTRIBUTE_MISSING' ||
                e.error.code === 'USER_NOT_REGISTERED' ||
                e.error.code === 'USER_PROFILE_NOT_FOUND') {

                EV.Event.publish("ev.close.modal.login-widget");
                EV.Event.publish("ev.open.modal.socialRegistration", e.error);
            }
        }
    });

    EV.Event.on('forgot-password.cancelled', function () {
        EV.Event.publish("ev.close.modal.forgot-password-widget")
    })

    EV.Event.on('sent.email.forgot.password', function () {
        EV.Event.publish("ev.close.modal.login-widget");
        EV.Event.publish("ev.close.modal.forgot-password-widget");
    });

    EV.Event.on('ev.paywall.adblock.loaded', function () {
        $("html").css("overflow-y","hidden");
    })

    // inject custom text into the registration widget
    EV.Event.on('ev.open.modal.registration-widget', function () {
        initRegistrationText("registration-widget");
        $("html").css("overflow-y","hidden");

        if ($('.ev-open-modal-paywall-login').length <= 0) {
            EV.Event.on('ev.modal.close', function () {
                $("html").css("overflow-y","scroll");
            })
        }
    })

    // inject custom text into the login widget
    EV.Event.on('ev.open.modal.login-widget', function () {
        initLoginText("login-widget");
    })

    // inject custom text into the my account widget
    EV.Event.on('ev.open.modal.edit-profile-widget', function () {
        initEditProfileText("edit-profile-widget");
    })

    // inject custom text into the create account widget
    EV.Event.on('ev.open.modal.socialRegistration', function () {
        initCreateAccountText("socialRegistration");
    })

    function initRegistrationText(widgetId) {
        var isRegistrationReadyInterval = setInterval(function () {
            if (isTemplateReady(widgetId)) {
                addCustomTextToRegistrationTemplate(widgetId);
                clearInterval(isRegistrationReadyInterval)
            }
        }, 300);
    }

    function initLoginText(widgetId) {
        var isLoginReadyInterval = setInterval(function () {
            if (isLoginTemplateReady(widgetId)) {
                addCustomTextToLoginTemplate(widgetId);
                clearInterval(isLoginReadyInterval)
            }
        }, 300);
    }

    function initEditProfileText(widgetId) {
        var isEditProfileReadyInterval = setInterval(function () {
            if (isEditProfileTemplateReady(widgetId)) {
                addCustomTextToEditProfileTemplate(widgetId);
                clearInterval(isEditProfileReadyInterval)
            }
        }, 300);
    }

    function initCreateAccountText(widgetId) {
        var isCreateAccountReadyInterval = setInterval(function () {
            if (isCreateAccountTemplateReady(widgetId)) {
                addCustomTextToCreateAccountTemplate(widgetId);
                clearInterval(isCreateAccountReadyInterval)
            }
        }, 300);
    }

    function addCustomTextToRegistrationTemplate(widgetId) {
        addTextAfterNonAuthAttribute(widgetId, 2, "<p>Would you like to receive the latest news, news features and service updates from Lancashire Evening Post via email?</p>")
        addTextAfterNonAuthAttribute(widgetId, 2, "<h4 class='ev-newsletter-title'>Newsletters</h4>")
        addTextAfterNonAuthAttribute(widgetId, 3, "<p>Would you like to receive more information from Johnston Press, via occasional emails, to keep you informed about new features, special offers, promotions, events and services that might be of interest to you?</p>")
        addFooterText(widgetId, '<p>By clicking on register, you will be creating an account with Lancashire Evening Post and agreeing to Johnston Press&#39; Standard <a class="ext-link" href="http://www.johnstonpress.co.uk/terms-conditions" target="_blank" title="Terms and Conditions">Terms and Conditions</a>.</p>')
        addLinkToAnotherScreen(widgetId, '<p class="text-center">Already registered? <a onclick="EV.Event.publish(\'ev.close.modal.registration-widget\'); EV.Event.publish(\'ev.open.modal.login-widget\')">Login here.</a></p>')
    }

    function addCustomTextToLoginTemplate(widgetId) {
        addLinkToAnotherScreen(widgetId, '<p class="text-center">Don&#39;t have an account with us? <a onclick="EV.Event.publish(\'ev.close.modal.login-widget\'); EV.Event.publish(\'ev.open.modal.registration-widget\')">Create one today.</a></p>')
    }

    function addCustomTextToEditProfileTemplate(widgetId) {
        addTextAfterNonAuthAttribute(widgetId, 8, "<h4 class='ev-newsletter-title'>Newsletters</h4><br><p>Would you like to receive the latest news, news features and service updates from Lancashire Evening Post via email?</p>")
        addTextAfterNonAuthAttribute(widgetId, 9, "<p>Would you like to receive more information from Johnston Press, via occasional emails, to keep you informed about new features, special offers, promotions, events and services that might be of interest to you?</p>")
    }

    function addCustomTextToCreateAccountTemplate(widgetId) {
        addTextAfterNonAuthAttribute(widgetId, 3, "<h4 class='ev-newsletter-title'>Newsletters</h4><br><p>Would you like to receive the latest news, news features and service updates from Lancashire Evening Post via email?</p>")
        addTextAfterNonAuthAttribute(widgetId, 4, "<p>Would you like to receive more information from Johnston Press, via occasional emails, to keep you informed about new features, special offers, promotions, events and services that might be of interest to you?</p>")
    }

    function isTemplateReady(widgetId) {
        var expectedDivs = 6;
        var currentDivs = $("#" + widgetId + " #serviceAttributes > div").length;
        return expectedDivs == currentDivs;
    }

    function isLoginTemplateReady(widgetId) {
        var rememberMe = $("#" + widgetId + " .remember-me").length;
        var isLoaded = false;
        if(rememberMe > 0 ) {
            isLoaded = true;
        }
        return isLoaded;
    }

    function isEditProfileTemplateReady(widgetId) {
        var expectedDivs = 11;
        var currentDivs = $("#" + widgetId + " #serviceAttributes > div").length;
        return expectedDivs == currentDivs;
    }

    function isCreateAccountTemplateReady(widgetId) {
        var expectedDivs = 7;
        var currentDivs = $("#" + widgetId + " #serviceAttributes > div").length;
        return expectedDivs == currentDivs;
    }

    function addTextAfterNonAuthAttribute(widgetId, index, text) {
        $("#" + widgetId + " #serviceAttributes > div.ng-scope:eq(" + index + ")").after('<div class="ev form-group col-sm-12">' + text + '</div>');
    }

    function addHeaderText(widgetId, text) {
        $("#" + widgetId + " #widget-body").prepend('<div class="ev form-group col-sm-12">' + text + '</div>');
    }

    function addFooterText(widgetId, text) {
        $("#" + widgetId + " #buttonPanel").before('<div class="ev form-group col-sm-12">' + text + '</div>');
    }

    function addLinkToAnotherScreen(widgetId, text) {
        $("#" + widgetId + " #buttonPanel").after('<div>' + text + '</div>');
    }
</script>

