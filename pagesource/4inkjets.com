<script type="text/javascript">
    var clicks = 0;
    function ttOn (e) {
        clicks += 1;
        if(!jQuery(e.target).hasClass('learn-more') || !jQuery(e.target).hasClass('learn-link')) {
            e.preventDefault();
            if (!jQuery('.promo-tooltip').hasClass('over')) {
                jQuery('.promo-tooltip').addClass('over');
            } else {
                ttOff();
            }
            
        }
    }
    function ttOff (e) {
        if(!jQuery(e.target).hasClass('learn-more') || !jQuery(e.target).hasClass('learn-link')) {
            e.preventDefault();
            jQuery('.promo-tooltip').removeClass('over');
            clicks = 0;
        }
    }
    function tooltip(e) {
        jQuery(".tool-tip-container").on('click', 'a.learn-link', ttOn);
        jQuery(".promo-tooltip").on('click', '.close-tt-bot', ttOff);
        jQuery('html').on('click', 'body', function(e) {
            if(clicks != 1) {
                if(jQuery('.promo-tooltip').hasClass('over') ) {
                    ttOff(e);
                }               
            }
            if (clicks == 1) {
                clicks++;
            }
        });
    }
    function tooltipOff() {
        jQuery(".tool-tip-container").off('click', 'a.learn-link', ttOn);
        jQuery(".promo-tooltip").off('click', '.close-tt, .close-tt-bot', ttOff);
    }

    Respond.to({
        'media' : '(max-width: 768px)',
        'fallback' : 'else',     // ie8 fallback callback (optional - defaults to 'if' callback)
        'if' : function() { // Mobile
            tooltipOff();
        },
        'else' : function() { // Desktop
            tooltip();
        }
    });
