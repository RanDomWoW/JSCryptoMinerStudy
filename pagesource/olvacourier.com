/* Form Module */

.form-module {
  position: relative;
  background: #ffffff;
  max-width: 540px;
  width: 100%;
  border-top: 5px solid #777777;
  box-shadow: 0 0 3px rgba(0, 0, 0, 0.1);
  margin: 0 auto;
}

.form-module .form {
  display: none;
  padding: 23px 40px 121px;
}

.form-module .form:nth-child(2) {
  display: block;
}

.form-module h2 {
  margin: 0 0 0;
  font-size: 25px;
  font-weight: 600;
  line-height: 1;
}

.form-module input {
  outline: none;
  display: block;
  width: 100%;
  border: 1px solid #d9d9d9;
  padding: 10px 15px;
  box-sizing: border-box;
  font-wieght: 400;
  -webkit-transition: 0.3s ease;
  transition: 0.3s ease;
}

.form-module input:focus {
  border: 1px solid #33b5e5;
  color: #333333;
}

.form-module select {
  outline: none;
  display: block;
  width: 100%;
  border: 1px solid #d9d9d9;
  padding: 9px 0;
  box-sizing: border-box;
  font-wieght: 400;
  -webkit-transition: 0.3s ease;
  transition: 0.3s ease;
}

.form-module select:focus {
  border: 1px solid #33b5e5;
  color: #333333;
}

.form-module select:invalid { 
  color: grey; 
}

.form-module button {
  cursor: pointer;
  background: #ffc107;
  width: 35%;
  margin-bottom: 20px;
  float: right;
  margin-top: 20px;
  border: 0;
  padding: 10px 15px;
  color: #000000;
  font-weight: 600;
  -webkit-transition: 0.3s ease;
  transition: 0.3s ease;
}

.form-module button:hover {
  background: #8e8e8e;
}

.form-module .cta {
  background: #f2f2f2;
  width: 100%;
  padding: 15px 40px;
  box-sizing: border-box;
  color: #666666;
  font-size: 12px;
  text-align: center;
}

.form-module .cta a {
  color: #333333;
  text-decoration: none;
}


