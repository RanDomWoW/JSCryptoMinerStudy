<script type="text/javascript">
	$(document).ready(function(){
		NavigationBar.init();
		NavigationBar.subNavClosed();				if($('.essential-guides').length == 0 && $('.page.enroll.courses').length == 0 ) {
			$('#nav-sub').scrollToFixed({ 
	           marginTop: 0, 
	           preFixed: function() {
	                $("#nav-sub").css("opacity",0.9);
	           },
	           postFixed: function() { 
	           	    $("#nav-sub").css("opacity",1); 
	           },
	           limit: function() {
	            	if($('.course_filters').length > 0) {
	            		var limit = $('.course_filters').offset().top - 60;
	            		return limit;
	            	}
	            	if($('#school-nav').length > 0) {
	            		var limit = $('#school-nav').offset().top - 60;
	            		return limit;
	            	}
	            	
	           }
	       });
		}
		else if ($('.page.enroll.courses').length > 0) {
			$("#nav-sub li[class*='orange']").hide();
		}
		/** couldn't use previous condition because we do need fixed subnav in this section **/
		switch (true) {
	        case ($("#wp-gmat-gmat-course-options").length > 0):
	            $("#nav-sub li[class*='orange']").hide();
	            $("#nav-bar .enroll-gmat").remove();
	            break;
	        case ($("#wp-act-sat-prep-enroll").length > 0):
	            $("#nav-sub li[class*='orange']").hide();
	            $("#nav-bar .enroll-sat").remove();
	            break;
	        case ($("#wp-gre-enroll").length > 0):
	            $("#nav-sub li[class*='orange']").hide();
	            $("#nav-bar .enroll-gre").remove();
	            break;
	        default:
	            break;
	    }
	})
</script>

