        <script>
window.fbAsyncInit = function() {
    FB.init({
        appId: '177229399075926',
        status: true,
        cookie: true,
        xfbml: true,
        oauth: true
    });

    FB.getLoginStatus(function(response) {
        if ( response.status === 'connected' ) {
            $(".fb-login-button-o").hide();
            var token = response.authResponse.accessToken;
            
                $("#consumer_fb_access_token").val(token);

                FB.api('/me/picture?height=100&redirect=false', function(me) {
                    console.log(me.data.url);
                    $("#consumer_fb_profile_picture").val(me.data.url);
                    console.log($("#consumer_fb_profile_picture").val());
                });

                FB.api('/me', function(me){
                    console.log(me);
                    if( me.name ) {
                        console.log(me.name);
                        $("#consumer_fb_name").val(me.name);
                        $("#consumer_fb_first_name").val(me.first_name);
                        $("#consumer_fb_id").val(me.id);
                        $("#consumer_fb_gender").val(me.gender);
                        $("#consumer_fb_email").val(me.email);
                        $(".fb-login-button").hide();
                        $(".fb-login-button-o").hide();
                        $(".fb_button").show();
                    }
                });
            
        } else {
            
            $(".fb_button").hide();

            FB.Event.subscribe('auth.statusChange', function(response) {
                if(response.authResponse){
                    var token = response.authResponse.accessToken;
                    $("#consumer_fb_access_token").val(token);

                    FB.api('/me/picture?height=100&redirect=false', function(me) {
                        console.log(me.data.url);
                        $("#consumer_fb_profile_picture").val(me.data.url);
                        console.log($("#consumer_fb_profile_picture").val());
                    });

                    FB.api('/me',function(me) {
                        console.log(me);
                        if( me.name ) {
                            console.log(me.name);
                            $("#consumer_fb_name").val(me.name);
                            $("#consumer_fb_first_name").val(me.first_name);
                            $("#consumer_fb_gender").val(me.gender);
                            $("#consumer_fb_email").val(me.email);
                            $("#consumer_fb_id").val(me.id);
                            $(".fb-login-button").hide();
                            $(".fb-login-button-o").hide();
                            $(".fb_button").show();
                            setTimeout(function(){
                                submit_fb_callback();
                            }, 1000);
                        }
                    });
                }
            });
            
        }
    });
};
    
(function(d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/en_US/all.js";
    fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));


function submit_fb_callback() {
    if ( $("#consumer_fb_id").val().length> 5 ) {
        $("#form-fb").submit();
    } else {
        setTimeout(function() {
            submit_fb_callback();
        }, 1000);
    }
}
