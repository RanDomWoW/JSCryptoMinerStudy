<script type="text/template" id="tableBiathlonTpl">
    <div class="box clr clearfix margin-bottom-40">
      <div class="all-news">
        <% if(title && title !== undefined && title !== '') { %>
            <div class="block-title"><%=title%></div>
        <% } %>
        <div class="all-stages-tables" data-jcarousel="true" style="position: relative; overflow: hidden;width: 100%;">
          <div id="stagesSlider" style="width: 20000em;position: relative;list-style: none; margin: 0;padding: 0;">
            <% allstages.forEach(function(one, index){ %>
              <div class="one-stage" style="float: left; overflow: hidden;">
                <div class="choice-news">
                  <div class="arrow-box nosel biathlon-nav">
                    <span class="block-change-news-prev nav nosel" data-jcarouselcontrol="true">prew</span>
                    <div class="biathlon-title"><%=one.name%></div>
                    <span class="block-change-news-next nav nosel" data-jcarouselcontrol="true">next</span>
                  </div>
                </div>
                <table class="table-list-box margin-bottom-10 all-news" >
                    <tbody>
                        <% one.stages.forEach(function(item, index){ %>
                          <tr>
                              <td class="col_team_player">
                                  <div class="date"><%=moment(item.start_date).format('D MMMM LT')%></div>
                                  <div class="stage-name"><%=item.name%></div>
                                <% if(item.winners) { %>
                              </td>
                                  <td style = "max-width:30%">
                                    <div class="stage-winners">
                                      <% item.winners.forEach(function(winner, index){ %>
                                        <%=winner.position%>. <%=winner.name%></br>
                                      <% }); %>
                                    </div>
                                <% } %>
                              </td>
                          <tr/>
                          <% }); %>
                    </tbody>
                </table>
              </div>
            <% }); %>
          </div>
      </div>
        <% if(linkVal && linkVal != '' && linkTitle && linkTitle != '') {  %>
            <div class="more-news margin-bottom-20">
                <a href="<%=linkVal%>"><%=linkTitle%></a>
            </div>
        <% } %>
      </div>
    </div>
</script>
