		<script type="text/template" id="livescoreMiniTpl">
    <div id="result" class="jcarousel-wrapper" data-time="<%=moment(date).format('dddd, DD MMMM, HH:mm:ss') %>">
        <div class="select-box">
            <div id="select">
                <select name="sport-type" style="width: 200px;" class="select2"></select>
                <a href="/scoreboard" class="select other-box-container"><%=locale[langCount].all_mathes%></a>
            </div>
        </div>
        <div class="match-box ">
            <div class="jcarousel">
                <ul id="livescoreMiniSlider"></ul>
            </div>
        </div>
        <div class="other-box nosel">
            <span class="jcarousel-control-prev nav nosel" id="prew">prew</span>
            <span class="jcarousel-control-next nav nosel" id="next">next</span>
        </div>
    </div>
</script>

<script type="text/template" id="livescoreMiniSliderTpl">
    <% for (var i = 0; i < tournaments.length; i++) { %>
        <% if(tournaments[i].tournament_id == selected) { %>
            <% tournaments[i].matches.forEach(function(match){ %>
                <li class="match_block sport-<%=match.sport_id %>" data-match_id="<%=match.match_id%>" data-betradar_match_id="<%=match.betradar_match_id%>">
                    <span class="hide tournament_id"><%=tournaments[i].tournament_id%></span>
                    <span class="hide tournament_name"><%=tournaments[i].tournament_name%></span>

                    <% if(match.match_id != null) { %>
                        <% if(( +match.sport_id === 1 || +match.sport_id === 2 || +match.sport_id === 4 || +match.sport_id === 5 || +match.sport_id === 29 || +match.sport_id === 16) && (window.location.hostname === "sportarena.com" || window.location.hostname === "sportarena.hr" || window.location.hostname === "localhost" || window.location.hostname === "new.sportarena.com" || window.location.hostname === "dev.sportarena.hr")) {%>
                            <a href="/<%=match.sport_slug%>/game/<%=match.match_id%>/" class="more-link">
                        <% } else { %>
                            <div class="more-link">
                        <% } %>

                    <% } else { %>
                        <div class="more-link">
                    <% } %>
                        <header>
                            <div class="team-left">
                                <% if(match.sport_id == 5) { %>
                                     <span class="box-img">
                                        <img src="<%=match.team_home.logo_medium%>" alt="<%=match.team_home.name%>">
                                    </span>
                                <% } else { %>
                                    <img src="<%=match.team_home.logo_small%>" alt="<%=match.team_home.name%>">
                                <% } %>
                            </div>
                            <div class="score">
                                <% if (match.scores == null) { %>
                                    <span class="time"><%=moment(match.date_of_match).format('LT') %></span>
                                <% } else { %>
                                    <% if (match.scores.overtime && match.scores.overtime !== null) { %>
                                        <%=match.scores.overtime%>
                                    <% } else if (match.scores.normaltime && match.scores.normaltime !== null){ %>
                                        <%=match.scores.normaltime%>
                                    <% } else { %>
                                        <%=match.scores.current%>
                                    <% } %>
                                <% } %>
                            </div>
                            <div class="team-right">
                                <% if(match.sport_id == 5) { %>
                                    <span class="box-img">
                                        <img src="<%=match.team_away.logo_medium%>" alt="<%=match.team_away.name%>">
                                    </span>
                                <% } else { %>
                                    <img src="<%=match.team_away.logo_small%>" alt="<%=match.team_away.name%>">
                                <% } %>
                            </div>
                        </header>
                        <footer>
                            <span>
                                <% if (match.scores && match.scores !=null && match.scores.penalties && match.scores.penalties !=null && match.sport_id !=4){ %>
                                    <% var pt = match.scores.penalties.split(':'),
                                      ot =  match.scores.normaltime.split(':'),
                                      pt1 = parseInt(pt[0])-parseInt(ot[0]),
                                      pt2 = parseInt(pt[1])-parseInt(ot[1]);
                                    %>
                                   <%=locale[langCount].penalty_long%> <%=pt1+':'+pt2%>
                                <% } else if (match.scores && match.scores !=null && match.scores.penalties && match.scores.penalties !=null && match.sport_id ==4){ %>
                                     <% var pt = match.scores.penalties.split(':'),
                                       ot =  match.scores.normaltime.split(':'),
                                       pt1 = parseInt(pt[0])-parseInt(ot[0]),
                                       pt2 = parseInt(pt[1])-parseInt(ot[1]);
                                     %>
                                    <%=locale[langCount].penalty_a%> <%=pt1+':'+pt2%>
                                <% } else if (match.scores && match.scores !=null && match.scores.overtime && match.scores.overtime !=null){ %>
                                    <%=locale[langCount].after_ot_long%>
                                <% } else { %>
                                    <%=match.tournament_name%>
                                <% } %>
                            </span>
                        </footer>
                    <% if(match.match_id != null) { %>
                        <% if(( +match.sport_id === 1 || +match.sport_id === 2 || +match.sport_id === 4 || +match.sport_id === 5 || +match.sport_id === 29 || +match.sport_id === 16) && (window.location.hostname === "sportarena.com" || window.location.hostname === "sportarena.hr" || window.location.hostname === "localhost" || window.location.hostname === "new.sportarena.com" || window.location.hostname === "dev.sportarena.hr")) {%>
                            </a>
                        <% } else { %>
                            </div>
                        <% } %>
                    <% } else { %>
                        </div>
                    <% } %>
                    <% if (match.scores != null) { %>
                       <% var result = ''; %>
                       <% if (match.scores.overtime && match.scores.overtime !== null) { %>
                           <% result = match.scores.overtime.split(':');%>
                        <% } else if (match.scores.normaltime && match.scores.normaltime !== null){ %>
                            <% result = match.scores.normaltime.split(':');%>
                        <% } else { %>
                            <% if (match.scores.current && match.scores.current !== null) { %>
                                <% result = match.scores.current.split(':');%>
                            <% } else { %>
                                <% result = '-:-'.split(':');%>
                            <% } %>
                        <% } %>
                    <% } %>
                    <div class="match_font time">
                        <%=match.sport_name%>
                        <% if (match.playing == true){ %>
                            <% if (match.status_code != 30 || match.status_code != 92){ %>
                                <% if (match.minute != null){ %>
                                    <span class="right red"><%=match.minute%>&#96;</span>
                                <% } else { %>
                                    <span class="right red"><%=match.status %></span>
                                <% } %>
                            <% } else{ %>
                                <span class="right red"><%=match.status %></span>
                            <% } %>
                        <% } else { %>
                            <span class="right">
                                <% if(match.status_code >= 100) { %>
                                      <% if (match.scores.penalties && match.scores.penalties !==null && match.sport_id != 4 ){ %>
                                        <% var pt = match.scores.penalties.split(':'),
                                               ot =  match.scores.normaltime.split(':'),
                                               pt1 = parseInt(pt[0])-parseInt(ot[0]),
                                               pt2 = parseInt(pt[1])-parseInt(ot[1]);
                                         %>
                                        <%=locale[langCount].penalty_p%>. <%=pt1+':'+pt2%>
                                      <% } else if (match.scores.penalties && match.scores.penalties !==null && match.sport_id == 4 ){ %>
                                        <% var pt = match.scores.penalties.split(':'),
                                               ot =  match.scores.normaltime.split(':'),
                                               pt1 = parseInt(pt[0])-parseInt(ot[0]),
                                               pt2 = parseInt(pt[1])-parseInt(ot[1]);
                                         %>
                                        <%=locale[langCount].penalty_a_short%> <%=pt1+':'+pt2%>
                                    <% } else if (match.scores.overtime && match.scores.overtime !==null){ %>
                                        <%=locale[langCount].after_ot%>
                                    <% } else { %>
                                        <%=match.status %>
                                    <% } %>
                                <% } else { %>
                                    <%=moment(match.date_of_match).format('LT') %>
                                <% } %>
                            </span>
                        <% } %>
                    </div>
                    <div class="match_font from">
                        <% if(match.sport_id != 5) { %>
                            <div class="img-container">
                                <img src="<%=match.team_home.logo_small%>" alt="<%=match.team_home.name%>">
                            </div>
                        <% } %>
                        <span><%=match.team_home.short_name%></span>
                        <% if(result) { %>
                            <span class="right bold"><%=result[0]%></span>
                        <% } %>
                    </div>
                    <div class="match_font from">
                        <% if(match.sport_id != 5) { %>
                            <div class="img-container">
                                <img src="<%=match.team_away.logo_small%>" alt="<%=match.team_away.name%>">
                            </div>
                        <% } %>
                        <span><%=match.team_away.short_name%></span>
                        <% if(result) { %>
                            <span class="right bold"><%=result[1]%></span>
                        <% } %>
                    </div>
                </li>
            <% }); %>
        <% } %>
    <% } %>
</script>

