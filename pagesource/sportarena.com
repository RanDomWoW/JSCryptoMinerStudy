<script type="text/template" id="tableCalendarTennisTpl">
    <div class="box clr clearfix margin-bottom-40">
      <table class="calendar">
        <thead class=" tennis">
            <tr>
                <th class="date"><%=locale[langCount].date%></th>
                <th><%=locale[langCount].tournament%></th>
                <th><%=locale[langCount].winner%></th>
            </tr>
        <% data.forEach(function(one, index){ %>
          <tr>
             <td class="date">
                  <span class="main-name"><%=moment(one.details.start_date).format('DD.MM')%> — <%=moment(one.details.end_date).format('DD.MM')%></span>
              </td>
              <td class="tournament-name">
                  <span class="main-name" ><%=one.tournament_name%><%if(one.details.ground_type && one.details.ground_type!==null){%> (<%=one.details.ground_type%>)<%}%></span>
                  <%if(one.details.total_prize_money && one.details.total_prize_money!==null){%>
                  </br><span class="prize"><%=locale[langCount].prize_fond%>: <%=one.details.prize_currency%> <%=one.details.total_prize_money%></span>
                  <%}%>
              </td>
              <td class="winner-name team-right">
                <%if(one.details.final_match && one.details.final_match.winner && one.details.final_match.winner!=null ){%>
                  <%if(one.details.final_match.winner==2 && one.details.final_match.team_away.name!=null ){%>
                    <a href="/tennis/player/<%=one.details.final_match.team_away.team_id%>/">
                      <span class="img-container">
                        <img src="<%=one.details.final_match.team_away.logo_small%>">
                      </span>
                      <span class="name main-name">
                        <%=one.details.final_match.team_away.name%>
                      </span>
                    </a>
                  <%} else if(one.details.final_match.winner==1 && one.details.final_match.team_home.name!=null){%>
                    <a href="/tennis/player/<%=one.details.final_match.team_home.team_id%>/">
                      <span class="img-container">
                        <img src="<%=one.details.final_match.team_home.logo_small%>">
                      </span>
                      <span class="name main-name">
                        <%=one.details.final_match.team_home.name%>
                      </span>
                    </a>
                  <% } %>
                <%} else {%>
                  -
                <% } %>
              </td>
            </tr>
          <% }); %>
        </thead>
      </table>
    </div>
</script>
