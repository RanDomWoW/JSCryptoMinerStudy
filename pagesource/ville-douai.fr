	function loadGeoloc(idmeta, idpage){

	$(document.body).append('<div id="geolocmodal" style="display:none;"></div>');
	var img='<img src="http://www.ville-douai.fr/image/pictos/gmap/map.gif" align="middle" alt="Localiser avec Google Maps" title="Localiser  Google Maps"/>';
	
	
	$("#geolocmodal").load("http://www.ville-douai.fr/index.php/geolocalisation/display?idmetacontenu="+idmeta+"&idpage="+idpage);
	$("#geolocmodal").dialog({
		bgiframe: true,
		height: 750,
		width:940,
		zIndex: 9999,	
		close: function(event, ui) { $("#geolocmodal").remove();}

	}).parent().find('.ui-dialog-title').html('<span >'+img+' Géolocalisation des contenus</span>');
	}
	
	</script>
	
