	 function checkLive(){
		$.ajax({
		  url: 'islive.php',
		  success: function(data){
			if(data>0){
				isLive=true;
				auto_refresh=true;
			}else{			
				isLive=false;
				auto_refresh=false;
			}
			toggleRefresh();
		  }
		});
	 }
	 
	 function toggleRefresh(){		
		if(isLive){
			refresh_interval=6000;
			auto_refresh=true;
		}else{
			refresh_interval=30000;		
		}
		clearInterval(int_refresh);
		if(auto_refresh)
			int_refresh = setInterval( "load(1)", refresh_interval);
	 }	
	 
