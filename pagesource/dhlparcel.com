            },
            
            "express_plz_bereiche": {  
                  
                    regex: "^([0-9]{2,5})?$",
                  
                    message: "Please enter a postcode-operating range. Please enter a at least two-digit postcode-ranges. "
                
            },
            
            "Aktivierungscode": {  
                  
                    regex: "^(53113)?$",
                  
                    message: "Please enter a valid activation code"
                
            },
            
            "Nur Zahlen und Komma": {  
                  
                    regex: "^([0-9]+\,[0-9]{1,2})?$",
                  
                    message: "Please enter the value separated by comma"
                
            },
            
            "Sendungsnummer 12-stellig": {  
                  
                    regex: "^([0-9]{12,12})?$",
                  
                    message: "Please enter a valid 12-digit shipment number (without spaces or special characters as e.g.: .,;'< >)"
                
            },
            
            "packstation-nummer": {  
                  
                    regex: "^([0-9]{3,3})?$",
                  
                    message: "Please enter a 3-digit DHL Packstation number."
                
            },
            
            "SAP PL": {  
                  
                    regex: "^([0-9]{7,7})?$",
                  
                    message: "Podaj Twój, nadany przez DHL, 7-cyfrowy numer SAP."
                
            },
            
            "phonenumber": {  
                  
                    regex: "^([0-9\(\)\/\+ \-]*)?$",
                  
                    message: "Not a valid phone number. Please use only digits, space and '( ) / - _ +'."
                
            },
            
            "numeric": {  
                  
                    regex: "^([0-9]+)?$",
                  
                    message: "Please enter numbers only."
                
            },
            
            "postnumber": {  
                  
                    regex: "^([0-9]{6,10})?$",
                  
                    message: "Not a valid Post number. Please use only number with 6-10 characters."
                
            },
            
            "Kundennummer Express": {  
                  
