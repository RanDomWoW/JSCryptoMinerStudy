<script>
    //global validators
    ui.set('form','default', {
        validators: {
            
            "Benachrichtigungscode 16-stellig": {  
                  
                    regex: "^([0-9]{16,16})?$",
                  
                    message: "Please enter a 16-digit notification-code (without spaces or special characters as e.g. .,;'< >)."
                
            },
            
            "emailexpress": {  
                  
                    regex: "^([_a-zA-Z0-9-]+)(\.[_a-zA-Z0-9-]+)*@[_a-zA-Z0-9-]{2,}\.[a-zA-Z]{2,4}$",
                  
                    message: "Please enter a valid email address."
                
            },
            
            "wert_pro_stueck": {  
                  
                    regex: "^([0-9]*\,?[0-9]*)?$",
                  
                    message: "Please enter only numbers or comma-separated numbers. Dots are not allowed. "
                
            },
            
            "Telefonnummer AT ohne Sondernummern": {  
                  
                    regex: "^(0043(?!810)(?!820)(?!821)(?!900)(?!901)(?!930)(?!931)(?!939)(?!118)[0-9]{4,30})?$",
                  
                    message: "Not a valid phone number. Please use only digits starting with 0043. Paid-numbers (like 810, 820, etc.) are not supported."
                
            },
            
            "email_oka": {  
                  
                    regex: "^([^@]+([@]{1})[0-9a-zA-Z\\._-]+([\\.]{1})[0-9a-zA-Z\\._-]+)?$",
                  
                    message: "Please enter a valid email address."
                
            },
            
            "countrycode": {  
                  
                    regex: "^([0-9]*)?$",
                  
                    message: "Please insert your countrycode here."
                
            },
            
            "Sendungsnummer 10-stellig": {  
                  
                    regex: "^([0-9]{10,10})?$",
                  
                    message: "Please enter a valid 10-digit shipment number (without spaces or special characters as e.g.: .,;'< >)"
                
            },
            
            "Hausnummer": {  
                  
                    regex: "^([0-9]{1,4}[a-zA-Z]?)?$",
                  
                    message: "Please enter a valid street nubmer"
                
            },
            
            "anzahl_urnen": {  
                  
                    regex: "^([1-9]|1[0-9]|20)?$",
                  
                    message: "Please enter a shipment-quantity of 1 to 20. "
                
            },
            
            "plz-urnenversand": {  
                  
                    regex: "^((?!18565|25845|25849|25859|25863|25869|25938|25946|2598025992|25996|25997|25999|26465|26474|26486|26548|26571|26579|26757|27498|27499|83256)([0-9]{5,5}))?$",
                  
                    message: "Der Versand und Empfang auf Inseln ohne Straßenanbindung ist nicht möglich."
                
            },
            
            "plz-es": {  
                  
                    regex: "^([a-zA-Z0-9]{5,5})?$",
                  
                    message: "Please enter a valid postcode."
                
            },
            
            "Zeichen 0- bis 80-stellig": {  
                  
                    regex: "^(.{0,80})?$",
                  
                    message: "Please enter a maximum of 80 characters."
                
            },
            
            "Sendungsnummer 10- bis 39-stellig": {  
                  
                    regex: "^([0-9a-zA-ZÄäÖöÜüß]{10,39})?$",
                  
                    message: "Please enter a valid shipment number (min. 10 digits, max. 39 digits, without spaces or special characters as e.g.: .,;'< >)"
                
            },
            
            "Sendungsnummer 9- bis 39-stellig": {  
                  
                    regex: "^([0-9a-zA-ZÄäÖöÜüß]{9,39})?$",
                  
                    message: "Please enter a valid shipment number (min. 9 digits, max. 39 digits, without spaces or special characters as e.g.: .,;'< >) or the word 'unbekannt', if you don't know the shipment number."
                
            },
            
            "BLZ oder BICSwift": {  
                  
                    regex: "^([0-9]{8}$|^[a-zA-Z]{4}[a-zA-Z]{2}[a-zA-Z0-9]{2}([a-zA-Z0-9]?){3})?$",
                  
                    message: "Please enter a valid BLZ / BIC-SWIFT"
                
            },
            
            "express_accountnr": {  
                  
                    regex: "^([0-9]{9,9})?$",
                  
                    message: "Please enter a 9-digit DHL Express-Accountnumber."
                
            },
            
            "phonenumber-iberia": {  
                  
                    regex: "^([0-9]{9,30})?$",
                  
                    message: "Not a valid phone number. Please use only numbers with at least 9 characters."
                
            },
            
            "Vorgangsnummer Kundenservice": {  
                  
                    regex: "^(1-[0-9]{10})?$",
                  
                    message: "Please enter a valid transaction number of the customer service"
                
            },
            
            "Rechnungsnummer Express Ustd": {  
                  
                    regex: "^([A-Z]{3}[0-9]{7})?$",
                  
                    message: "Please enter your 10-digit invoice number"
                
            },
            
            "Sendungsnummer 12- bis 39-stellig": {  
                  
                    regex: "^([0-9a-zA-ZÄäÖöÜüß]{12,39})?$",
                  
