                    message: "Please enter a valid customer number"
                
            },
            
            "rechnungsbetrag": {  
                  
                    regex: "^([0-9]{1,},[0-9]{2})?$",
                  
                    message: "Please enter your comma-separated billing amount."
                
            },
            
            "express_abrechnungsnr": {  
                  
                    regex: "^([0-9]{14,14})?$",
                  
                    message: "Please enter a 14-digit Express billing number."
                
            },
            
