                    message: "Please enter a valid postcode."
                
            },
            
            "E-Mail-Postnumber": {  
                  
                    regex: "^((([a-zA-Z0-9_-]+)(.[a-zA-Z0-9_-]+)*)?([a-zA-Z0-9._-]*)@[a-zA-Z0-9][a-zA-Z0-9_-]+(.[a-zA-Z0-9_-]+)*.[a-zA-Z]{2,8}|[0-9]{6,10})$",
                  
                    message: "Please enter a valid email address or Post number."
                
            },
            
            "emailtest2": {  
                  
                    regex: "^[A-Z0-9._%-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$",
                  
                    message: ""
                
            },
            
            "plz": {  
                  
                    regex: "^([0-9]{5,5})?$",
                  
                    message: "Please anter a 5-digit postcode."
                
            }
            
        }
