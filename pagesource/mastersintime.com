(function() {
	
    var request = null;
    var value = $('#search').val();
    var timer = null;

    // search suggestion
    $('#search').keyup(function(){
        var val = $('#search').val();

        if (value != val || val.length < 3) {
            if (request) {
                request.abort();
            }
            if (timer) {
                clearTimeout(timer);
            }
        }

        if (val.length < 3) {
            $('.core-search-form-suggestions').fadeOut(200);
        } else if(value != val) {
            value = val;
            timer = setTimeout(function () {
                request = $.ajax({
                    url: '/ajax/core-ajax-suggestions.php',
                    type: 'POST',
                    dataType: 'json',
                    data: {
                        userInput : val,
                        site : "mastersintime.com",
                        domainID : "61",
                        tag : ""
                    },
                    success: function(data){
                        $('#core-ajax-suggestions').html(data.right);
                        $('#core-ajax-suggestions-text').html(data.left);
                        $('.core-search-form-suggestions').fadeIn(200);
                    }
                });
            }, 200);
        }
    });

    $('#search').focus(function(){
        if (request) {
            $('.core-search-form-suggestions').fadeIn(200);
        }
    });
    $('#search').blur(function(){
        $('.core-search-form-suggestions').fadeOut(200);
    });

})($);

