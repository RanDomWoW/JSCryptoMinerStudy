function getCookie(e) {
    for (var r = e + "=", t = document.cookie.split(";"), o = 0; o < t.length; o++) {
        for (var n = t[o];
            " " == n.charAt(0);) n = n.substring(1);
        if (0 == n.indexOf(r)) return n.substring(r.length, n.length)
    }
    return ""
}

function getJsonFromParams(e) {
    var r = {};
    return e.split("&").forEach(function(e) {
        var t = e.split("=");
        r[t[0]] = decodeURIComponent(t[1])
    }), r
}

function decodeParameter(e) {
    return decodeURIComponent(e.replace(/\+/g, " "))
}
$(document).ready(function() {
  JSON.stringify(getJsonFromParams(getCookie("jsCrit")));
  JSON.stringify(getJsonFromParams(getCookie("m30_CurrentSearch")));
  
