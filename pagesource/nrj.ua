			stat.sendStat(TnsStatuses.ERROR);
			console.log('----> tns error');
		});
		s.dom.window.on("sitePlayer.error", function(){
			stat.sendStat(TnsStatuses.ERROR);
			console.log('----> tns site player error');
		});
	});
</script></body>
