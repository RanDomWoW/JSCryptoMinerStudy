<script type="text/javascript">
    var fiziHomePagePopoverTemplate = window.fiziHomePagePopoverTemplate || {};
    fiziHomePagePopoverTemplate.options = window.fiziHomePagePopoverTemplate.options || function () {
        var self = this;

        self.ESCAPE_KEY = 27;
        self.FIZI_SEARCH_BOX_SELECTOR = "#findByZipCode.HomePagePopover";
        self.RESET_BUTTON_SELECTOR = "#resetButton";
        self.DIMMED_BACKGROUND_SELECTOR = "#fiziHomePagePopoverModal";
        self.HERO_VIDEO_CAPTION_SELECTOR = ".home-video";
        self.HERO_VIDEO_VISIBILITY_TOGGLE_CLASS = "homepagePopoverVisibilityToggle";
        self.VALIDATION_MESSAGE_CSS_SELECTOR = ".fiziValidation .validationMessage";

        self.focusInput = function () {
            ConnectionsEducation.FindByZip.FocusInput();
        };
        self.focusFirstResult = function () {
            ConnectionsEducation.FindByZip.FocusFirstResult(true);
        };
        self.makeFiziAccessible = function () {
            ConnectionsEducation.FindByZip.MakeFiziAccessible(true);
        };
        self.onSearchCompleted = function () {
            self.makeFiziAccessible();
            self.focusFirstResult();
    
            if (!self.hasValidationMessage()) 
                $(self.FIZI_SEARCH_BOX_SELECTOR).css("z-index", 99999);
        };
        self.onSearchCleared = function () {
            $(self.FIZI_SEARCH_BOX_SELECTOR).css("z-index", 190);
        };
        self.hasValidationMessage = function() {
            return $(self.VALIDATION_MESSAGE_CSS_SELECTOR).length > 0;
        };

        $(document).ready(function () {
            $(self.HERO_VIDEO_CAPTION_SELECTOR).addClass(self.HERO_VIDEO_VISIBILITY_TOGGLE_CLASS);
            $(self.DIMMED_BACKGROUND_SELECTOR).appendTo("body");

            $(document).keyup(function (e) {
                if (e.keyCode === self.ESCAPE_KEY) {
                    if ($(self.RESET_BUTTON_SELECTOR).is(':visible')) {
                        $(self.RESET_BUTTON_SELECTOR).click();
                    }
                }
            });

        });

    };
</script>
