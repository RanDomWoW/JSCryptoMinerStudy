<script type="text/javascript">
    
    var date_start = '';
    var date_end = '';

    if(isSavedDate()){
        date_start = ConvertDateFrToEn(localStorage.getItem("date_start"));
        date_end = ConvertDateFrToEn(localStorage.getItem("date_end"));
    }
    

    //TM Declared Variables
    var tm_vars = new Array();
    tm_vars['env_country'] = 'France';
    tm_vars['env_language'] = 'FR';

    tm_vars["user_category"] = 'new client';
    if(tm_vars["user_category"] != 'logged')
        tm_vars["user_id"] = getUserAnonymousId();
    else
        tm_vars["user_id"] = '';

    tm_vars["user_email"] = '';
    tm_vars["user_prenom"] = '';
    tm_vars["user_tel"] = '';

    tm_vars["village_id"] = '';
    tm_vars["village_name"] = '';
    tm_vars["village_region"] = '';

    tm_vars["confort_heb"] = '';
    tm_vars["heb_name"] = '';
    tm_vars["segment"] = '';
    tm_vars["heb_type"] = '';
    tm_vars["code_hebergement"] = '';
    tm_vars["date_start"] = date_start;
    tm_vars["date_end"] = date_end;

    tm_vars["nom_object"] = 'Site marchand';
    tm_vars["id_objet"] = '11265';
    tm_vars["ids_villages"] = '';

    var first_folder = $(location).attr('pathname');
    first_folder.indexOf(1);
    tm_vars["page_category"] = first_folder.split("/")[1];

    tm_vars["support"] = 'Desktop';
    if(isMobile)
        tm_vars["support"] = 'Tablet';

    
    // Data Layer Site
    var dataLayer = [];
    dataLayer.push({
        'groupedesite': 'SiteMarchand', // SiteMarchand, Mag, SitePrivillege ou AppliMobile iOS Android
        'pays': tm_vars['env_country'], // Pays du Site, France
        'langue': tm_vars['env_language'] // Langue du site, FR
    });
    // Data Layer Produits
    dataLayer.push({
        'name': tm_vars["heb_name"], // Nom Hébergement
        'id': tm_vars["code_hebergement"], // Id Hébergement
        'price': '', // Prix du produit, séparateur décimal : "."
        'brand': tm_vars["segment"], // Segment Authentic ou Club
        'category': tm_vars["village_name"], // Nom du village
        'variant': tm_vars["heb_name"], // Confort de hébergement, 3 fleurs par exemple
        'quantity': '', // Quantité
        'list': tm_vars["heb_type"], // Type de liste d'hébergement : Location ou Emplacement
        'datedebut': tm_vars["date_start"], // Date de début du séjour 2009-06-15 format AAAA-MM-JJ
        'datefin': tm_vars["date_end"] // Date de fin du séjour 2009-06-15 format AAAA-MM-JJ
    });
    // Data Layer Visiteurs
    dataLayer.push({
        'visitorid': tm_vars["user_id"], // Identifiant du visiteur si loggué sinon un nombre aléatoire avec la date du jour, l'heure, la minute
        'visitorloginstatus': tm_vars["user_category"], // Statut de connexion : logged ou new client
        'visitorprenom': tm_vars["user_prenom"], // Prénom du visiteur
        'visitoremail': tm_vars["user_email"], // Email du visiteur
        'visitortel': tm_vars["user_tel"] // Téléphone du visiteur
    });
    // Data Layer Contenus
    dataLayer.push({
        'pagecategory': tm_vars["page_category"], // Catégorie de page :  premier dossier de l'url (ou home page si aucun dossier)
        'support':  tm_vars["support"],  // Desktop, Mobile, Tablet
        'idobjet':  tm_vars["id_objet"], // Id de l'objet EZ
        'nomobjet': tm_vars["nom_object"], // Nom de l'objet EZ
        'idsvillages': tm_vars["ids_villages"] // Ids webcamp des villages sur la page séparés par une virgule
    });
    // Data Layer Village
    dataLayer.push({
        'villageid': tm_vars["village_id"], // Id Webcamp du village
        'villagenom': tm_vars["village_name"] , // Nom du Village
        'villageregion': tm_vars["village_region"] // Région du village
    });
    
</script>
