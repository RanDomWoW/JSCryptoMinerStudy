function checkForm() {
    var dateIsValide = true;

    if(!IsDateInFuture(jQuery("#searchd1").val(), jQuery("#searchd2").val())){
        dateIsValide = false;
    }

    if((jQuery("#searchd1").val() == "") && (jQuery("#searchd2").val() == "")) {
    
        jQuery("#searchd1").val("JJ/MM/AAAA");
        jQuery("#searchd2").val("JJ/MM/AAAA");
    
        dateIsValide = true;
    }

    if( dateIsValide ) {
        jQuery("#main-search-camping").removeClass('error');
        return true;
    }
    else {
        jQuery("#main-search-camping").addClass('error');
        return false;
    }
}

window.onload=function() {    $('#waiting-bt-filtre-village').hide();
    $('#bt-filtre-village').show();
    initDateRangePicker(jQuery('#container-date'), 'JJ/MM/AAAA', jQuery('#searchd1'),  jQuery('#searchd2'), 'fr', true);};
jQuery(document).ready(function() {    // On insere dans les champs les dates saisies par l'utilisateur
    inputDate( jQuery("#searchd1"), jQuery("#searchd2") );

    // Style Univers selectionne
    $("#main-search-camping .block-input-form label").click(function(e) {        e.preventDefault();
        var current = $(this);
        if($(this).hasClass('selected')){            current.removeClass('selected');
            current.css("color", "#444");
            current.find(".radio-circle-univers").css("color", "#444");
            $("input[name=univers]").prop('checked', false);
        }        else {            var prev_selected_univers = $('#main-search-camping label.selected');
            $(prev_selected_univers).find( "input" ).prop('checked', false);
            prev_selected_univers.removeClass('selected');
            current.addClass('selected');
            $(current).find( "input" ).prop('checked', true);
        }    });
    jQuery("#submit_button").click(function() {        return checkForm();
    });});
</script>        <!-- FIN Moteur de recherche -->
