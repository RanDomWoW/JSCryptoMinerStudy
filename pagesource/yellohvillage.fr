        $(document).ready(function() {
            $("#myModal #moncompte_login_form").on("submit", function(event) {
                event.preventDefault();
                var login = $("#mon_compte_login_input_numeroid").val();
                var pass = $("#mon_compte_password_input_numerofid").val();
                var code_camping = $("#id_camping_favoris").val();

                $.ajax({
                    type: "POST",
                    url: "/villages_favoris/ajax_connection/",
                    data: "login=" + login + "&password=" + pass + "&code_camping=" + code_camping,
                    error: function (request, error) {
                        console.log(error);
                    },
                    success: function (data) {
                        console.log(data);
                        if (data.substring(0, 5) === "error") {
                            $(".popup_error").css("display", "none");
                            $("." + data).css("display", "block");
                        } else{
                            $(".popup_error").css("display", "none");
                            $.ajax({
                                url: "/villages_favoris/check_favoris/" + code_camping + "/add",
                                error: function(request, error) {
                                    console.log(error);
                                },
                                success: function(data) {
                                    console.log(data);
                                    $("body").find('[data-camping="' + result.code_camping + '"]').find("path").attr("class","favoris");
                                    console.log("favoris ok");
                                }
                            });
                            location.reload();
                        }
                    }
                });
            });
        });
    
</script>
