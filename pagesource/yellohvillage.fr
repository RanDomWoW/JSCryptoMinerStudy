<script type="text/javascript">
    
    // Si l'utilisateur a renseigné ses dates, on modifie le lien du header qui pointe vers le moteur de recherche en mettant ses dates
    jQuery(document).ready(function() {

        if(!isMobile) {
            $("#switch-village").combobox();
        }

        if(isSavedDate()) {
            var nb_personne = 1;
            var destination = '';
            var hebergement = 'location';
            $(".bt-start-search").each(function( index ) {
                if(localStorage.getItem("nb_personne") != null)
                    nb_personne = localStorage.getItem("nb_personne");
                if(localStorage.getItem("destination") != null)
                    destination = localStorage.getItem("destination");
                if(localStorage.getItem("hebergement") != null)
                    hebergement = localStorage.getItem("hebergement");

                $(this).attr("href", "/camping/search?search_d1="+localStorage.getItem("date_start")+"&search_d2="+localStorage.getItem("date_end")+"&nb_personnes="+nb_personne+"&search_heb="+hebergement+"&univers="+destination+"&search_fid="+$(this).data("fid"));

            });
        }
    });
    
</script>        <div class="clear"></div>
