<script>
$(document).ready(function () {
	$(window).resize(function(){
		var picked = $cookie("picked") || [];
		if (!picked.length) {
			return;
		}
		
		//  브라우저의 크기가 일정 사이즈 이하로 작아지면 오늘 본 상품 목록 안보이게
		var currentWidth = $(window).width();
		currentWidth <= 1254 ? $(".history").hide() : $(".history").show();
			  
	}).resize();


	///	마우스오버 시에 글씨가 좌로 이동
	$(".history ul li")
		.mouseenter(function () {
			var $span = $(this).find(".list-name span").css("width", "auto");
			$span.data("animate", true);
			
			var textWidth = $span.width();
			var parentWidth = $span.parent().width();
	
			if (parentWidth > textWidth) {
				$span.css({"left": 0, "width": "100%"});
				return;
			}
			
			var speed = textWidth * 15;
			
			function temp() {
				if (!$span.data("animate")) {
					return;
				}

				$span.hide().stop().css({left:parentWidth}).show().animate({left:-textWidth+'px'}, speed, 'linear', function(){
						temp();
				});
			};
			temp();
		})
		
		.mouseleave(function () {
			var $span = $(this).find(".list-name span").css("width", "auto");
			$span.data("animate", false);
			$span.stop();
		});	
	
	
	$(function() {
		$("section.section input:not(.no-focus)").eq(0).focus();
	});
	
});

</script>