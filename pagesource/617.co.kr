<script>
///	마우스오버 시에 글씨가 좌로 이동
$(".flim-popup-viewer .info td.name").live({

	"mouseenter": function () {
		var $span = $(this).find("span");
		$span.data("animate", true);


		$span.parent().css("text-overflow", "clip");

		var textWidth = $span.width();
		var parentWidth = $span.parent().width();

		$span.parent().css("text-overflow", "ellipsis");


		if (parentWidth > textWidth) {
			return;
		}

		function temp(start) {
			start = start || parentWidth;
			$span.parent().css("text-overflow", "clip");

			if (!$span.data("animate")) {
				return;
			}

			var speed = (textWidth + start) * 15;

			$span.hide().stop().css({left:start}).show().animate({left:-textWidth+'px'}, speed, 'linear', function(){
				temp();
			});
		};

		temp(1);
	},
	
	"mouseleave": function () {
		var $span = $(this).find("span").css("width", "auto");
		$span.data("animate", false);
		$span.parent().css("text-overflow", "ellipsis");
		$span.stop().css("left", 0);
	}
});
</script>

<script>
function ViewController(self, BaseViewController) {
return {
	init: function() {
		BaseViewController.init();
		self.main_popup = false;
		self.file_type = false;
	},

	"메인팝업닫기": function() {
		if (self.main_popup == false) {
			return;
		}
		self.main_popup = false;
	}
}}


function 메인링크(type, id) {
	if (type !== "film") {
		return;
	}
	var self = document.$scope;

	var params = {};
	params.id = id;
	params.type = type;
	
	var item = $$("film.load", params);
	self.popup('상세보기', item);
	document.update();
	
	return false;
}




var loading = false;

$("a.main-summary-more").live("click", function(e) {	
	e.preventDefault();
	
	if (loading) {
		return;
	}		

	loading = true;

	var self = $(this);
	self.html("");
	self.css("background", "url('/img/1px-loading.gif') no-repeat center center");

	$.post(self.attr("href"), function(data) {				
		if (!data) {
			return;
		}
		
		var a = $(data).find(".main-summary");
		self.parent().after(a);
		self.remove();

		loading = false;
	});	
});
</script>








