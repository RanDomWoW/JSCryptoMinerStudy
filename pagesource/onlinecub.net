	function caroSlider(){
		var timeoutId;
		var slideImage = function( step ,myid) {
		var indx = null;
			if ( step == undefined ) step = 1;
			clearTimeout ( timeoutId );
			if(myid=="s1"){
				indx=0;
			}
			else if(myid=="s2"){
				indx=1;
			}
			else if(myid=="s3"){
				indx=2;
			}
			else if(myid=="s4"){
				indx=3;
			}
			if(indx == null){
				var indx = $('.caro:visible').index('.caro');
				if ( step != 0 ) {
				   $('.caro:visible').css('display','none');
				   $('.indicator').attr('src','../imagesIB/indicator.png');
				}
				indx = indx + step ;
				if ( indx >= $('.caro').length ) {
					indx = 0;
				} else if ( indx < 0 ) {
					indx = $('.caro').length - 1;
				}
				if ( step != 0 ) {
				   $('.caro:eq(' + indx + ')').css('display','inline');
				   $('.indicator:eq(' + indx + ')').attr('src','../imagesIB/indicator-active.png');
				}
				timeoutId = setTimeout ( slideImage, 3000 );
			}
			else{
				$('.caro:visible').css('display','none');
				$('.indicator').attr('src','../imagesIB/indicator.png');
				$('.caro:eq(' + indx + ')').css('display','inline');
				$('.indicator:eq(' + indx + ')').attr('src','../imagesIB/indicator-active.png');
				timeoutId = setTimeout ( slideImage, 3000 );
			}
			
		};
		slideImage(0);
		/*$('#previous').click(function() {
			slideImage ( -1 );   
		});
	
		$('#next').click(function() {
			clearTimeout ( timeoutId );
			slideImage ( 1 );
		});*/
		$('#s1').click(function(){
			clearTimeout ( timeoutId );
			slideImage ( 0 ,'s1');
		});
		$('#s2').click(function(){
			clearTimeout ( timeoutId );
			slideImage ( 1 ,'s2');
		});
		$('#s3').click(function(){
			clearTimeout ( timeoutId );
			slideImage ( 2 ,'s3');
		});
		$('#s4').click(function(){
			clearTimeout ( timeoutId ); 
			slideImage ( 3 ,'s4');
		});
		$('#pause').click(function() {
		   clearTimeout ( timeoutId );    
			$(this).hide();
			$('#play').show();
		});
		$('#caroimgs').hover(function(event){
			clearTimeout ( timeoutId ); 
			$(this).css("opacity",".9");
			 event.stopPropagation();
			}, function(){
			$(this).css("opacity","1");
			slideImage(0);
		});
		/*$('#play').click(function() {
		   slideImage(0);
		   $(this).hide();
		   $('#pause').show();    
		});*/
	}
