function setMenuExpand(){
if(width <= 801){
	var lilengthA = 0;
	lilengthA = $('.sidenavul > li').length;
	for(var i = 1;i < lilengthA+1; i++){
		$('#menu'+i).css('display','none');
		$('#menu'+i+'li > p').css({"background-color":"inherit","margin-left":"10px","padding":"0px"});
	}
}
}
function menuExpand(id){
var lilength = 0;
lilength = $('.sidenavul > li').length;
if($('#'+id).css("display")==="none"){
for(var i = 1;i < lilength+1; i++){
$('#menu'+i).css('display','none');
$('#menu'+i+'li > p').css({"background-color":"inherit","margin-left":"10px","padding":"0px"});
}
	$('#'+id).css('display','block');
	$('#'+id+'li > p').css({"background-color":"#e60677","margin-left":"0px","padding":"5px"});
}
else{
	$('#'+id).css('display','none');
}
}

function showMore(){
