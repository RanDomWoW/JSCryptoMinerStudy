function launchFunc(str){
	if(str === "login"){
		if($('.LoginBoxRight').hasClass('nodisplay')){
			$('.LoginBoxRight').removeClass('nodisplay');
		}
		else{
			$('.LoginBoxRight').addClass('nodisplay');
		}
	}
	else if(str === "nav"){
		if($('.mobSideNav').hasClass('nodisplay')){
			$('.mobSideNav').removeClass('nodisplay');
		//	$('.mobSideNav').addClass('activenav');
			//$('.mobSideNav').css('height',height-64+'px');
			$('#mobNavOpen').addClass('nodisplay');
			$('#mobNavClose').removeClass('nodisplay');
			
		}
		else{
			$('.mobSideNav').addClass('nodisplay');
			$('#mobNavOpen').removeClass('nodisplay');
			$('#mobNavClose').addClass('nodisplay');
			setMenuExpand();
			//$('.mobSideNav').removeClass('activenav');
		}
	}
}
