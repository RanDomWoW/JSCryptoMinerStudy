<script type="text/javascript">
 $(document).on("ready", function(){
 /*set initial height-firstz paragraph*/
  var maxHeigth=$(".firstV").height();
  $(".customReadMore").css("height", maxHeigth+"px");
 /*insert button read more*/
  $("<div class='readMoreBtnC'>mehr anzeigen</div>").insertAfter($(".customReadMore"));
 /*click on button read more*/
	$(".readMoreBtnC").on("click touch touchend", function(e){
		   e.preventDefault();
           var readMoreTxt="mehr anzeigen";
           var readLessTxt="weniger anzeigen";
           var readMoreBtn=$(".readMoreBtnC");
           var contanier=$(".customReadMore");
	

	if(readMoreBtn.html()==readMoreTxt){
		contanier.css("height",parseInt(contanier.prop("scrollHeight"))+"px");
		readMoreBtn.html(readLessTxt);
	}

	else{
		contanier.css("height",+maxHeigth+"px");
		readMoreBtn.html(readMoreTxt);
		$('html, body').animate({scrollTop: $(".customReadMoreC").offset().top-20}, 1000);
	}
		e.stopPropagation();
 }); 
 });
</script>



