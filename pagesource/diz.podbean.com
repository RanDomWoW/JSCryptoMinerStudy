		function bindFollow()
		{
			$.ajax({
		     url:"https://www.podbean.com/site/userCenter/getFollowStatus",
		     type:"GET",
		     data:"id=1564148&type=pbblog",
		     dataType:"jsonp",
		     context:$("#follow"),
		     success:function(data){  
		      
		      if(data.code=="200"&&data.followStatus=="2")
		      {
		      	 $(this).click(function(event) {
		            alert("You don't need to follow yourself.");
		            return false;
		         });
			  } 
		      else if(data.code=="200")
		      {
		       if(data.followStatus==1)
		       {
		        $(this).addClass("followed");
		        $(this).text("Following");
		       } 

		       $(this).click(function(){
		        var node = $(this);
		        if(node.hasClass("followed"))
		              { 
		                $.ajax({
		                    url: "https://www.podbean.com/site/userCenter/follow",
		                    type:"GET",
		                    data:"id=1564148&type=pbblog&action=unfollow",
		                    context:node,
		                    dataType:"jsonp",
		                    success: function(data){
		                      if(data.code == 200)
		                      { 
		                       	$(this).removeClass("followed");
		                          $(this).addClass("unfollow");
		                          $(this).text("Follow");
		                          $("#header-follow").removeClass('followed');
		                          $("#header-follow").html("<i class='pbicon-plus'></i> Follow");
		                          $(".audiocast-follow").removeClass('pb-following');
