	var timer = null;
	function mobileLoadIframe(){
		if(timer !== null){
			clearTimeout(timer);
		}
		timer = setTimeout(function(){
			
			var window_height,scrollTop;
			window_height 	= $(window).height();
			scrollTop 		= $(window).scrollTop();
			$('.aplayer-panel').not('[data-uri]').each(function(){
				var offset_top = $(this).offset().top;
				var height = $(this).height();
				console.log(offset_top);
				if(offset_top>=scrollTop-height&&offset_top<=scrollTop+window_height){
					(showIframe.bind(this))(true);
				}
			});
		},300);
	}

	function blockPlay(){
		$('.aplayer-panel[rel="1"]').css('background-image','url("/pb-assets/assets/images/player_theme_vjs/all.png")')
							.find("iframe").remove()
							.end()
							.attr('rel','0')
							.children('.aplayer-bg')
							.css('display','block');
	}
	function showIframe(){
		if(!isMobile){
			blockPlay();
		}
		if($(this).find('iframe').length <= 0 ){
			$(this).append('<iframe allowtransparency="true" width ="100%" height="38" frameborder="0" scrolling="no" src="'+$(this).attr('data-source')+'"></iframe>')
					.attr('rel','1')
					.css('background-image','none')
					.children('.aplayer-bg')
					.css('display','none');
		}
		return false;
	}
	function playerLayout(){
		var $this = $(".aplayer-panel");
		var $playerWidth = $this.outerWidth();
		if($playerWidth <= "480"/* && $playerWidth >= "320"*/){
			$this.find(".aplayer-bg").css({'background':'none','padding-right':'0'})
			     .find('.l').css('right','5px')
			     .siblings('span').css('padding-right','48px');
		}
	}
