    <script type="text/javascript">
        $(document).ready(function(){ 				        
            $('#HlPanelHide').click(
                function () {  
                    var c = new $.cookie("mlpanel");
                
                    c.panel = "hidden";
                    c.set({ expires: 7, path: '/'});
                    $(this).hide();
                    $('#HlPanelShow').show();
                    $(".right-col").slideToggle(100);
                    $(".middle-col").stop().animate( { 'margin-right':'0' }, 400 );    
                                        	                        		
                    return false;
                });  	
            
            $('#HlPanelShow').click(
                function () { 
                    var c = new $.cookie("mlpanel");
                   
                    c.panel = "";				 
                    c.set({ expires: -7, path: '/'}); 
                    $(this).hide();
                    $('#HlPanelHide').show();
                    $(".middle-col").css("margin-right", "22.5em");      		
                    $(".right-col").slideToggle(200);
        		
                    return false;
                });  
        });
    </script>
