        $("#Today").click(function (e) {
            var today = new Date();
            $("#date").val(today.getDate() + "/" + (today.getMonth() + 1) + "/" + today.getFullYear());
            window.location = "Home.aspx?date=" + $("#date").val();
        });

        $("#Previous").click(function (e) {
            var arrMeetingDate = $("#date").val().split("/");
            var previousDate = new Date(arrMeetingDate[2], (parseInt(arrMeetingDate[1], 10) - 1), arrMeetingDate[0]);
            previousDate.setDate(previousDate.getDate() - 7);
            $("#date").val(previousDate.getDate() + "/" + (previousDate.getMonth() + 1) + "/" + previousDate.getFullYear());
            window.location = "Home.aspx?date=" + $("#date").val();
        });

        $("#Next").click(function (e) {
            var arrMeetingDate = $("#date").val().split("/");
            var nextDate = new Date(arrMeetingDate[2], (parseInt(arrMeetingDate[1], 10) - 1), arrMeetingDate[0]);
            nextDate.setDate(nextDate.getDate() + 7);
            $("#date").val(nextDate.getDate() + "/" + (nextDate.getMonth() + 1) + "/" + nextDate.getFullYear());
            window.location = "Home.aspx?date=" + $("#date").val();
        });
    </script>



    

