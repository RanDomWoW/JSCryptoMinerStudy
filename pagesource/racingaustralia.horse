<script type="text/javascript">
   $(document).ready(function(){
    function update_countdown() {
    var schedule = [
    new Date('2017-08-01T00:00:00+10:00')
];

var events = [];

var offset = new Date().getTimezoneOffset() * 60 * 1000;

for (var i = 0; i < schedule.length; i++) {
    var local_schedule = schedule[i];
    if (local_schedule > new Date()) { events.push(local_schedule); }
}
events.sort(function(a, b) {
  return (new Date() - b) - (new Date() - a);
});

var next_event = events[0];
    var now = new Date();
    var timediff = next_event - now;
     var msec = timediff;
     var dd = Math.floor(msec / 1000 / 60 / 60 / 24);
     msec -= dd * 1000 * 60 * 60 * 24;
     var hh = Math.floor(msec / 1000 / 60 / 60);
     msec -= hh * 1000 * 60 * 60;
     var mm = Math.floor(msec / 1000 / 60);
     msec -= mm * 1000 * 60;
     var ss = Math.floor(msec / 1000);
     msec -= ss * 1000;
      
       $('#countdown-day').text(dd = dd < 10 ? '0' + dd : dd);
       $('#countdown-hour').text(hh = hh < 10 ? '0' + hh : hh);
       $('#countdown-minute').text(mm = mm < 10 ? '0' + mm : mm);
       $('#countdown-second').text(ss = ss < 10 ? '0' + ss : ss);
       }
     setInterval(update_countdown, 1000);
    });

    </script>

 
 
