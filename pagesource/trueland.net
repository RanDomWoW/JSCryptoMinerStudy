    // search
    function focusBlur(obj, str) {
        obj.focus(function() {
            if ($(this).val() == str) {
                $(this).val('');
            }
            else {
                $(this).select();
            }
        }).blur(function() {
            if ($(this).val() == '') {
                $(this).val(str);
            }
        });
    }

    // nav
    function nav(obj, showObj) {

        obj.hover(function() {
            $(this).children('a').addClass('navCur').siblings(showObj).show().parent().siblings().children('a').removeClass('navCur');
        }, function() {
            $(this).children('a').removeClass('navCur').siblings(showObj).hide();
        });
    }

    // news
    function news(obj) {
        var aEle = obj.children();
        var timer = null;
        var i = 0;

        function autoPlay() {
            i++;
            if (i > aEle.length - 1) {
                i = 0;
            }
            aEle.eq(i).slideDown().siblings().slideUp();
        }
        timer = setInterval(autoPlay, 3000);

        obj.hover(function() {
            clearInterval(timer);
        }, function() {
            timer = setInterval(autoPlay, 3000);
        });
    }

    // 二维码
    function showHide(obj, showObj) {
        var timer = null;
        obj.hover(function() {
            clearTimeout(timer);
            showObj.show();
        }, function() {
            timer = setTimeout(function() {
                showObj.hide();
            }, 300);
        });

        showObj.hover(function() {
            clearTimeout(timer);
            showObj.show();
        }, function() {
            timer = setTimeout(function() {
                showObj.hide();
            }, 300);
        });
    }

</script>



