<script type="text/javascript">
    $(document).ready(function(e) {
        focusBlur($('.keywords'), '搜索');

        nav($('.nav > ul > li'), $('.subNavT'));

        news($('.newsBarListDiv'));

        showHide($('#wap'), $('.top_erweima'));
		showHide($('#weixin'), $('.top_weixin'));
