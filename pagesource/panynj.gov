function latestTwitter()
{
	xmlHttpT=GetXmlHttpObject();
	if (xmlHttpT==null)
	{
		alert ("Browser does not support HTTP Request");
		return;
	}
	
	// https://api.twitter.com/1/statuses/user_timeline.rss?screen_name=PANYNJ
	var tab = document.getElementById('tabletwitter');
	for(var i = tab.rows.length;i>0;i--)
	{
		tab.deleteRow(i -1);
	}
	
	var url = 'ajax-twitter-rss-feed.cfm';	
	url=url + '?maxposts=1';
	url=url + '&sid=' + Math.random();
	
	xmlHttpT.onreadystatechange=latestTwitterCallBack;		
	xmlHttpT.open("GET",url,false);
	xmlHttpT.send(null);		
}


function latestTwitterCallBack()
{
	var postArray = new Array();
	var fieldNameValueArray = new Array();
	var fieldNameArray = new Array();
	var fieldValueArray = new Array();
	var fieldNameValuePair = new Array();
	var fieldName;
	var fieldValue;
		
	var title='';
	var description='';
	var pubDate='';

	if (xmlHttpT.readyState==4 || xmlHttpT.readyState=="complete")
	{		
		//alert(xmlHttpT.responseText.slice(2500));
		//alert(xmlHttpT.responseText);
		if (xmlHttpT.responseText != '')
		{
			postArray = xmlHttpT.responseText.split('~');	// Twitter uses '|' in feed.
			for (var i=0;i<postArray.length;i++)
			{					
				fieldNameValueArray = postArray[i].split('^');
				for (var j=0;j<fieldNameValueArray.length;j++)
				{
					fieldNameValuePair = fieldNameValueArray[j].split('=');
					fieldName = fieldNameValuePair[0];
					fieldValue = fieldNameValuePair[1];		
					switch(fieldName.toLowerCase())
					{
						case 'title':
							title = fieldValue;
							break;						
						case 'description':
							description = fieldValue;
							break;	
						case 'pubdate':
							pubDate = fieldValue;
							break;	
					}
				}		
				addPost(pubDate, title, description);				
			}
		}
	} 
}


function addPost(pubDate, title, description)
{	
	var tr;
	var td;
	var tab = document.getElementById('tabletwitter');
		
	tr = tab.insertRow(-1);		
	td = tr.insertCell(0);
	
	//Fri, 03 May 2013 21:15:24 +0000
	var dateArray = new Array();
	dateArray = pubDate.split(' ');
	var tempDay = dateArray[1];
	if (dateArray[1].substring(0,1)=='0') 
	{
		tempDay = dateArray[1].substring(1,2);
	}
	
	var formattedDate = dateArray[2] + ' ' + tempDay + ', ' + dateArray[3];
	var milTime = new Date( 'Jan 1, 1970 ' + dateArray[4]);   
	var formattedTime = (milTime.toLocaleString().split( '1970 '))[1];  

	td.innerHTML = '<span style="font-size:11px;">' + formattedDate + ', ' + formattedTime + '</span>' + '<br /><span style="font-size:11px; color:#2368a9; font-weight:800;">' + description + '</span>';	
	//td.style=.fontSize = '11px';
	//td.style.display = 'none';


	//td = tr.insertCell(0);
	//td.className = 'small';
	//td.innerHTML = '<br />' & title;
}


function highlightAlerts()
{
	var facilityList = 'ewr,jfk,lga,swf,path,bus,btt,port';
	var facilityArray = new Array();
	facilityArray = facilityList.split(',');
	var tabSuffixList = 'b,c,d,e,f,g,h,i';
	var tabSuffixArray = new Array();
	tabSuffixArray = tabSuffixList.split(',');
	
	for (i=0;i<tabSuffixArray.length;i++)
	{	
		showAlerts(facilityArray[i],tabSuffixArray[i],false);
	}
}

var tabClickedSuffix= '';
var tabClickedClassName = '';

function unselectTab(oldTabClickedSuffix,clickedTabSuffix,clickedClassName)
{
	if (oldTabClickedSuffix.length > 0)
	{
		document.getElementById('simpleTabs-nav-' + oldTabClickedSuffix).className = tabClickedClassName;	
		tabClickedSuffix = clickedTabSuffix;
		tabClickedClassName = clickedClassName;
	}
	else
	{
		oldTabClickedSuffix = clickedTabSuffix;
		document.getElementById('simpleTabs-nav-' + oldTabClickedSuffix).className = tabClickedClassName;	
		tabClickedSuffix = clickedTabSuffix;
		tabClickedClassName = clickedClassName;
	}
}


/* Cross-browser function that handles XML Http Request: */
function GetXmlHttpObject()
{
	var xmlHttp=null;
	try{
		// Firefox, Opera 8.0+, Safari
		xmlHttp=new XMLHttpRequest();
		}catch (e){
		//Internet Explorer
		try{
			xmlHttp=new ActiveXObject("Msxml2.XMLHTTP");
			}catch (e){
			xmlHttp=new ActiveXObject("Microsoft.XMLHTTP");
		}
	}
	return xmlHttp;
} 

