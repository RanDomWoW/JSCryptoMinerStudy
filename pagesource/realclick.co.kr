            }else if(type == 'ssp'){
                window.open('http://media.realssp.co.kr/');
            }else if(type == 'rs'){                
                                window.open('http://www.realclick.co.kr/rsense/');
                            }else if(type == 'rm'){                
                                window.open('http://www.realclick.co.kr/remonsdk/');
