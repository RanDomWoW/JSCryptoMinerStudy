<script type="text/javascript">
$(function() {
	$('#submit_btn').click(function() {
		if($.trim($('#name').val())=='') { alert('성명을 입력해주세요'); $('#name').focus(); return false;}
		if($.trim($('#email').val())=='') { alert('이메일을 입력해주세요'); $('#email').focus(); return false;}
		var re_mail = /^([\w\.-]+)@([a-z\d\.-]+)\.([a-z\.]{2,6})$/;
		if(re_mail.test($('#email').val()) != true){ alert('이메일 형식에 맞게 입력해주세요'); $('#email').focus(); return false;}
		if($.trim($('#phone').val())=='') { alert('연락처를 입력해주세요'); $('#phone').focus(); return false;}
		else{
					 var inText = $('#phone').val();
					 var ret;
				     for (var i=0; i<inText.length; i++){
				        ret = inText.charCodeAt(i);
				        if (ret != 45){
				            if ((ret<48) || (ret>57)){
				                alert("연락처 형식에 맞게 입력해주세요.");
				                $('#phone').val("");
				                $('#phone').focus();
				                return false
				            }    
				        }
				    }
				}
		if($.trim($('#title').val())=='') { alert('문의제목을 입력해주세요'); $('#title').focus(); return false;}
		if($.trim($('#inquiries').val())=='') { alert('문의분류를 선택해주세요'); $('#inquiries').focus(); return false;}
		if($.trim($('#message').val())=='') { alert('문의내용을 입력해주세요'); $('#message').focus(); return false;}            
		if($("input:checkbox[id='term_chk']").is(":checked") == false) { alert('개인정보 수집목적 및 이용목적에 동의 해주세요'); $('#term_chk').focus(); return false;}

		$("#inq_fm").attr({'action':'/contact_inquiry.php'});
		$("#inq_fm").submit();
	});
});
