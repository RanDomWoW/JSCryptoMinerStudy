<script type="text/javascript">
	
	var isLogin = false;
	

	var loginUrl = "http://www.godo.co.kr/member/login.gd"+"?returnURL="+encodeURIComponent(self.location.href);
	var openAllService = function(){
		var
				allServiceBg = $("#allServiceBg"),
				allService = $("#allService");

		$('#allService').show();

		allServiceBg.css({
			"position":"absolute",
			"top":allService.offset().top,
			"left":"0px",
			"width":"100%",
			"height":$( document ).height() - allService.offset().top,
			"background-color":"#000",
			"z-index":"100",
			"display":"none",
			"opacity":"0.3"
		});

		allServiceBg.show();
	};

	$(function(){

		
		$("#btn-gnb-login").click(function(e){
			e.preventDefault();
			self.location.href = loginUrl;
		});

		
		$("body").append("<div id='allServiceBg'></div>");

		$('#gnb-header a.link-all-service, a.link_all_service').click(function (e) {
			e.preventDefault();
			openAllService();
		});

		$('#allService button.close').click(function(e){
			e.preventDefault();
			$("#allServiceBg").hide();
			$("#allService").hide();
		});

		$('#allService div.left ul li:first-child').addClass('on');
		$('#allService div.right > div:first-child').show();
		$('#allService div.left ul li a').click(function() {
			var currentTab = $(this).attr('href');
			$('#allService div.right > div').hide();
			$(currentTab).show();
			$('#allService div.left ul li').removeClass('on');
			$(this).parent().addClass('on');
			return false;
		});

		
		$(".btn-business-consult-gnb").click(function(e){
			e.preventDefault();
			if($.godo) {
				$("#allServiceBg").hide();
				$("#allService").hide();
				$.godo.businessConsult();
			} else {
				top.location.href = "http://www.godo.co.kr/?act=consult";
			}
		});
		

		
		$('.main_gnb_menu li a').mouseenter(function(){
			$('.main_gnb_menu li a').removeClass("on");
			$(this).addClass('on');
		});
		$('.main_header').mouseleave(function(){
			$('.main_gnb_menu li a').removeClass('on');
			$('.all_menu_wrap').removeClass('on');
		});

		$('.main_top_menu, .main_header_top h1').mouseenter(function(){
			$('.main_gnb_menu li a').removeClass("on");
			$('.all_menu_wrap').removeClass('on');
		});
		

		
		
		$('.sub_dep02 li a').mouseenter(function(){
			
			
			var _data = $(this).data('key');
			if (_data!="edu_fast") {
				$('.sub_dep02').addClass('on');
				$('.sub_dep02 li a').removeClass('on');
				$(this).addClass('on');
				
				$('.sub_dep03').removeClass('on');
				$('.all_menu_wrap').removeClass('on');
				$('.sub_dep03.'+_data).addClass('on');
			}
		});

		$('#sub_header').mouseleave(function(){
			$('.sub_dep02').removeClass('on');
			$('.sub_dep02 li a').removeClass('on');
			$('.sub_dep03').removeClass('on');
			$('.all_menu_wrap').removeClass('on');
		});

		
		$('.all_menu').mouseenter(function(){
			var _this = $(this).closest('#sub_header').attr('class');
			$('.all_menu_wrap.'+_this).addClass('on');
		});
		$('.all_menu_wrap').mouseleave(function(){
			$('.sub_dep02').removeClass('on');
			$('.sub_dep02 li a').removeClass('on');

			var _this = $(this).closest('#sub_header').attr('class');
			$('.all_menu_wrap.'+_this).removeClass('on');
		});

		
		$('.main_gnb_menu a').mouseenter(function(){
			var _this = $(this).attr('class');
			_this = _this.substring(0,_this.length-3);
			$('.all_menu_wrap').removeClass('on');
			$('.all_menu_wrap.'+_this).addClass('on');
		});
		$('.all_menu_wrap').mouseleave(function(){
			$('.sub_dep02').removeClass('on');
			$('.sub_dep02 li a').removeClass('on');

			var _this = $(this).closest('#sub_header').attr('class');
			$('.all_menu_wrap.'+_this).removeClass('on');
		});

		
				
		

	});

</script>


	
