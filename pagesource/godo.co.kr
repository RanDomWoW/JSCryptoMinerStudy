	<script type="text/javascript">
	jQuery.godo = {};
	jQuery.godo.isLogin = false;
	jQuery.godo.crossScript = function(scriptCode) {
		try {
			eval(scriptCode);
		} catch(err) {
			var $form = $("<form></form>"),
					$input = $("<input type='hidden' name='code'>"),
					actionUrl;
			if(document.location.protocol === 'http:') {
				actionUrl = 'https://';
			} else {
				actionUrl = 'http://';
			}
			actionUrl = actionUrl + "www.godo.co.kr/etc/js-exec.gd";

			$form.attr("action",actionUrl);
			$form.attr("method","post");
			$form.appendTo("body");

			$input.val(scriptCode);

			$form.append($input);
			$form.submit();
		}
	}

	if (!String.prototype.trim) {
		String.prototype.trim=function(){return this.replace(/^\s+|\s+$/g, '');};
	}

	window.crossDomain = {
		originalDomain : document.domain,
		commonDomain : "godo.co.kr",
		common : function() {
			document.domain = window.crossDomain.commonDomain;

		},
		restore : function() {
			document.domain = window.crossDomain.originalDomain;
		}
	};

	function is_touch_device() {
		return (('ontouchstart' in window)
		|| (navigator.MaxTouchPoints > 0)
		|| (navigator.msMaxTouchPoints > 0));
	}

	document.domain = "godo.co.kr";

	// 로그인 플러그인 등록
	jQuery.godo.loginLayer = function(callback,returnUrl,domain) {

		domain = typeof domain !== 'undefined' ? domain : 'godo';

		$("body").css("overflow","hidden");
		$("body").append("<div id='popupLoginBackground'></div>");
		backgroundLayer = $("#popupLoginBackground");
		backgroundLayer.css({
			"position":"fixed",
			"top":"0px",
			"left":"0px",
			"width":"100%",
			"height":"100%",
			"background-color":"#000",
			"z-index":"5002",
			"opacity":"0.3"
		});


		$("body").append("<iframe id='popframeMemberLogin' frameborder='0'></iframe>");
		$("#popframeMemberLogin").css({
			"position":"fixed",
			"top":"50%",
			"left":"50%",
			"width":"538px",
			"height":"314px",
			"margin-left":"-269px",
			"margin-top":"-157px",
			"display":"block",
			"z-index":"5003",
			"border":"0px"
		});

		if (domain == 'godo') {
			$("#popframeMemberLogin").attr("src","https://www.godo.co.kr/member/login-layer.gd?returnUrl="+encodeURIComponent(returnUrl));
		} else {
			$("#popframeMemberLogin").attr("src","https://"+domain+"/member/login-layer.gd?returnUrl="+encodeURIComponent(returnUrl));
		}

		window.popCloseLogin = function(){
			$("body").css("overflow","auto");
			$("#popupLoginBackground").remove();
			$("#popframeMemberLogin").remove();
		};

		window.loginDone = function(){
			$.godo.isLogin = true;
			$("body").css("overflow","auto");
			$("#popupLoginBackground").remove();
			$("#popframeMemberLogin").remove();
			callback();
		};
	}

	// 1:1문의하기 플러그인 등록
	jQuery.godo.memberInquiry = function(refcd,catcd) {
		$("body").css("overflow","hidden");
		$("body").append("<div id='backgroundBusinessConsult'></div>");
		$("#backgroundBusinessConsult").css({
			"position":"fixed",
			"top":"0px",
			"left":"0px",
			"width":"100%",
			"height":"100%",
			"background-color":"#000",
			"z-index":"5000",
			"opacity":"0.3"
		});

		$("body").append("<iframe id='popframeMemberInquiry'></iframe>");
		$("#popframeMemberInquiry").css({
			"position":"fixed",
			"top":"30%",
			"left":"50%",
			"width":"802px",
			"height":"1000px",
			"margin-left":"-401px",
			"margin-top":"-202px",
			"display":"block",
			"z-index":"5001",
			"border":"0px"
		});

		$("#popframeMemberInquiry").attr("src","https://www.godo.co.kr/mygodo/inquiry-layer.gd?refcd="+refcd+"&catcd="+catcd);

		window.popClose = function(){
			$("body").css("overflow","auto");
			$("#backgroundBusinessConsult").remove();
			$("#popframeMemberInquiry").remove();
		};
	}

	// 창업상담
	jQuery.godo.businessConsult = function() {
		$("body").css("overflow","hidden");
		$("body").append("<div id='backgroundBusinessConsult'></div>");
		$("#backgroundBusinessConsult").css({
			"position":"fixed",
			"top":"0px",
			"left":"0px",
			"width":"100%",
			"height":"100%",
			"background-color":"#000",
			"z-index":"5000",
			"opacity":"0.3"
		});

		$("body").append("<iframe id='popframeBusinessConsult' name='popframeBusinessConsult'></iframe>");
		$("#popframeBusinessConsult").css({
			"position":"fixed",
			"top":"30%",
			"left":"50%",
			"width":"802px",
			"height":"1000px",
			"margin-left":"-401px",
			"margin-top":"-302px",
			"display":"block",
			"z-index":"5001",
			"border":"0px"
		});

		$("#popframeBusinessConsult").attr("src","https://www.godo.co.kr/customer/business-consult.gd");
		window.popClose = function(){
			$("body").css("overflow","auto");
			$("#backgroundBusinessConsult").remove();
			$("#popframeBusinessConsult").remove();
		};

		window.changeInquiry = function(){
			$("body").css("overflow","auto");
			$("#backgroundBusinessConsult").remove();
			$("#popframeBusinessConsult").remove();

			if($.godo.isLogin) {
				$.godo.memberInquiry();
			} else {
				$.godo.loginLayer(function(){
					$.godo.memberInquiry();
				},self.location.href);
			}
		};
	}

	// 글로벌 서비스 통합안내 레이어
	jQuery.godo.globalService = function() {
		$("body").css("overflow","hidden");
		$("body").append("<div id='backgroundGlobalService'></div>");
		$("#backgroundGlobalService").css({
			"position":"fixed",
			"top":"0px",
			"left":"0px",
			"width":"100%",
			"height":"100%",
			"background-color":"#000",
			"z-index":"5000",
			"opacity":"0.3"
		});

		$("body").append("<iframe id='popframeGlobalService'></iframe>");
		$("#popframeGlobalService").css({
			"position":"fixed",
			"top":"50%",
			"left":"50%",
			"width":"1162px",
			"height":"645px",
			"margin-left":"-627px",
			"margin-top":"-332px",
			"display":"block",
			"z-index":"5001",
			"border":"0px"
		});

		$("#popframeGlobalService").attr("src","https://www.godo.co.kr/echost/global/global-service-layer.gd");

		window.popClose = function(){
			$("body").css("overflow","auto");
			$("#backgroundGlobalService").remove();
			$("#popframeGlobalService").remove();
		};
	}


	$(function(){

		$('div.layer-login button.close').click(function () {
			$("#popupLoginBackground").remove();
			$(".layer-login").hide();
			$("body").css("overflow","auto");
			$("div.layer-login input.label-id, div.layer-login input.label-id").val('');
			$("div.layer-login input.label-id, div.layer-login input.label-pw").val('');
			$("div.layer-login input.label-id, div.layer-login input.label-pw").addClass('text');
		});

		$("div.layer-login input.label-id, div.layer-login input.label-pw").focus(function() {
			$(this).removeClass('text');
		});
		$("div.layer-login input.label-id, div.layer-login input.label-pw").blur(function() {
			if ($(this).val() === "") {
				$(this).addClass('text');
			}
			else {
				$(this).removeClass('text');
			}
		});
		$("div.layer-login input.label-id, div.layer-login input.label-pw").blur();
		$("a.btn-login, .btn-login").click(function(e){
			var btnUrl = $(this).attr("href");
			if($.godo.isLogin === false) {
				e.preventDefault();
				$.godo.loginLayer(function(){
					self.location.href = btnUrl;
				},btnUrl);
			}
		});

		$(".btn-business-consult").click(function(e){
			e.preventDefault();
			$.godo.businessConsult();
		});

		$(".btn-member-inquiry").click(function(e){
			var refcd = $(this).data("topno"),
					catcd = $(this).data("subno");
			e.preventDefault();

			if($.godo.isLogin) {
				$.godo.memberInquiry(refcd,catcd);
			} else {
				$.godo.loginLayer(function(){
					$.godo.memberInquiry(refcd,catcd);
				},self.location.href);
			}
		});

		$(".btn-global-service").click(function(e){
			e.preventDefault();
			$.godo.globalService();
		});

		$("input,textarea").each(function(){
			var $this = $(this),
					titleClass = $this.data("godo-titleclass"),
					v = $this.val().trim();

			$this.val(v);
			if(titleClass) {
				$this.on("focus",function(e){
					$this.removeClass(titleClass);
				}).on("blur",function(e){
					var v = $this.val().trim();
					$this.val(v);
					if(v === "") {
						$this.addClass(titleClass);
					}
				});

				if(v === "") {
					$this.addClass(titleClass);
				} else {
					$this.removeClass(titleClass);
				}
			}
		});

		
		$('.btn-go-top').click(function() {
			$('html, body').animate({scrollTop: '0px'}, 300);
			return false;
		});

	});

	$.cookie.defaults.domain = 'godo.co.kr';
	$.cookie.defaults.path = '/';
</script>

	
