	<script type="text/javascript" src="//static.godo.co.kr/extern/jquery-ui-1.10.4/jquery-ui-1.10.4.min.js"></script>
	<script type="text/javascript" src="//static.godo.co.kr/extern/jquery-cycle2/jquery.cycle2.min.js"></script>
	<script type="text/javascript" src="//static.godo.co.kr/extern/jquery-cycle2/jquery.cycle2.carousel.min.js"></script>
	<script type="text/javascript" src="//static.godo.co.kr/extern/zeroclipboard/ZeroClipboard.min.js"></script>
	<script type="text/javascript" src="//static.godo.co.kr/extern/placeholders/Placeholders.js"></script>
	<script type="text/javascript">
        'use strict';
        +(function($) {

            var ajaxState = false;

            $(function() {

                
				var cookieData = document.cookie;

                if ( cookieData.indexOf("ncookie_free_promotion=done") < 0 ){
                    $('#free_promotion').css('display', 'block');
                } else {
                    $('#free_promotion').css('display', 'none');
                }

                
                $("#godoLogin iframe").attr("src","https://www.godo.co.kr/member/login-iframe.gd");

                
                $('.login-box .godo-login').on('click', function(e) {
                    e.preventDefault();
                    $("#mallAdminLogin").hide();
                    $(".login-box .mall-admin-login").removeClass("on");

                    $(this).addClass("on");
                    $("#godoLogin").show();
                });

                $('.login-box .mall-admin-login').on('click', function(e) {
                    e.preventDefault();
                    $("#godoLogin").hide();
                    $(".login-box .godo-login").removeClass("on");

                    $(this).addClass("on");
                    $("#mallAdminLogin").show();

                });

                $(".login-box .mall-admin-login").one('click',function() {
                    if($.godo.isLogin) {
                        $("#mallAdminLogin iframe").attr("src","https://www.godo.co.kr/member/login-iframe-mall.gd");
                    } else {
                        $("#mallAdminLogin iframe").attr("src","https://www.godo.co.kr/member/login-iframe-mall.gd");
                    }
                });

                
                
                
                ZeroClipboard.config({moviePath: "//static.godo.co.kr/extern/zeroclipboard/ZeroClipboard.swf","debug":false});
                var clip = ZeroClipboard($(".btn-nscopy"));
                clip.on("load", function (client) {
                    client.on("complete", function (client, args) {
                        alert("복사되었습니다.");
                    });
                });

                
                $('.main-goodshop #brand').cycle(); // default 키워드>유명브랜드
                $('.main-goodshop > .box > div').hide();
                $('.goodshop-menu li > div:first-child').addClass('on');

                
                var cateTab = $('.goodshop-type-tab a');

                $(cateTab).each(function() {
                    var currentTarget = $(this).attr('href');
                    var groupCate = $(this).data('group');
                    var category = currentTarget.split('#');
					if($(this).hasClass('active')) {
                        $('.main-goodshop > .box > div').hide();
                        $(currentTarget).show();
                    }

                    $(this).on('click', function(e) {
                        $('.btn_payco_type').removeClass('on');
                        $('.main-goodshop').removeClass('bg_payco');

                        $('.goodshop-'+groupCate).find('li').find('a').removeClass('on');
                        $('.goodshop-'+groupCate).find('li').eq(0).find('a').addClass('on');
                        $('.goodshop-type-tab a').removeClass('active');
                        $(this).addClass('active');

                        if($(this).parent().index() == 0) {
                            $('.goodshop-keyword').css({'display':'block'});
                            $('.goodshop-business').css({'display':'none'});
                        } else {
                            $('.goodshop-keyword').css({'display':'none'});
                            $('.goodshop-business').css({'display':'block'});
                        }
                        if(category[1] == 'fashion' || category[1] == 'brand') {
                            $(currentTarget).remove();
                        }
						ajaxProcess(groupCate, category[1], currentTarget);

                    });
                });

				
                var gsTab = $('.goodshop-menu .tab a');

                $(gsTab).each(function() {
                    var currentTarget = $(this).attr('href');
                    if($(this).hasClass('on')) {
                        $('.main-goodshop > .box > div').hide();
                        $(currentTarget).show();
                    }

                    $(this).on('click', function(e) {
                        e.preventDefault();
                        $('.btn_payco_type').removeClass('on');
                        $('.main-goodshop').removeClass('bg_payco');
                        $('.goodshop-contents').hide();
                        $(currentTarget).show();
                        $('.goodshop-menu .tab a').removeClass('on');
                        $(this).addClass('on');
                    });

                    $(this).one("click", function() {
                        var groupCate = $(this).closest('ul').data('group');
                        var category = currentTarget.split('#');
                        if (ajaxState == true || category[1] == 'brand') {
                            return false;
                        }
                        ajaxState = true;
                        if(category[1] == 'fashion' || category[1] == 'brand') {
                            $(currentTarget).remove();
                        }

                        ajaxProcess(groupCate, category[1], currentTarget);
                    });
                });

                // 페이코 모아보기 클릭
                $('.btn_payco_type').on('click', function () {
                    $('.main-goodshop').addClass('bg_payco');
                    var currentTarget = $(this).attr('href');
                    $(this).addClass('on');
                    $('.goodshop-menu .tab a').removeClass('on');
                    ajaxProcess('payco','payco',currentTarget);
                });

                // 굿샵 ajax 통신 함수
               function ajaxProcess(group, cate, href) {
                   $.post("./etc/goodshop-process.gd", {
                       mode:'tabChange', group:group, cate:cate, pageCount:'12'
                   }, function(data){
                       ajaxState = false;
                       $('.goodshop-contents').hide();
                       $('.main-goodshop .box').append(data);
                       $('.main-goodshop '+href).cycle();
                   });
               }

                
                var
                    renderGlobalService = function renderGlobalService() {
                        $('.global-icons a').each(function(i) {
                            $(this).find('img').attr('src',$(this).find('img').attr('src').replace('_on.png', '.png'));
                            if( $(this).hasClass('active') ) {
                                $(this).find('img').attr('src',$(this).find('img').attr('src').replace('.png', '_on.png'));
                            }
                        });
                    },
                    globalServiceEvent = function globalServiceEvent() {
                        $('.global-icons a').removeClass('active').eq($(this).index()).addClass('active');
                        renderGlobalService();
                    },
                    resetGlobalService = function resetGlobalService() {
                        $('.global-icons a').each(function(i) {
                            $(this).addClass('active');
                            $(this).find('img').attr('src',$(this).find('img').attr('src').replace('_on.png', '.png'));
                            if( $(this).hasClass('active') ) {
                                $(this).find('img').attr('src',$(this).find('img').attr('src').replace('.png', '_on.png'));
                            }
                        });
                    };

                
                var btnHover = function btnHover(e) {
                    var selector = $(this);
                    selector.find('img').attr('src',$(this).find('img').attr('src').replace('_on.png', '.png') );
                    if(e.type == 'mouseenter') {
                        selector.find('img').attr('src',$(this).find('img').attr('src').replace('.png', '_on.png') );
                    }
                };

                
                $('#ge_week_box').on('click', function () {
                    var idWeek = $("#ge_week_box input[name=week_close]").val();
                    if(idWeek == 'y') {
                        $('.chk_week').attr('src',$('.chk_week').attr('src').replace('_on.png', '_off.png') );
                        $("#ge_week_box input[name=week_close]").val('n');
                    } else {
                        $('.chk_week').attr('src',$('.chk_week').attr('src').replace('_off.png', '_on.png') );
                        $("#ge_week_box input[name=week_close]").val('y');
                    }
                    closeLayer('free_promotion');
                });

                $('.service-contents li, .btn-circle.yes, .btn-circle.no').on('mouseenter', btnHover);
                $('.service-contents li, .btn-circle.yes, .btn-circle.no').on('mouseleave', btnHover);

                $(document).on('mouseover', '.global-icons a', globalServiceEvent);
                $('.global-icons').on('mouseleave', resetGlobalService);

                // 대표 쇼핑몰 클릭 시 굿샵 영역으로 이동합니다.
                $('.gs-move').on('click', function () {
                    var gsOffset = $('.main-goodshop').offset();
                    $('html, body').animate({scrollTop : gsOffset.top}, 400);
                });

                // 배너닫기
                $('.main_banner_close').on('click', function () {
                    $('.main_banner').animate({ 'height' : 0 }, 200,function(){
                        $('.main_banner img').hide();
                    });
                });

				function swing() {
					$('.btn_start_wrap .ico_arrow_right').animate({'left':'131px'},600).animate({'left':'121px'},600, swing);
				}
				swing();

				// vip 레이어 팝업 이벤트 체크박스(닫기) 
				$('#ge_ly_close').on('click', function () {
					var idWeek = $("#ge_ly_close input[name=ly_close]").val();
					if(idWeek == 'y') {
						$('.chk_close').attr('src',$('.chk_close').attr('src').replace('_on.png', '_off.png') );
						$("#ge_ly_close input[name=ly_close]").val('n');
					} else {
						$('.chk_close').attr('src',$('.chk_close').attr('src').replace('_off.png', '_on.png') );
						$("#ge_ly_close input[name=ly_close]").val('y');
					}
					$('.layer_vip_popup').hide();
				});

            });



        })(jQuery);



        // 쿠키 생성 스크립트
        function setCookie( name, value, expiredays )
        {
            var todayDate = new Date();
            todayDate.setDate( todayDate.getDate() + expiredays );
            document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";"
        }

        function closeLayer(name)
        {
            setCookie( "ncookie_"+name, "done" , 7 );
            document.getElementById(name).style.display = "none";
        }
		


	</script>

