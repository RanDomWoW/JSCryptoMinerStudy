<script>
setInterval(get_stoc, 60000);
function get_stoc(){
    jQuery.ajax({
      url : "?q=stock_exchange",
      type : 'post',
      dataType: 'json',
      success : function(res){
        if(res.increment == 'increment'){
          jQuery('.stock').removeClass('decrement');
          jQuery('.stock').addClass(res.increment);
        }else{
          jQuery('.stock').removeClass('increment');
          jQuery('.stock').addClass(res.increment);
        }        
        jQuery('.value').html(res.open);
        jQuery('.stock').html(res.percent);
      }
    });
}

