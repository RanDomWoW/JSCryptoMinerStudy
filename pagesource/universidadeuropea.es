    // Set YouTube videos at full width

    (function() {

      var youtubeVideos = $(".youtube.async[data-youtube-id]");

      if (youtubeVideos.length) {
        var tag = document.createElement('script');
        tag.src = "https://www.youtube.com/iframe_api";
        var firstScriptTag = document.getElementsByTagName('script')[0];
        firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);



        window.onYouTubeIframeAPIReady = function() {
          youtubeVideos.each(function() {
            var $this = $(this);
            var player = new YT.Player($this.attr("id"), {
              videoId: $this.data("youtube-id"),
              width: 1500,
              playerVars: {
                volume: 0,
                autoplay: 1,
                controls: 0,
                modestbranding: 1,
                rel: 0,
                showinfo: 0,
                playlist: $this.data("youtube-id"),
                loop: 1
              }, events: {
                onReady: function(e) {
                  e.target.mute()
                }
              }
            });
          });
        }
      }

      $(function() {
        $("video.async[data-src]").each(function() {
          var $source = $("<source>");
          var $video  = $(this);
          $source.attr("src", $video.data("src"));
          $source.attr("type", $video.data("type"));
          $video.append($source);
          this.play();
        });
      })
      var slides = $('#slides');
      var videos = $('.video-frame');

      function resize_video() {
        var container_width = slides.width();
        var video_height = container_width / (16 / 9);

        videos.height(video_height);
      }

      $(window).on('resize', resize_video);

      resize_video();
    })();
  </script>

