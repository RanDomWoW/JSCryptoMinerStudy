	</div>	<script type='text/javascript'>
		$(document).ready(function(){
			var last_update = 1521311223;
			var timeout;
			
			var ajax_start = function(){
				timeout = setTimeout(function(){
					ajax_GetNews();
				}, 5000);
			}
			
			var ajax_GetNews = function(){
				clearTimeout(timeout);
				$.post("http://interfax.az/ajax/index", {last_update: last_update}, function(data){
					var arr = eval(data);
					arr = arr[0];
					last_update = arr['last_update'];
					for(var i in arr['news']){
						ajax_AddNews(arr['news'][i]['type'], arr['news'][i]['title'], arr['news'][i]['id']);
						last_update = arr['news'][i]['date_time'];
					}
					ajax_start();
				});
			}
			
			var ajax_AddNews = function(type, title, id){
				var obj = $("#ajax_last_news");
				var n = $("<p><a></a></p>");
				n.addClass("new3");
				n.find("a").text(title);
				n.find("a").attr("href", "http://interfax.az/view/"+id);
				if(type == 1){
					n.addClass("blocked");
					n.prepend("<img src='http://interfax.az/img/blocked.png' class='blocked_img' />");
				} else if(type == 2) {
					n.addClass("open");
					n.prepend("<img src='http://interfax.az/img/blocked.png' class='open_img' />");
				}
				n.prependTo(obj);
				obj.find("p").last().remove();
			}
			
			ajax_start();
		});	
	</script>
