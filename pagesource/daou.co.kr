<script>
  var mvCookie = getCookie('mvBanner');
  
  if(mvCookie == "true"){
    document.getElementById("banner_mv").style.display = "none";
  }  
  
  var height = $(window).height() - 107;
  if(height < 697) height = 697;
  $('.content_wrap').css('height', height+'px');

  var rnd = Math.floor(Math.random() * 5);
  $showObject = $("#Vimg_"+rnd);
  $showObject.show();

  $(document).ready(function() {
    
    function _showMenu($obj) {
      $(".black", $obj).fadeTo(6000, 0);  // 서서히 나타나기
      $(".black", $obj).fadeTo(6000, 0.2);  // 서서히 나타나기
      setTimeout(function() { _showMenu($showObject) }, 5000);
    }
    _showMenu($showObject);
  });

  function change_service(id){
    $(".mkt_comm").attr("class", "mkt_comm");
    $(".comm").attr("class", "comm");
    $(".bizinfra").attr("class", "bizinfra");
    $(".entersolution").attr("class", "entersolution");
    $(".ito").attr("class", "ito");
    $("." + id).attr('class', id + " on");

    $("#mkt_comm").hide();
    $("#comm").hide();
    $("#bizinfra").hide();
    $("#entersolution").hide();
    $("#ito").hide();
    $("#" + id).show();
  }

  //marketing
  var mkt = [0, 1, 2, 3];
  var af_mkt = [];
  for(var i = 0; i < 3; i++){
    mrnd = Math.floor(Math.random() * (mkt.length));
    $("#mkt_" + mkt[mrnd]).css("display", "");
    af_mkt.push(mkt[mrnd]);
    mkt.splice(mrnd, 1);
  }
  af_mkt.sort();
  $("#mkt_" + af_mkt[2]).addClass("last");

  //commerce
  var comm = [0, 1, 2, 3, 4];
  var af_comm = []
  for(var i = 0; i < 3; i++){
    crnd = Math.floor(Math.random() * (comm.length));
    $("#comm_" + comm[crnd]).css("display", "");
    af_comm.push(comm[crnd]);
    comm.splice(crnd, 1);
  }
  af_comm.sort();
  $("#comm_" + af_comm[2]).addClass("last");

  //Biz Infra
  var biz = [0, 1, 2, 3];
  var af_biz = [];
  for(var i = 0; i < 3; i++){
    brnd = Math.floor(Math.random() * (biz.length));
    $("#biz_" + biz[brnd]).css("display", "");
    af_biz.push(biz[brnd]);
    biz.splice(brnd, 1);
  }

  af_biz.sort();
  $("#biz_" + af_biz[2]).addClass("last");


  //Enterprise Solution
  var ent = [0, 3, 7, 8, 9];
  var af_ent = [];
  for(var i = 0; i < 3; i++){
    ernd = Math.floor(Math.random() * (ent.length));
    $("#ent_" + ent[ernd]).css("display", "");
    af_ent.push(ent[ernd]);
    ent.splice(ernd, 1);
  }
  
  function sortNumber(a,b) {
    return a - b;
  
  }
  af_ent.sort(sortNumber);
  $("#ent_" + af_ent[2]).addClass("last");

  var _isShowScrollTrace = true;
  var page_num = 1;
  
  $(function(){
    // scroll
    $(window).scroll(scrollWindowHandler);

    // keydown
    $(document).keydown(function(e) {
      // PageUp || ArrowUp
      if (e.keyCode == 33 || e.keyCode == 38) {
        e.preventDefault();
      }else if (e.keyCode == 34 || e.keyCode == 40) {
        if(page_num < 5){
          e.preventDefault();
        }
      }
    });

    // mouse scroll
    $(window).bind('mousewheel DOMMouseScroll', function(e) {
      if ($('html,body').is(':animated') ) {
        return false;
      }
      
      if(!e.originalEvent){
        return false;
      }

      if ((e.originalEvent.wheelDelta && e.originalEvent.wheelDelta > 0) || (e.originalEvent.detail && e.originalEvent.detail < 0)) {
        _isShowScrollTrace = false;
        e.preventDefault();
        gotoPreviousCategory();
      } else {
        if(page_num < 5){
          _isShowScrollTrace = false;
          e.preventDefault();
          gotoNextCategory();
        }
      }
    });
  });

  var scrollWindowHandler = function(e) {    
    if(_isShowScrollTrace == true){
      if (!$('html,body').is(':animated')) {
        var offsetY = $('.wrap').offset().top;
        
        var y = $(window).scrollTop() + offsetY + (($(window).height() - offsetY) / 2) ;
        var res = null;
        $('.content_wrap').each(function() {
          if (y >= $(this).offset().top) {
            var id = $(this).attr('id');
            res = id.split("_");
          }else{
            return false;
          }
        });
        
        if(res != null){
          page_num = res[1]*1;
          $('.nav_btn').removeClass('active');
          $('#nav_'+res[1]).addClass('active');
        }
      }
    }else{
      _isShowScrollTrace = true;
      e.preventDefault();
      return false;
    }
  };

  function gotoCategory(category) {
    scrollToCategory(category);
  }

  function scrollToCategory(category) {
    var height = $(window).height() - 107;
    if(height < 697) height = 697;
    $('.content_wrap').css('height', height+'px');
 
    page_num = category*1;

    var offsetY = $('.wrap').offset().top;
    var toTop = $('#page_' + category).offset().top - offsetY;

    //_isShowScrollTrace = false;

    $('html,body').stop().animate({scrollTop: toTop}, 600, function() {
      //_isShowScrollTrace = true;
    });

    $('.nav_btn').removeClass('active');
    $('#nav_'+category).addClass('active');
  }

  
  function gotoNextCategory() {
    page_num = (page_num*1) + 1;
    if(page_num < 6){
      gotoCategory(page_num);
    }
  }

  function gotoPreviousCategory() {
    page_num = (page_num*1) - 1;
    if(page_num == 0) page_num = 1;
    gotoCategory(page_num);
  }

  gotoCategory(page_num);

</script>
