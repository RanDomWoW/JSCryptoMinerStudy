				url: 'http://www.zorpidis.gr/popup.php?page='+page+'&amp;the_id='+the_id+'&amp;id=0&amp;category_id=0',
				success: function(response) 
				{	
					$('.popup').html(response);

					height_popup = $('.popup').height();

					var popup_top;
					if(winHeight>=height_popup)
					{
						popup_top=(winHeight - height_popup)/2;
					}
					else
					{
						popup_top=100;
					}
					
					var popup_top2=$(window).scrollTop()+popup_top;
			
					$('.popup').css('display','block');
					
					$('.popup_out').fadeIn(function()
					{
						$('.popup').animate(
						{ 
							left: popup_left,
							top: popup_top2,
							opacity:1
						}, 
						{
							duration: 'slow',
							easing: 'easeOutBack'
						},3000);	
						
					});
					
					$('.popup_photos_top_in').carouFredSel(
