	var is_the_mouse_hover;
	function slider_show(direction)
	{
		$('#slider_container').stop(true, true);
		
		if(is_the_mouse_hover==false || direction!='auto')
		{
			slider_margin_left=parseInt($('#slider_container').css('margin-left'));
			slider_previous=$('.slider_item[id="slider_prev"]');
			slider_main=$('.slider_item[id="slider_main"]');
			slider_next=$('.slider_item[id="slider_next"]');
			
			if(direction=='slider_left')
			{
				
				$('#slider_container').prepend($('.slider_item:last'));
				$('#slider_container').css('margin-left', slider_margin_left-1170);
				$('#slider_container').animate({marginLeft: slider_margin_left}, {duration: 1000, easing: 'easeOutBack'},800);
				
				$(slider_next).attr('id','');
				$(slider_main).find('.slider_bg_black').fadeIn(1000);
				$(slider_main).attr('id','slider_next');
				$(slider_previous).find('.slider_bg_black').fadeOut(1000);
				$(slider_previous).attr('id','slider_main');
				$(slider_previous).prev().attr('id','slider_prev');		
			}
			else
			{
				$('#slider_container').animate({marginLeft: slider_margin_left-1170}, 1000, 'easeOutBack', function() 
				{
					$('#slider_container').append($(slider_previous));
					$('#slider_container').css('margin-left', slider_margin_left);
				});
				
				$(slider_previous).attr('id','');
			 	$(slider_main).find('.slider_bg_black').fadeIn(1000);
			 	$(slider_main).attr('id','slider_prev');
			 	$(slider_next).find('.slider_bg_black').fadeOut(1000);
			 	$(slider_next).attr('id','slider_main');
			 	$(slider_next).next().attr('id','slider_next');
			}
		}
		
		if(direction=='auto')
		{
			window.setTimeout('slider_show("auto")',8000);
		}
	}
	
