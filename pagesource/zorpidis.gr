		$('.select_box_popup').click(function()
		{
			var this_popup=$(this).parent().find('.popup_filter');
			
			var popup_filter_width = this_popup.width();
		
			var popup_left_filter = (winWidth-popup_filter_width)/2;
			
			$.ajax(
			{
				type:"GET",
				cache: false,
				url: 'http://www.zorpidis.gr/popup_filter.php?id=0',
				success: function(response) 
				{	
					this_popup.find('.popup_filter_in').html(response);

					height_popup_filter = this_popup.height();

					var popup_top_filter;
					
					if(winHeight>=height_popup_filter)
					{
						popup_top_filter=(winHeight - height_popup_filter)/2;
					}
					else
					{
						popup_top_filter=100;
						
					}
					var popup_top_filter2=$(window).scrollTop()+popup_top_filter;
			
					this_popup.css('display','block');
					
					$('.popup_out').fadeIn(function()
					{
					
						this_popup.animate(
						{ 
							left: popup_left_filter,
							top: popup_top_filter2,
							opacity:1
						}, 
						{
							duration: 'slow',
							easing: 'easeOutBack'
						},3000);	
						
					});
				}
			});
			
			return false;
		});
		
