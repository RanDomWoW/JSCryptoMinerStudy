				left: 0 ,
				top: 0 ,
				opacity:0
			}, 
			{
				duration: 'slow',
				easing: 'easeOutBack'
			},3000);
				   
			$('.popup_out').fadeOut(function()
			{
				$('.popup_filter').css('display','none');
			});
		});
		
		
		
		$('#filter_submit_d').click(function()
		{	
			req='';
			
			$("#tab_book_top input:radio").each(function() 
			{
				if($(this).is(':checked'))
				{
					req+=$(this).attr('name')+'='+urlencode($(this).val())+'&';
				}
			});
			
			$("#tab_book_top input:text").each(function() 
			{
				req+=$(this).attr('name')+'='+urlencode($(this).val())+'&';
			});
			
			$("#tab_book_top input:hidden").each(function() 
			{
				req+=$(this).attr('name')+'='+urlencode($(this).val())+'&';
			});
			
			$("#tab_book_top textarea").each(function() 
			{
				req+=$(this).attr('name')+'='+urlencode($(this).val())+'&';
			});
			
			$("#tab_book_top select").each(function() 
			{
				req+=$(this).attr('name')+'='+urlencode($(this).val())+'&';
			});
			
			$('#trip_tabs_area_bottom_in').height($(".tab_item[the_id=5]").height());
			
						
			$.ajax(
			{
			
				url: 'http://www.zorpidis.gr/included_pages/departure_table.php?id=0&amp;'+req,
				success: function(response) 
				{
					$('#tab_book_bottom').html(response);
					
					//alert($('.sub_tab_item').length);
					if($('.sub_tab_item').length>0)
					{
						var new_height=$(".sub_tab_item[the_id=a]").height();
					}
					else
					{
						$('#tab_book_bottom').css('height','auto');
						var new_height=$('#tab_book_bottom').height();
					}
					
					$('#tab_book_bottom').animate(
					{
						height: new_height
					},
					{
						complete: function()
						{
							$(".sub_tab_item[the_id=a]").fadeIn(500);
							
							$('.none_padding').each(function() 
							{
								var the_height=$(this).parent().find('.orange_col').height();
								
								var length_tr = $(this).find('table tr').length;
								
								new_height=the_height/length_tr;
								
								//alert(the_height+' '+length_tr+' '+new_height);
								$(this).find('table').height(the_height+10);
							});
							
							$('.sub_tab_item').css('display','none');
							$('.sub_tab').removeClass('sub_tab_active');
							
							$('.sub_tab_item:first').css('display','block');
							$('.sub_tab:first').addClass('sub_tab_active');
							
							$('#tab_book_bottom').height($(".sub_tab_item[the_id=a]").height());
							$('#trip_tabs_area_bottom_in').height($(".tab_item[the_id=5]").height());
							
						}
					},500);

				},timeout: 9000
	
			});
			
			//return false;
		});
		
