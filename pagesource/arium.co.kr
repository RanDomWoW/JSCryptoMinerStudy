			<script>
				var length_all = 10;
				var length_nc = 8;
				var length_ta = 6;
				var length_cp = 4;
				var length_st = 6;
				var length_ev = 5;
				var length_sq = 3;
				var length_pr = 5;
				var length_sb = 4;
				$(document).ready(function() {
					$(window).resize(function() {
						$("#div_all_menu").css("left", (parseInt($(window).width(), 10)-922)/2);
					});
					$("#btn_head_my").mouseover(function() {
						$("#div_head_my").show();
						setTimeout(function() {
							if($("#div_head_my:hover").length == 0) {
								$("#div_head_my").hide();
							}
						}, 300);
					});
					$("#div_head_my").mouseleave(function() {
						$("#div_head_my").hide();
					});
					for(i=1; i<length_all; i++) {
						$("#all_menu_"+i).mouseover(function() {
							for(j=1;j<length_nc; j++) {
								$("#all_menu_nc_title_"+j).css("border-bottom", "3px solid #FFFFFF");
								$("#all_menu_nc_menu_"+j).css("border-bottom", "3px solid #FFFFFF");
								$("#all_menu_nc_arrow_"+j).css("display", "none");
							}
							for(j=1;j<length_ta; j++) {
								$("#all_menu_ta_title_"+j).css("border-bottom", "3px solid #FFFFFF");
							}
							for(j=1;j<length_cp; j++) {
								$("#all_menu_cp_title_"+j).css("border-bottom", "3px solid #FFFFFF");
							}
							for(j=1;j<length_st; j++) {
								$("#all_menu_st_title_"+j).css("border-bottom", "3px solid #FFFFFF");
							}
							for(j=1;j<length_ev; j++) {
								$("#all_menu_ev_title_"+j).css("border-bottom", "3px solid #FFFFFF");
							}
							for(j=1;j<length_sq; j++) {
								$("#all_menu_sq_title_"+j).css("border-bottom", "3px solid #FFFFFF");
							}
							for(j=1;j<length_pr; j++) {
								$("#all_menu_pr_title_"+j).css("border-bottom", "3px solid #FFFFFF");
							}
							for(j=1;j<length_sb; j++) {
								$("#all_menu_sb_title_"+j).css("border-bottom", "3px solid #FFFFFF");
							}
							$("#all_menu_lg_title").css("border-bottom", "3px solid #FFFFFF");
							$("#all_menu_lg_menu").css("border-bottom", "3px solid #FFFFFF");
							$("#all_menu_lg_arrow").css("display", "none");
							for(j=1;j<length_all; j++) {
								if(this.id == "all_menu_"+j) {
									$("#all_menu_"+j).css("left", 0);
									$("#all_menu_div_"+j).css("display", "block");
								} else {
									$("#all_menu_"+j).css("left", -102);
									$("#all_menu_div_"+j).css("display", "none");
								}
							}
						});
					}
					for(i=1; i<length_nc; i++) {
						$("#all_menu_nc_"+i).mouseout(function() {
							$("#all_menu_nc_title_"+this.id.split("_")[3]).css("border-bottom", "3px solid #FFFFFF");
							$("#all_menu_nc_menu_"+this.id.split("_")[3]).css("border-bottom", "3px solid #FFFFFF");
