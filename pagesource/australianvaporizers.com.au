        var fromPoop = 0;
        

          

          
            var store = window.FindifyHelpers.createCollection(url, {
              key: apiKey,
              user: user
            });
            var pageNum = window.location.href.match(/\#page=[0-9]*/g)
            if(pageNum){
              pageNum = pageNum[0].replace(/\#page=/, '')
              var pageRequest = {
                name: 'setPage',
                payload:{
                  page: pageNum
                }
              }
              store.emit(pageRequest);
            }
            var request = {
              name: 'request',
              payload: {
                limit: collectionPageItems
              }
            }
          

        if(url == 'collections/home'){
          store.emit(request);
        } else if(url.indexOf('/search') === -1 ){
          store.emit(request);
        }
        store.subscribe( function(event) {
          if (neverCalled && event.name === 'responseSuccess') {
            neverCalled = false
          }
          if (event.name === 'request') {
            var requestBody = store.get('request');
          }
          if (event.name === 'responseSuccess') {
            var responseBody = Promise.resolve(store.get('response'))
