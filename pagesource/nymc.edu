    <script>
      function adjustHeader () {
        if ($( window ).width() <= 991 && $( window ).width() >= 768) {
          var headerHeight = ($('#layerslider').length > 0)? $('#layerslider').height() : $('header .background-img').height();

          if ($('header #sub-nav').length > 0) {
