<script language="javascript">
	$(document).click(function() {
		$('#suggestions').slideUp('fast');
	});

	$("#suggestions").click(function(e) { // Wont toggle on any click in the div
		e.stopPropagation();
	});

	function lookup(inputString) {

		if(inputString.length < 2) {
			$('#suggestions').fadeOut(); // Hide the suggestions box
		}
		else {
			var request = $.ajax({
				type: "GET",
				url: "/search",
				data: "q="+inputString+"&type=autocomplete",
				cache: false,
				dataType: "html",

				success: function(responce){
					if ((responce !== undefined) && (responce.length > 0)) {
						$('#suggestions').fadeIn(); // Show the suggestions box
						$('#suggestions').html(responce); // Fill the suggestions box
					}
					else {
						$('#suggestions').fadeOut(); // Hide the suggestions box
					}
				}
			});
		}
	}
</script>


