	var countdown = 60;
	function setmytime(obj) {
		if (countdown == 0) {
			obj.removeAttribute("disabled");
			jQuery(obj).html("<span>重新发送</span>");
			countdown = 60;
			return;
		} else {
			obj.setAttribute("disabled", true);
			jQuery(obj).html("<span>已发送(" + countdown + ")</span>");
			countdown--;
		}
		setTimeout(function() {
			setmytime(obj)
		}, 1000)
	}

