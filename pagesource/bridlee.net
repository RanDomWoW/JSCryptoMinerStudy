    <script>
        function validate_login()
        {
            var emailaddressVal = $("#login-username").val();
            var passwordVal = $("#login-password").val();

            if (emailaddressVal == '' && passwordVal == '') {
                $('div#message').html('<p class="login-error" data-shp="lgnform-error">Please enter your email address and password.</p>');
                setError(true);
            } else if (emailaddressVal == '') {
                $('div#message').html('<p class="login-error" data-shp="lgnform-error">Please enter your email address.</p>');
                setError(true);
            } else if (passwordVal == '') {
                $('div#message').html('<p class="login-error" data-shp="lgnform-error">Please enter a password.</p>');
                setError(true);
            } else {
                var serializedData = $('#login').serialize();
                $.ajax({
                    type: 'POST',
                    url: 'https://www.bridlee.net/front/ajax_login_validation',
                    async: false,
                    data: serializedData,
                    dataType: "json",
                    success: function (data, textStatus, errorThrown) {
                        if (data.result == 'valid') {
                            $('#login').submit();
                            return true;
                        } else if (data.result == 'already_connected') {
                            /* Redirect the user to MA*/
                            window.location.href = data.redirect_url;
                        } else {
                            var resultHtml = '<p class="login-error" data-shp="lgnform-error">' + data.error_msg + '</p>';

                            $('div#message').html(resultHtml);
                            return false;
                        }
                    },
                    error: function (qXHR, textStatus, errorThrown) {
                        var resultHtml = '<p class="login-error" data-shp="lgnform-error">There was an error please submit form again.</p>';
                        $('div#message').html(resultHtml);
                        return false;
                    }
                });
            }
        }

        function doForgotPassword()
        {
            $("#bt_signin_password_form").prev("span").html("Loading...");
            $("#message_password_form").html();

            var action = "https://www.bridlee.net/front/forgot_password_ajax";
            var form_data = {
                email: $("#username_password_form").val(),
                forgot_password: 1,
                is_ajax: 1
            };

            $.ajax({
                type: "POST",
                url: action,
                data: form_data,
                success: function (response)
                {
                    $("#bt_signin").html("Submit");

                    if (response == 'Your password has been sent to your email.') {
                        $("#message_password_form").html("<p class='forgot-success'>" + response + "</p>");
                        $("#bt_signin_password_form").prev("span").html("Submit");
                    } else {
                        $("#message_password_form").html("<p class='login-error' data-shp='recform-error'>" + response + "</p>");

                        $("#bt_signin_password_form").prev("span").html("Submit");
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {

                }
            });

            return false;
        }

        $(".open-email").click(function () {
            $("#login, .open-email").hide();
            $("#password_form, .return-login").show();
            return false;
        });
        $(".return-login").click(function () {
            $("#password_form, .return-login").hide();
            $("#login, .open-email").show();
            return false;
        });
    </script>

