                          firebase.initializeApp(config);
                          database = firebase.database();
                      }
                      database.ref("HipStamp" + element.data("realtime")).on("value", function(snapshot) {
                          if (snapshot.val() !== null && snapshot.val().toString() !== element.text()) element.text(snapshot.val()).effect("pulsate", { times:3 }, 2500);
                      });
                  });
                }
                $(document).ready(function() {
