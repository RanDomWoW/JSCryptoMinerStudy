<div aria-hidden="true" aria-labelledby="myModalLabel" class="modal fade" id="ModalTermosAtualizados" role="dialog" tabindex="-1">
<div class="modal-dialog">
<div class="modal-content">
<div class="modal-header">
<h4 class="modal-title" id="myModalLabelStatus">Houve um erro na aceitação do contrato.</h4>
</div>

<div class="modal-body">
<div><label class="modal-label" id="myModalBodyMessage" style="min-height:400px;">Por favor, contate nossa central de suporte para reportar o problema.</label><button class="modal-buttom" onclick="$('#ModalTermosAtualizados').modal('hide');" type="button">Fechar</button></div>
</div>
</div>
</div>
</div>
<script>
    function showModalContract() {
        document.getElementById('contractIframe').src = '/pt_br/contrato/';
        $('#ModalTermosNaoAceitos').modal('show');
    }
    
    function acceptTerms () {
        var email = $('.loginaberto #login-fragment #form_username').val()
        
        $('#ModalTermosNaoAceitos .modal-header h4').text('Atualizando seus termos...');
        $('#ModalTermosNaoAceitos .modal-body').html('<div class="preloader2" style="width: 100%;margin: 0px 45% 0px; display: inline-block;"><span></span><span></span><span></span><span></span><span></span></div>');
        
        $('#loginMessage').html('');
        
        $.ajax({
            type: 'PUT',
            url: '/rest/term/accept',
            contentType: "application/json; charset=utf-8",
            crossDomain: true,
            dataType: "json",
            success: function(data, status, jqXHR) {
                $('#ModalTermosNaoAceitos').modal('hide');
                
                location.href="/dashboard/#/";
            },
            error: function(jqXHR, status) {
                $('#ModalTermosNaoAceitos').modal('hide');
                
                $('#ModalTermosAtualizados').modal('show');
            }
        });
    }
  
    function logout() {
        $.ajax({
            type: 'POST',
            url: '/rest/logout',
            success: function() {
                location.href='/';
            },
            error: function() {
                
            }
        });
    }
</script></div>
