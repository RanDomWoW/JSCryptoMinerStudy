		<script>
		if(jQuery(window).outerWidth() > 600) {				
			var config = {
				complete: function(el) {
					el = jQuery(el);
				
					if(el.hasClass('bar')) {
						el.addClass('gk-loaded');
					}
					
					circle1.each(function(sum, i) {
						circle1[i].circle.animate(jQuery(circle1[i].sum).attr('data-final'));
					});
					
					circle2.each(function(sum, i) {
						setTimeout(function() {
