 <script type="text/cjs">
        $(document).ready(function(){

            function selectMytab() {
                $('.each-theme-category').click(function(){
                    $('#dy_Teste_Cluster_Person_011217').removeClass('remove-person');
                    <!-- $(this).addClass('teste-person-2'); -->
                });
             }

            $mainTheme = $('.a-cms-categories');

            function selectButton(){
                $(".each-theme-category").click(function(){
                    $('.each-theme-category').removeClass('selected default-class');
                    $(this).addClass('selected');
                });
                //pega todas as mat√©rias
                $mainTheme = $('.a-cms-categories');
                //fun√ß√£o chamada quando clicar em algum filtro
                $('.each-theme-category').click(function(){
                    $('.each-theme-category').removeClass('selected default-class');
                    $(this).addClass('selected');
                    //pega identificador do filtro que foi clicado
                    $newForma = $(this).attr('id');
                    //se o identificador for tajado como 'tudo', aparecer√° todos as mat√©rias
                    if($newForma == "tudo"){
                        $($mainTheme).css({'display':'inline-block'});
                        for(i = 0;i < $mainTheme.length;i++){
                            $mainTheme[i].setAttribute('data-check-ctg', 'true');
                        }
                    }
                    //se for qualquer outro filtro clicado, aparecer√° apenas as mat√©rias da mesma categoria
                    else{
                        $($mainTheme).css({'display':'inline-block'});
                        for(i = 0;i < $mainTheme.length;i++){
                            $segmentsItens = $($mainTheme[i]).filter('[data-theme-segment]').data();
                            $dataValueCpn = $segmentsItens.themeSegment.split(';');
                            $($mainTheme[i]).css({'display':'none'});
                            for(j = 0;j < $dataValueCpn.length; j++){
                                if($dataValueCpn[j] == $newForma){
                                    $($mainTheme[i]).css({'display':'inline-block'});
                                    $mainTheme[i].setAttribute('data-check-ctg', 'true');
                                    break;
                                }else{
                                    $mainTheme[i].setAttribute('data-check-ctg', 'false');
                                }
                            }
                        }
                    }
                    var categoryToDisplay = $('div[data-check-ctg="true"]');
                    if(categoryToDisplay.length < 7){
                        $('.scrol-center').css({'display':'none'})
                    }else{
                        $('.scrol-center').css({'display':'inline-block'});
                    }
                    countThemes($mainTheme, $newForma);
                });
            }
            function countThemes(mainTheme){
                x = arguments.length > 1 ? arguments[1] : 'tudo';
                $dataSegment = $('div[data-theme-segment="' + x +'"]');
                if(x == 'tudo'){
                    for(var i = 0; i < mainTheme.length; i++){
                        if(i < 7){
                            $(mainTheme[i]).css({'display':'inline-block'});
                        }else{
                            $(mainTheme[i]).css({'display':'none'});
                        }
                    }
                }else{
                    for(var i = 0; i < $dataSegment.length; i++){
                        $segmentsItens = $($dataSegment[i]).filter('[data-theme-segment]').data();
                        $dataValueCpn = $segmentsItens.themeSegment.split(';');
                        if(i < 7 && $dataValueCpn == x){
                            $($dataSegment[i]).css({'display':'inline-block'});
                        }else{
                            $($dataSegment[i]).css({'display':'none'});
                        }
                    }
                }
            }

            function scrollPageShowThemes(container){
                if($(container).offset().top<$(window).scrollTop()+$(window).height()&&$(container).offset().top+$(container).height()>$(window).scrollTop()){
                    $(window).unbind('.myScroll');
                    setTimeout(function(){
                        var categoryToDisplay = $('div[data-check-ctg="true"]');
                        for(i = 0; i < categoryToDisplay.length; i++){
                            categoryDisplayed = categoryToDisplay[i].style.cssText;
                            if(categoryDisplayed == 'display: none;'){
                                for(var j = i; j < i + 7; j++){
                                    if(j >= categoryToDisplay.length){
                                        $(container).css({'display':'none'});
                                        break;
                                    }else{
                                        $(categoryToDisplay[j]).css({'display':'inline-block'});
                                    }
                                }
                                break;
                            }
                        }
                        $(window).bind("scroll.myScroll", function(){
                            console.log('get it');
                            scrollPageShowThemes('.scrol-center');
                        });
                    }, 1000);
                }
            }

            $(window).bind("scroll.myScroll", function(){
                scrollPageShowThemes('.scrol-center');
            });

            countThemes($mainTheme);
            selectButton();
            selectMytab();
        });
    </script>


