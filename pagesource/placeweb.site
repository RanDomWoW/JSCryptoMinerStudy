<script>

$(document).ready(function(){
    $('#contact-form').bind('submit', function(event){
        event.preventDefault();
        var forms = $(this);
        dados = $(this).serialize();
        $('#send-message-success').remove();
        $('#send-message-error').remove();
        $.ajax({
            url: "http://www.pagefree.net/System/send_msg",
            data: dados,
            type: 'POST',
            dataType: 'json',
            success: function(e){
                if(e.status === 1) {
                    $(forms).after('<div id="send-message-success">'+e.message+'</div>');
                    $('#email').val('');
                    $('#name').val('');
                    $('#message').val('');
                } else {
                    $(forms).after('<div id="send-message-error">'+e.message+'</div>');
                }
            }
        });
    });
});

</script>