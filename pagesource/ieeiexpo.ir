        jQuery(document).ready(function () {
            jQuery('#home-video').attr('src' , '');
            jQuery('#home-video').load();
            jQuery.fn.isVisible = function() {
                // Am I visible?
                // Height and Width are not explicitly necessary in visibility detection, the bottom, right, top and left are the
                // essential checks. If an image is 0x0, it is technically not visible, so it should not be marked as such.
                // That is why either width or height have to be &gt; 0.
                try{
                var rect = this[0].getBoundingClientRect();
                return (
                    (rect.height > 0 || rect.width > 0) &&
                    rect.bottom >= 0 &&
                    rect.right >= 0 &&
                    rect.top <= (window.innerHeight || document.documentElement.clientHeight) &&
                    rect.left <= (window.innerWidth || document.documentElement.clientWidth)
                );
              }catch(e){}
            };
            var aflag = true;
            var whoFlag = true;
            function doCheck() {
                var elementToDetect = jQuery('#summary');
                if (elementToDetect.isVisible() && aflag) {
                    aflag = false
                    jQuery('.count').each(function () {
                        jQuery(this).prop('Counter',0).animate({
                            Counter: jQuery(this).text()
                        }, {
                            duration: 4000,
                            easing: 'swing',
                            step: function (now) {
                                jQuery(this).text(Math.ceil(now));
                            }
                        });
                    });
                } else {
                }
                var whoWe  = jQuery('#whoWeAre');
                if (whoWe.isVisible() && whoFlag) {
                    whoFlag=false;
                    jQuery('.roll-progress').each(function(){
                        var whoWid = jQuery(this).find('.progress-animate').data("percent") + "%"
                        jQuery(this).find('.progress-animate').animate({
                            width: whoWid
                        }, 2000 );
                        jQuery(this).find('.perc').animate({
                            width: whoWid
                        }, 2000 );
                        //.css("width" , whoWid)
                    })
                }
                var animation_element = jQuery('.social').first();
                if (animation_element.isVisible()) {
                var  $animation_elements = jQuery('.animated');
                var  $window = jQuery(window);
                  var window_height = $window.height();
                  var window_top_position = $window.scrollTop();
                  var window_bottom_position = (window_top_position + window_height);
                  jQuery.each($animation_elements, function() {
                    var $element = jQuery(this);
                    var element_height = $element.outerHeight();
                    var element_top_position = $element.offset().top;
                    var element_bottom_position = (element_top_position + element_height);
                      jQuery('.animated').addClass('animation_started');
                  });
                }
                if (!animation_element.isVisible()) {
                var  $animation_elements = jQuery('.animated');
                var  $window = jQuery(window);
                  var window_height = $window.height();
                  var window_top_position = $window.scrollTop();
                  var window_bottom_position = (window_top_position + window_height);
                  jQuery.each($animation_elements, function() {
                    var $element = jQuery(this);
                    var element_height = $element.outerHeight();
                    var element_top_position = $element.offset().top;
                    var element_bottom_position = (element_top_position + element_height);
                      jQuery('.animated').removeClass('animation_started');
                  });
                }
            }
            jQuery(document)
                .ready(function(e){
                    doCheck();
                });
            jQuery(window)
                .scroll(function(e){
                    doCheck();
                });
        })
