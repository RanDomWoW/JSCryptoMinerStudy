<script type="text/JavaScript">
function createExpires(dd){
  var now = new Date();
  var expires = new Date(now.getTime()+dd);
  //var expires = new Date(now.getTime()+24*60*60*1000*parseInt(dd));
  //var expires = new Date(now.getTime()+1000*60);
  return(expires.toGMTString());
}
function getExpires(){
  var cValue = document.cookie;
  var sIndex= cValue.indexOf("expires_time",0);
  if(sIndex > -1){
    sIndex = cValue.indexOf("=", sIndex) + 1;
    eIndex = cValue.indexOf("END", sIndex);
    return unescape(cValue.substring(sIndex, eIndex));
  }else{
    return 0;
  }
}
function setCookie(key, value, limit){
  var expires_time = new Date();
  expires_time = expires_time.toGMTString()
  var create_expires = createExpires(limit);
  var get_expires = getExpires();
    var now = new Date();
  var now = new Date(now.getTime());
  if (get_expires == 0) {
    expires_time = create_expires;
  }
  if (now.toGMTString() < get_expires) {
    expires_time=get_expires;
  }
  var cValue = key + "=" + escape(value) + "END;expires=" + expires_time + ";";
  document.cookie = cValue;
  var cValue = "expires_time" + "=" + escape(expires_time) + "END;expires=" + expires_time + ";";
  document.cookie = cValue;
}
function getCookie(key){
  var cValue = document.cookie;
  var sIndex= cValue.indexOf(key,0);
  if(sIndex > -1){
    sIndex = cValue.indexOf("=", sIndex) + 1;
    eIndex = cValue.indexOf("END", sIndex);
    return unescape(cValue.substring(sIndex, eIndex));
  }else{
    return 0;
  }
}
</script>
