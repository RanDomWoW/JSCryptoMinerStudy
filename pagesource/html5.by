if(uri.queryKey.action){
$('.action-section').hide();
$('.action-' + uri.queryKey.action ).show();
if('logout'==uri.queryKey.action && 'undefined'==typeof(uri.queryKey.showonly) ){
var args = {
'rhl_action':'logout',
'action':'logout',
'_wpnonce':uri.queryKey._wpnonce||'',
'redirect_to': redirect_url
};
$.ajax({
url: ajax_url,
data: args,
dataType: 'json',
success: function(data){					
