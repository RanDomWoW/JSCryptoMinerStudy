function fixPostImage ($img)
{
	var banned_image_hosts = /tinypic|imagebanana|hidebehind|ipicture/i;  // imageshack
	var src = $img[0].src;
	if (src.match(banned_image_hosts)) {
		$img.wrap('<a href="'+ this.src +'" target="_blank"></a>').attr({ src: "http://rgfootball.net/images/smiles/tr_oops.gif", title: "RGFootball.tv" });
	}
	return $img;
}


