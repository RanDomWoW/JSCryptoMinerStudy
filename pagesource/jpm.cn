	function resresh_kuaixun(){
		$.ajax({
			type:'get',
			url:'/index/getNewsletter',
			dataType:'json',
			success:function(data){
				var ok = false;
				var d = new Date();
				console.log(d.getHours()+":"+d.getMinutes()+":"+d.getSeconds());
				d.setHours(0,0,0,0);
				var today = d.getTime();
				var html = '';
				if(data){
					$.each(data,function(i,n){
						var date = new Date(n.createtime*1000);
						if(n.createtime*1000 <= today && !ok){
							html += '<div class="jpm_news_data clearfloat" style=""><strong></strong><span>'+ (date.getMonth()+1)+'月'+date.getDate()+'日'+'</span><strong></strong></div>';
							ok=true;
						}
						var hour = date.getHours() < 10 ? '0' + date.getHours() : date.getHours();
						var minute = date.getMinutes() < 10 ? '0' + date.getMinutes() : date.getMinutes();
						var time = hour+':'+minute;
						html += '<li class="clearfloat"><span>'+time+'</span><a href="/page/newsletter?id='+n.id+'" title="'+n.title+'" target="_blank">'+n.title+'</a>	</li> '; 
					});
					$("#kuaixun").html(html);
				}
			}
		});
	}
	setInterval("resresh_kuaixun()",60000);
	
