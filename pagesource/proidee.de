        $('#THEMENWELT_SLIDER').on('initialized.owl.carousel', function(e) {
            var pages     = e.page.count;
            var page      = e.page.index+1;

            if (pages == 1){
                $('#THEMENWELT_SLIDER' + ' .owl-nav').hide();
                $('#THEMENWELT_SLIDER .slider-seitenschaltung').hide()
            }

            setTimeout(function() {
                if ($('#THEMENWELT_SLIDER').find('.owl-stage div.owl-item:first').hasClass('active')) {
                    $('#THEMENWELT_SLIDER').find('.owl-prev').addClass('inactive');
                }
                else {$('#THEMENWELT_SLIDER').find('.owl-prev').removeClass('inactive');}
                if ($('#THEMENWELT_SLIDER').find('.owl-stage div.owl-item:last').hasClass('active')) {
                    $('#THEMENWELT_SLIDER').find('.owl-next').addClass('inactive');
                }
                else {$('#THEMENWELT_SLIDER').find('.owl-next').removeClass('inactive');}

                setSliderControls(e);
            }, 100);
            aktualisierenSliderNavStatus('#THEMENWELT_SLIDER', page, pages);

            $('#THEMENWELT_SLIDER .slider-seitenschaltung').html(page + ' ' + texteGlobal['von'] + ' ' + pages);

        }).on('mouseenter', function() {
            $('#THEMENWELT_SLIDER').find('.owl-nav').css('opacity', 1);
        }).on('mouseleave', function() {
            $('#THEMENWELT_SLIDER').find('.owl-nav').css('opacity', 0);
        }).on('translated.owl.carousel', function(e) {

            var pages     = e.page.count;
            var page      = e.page.index+1;

            $('#THEMENWELT_SLIDER' + ' .owl-item.active').find('img').each(function() {
                var newsrc = $(this).attr('data-original');
                $(this).attr('src', newsrc);
                $(this).removeClass('lazy');
            });
             $('#THEMENWELT_SLIDER' + ' .owl-item.active:last').next().find('img').each(function() {
                var newsrc = $(this).attr('data-original');
                $(this).attr('src', newsrc);
                $(this).removeClass('lazy');
            });

            aktualisierenSliderNavStatus('#THEMENWELT_SLIDER', page, pages);

            if ($('#THEMENWELT_SLIDER' + ' .owl-stage div.owl-item:last').hasClass('active')) {
                page = pages;
            }
            $('#THEMENWELT_SLIDER .slider-seitenschaltung').html(page + ' ' + texteGlobal['von'] + ' ' + pages);

        }).on('resized.owl.carousel', function(e){

