<script>
document['filter_main_setting']={
	type:'pbbitem',
	submit_btn:'#pbbitem_filter_submit',
	form_action_mask:'%folder-url_chpu%',
	count_num:18418,
	count_ajax:'my_pbbitem_filter_prev',
	count_word:{0:'объектов',1:'объект',2:'объекта',5:'объектов'},
	advanced_ajax:'my_pbbitem_filter_advanced&table=%folder-table%',
};
$(document).ready(function(){
	validationForm.costEvent('.main-filter input[splitNum]', 16);
});
var save_filter_obj={
	arr_info:{
		'type_trade':{
			'sale':'Продажа','rent':'Аренда','sutki':'Аренда посуточно','exchange':'Обмен',		},
		'limit':{
			'10':'По 10 на странице','30':'По 30 на странице','50':'По 50 на странице',		},
		'cost_type':{
			'r':'Руб.','d':'$','e':'€','br':'Бел. руб.','g':'Грн.',		},
		'folder':{}
	},
	init:function(){
		$('.save-filter .openCloseBtn').click(save_filter_obj.openCloseList);

		get=$.getUrlVar();

		$('.save-filter .save_btn').click(save_filter_obj.openCloseList);
		if (get['type_trade']!=undefined && get['folder']!=undefined ) $('.save-filter .save_btn').click(save_filter_obj.actSaveFilter);

		list_str=userStorage.get('save_filter');
		save_filter_obj.list_obj=$.parseJSON(list_str);
		if (save_filter_obj.list_obj==null) save_filter_obj.list_obj={'list':[]};
		save_filter_obj.refreshList();
		$('body').click(function(){
			if ($('.save-filter .filter-drop').css('display')!='none') save_filter_obj.openCloseList();
		});
		$('.save-filter .filter-list').click(function(event){
			if (event!=undefined) event.stopPropagation();
		})
		if ((get['type_trade']==undefined || get['folder']==undefined) && save_filter_obj.list_obj.list.length<1) $('.save-filter').css('display', 'none');
	},
	openCloseList:function(event){
		if (event!=undefined) event.stopPropagation();
		if ($('.save-filter .filter-drop').css('display')=='none')	$('.save-filter .filter-drop').css('display','block');
		else $('.save-filter .filter-drop').css('display','none');
	},
	actSaveFilter:function(event){
		if (event!=undefined) event.stopPropagation();
		get=$.getUrlVar();
		if (get['type_trade']==undefined || get['folder']==undefined ) return;
		
		var pos_name_loc=$('#filter_select_loc').find('option').html();
		url=location.href+'&pos_name_loc='+pos_name_loc;
		list_str=userStorage.get('save_filter');
		save_filter_obj.list_obj=$.parseJSON(list_str);
		if (save_filter_obj.list_obj==null) save_filter_obj.list_obj={'list':[]};

		flag_return=false;
		$.each(save_filter_obj.list_obj.list, function(index, url_tmp){
			if (url_tmp==url) {
				flag_return=true;
				alert('Такая конфигурация фильтра уже сохранена');
				return false;
			}
		})
		if (flag_return) return;
		
		if (save_filter_obj.list_obj.list.length>=10) {
			alert('Вы можете сохранить не более 10 конфигураций фильтра');
			return;
		}

		save_filter_obj.list_obj.list.push(url);
		list_str=JSON.stringify(save_filter_obj.list_obj);
		userStorage.set('save_filter', list_str);
		save_filter_obj.refreshList();
	},
	actDelFilter:function(id){
		list_str=userStorage.get('save_filter');
		save_filter_obj.list_obj=$.parseJSON(list_str);
		if (save_filter_obj.list_obj==null) save_filter_obj.list_obj={'list':[]};
		save_filter_obj.list_obj.list.splice(id, 1);
		list_str=JSON.stringify(save_filter_obj.list_obj);
		userStorage.set('save_filter', list_str);
		save_filter_obj.refreshList();
	},
	refreshList:function(){
		list_str=userStorage.get('save_filter');
		save_filter_obj.list_obj=$.parseJSON(list_str);
		if (save_filter_obj.list_obj==null) save_filter_obj.list_obj={'list':[]};
		$('.save-filter .filter-list').html('');
		$.each(save_filter_obj.list_obj.list, function(index, url){
			get=$.getUrlVar(url);
			text=[];
			if (save_filter_obj.arr_info['type_trade'][get['type_trade']]!=undefined) text.push(save_filter_obj.arr_info['type_trade'][get['type_trade']]); else text.push(save_filter_obj.arr_info['type_trade']['sale']);
			if (save_filter_obj.arr_info['folder'][get['folder']]!=undefined) text.push(save_filter_obj.arr_info['folder'][get['folder']]); else text.push(save_filter_obj.arr_info['folder']['175']);
			text.push(get['pos_name_loc']);
			if ((get['price_from']!=undefined && get['price_from'].match(/^\d+/)!==null) || (get['price_to']!=undefined && get['price_to'].match(/^\d+/)!==null)){
				tmp_text='Цена';
				get['price_from']=get['price_from'].split(' ').join('');
				get['price_to']=get['price_to'].split(' ').join('');
