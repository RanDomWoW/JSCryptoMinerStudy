.toggleMenu {font-family:'bahij',Roboto;display:none;padding:0 15px 0 0;text-align:center;height:36px;line-height:36px;color: #333 !important;font-size:16px;position:relative;text-transform:capitalize!important;font-weight:700;z-index:999999}
