<script>
    var player;

    function onYouTubeIframeAPIReady() {
        player = new YT.Player('muteYouTubeVideoPlayer', {
            videoId: 'esdg72QUBE8', // YouTube Video ID
            playerVars: {
                autoplay: 1, // Auto-play the video on load
                controls: 1, // Show pause/play buttons in player
                showinfo: 0, // Hide the video title
                modestbranding: 0, // Hide the Youtube Logo
                loop: 1, // Run the video in a loop
                fs: 1, // Hide the full screen button
                cc_load_policty: 0, // Hide closed captions
                iv_load_policy: 3, // Hide the Video Annotations
                autohide: 1 // Hide video controls when playing
            },
            events: {
                onReady: function(e) {
                    e.target.mute();
                },
                onStateChange: function(e){
                    if (e.data === YT.PlayerState.ENDED) {
                        player.playVideo();
                    }
                }
            }
        });
    }

    function toggleMute()
    {
        if(player.isMuted())
        {
            player.unMute();
            $("#nsru_channel_toggle_mute i").removeClass("uk-icon-volume-off");
            $("#nsru_channel_toggle_mute i").addClass("uk-icon-volume-up");
        } else {
            player.mute();
            $("#nsru_channel_toggle_mute i").removeClass("uk-icon-volume-up");
            $("#nsru_channel_toggle_mute i").addClass("uk-icon-volume-off");
        }
    }
