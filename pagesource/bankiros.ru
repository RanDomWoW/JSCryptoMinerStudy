                slider.slider('setAttribute', 'min', d.sliderMin);
                slider.slider('setAttribute', 'max', d.sliderMax);
                slider.slider('setValue', d.sliderValue);
                label = setDenomLabel(d.customValues[d.sliderValue], d.sliderDenom);
