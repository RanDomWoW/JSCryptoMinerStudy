/* - Hello / Region Bar - */
#regionSiteBar, #helloBar {
    position: relative;
    width: 100%;
    background-color: #177cf9;
    background-position: center;
    background-repeat: no-repeat;
    text-align: center;
    color: #ffffff;
    height: 55px;
    padding-top: 22px;
    font-weight: 500;
    -webkit-font-smoothing: antialiased;
    font-size: 18px;
    display: none;
    z-index: 9999999;
}
#regionSiteBar p, #regionSiteBar a, #helloBar p, #helloBar a{
    display:inline;
    color:#ffffff;
}
#regionSiteBar a.cta, #helloBar a.cta{
    padding: 8px 20px;
    border: solid #ffffff 1px;
    border-radius: 5px;
    color:#ffffff;
    background-color: transparent;
    margin:0px 20px 0px 20px;
    font-size: 16px;
}
#regionSiteBar a.cta:hover, #helloBar a.cta:hover{
    background-color:rgba(255, 255, 255, 1);
    color:#177cf9;
}
#regionSiteBar .regionClose, #helloBar .regionClose{
    font-size: 24px;
    position: relative;
    top: 3px;
    cursor:pointer;
}
#helloBar{
    background-image: linear-gradient(to left,#177cf9,#22A5FA);
    position: relative;
}
#helloBar img{
    width: 47px;
    height: 37px;
    position: relative;
    top: -2px;
    margin-right: 20px;
}
#helloBar a.cta:hover{
    color:#177cf9 !important;
}



