 <script>
		//切换首页的新闻图片
		var currslid="", slidint="";
		autoChange();//自动播放
		function changeNewImg(id)
		{
            currslid=id;
            var items = window.document.getElementsByTagName("table");
	        for (var i=0; i<items.length; i++)
	        {

		        if(items[i].id.indexOf("pic_")==0)
		        {
		            items[i].style.display="none";
		        }			        
		        if(items[i].id.indexOf("pic_" + id)==0)
		        {
		            items[i].style.display="inline";
		        }			
				if(items[i].id.indexOf("picList_")==0)
		        {
		           items[i].style.background = "#ffffff";
		        }			        
		        if(items[i].id.indexOf("picList_" + id)==0)
		        {
		           items[i].style.background = "#FAFAD2";
		        }		
	        }
            
            var items1 = window.document.getElementsByTagName("div");
	        for (var i=0; i<items1.length; i++)
	        {        
		        if(items1[i].id.indexOf("show_biankuang_")==0)
		        {
		           items1[i].style.display = "none";
		        } 
		        if(items1[i].id.indexOf("Black_")==0)
		        {
		           items1[i].style.display = "block";
		        }
	        }
            
	       document.getElementById("show_biankuang_"+id+"_1").style.display='block';
	       document.getElementById("show_biankuang_"+id+"_2").style.display='block';
	       document.getElementById("show_biankuang_"+id+"_3").style.display='block';
	       document.getElementById("show_biankuang_"+id+"_4").style.display='block';
	       document.getElementById("Black_"+id).style.display='none';
	        
		}
		function playnext()
        {
            var arr=document.getElementById("picID").value.split(',');
            var picid="";//切换图片ID
            for(var i=0;i<arr.length;i++)
            {
                if(arr[i]==currslid)
                {
                    picid=i;
                } 
            }
            if(picid==arr.length-1){picid=0;}
            else{picid++;}            
            changeNewImg(arr[picid]);

        }
        function autoChange()
        {
            slidint=setInterval("playnext()",5000);
        }
        function stopit()
        {
            clearInterval(slidint);
        }
        function ChangeDiv(args,num)
        {
            if(args=='1')
            {
                var arrays = new Array('34','35','37');
                for(var h_i=0;h_i<arrays.length;h_i++)
                {
                    document.getElementById(arrays[h_i]).className="font1200_000000";
                    document.getElementById('div_'+arrays[h_i]).style.display='none';
                }
                document.getElementById(num).className='font1200_ff0000_1';
                document.getElementById('div_'+num).style.display='block';
                
            }
            else if(args=='2')
            {
                var arrays = new Array('26','25','138');
                for(var h_i=0;h_i<arrays.length;h_i++)
                {
                    document.getElementById(arrays[h_i]).className="font1200_000000";
                    document.getElementById('div_'+arrays[h_i]).style.display='none';
                }
                document.getElementById(num).className='font1200_ff0000_1';
                document.getElementById('div_'+num).style.display='block';
            }
            else if(args=='3')
            {
                var arrays = new Array('18','112');
                for(var h_i=0;h_i<arrays.length;h_i++)
                {
                    document.getElementById(arrays[h_i]).className="font1200_000000";
                    document.getElementById('div_'+arrays[h_i]).style.display='none';
                }
                document.getElementById(num).className='fty';
                document.getElementById('div_'+num).style.display='block';
            }
            else if(args=='4')
            {
                var arrays = new Array('73','80');
                    if(num=='73')
                    {
                        document.getElementById("menu_73").style.paddingLeft='0px';
                        document.getElementById('menu_73').innerHTML="<img src='images/default_040.gif' width='147' height='36' />";
                        document.getElementById('menu_80').innerHTML="<img src='images/default_041.gif' width='72' height='36' />";
                    }
                    else
                    {
                        document.getElementById("menu_73").style.paddingLeft='40px';
                        document.getElementById('menu_73').innerHTML="<img src='images/default_092.gif' width='72' height='36' />";
                        document.getElementById('menu_80').innerHTML="<img src='images/default_093.gif' width='147' height='36' />";
                    }
                for(var h_i=0;h_i<arrays.length;h_i++)
                {
                    document.getElementById('div_'+arrays[h_i]).style.display='none';
                }
                document.getElementById('div_'+num).style.display='block';
            }
            
        }
 

</script>
