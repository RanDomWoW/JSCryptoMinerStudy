	<script>
		var brandBar = document.getElementById('proclaim-brand-bar');
		if (brandBar) {
			ReactDOM.render(React.createElement(ProclaimBrandBar, {}), brandBar);
		}

		// defer load images from custom data attributes
		$(window).load(function init() {
			$.each($("img[data-delay-load]"), function () {
				$(this).attr('src', $(this).data('delay-load'));
			});

			$.each($("div[data-delay-load-class]"), function () {
				$(this).addClass($(this).data('delay-load-class'));
			});
		});

		$(document).ready(function () {
			$('.navbar-nav').on('click', 'li', function () {
				$(this).addClass('active').siblings().removeClass('active');
			});

			$('a[href^=#]').click(function (e) {
				e.preventDefault();

				var destintation = $(this).attr('href');
				if (destintation.length)
					$('html,body').animate({ scrollTop: $(destintation).offset().top }, 'slow');
			});
		});

		function getParameterByName(name) {
			var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
			return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
		}
	</script>
