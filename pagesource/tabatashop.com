  <script>
    (function($){
      var $slider = $('#tabata-instagram-slider-levostrefoto .products-slider');
      var $prev = $('#tabata-instagram-slider-levostrefoto .slick-prev');
      var $next = $('#tabata-instagram-slider-levostrefoto .slick-next');
      var $controls = $('#tabata-instagram-slider-levostrefoto .products-slider-controls').remove();
      function _initSlider (slider, prev, next, n) {
        addControls();
        $(slider).slick({
          autoplay: true,
          arrows: false,
          dots: false,
          fade: false,
          infinite: true,
          slidesToShow: n,
          slidesToScroll: 2,
          pauseOnHover: true
        });
        $(prev).click(function() {
          $(slider).slick('slickPrev');
        });
        $(next).click(function() {
          $(slider).slick('slickNext');
        })
      }
      function init() {
        var ww = $(window).width();
        removeControls();
        if(ww < 768) {
          _initSlider($slider, $prev, $next, 2);
        } else if(ww >= 768 && ww < 992 &&  count_levostrefoto > 4) {
          _initSlider($slider, $prev, $next, 4);
        } else if(ww > 992  &&  count_levostrefoto > 6) {
          _initSlider($slider, $prev, $next, 6);
        }
      }
      function removeControls() {
        if($('#tabata-instagram-slider-levostrefoto .products-slider-controls'))
          $('#tabata-instagram-slider-levostrefoto .products-slider-controls').remove();
      }
      function addControls() {
        $('#tabata-instagram-slider-levostrefoto .products-slider').parent().append($controls);
      }
      $(document).ready(function(){
        init();
        if(Modernizr.touchevents) {
          $('.product-wrapper').click(function(e) {
            e.preventDefault();
            var link = $(this).data('product-link');
            document.location = link;
          });
        }
      });
      $(window).onResizeEnd({
        doThis: function(){
          if($slider.hasClass('slick-slider')) {
            $($slider).slick('unslick');
          }
          init();
        },
        wait: 600
      });
    })(jQuery);
  </script>
