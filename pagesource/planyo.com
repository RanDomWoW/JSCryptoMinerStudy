function name_changed () {
  var first = $('#first').val() || '';
  var last = $('#last').val() || '';
  if (first || last) {
    var defaultuser = first+last;
    defaultuser = defaultuser.replace(/[^a-zA-Z0-9\-\_\.]/g, "");
    $('#user').val(defaultuser);
  }
}

$('#first').keyup(name_changed);
