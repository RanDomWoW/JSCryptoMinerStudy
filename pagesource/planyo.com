    url: 'https://www.planyo.com/update-homepage.php',
        dataType: 'json',
        data: $('#contact_form').serialize(),
        type: 'POST',
        success: function(data) {
        if (data && data.result) {
          $('#ok_status_msg').html(data.html);
          $('#ok_status_msg').css('display', 'block');
          $('#contact_form').css('display', 'none');
        }
        else {
          jQuery('#contact_status_msg').html(data.html);
          $('#contact_status_msg').css('display', 'block');
        }
      }
    });
}

jQuery('#msg').keydown(show_full_contact_form);
jQuery('#email').keydown(show_full_contact_form);
jQuery('#email').change(show_full_contact_form);
