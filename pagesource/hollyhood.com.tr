function swap(newImg, id){
	
	$("#"+id).stop();
	$("#"+id).animate({left:"-50", opacity: 0}, 400, function(){
		$("#"+id).attr("src", newImg).one('load', function() {
			$("#"+id).animate({left:"0", opacity: 1}, 400);
		});
	})
	
}

