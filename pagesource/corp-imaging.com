$(function(){
var navbh = $(".navbar").height();
var body = $("html, body");
	$(".sectionlink").click(function(){
		var hash = gethash(this.href);
		if(hash.length>0){
			
			body.animate({scrollTop:$("#"+hash).offset().top-navbh }, '500', 'swing', function() { 
			
			});
		}

