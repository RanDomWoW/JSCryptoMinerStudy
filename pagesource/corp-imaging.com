var hash = gethash(window.location.href);
if(hash.length>0){

body.animate({scrollTop:$("#"+hash).offset().top-navbh }, '500', 'swing', function() { 
			
			});
}
});

function gethash(url){
if(url.indexOf("#")>-1){
	return url.substring(url.indexOf('#')+1);
}else{
	return "";
}
}



