        function GotoLink(link, title, state) {
            
            $.ajax({
                url: link,
                beforeSend: function () {
                    // setting a timeout
                    NProgress.start();
                },
                success: function (data) {
                    NProgress.done();
                    NProgress.remove();
                    $("#Data").html(data);
                    var temp = radio_online;
                    var temp_idx = rungetdata;

                    radio_online = temp;
                    rungetdata = temp_idx;
                    if (title != undefined) {
                        document.title = title;
                    } else {
                        document.title = "VOH Đài tiếng nói nhân dân TPHCM";
                    }
                    window.scrollTo(0, 0);

                    $("body").removeClass("modal-open");
                    $(".modal-backdrop").remove();

                    window.onerror = function (message, source, lineno, colno, error) {
                    };

                    $("#Data").ready(function () {
                        document.title = $('#Data').find('meta[name="title"]').attr('content');
                        //FB.XFBML.parse();
                        $(window).trigger('load');

                        // scroll to restoration
                        if (state.scrollTop > 0) { // back button
                            nextPageAjax(state.scrollTop);
                            var h = $(window).height();
                            if (h < state.scrollTop && $('#loading-more').length) {
                                bindingHideLoadingMore(state.scrollTop);
                            }
                        }
                    });
                },
                error: function (xhr) { // if error occured
                    
                },
                complete: function () {
                 
                }
            });
        }

        var runShedule = null;
        window.onpopstate = function (event) {
            var state = event.state;
            if (document.location.pathname.startsWith("/radio/")) {
                document.location.href = document.location;
            }
            GotoLink(document.location, state.title, state);

            if ('scrollRestoration' in window.history) {
                // Disable auto scroll when go back
                window.history.scrollRestoration = 'manual';
            }
        };

        function PlayChannelMobile(link, index) {
            if ($($(".radioPlay .breaking-radio li")[index]).hasClass("active") ||
                $(".radioPlay .breaking-radio").attr("disabled")) {
                return;
            }

            $(".radioPlay .breaking-radio").attr('disabled', 'disabled'); // disable switch channel

            //$("#radioPlay audio").attr("src", link);
            $("#radioPlay .breaking-radio li").removeClass("active");
            if (index == 0) {
                rungetdata = 1;
            } else if (index == 1) {
                rungetdata = 2;
            } else {
                rungetdata = 3;
            }
            $($("#radioPlay .breaking-radio li")[index]).addClass("active");

            if ($('audio#radio_bottom_native_hls').length == 1) {
                $('audio#radio_bottom_native_hls')[0].pause();
                $('audio#radio_bottom_from_mejs')[0].player.setSrc(link);
                $('audio#radio_bottom_native_hls')[0].play();
            } else if ($('audio#radio_bottom_html5').length == 1) {
                $('audio#radio_bottom_html5')[0].player.pause();
                $('audio#radio_bottom_html5')[0].player.setSrc(link);
                $('audio#radio_bottom_html5')[0].player.play();
            }

            setTimeout(function () {
                $(".radioPlay .breaking-radio").removeAttr('disabled');
            },
                3000);
        }

