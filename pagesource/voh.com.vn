        $('#Data').one('click',
            'a',
            function (event) {
                var linkdata = $(this).attr("href");
                var target = $(this).attr('target');
                
                if (linkdata == undefined || event.originalEvent == undefined) {
                    return;
                }
                if (target != undefined) {
                    return;
                }

                linkdata = linkdata.trim();
                if (linkdata != null &&
                    (linkdata.startsWith("/radio/") ||
                        linkdata.startsWith("#") ||
                        linkdata.startsWith("/lich-phat-song/")) ||
                    linkdata.startsWith("http://www.voh.com.vn") ||
                    linkdata.startsWith("http://www.facebook.com") ||
                    linkdata.startsWith("https://www.facebook.com") ||
                    linkdata.startsWith("http://audio.namvietmedia.vn") ||
                    linkdata.startsWith("http://radio.voh.com.vn") ||
                    linkdata.startsWith("https://radio.voh.com.vn") ||
                    linkdata.startsWith("http://www.google.com") ||
                    linkdata.startsWith("https://www.google.com") ||
                    linkdata.startsWith("http://www.youtube.com") ||
                    linkdata.startsWith("https://www.youtube.com") ||
                    linkdata.startsWith("javascript:") ||
                    event.target.className.indexOf('overlayContainer') >= 0) {
                    return;
                }
                event.preventDefault();
                // overwrite current entry in history to store the scroll position
                var stateData = {
                    path: window.location.href,
                    title: document.title,
                    scrollTop: $(window).scrollTop()
                };
                window.history.replaceState(stateData, document.title, window.location.href);
                // load new page
                stateData = {
                    path: window.location.href,
                    title: null,
                    scrollTop: 0
                };

                window.history.pushState(stateData, null, linkdata);

                // check to continous play radio online
                if (radio_online_playing) {
                    if ($('audio#radio_bottom_native_hls').length == 1) {
                        $('audio#radio_bottom_native_hls')[0].play();
                    } else if ($('audio#radio_bottom_html5').length == 1) {
                        $('audio#radio_bottom_html5')[0].player.play();
                    }
                } else {
                    getChannelDetail(rungetdata);
                }

                var title = $(this).attr("title");
                GotoLink(linkdata, title, stateData);
            });

        $(document).ready(function () {
