            $('footer').height($('footer').height() + 40);
        }

        var timwait = 0;

        function SetTitleMobile(time) {
            if (time > timwait && timwait != 0) {
                getChannelDetail(rungetdata);
            }
        }

        function ShowChannelTitle(data, index) {
            var hour = data.split("-")[0].split(":")[0];
            var minus = data.split("-")[0].split(":")[1];

            var scheduleHour = data.split("-")[1].split(":")[0];
            var scheduleMinus = data.split("-")[1].split(":")[1];

            var seconWait = scheduleHour * 60 + scheduleMinus * 1 - hour * 60 - minus * 1;
            timwait += seconWait * 60;
        }

        function getChannelDetail(index) {
            $.ajax({
                url: "/Chanel/getSchedule?id=" + index,
                type: "GET",
                error: function (response) {
                    console.log(response);
                },
                success: function (response) {
                    if (response != "" && response.length == 2) {
                        rungetdata = index;
                        ShowChannelTitle(response[1], index);
                        if (index == 1) {
                            playChannel = "Radio Online. 610Khz - " + response[0];
                        } else if (index == 2) {
                            playChannel = "Radio Online. 99,9Mhz - " + response[0];
                        } else {
                            playChannel = "Radio Online. 95,6Mhz - " + response[0];
                        }

                        $("#radioPlay .mejs__controls .mejs__time marquee").text(playChannel);
                    }
                }
            });
        }

