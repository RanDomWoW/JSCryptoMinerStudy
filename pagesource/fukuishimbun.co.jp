<script>
var setLazyLoad = function(img){
  img.lazyload({
    skip_invisible : false,
    effect : 'fadeIn',
    effect_speed: 300
  }) ;
}
  
var tabSet = function(el){
  el.find('.jp-news-tab li').on('click', function(){
    if($(this).not('current')){
      $(this).addClass('current').siblings('li').removeClass('current');
      var index = el.find('.jp-news-tab li').index(this);
      var content = el.find('.jp-news-list-tab .contents').eq(index);
      content.addClass('current').siblings('.contents').removeClass('current');
      var imgs = content.find('img');
      imgs.hide();
      imgs.each(function(){
        var imgpath = $(this).data('original');
        $(this).attr('src',imgpath);
        $(this).fadeIn('slow');
      });
    }
  });
}

var jpNewsLoad = function(el){
  var img = el.find( 'img.lazy' );
  setLazyLoad(img);
  tabSet(el);
}

var entertainLoaded = false
var jpNewsLoaded = [false,false,false];
$(window).scroll(function(){
  if (sct <= 1000) {
    return false;
  } else {
    var sct = $(window).scrollTop();

    if ((sct > 1000) && !entertainLoaded ){
      entertainLoaded = true;
      $('#entertain-block').load(
        '/list/ajax-module/entertain',
        function(){
          var img = $(this).find( 'img.lazy' );
          setLazyLoad(img);
          //entertainLoaded = true;
        }
      );
    }

    if (sct > 1500) {
      //jpNewsLoaded = true;
      $('.jp-news-block').each(function(i){
        if (jpNewsLoaded[i]) {
          return false;
        } else {
          jpNewsLoaded[i] = true;
          var el = $(this);
          var url = '/list/ajax-module/' + el.attr('id');
          el.load(
            url,
            function(){
              jpNewsLoad(el);
              //jpNewsLoaded = true;
            }
          );
        }
      });
    }
  }
});

$(function(){

  $('.fukui-news-block').each(function(){
    var el = $(this);
    var url = '/list/ajax-module/' + el.attr('id');
    el.load(
      url,
      function(){
        var img = $(this).find( 'img.lazy' );
        setLazyLoad(img);
      }
    );
  });

});
</script>

