#sp-header .close-offcanvas {
    position: absolute;
    top: 0;
    right: 0;
    width: 60px;
    height: 50px;
    line-height: 50px;
    z-index: 1;
    color: #fff;
    text-align: center;
    font-size: 22px;
    background: #000
}

#sp-header .close-offcanvas:before,
#sp-header .close-offcanvas:after {
    content: '';
    position: absolute;
    width: 3px;
    height: 40%;
    top: 15px;
    left: 50%;
    background: #fff
}

#sp-header .close-offcanvas:before {
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg)
}

#sp-header .close-offcanvas:after {
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg)
}

#sp-header .close-offcanvas i {
    display: none
}

#sp-header .close-offcanvas:hover {
    background: rgba(0, 0, 0, 0.5);
    color: #fff
}

