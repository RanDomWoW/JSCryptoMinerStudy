				if ($(this).scrollTop() > 100)
				{
					$('#scroll-to-top').fadeIn();
				}
				else
				{
					$('#scroll-to-top').fadeOut();
				}
			
			}, 500);
			
		});
		
		$('#scroll-to-top').click(function(){
			$("html, body").animate({scrollTop: 0}, 600, "easeOutCubic");
			return false;
		});
		
		
		
		//Products grid: item hover effect
        var startHeight;
        var bpad;
        $('.category-products-grid').on('mouseenter', '.item', function() {
            if ($(window).width() >= 640)
            {
                startHeight = $(this).height();
                $(this).css("height", "auto"); //Release height
                $(this).find(".display-onhover").fadeIn(400, "easeOutCubic"); //Show elements visible on hover
                var h2 = $(this).height();

                //Compare start height with on-hover height, calculate the difference
                ////////////////////////////////////////////////////////////////
                var addtocartHeight = 0;
                var addtolinksHeight = 0;

                var diff = 0;
                if (h2 < startHeight)
                {
                    $(this).height(startHeight);
                }
                else
                {
                    $(this).height(h2);
                    diff = h2 - startHeight;
                }
                ////////////////////////////////////////////////////////////////

                $(this).css("margin-bottom", "-" + diff + "px"); //Apply difference as nagative margin
            }
        }).on('mouseleave', '.item', function() {
            if ($(window).width() >= 640) {
                //Clean up
                $(this).find(".display-onhover").stop(true).hide();
                $(this).css("margin-bottom", "");

                //Return the default height. If "Egual Height" disabled, unset item's height.
                $(this).height(startHeight);
            }
        });

		//Display alternative image
		$('.products-grid, .products-list').on('mouseenter', '.item', function() {
			$(this).find(".alt-img").fadeIn(400, "easeOutCubic");
		}).on('mouseleave', '.item', function() {
			$(this).find(".alt-img").stop(true).fadeOut(400, "easeOutCubic");
		});

		//Fade on hover
		$('.fade-on-hover').on('mouseenter', function() {
			$(this).animate({opacity: 0.75}, 300, 'easeInOutCubic');
		}).on('mouseleave', function() {
			$(this).stop(true).animate({opacity: 1}, 300, 'easeInOutCubic');
		});

		//Event: resize
		//Window size variables
		var winWidth = $(window).width();
		var winHeight = $(window).height();
		$(window).resize(
			$.debounce(50, onEventResize)
		); //end: resize

		//Execute on resize
		function onEventResize() {
			//Prevent from executing the code in IE when the window wasn't actually resized
			var winNewWidth = $(window).width();
			var winNewHeight = $(window).height();
			if (winWidth != winNewWidth || winHeight != winNewHeight)
			{
				afterResize(); //Executed if window was actually resized
			}
			//Update window size variables
			winWidth = winNewWidth;
			winHeight = winNewHeight;
		} //end: onEventResize

		//Execute if window was actually resized
		function afterResize() {

			//Trigger themeResize event
			$(document).trigger("themeResize");

			//Itemsliders: rollback and refresh //TODO: remove flexslider references
			$('.itemslider').each(function(index) {
				var flex = $(this).data('flexslider');
				if (flex != null)
				{
					flex.flexAnimate(0);
					flex.resize();
				}
			});
							
			//Slideshow: refresh
			var slideshow = $('.the-slideshow').data('flexslider');
			if (slideshow != null)
			{
				slideshow.resize();
			}
		}
	});

    
