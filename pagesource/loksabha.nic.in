    <script type="text/javascript">

        function getCurrentIST() {
            var dte = new Date();
            dte.setTime(dte.getTime() + (dte.getTimezoneOffset() + 330) * 60 * 1000);
            nhour1 = dte.getHours();
            nmin1 = dte.getMinutes();
            if (nhour1 == 0) { ap1 = " AM"; nhour1 = 12; }
            else if (nhour1 <= 11) { ap1 = " AM"; }
            else if (nhour1 == 12) { ap1 = " PM"; }
            else if (nhour1 >= 13) { ap1 = " PM"; nhour1 -= 12; }
            if (nmin1 <= 9) { nmin1 = "0" + nmin1; }
            document.getElementById('clockbox1').innerHTML = "" + nhour1 + ":" + nmin1 + ap1 + "";
            setTimeout("getCurrentIST()", 1000);
        }

        function GetClock() {
            d = new Date();
            nhour = d.getHours();
            nmin = d.getMinutes();
            if (nhour == 0) { ap = " AM"; nhour = 12; }
            else if (nhour <= 11) { ap = " AM"; }
            else if (nhour == 12) { ap = " PM"; }
            else if (nhour >= 13) { ap = " PM"; nhour -= 12; }
            if (nmin <= 9) { nmin = "0" + nmin; }
            document.getElementById('clockbox2').innerHTML = "" + nhour + ":" + nmin + ap + "";
            setTimeout("getCurrentIST()", 1000);
            setTimeout("GetClock()", 1000);
        }
        window.onload = GetClock;
        
    </script>
