                                        <i v-else-if="notification.object_type === 'DEPOSIT_REMINDING'" class="fa fa-star"></i>
                                        <i v-else-if="notification.object_type === 'GLOBAL_NOTIFICATION'" class="fa fa-bullhorn"></i>
                                        <i v-else class="fa fa-comment-o"></i>
