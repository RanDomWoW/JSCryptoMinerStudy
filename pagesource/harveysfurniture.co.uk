.accordian * {
    margin: 0; 
    padding: 0;
}

.accordian *, .accordian *::before, .accordian *::after {
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

/*
Width of image = 777px
total images = 4
so width of hovered image = 777px
width of un-hovered image = 66px - you can set this to anything
so total container width = 777 + 66*3 = 975px;
default width = 940/4 = 244px;
*/

.accordian {
    width: 975px; height: 510px;
    overflow: hidden;
    margin: 0 auto;
    -webkit-box-shadow: 0 0 10px 1px rgba(0, 0, 0, 0.35);
    -moz-box-shadow: 0 0 10px 1px rgba(0, 0, 0, 0.35);
    box-shadow: 0 0 10px 1px rgba(0, 0, 0, 0.35);
}

/*A small hack to prevent flickering on some browsers*/
.accordian ul {
    width: 2000px;
    /*This will give ample space to the last item to move
    instead of falling down/flickering during hovers.*/
}

.accordian li:first-child {
    border: 0;
}

.accordian li {
    position: relative;
    display: block;
    width: 243px;
    float: left;

    /*Transitions to give animation effect*/
    
    -webkit-transition: all 0.5s;
    -moz-transition: all 0.5s;
    -ms-transition: all 0.5s;
    -0-transition: all 0.5s;
    transition: all 0.5s;
    /*If you hover on the images now you should be able to 
    see the basic accordian*/
}

/*Reduce with of un-hovered elements*/
.accordian ul.active li {width: 66px;}

/*The LI hover style should override the UL hover style*/
.accordian ul li.open {width: 777px;}


.collections-btn-small-open, .collections-btn-primary, .collections-btn-small-close {
    display: none;
}

.accordian ul.active li .collections-subtitle {
  display: none;
}
.accordian ul.active li .collections-btn-small-open {
  display: block;
}
.accordian ul.active li .logo-container img {
    width: 64px;
    margin-left: 5px;
