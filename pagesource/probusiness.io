<script>
$( document ).ready(function() {
        if ($.cookie('hide_subscribe_popup') != 1 && (typeof ($('.b-sbscr-popup')) !== "undefined") && (!$('.b-sbscr-popup').hasClass('closed')) ){
                 var timer,
                          contentHeight = $('body').height();
                 $(window).scroll(function() {
                         if ((!$('.b-sbscr-popup').hasClass('closed')))  {
                                if(timer) {
                                         window.clearTimeout(timer);
                                }
                                timer = window.setTimeout(function() {
                                         if($(window).scrollTop() > (0.5 * contentHeight)) {
                                                  $('.b-sbscr-popup').css('opacity', 1);
                                         } else {
                                                  $('.b-sbscr-popup').css('opacity', 0);
                                         }
                                }, 200);
                         }
                 });
        }
        $('.b-sbscr-popup .b-sbscr .i-close_popup').on('click', function(){
                 $('.b-sbscr-popup').css('opacity', 0);
                 $('.b-sbscr-popup').addClass('closed');
                 var date = new Date();
                 date.setTime(date.getTime() + (60 * 24 * 60 * 60 * 1000)); // 60 суток
                 $.cookie('hide_subscribe_popup', 1, { path: '/', expires: date });
                 return false;
        });
});
function CheckFieldsSub(f) {
        var correct = true;
        var email = $("input[name='email']", f).val();
        if(email && typeof(email)!="undefined") {
            email = email.trim();
            var pattern = /^([a-z0-9_\.-])+@[a-z0-9-]+\.([a-z0-9]{2,4}\.)?[a-z]{2,4}$/i;
            if(!pattern.test(email)){
                $('.sbscr-form').addClass('error');
                $('.sbscr-form-error').removeClass('hidden');
                correct = false;
            }
        }
       
        if(correct) {
           // f.submit();
        }
        return false;
}
$(document).on("submit", ".sbscr-form form", function(event)
{
    event.preventDefault();  
    var url = window.location.href;
    $.ajax({
        type: "POST",
        url: url,
        data: $(this).serialize(),
        success: function(data)
            {
                if(data == "OK") {
                    $('.sbscr-form').remove();
                    $('.sbscr-title').html('Вы подписаны! Ждите новостей.');
                }
            }
        });   
    return false;
});
</script>
