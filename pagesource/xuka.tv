		function theater(thea){
			if (thea==0){
				if (w<800){
					$('body').addClass("mini theater");
					screen_type=-1;
				}else if (w < 1320) {
					$('body').addClass("small theater");
					screen_type=0;
				} else if (w < 1400) {
					$('body').addClass("medium theater");
					screen_type=1;
				} else {
					$('body').addClass("large full theater");
					screen_type=2;
				}
			}else{
				if (w<800){
					$('body').addClass("mini");
					screen_type=-1;
				}else if (w < 1320) {
					$('body').addClass("small");
					screen_type=0;
				} else if (w < 1400) {
					$('body').addClass("medium");
					screen_type=1;
				} else {
					$('body').addClass("large full");
					screen_type=2;
				}
			}
		}
 
