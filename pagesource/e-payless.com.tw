        $(function () {
            //導覽區。
            var goto;
            $('#banner-nav li').bind('mouseenter', function () {
                var index = $(this).attr('index');
                var length = $('#banner-nav li').not('.slick-cloned').length;
                //如果停一秒了，則goto。
                goto = setTimeout(function () {
                    if (index > length - 1)
                        index = index - length;
                    $('#banner-nav ul').slickGoTo(index);
                }, 600);
            });

            $('#banner-nav li').bind('mouseleave', function () {
                clearTimeout(goto);
            });
        });
    </script>
