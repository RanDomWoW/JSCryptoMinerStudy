<script>

    $(function () {
        $("#txtKey").autocomplete({
            delay: 500,
            minLength: 1,
            source: '/SampoECHeader/keyWordsSearchAutoComplete',
            appendTo: '.input-append',
            select: function (event, ui) {
                $("#txtKey").val(ui.item.label);
                setLocation(ui.item.searchurl);
                return false;
            }
        }).data("ui-autocomplete")._renderItem = function (ul, item) {
            var t = item.label;
            //html encode
            t = htmlEncode(t);
            return $("<li class=\"list-group-item\"></li>")
                 .data("item.autocomplete", item)
                 .append("<a class=\"generalImg\"><span>" + t + "</span></a>")
            .appendTo(ul);
            alert(t);
        }
    });


    function setLocation(url) {
        window.location.href = url;
    }

    function enter(event) {
        if (event.keyCode == 13) //判断按下的是否是回车键
        {
            $("#btnSearch").click(); //按回车激发搜索按钮事件
        }
    }
    $(function () {
        $('#btnSearch').on('click', function (e) {
            e.preventDefault();
            var key = $("#txtKey").val();
            window.location = $("#btnSearch").attr("href") + escape(key) + "&isKey=yes";
            //SetCookie("search","search");
        });
    });
</script>
