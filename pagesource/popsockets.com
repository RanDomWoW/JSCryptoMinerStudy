<script>
    function upsellClose(){
        $('#upsell-modal').hide();
        location.href = '/checkout';
    }
    $(function(){
        console.log("Function called");
        $('#upsell-modal .upsell-closer').click(function(){
          upsellClose();
        });
        $("#upsell-modal .slick-current img").addClass('active');
            $('#upsell-modal .image-gallery img').click(function(){
                var imgSrc = $(this).attr('src');
                $('#upsell-modal .featured-image img').attr('src',imgSrc);
              	$("#upsell-modal .slick-current img").removeClass('active');
                $("#upsell-modal .image-gallery img").removeClass('active');
                $(this).addClass('active');
        });
        
        $('.cart-flyout').on('click','.cart-flyout__totals-checkout__button',function(e){
            if(!getCookie('mountUpsell')){
                e.preventDefault();
                setCookie('mountUpsell', true);
                $('#upsell-modal').css('display','flex');
                $(".center").slick({
                    dots: false,
                    arrow: true,
                    infinite: true,
                    slidesToShow: 3,
                    slidesToScroll: 1
                });
                quantitychange();
            }
        });
        
        

    });

    function quantitychange(){
      	$('.product-cta__quantity-increment, .product-cta__quantity-decrement').unbind().click(function (e) {
			//alert(0);
            e.preventDefault();
            var $input = $(this).siblings('.product-cta__quantity-input');
            $input.val(Math.max(1, +$input.val() + ($(this).is('.product-cta__quantity-increment') ? 1 : -1)));
            $('#popmount-price').text('$'+ 10 * $input.val() + '.00');
        });


       $('.product-cta__quantity-input').keyup(function () {
             $('#popmount-price').text('$'+ 10 * $('.product-cta__quantity-input').val() + '.00');
        });
        $('.product-cta__quantity-input').keypress(validateNumber);


        function validateNumber(e) {
          if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
              return false;
          }
        };
      }


</script>





