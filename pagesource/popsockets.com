<script>

    $(function(){
        $(".mobile-nav__link__expandable__heading").click(function(){
          var ht = $(this).next(".submenu").children().height();
          if (ht != 0){
            ht = ht + 20;
          }
          if($(this).next(".submenu").height() > 0) {
            $(this).next(".submenu").css( "height", 0 );
          } else {
            $(this).closest(".submenu").css("height", "auto");
            $(this).next(".submenu").css( "height", ht );
          }
        });
        //Remove the symbol if there is an img
        $(".mobile-nav__link__expandable__heading").each(function(){
          var hasChildImg = $(this).siblings(".submenu").find(".mobile-nav__link__img");
          if (hasChildImg.attr("class") == "mobile-nav__link__img"){
            $(this).find("i").css("display", "none");
          }
        });
        // For changing circle to minus, could use optimization
        $(".mobile-nav__link__expandable__heading").click(function(){
            var hasChildImg = $(this).siblings(".submenu").find(".mobile-nav__link__img");
          if (hasChildImg.attr("class") == "mobile-nav__link__img"){
             window.location.href = hasChildImg.find("a").attr('href');
          }
          $(this).children().toggleClass("fa-minus-circle");
        });

    });

