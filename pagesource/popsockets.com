	function cartlogic(e) {
        
        var event = e;

        if(!!event) {
		    e.preventDefault();
        }

		console.log('A');

		var $this = $(this);
        var url = $this.attr('href') || $this.attr('action');

        console.log($this.attr('href') || $this.attr('action'), url);

		$.ajax({
			type: $this.attr('method') || 'get',
			url: url,
			data: $this.serialize() || undefined,
			dataType: 'text',
		}).then(function(){
            return $.get({
                url: '/cart.js',
                dataType: 'json',
            }).then(function(cart) {
                console.log(cart);

                var orphansData = "";
                var isButton = false;
                var pdpURL = "";

                var customs = $.grep(cart.items, function(item, index) {
                    return item.properties && item.properties["_setid"];
                });

                console.log("Customs", customs);

                var countedSetIDs = customs.reduce(function (items, item) { 
                    if (item.properties["_setid"] in items) {
                        items[item.properties["_setid"]]++;
                    }
                    else {
                        items[item.properties["_setid"]] = 1;
                    }
                    return items;
                    }, 
                {});

                console.log("Freq", countedSetIDs);

                var orphans = $.grep(customs, function(item, index) {
                    return countedSetIDs[item.properties["_setid"]] < 3;
                }).map(function(item, index) { 

                    if(item.variant_options[0] == "Custom")
                    {
                        isButton = (!isButton) ? item.variant_options[0] == "Custom" : isButton;
                        pdpURL = (isButton) ? item.url : "";
                    }

                    console.log(index, isButton);
                    return item.key;
                }); 

                console.log("Orphans", orphans);


                $.each(orphans, function(index, key) {
                    console.log("index", index);
                    if(index > 0)
                    {
                        orphansData += "&";
                    }

                    orphansData += "updates[" + key + "]=0";

                });
                
                console.log(orphansData);

                if(isButton)
                {
                    $("#orphan-modal .pdp-link").attr("href", pdpURL);
                    $("#orphan-modal").css("display", "flex");
                }
                
                return $.post('/cart/update.js', orphansData, null, "text");

            });
        }).then(function () {
			console.log('B');
			$('#cart-flyout').load('/cart?view=flyout #cart-flyout > *', function () {
				var cart_count = +$('[data-cart-count]').attr('data-cart-count');

				// update cart quantities on the site here...

				$('.cart-flyout-toggle text').attr('font-size', cart_count > 99 ? 10 : 14);
				$('.cart-flyout-toggle tspan').attr('y', cart_count > 99 ? 12 : 14);
				$('.cart-flyout-toggle tspan').text(cart_count > 999 ? Math.floor(cart_count / 1000) + 'K' : cart_count);
			});

                    
            if(!!event) {
                $(document).trigger('cart:show');
            }

            if($this[0].className != 'upsell-form'){
        
            }else{
                location.href = '/checkout';
            }
		}, function () {
			console.log('cart issue');
		});
	}

	if ($('.template--cart').length === 0) {
		$(document).on('click', '[href^="/cart/"]', cartlogic);
		$(document).on('submit', '[action^="/cart/"]', cartlogic);
	}

	function lockScroll(panelIsVisible) {
		var $html = $("html"),
			$wrapper = $(".page-content-wrapper"),
			scrollTop = $(window).scrollTop();

		$html.toggleClass("scroll-lock", panelIsVisible);

		if(!panelIsVisible)
		{
			window.scrollTo(0, -parseInt($wrapper.css('top'), 10));
			$wrapper.css({
				top: ''
			});
		}
		else
		{
			$wrapper.css({
				top:  -(scrollTop)
			});
			window.scrollTo(0, parseInt($wrapper.css('top'), 10));
		}
	}

    cartlogic();
    warningModal("There's an error with your PopSocket.<br/>Click <a class='pdp-link' href=''>HERE</a> to add an updated PopSocket to your cart.<br/>Happy Pop Shopping!", "HANG ON A SECOND!");
});
