<script type="text/javascript">
    function SubmitNewsletterForm(button) {
        var form = $(button).closest('.newsletter-form');

        var emailValidationMessage = $(form).parent().parent().next('.row').children('div').children('#email-validation-message');
        var conflictError = $(form).parent().parent().next('.row').children('div').children('#newsletterConflict');
        var thankyouMsg = $(form).parent().parent().next('.row').children('div').children('#newsletterThankYou');
        var errorGroup = $(form).children('.form-group');
        var firstName = $(form).children('.form-group').children('#txtName').val();
        var lastName = "";
        var email = $(form).children('.form-group').children('#txtEmail').val();


        emailValidationMessage.hide();
