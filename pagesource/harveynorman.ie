<script type="text/javascript">
	function fn_sli_toggle_filters() {
		$('.sli_filter_input:checked').attr('readonly', 'readonly');
		fn_sli_filter_search();
	}

	function fn_sli_filter_search() {
		var data = $.extend({ }, $('form[name="search_form"]').serializeObject(), $(':input.sli_filter_input').serializeObject());
		$(':input.sli_filter_input').attr('disabled', 'disabled');
		$('.sli_slider').each(function() {
			if($(this).slider('values', 0) == $(this).slider('option', 'min') && $(this).slider('values', 1) == $(this).slider('option', 'max')) {
				delete data['sli_af['+$(this).data('facet-name')+']'];
			}
		});

		$.ceAjax(
			'request',
			'http://www.harveynorman.ie/index.php?dispatch=products.search',
			{
				callback:     function() {
					if($('#pagination_contents')) {
						$('html, body').animate({
							scrollTop: $('#pagination_contents').offset().top
						}, 1000);
					}
					// regenerate sliders
					$('.sli_slider').each(fn_sli_generate_sliders);
				},
				data:         data,
				cache:        false,
				save_history: true,
				result_ids:   'pagination_contents,sli_filters'
			}
		);
	}

	function fn_sli_adjust_slider_inputs(event, ui) {
		$('#'+$(this).data('facet-name')+'_slider_min').val(ui.values[0]);
		$('#'+$(this).data('facet-name')+'_slider_max').val(ui.values[1]);
		$('input[name="sli_af['+$(this).data('facet-name')+']"]').val('['+(ui.values[0] * 100)+','+(ui.values[1] * 100)+']');
	}

	function fn_sli_generate_sliders(i,o) {
		$(o).slider({
			range:   true,
			min:     $(o).data('range-min'),
			max:     $(o).data('range-max'),
			animate: true,
			step:    5,
			values:  [$(o).data('value-min'), $(o).data('value-max')],
			change:  fn_sli_adjust_slider_inputs,
			slide:   fn_sli_adjust_slider_inputs
		});

	}

	function fn_sli_fix_results(){
		$('.sli-search .hproduct, .sli-search .hproduct-col').each(function(){
			var rank = $(this).find('span.sli_rank').data('value');

			var href = $(this).find('a.product-title').attr('href');

			var n = href.indexOf('?');
			href = href.substring(0, n != -1 ? n : href.length);

			var newHref = href + '?r=' + rank;

			$.each(this.querySelectorAll('a[href="' + href + '"]'), function(){
				this.setAttribute('href', newHref);
			});
		});
	}

	$(function() {
		// sliders
		$('.sli_slider').each(fn_sli_generate_sliders);
		// filters
		
		$('#sli_filters').on('change', '.sli_filter_input', fn_sli_toggle_filters);

		$('#sli_filters').on('click', '.clear_sli_filters', function(e) {
			e.preventDefault();
			$('.sli_filter_input').removeAttr('checked');
			// clear price range
			$('.sli_slider').each(function() {
				$(this).slider('option', 'values', [$(this).data('range-min'), $(this).data('range-max')]);
			});
			fn_sli_filter_search();
		});

		$('.facet_inputs :input').change(function() {
			$('#facet_'+$(this).data('facet-name')).slider(
				'values',
				$(this).data('facet-input-type') == 'min' ? 0 : 1,
				$(this).val()
			);
			$('input[name="sli_af['+$(this).data('facet-name')+']"]').val(
				'['+(Number($(this).parent().find('input[data-facet-input-type="min"]').val()) * 100)+','+(Number($(this).parent().find('input[data-facet-input-type="max"]').val()) * 100)+']'
			);
		});

		$('#sli_filters').on('click', '.sli_filter_results', fn_sli_filter_search);

		$('#sli_filters').on('click', '.facet_list_toggle', function(e) {
			e.preventDefault();
			$(this).parents('div.sli_facet').find('.sli_facet_variant:gt('+$('#sli_filters').data('filter-limit')+')').toggle($(this).data('facet-action') == 'more');
			
			if($(this).data('facet-action') == 'more') {
				$(this).data('facet-action', 'less').html('Show Less');
			} else {
				$(this).data('facet-action', 'more').html('Show More');
			}
			return false;
		});

		fn_sli_fix_results();$.ceEvent('on', 'ce.ajaxdone', function(){fn_sli_fix_results();});
	});
