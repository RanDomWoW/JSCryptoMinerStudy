<script type="text/javascript">
jQuery(function($){
    $(document).ready(function(e) {
        $('ul.m_reser>li').click(function(e) {
            var $this = $(this);
            // if($this.hasClass('m01')) $this.find('ul').show();
            // else $this.find('.m_calendar').show();
            $this.toggleClass('active');
            // if($this.hasClass('active'))
        });
        $('ul.m_reser>li').mouseleave(function(e) {
            var $this = $(this);
            // if($this.hasClass('m01')) $(this).find('ul').fadeOut(200);
            // else $(this).find('.m_calendar').fadeOut(200);
            // $this.removeClass('active');
        });
        var $startPicker = $('#calendar-start');
        var $endPicker = $('#calendar-end');
        var $resType = $('.m_reser .m01 ul');
        $startPicker.datetimepicker({
            inline: true,
            // showTodayButton: true,
            locale: 'en',
            format: 'YYYY-MM-DD'
            // showHours: false
        });
        $endPicker.datetimepicker({
            inline: true,
            // showTodayButton: true,
            locale: 'en',
            format: 'YYYY-MM-DD'
            // showHours: false
        });
        $resType.find('li a').click(function(e){
            e.preventDefault();
            $startPicker.parent().parent().addClass('active');
            $resType.parent().removeClass('active');
            $resType.parent().find('>a').html($(this).html());
            return false;
        });
        $startPicker.on("dp.change", function (e) {
            $('input[name=signin]').val(e.date.format("YYYY-MM-DD"));
            var type = $resType.parent().find('>a').html().toLowerCase();
            
            if(type == 'reservation'){
                $resType.find('li a').click();
                return false;
            }
            if(type != 'room'){
                $endPicker.parent().parent().removeClass('active');
                $startPicker.parent().parent().removeClass('active');
                // $('#resr_form').submit();
            }else{
                $endPicker.data("DateTimePicker").minDate(e.date);
                $endPicker.parent().parent().addClass('active');
                // $startPicker.parent().parent().fadeOut(200);
            }
            
        });
        $endPicker.on("dp.change", function (e) {
            // $startPicker.data("DateTimePicker").maxDate(e.date);
            $endPicker.parent().parent().removeClass('active');
            $startPicker.parent().parent().removeClass('active');
            $('input[name=signout]').val(e.date.format("YYYY-MM-DD"));
            var type = $resType.parent().find('>a').html().toLowerCase();
            if(type == 'room'){
                // $('#resr_form').submit();
            }
        });
        var submit = function(e){
            e.preventDefault();
            var $form = $('#resr_form');
            var type = $resType.parent().find('>a').html().toLowerCase();
            
            if(type == 'wedding' || type == 'banquet' || type == 'room'){
                // $('input[name=mid]').val(type + 'reservation');
                $form.attr('action', type + 'reservation');
                $('input[name=error_return_url]').remove();
                $('input[name=mid]').remove();
                $('input[name=act]').remove();
                // $('input[name=act]').val('dispBoardWrite');
                $('input[name=vid]').remove();
                $form.submit();
                return true;
            }else{
                $resType.parent().addClass('active');
            }
            
            return false;
        }
        $('.m_reser .m04 a').click(submit);
        // $('#resr_form').submit(submit);
        $.supersized({
            slide_interval          :   5000,
            transition              :   1,
            transition_speed        :   700,
            slide_links             :   'blank',
            slides                  :   [
				{image : '/layouts/prima/images/main/ms_img0.jpg', title : '', thumb : '', url : ''},
                {image : '/layouts/prima/images/main/ms_img01.jpg', title : '', thumb : '', url : ''},
                {image : '/layouts/prima/images/main/ms_img02.jpg', title : '', thumb : '', url : ''},
                {image : '/layouts/prima/images/main/ms_img03.jpg', title : '', thumb : '', url : ''}
            ]
        });
    });
});
