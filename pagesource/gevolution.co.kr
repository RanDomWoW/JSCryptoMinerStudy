// Ajax 가져오기
function getScoreConts(objID, aType, strVal) {
	var arrRtnData;
	var strGetUrl = "";
	switch (aType) {
		case "apple_rank" : strGetUrl = "/score/get_mainapple_rank.asp";break;
		case "google_rank" : strGetUrl = "/score/get_maingoogle_rank.asp";break;
		case "score_recom" : strGetUrl = "/score/get_mainscore_rank.asp";break;
		case "review_conts" : strGetUrl = "/score/get_score_review.asp";break;
		default : return false;
	}

	$.ajax({
		type:"GET",
		url:strGetUrl,
		dataType:"html",	//xml,json,script,html 등
		data:strVal,
		success:function(rtnData){
			switch (aType) {
				case "apple_rank" :break;
				case "google_rank" :break;
				case "score_recom" :break;
				case "review_conts" :break;
			}
			$("#"+objID).html(rtnData);
		}
	});
};

