        var addedPostLinks = ["http://www.genialetricks.de/nieren-fehler/","http://www.genialetricks.de/effiziente-haushaltstricks/","http://www.genialetricks.de/poren-verkleinern/","http://www.leckerschmecker.me/best-of-blumenkohl/","http://www.genialetricks.de/aufbewahrungs-tricks/","http://www.genialetricks.de/basteln-mit-pet/","http://www.genialetricks.de/wanddeko-tips/","http://www.genialetricks.de/fingernagel-form/","http://www.genialetricks.de/kuriose-gegenstaende/","http://www.genialetricks.de/depressionen-taktgefuehl/","http://www.genialetricks.de/haar-spray/","http://www.genialetricks.de/cleverer-buegeln/","http://www.genialetricks.de/katzen-suchfoto/","http://www.genialetricks.de/drei-fragen-intelligenz/","http://www.genialetricks.de/geheime-orte-2/","http://www.genialetricks.de/haaatschi/","http://www.genialetricks.de/fuer-perfektionisten/","http://www.genialetricks.de/hotelgeheimnisse/","http://www.genialetricks.de/fruehlings-deko/","http://www.genialetricks.de/tiger-balsam/","http://www.genialetricks.de/das-unbewohnbare-haus/","http://www.genialetricks.de/bussitzplatz/","http://www.genialetricks.de/dickes-haar/","http://www.genialetricks.de/nikitagolubev/","http://www.genialetricks.de/die-wunderwaffe-foehn/","http://www.genialetricks.de/zwei-eier-essen/"];

    	// Load new content 2x post before end but limit to [33px;333px] range
    	var loadOffset = Math.min(
    		Math.max(
    			33,
    			$( '.latest-block a:last-of-type' ).height() * 2 ),
    		333
    	);

    	var $win = $(window);
        var $csLoader = $(".js-cs-loader");
        var scrollBound = true;

    	function fetchNextFeed() {
            scrollBound = false;
            $win.off( "scroll",contScroll );
    		if ( feed.next == 0 ) {
    			return;
    		}
            $csLoader.show();

    		$.ajax({
                url: '/_lifehacks_json/' + feed.next + '.json',
    			dataType: 'json',
    			success: handleFeedUpdate,
                error: function(){
                    $csLoader.hide();
                }
    		});
    	}

    	function handleFeedUpdate(json) {
            $csLoader.hide();
    		feed = json;
    		oLength = feed.feed === undefined ? 0 : feed.feed.length;
	    	oI = 0;
            scrollBound = true;
	    	contScroll();
            $win.scroll( contScroll );
    	}

    	function contScroll() {
            if ( !scrollBound ) {
                return;
            }
			if ( $(document).height() - (window.innerHeight ? window.innerHeight : $win.height()) <= $win.scrollTop() + loadOffset ) {

				var lastIndex = Math.min( oI + chunkSize, oLength );

				var html = '';
                var changeCount = 0;

				for ( var i = oI; i < lastIndex; ++i ) {
    				var post = feed.feed[i];
                    if ( $.inArray( post.link, addedPostLinks ) != -1 ) {
                        continue;
                    } else {
                        addedPostLinks.push( post.link );
                    }

                    changeCount += 1;

                    var prettyDate = df.prettyDate(post.date);
                    if (post.date_line && post.date_line.length > 0){
                        prettyDate = df.stringFormat(post.date_line, prettyDate);
                    }

					html = html + '\
					<a class="left-img-block" href="' + post.link + '">\
