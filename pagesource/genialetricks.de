						<div class="left-img-block__text">\
							<div class="left-img-block__text__inner">\
                                <div class="top-img-block__img__overlay js-date" title="' + post.date + '">'+ prettyDate +'</div>\
								' + post.headline + '\
							</div>\
						</div>\
					</a>';
				}


				$( '.latest-block' ).append( html );
				//console.log('added ' + (lastIndex - oI));
				oI = lastIndex;

				if ( oI >= oLength ) {
					//console.log('need new items');
					fetchNextFeed();
				} else if ( changeCount === 0 ) {
                    contScroll();
                }
			}
    	}

