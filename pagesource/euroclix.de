<script>
    $(document).ready(function() {
        $( '.dropdown' ).hover(
            function(){
                $(this).children('.sub-menu').slideDown(100);
				$( '.country' ).addClass('down');
            },
            function(){
                $(this).children('.sub-menu').slideUp(100);
            }
        );
		$(document).on ("click", ".dropdown", function () {
            if($(".country").hasClass("down")) {
      			 $(this).children('.sub-menu').slideUp(100);
	  			 $( '.country' ).removeClass('down');
    		} else {
        		$(this).children('.sub-menu').slideDown(100);
				$( '.country' ).addClass('down');
    		}
		});
    });
</script>
