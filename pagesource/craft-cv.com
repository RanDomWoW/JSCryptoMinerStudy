	{
		if (email == 'magdalena.kowalska@gmail.com' || email == 'jane.smith@gmail.com') //REMOVE default email
		{
			email = '';
		}
	}
	$logBox.find('input[name=email]').val(  email  );


	$('.when_log_register').show();
	$('.when_register, .when_pass').hide(); 
	
	$('.when_log').hide();
	$('.when_register').css('display', 'block'); 		
	MODE = 'register';
	
	$('#log_shadow').fadeIn(300);	
	$logBox.css('top', y).css('left', x).fadeIn(400);	
}
