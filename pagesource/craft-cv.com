	$logBox.find('input[name=email]').val(  email  );


	$('.when_log, .when_log_register').show();
	$('.when_register, .when_pass').hide(); 
	MODE = 'log';
	
	$('#log_shadow').fadeIn(300);	
	$logBox.css('top', y).css('left', x).fadeIn(400);	
}
$('#log_shadow').click(function()
{
	$('#log_shadow, #log_box').fadeOut(200);
});
$('#log_box form').submit(function()
{
	if (MODE == 'log')
	{
		$.post('/' + LANG + '/ajax/login', $(this).serialize(), function(js)
		{
			if (!js.success)
			{
				if (js.fresh)
				{
					alert('Potwierdź swój adres e-mail - sprawdź swoją pocztę');				
				}
				else
				{
					alert('Sprawdź dane logowania');
				}
			}
			else
			{
				location.href = '/generator/?l=' + LANG;				
			}
		});	
	}
	else if (MODE == 'pass')
	{
		$.post('/' + LANG + '/ajax/reset', $(this).serialize(), function(js)
		{
			alert('Link do odzyskania hasła wysłaliśmy na Twój adres e-mail');
			//location.href += '';
			window.location.reload(false);
		});	
	}	
	else
	{
		$.post('/generator/ajax/register.php?l=' + LANG, $(this).serialize(), function(js)
		{
			if (!js.success)
			{
				alert(js.message);
			}
			else
			{
				if (js.promo)
				{
					var str = js.promo_txt;
					str = str.replace('7', js.promo_days);
					str = str.replace('starter', js.promo_pack);
					$('.promobox_body').html(str);

					$('#log_shadow, #log_box').hide();
					
					var y = $(window).scrollTop() + 150;
					$('#promobox').css('top', y);					
					$('#promobox_shadow, #promobox').fadeIn(200);
				}
				else
				{
					location.href = '/generator/?l=' + LANG;
				}
			}
		});
	}
	return false;
});
</script>
