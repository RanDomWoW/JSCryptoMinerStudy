function showSignPop()
{
	$logBox = $('#log_box');
	
	var y = $(window).scrollTop() + 150;
	var x = 0.5* ($(window).width() - $logBox.width());
	
	var email = $('#option_who input[name=email]').val();
	
	$logBox.find('input[name=name]').val(  $('#option_who input[name=name]').val()  );
	$logBox.find('input[name=surname]').val(  $('#option_who input[name=surname]').val()  );
	
