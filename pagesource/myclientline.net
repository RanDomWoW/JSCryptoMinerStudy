/*
 * modifies page elements based on the existence of the fdc-unbranded cookie
 * inserts appropriate stylesheet (class names reused)
 * keeping innerHTML insertion in each page in the event of page specific changes
 */
 function checkCookie() {
		// using referer as the content
		var unbranded = Cookie.get("fdc-unbranded");
		var link = document.createElement('link');
		link.rel = 'stylesheet';
		link.type = 'text/css';
		if (unbranded == "true"){
			// not allowing non branded users to main page; stylesheet seeting is moot 
			top.window.location=portalPageURL;
			link.href = '/style/styles_Business_Track.css';
			document.getElementsByTagName('head')[0].appendChild(link);
			document.getElementById("brand").innerHTML= "<div class='new-look'>Business Track<sup>&reg;</sup></div>";
			document.getElementById("header-text").innerHTML= "<div class='header-text bold'>The enhanced web tool for tracking, analyzing, and monitoring payment processing information</div>";
		    	
		} else {		
			link.href = '/style/styles.css';
			document.getElementsByTagName('head')[0].appendChild(link);
			document.getElementById("brand").innerHTML= "<div class='logo-ClientLine'>ClientLine<span>&reg;</span></div><div class='new-look'>Now Introducing Business Track</div>";
			document.getElementById("menu").innerHTML= "<div id='menu' class='horizontal-list'><ul><li class='last'><a href='/welcome.html'>Home</a></li></ul></div>";
			document.getElementById("header-text").innerHTML= "<div class='header-text'>The enhanced web tool for tracking, analyzing, and monitoring payment processing information</div>";
		    
		}
	}
</script>
