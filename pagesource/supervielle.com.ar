		function callModal()
		{
			setCookie('modal-home');
		}
		
		function setCookie(cname) 
		{
			var cookievalue = getCookie(cname);
			
			if (cookievalue == '') 
			{
				//It will create a cookie for 24hr (1 day) = 24*60*60 = 86400
			    	document.cookie = cname+ "=value; max-age=86400; path=/";
			    	//MODALITE
				Modalite.open('modal-home');
			}
		}
		
		//function getCookie -> return value of the key.       
		function getCookie(cname) 
		{
		    var name = cname + "=";
		    var ca = document.cookie.split(';');
		    for (var i = 0; i < ca.length; i++) 
		    {
		        var c = ca[i];
		        while (c.charAt(0) == ' ') c = c.substring(1);
		        if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
		    }
		    return "";
		}
	</script>
