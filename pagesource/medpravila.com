                                <button class="btn">Подписаться</button>
                            </div>
                        </form>
                    </figure>
                </div>
                <div class="b-modal__bg"></div>
            </div>
        </script>
        <div ng-include src="'/notifications/join-guest.html'"></div>
    <script type="text/ng-template" id="/popup.html">
        <div class="b-modal-aligner animated-background ng-cloak" ng-cloak ng-if="$root.showPopupMessage" ng-click="$root.showPopupMessage = false">
            <div class="b-modal b-modal--error">
                <figure class="b-modal__holder">
                    <div class="b-modal__content">
                        <div class="b-modal__text">[[ $root.showPopupMessage ]]</div>
                    </div>
                    <div class="b-modal__footer b-modal__footer--centered">
                        <span class="btn btn--error" ng-click="hideBlockPopup();">Закрыть</span>
                    </div>
                </figure>
            </div>
            <div class="b-modal__bg ng-cloak" ng-click="hideBlockPopup();"></div>
        </div>
    </script>

    <div ng-include src="'/popup.html'"></div>

    <script type="text/javascript">
        window.tz_detect_options = {
            offset: '',
            url: '/tz-detect/set/'
        };
    </script>
</body>
</html>