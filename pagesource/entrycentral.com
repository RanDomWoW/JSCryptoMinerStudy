        );

        jQuery(document).ready(function () {

            function protect_distance_limit() {
                $('#distance_limit')
                        .attr('type', 'text')
                        .val('')
                        .attr('placeholder', 'Please set your location...')
                        .attr('readonly', 'readonly');
            }

            function unprotect_distance_limit(){
                $('#distance_limit')
                        .attr('type', 'text')
                        //.val('')
                        .attr('placeholder', '')
                        .removeAttr('readonly', 'readonly');
            }

            if (localStorage.getItem('filter_postcode')){
                $("#postcode").val(localStorage.getItem('filter_postcode'));
            }
            if (localStorage.getItem('filter_longitude')){
                $browser_longitude = localStorage.getItem('filter_longitude');
            }
            if (localStorage.getItem('filter_latitude')){
                $browser_latitude = localStorage.getItem('filter_latitude');
            }

            var tableContainer = $('#eventTable');

            var local_distance_limit = "";
            if (localStorage.getItem("events_distance_limit")){
                local_distance_limit = localStorage.getItem("events_distance_limit");
            }
            $('#distance_limit').val(local_distance_limit);

            //
            var dataTable = tableContainer.dataTable({
                "aoColumns":[
                    {"bSortable": false},
                    {"bSortable": true, "sType": "eu_date"},
                    {"bSortable": true},
                    {"bSortable": true, "sType": "eu_date"},
                    {"bSortable": false, "bVisible": false},
                    {"bSortable": false, "bVisible": false},
                    {"bSortable": false, "bVisible": false},
                    {"bSortable": true, "bVisible": false}
                ],
                language: { search: "", searchPlaceholder: "Search events...", "lengthMenu": "_MENU_ rows"},
                responsive: true,
                bFilter: true,
                "pagingType": "full",
                "aaSorting": [[ 1, "asc" ]],
                "bStateSave": true,
                "fnStateSave": function (oSettings, oData) {
                    localStorage.setItem( 'EventsTableSettings', JSON.stringify(oData) );
                },
                "fnStateSaveParams": function (oSettings, oData) {
                    oData.abVisCols = "";
                },
                "fnStateLoad": function (oSettings) {
                    return JSON.parse( localStorage.getItem('EventsTableSettings') );
                }
            });

            var $browser_longitude;
            var $browser_latitude;

            function calculate_distances(){
                dataTable.DataTable().rows().every( function (rowIdx, tableLoop, rowLoop){
                    var rowData = this.data();
                    var lat = rowData[5];
                    var lon = rowData[4];
                    var distance = getDistanceFromLatLonInMiles(lat, lon, $browser_latitude,$browser_longitude);
                    rowData[7] = Math.floor(distance);
                    this.data(rowData);
                    this.invalidate();
                });
                dataTable.DataTable().column( 7 ).visible(true);
                dataTable.DataTable().draw();
            }

            function hide_distances(){
                dataTable.DataTable().column( 7 ).visible(false);
            }

            $(":checkbox").change(function(){
                $icon     = $(this).siblings(".fa");
                var checked   = $(this).is(":checked");
                $icon.toggleClass('fa-check-square-o', checked)
                        .toggleClass('fa-square-o', !checked);
            });

            $(".tickable").click(function(event){
                $checkbox = $(this).children(":checkbox");
                var check = !$checkbox.is(":checked");
                $checkbox.prop("checked", check);
                var sport_type = $checkbox.prop("id").split("_").pop(); // assumes only one underscore
                localStorage.setItem("events_" + sport_type + "_checked", (check ? "checked" : " "));
                dataTable.fnDraw();
            });

            var local_canisports_checked = "checked";
            if (localStorage.getItem("events_canisports_checked")){
                local_canisports_checked = localStorage.getItem("events_canisports_checked");
            }
            if (local_canisports_checked != "checked") {
                $('#check_canisports').parent(".tickable").click();
            }

            var local_cycling_checked = "checked";
            if (localStorage.getItem("events_cycling_checked")){
                local_cycling_checked = localStorage.getItem("events_cycling_checked");
            }
            if (local_cycling_checked != "checked") {
                $('#check_cycling').parent(".tickable").click();
            }

            var local_golf_checked = "checked";
            if (localStorage.getItem("events_golf_checked")){
                local_golf_checked = localStorage.getItem("events_golf_checked");
            }
            if (local_golf_checked != "checked") {
                $('#check_golf').parent(".tickable").click();
            }

            var local_motorsport_checked = "checked";
            if (localStorage.getItem("events_motorsport_checked")){
                local_motorsport_checked = localStorage.getItem("events_motorsport_checked");
            }
            if (local_motorsport_checked != "checked") {
                $('#check_motorsport').parent(".tickable").click();
            }

            var local_multisport_checked = "checked";
            if (localStorage.getItem("events_multisport_checked")){
                local_multisport_checked = localStorage.getItem("events_multisport_checked");
            }
            if (local_multisport_checked != "checked") {
                $('#check_multisport').parent(".tickable").click();
            }

            var local_run_checked = "checked";
            if (localStorage.getItem("events_run_checked")){
                local_run_checked = localStorage.getItem("events_run_checked");
            }
            if (local_run_checked != "checked") {
                $('#check_run').parent(".tickable").click();
            }

            var local_swim_checked = "checked";
            if (localStorage.getItem("events_swim_checked")){
                local_swim_checked = localStorage.getItem("events_swim_checked");
            }
            if (local_swim_checked != "checked") {
                $('#check_swim').parent(".tickable").click();
            }

            var local_other_checked = "checked";
            if (localStorage.getItem("events_other_checked")){
                local_other_checked = localStorage.getItem("events_other_checked");
            }
            if (local_other_checked != "checked") {
                $('#check_other').parent(".tickable").click();
            }

            // new item, if unset tick if 'Run' or 'Other' ticked
            var local_ocr_checked="checked";
            var tester = localStorage.getItem("events_ocr_checked");
            if (tester === undefined || tester === null || tester.length === 0){
                local_ocr_checked = (local_run_checked == "checked" || local_other_checked == "checked") ? "checked" : " ";
                localStorage.setItem("events_ocr_checked", local_ocr_checked);
            } else{
                local_ocr_checked = localStorage.getItem("events_ocr_checked");
            }
            if (local_ocr_checked != "checked") {
                $('#check_ocr').parent(".tickable").click();
            }

            $('#tableWrapper').removeClass('hide');
            $('#eventTable_loading').hide();
            dataTable.DataTable().columns.adjust().responsive.recalc();

            $(".numeric").keydown(function (e) {
                // Allow: backspace, delete, tab, escape, enter and .
                if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 190]) !== -1 ||
                    // Allow: Ctrl+A
                        (e.keyCode == 65 && e.ctrlKey === true) ||
                    // Allow: home, end, left, right
                        (e.keyCode >= 35 && e.keyCode <= 39)) {
                    // let it happen, don't do anything
                    return;
                }
                // Ensure that it is a number and stop the keypress
                if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                    e.preventDefault();
                }
            });

            $('#distance_limit').change( function() {
                dataTable.fnDraw();
                localStorage.events_distance_limit = this.value;
            });

            function unset_distance_filters(){
                localStorage.removeItem("filter_postcode");
                localStorage.removeItem("filter_longitude");
                localStorage.removeItem("filter_latitude");
                localStorage.removeItem("events_distance_limit");
                protect_distance_limit();
            }
            $('#request_gps').click(function(){
                if("geolocation" in navigator) {
                    navigator.geolocation.getCurrentPosition(function(position) {
                        $browser_longitude = position.coords.longitude;
                        $browser_latitude = position.coords.latitude;
                        //radius 1.5km
                        $.getJSON('https://api.postcodes.io/postcodes', { lon: $browser_longitude, lat: $browser_latitude, radius: 1500 } )
                                .done(function(data){
                                    if (data.status == 200 && data.result && data.result.length > 0){
                                        var filter_postcode = data.result[0].outcode + " " + data.result[0].incode;
                                        $('#postcode').val(filter_postcode);
                                        $('#postcode').closest('.form-group').removeClass('has-error').addClass('has-success');
                                        $('#postcode-hint').html('');
                                        localStorage.setItem("filter_postcode", filter_postcode);
                                    } else if (data.status == 200){
                                        $('#postcode').val('');
                                        localStorage.removeItem("filter_postcode");
                                        $('#postcode-hint').html('Success - Location found, but no UK postcode nearby. Your browser said you are <a target="_blank" href=http://maps.google.com/?q=' + $browser_latitude + ',' + $browser_longitude + '>here</a>');
                                    }
                        });
                        // we can work with geoposition but no postcode
                        localStorage.setItem("filter_longitude", $browser_longitude);
                        localStorage.setItem("filter_latitude", $browser_latitude);
                        calculate_distances();
                        unprotect_distance_limit();
                    }, function(error){
                        var message;
                        switch(error.code) {
                            case error.PERMISSION_DENIED:
                                message = "User denied the request for Geolocation.";
                                break;
                            case error.POSITION_UNAVAILABLE:
                                message = "Location information is unavailable.";
                                break;
                            case error.TIMEOUT:
                                message = "The request to get user location timed out.";
                                break;
                            case error.UNKNOWN_ERROR:
                                message = "An unknown error occurred.";
                                break;
                        }
                        $('#postcode').closest('.form-group').removeClass('has-success').addClass('has-error');
                        hide_distances();
                        $('#postcode-hint').html(message);
                        unset_distance_filters();
                    });
                } else {
                    $('#postcode').closest('.form-group').removeClass('has-success').addClass('has-error');
                    hide_distances();
                    unset_distance_filters();
                    $('#postcode-hint').html("Your geolocation data is not available, please set a postcode.");
                }
                dataTable.fnDraw();
            });

            $('#postcode').change(function() {
                // validate entered
                var entered_val = $('#postcode')[0].value.trim();
                if (entered_val.trim() == ""){
                    hide_distances();
                    $('#postcode').closest('.form-group').removeClass('has-error').removeClass('has-success');
                    unset_distance_filters();
                }
                else if (entered_val.indexOf(" ") >= 0){
                    $.getJSON('https://api.postcodes.io/postcodes/' + entered_val )
                            .done(function(data){
                                localStorage.setItem("filter_postcode", entered_val);
                                $browser_latitude = data.result.latitude;
                                $browser_longitude = data.result.longitude;
                                localStorage.setItem("filter_longitude", $browser_longitude);
                                localStorage.setItem("filter_latitude", $browser_latitude);
                                calculate_distances();
                                $('#postcode').closest('.form-group').removeClass('has-error').addClass('has-success');
                                $('#postcode-hint').html('');
                                unprotect_distance_limit();
                            })
                            .fail(function( jqxhr, textStatus, error ){
                                $('#postcode').closest('.form-group').removeClass('has-success').addClass('has-error');
                                $('#postcode-hint').html(error);
                                hide_distances();
                                unset_distance_filters();
                            });
                } else {
                    $.getJSON('https://api.postcodes.io/outcodes/' + entered_val)
                            .done(function (data) {
                                localStorage.setItem("filter_postcode", entered_val);
                                $browser_latitude = data.result.latitude;
                                $browser_longitude = data.result.longitude;
                                localStorage.setItem("filter_longitude", $browser_longitude);
                                localStorage.setItem("filter_latitude", $browser_latitude);
                                $('#postcode').closest('.form-group').removeClass('has-error').addClass('has-success');
                                $('#postcode-hint').html('');
                                calculate_distances();
                                unprotect_distance_limit();
                            })
                            .fail(function( jqxhr, textStatus, error ){
                                $('#postcode').closest('.form-group').removeClass('has-success').addClass('has-error');
                                $('#postcode-hint').html('Postcode not found');
                                unset_distance_filters();
                                hide_distances();
                            });
                }
                dataTable.DataTable().draw();
                $('#distance_limit').change();
            }).change();

        });
    </script>
</body>
</html>