<script>
    var sSearchBaseUrl = '/turneringer_og_resultater/resultatsoegning/';

    defer(function () {
        var j$ = jQuery.noConflict();
    });

    function SearchClubACDownEvent(evt) {
        var keyCode;
        if (evt.keyCode > 0) { keyCode = evt.keyCode; } else if (typeof (evt.charCode) != "undefinded") { keyCode = evt.charCode; }
        
        if (keyCode == 13)
        {
            evt.preventDefault();
            SearchClub();
        }
    }

    function SearchClubAC(evt) {

        //e = e || window.event;
        var keyCode;
        if (evt.keyCode > 0) { keyCode = evt.keyCode; } else if (typeof (evt.charCode) != "undefinded") { keyCode = evt.charCode;}

        //switch (e.keyCode) {
        switch (keyCode) {
            case 13:
                SearchClub();
            case 37:
                break;
            case 38:
                break;
            case 39: 
                break;
            case 40:
                break;
            default:
                var IsTest = getParameterByName('IsTest');
            
                if (IsTest == '1') {

                    var oClubName = document.getElementById('txtSearchClub');
                    if ((oClubName.value).length > 2) {
                    
                        document.getElementById('clubsearchloader').style.display = 'block';
                    
                        var target = j$(e.target).siblings(".ac-results");
                        target.html("");

                
                        j$.getJSON('/Layouts/Integration/SR/SearchLoadHandler.ashx' + '?Club=' + oClubName.value, function (clubs) {
                            j$.each(clubs, function() {
                                document.getElementById('clubsearchloader').style.display = 'block';
                                var sClubName = this['Name'];
                                var sCoopName = this['Coop'];
                                var sTerm = oClubName.value;

                                sTerm = sTerm.replace(/(\s+)/, "(<[^>]+>)*$1(<[^>]+>)*");
                                var sPattern = new RegExp("(" + sTerm + ")", "gi");

                                sClubName = sClubName.replace(sPattern, "<b>$1</b>");
                                sCoopName = sCoopName.replace(sPattern, "<b>$1</b>");

                                //add search results
                                var sClub = "<div style='width: 25px; float: left; height: 25px;'>"
                                if (this['Logo'] == '')
                                    sClub = sClub + "&nbsp;</div><div style='float: left;'>" + sClubName + "</div>"
                                else
                                    sClub = sClub + "<img style='padding-top: 2px;' width='20' src='" + this['Logo'] + "'>&nbsp;</div><div style='float: left;'>" + sClubName + "</div>"

                                if (this['Coop'] != '')
                                    sClub = sClub + "<div style='float: right; font-size: 10px;'>(Samarb.: " + sCoopName + ")</div>"

                                sClub = sClub + "</div>"
                        
                                var sResult = j$("<div class='ac-result' onclick='GoToClub(this)' data-id='" + this['ID'] + "' style='clear: both;cursor: pointer;'>&nbsp;" + sClub + "</div>");

                                j$(target).append(sResult);
                                document.getElementById('clubsearchloader').style.display = 'none';
                            });
                        });
                
                        var html = j$(target).html();
                    }
                    else {
                        j$(e.target).siblings(".ac-results").html("");
                    }
                }
            }      
            return false;
    }

    function GoToClub(o)
    {        
        window.location = '/turneringer_og_resultater/resultatsoegning//clubTeam.aspx?clubId=' + o.dataset['id'];
    }

    function SearchTypeChanged(searchType) {
        HideAllSearchTypes();
        switch (searchType.value) {
            case '1':
                document.getElementById('divSearchRow').style.display = 'block';
                break;
            case '2':
                document.getElementById('divSearchClub').style.display = 'block';
                break;
            case '3':
                document.getElementById('divSearchStadium').style.display = 'block';
                break;
            case '4':
                document.getElementById('divSearchGame').style.display = 'block';
                break;
        }
    }

    function HideAllSearchTypes() {
        document.getElementById('divSearchRow').style.display = 'none';
        document.getElementById('divSearchStadium').style.display = 'none';
        document.getElementById('divSearchClub').style.display = 'none';
        document.getElementById('divSearchGame').style.display = 'none';
    }

    function SearchClub() {
        var oClubName = document.getElementById('txtSearchClub');

        if (oClubName.value == '') {
            alert("Du skal indtaste et klub/hold navn.");
            oClubName.focus();
            return false;
        } else {
            window.location.href = sSearchBaseUrl + 'search.aspx?clubname=' + oClubName.value;
            return false;
        }
    }

    function SearchStadium() {
        var oStadiumName = document.getElementById('txtSearchStadium');

        if (oStadiumName.value == '') {
            alert("Du skal indtaste navnet pÃ¥ et spillested.");
            oStadiumName.focus();
            return false;
        } else {
            window.location.href = sSearchBaseUrl + 'search.aspx?stadiumname=' + oStadiumName.value;
            return false;
        }
    }

    function SearchMatch() {
        var oMatchNumber = document.getElementById('txtSearchGame');

        if (oMatchNumber.value == '') {
            alert("Du skal indtaste et kampnummer pÃ¥ 6 cifre.");
            oMatchNumber.focus();
            return false;
        }
        if (!oMatchNumber.value.match(/^\d{6}$/)) {
            alert("Du skal indtaste et gyldigt kampnummer pÃ¥ 6 cifre.");
            oMatchNumber.focus();
            return false;
        }
        else {
            window.location.href = sSearchBaseUrl + 'search.aspx?matchnumber=' + oMatchNumber.value;
            return false;
        }
    }

    function SearchRow() {
        var sParam;

        sParam = "?unionid=" + document.getElementById("ph_resultssearch_0_ddlUnion").value;
        sParam += "&genderid=" + document.getElementById("ph_resultssearch_0_ddlGender").value;
        sParam += "&divisionid=" + document.getElementById("ph_resultssearch_0_ddlDivision").value;
        sParam += "&year=" + document.getElementById("ph_resultssearch_0_ddlSeason").value;

        window.location.href = sSearchBaseUrl + 'search.aspx' + sParam;
        return false;
