    function searchMapKeyPress(e) {
        e = e || window.event;
        if (e.keyCode == 13) {
            window.location.href = '/klubservice/Map/' + 'SearchClubOnMap.aspx?search=' + document.getElementById('txtClubMap').value;
            return false;
        }
        return true;
    }

    function searchClubByMap() {
        window.location.href = '/klubservice/Map/' + 'SearchClubOnMap.aspx?search=' + document.getElementById('txtClubMap').value;
        return false;
    }

    function getParameterByName(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
    }
