<script type="application/javascript">
    var yiModal = {modal: jQuery('#yi-modal')};
    yiModal.wait = function(message) {
        this.modal.show();
        jQuery('#message-main').html('');
        jQuery('#message-sub').html('');
        jQuery('#message-icon').removeClass().addClass('fa fa-refresh fa-spin');
    }
    yiModal.show = function(main, type, sub) {
        this.modal.show();
        jQuery('#message-main').html(main);
        jQuery('#message-sub').html(sub);
        var iconClass = '';
        if(type == 'success') {
            iconClass = 'success fa fa-check-circle-o';
        }
        else if(type == 'error') {
            iconClass = 'error fa fa-times-circle-o';
        }
        else if(type == 'warning') {
            iconClass = 'warning fa fa-lightbulb-o';
        }
        jQuery('#message-icon').removeClass().addClass(iconClass);
    }
    yiModal.hide = function(message) {
        this.modal.hide();
        jQuery('#message-main').html(message);
        jQuery('#message-sub').html('');
        jQuery('#message-icon').removeClass().addClass('fa-refresh fa-spin');
    }
</script>
                

