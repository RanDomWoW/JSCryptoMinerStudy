    },
    getSearchParams : function(url) {
       var params = {}
       var search = this.getSearch(url);
       if(search) {
          var paramsArr = search.split('?');
          var i=0,len = paramsArr.length;
          for(;i<len;i++) {
             var param = paramsArr[i].split('=');
             var k = param[0];
