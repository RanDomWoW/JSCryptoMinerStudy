				$(alert).on('click', 'button.ok', function(){
					$('#mask:eq(0)').remove();
				}).on('click', 'button.btn_ca_close', function(){
					$('#mask:eq(0)').remove();
				});
			} else {
				webIdDupIdCheck=false;

				var alert = messageBox(data.resultMessage);
				$(alert).on('click', 'button.ok', function(){
					$('#mask:eq(0)').remove();
				}).on('click', 'button.btn_ca_close', function(){
					$('#mask:eq(0)').remove();
				});
			}
		});
	}
	/**
	 * 웹아이디 validation check
	 * @param $webId
	 */
	function webIdRegisterValidation($webId) {
		if ($.trim($webId.val()) == '') {
			webIdDupIdCheck=false;

			var alert = messageBox('ID를 입력해주세요.');
			$(alert).on('click', 'button.ok', function(){
				$('#mask:eq(0)').remove();
			}).on('click', 'button.btn_ca_close', function(){
				$('#mask:eq(0)').remove();
			});
			return false;
		}

		if (!/^[a-zA-Z0-9]{8,12}$/g.test($webId.val())) {
			webIdDupIdCheck=false;

			var alert = messageBox('아이디는 숫자, 영문포함 8자리 이상만 가능합니다.');
			$(alert).on('click', 'button.ok', function(){
				$('#mask:eq(0)').remove();
			}).on('click', 'button.btn_ca_close', function(){
				$('#mask:eq(0)').remove();
			});
			return false;
		}

		return true;
	}

	/**
	 * 웹아이디 등록
	 */
	function webIdRegisterSave()
	{
		if(webIdRegisterCode.trim()=="" || webIdRegisterCi.trim()=="" || webIdRegisterMemberClass.trim()=="")
		{
			messageBox('시스템 오류로 재인증이 다시 하셔야 됩니다.',function(){
				location.reload(true);
			});
			return;
		}

		if(webIdRegisterIsSaveCheck) return;

		if(validate('#messageBox_IDregisterPopup')) {

			var $webId = $('#messageBox_IDregisterPopup input[name=webId]');
			var $webPwd = $('#messageBox_IDregisterPopup input[name=webPassword]');
			var $webPwd2 = $('#messageBox_IDregisterPopup input[name=webPassword2]');

			if (!webIdRegisterValidation($webId)) {
				webIdRegisterIsSaveCheck=false;
				return;
			} else {
				if (!webIdDupIdCheck) {
					var alert = messageBox('아이디 중복을 확인해주세요.');
					$(alert).on('click', 'button.ok', function(){
						$('#mask:eq(0)').remove();
					}).on('click', 'button.btn_ca_close', function(){
						$('#mask:eq(0)').remove();
					});

					webIdRegisterIsSaveCheck=false;
					return;
				}
			}

			if (!/((?=.{10,})(?=.*[0-9])(?=.*[a-zA-Z]).*$)|((?=.{10,})(?=.*[!@#$%^&*+=-])(?=.*[a-zA-Z]).*$)|((?=.{10,})(?=.*[!@#$%^&*+=-])(?=.*[0-9]).*$)/g.test($webPwd.val().trim())) {
				var alert = messageBox( "영문, 숫자, 특수문자 중 2가지 이상 조합 10자리 이상으로 입력 해 주세요.");
				$(alert).on('click', 'button.ok', function(){
					$('#mask:eq(0)').remove();
				}).on('click', 'button.btn_ca_close', function(){
					$('#mask:eq(0)').remove();
				});

				$('#messageBox_IDregisterPopup input[name=webPassword]').val('');
				$('#messageBox_IDregisterPopup input[name=webPassword2]').val('');
				$('#messageBox_IDregisterPopup input[name=webPassword]').focus();
				webIdRegisterIsSaveCheck=false;
				return;
			}

			// 비밀번호 재입력확인
			if ($webPwd.val().trim() != $webPwd2.val().trim()) {
				var alert = messageBox( "비밀번호 재입력을 다시 확인해주세요.");
				$(alert).on('click', 'button.ok', function(){
					$('#mask:eq(0)').remove();
				}).on('click', 'button.btn_ca_close', function(){
					$('#mask:eq(0)').remove();
				});

				$('#messageBox_IDregisterPopup input[name=webPassword2]').val('');
				$('#messageBox_IDregisterPopup input[name=webPassword2]').focus();
				webIdRegisterIsSaveCheck=false;
				return;
			}

			if ($webId.val().trim().indexOf($webPwd.val().trim()) > -1) {
				var alert = messageBox( "비밀번호는 아이디와 동일한 문자가 포함될 수 없습니다.\n다른 비밀번호를 입력해주세요!");
				$(alert).on('click', 'button.ok', function(){
					$('#mask:eq(0)').remove();
				}).on('click', 'button.btn_ca_close', function(){
					$('#mask:eq(0)').remove();
				});

				$('#messageBox_IDregisterPopup input[name=webPassword]').val('');
				$('#messageBox_IDregisterPopup input[name=webPassword2]').val('');
				$('#messageBox_IDregisterPopup input[name=webPassword]').focus();
				webIdRegisterIsSaveCheck=false;
				return;
			}

			webIdRegisterIsSaveCheck=true;

			var ajaxOptions={
					url: getServerUrl(true) + '/DataProvider',
					type: 'POST',
					async: false,
					data: {
						_command: 'User.registerWebIdAndPassword',
						memberCode: webIdRegisterCode,
						iPinCi: webIdRegisterCi,
						memberClass: webIdRegisterMemberClass,
						orgwebid:'',
						newWebId: $webId.val().trim(),
						newAuthKey: sha256_digest($webPwd.val().trim()),
						siteCode: '36'
					}
				};

			if(!lowBrowserVer)
			{
				ajaxOptions.crossDomain=true;
				ajaxOptions.xhrFields={withCredentials : true};
			}

			$.ajax(ajaxOptions).success(function( data ) {

				if (data.resultCode == '0000') {
					messageBox('등록이 완료 되었습니다.<br />등록하신 아이디로 로그인 하시기 바랍니다.',function(){
						location.href="/";
					});
				} else {
					webIdRegisterIsSaveCheck=false;
					messageBox(data.resultMessage);
				}
			});
		} else {
			webIdRegisterIsSaveCheck=false;
		}
	}

