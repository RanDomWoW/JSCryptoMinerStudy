	function authNoCheck()
	{
		if(reAuthData.reAuthGbn=="PHONE")
		{
			var authNo=$("#messageBox_authNoCheck input[name=reAuthNo]").val().trim();

			if(authNo=="")
			{
				messageBox('인증번호를 입력하세요.');
				return;
			}

			if(reAuthData.reAuthNo==authNo)
			{
				closeMessageBox('messageBox_authNoCheck', 'custom');
				reAuthSuccess(reAuthData.reAuthMemberCode);
			}
			else
			{
				messageBox('로그인', "재인증이 실패 하였습니다.<br />다시 재인증하여 주시기 바랍니다.", function(){
					closeMessageBox('messageBox_authNoCheck', 'custom');
					showReAuthPopup();
				});
			}
		}
		else
		{
			var authNo=$("#messageBox_mailAuthNoCheck input[name=reAuthNo]").val().trim();

			if(authNo=="")
			{
				messageBox('인증번호를 입력하세요.');
				return;
			}

			var ajaxOptions={
					url: getServerUrl(true) + '/DataProvider',
					type: 'POST',
					data: {
						_command: 'User.confirmMailCertification',
						certCode: authNo
					}
				};

			if(!lowBrowserVer)
			{
				ajaxOptions.crossDomain=true;
				ajaxOptions.xhrFields={withCredentials : true};
			}

			$.ajax(ajaxOptions).success(function( data ) {

				if(data.resultCode=="0000")
				{
