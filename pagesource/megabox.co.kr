<script type="text/javascript">
<!--//-->
function registerJoongAngMemberShip(resetYn) {
 	loginCheck(function( data ) {
		if (data.resultCode === 'success') {
			var mda = "N";
			var isChoice1 = $('#joongangVipPopup').find('#agree_vip3').is(':checked');
	 		var isChoice2   = $('#joongangVipPopup').find('#agree_vip2').is(':checked');
			var isRequired = $('#joongangVipPopup').find('#agree_vip1').is(':checked');

	 		if (!isChoice1) {
	 			messageBox("MEGABOX VIP 새로운 혜택1에 동의해주세요");
	 			return;
	 		}

	 		if (!isRequired) {
	 			messageBox("제3자 정보제공 내용에 동의해주세요");
	 			return;
	 		}

	 		if (isChoice2) {
	 			mda = "Y";
	 		}

	 		var registerTime = new Date();

			$.ajax({
				url : '/DataProvider',
				type : "POST",
				async : true,
				data : {
					_command : 'Membership.registerJoongangMemberShip',
					mda : mda,
					siteCode : "36",
					registerTime : registerTime.getTime()
				}
			}).success(function(data) {
				var message = data.resultMessage;
				if("A" == data.cardStatus || "R" == data.cardStatus){
					message = "중앙멤버십 기회원으로 이미 발급된 카드가 있습니다.<br/>중앙 멤버십으로 문의 부탁드립니다.<br/>중앙멤버십 고객 센터 : 02-2108-3456(평일 9~18시)";
				}
				messageBox(message, function(){
					$('#joongangVipPopup').modal("hide");
					if(!BookingSeatDatas.movieData.movieCode) loginUtil.executeCallBack();
				});
			});
		} else {
			messageBox("로그인 후 이용 가능합니다");
		}
	});
}

</script>
