	/**
	 * 로그인처리 함수
	 * @param loginDiv
	 * @param keepLogin
	 * @param fn_callback
	 */
	function loginProc(loginDiv, keepLogin, fn_callback){

		var inputId 	  = $('#' + loginDiv + ' input[name=userId]');
		var inputPw 	  = $('#' + loginDiv + ' input[name=userPw]');
		var userId 	 	  = $(inputId).val();
		var userPw 	 	  = $(inputPw).val();

		if (userId == "") {
			messageBox('아이디를 입력하세요.', function(){
				$(inputId).focus(function(){
					return false;
				});
			});
			return;
		}

		if (userPw == "") {
			messageBox('비밀번호를 입력하세요.', function(){
				$(inputPw).focus(function(){
					return false;
				});
			});
			return;
		}

		if (keepLogin != "Y") {
			keepLogin = "N";
		}

		if (Modernizr.canvas) {
			var ajaxOptions={
					url: getServerUrl(true) + '/DataProvider',
					type: 'POST',
					data: {
						_command: 'User.webIdLogin',
						web_Id: userId,
						pwd_Md5: '',
						pwd_Sha256: sha256_digest(userPw),
						keepLogin: keepLogin
					}
				};

			if (!lowBrowserVer) {
				ajaxOptions.crossDomain=true;
				ajaxOptions.xhrFields={withCredentials : true};
			}
			$.ajax(ajaxOptions).success(function( data ) {

				loginUtil.setLoginResultCode(data.resultCode);
				loginUtil.setLoginJVipPopupYN(data.jVipPopupYN);
				loginUtil.setCallBackFunc(fn_callback);

				if (data.resultCode == '0000') {
					customHideModal('#'+loginDiv);
					if(BookingSeatDatas.movieData.movieCode) loginUtil.executeCallBack();
					else checkJoongangVipPop(data.jVipPopupYN);
				} else if (data.resultCode == '7001') {	//비밀번호5회오류
					loginUtil.setLoginWebId(userId);
					customHideModal('#'+loginDiv);
					customMessageBox('messageBox_loginFail1');
					return;
				} else if (data.resultCode == '7002') {	//비밀번호변경 3개월초과
					customHideModal('#'+loginDiv);
					checkChangePassword(data.jVipPopupYN);
					return;
				} else if (data.resultCode == '8002') {	//비밀번호 초기화
					customHideModal('#'+loginDiv);
					customMessageBox('passwordInitPopup');
					return;
				} else if (data.resultCode == '8003') {	//장기미사용
					customHideModal('#'+loginDiv);
					customMessageBox('messageBox_loginFail2');
					return;
				} else if (data.resultCode == '8004') {	//마케팅 수신동의
					customHideModal('#'+loginDiv);
					checkMarketingAgree(data.jVipPopupYN);
					return;
				} else if (data.resultCode == '8009') {	//마케팅 수신 미동의
					customHideModal('#'+loginDiv);
					processMarketingNotAgree();
					return;
				} else if (data.resultCode == '0901') {
					customHideModal('#'+loginDiv);
					customMessageBox('no_symbol_popup');
		 			return;
				} else if (data.resultCode == '6150') { //휴면계정
					confirmBox('로그인', data.resultMessage, function(){
						checkDormantAccount();
					});
					return;
				} else {
					messageBox('로그인', data.resultMessage);
		 			return;
				}
			});
		} else {
			var hostNameValue=location.hostname;
			var message = userId + ';' + userPw + ';' + keepLogin + ';';
			$('#loginForIE8Frame')[0].contentWindow.postMessage(message, 'https://'+hostNameValue);
		}
	}
	/**
	 * 로그인완료 함수
	 */
	function completeLogin(fn_callback){
		$.ajax({
			url: '/DataProvider',
			type: 'POST',
			data: {
				_command: 'User.completeLogin'
			}
		}).success(function( data ) {
			if (data.resultCode == "0000") {
				if(fn_callback)
					fn_callback();
			} else {
				messageBox(data.resultMessage);
			}
		});
	}

	function checkChangePassword(jVipPopupYN){
		$.ajax({
			url: getServerUrl(true) + '/DataProvider',
			type: 'POST',
			crossDomain: true,
		      xhrFields: {
		        withCredentials: true
		      },
			data: {
				_command: 'User.getUserInfo'
			}
		}).success(function(response) {
			if (response.resultCode == '0000') {
				var info = response.info;
				if (info != null) {
					var changePwdDtm = info.changePwdDtm;
						var changePwdDtmArr = changePwdDtm.split(' ');
					$('#passwordChangePopup .changePasswordDate').text('비밀번호 변경일 : ' + info.changePwdDay + '일 전에 변경됨.(' + changePwdDtmArr[0] + ')');

					password_chg();
					$('#passwordChangePopup').on('click', 'button.no_chg', function(){
						if(jVipPopupYN == "Y") {
							checkJoongangVipPop(jVipPopupYN);
						} else {
							loginUtil.executeCallBack();
						}
					}).on('click', 'button.pop_close', function(){
						if(jVipPopupYN == "Y") {
							checkJoongangVipPop(jVipPopupYN);
						} else {
							loginUtil.executeCallBack();
						}
					});
					return;
				}
			} else {
				messageBox(response.resultMessage, function(){
					location.href = "/";
				});
				return;
			}
		});
	}

	function checkMarketingAgree(jVipPopupYN){
		$.ajax({
			url: '/DataProvider',
			type: 'POST',
			data: {
				_command: 'User.checkMarketingAgree'
			}
		}).success(function(response) {
			if (response.resultCode == '0000') {
				var receiveType = "";
				var receiveDtm  = "";
				if (response.smsReceiveYN == "Y" && response.smsReceiveDTM != null && response.smsReceiveDTM != undefined) {
					receiveType = "SMS   : " + response.smsReceiveDTM;
				}
				if (response.mailingReceiveYN == "Y" && response.mailingReceiveDTM != null && response.mailingReceiveDTM != undefined) {
					receiveDtm = "Email : " + response.mailingReceiveDTM;
				}
				$('#maketingChangePopup .marketingEmailSms').text(receiveType);
				$('#maketingChangePopup .marketingDate').text(receiveDtm);

				maketing_chg();
				$('#maketingChangePopup').on('click', 'button.no_chg', function(){
					if(jVipPopupYN == "Y") {
						checkJoongangVipPop(jVipPopupYN);
					} else {
						loginUtil.executeCallBack();
					}
				}).on('click', 'button.pop_close', function(){
					if(jVipPopupYN == "Y") {
						checkJoongangVipPop(jVipPopupYN);
					} else {
						loginUtil.executeCallBack();
					}
				});

			} else {
				messageBox(response.resultMessage, function(){
					location.href = "/";
				});
				return;
			}
		});
	}

