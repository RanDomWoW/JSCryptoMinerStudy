	function checkJoongangVipPop(jVipPopupYN){
		if ("Y" != getCookie("joongangVip") && jVipPopupYN == "Y") {
			joongangVipPop('Y');

			$("#joongangVipPopup.joongang_popup_wrap").find('#joongangVip_close').bind("click", function(){
				if ($("#joongangVipPopup.joongang_popup_wrap").find('#joongang_chk').is(":checked")) {
					setCookie('joongangVip', 'Y', 7);
				}
				if(Booking.loginFlag != 'Y') loginUtil.executeCallBack();
			});
		} else {
			if(Booking.loginFlag != 'Y') loginUtil.executeCallBack();
		}
	}

	function joongangVipPop(resetYn){
		var _pop = $('#joongangVipPopup');
		if (_pop.hasClass('in')){
			var resetYn = $('#joongangVipPopup').find('[name=resetYn]').val();
			_pop.modal('hide');
			if(resetYn && resetYn=='Y' && Booking.loginFlag != 'Y'){
				loginUtil.executeCallBack();
			}
		} else {

			if(resetYn && resetYn=='Y'){
				_pop.find("#joongang_chk_wrap").show();
			}else{
				_pop.find("#joongang_chk_wrap").hide();
			}
			_pop.find(".icheckbox_minimal").removeClass("checked");
			_pop.find(":checked").prop('checked', false);
			_pop.find('[name=resetYn]').val(resetYn);
			_pop.css("z-index", "2001");
			_pop.modal('show');

			$(".modal-backdrop.joongangVipPopup.in").css("z-index", "2000");
		}
	}

