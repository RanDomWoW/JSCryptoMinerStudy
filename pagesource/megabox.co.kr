	function storeOpenReceiptPrint(transNo, seqNo)
	{
		var frm = document.getElementById('frm_ReceiptPrint');
		frm.method = "post";
		frm.target = "popupReceipt";
		frm.action = "https://"+hostname+"/pages/common/Payment_ReceiptPrint.jsp";

		var transNoInput = document.createElement("input");
		transNoInput.setAttribute("type", "hidden");
		transNoInput.setAttribute("name", "transNo");
		transNoInput.setAttribute("id", "transNo");
		transNoInput.setAttribute("value", transNo);

		var seqNoInput = document.createElement("input");
		seqNoInput.setAttribute("type", "hidden");
		seqNoInput.setAttribute("name", "seqNo");
		seqNoInput.setAttribute("id", "seqNo");
		seqNoInput.setAttribute("value", seqNo);

		var tranTypeInput = document.createElement("input");
		tranTypeInput.setAttribute("type", "hidden");
		tranTypeInput.setAttribute("name", "tranType");
		tranTypeInput.setAttribute("id", "tranType");
		tranTypeInput.setAttribute("value", "S");

		frm.appendChild(transNoInput);
		frm.appendChild(seqNoInput);
		frm.appendChild(tranTypeInput);

		var Receipt = window.open('about:blank', 'popupReceipt', 'scrollbars=no, width=404, height=710');
		Receipt.focus();

		frm.submit();
	}

	/**
	 * 비밀번호 초기화
	 */
	function resetChangePwd()
	{
		var password 	= $('#passwordInitPopup input[name=password]').val();
		var cPassword 	= $('#passwordInitPopup input[name=cPassword]').val();

		if (password.trim()=="" || cPassword.trim()=="") {
			messageBox('비밀번호 변경', '비밀번호를 입력해 주세요.');
			return;
		}

		if (password !== cPassword) {
			messageBox('비밀번호 변경', '비밀번호 재입력을 다시 확인해주세요.');
			return;
		}

		if (!(/((?=.{10,})(?=.*[0-9])(?=.*[a-zA-Z]).*$)|((?=.{10,})(?=.*[!@#$%^&*+=-])(?=.*[a-zA-Z]).*$)|((?=.{10,})(?=.*[!@#$%^&*+=-])(?=.*[0-9]).*$)/g.test(password))) {

			$('span#passwordMessage').html('영문, 숫자, 특수문자 중 2가지 이상 조합<br>10자리 이상으로 입력 해 주세요.');

			return;
		}

		$.ajax({
			url: getServerUrl(true) + '/DataProvider',
			type: 'POST',
			crossDomain: true,
		      xhrFields: {
		        withCredentials: true
		      },
			data: {_command: 'User.resetPassword', passwd: sha256_digest(password)}
		}).success(function( data ) {
			if (data.resultCode == '0000') {
				closeMessageBox('passwordInitPopup', 'custom');
				messageBox('비밀번호 변경완료', data.resultMessage, function() {

					//logout();
					completeLogin(function(){
						loginUtil.executeCallBack();

					});
				});
			} else {
				closeMessageBox('passwordInitPopup', 'custom');
				messageBox(data.resultMessage, function(){
					customMessageBox('passwordInitPopup');
				});
			}
		});

	}
