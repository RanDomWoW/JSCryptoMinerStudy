				$(".prevNext").hide();	//전체링크오버시에만 이전다음버튼 보이기
		});

		//탭이동 설정
		$(".autoOff a, .sm_mv_slide dt a").focus(function(){
			$(".prevNext").fadeIn("fast");//무비재생정지버튼, 이전다음버튼 보이기
		});
	}



});
