			}
		});
	}

	/**
	 * 인증번호 요청함수
	 */
	function reAuthCheckProcess()
	{
		$('div#messageBox_reAuthPopup input:radio[name^=reAuthCheck_1]').each(function(index) {
			if ($(this).is(':checked')) {
				var authType 	= $(this).val().trim();
				var webID		= $('#reauth-id-webid').val().trim();
				var memberName	= $('#reauth-id-name').val().trim();
				var mobile1		= $('#reauth-id-phone').val().trim();
				var mobile2		= $('#reauth-id-phone2').val().trim();
				var mobile3		= $('#reauth-id-phone3').val().trim();
				var emailAddr	= $('#reauth-id-email').val().trim();

				if(!validate("li#reAuthTab1_webID"))	return;
				if(!validate("li#reAuthTab1_name"))		return;

				if (authType=="1") {
					if(!validate("li#reAuthTab1_mobile"))	return;
					reAuthData.reAuthGbn="PHONE";
				} else {
					if(!validate("li#reAuthTab1_email"))	return;
					reAuthData.reAuthGbn="MAIL";
				}

				var ajaxOptions={
						url: getServerUrl(true) + '/DataProvider',
						type: 'POST',
						data: {
							_command: 'User.recoverPasswordBySimple2',
							authGubun: authType,	// 1:휴대폰 2:이메일
							webID: webID,
							memberName: memberName,
							mobile1: mobile1,
							mobile2: mobile2,
							mobile3: mobile3,
							emailAddr: emailAddr,
							siteCode : "36"
						}
					};

				if(!lowBrowserVer) {
					ajaxOptions.crossDomain=true;
					ajaxOptions.xhrFields={withCredentials : true};
				}

				$.ajax(ajaxOptions).success(function( data ) {
					if (data.resultCode == "0000") {

						if (reAuthData.reAuthGbn == "PHONE") {
							reAuthData.reAuthMemberCode = data.memberCode;
							reAuthData.reAuthMobileNo	= String(mobile1) + String(mobile2) + String(mobile3);
							reAuthMobileProcess();
