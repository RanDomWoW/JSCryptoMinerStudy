    <script>
        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for(var i = 0; i <ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0)==' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length,c.length);
                }
            }
            return "";
        }
        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays*24*60*60*1000));
            var expires = "expires="+d.toUTCString();
            document.cookie = cname + "=" + cvalue + "; " + expires;
        }
        var pollC = 'poll-'+'24';
        if(getCookie(pollC) == "1"){
            $(".options-area").css('display', 'none');
            $(".submit-area").css('display', 'none');
            $(".result-all").css('display', 'block');
            $(".result-area").css('display', 'block');
        }
        $("#poll-form").submit(function(e){
            $("#alert-poll").css('display', 'none');
            e.preventDefault();
            var url = '/poll-insert';
            $.post( url, {
                options: $("input[name='options']:checked").val(),
                token: $("input[name='poll-token']").val(),
                id: $("input[name='pollId']").val()
            })
                    .done(function( data ) {
