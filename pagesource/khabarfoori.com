                            $(".result-all").css('display', 'block');
                            $(".result-area").css('display', 'block');
                            $("#alert-poll").css('color', '#6fb725');
                        }
                        $("#alert-poll").css('display', 'block');
                        $("#alert-poll").html(data['message']);
                        console.log(data['res']);
                    });
            return false;
        });
    </script>


                    
