        }
      } catch (e) {
        console.warn('theme.assessPromoShow error:', e);
        // Reset in case there is any error
        window.localStorage.removeItem('promoBarCloseDate');
        $promo.removeClass('hide');
      }      
    };
    theme.addCloseDateLocalStorage = function() {
      if (!window.localStorage.promoBarCloseDate) {
        window.localStorage.promoBarCloseDate = new Date();
      }
    };
    theme.addPromoHandlers = function() {
      if (theme.checkFixedHeaderClassOnBody()) theme.adjustPageContentHeight();

      $('.promo-top-close').on('click',function() {
        $('.promo-top').addClass('hide');
        theme.addCloseDateLocalStorage();
        if (theme.checkFixedHeaderClassOnBody()) theme.adjustPageContentHeight();
      });

      if (theme.checkFixedHeaderClassOnBody()) {
        $(window).on('debouncedresize.adjustPageContentTop', theme.adjustPageContentHeight);
      }
    };
  </script>


