function loadImageScrollComment(){
    var min = $(".lastComment .box").offset().top;    
    var max = min + $(".lastComment .box").height();

    $(".lastComment .box .bg:not(.is-visited)").each(function(){
        var top = $(this).offset().top;
        if(top >= min && top <= max){
            $(this).addClass("is-visited");
            $(this).find("img").each(function(){
                $(this).load(function(e){ $(this).parents("a").addClass("loaded");}).attr("src", $(this).data("src"));
            });
        }
    }); 
}




