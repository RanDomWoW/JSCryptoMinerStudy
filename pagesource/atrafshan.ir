var ProductCarousel = function(carousel){
    var carousel = carousel;
    
    carousel.owlCarousel({
        rtl: true,
        nav: false,
        loop: false,
        mouseDrag: false,
        items : 4,
        smartSpeed: 750,
        onInitialized: function(){
            loadImage();
        }
    });
    var owl = carousel.data("owlCarousel");
    
    carousel.on("translated.owl.carousel", function(){
        console.log("changed is required");
        loadImage();
    });

    
    function loadImage(){
        carousel.find(".owl-item.active:not(.loaded)").each(function(){
            var img = $(this).find("img");
            img.load(function(e){ $(this).addClass("load");}).attr("src", img.data("src"));
            $(this).addClass("loaded");
        });
    }
    
    carousel.siblings(".arrow").find(".right").on("click", function(){
        owl.prev();
        if(owl._current == 0){
            $(this).addClass("disabled");
        }
        if(owl._current == (carousel.find(".item").length - 5)){
            carousel.siblings(".arrow").find(".left").removeClass("disabled");
        }
    });
    carousel.siblings(".arrow").find(".left").on("click", function(){
        owl.next();
        if(owl._current == 1){
            carousel.siblings(".arrow").find(".right").removeClass("disabled");
        }
        if(owl._current == (carousel.find(".item").length - 4)){
            $(this).addClass("disabled");
        }
    });
}


