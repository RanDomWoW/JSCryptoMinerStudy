<script>
	$(document).ready(InitRouteBox);
	var RouteBox = {};
	RouteBox.fromWM = "Откуда:";
	RouteBox.toWM = "Куда:";
	
	function InitRouteBox() {
		RouteBox.from = $("#PlaceHolderRight_boxMainPage_atxtFromCity_TextBox");
		RouteBox.to = $("#PlaceHolderRight_boxMainPage_atxtToCity_TextBox");
		RouteBox.chkShowMap = $("#PlaceHolderRight_boxMainPage_chkMapTrace");
		RouteBox.imgBtn = $("#PlaceHolderRight_boxMainPage_IB_Calc");
		RouteBox.link = $("#PlaceHolderRight_boxMainPage_L_Calc");

		RouteBox.from.keypress(RouteBox_OnKeyPress);
		RouteBox.to.keypress(RouteBox_OnKeyPress);
		RouteBox.imgBtn.keypress(RouteBox_OnKeyPress);
		RouteBox.link.keypress(RouteBox_OnKeyPress);
		RouteBox.chkShowMap.keypress(RouteBox_OnKeyPress);
	}
	
	function RouteBox_OnKeyPress(e) {
		if (e.keyCode == 13) {
			RedirectToRoute(e);
		}
	}

	function RedirectToRoute(e) {
		var rbFromVal = RouteBox.from.val();
		if (rbFromVal == RouteBox.fromWM || rbFromVal == '') {
			RouteBox.from.focus();
			e.preventDefault();
		} else {
			var rbToVal = RouteBox.to.val();
			if (rbToVal == RouteBox.toWM || rbToVal == '') {
				RouteBox.to.focus();
				e.preventDefault();
			} else {
				window.location = RouteBox.link[0].href;
				e.preventDefault();
			}
		}
	}
</script>
