	//<![CDATA[
	Sys.Application.add_load(function () {
		$get('PlaceHolderMain_FastSearch1_txtFromGeoRadius').value = "";
		$get('PlaceHolderMain_FastSearch1_chkEllipseRouteSearch').checked = false;
	});

	function ChangeSearchModeHandler() {
		ApplySearchMode($get("PlaceHolderMain_FastSearch1_chkEllipseRouteSearch").checked
			? ATI.Ajax.Scripts.SearchMode.EllipseRoute
			: ATI.Ajax.Scripts.SearchMode.Default);

		//check region type for seach with ellipse
		if ($get("PlaceHolderMain_FastSearch1_chkEllipseRouteSearch").checked) {
			var ctlFromGeo = $get('PlaceHolderMain_FastSearch1_atxtFromGeo');
			var ctlToGeo = $get('PlaceHolderMain_FastSearch1_atxtToGeo');
			if (SearchManager.GetAutoCompleteValue(ctlFromGeo) != null && SearchManager.GetAutoCompleteValue(ctlFromGeo).split('_')[0] != '2')
				PageManager.SetAutocompleteValue(ctlFromGeo, '');
			if (SearchManager.GetAutoCompleteValue(ctlToGeo) != null && SearchManager.GetAutoCompleteValue(ctlToGeo).split('_')[0] != '2')
				PageManager.SetAutocompleteValue(ctlToGeo, '');
			CalcDistance();
		}
	};

	function ApplySearchMode(searchMode) {
		SearchManager.SetSearchMode(searchMode);
		var ctlFromGeo = $find("atxtFromGeo_AutoCompleteBehavior");
		var ctlToGeo = $find("atxtToGeo_AutoCompleteBehavior");
		var ctlFromGeoRadius = $get('divFromGeoRadius');
		var ctlToGeoRadius = $get('divToGeoRadius');
		var ctlEllipseParams = $get('tblEllipseParams');
		var ctlButtonCar = $find("PlaceHolderMain_FastSearch1_lbtTruck");
		var ctlImageCar = $find("PlaceHolderMain_FastSearch1_ibtTruck");
		var divSearchTruckButtons = $get('divSearchTruckButtons');
		ctlFromGeo._cache = null;
		ctlToGeo._chache = null;
		if (SearchManager.GetSearchMode() == ATI.Ajax.Scripts.SearchMode.EllipseRoute) {
			ctlFromGeoRadius.style.visibility = ctlToGeoRadius.style.visibility = "hidden";
			ctlEllipseParams.style.display = "block";
			divSearchTruckButtons.style.display = 'none';
			var displayGeoType = 'city_0$Rus';
			ctlFromGeo._contextKey = displayGeoType;
			ctlToGeo._contextKey = displayGeoType;
		} else {
			ctlFromGeoRadius.style.visibility = ctlToGeoRadius.style.visibility = "visible";
			ctlEllipseParams.style.display = "none";
			divSearchTruckButtons.style.display = "block";
			var displayGeoType = 'all_0$Rus';
			ctlFromGeo._contextKey = displayGeoType;
			ctlToGeo._contextKey = displayGeoType;
		}
	};

	function BuildQueryString(p_sStart, p_bBackwards, p_sEntityType) {
		var sb = new Sys.StringBuilder(p_sStart);
		var ctl = $get('PlaceHolderMain_FastSearch1_atxtFromGeo');
		SearchManager.AppendValue(sb, ctl, SearchManager.GetAutoCompleteValue(ctl));
		ctl = $get('PlaceHolderMain_FastSearch1_atxtToGeo');
		SearchManager.AppendValue(sb, ctl, SearchManager.GetAutoCompleteValue(ctl));
		if (SearchManager.GetSearchMode() == ATI.Ajax.Scripts.SearchMode.EllipseRoute) {
			ctl = $get("PlaceHolderMain_FastSearch1_chkEllipseRouteSearch");
			SearchManager.AppendValue(sb, ctl, SearchManager.GetCheckBoxValue(ctl));
			ctl = $get('PlaceHolderMain_FastSearch1_txtExtraEllipseRoute');
			if (SearchManager.GetEllipseRouteValue(ctl)) {
				SearchManager.AppendValue(sb, ctl, SearchManager.GetEllipseRouteValue(ctl));
				var ctl = $get('PlaceHolderMain_FastSearch1_txtMinEllipseRoute');
				if (SearchManager.GetEllipseRouteValue(ctl)) {
					SearchManager.AppendValue(sb, ctl, SearchManager.GetEllipseRouteValue(ctl));
				}
			}
		} else {
			ctl = $get('PlaceHolderMain_FastSearch1_txtFromGeoRadius');
			var defaultFromGeoRadius = p_sEntityType == "Load" ? "" : "";
			var ctlValue = SearchManager.GetNumericTextBoxValue(ctl);
			// Включаем радиус в поиск, если он определен
			if (ctlValue != null || (defaultFromGeoRadius && defaultFromGeoRadius != "-1")) SearchManager.AppendKeyValue(sb, "ExactFromGeos", "true");
		    SearchManager.AppendValue(sb, ctl, ctlValue == null ? defaultFromGeoRadius : ctlValue);

		    ctl = $get('PlaceHolderMain_FastSearch1_txtToGeoRadius');
		    var defaultToGeoRadius = p_sEntityType == "Load" ? "" : "";
			var ctlValue = SearchManager.GetNumericTextBoxValue(ctl);
			// Включаем радиус в поиск, если он определен
			if (ctlValue != null || (defaultToGeoRadius && defaultToGeoRadius != "-1")) SearchManager.AppendKeyValue(sb, "ExactToGeos", "true");
		    SearchManager.AppendValue(sb, ctl, ctlValue == null ? defaultToGeoRadius : ctlValue);
		}
		ctl = $get('PlaceHolderMain_FastSearch1_ddlCarType');
    	SearchManager.AppendValue(sb, ctl, SearchManager.GetDropDownListValue(ctl));
    	ctl = $get('PlaceHolderMain_FastSearch1_txtFirstDate');
	    var firstDate = SearchManager.GetDate(ctl);
	    SearchManager.AppendValue(sb, ctl, firstDate);

	    if (p_sEntityType === "Load" && firstDate) {
	        SearchManager.AppendKeyValue(sb, "DateOption", "manual");
	    }
        
		ctl = $get('PlaceHolderMain_FastSearch1_txtWeight');
		SearchManager.AppendValue(sb, ctl, SearchManager.GetNumericTextBoxValue(ctl));
		ctl = $get('PlaceHolderMain_FastSearch1_txtWeight2');
		SearchManager.AppendValue(sb, ctl, SearchManager.GetNumericTextBoxValue(ctl));
		ctl = $get('PlaceHolderMain_FastSearch1_txtVolume');
		SearchManager.AppendValue(sb, ctl, SearchManager.GetNumericTextBoxValue(ctl));
		ctl = $get('PlaceHolderMain_FastSearch1_txtVolume2');
		SearchManager.AppendValue(sb, ctl, SearchManager.GetNumericTextBoxValue(ctl));
		return sb.toString();
	};

	function Search(entityType, url) {
		return SearchManager.Search(entityType, url);
	};

	function atxtGeo_OnValueChanged() {
		if (typeof (CalcDistance) == 'function') {
			CalcDistance();
		}
	};

	function atxtGeo_OnValueSelected(source, args) {
		if (typeof (CalcDistance) == 'function') {
			CalcDistance();
		}
	};

	function CalcDistance() {
		var fromCity = SearchManager.GetAutoCompleteValue($get("PlaceHolderMain_FastSearch1_atxtFromGeo"));
		var toCity = SearchManager.GetAutoCompleteValue($get("PlaceHolderMain_FastSearch1_atxtToGeo"));
		if ((SearchManager.GetSearchMode() == ATI.Ajax.Scripts.SearchMode.EllipseRoute) && fromCity != null && toCity != null && fromCity != 0 && toCity != 0 && fromCity.split('_')[0] == "2" && toCity.split('_')[0] == "2") {
			{
				GeoManager.calculateDistance(
					fromCity.split('_')[1], toCity.split('_')[1], null, false, OnCalcDistanceSucceeded, OnCalcDistanceFailed);
			}
		} else DomManager.setValue($get("PlaceHolderMain_FastSearch1_hdnDistance"), 0);
	};

	function OnCalcDistanceSucceeded(calculatedDistance, eventArgs) {
		DomManager.setValue($get("PlaceHolderMain_FastSearch1_hdnDistance"), calculatedDistance);
	};

	function OnCalcDistanceFailed(error) {
		DomManager.setValue($get("PlaceHolderMain_FastSearch1_hdnDistance"), 0);
	};
	function ValidateCityCustom(source, args) {
		if (SearchManager.GetSearchMode() == ATI.Ajax.Scripts.SearchMode.EllipseRoute) {
			return ValidateCity(source, args);
		}
		args.isValid = true;
	};
	//]]>
</script>
