<script type="text/javascript">
    var _mediaRegionCookieName = 'AtiMediaRegion';
    var _afterChangeRedirectUrl = '/';
    var _mediaRegionInfo = {"RegionID":2,"State":"Propose","ProposeTimes":0,"ChangedOnClient":false}; var _mediaRegionName = 'Северо-Запад';
    var regionProposeShown = false;
    var regionPopover;
   
    $(function () {
        var pOpts = {
            placement: 'auto right',
            container: 'body', 
            trigger: 'manual',
            html: true,
            content: function () {
                var _c = $("<div/>");
                _c.append($("<div class='mediaRegionChooseHeader' />").html("<span>Ваш регион - " + _mediaRegionName + (_mediaRegionInfo.State == "Propose" ? "?" : "") + "</span>")
                    .append($("<span class='ati-ico-close' style='font-size:22px; float:right' onclick='CloseMediaRegionPopover();'></span>"))).append("<span style='clear:both' />");;
              
                if (_mediaRegionInfo.State == "Propose") {
                    var _buttonDiv = $("<div style='float:left' id='regionConfirm' />");
                    _buttonDiv.append($("<button type='button' class='ati-button' style='margin-right:10px' onclick='ConfirmMediaRegion()' />").text("Да, верно"));
                    _c.append(_buttonDiv);
                }
                var _chooseDiv = $("<div class='mediaRegionChooseDiv'/>");
                _chooseDiv.append($("<a class='ati-link-button' style='margin-left:5px' onclick='ShowRegionsRadioList(this)' />").text('выбрать другой'));
                var _radioListDiv = $("#mediaRegionsListDiv").clone().attr('id', 'mediaRegionSelectAct');
                _radioListDiv.find("input[value='" + _mediaRegionInfo.RegionID + "']").prop("checked", true);
                _radioListDiv.find("input[type='radio']").change(function() {
                    ConfirmMediaRegion(this.value); 
                    window.location.assign(_afterChangeRedirectUrl);
                });
                _chooseDiv.append(_radioListDiv);
                _c.append(_chooseDiv);
                _c.append("<div style='clear:both' />");
                _c.append($("<p class='mediaRegionChooseHint' />").text("От региона зависит то, какие новости Вы увидите в рубрике Региональная логистика"));
                return _c;
            }
        }
    
        var _rLink = $("#regionEditLink");
        _rLink.popover(pOpts);
        regionPopover = _rLink.data('bs.popover');
        _rLink.on('shown.bs.popover', function() {
            var t = regionPopover.$arrow.position().top + "px";
            regionPopover.$arrow.css('top', t);
        });


        if (_mediaRegionInfo.State == "Propose" && _mediaRegionInfo.ProposeTimes < 9) {
            if (!regionProposeShown && _rLink.visible())
                ShowProposedRegion();
            else {
                $(window).scroll(showRegionProposeFunc);
            }
        }
    });
    var showRegionProposeFunc = function () {
        var _rLink = $("#regionEditLink");
        if (!regionProposeShown && _rLink.visible())
            ShowProposedRegion();
    }

    function ShowProposedRegion() {
        regionProposeShown = true;
        $(window).off("scroll", showRegionProposeFunc);
        setTimeout(function () {
            $("#regionEditLink").popover('show'); 
            _mediaRegionInfo.ProposeTimes++;

            cookieHelper.set(_mediaRegionCookieName, BuildMediaRegionCookieValue(_mediaRegionInfo), 1);
            if (_mediaRegionInfo.ProposeTimes >= 9) { 
                ConfirmMediaRegion();
            }
            
        }, 200);
    }

    function ShowRegionsRadioList(link) {
        link = $(link);
        link.parent().parent().find('#regionConfirm').hide();
        link.next().show();
        link.hide();
       
    }
    function ToggleMediaRegionPopover() {
        $("#regionEditLink").popover('toggle');
    }
    function CloseMediaRegionPopover() {
        $("#regionEditLink").popover('hide');
    }
    function ConfirmMediaRegion(regionId) {
        _mediaRegionInfo.ProposeTimes = 0;
        _mediaRegionInfo.State = "Choosed";
        if (regionId) {
            _mediaRegionInfo.RegionID = regionId;
        }
        _mediaRegionInfo.ChangedOnClient = true;
        cookieHelper.set(_mediaRegionCookieName, BuildMediaRegionCookieValue(_mediaRegionInfo), 365);
        CloseMediaRegionPopover();
    }
    function BuildMediaRegionCookieValue(mediaRegionInfo) {
        return "" + mediaRegionInfo.RegionID + "," + mediaRegionInfo.State + "," + mediaRegionInfo.ProposeTimes + "," + (mediaRegionInfo.ChangedOnClient ? 1 : 0);
    }
</script>
