<script type='text/javascript'>
$(document).ready(function(){
    if ($.cookie('subscribe') != 'yes') {
        setTimeout(function(){
            $.cookie('subscribe', 'yes', { path: '/', expires: 30 });
            $('#box_subscribe .user_log').click();
            $('#box_subscribe .btn_login').click(function(){
                var email = $('#box_subscribe .txt_login').val();
                $.ajax({
            		type: "GET",
            		url: '/?mod=api&act=addSubscribe&email='+email,
            		dataType: "html",
            		success: function(msg){
            			if(msg==1) {
                            $('#box_subscribe .msg').text('Thành công!').show();
                            setTimeout(function(){
                                $('#box_subscribe').parents('.fancybox-skin').find('.fancybox-close').click();
                            }, 1000);
                        }
                        else {
                            $('#box_subscribe .msg').text('Sai email').show();
                        }
            		}
            	});
                return false;
            });
        }, 180000);
    }
});
</script>
<script type="text/javascript">
$(document).ready(function(){
    $('.btn_poll.js').click(function(){
        if(!$(this).hasClass('js')) return false;
        var val = $(this).parents('.js_boxvote').find('input[type=radio]:checked').val();
        if(val!=undefined) {
            $(this).parents('.js_boxvote').find('input[type=radio]').attr('disabled', 'disabled');
            var id = $(this).attr('data-id');
            var total = $(this).attr('data-total'); total++;
            var obj = $(this).parents('.js_boxvote').find('.result_poll ul');
            $(this).removeClass('js').css('opacity', '0.5');
            $.ajax({
        		type: "POST",
        		url: '/?mod=api&act=setVote&id='+id,
                data:  {t: val},
        		dataType: "html",
        		success: function(msg){
                  var all_result = jQuery.parseJSON(msg);
                  $.each(all_result, function(key, val) {
                    var p = parseInt(val/total*100);
                    obj.find('li:eq('+key+') span.color_poll').width(p+'%');
                    obj.find('li:eq('+key+') .col3 span').text(p+'%');
                  })
        		}
        	});
        }
        return false;
    });
    if($('#js_box_relation').length) {
        var html = $('#js_box_relation').html();
        $('#js_box_relation').html('');
        var e = $('#content_detail .exp_content figure:eq(0)');
        if(e.length) {
            e = e.next();
            if(e.length) e.after(html);
        }
    }
    $('.letsop-leagues-selector').change(function(){
        var val = $(this).val();
        var url = 'http://tyso.bongda.com.vn/widgets/widget-standing.php?league_id='+val+'&season=1415&css=http%3A%2F%2Ftyso.bongda.com.vn%2Fcss%2Fbongda.com.vn%2Fstanding-xsmall.css';
        $(this).parents('section').find('iframe').attr('src', url);
    });
});
</script>

