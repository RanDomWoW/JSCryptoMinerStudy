<script type="text/javascript">
 $(document).ready(function() {

  function createCookie(name, value, days) {
   var expires;
   if (days) {
    var date = new Date();
    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
    expires = '; expires=' + date.toGMTString();
   }
   else {
    expires = '';
   }
   document.cookie = encodeURIComponent(name) + "=" + encodeURIComponent(value) + expires + '; path=/';
  }

  function readCookie(name) {
   var keyValue = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
   return keyValue ? keyValue[2] : null;
  }

  // different slide in approaches for desktop / tablet vs mobile
  if (readCookie('promo_prices_slidin_shown') != '1') {
    $(window).scroll(function() {
      if ( $(window).scrollTop() > $(window).height() ) {
        $('#promo_prices_slidin').animate({'bottom':'0'}, 1000);
      }
    });
  }

  // close button functionality

  $("#promo_prices_slidin_close").click(function(){
      $(window).off('scroll');
//      $('#promo_prices_slidin').animate({'bottom':'-500px'}, 1000);
      $('#promo_prices_slidin').hide();
      createCookie('promo_prices_slidin_shown', '1', false);
  });

  // form processing (new slide-in)

  $('#promo_prices_slidin_button').click(function(){
      // $('#promo_prices_slidin_loading').show();
      $('#promo_prices_slidin_button').prop('disabled',true);
      $.post('/slide-campaign-01.php', {'email':$('#promo_prices_slidin_email').val(),'gdpr':$("input[name='gdpr_check']:checked").val()},function(data){
          // $('#promo_prices_slidin_loading').hide();
          $('#promo_prices_slidin_button').prop('disabled',false);
          if (data.error != '') {
              $('#promo_prices_slidin_error').html('<strong>'+data.error+'</strong>').show();
          } else {
              $('#promo_prices_slidin_form').hide(function(){
                  $('#promo_prices_slidin_close').hide();
                  $('#promo_prices_slidin_thanks').show().delay(4000).fadeOut('slow',function(){
                  //    $('#promo_prices_slidin').hide();
                      createCookie('promo_prices_slidin_shown', '1', 365);
                  //    Re-directs them to the prices page and the discount is automatically applied.
	       window.location.href = '/paris-pass-prices.php';

                  });
              });
          }
      });
  });


 });
</script>

