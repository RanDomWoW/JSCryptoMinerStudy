$(document).ready(function() {
	var slider = $('#fun .slider').bxSlider({
		auto:true,
		pause:6000,
		pager:true,
		controls:false,
		captions:true,
		adaptiveHeight: false,
		onSlideBefore : function(el,prev,active) {
			$('#fun .one_day .items .key_'+prev).removeClass("active");
			$('#fun .one_day .items .key_'+active).addClass("active");
		}
	});

	$.getJSON("/fun/popular.php", function(data) {
		var last_is = 4;
		$.each(data.one_day, function(key, val) {
			$('#fun .slider').append('<li><a href="/fun/'+val.id+'-'+val.alias+'"><img src="http://ugaga.net/api/thumbs/'+val.image+'" data-caption="<a href=\'/fun/'+val.id+'-'+val.alias+'\'>'+val.title+'</a>"></a></li>');
			$('#fun .one_day .items').append('<div class="item key_'+key+'"><a href="javascript:void(0);" data-id="'+key+'">'+val.title+'</a></div>');
			if(key != last_is) {
				$('#fun .one_day .items').append("<hr>");
			}
			if (key == 0) {
				$('#fun .one_day .items .key_0').addClass("active");
			}
		});

		$('#fun .one_day .items a').bind("click", function(e) {
			slider.goToSlide($(this).data('id'));
			slider.stopAuto(true);
			e.preventDefault();
			//slider.startShow();
			return false;
		});

		slider.reloadSlider();

