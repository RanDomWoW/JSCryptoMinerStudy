<script type="text/javascript">
    $('#lead-form').each(function(){
        var options = {
            //ELEMENT TO BE UPDATED WITH SERVER RESPONSE
            target: $('#response'),
            beforeSubmit:  function(formData, $form, options) {
                $('.response_area').addClass('hide');
                //CHECK PRIVACY CHECKBOX
                if($('.privacy_check').is(':checked') === false){
                    $('#response_area_privacy').removeClass('hide');
                    $.Tc_TrackEvent($('.invia-nl'), 'ga_event', {'type':'iscrizione newsletter', 'action':'invia', 'label':'non accetto privacy'});
                    return false;
                }
                return true;
            },
            success: function(responseText, statusText, xhr, $form)  {
                if(typeof responseText === "string")
                {
                    responseText = JSON.parse(responseText);
                }

                var email = $('#email-nl').val();
                $('#email-nl').val('');

                switch(parseInt(responseText['result'])){
                    case 0:
                        $('#response_area0').removeClass('hide');
                        $.Tc_TrackEvent($('.invia-nl'), 'ga_event', {'type':'iscrizione newsletter', 'action':'invia', 'label':'email non valida'});
                        break;
                    case 1:
                        $('#response_area_success').removeClass('hide');

                        if(typeof(tC.internalvars.fbk_pixel_id_retarg) !== 'undefined'){
                            fbq('track', 'Lead');
                        }

                        if(typeof ga !== "undefined" && typeof customDimensionsData !== "undefined") {
                            if(customDimensionsData.userID === '') { // caso di utente non presente in DB o che non ha cliccato newsletter con parametro
                                customDimensionsData.userID = responseText['sk'];
                            }
                            // Trigger TagCommander Event Send Custom Dimensions
                            $.Tc_TrackEvent($('#submit-button'), 'custom_dimensions_data_set', {
                                    'custom_data': customDimensionsData
                                }
                            );
                        }

                        $.Tc_TrackEvent($('#submit-button'), 'iscrizione_newsletter', {
                                'sub_email' : email,
                                'sub_email_new' : 1
                            }
                        );
                        $.cookie('already_subscribed_NL',true,{path:'/',expires:90});
                        break;
                    case 2:
                        $('#response_area2').removeClass('hide');

                        if(typeof ga !== "undefined" && typeof customDimensionsData !== "undefined") {
                            if(customDimensionsData.userID === '') { // caso di utente non presente in DB o che non ha cliccato newsletter con parametro
                                customDimensionsData.userID = responseText['sk'];
                            }
                            // Trigger TagCommander Event Send Custom Dimensions
                            $.Tc_TrackEvent($('#submit-button'), 'custom_dimensions_data_set', {
                                    'custom_data': customDimensionsData
                                }
                            );
                        }

                        //Trigger TagCommander Event Registrazione Effettuata
                        $.Tc_TrackEvent($('#submit-button'), 'iscrizione_newsletter', {
                                'sub_email' : email,
                                'sub_email_new' : 0
                            }
                        );
                        break;
                    default:
                        break;
                }
            },
            beforeSend: function() {
            },
            error: function(xhr, status, error, $form){
                console.log('Error', xhr, status, error, $form);
            }
        };
        $(this).ajaxForm(options);

    });
</script>    </div>
        
