				}
			});

			$('#sendReportModal input[type="submit"]').click(function(e) {
				$('#sendReportModal input[type="submit"]').attr('disabled', 'disabled');
				e.preventDefault();
				sendletter.message.subject = "Ошибка на странице "+ sendletter.message.sourcePage;
				sendletter.message.last_pages = lastPages;

				html2canvas(document.body, {
					onrendered: function(canvas) {
						var canvas = canvas.toDataURL('image/png').replace(/data:image\/png;base64,/, '');
						sendletter.message.screenshot = canvas; //don't appends. async issue?
						$('canvas').remove();
						senderRouter();
					}
				});
			});

			appendItem('last_pages', window.location.href);
			var pagesHistory = JSON.parse(localStorage.getItem('last_pages'));
			var lastPages = pagesHistory.page.slice(-10);

			if (isEmptyFields()) {
				$('#sendReportModal input[type="submit"]').attr('disabled', 'disabled');
			} else {
				$('#sendReportModal input[type="submit"]').removeAttr('disabled');
			}

		}

		if (window.frameCacheVars !== undefined) {
			BX.addCustomEvent('onFrameDataReceived', function(json) {
				sendletter.init();
			});
		} else {
			BX.ready(function() {
				sendletter.init();
			});
		}
	</script>
