		e.preventDefault();
		var that = $(this);
		that.parent().find('a').removeClass('active');
		that.addClass('active');
		$('.newsline .tab-widget-body ul').html('<center style="padding:10px 0"><i class="fa fa-spinner fa-spin fa-2x" style="color:#000"></i></center>');
		$.get('/ajax.php?lang=ru&act=popular&date='+that.attr('data-date'), function(data){
			$('.newsline .tab-widget-body ul').html(data);
		});
	});

	$('.pull-post-items a').click(function(e){
		e.preventDefault();
		var that = $(this);
		var page = Number(that.parent().attr('data-page')) + ( that.attr('data-step') == 'next' ? 1 : -1 );
			page = page > 1 ? page : 1;

		that.parent().attr('data-page', page);
		$('#category'+that.parent().attr('data-cid')).html('<center><i class="fa fa-spinner fa-spin fa-2x" style="color:#000"></i></center>');
		$.get('/ajax.php?lang=ru&act=category&cid='+that.parent().attr('data-cid')+'&page='+page, function(data){
			$('#category'+that.parent().attr('data-cid')).html(data);
		});
	});

	$('.login-btn').click(function(e){
		e.preventDefault();
		doModal('Вход', '<div><div class="form-group"><input type="text" placeholder="Логин" id="username" name="username" class="form-control" autocomplete="off"></div><div class="form-group"><input type="password" placeholder="пароль" id="password" name="password" class="form-control" autocomplete="off"></div><button type="button" id="login-submit" class="btn btn-primary" style="width:100%">Вход</button></div>', '', '');
	});

	$('body').on('click', '#login-submit', function(){
		$(this).parent().append('<i class="fa fa-spinner fa-spin fa-2x"></i>');
		$.post('/login.php?lang=ru', { 'login': Math.random(), 'username': $('#username').val() , 'password': $('#password').val() }, function(data){
			window.location = data;
		});
	});


	var trackScrollPage = 1;
	$('.newsline .twb1').scroll(function(){
		if( ($('.newsline .twb1')[0].scrollHeight - $('.newsline .twb1').height()) - $('.newsline .twb1').scrollTop() < 50 && $('.trackScrollPage').length == 0 && $('.newsline .tab-widget .active').attr('data-tab') == 'latest' )
		{
			trackScrollPage++;
			$('.newsline .twb1').append('<center class="trackScrollPage"><i class="fa fa-spinner fa-spin fa-2x" style="color:#000"></i></center>');
			$.get('/ajax.php?lang=ru&act=newsline&page='+trackScrollPage, function(data){
				$('.trackScrollPage').remove();
				$('.newsline .twb1 ul').append(data);
			});
		}
	});

});

