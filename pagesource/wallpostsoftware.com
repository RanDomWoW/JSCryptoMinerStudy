<script>
        $(document).ready(function(){
    $("#sendingbtn_newsletter").click(function (e)
        {
            e.preventDefault();

            $('#message_newsletter').text("Please wait.....");
            $('#sendingbtn_newsletter').attr("disabled", true);   
           
            $.ajax({
                url: "https://wallpostsoftware.com/home/subscribeNewsLetters",
                type: 'POST',
                cache: false,
                datatype: 'json',
                data: {
                    'email': $("#email").val(),
                     
            },
                success: function (data)
                {     
                    //spinnerOff();
                  $('#sendingbtn_newsletter').attr("disabled", false);   
                  if(data=="Successfully subscribe our newsletter"){
                        
                    $("#message_newsletter").addClass("newsletter_success");
                  }else{
                    $("#message_newsletter").addClass("newsletter_error");
                  }
                   
                  $('#message_newsletter').text(data); 
                   $("#message_newsletter").show();
                     
                    
                   return false;
                },
                error: function () {
                    alert("Server is busy!! Please try again");
                }
            });


        });


function spinnerOn() {
        $('#spinner-overlay').css('display', 'block')
        $('#spinner-div').css('display', 'block')
        $('#loading-image').css('display', 'block')
    }

    function spinnerOff() {
        $('#spinner-overlay').css('display', 'none');
        $('#spinner-div').css('display', 'none');
        $('#loading-image').css('display', 'none');
    }

        });
        </script>



