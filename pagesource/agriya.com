			$("#chat_button").removeClass('fn_chat_toggle_button');
			$(".chat_toggle").animate({width: "420px"});
			$(".chat_toggle").animate({height: "118px"});
			clearInterval(toggle_chat);
			$(".chat_toggle .body").fadeIn(3000);
			//$('.chat-bg').addClass("chat-overlay");
			$("body").addClass("chat-mob");
		},10000);	

		$(document).on("click","#chat_button",function()
		{
			if(chat_button)
			{
				clearInterval(toggle_chat);
				window.open('http://customers.agriya.com/chat.html','Livechat','width=500,height=525,scrollbars=yes');
			}
		});

		$(document).on("click",".fn_close_chat",function()
		{
			chat_button = true;
			$(".chat_toggle").animate({height: "26px"});
			$(".chat_toggle").animate({width: "115px"});
			$("#chat_button").addClass('fn_chat_toggle_button');
			$(".chat_toggle .header").fadeIn(3000);
			$(this).css("position","static");
			//$('.chat-bg').removeClass("chat-overlay");
		});

		$(document).on("click",".fn_yes_btn",function()
		{
			window.open('http://customers.agriya.com/chat.html','Livechat','width=500,height=525,scrollbars=yes');
			$(".fn_close_chat").trigger("click");
		});

    });
</script>    </body>
</html>