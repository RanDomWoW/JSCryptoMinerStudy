    			function placeholder(){
        			$("input[type=text],textarea").each(function(){
            		var placeh = $(this).attr("placeholder");
            		$(this).val(placeh);
        		});
    		}
    		placeholder();
    		$("input[type=text],textarea").focus(function(){
        		var placeh = $(this).attr("placeholder");
        			if (placeh == $(this).val()) {
        			$(this).val("");
        		}
    		});
    		$("input[type=text],textarea").blur(function(){
        		var placeh = $(this).attr("placeholder");
        		if ($(this).val() == "") {
            		$(this).val(placeh);
        		}
    		});
		});
	</script>

