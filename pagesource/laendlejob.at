              if (cand_email.length != ''){
                $('#candidate-mail').val(cand_email); 
              }
              if (comp_email.length != ''){
                $('#company-mail').val(comp_email); 
              }
            }
            
            $(document).foundation('equalizer', 'reflow');
        });

        return false;
    });
  });

  $(document).keyup(function(e) {
