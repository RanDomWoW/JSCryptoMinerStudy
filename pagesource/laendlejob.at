                  $("#right-login").fadeOut(function() {
                    $("#left-login").animate({width: "100%"});
                    $("h2").siblings().fadeOut();
                    $(".preloader").fadeIn();
                  });
                }
                else
                {
                  $("#left-login").fadeOut(function() {
                    $("#right-login").animate({width: "100%"});
                    $("h2").siblings().fadeOut();
                    $(".preloader").fadeIn();
                  });
                }
                $(".middle-or").fadeOut();
                isSignedIn("/account/index");
