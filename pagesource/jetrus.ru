    <script type="text/javascript">

        document.addEventListener('DOMContentLoaded', function() {

            var nextArrow = document.querySelectorAll('.acs-next')[0],
                prevArrow = document.querySelectorAll('.acs-prev')[0],
                slides = document.querySelectorAll('.acs-slide'),
                slider = document.querySelectorAll('.acs-slider')[0],
                longSlider = slides.length;
            if(slider) {
                if(longSlider <= 2 ) {
                    longSlider = longSlider * 160;

                    slider.style.height = longSlider + 'px';
                    nextArrow.style.display = 'none';
                    prevArrow.style.display = 'none';


                } else {
                    $('.acs-slider').slick({
                        'vertical': true,
                        'infinite' : false,
                        'slidesToShow' : 3,
                        'slideToScroll' : 1,
                        'draggable' : false,
                        'swipe' : false,
                        'zIndex' : 1,
                        'nextArrow': nextArrow,
                        'prevArrow': prevArrow,
                        'touchMove' : false
                    });

                    $('.acs-slider').mousewheel(function(e) {
                        e.preventDefault();

                        if (e.deltaY < 0) {
                            $(this).slick('slickNext');
                        }  else {
                            $(this).slick('slickPrev');
                        }
                    });

                }
            }
        });

    </script>

