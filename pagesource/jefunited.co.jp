		$(".ranking .top").css("display","block");
		$(".ranking .top").animate({opacity:1},{duration:200});

		$('.rankingNav li.topTeam').addClass('current');
		$('.rankingNav li.topTeam').removeClass('off');
		$('.rankingNav li.ladiesTeam').removeClass('current');
		$('.rankingNav li.ladiesTeam').addClass('off');
	});
	
