<script type="text/javascript">
var mvList = $(".mainvisual ul li").length-1;
var mvIdx = 0;
function arrnext(){
	$(".mainvisual ul li").removeClass("on");
	if(mvIdx < mvList){mvIdx++;}else{mvIdx = 0;}
	$(".mainvisual ul li:eq("+mvIdx+")").addClass("on");
}
function arrprev(){
	$(".mainvisual ul li").removeClass("on");
	if(mvIdx > 0){mvIdx--;}else{mvIdx = mvList;}
	$(".mainvisual ul li:eq("+mvIdx+")").addClass("on");
}
rollstart();
function rollstart(){rollgo = setInterval(rollauto,5000);}
function rollstop(){clearInterval(rollgo);}
function rollauto(){arrnext();}
function skipNavi(skipNum){
	if(skipNum == "1"){
		window.scrollTo(0, 92);
		$("#topmenu li.menu_0").addClass("on");
		$("#topmenu li.menu_0 .submenubox").show();
		$("#topmenu li.menu_0 h2 a").focus();
	}else if(skipNum == "2"){
		window.scrollTo(0, 142);
		$("#maincontents .mainvisual li:eq(0) a").focus();
	}
}
</script>
