//팝업관련스크립트
function closeLayer() {
	jQuery(".popMain").each(function(){
		jQuery(this).css("display","none");
	});
	jQuery(".popMain171124").each(function(){
		jQuery(this).css("display","none");
	});
}
function setCookie( name, value, expiredays ) { 
	var todayDate = new Date(); 
	todayDate.setDate( todayDate.getDate() + expiredays ); 
	document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";";
} 
function closePop() { 
//	setCookie("popMain", "no" , 1); 
	setCookie("popMain171124", "no" , 1); 
	closeLayer();
} 
function getCookie(name) { 
	var Found = false; 
	var start, end;
	var i = 0;

	// cookie 문자열 전체를 검색 
	while(i <= document.cookie.length) { 
		start = i;
		end = start + name.length;
	// name과 동일한 문자가 있다면 
		if(document.cookie.substring(start, end) == name) { 
			Found = true;
			break;
		} 
		i++;
	}
	// name 문자열을 cookie에서 찾았다면 
	if(Found == true) { 
		start = end + 1;
		end = document.cookie.indexOf(";", start);
		// 마지막 부분이라는 것을 의미(마지막에는 ";"가 없다) 
		if(end < start) 
			end = document.cookie.length;
		// name에 해당하는 value값을 추출하여 리턴한다. 
		closeLayer();
		closePopupLayer();
	}
	// 찾지 못했다면 
	else {
		if(name != "slidepopwrap") jQuery(".popMain171124").css("display","");
		if(popup_Seq>0) jQuery(".slidepopwrap").css("display","");
	}
}

