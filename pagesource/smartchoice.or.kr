// 통신사이벤트 페이징
function fn_eventPageing(val) {
	if (val == "prev" && eventPageNum > 1) {	// 이전
		$(".dev_event > div").hide();
		eventPageNum--;
		$(".dev_event > div:nth-child("+eventPageNum+")").show();
		$(".dev_eventPaging").text(eventPageNum+"/"+eventPageSize);
	} else if (val == "next" && eventPageNum < eventPageSize) {	// 다음
		$(".dev_event > div").hide();
		eventPageNum++;
		$(".dev_event > div:nth-child("+eventPageNum+")").show();
		$(".dev_eventPaging").text(eventPageNum+"/"+eventPageSize);
	}
}					
