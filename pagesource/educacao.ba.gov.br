		- Caso exista apenas 1 slide, as setas laterais devem sumir;
		- Existindo mais de 1 slide, deve haver rotação automática
		- Caso o usuário clique em uma das setas laterais para rotacionar os slides, a rotação automática é suspensa, evitando embolar com a vontade
		do usuário. Após o usuário parar de rotacionar os slides, a rotação automática voltará em, aproximadamente, 1 minuto.
		- Cada vez que o usuário clica em "Personalizar Página", altera quantidade de slides e clica em "Salvar", a página é salva e um reload é efetuado
		para exibir as alterações no slider (sites->all->modules-> panels-> js -> panels_ipe.js)
	*/
	
	var qtdSlidesTotal =0;
	var interval = [];
	var intervaloAvancar = [];
	var intervaloVoltar =  [];
	var inter = [];
	
	var qtdSlidesMe = 1;
	
	var idMetabloco = 0;
	//var idSlider = "slide_"+ idMetabloco;
	var vetPassadorAutomatico = [];
	
	
	

	$.fn.slideMe = function(params){
		var v =0;
		var self = this;
		var rReiniciado = 0;
		idSlider = params["id"];
		
			
			
			transition = function(id){
				
		
						
						// Iniciar apenas uma vez a função de timeout por bloco
						
						v = 0;
								
						nomeSlide = "";
											
						rReiniciado = retornaIndiceAtualBloco(id);
						//console.log("ID RECEBIDO ->"+id + "R RETORNADO ->"+rReiniciado)	;
								
						nomeSlide = "slide_"+ id + "_"+ rReiniciado;
						document.getElementById(nomeSlide).checked = true;	
								
							
						clearTimeout(vetPassadorAutomatico[id]);
						vetPassadorAutomatico[id] = 0;
						v = setTimeout(function(){
							transition(id)}, 8000);
						vetPassadorAutomatico[id] = v;
				
			 
			} // fim transition
			
			
		var v = setTimeout(transition(idSlider), params.timer);	
	}
	
	/*
	   Startando todos os blocos de slides inicialmente.... 
	   
	 */
	
	document.onreadystatechange = function(){
     if(document.readyState === 'complete'){
     	for(i=1; i<= qtdSlidesMe; i++){	
			qtdSlidesTotal =0;
			
					
			vetPassadorAutomatico[i] =0;

			
			$("body").slideMe({
				id:i,
				timer : 8000
			});
						
			
			if(document.getElementById("slide_"+i+"_1") != undefined)
				++qtdSlidesTotal;
			if(document.getElementById("slide_"+i+"_2") != undefined)
				++qtdSlidesTotal;
			if(document.getElementById("slide_"+i+"_3") != undefined)
				++qtdSlidesTotal;
			if(document.getElementById("slide_"+i+"_4") != undefined)
				++qtdSlidesTotal;
			
		

			if(qtdSlidesTotal == 1){

				document.getElementById("seta_esquerda"+i).src = "";
				document.getElementById("seta_direita"+i).src = "";
				document.getElementById("divSetaEsquerda"+i).style.marginTop = "";
				document.getElementById("divSetaEsquerda"+i).style.marginTop = "145px";
				
				document.getElementById("divSetaEsquerda"+i).style.backgroundColor = "transparent";
				document.getElementById("divSetaDireita"+i).style.backgroundColor = "transparent";
		
						
			} else{



			}

		 }//fim for
     }//fim if
	}	



			


	
	
	function avancaSlides(id){
	
		
		var idSliderAvancar = "slide_"+ id;
		
		clearTimeout(vetPassadorAutomatico[id]);
		vetPassadorAutomatico[id] = 0;
		
		//console.log(idSliderAvancar);
		
	
		seconds = 9;
		second = 0;
		var r = 0;
		
		
		
		
		if(intervaloAvancar[id] != undefined && intervaloAvancar[id] != 0)
			clearInterval(intervaloAvancar[id]);
		
		if(intervaloVoltar[id] != undefined && intervaloVoltar[id] != 0)
			clearInterval(intervaloVoltar[id]);
		
		if(inter[id] != undefined && inter[id] != 0)
			clearInterval(inter[id]);
		
		if(interval[id] != undefined && interval[id] != 0)
			clearInterval(interval[id]);
		
		intervaloAvancar[id] = setInterval(function() {
				 
			  
			if (second >= seconds) {
				clearInterval(intervaloAvancar[id]);
					
				//r = retornaIndiceAtualBloco(id);
										
								
				$("body").slideMe({
					id:id,
					timer : 8000
				});
				
			} 
			second = second +1;
		}, 1000);
		
		
		var s = $( "label#"+idSliderAvancar+".slide-item.metabloco-background-grey-color-alpha-08" );
		
		 slide1 = document.getElementById(idSliderAvancar+"_1");
		 slide2 = document.getElementById(idSliderAvancar+"_2");
		 slide3 = document.getElementById(idSliderAvancar+"_3");
		 slide4 = document.getElementById(idSliderAvancar+"_4");
	 
	 
		var r = 0;
		
		if(slide4 != undefined)
			ultimoSlide = 4;
		else if (slide3 != undefined)
			ultimoSlide = 3;
		else if (slide2 != undefined)
			ultimoSlide = 2;
		
		
		if(slide1 != undefined && slide1.checked){
			if(slide2 != undefined)
				r = 2;
			else
				r = 1;
		}
		else if (slide2 != undefined && slide2.checked){
			if(slide3 != undefined)
				r = 3;
			else
				r = 1;
		}
		else if(slide3 != undefined && slide3.checked){
			if(slide4 != undefined)
				r = 4;
			else
				r = 1 ;
		}
		else if(slide4 != undefined && slide4.checked)
			r = 1; 
		
		nomeSlide = idSliderAvancar + "_" + r;
		
		if(document.getElementById(nomeSlide))
			document.getElementById(nomeSlide).checked = true;
		
			
		//$(s[r]).trigger('click'); 
		
		
	}
	
	

	function voltaSlides(id){
	
		// Limpa o passador de um único bloco, não interferindo com os demais
		
		
		var idSliderVoltar = "slide_"+ id;
		
		clearTimeout(vetPassadorAutomatico[id]);
		vetPassadorAutomatico[id] = 0;
		
		seconds = 9;
		second = 0;
		
		
		if(intervaloAvancar[id] != undefined && intervaloAvancar[id] != 0)
			clearInterval(intervaloAvancar[id]);
		
		if(intervaloVoltar[id] != undefined && intervaloVoltar[id] != 0)
			clearInterval(intervaloVoltar[id]);
		
		if(inter[id] != undefined && inter[id] != 0)
			clearInterval(inter[id]);
		
		if(interval[id] != undefined && interval[id] != 0)
			clearInterval(interval[id]);
		
		
		
		intervaloAvancar[id] = setInterval(function() {
		
        
			if (second >= seconds) {
				
				clearInterval(intervaloAvancar[id]);
						
				//console.log("ZERANDO INTERVALO E VOLTANDO AUTOMATICO");
								
				$("body").slideMe({
					id:id,
					timer : 8000
				});
			
			}

			second = second +1;
		}, 1000);
		
		//console.log(pararAutomatico);
		
		var s = $( "label#"+idSliderVoltar+".slide-item.metabloco-background-grey-color-alpha-08" );
		
		
		 slide1 = document.getElementById(idSliderVoltar+"_1");
		 slide2 = document.getElementById(idSliderVoltar+"_2");
		 slide3 = document.getElementById(idSliderVoltar+"_3");
		 slide4 = document.getElementById(idSliderVoltar+"_4");
	 
		
	 
		var r = 0;
		
		if(slide4 != undefined)
			ultimoSlide = 4;
		else if (slide3 != undefined)
			ultimoSlide = 3;
		else if (slide2 != undefined)
			ultimoSlide = 2;
		
		
		if(slide1 != undefined && slide1.checked)
			r = ultimoSlide;
		else if (slide2 != undefined && slide2.checked)
			r = 1;
		else if(slide3 != undefined && slide3.checked)
			r = 2;
		else if(slide4 != undefined && slide4.checked)
			r = 3;
		
		
		
		nomeSlide = idSliderVoltar + "_" + r;
		
		if(document.getElementById(nomeSlide))
			document.getElementById(nomeSlide).checked = true;
		else{
			nomeSlide = idSliderVoltar + "_"+ ultimoSlide;
			document.getElementById(nomeSlide).checked = true;
		}
		
		
		//$(s[r]).trigger('click'); 
	
		
	}
	
	
	
	
	function acessaSlide(idSlideIr,idBloco,indice){
	
		var idSliderAcessar = "slide_"+ idBloco;
		seconds = 8;
		second = 0;
		
		console.log(idSlideIr);
		
		clearTimeout(vetPassadorAutomatico[idBloco]);
		vetPassadorAutomatico[idBloco] = 0;
		
		
		document.getElementById(idSlideIr).checked = true;
		
		
		//Pára todos os intervalos que tenham sido iniciados no Avançar ou Voltar
		
		if(intervaloAvancar[idBloco] != undefined && intervaloAvancar[idBloco] != 0)
			clearInterval(intervaloAvancar[idBloco]);
		
		if(intervaloVoltar[idBloco] != undefined && intervaloVoltar[idBloco] != 0)
			clearInterval(intervaloVoltar[idBloco]);
		
		if(inter[idBloco] != undefined && inter[idBloco] != 0)
			clearInterval(inter[idBloco]);
		
		if(interval[idBloco] != undefined && interval[idBloco] != 0)
			clearInterval(interval[idBloco]);
		
		
		
		//Reinicializa todos os blocos de slides, pegando o índice atualde cada um
		
		
		inter[idBloco] = setInterval(function() {
		
        	//console.log("segundo ->"+second);
			if (second >= seconds) {
         	
				clearInterval(inter[idBloco]);
				
								
				$("body").slideMe({
					id:idBloco,
					timer : 8000
				});
		
			}

			second = second +1;
		}, 1000);
		
		
	}
	
	function retornaIndiceAtualBloco(id){
		
		var r =1;
		idSliderAtual = "slide_"+ id;
		
		var s = $( "label#"+idSliderAtual+".slide-item.metabloco-background-grey-color-alpha-08" );
			
		slide1_outros = document.getElementById(idSliderAtual+"_1");
		slide2_outros = document.getElementById(idSliderAtual+"_2");
		slide3_outros = document.getElementById(idSliderAtual+"_3");
		slide4_outros = document.getElementById(idSliderAtual+"_4");
						 
		if(slide4_outros != undefined)
			ultimoSlide = 3;
		else if (slide3_outros != undefined)
			ultimoSlide = 2;
		else if (slide2_outros != undefined)
			ultimoSlide = 1;
					
		if(slide1_outros != undefined && slide1_outros.checked){
			if(slide2_outros != undefined)
				r = 2;
			else
				r =1;
		}
		else if (slide2_outros != undefined && slide2_outros.checked){
			if(slide3_outros != undefined)
				r = 3;
			else
				r = 1;
		}
		else if(slide3_outros != undefined && slide3_outros.checked){
			if(slide4_outros != undefined)
				r = 4;
			else
				r = 1;
		}
		else if(slide4_outros != undefined && slide4_outros.checked)
			r = 1;
	
	
		return r;
	}
	
	</script>
