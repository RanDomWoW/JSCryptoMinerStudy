                if ((checkElement.is('ul')) && (checkElement.is(':visible'))) {
                    $(this).closest('li').removeClass('active');
                    checkElement.slideUp('normal');
                }
                if ((checkElement.is('ul')) && (!checkElement.is(':visible'))) {
                    $('.cssmenu ul ul:visible').slideUp('normal');
                    checkElement.slideDown('normal');
                }

            });
            $(".language-change-div li .lang-switcher-link").click(function () {
                var lang_selected = $(this).text().trim();
                $(".language-change-div li .lang-switcher-link").removeClass("active");
                $(this).addClass("active");
                localStorage.setItem("ir_language", lang_selected);
                $("body").attr("data-lang", lang_selected);

            });
            $('.cssmenu > ul > li > a').click(function (e) {
                if ($(this).hasClass('has-sub')) {
                    e.preventDefault();
                }
            });
        });

        $(window).resize(function () {
            var winWidth = $(window).width();

            if (winWidth < 768) {
                if ($('#mob-lang-section #language-change-div-console').length == 0) {
                    $('#language-change-div-console').appendTo('#mob-lang-section');
                }
            } else {
                if ($('.language-change-div #language-change-div-console').length == 0) {
                    $('#language-change-div-console').appendTo('.language-change-div');
                }
                
            }
        });

    </script>
