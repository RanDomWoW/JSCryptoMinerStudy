        window.easeInElastic = function (t) {
            return (.04 - .04 / t) * Math.sin(25 * t) + 1
        };
        window.easeInQuad = function (t) {
            return t * t
        };
        window.easeInCubic = function (t) {
            return t * t * t
        };
        window.easeInQuart = function (t) {
            return t * t * t * t
        };
        window.easeInQuint = function (t) {
            return t * t * t * t * t
        };
        window.easeOutQuad = function (t) {
            return t * (2 - t)
        };
        window.easeOutQuart = function (t) {
            return 1 - (--t) * t * t * t
        };
        window.easeOutQuint = function (t) {
            return 1 + (--t) * t * t * t * t
        };
        window.easeOutCubic = function (t) {
            return (--t) * t * t + 1
        };
        window.linear = function (t) {
            return t
