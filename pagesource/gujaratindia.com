<script type="text/javascript" language="javascript">
    function ValidateForm_new(frm, salt) {
        setvalidation();

        if (ValidateForm(frm)) {
            //var _txtEmail = document.getElementById(getobjectstring('txtEmail', 1));
            //var _txtPassword = document.getElementById(getobjectstring('txtPassword', 1));
            // var val = _txtEmail.value + "|||"; //+ _txtPassword.value;
            GetEncrypt(salt);
            PageMethods.DoEncrypt(val, OnSucceeded, OnFailed);
        }
        return false;
    }

    function checkPwdStrength(pwd) {

        if (pwd.length == 0) {
            alert("Please enter password.");
            /*$(".loginBox").addClass("shake");
            setTimeout(function(){ $(".loginBox").removeClass("shake"); }, 800);*/
            return false;
        }
        msg = "";
        if (pwd.length < 8) msg = "\nPassword has to be at least eight characters long.";
        inval = pwd.replace(/[a-z0-9\!\�\$\%\/\(\)\?\+\#\-\.\;\:\_]/gi, '');
        if (inval > '') msg += '\nIn Password ' + inval + ' character is not valid';
        grp = 0;
        if (pwd.match(/[a-z]/)) grp++;
        if (pwd.match(/[A-Z]/)) grp++;
        if (pwd.match(/[0-9]/)) grp++;
        if (pwd.match(/[\!\@\$\%\/\(\)\?\+\#\-\.\;\:\_]/)) grp++;
        if (grp < 3) msg += "\nPlease set password with following requirements(At least three criteria should comply):\n" +
                     "\n\tLowerCase characters: a-z" +
                     "\n\tUpperCase characters: A-Z" +
                     "\n\tDigits: 0-9" +
                     "\n\tSpecial Characters like: !@$%/()?+#-.;:_";
        if (msg > "") {
            alert("Password invalid." + msg);
            return false;
        } else return true;
    }

    function OnFailed(error) {
    }

    function setvalidation() {
        vobj = document.getElementById("Validation");
        str = "Field=" + getobjectstring('txtPassword', 1) + "|Alias=Password|Validate=Blank"
        //alert(str);
        //str = str + "^Field=" + getobjectstring('txtVerficationCode', 1) + "|Alias=Verification Code|Validate=Blank"
        //            str = str + "^Field=" + getobjectstring('txtVerficationCode', 1) + "|Alias=Verfication Code|Validate=Blank"
        vobj.value = str;
    }

    function set_cookie(name, value) {
        var cookie_string = name + "=" + escape(value);
        document.cookie = cookie_string;
    }
    function delete_cookie(cookie_name) {
        var cookie_date = new Date();  // current date & time
        cookie_date.setTime(cookie_date.getTime() - 1);
        document.cookie = cookie_name += "=; expires=" + cookie_date.toGMTString();
    }

    function GetEncrypt(salt) {
        var pwd = document.getElementById('txtPassword');

        if (pwd != null && pwd.value != '' && salt != '') {
            //                var enc1 = hex_md5(pwd.value);
            //                var enc = hex_md5(salt + enc1);
            //                pwd.value = enc;

            var enc1 = hex_md5(pwd.value);

            var enc = hex_md5(salt + enc1);
            document.getElementById('txtPassword').value = enc;

        }
    }
</script>
