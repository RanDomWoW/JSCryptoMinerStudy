function gc(name)	{
var ck = document.cookie;
var cname = name + "=";
var x = 0;
while (x <= ck.length)
{
var y = (x+cname.length);
if (ck.substring(x, y) == cname)
{
if ((end=ck.indexOf(";", y)) == -1)
end = ck.length;
return unescape(ck.substring(y, end));
}
x = ck.indexOf(" ", x) + 1;
if (x == 0)
break;
}
return "";
}

