<form action="/de/stromtarife.htm" method="POST" class="form form--grey consumption-calculator--s1">
    <h1 class="form__header ">
        <span class="icon--electricity--medium element--inline-table">Strom Tarife</span>
    </h1>
    <fieldset class="form__section">
        <div class="form__element">
            <label class="form__element__label ">
                Ihre Postleitzahl
            </label>
            <div class="form__control">
                <input class="form__input" 
                    id="input__place"
                    data-js-controller="typeahead"
                    data-typeahead-config='{
                        "type" : "places",
                        "topic" : "zip",
                        "remoteURL": "/mb-vf_de-apis_services/LocationDataService.action?evtCityRequestJson=&amp;energyType=ELECTRICITY&userGroup=PRIVATE"
                        
                    }'
                    type="number"
                    pattern="[0-9]*"
                    
                    name="place" 
                    data-reactive='{
                        "force.sanitize": {
                            "event" : "change blur input"
                        },
                        "input.clearValue": { 
                            "topic" : "inputClear", 
                            "type" : "electricity", 
                            "event" : "click focus" 
                       } 
                    }'
                    data-sanitize='{
                        "rules": {
                            "allowedRegex": {"regex" : "^[0-9]+$" },
                            "maxlength": {"max" : 5, "msg": "" }
                        }
                    }'
                    data-validate='{
                        "rules": {
                            "required": {
                                "msg": "Bitte geben Sie eine gültige fünfstellige PLZ ein"
                            },
                            "attribute": [
                                {
                                    "name": "data-typeahead-valid",
                                    "msg": "Bitte geben Sie eine gültige fünfstellige PLZ ein"
                                },
                                {
                                    "name": "data-typeahead-selected",
                                    "msg": "Bitte wählen Sie einen Wert aus der Vorschlagliste"
                                }
                            ]
                        }
                    }'    
                    />
                    <input name="zipCode" id="input_zipCode" type="hidden" 
                        data-reactive='{
                            "setValue":{
                            "topic":"zip",
                            "event":"change",
                            "regex":"(\\d{5})"                       
                            }
                        }'
                        value=""    
                    />                                 
                    <input name="cityName" id="input_cityName" type="hidden" data-reactive='{
                        "setValue":{
                        "topic":"zip",
                        "event":"change",
                        "regex":"(\\D.*)"                           
                        }
                        }'
                        value=""
                    />   
                    <input type="hidden" name="pageLevel" value="s1" />      
            </div>
            <div class="form__text"><!-- optional -->
                <p class="form__explaination"></p>
                <p class="form__errorText"></p>
            </div>
        </div>
        
        
         
        <div class="form__element mb32" 
            data-js-controller="SliderController" 
            data-slider-config='{
                "override": "electricity",
                "initValue": 2500
            }'>
            <label class="form__element__label ">Personen im Haushalt</label>
            <div class="form__control form__input--slider">
                <div class="slider__legend--electricity"  
                     data-reactive='{
                                 "label.electricity" : {
                                    "topic" : "consumption_electricity",
                                    "event" : "click",
                                    "factor" : 1,
                                    "baseValue": 1250
                                 },
                                 "input.clearValue": {
                                    "topic" : "inputClear",
                                    "type" : "electricity",
