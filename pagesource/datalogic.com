			<script type="text/javascript">
			$(function () {

				$('#homeCarousel').carousel({
					interval: false
				})

				if ($('.prod-carousel').length > 0) {
					$('.prod-carousel > div:first').addClass('active').fadeIn(0);

					$.fn.cycle = function (timeout) {
						var l = this.length,
								current = 0,
								prev = 0,
								elements = this,
								timerId;

						if (this.filter('.active').length > 0) {
							current = this.index(this.filter('.active')[0]) + 1;
							prev = current - 1;
						}

						this.parent().hover(function () {
							clearTimeout(timerId);
						},
						function () {
							timerId = setTimeout(next, timeout);
						})

						function next() {
							elements.eq(prev).removeClass('active').fadeOut(700);
							elements.eq(current).addClass('active').fadeIn(700);
							prev = current;
							current = (current + 1) % l;
							timerId = setTimeout(next, timeout);
						}
						timerId = setTimeout(next, timeout);
						return this;
					};

					$('.prod-carousel > div').cycle(5000);
				}

			})
		</script>


