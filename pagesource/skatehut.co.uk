	(function()
	{
		var slideshow = $('component-uid526103847');
		var stash = $('component-uid526103847-stash');
		var thumbnailContainer = $('component-uid526103847-thumbnails');
		var slides = stash.getChildren();
		var thumbnails = thumbnailContainer ? thumbnailContainer.getChildren('.thumbnail') : [];
		var navigationPrevious = $('component-uid526103847-previous');
		var navigationNext = $('component-uid526103847-next');
		var transitionInterval = null;
		
		if (slides.length == 0) return;
		
		var busyAnimating = false;
		var currentSlide = 0;
		slides[currentSlide].inject(slideshow, 'top');
		
		if (thumbnails.length > 0)
		{
			thumbnails[currentSlide].addClass('selected');
		}

		playVideo();

		function supportsVideo() {
			return !!document.createElement('video').canPlayType;
		}

		function updateSlide(slideInfo, value, complete)
		{
			playVideo();
			slideInfo.slide.setStyle('left', value + '%');
			if (complete)
			{
				busyAnimating = false;
				if (currentSlide != slideInfo.index)
				{
					slides[slideInfo.index].inject(stash);
				}
			}
		}
		
		function updateSize()
		{
			var slideSize = slides[currentSlide].getSize();
			slideshow.setStyle('height', slideSize.y + 'px');
		}
		
		function showSlide(index, direction)
		{
			if (busyAnimating) return;
			if (index == currentSlide) return;
			busyAnimating = true;
			
			Tweening.start(
				{'slide': slides[currentSlide], 'index': currentSlide},
				updateSlide,
				0,
				-100 * direction,
				500,
				'sine'
			);
			
			Tweening.start(
				{'slide': slides[index], 'index': index},
				updateSlide,
				100 * direction,
				0,
				500,
				'sine'
			);
			
			slides[index].inject(slideshow, 'top');
			currentSlide = index;
			updateSize();
			
			if (thumbnails.length > 0)
			{
				thumbnails.removeClass('selected');
				thumbnails[currentSlide].addClass('selected');
			}
		};
		
		window.addEvent('resize', updateSize);
		slideshow.getElements('img').addEvent('load', updateSize);
		updateSize();

		function playVideo()
		{
			if(supportsVideo()) {
				if($$('.slideshow-video-element')) {
					$$('.slideshow-video-element').each(function(e){
						e.play();
					});
				}
			}
		}

		function navigate(direction, manual)
		{
			var newSlide = currentSlide + direction;
			if (newSlide >= slides.length) newSlide = 0;
			if (newSlide < 0) newSlide = slides.length - 1;	
			
			showSlide(newSlide, direction);
			if (manual)
			{
				window.clearInterval(transitionInterval);
			}
		}
		
		transitionInterval = window.setInterval(
			function()
			{
				navigate(1, false);
			},
			6000		);
		if (navigationPrevious)
		{
			navigationPrevious.addEvent(
				'click',
				function()
				{
					navigate(-1, true);
				}
			);
		}
		
		if (navigationNext)
		{
			navigationNext.addEvent(
				'click',
				function()
				{
					navigate(1, true);
				}
			);
		}
		
		thumbnails.each(
			function(thumbnail, index)
			{
				thumbnail.addEvent(
					'click',
					function(event)
					{
						event.preventDefault();
						showSlide(index, currentSlide < index ? 1 : -1);
						window.clearInterval(transitionInterval);
					}
				);
			}
		);
	})();

