<script type="text/javascript">  //检索词汇联想代码
function autoCompleteInput(obj,docObj)  
{  		
    $(obj).autocomplete({ 
	   	source:'/Search/searchshowAUTO.jsp?v_tablearray='+ document.getElementById('v_tablearray').value+'&v_index='+document.getElementById('v_index').value+'&sortfield=score&sorttype=desc',//可以填数据，或URL，
   	 	//返回值为JSON格式数据,发送请求时，会附带一个term参数让服务器来获取输入框内容         
        minLength:2,//最小联想长度，当输入内容长度达到这个长度时才进行联想   
        select: function(event,ui){//选中事件，当鼠标点击下拉选项时触发，主要就是将输入框内容替换       
        }  
    });  
}	

$(function(){   		
      autoCompleteInput($(document.BibFrm.v_value),document.BibFrm.v_index);  //设置要进行联想的页面对象，就是那个输入框$("#v_value") 
});

$(function(){
	var $container = $('.rightside');
	var rightSide = function(){
		$container.imagesLoaded( function(){
			$container.isotope({
				itemSelector: '.sideitem'
			});
		});
	}

	if($(window).width()<1300){
		rightSide()
	}
	var resizeTimer = null;
	$(window).on('resize', function () {

		if (resizeTimer) {
			clearTimeout(resizeTimer)
		}
		resizeTimer = setTimeout(function(){
			if($(window).width()<1300){
				rightSide()
			}else{
				$container.isotope( 'destroy' );
			}
		}, 400);
	});
	
});

</script>






