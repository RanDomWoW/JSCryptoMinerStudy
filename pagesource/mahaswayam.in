    <script type="text/javascript">
         $(document).ready(function() {
            $('#incfont').click(function(){
            curSize= parseInt($('.font-14').css('font-size')) + 2;
            if(curSize<=16)
            $('.font-14').css('font-size', curSize);
            });

           $('#deffont').on('click', function() {
              $('.font-14').css('font-size', '14px')
            })

            $('#decfont').click(function(){
            curSize= parseInt($('.font-14').css('font-size')) - 2;
            if(curSize>=12)
            $('.font-14').css('font-size', curSize);
            });
        });
    </script>


