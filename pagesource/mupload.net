<h3>Distribute your files to selected services</h3>
<script type='text/javascript'>
	var upload_id = '152154615727716806';
	var ext_allowed='';
	var ext_not_allowed='';
	var max_upload_files='5';
	var max_upload_filesize='';
	var descr_mode='1';
	var public_on = '';
	var lang_description='Description:';
	var lang_published='';

	jQuery.support.cors = true;
	function check_sites_limit(id) {
		
		var selected_sites = 0;
		$('[name="site"]').each(function() {
			if($(this).prop('checked'))selected_sites++;
		});
		if(selected_sites > 5) {
			document.getElementById(id).checked=false;
			alert('You allowed to select only 5 sites');
			return false;
		}
		
		return false;
	}
	var go_t = 1;
	function StartUpload(form, c) {
		
		var selected_sites = 0;
		$('[name="site"]').each(function() {if($(this).prop('checked'))selected_sites++;});
		if(selected_sites > 5) {
			alert('Selected too many sites for upload. Please uncheck some sites.');
			return false;
		}
		
		if(c) {
			var cf = checkForm(form);
			if(!cf) return cf;
			
		}
		$('#uploadbtn').prop("disabled", true);
		$('#filebtn').prop("disabled", true);
		$('#fileupload').hide();
		$('#bar1').show();
		setTimeout('UpdateStat()', 1000);
	}
	function update_stat(json) {
					var a = json
					if(a.error) {
						$('#error_msg').html(a.error);
						$('#uploadbtn').prop("disabled", false);
						$('#filebtn').prop("disabled", false);
						if($('#remote').val()==1 ) {
							$('#remoteupload').show();
							$('#sdownload').hide();
						} else {
							$('#fileupload').show();
						}
						$('#bar1').hide();
						$('#speed').html("");
						$('#eta').html("");
						go_t = 0;
						return ;
					}
					if(a.code && a.percent>=99) {document.location='https://www.mupload.net/'+a.code;}
					$('#bar2').width(a.percent+'%');
					$('#bar3').html(a.percent+'%');
					if(a.speed) {
						$('#speed').html(a.speed);
						$('#eta').html(a.eta+' left');
					}
					if(a.percent>0) $('#sdownload').hide();
	}
	function UpdateStat() {
//		$.get('https://www.mupload.net/'+($('#remote').val()==1?'r':'u')+'stat/'+upload_id+'&rand='+Math.random()*100, function(data) {
		$.getJSON(($('#remote').val()==1?'https://www.mupload.net/rstat/'+upload_id:'https://www.mupload.net/tmp/'+upload_id+'.json')+'?callback=?&rand='+Math.random()*100, function(json) {
			update_stat(json);
			if(go_t == 1) setTimeout('UpdateStat()', 500);
		} ).fail(function(data){setTimeout('UpdateStat()', 3000);});
	}
	var files = 0;
	function changeUpload(utype) {
		if(utype == 'file') {
				$('#fileupload').show();
			$('#filebtn').addClass('active');
			$('#remote').val(0);
		}
	}
	function StartUpload0(el) {
		var ids = new Array()
		var sites = $('input:checked');
		$(sites).each(function(){ids.push($(this).val())});
		setCookie('sites',ids.toString(), 100000, '.');
		return StartUpload(el, 1-$('#remote').val());

	}
</script>

