<script type="text/javascript">
    function onload() {
//      var cookies = decodeURIComponent(document.cookie);
      var cookies =  document.cookie;
      var cookieItem = cookies.split(";");
      var cookieArray = new Array();

      for (i = 0; i < cookieItem.length; i++) {
        cookieItem[i] = cookieItem[i].trim();
        var elem = cookieItem[i].split("=");
        cookieArray[elem[0]] = decodeURIComponent(elem[1]);
      }
      if (!cookieArray["exuname"]) {
        document.getElementById("msg").innerHTML += "ようこそ、<strong>ゲスト</strong> さん。<br/>";
      } else {
        document.getElementById("msg").innerHTML += "ようこそ、<strong>" + cookieArray["exuname"] + "</strong> ( " + cookieArray["exuid"] + " ) さん。<br/>";
      }
    }
</script>
