					if (playerId === pId) continue;
					document.getElementById(pId).sendNotification("doPause");
				}
			}
		});

	}

	var links = document.getElementsByTagName("a");
	for (var j = links.length; j--;) {
		links[j].addEventListener("click", function () {
			for (var i = kal_list.length; i--;) {
				var pId = kal_list[i];
				document.getElementById(pId).sendNotification("doPause");
			}
			return true;
		})
