					centerPadding: '20px',
					arrows: true,
					slidesToShow: 2,
					nextArrow: '<i class="fa fa-arrow-right"><span class="right"></span></i>',
					prevArrow: '<i class="fa fa-arrow-left"><span class="left"></span></i>',
					variableWidth: true
				});
		}
		
		if(document.documentElement.clientWidth < 640){
			setTimeout(function(){ 
				createMobileSlider();
				$('.row.link-slider').show();
			}, 300);
		}else{
			$('.row.link-slider').show();
		}
		
		$( window ).resize(function() {
			
			if(document.documentElement.clientWidth < 640){
				if(!$('.mobile-slider').hasClass('slick-initialized')){
					createMobileSlider();
				}
			} else {
				if($('.mobile-slider').hasClass('slick-initialized')){
					$('.mobile-slider').slick('unslick');
				}
			}
			
		});

	});
});

