				}
				translationEngine(formLabels, promoLabels);
			}
			//TRANSLATES THE PAGE
			var translationEngine = function(formLabels, promoLabels) {
				if (promoActive == 0) { // & currentCountry != "fi" & currentCountry != "lu" & currentCountry != "be" & currentCountry != "es") {
					document.getElementById('formTitlePopUp').innerHTML = formLabels[0].formTitlePopUp + "<br><div class='popup-subtitle'>" + formLabels[0].formSubTitlePopUp + "</div>";
				} else {
					document.getElementById('formTitlePopUp').innerHTML = promoLabels[0].promoTitle + "<br><div class='popup-subtitle'>" + promoLabels[0].promoSubTitle + "</div>";
				}
				document.getElementById('emailLabel').innerHTML = formLabels[0].emailLabel;
				document.getElementById('emailFieldPopUp').placeholder = formLabels[0].emailLabel;
				document.getElementById('privacyCopy').innerHTML = formLabels[0].privacyCopy;
				document.getElementById('formErrorContainerEmailCopy').innerHTML = formLabels[0].formErrorContainerEmailCopy;
				document.getElementById('formErrorContainerPrivacyCopy').innerHTML = formLabels[0].formErrorContainerPrivacyCopy;
				document.getElementById('submitButton').value = formLabels[0].submitButton;
				document.getElementById('menButton').value = formLabels[0].menButton;
				document.getElementById('womenButton').value = formLabels[0].womenButton;
				document.getElementById('okMessagePopUp').innerHTML = formLabels[0].okMessagePopUp;
				document.getElementById('okSubMessagePopUp').innerHTML = formLabels[0].okSubMessagePopUp;
				document.getElementById('koMessagePopUp').innerHTML = formLabels[0].koMessagePopUp;
				document.getElementById('opsMessagePopUp').innerHTML = formLabels[0].opsMessagePopUp;
			}
			//BUTTON HANDLING
			var buttonControl = function() {
				var buttons = document.getElementById('buttonContainerPopUp');
				var formStatus = "";
				for (var i in formControl) {
					formStatus = formStatus + formControl[i];
				}
				if (formStatus === "11") {
					for (i = 0; i < buttons.childElementCount; i++) {
						if(!buttons.children[i].classList.contains("hidden")) {
							buttons.children[i].disabled = false;
							buttons.children[i].className = "submit_button";
						}
					}
				} else {
					for (i = 0; i < buttons.childElementCount; i++) {
						if(!buttons.children[i].classList.contains("hidden")) {
							//buttons.children[i].disabled = true;
							buttons.children[i].className = "submit_button_off";
						}
					}
				}
			}
			//DATE HANDLING
	
			var reverseDate = function(date) {
				if (date != "") {
					var getDay = date.split("/")[0];
					var getMonth = date.split("/")[1];
					var getYear = date.split("/")[2];
					var newDate = getYear + "." + getMonth + "." + getDay;
					return newDate;
				} else {
					return "";
				}
			}
			//FIELD HANDLING
			var checkField = function(field, mandatory) {
				switch (mandatory) {
					case 0:
						if (field.value.trim() == '' || field.value.trim() == null) {
							field.style.cssText = okStyle;
							document.getElementById('formErrorContainerEmailCopy').classList.add("vHidden");
							formControl[field.id] = "1";
							buttonControl();
							return true;
	
						} else {
							return checkValue(field);
						}
						break;
					case 1:
						if (field.value.trim() == '' || field.value.trim() == null) {
							field.style.cssText = koStyle;
							document.getElementById('formErrorContainerEmailCopy').classList.remove("vHidden");
							formControl[field.id] = "0";
							return false;
						} else {
							return checkValue(field);
						}
				}
			}
	
			$("#emailFieldPopUp").on("input", function(e) {
				checkField(this, 1);
			});
	
			var checkValue = function(field) {
				var dateRegex = /^(?:(?:31(\/)(?:0?[13578]|1[02]))\1|(?:(?:29|30)(\/)(?:0?[1,3-9]|1[0-2])\2))(?:(?:1[6-9]|[2-9]\d)?\d{2})$|^(?:29(\/)0?2\3(?:(?:(?:1[6-9]|[2-9]\d)?(?:0[48]|[2468][048]|[13579][26])|(?:(?:16|[2468][048]|[3579][26])00))))$|^(?:0?[1-9]|1\d|2[0-8])(\/)(?:(?:0?[1-9])|(?:1[0-2]))\4(?:(?:1[6-9]|[2-9]\d)?\d{4})$/;
				var emailRegex = /[a-z0-9!#$%&'*+\/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+\/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?/i;
				var fieldType = "";
				if (field.id.match("email")) {
					fieldType = "email";
				} else {
					fieldType = "date";
				}
				switch (fieldType) {
					case "email":
						if (emailRegex.test(field.value)) {
							document.getElementById('emailFieldPopUp').style.cssText = okStyle;
							document.getElementById('formErrorContainerEmailCopy').classList.add("vHidden");
							formControl[field.id] = "1";
							return true;
						} else {
							document.getElementById('emailFieldPopUp').style.cssText = koStyle;
							document.getElementById('formErrorContainerEmailCopy').classList.remove("vHidden");
							formControl[field.id] = "0";
							return false;
						}
						break;
					case "date":
						if (dateRegex.test(field.value)) {
							document.getElementById("tempDate").style.cssText = okStyle;
							document.getElementById("dateField").value = reverseDate(field.value);
							formControl[field.id] = "1";
							return true;
						} else {
							document.getElementById("tempDate").style.cssText = koStyle;
							formControl[field.id] = "0";
							return false;
						}
				}
				buttonControl();
			}
			//GENDER HANDLING
			var setMan = function() {
				document.getElementById('radioMen').checked = true;
				sendFormPopUp();
			}
			var setWoman = function() {
				document.getElementById('radioWomen').checked = true;
				sendFormPopUp();
			}
			$("#submitButton").on("click", function(e) {
				if(legalCheck(document.getElementById('legalFlag'))) {
					document.getElementById("campaignField").value = "PopUp";
					sendFormPopUp();
				}
			});
			$("#menButton").on("click", function(e) {
				if(legalCheck(document.getElementById('legalFlag'))) {
					setMan();
				}
			});
			$("#womenButton").on("click", function(e) {
				if(legalCheck(document.getElementById('legalFlag'))) {
					setWoman();
				}
			});
	
			//LEGAL STATUS HANDLING
			var legalCheck = function(field) {
				var buttons = document.getElementById('buttonContainerPopUp');
				var addressField = document.getElementById('emailFieldPopUp');
				if (field.checked) {
					buttons.className = "";
					formControl[field.id] = "1";
					document.getElementById('formErrorContainerPrivacyCopy').classList.add("vHidden");
					if(checkField(addressField, 1)) {
						buttonControl();
						return true;
					} else {
						buttonControl();
						return false;
					}
				} else {
					buttons.className = "disabledButtons";
					formControl[field.id] = "0";
					document.getElementById('formErrorContainerPrivacyCopy').classList.remove("vHidden");
					checkField(addressField, 1);
					buttonControl();
					return false;
				}
			}
	
			$("#legalFlag").on("click", function(e) {
				legalCheck(this);
			});
	
			//BUTTON HANDLING
			var hideButtonsPopUp = function(hide) {
				//var buttons = document.getElementById("buttonContainerPopUp");
				//var wrapper = document.getElementsByClassName("wrapper")[0];
	
				var hidebuttons = $("#buttonContainerPopUp .submit_button");
				var wrapperspinner = $("#buttonContainerPopUp");
	
				var spinnerGif = document.createElement("img");
				spinnerGif.setAttribute("width", "100");
				spinnerGif.setAttribute("src", "https://images.timberland.com/is/content/TimberlandBrand/Europe/newsletter/largeSpinner.gif");
				switch (hide) {
					case 0:
						$("#buttonContainerPopUp .submit_button").show();
						$("#buttonContainerPopUp img").remove();
						break;
					case 1:
						$("#buttonContainerPopUp .submit_button").hide();
						wrapperspinner.append(spinnerGif);
				}
			}
			//FORM SUBMIT
			var sendFormPopUp = function() {
				hideButtonsPopUp(1);
				var errorMessages = /(UserAlreadySubscribed|)/;
				var formAction = document.forms["subscribeFormPopUp"].action;
				var formMethod = document.forms["subscribeFormPopUp"].method;
				jQuery.ajax({
					type: formMethod,
					url: formAction,
					data: jQuery("#subscribeFormPopUp").serialize(),
					success: function(data) {
						if (data.hasOwnProperty("errorMessage")) {
							var errorCode = data.errorMessage;
							switch (errorCode) {
								case "UserAlreadySubscribed":
									jQuery('#koMessagePopUp').removeClass('hidden');
									jQuery('#formTitlePopUp').addClass('hidden');
									hideButtonsPopUp(0);
									break;
								case "GenericError":
									jQuery('#subscribeFormPopUp').addClass('hidden');
									jQuery('#formTitlePopUp').addClass('hidden');
									jQuery('#opsMessagePopUp').removeClass('hidden');
							}
						} else {
							jQuery('#subscribeFormPopUp').addClass('hidden');
							jQuery('#formTitlePopUp').addClass('hidden');
							jQuery('#okMessagePopUp').removeClass('hidden');
							jQuery('#okSubMessagePopUp').removeClass('hidden');
							jQuery('#koMessagePopUp').addClass('hidden');
							if(data.sfAccountId != undefined) {
								dataLayer.push({
									"event":"sfUserIdentification",
									"sfUserIdentificationSource":"Newsletter Subscription: Modal",
									"sfUserId":data.sfAccountId
								});
							}
							return true;
						}
					},
					error: function(data) {
						jQuery('#subscribeFormPopUp').addClass('hidden');
						jQuery('#formTitlePopUp').addClass('hidden');
	
						jQuery('#opsMessagePopUp').removeClass('hidden');
						return false;
					}
				});
			}
	
	
			// ---------------------------------------------------------------------------------------------------------
	
			//close popup events
			$('#full-container').bind('click', function() {
				write_cookie('TBLCookiesNewsletter10off', true, '/');
				$(this).fadeOut(function() {
					$(this).remove();
				});
			}).children().on('click', function(event) {
				event.stopPropagation();
			});
	
			//$('#close-button, #full-container #container #menButton, #full-container #container #womenButton').bind('click', function () {
			$('#close-button').bind('click', function() {
				write_cookie('TBLCookiesNewsletter10off', true, '/');
				$("#full-container").fadeOut(function() {
					$(this).remove();
				});
			});
	
			// ---------------------------------------------------------------------------------------------------------
	
			$(window).scroll(function() {
				$("#container").css("top", Math.max(0, (($(window).height() - $("#container").height()) / 2) + $(window).scrollTop()) + "px");
				$("#container").css("left", Math.max(0, (($(window).width() - $("#container").width()) / 2) + $(window).scrollLeft()) + "px");
			});
	
			//EVENT LISTENER
			getCountry();
		} else {
			if(document.cookie.indexOf('TBLCookiesNewsletter10off=true') < 0) {
				write_cookie('TBLCookiesNewsletter10off', true, '/');
			}
		}
	}); //document ready
}); // require
