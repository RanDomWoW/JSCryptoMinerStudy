    jQuery(document).ready(function($) {
        var flag_url = "/wp-content/themes/hortonworks/images/svg/flags/";

        // Get the current page domain to determine if you are on a localized page
        var curr_page_host = window.location.hostname;
        var curr_page_host_arr = curr_page_host.split('.');
        var curr_page_lang = curr_page_host_arr[0];

        var curr_page_protocol = window.location.protocol;
        var curr_page_uri = window.location.pathname;

        if (curr_page_lang != 'hortonworks' && curr_page_lang != 'dev') {
            $('.lang-link').each(function(index) {
                var code = $(this).attr('code');
                var loc_code = code.split('-')[1];
                if ( loc_code == curr_page_lang ) {
                    $('.the-lang').html($(this).html());
                    $('.lang-trigger .lang-trigger-flag').attr('src', flag_url + loc_code + '.svg');
                    return false;
                }
            });
        } else {
            //Check if needs redirect which is only if they are loading the english page but have selected a language in the past
            var lang_cookie = $.cookie('selected_language');
            if ( lang_cookie && lang_cookie != '' ) {
                if (lang_cookie != 'en-us'){
                    var lang_cookie_arr = lang_cookie.split('-');
                    var loc_code = '';
                    if ( 1 in lang_cookie_arr ) {
                        loc_code = lang_cookie_arr[1] + ".";
                    } else {
                        loc_code = lang_cookie_arr[0] + ".";
                    }

                    window.location.href = curr_page_protocol + '//' + loc_code + 'hortonworks.com' + curr_page_uri;
                }
            }
        }
    }); // end document ready



    // Event handlers for the language drop-down
    jQuery('.lang-trigger-link').on('click', function( $ ) {
        jQuery('.lang-trigger ul').toggle();
    });
    jQuery('.lang-link').on('click', function( $ ) {
        var code = jQuery(this).attr('code');
        jQuery.cookie('selected_language', code, { expires : 365, path : '/', domain: '.hortonworks.com' });
    });

</script>


