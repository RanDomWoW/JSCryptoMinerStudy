<script type="text/javascript">
$(document).ready(function () {
    var images = document.querySelectorAll(".img-responsive");
    //$(".img-responsive").lazyload();
    lazyload(images);
    $('.footer-links h2').on('click', function () {
        $(this).toggleClass("highlight");
    });
    
    var TopMenuManager = function(menuSelector, unbindSelector) {
        this.menuSelector = menuSelector;
        this.unbindSelector = unbindSelector;
        this.activeItem = null;
        this.activeItemContainer = null;
        this.prevX = null;
        this.currentItem = null;
        this.isMouseMoveBound = false;
        this.transitTimer = false;
        this.setActiveItem = function(item, e) {
            this.activeItem = item.parent();            
            this.activeItem.addClass('hover');
            this.activeItemContainer = this.activeItem.parent();
            if (!this.isMouseMoveBound) {
                this.bindMouseMove(e);
            }
        }
        
        this.init = function() {
            var self = this;
            $(this.menuSelector).on('mouseover', function(e) {
                if (null === self.activeItem && null === self.activeItemContainer) {
                    self.setActiveItem($(this), e);
                    self.currentItem = $(this).parent();
                } else {
                    if (!self.isMouseMoveBound) {
                        self.bindMouseMove(e);
                    }
                    self.currentItem = $(this);
                }
            });
            $(this.unbindSelector).on('mouseover', function(e) {
                self.unbindMouseMove();
            });
        }
        this.checkMove = function(e) {
            var self = this;
            if (this.currentItem) {
                if (e.clientX < this.prevX) {
                    // moving left
                    // check if the activeItem is the same with the currentItem or not...
                    if (!this.currentItem[0].isEqualNode(this.activeItem[0])) {
                        this.reset();
                        this.setActiveItem(this.currentItem, e);
                    }
                }
                if (e.clientX > this.prevX) {
                    // moving right
                    clearTimeout(this.transitTimer);
                    this.transitTimer = setTimeout(function() {
                        self.reset();
                        self.setActiveItem(self.currentItem, e);
                    }, 50);
                }
                this.prevX = e.clientX;
            }
        }
        this.reset = function() {
            if (this.activeItem) {
                this.activeItem.removeClass('hover');
                this.activeItem = null;
                this.unbindMouseMove();
                this.activeItemContainer = null;
            }
        }
        this.bindMouseMove = function(e) {
            var self = this;
            this.isMouseMoveBound = true;
            this.prevX = e.clientX;
            this.activeItemContainer.mousemove(function(e) {
                self.checkMove(e);
            });
        }
        this.unbindMouseMove = function() {
            this.isMouseMoveBound = false;
            if (this.activeItemContainer) {
                this.activeItemContainer.unbind("mousemove");
            }
            clearTimeout(this.transitTimer);
        }
        
        this.init();
    }; 
    var topMenuManager = new TopMenuManager('.menu-opener', '.dmLevel2');
    
    $('.dropdown').hover(function () {
        $(this).find('.dropdown-menu.dmLevel1').stop(true, true).delay(250).fadeIn();
        $(this).find('.dropdown-menu.dmLevel2').first().fadeIn();
    }, function () {
        $(this).find('.dropdown-menu.dmLevel1').stop(true, true).delay(250).fadeOut();
        setTimeout(function() {
            topMenuManager.reset();
        }, 250);
    }); 
    
    $(".product-images-container").on("hover", function() {
        $.each($(this).find('.img-responsive'), function(i, v) {
            $(v).attr('src', $(v).attr('data-src'))
        })
    })
