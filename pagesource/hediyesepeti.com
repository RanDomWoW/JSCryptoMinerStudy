          $(this).find("option").each(function() {
              if($(this).attr("value") !=""){
                  template += '<a class="custom-option ' + $(this).attr("class") + '" href="https://www.hediyesepeti.com'+ $(this).attr("value") +'" data-value="' + $(this).attr("value") + '">' + $(this).html() + '</a>';
              }

          });
      } else {
          $(this).find("option").each(function () {
              if ($(this).attr("value") != "") {
                  template += '<span class="custom-option ' + $(this).attr("class") + '" data-value="' + $(this).attr("value") + '">' + $(this).html() + '</span>';
              }

          });
      }
  template += '</div></div>';
  $(this).wrap('<div class="custom-select-wrapper"></div>');
  $(this).hide();
  $(this).after(template);
  customCounter++;
  if(customCounter == customLength){
    $(".loaded").hide();
  }
});
