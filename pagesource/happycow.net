    <script id="nearby" type="text/template">

    <h2 class="geo__title mt--1 mb--2">
        <a href="/searchmap?lat={{lat}}&amp;lng={{lng}}&amp;distance=15">
            Vegan food near me
        </a>
        <i class="fa fa-map-marker" aria-hidden="true"></i>
    </h2>
    <hr>
    {{data}}
    <div class="geo__load__block media">
        {{if hasPicture }}
            <div class="geo__load__img">
                <a href="/reviews/{{pretty_url}}" class="geo__load__link">
                    <img class="geo__img img--hover thumbnail--vegan hidden-xs" title="Venue Picture of {{name}}" src="{{picture}}"  alt="Venue Picture of {{name }}"/>
                    <img class="geo__img img--hover thumbnail--vegan visible-xs" title="Venue Picture of {{name}}" src="{{thumb}}"  alt="Venue Picture of {{name}}"/>
                </a>
            </div>
        {{else}}
            <div class="geo__load__no__img {{type}}">
                <a href="/reviews/{{pretty_url}}" class="geo__load__link">
                    <div class="geo__box">
                        <p class="geo__type"></p>
                        <h4 class="geo__title">Add Photos</h4>
                    </div>
                </a>
            </div>
        {{/if}}

        <div class="geo__load__body">
            <h4 class="geo__title">
                <a href="/reviews/{{pretty_url}}" class="link--green">
                    {{name}}
                </a>
            </h4>

            <ul class="list list--inline">
                <li class="list__item">
                    {{if veganCategory|notempty }}
                        <span class="label label--vegan wenk--right wenk-length--large"
                            data-wenk="Offers completely plant based food. No animal or insect
                                ingredients (e.g. honey) are used.">
                            <img src="{{cdn}}/category/category_vegan.svg" class="label__img" alt="Vegan">{{ type|capcase}}
                        </span>
                    {{else}}
                        {{if vegetarianCategory|notempty }}
                            <span class="label label--vegetarian wenk--right wenk-length--large"
                                data-wenk="Offers vegan food with the addition
                                of dairy, eggs, honey, or other animal by-product where no animal is directly killed to produce it.">
                                <img src="{{cdn}}/category/category_vegetarian.svg" class="label__img" alt="Vegetarian">{{ type|capcase}}
                            </span>
                        {{else}}
                            {{if vegfriendlyCategory|notempty }}
                                <span class="label label--veg-options wenk--right wenk-length--large"
                                    data-wenk="Offers vegan options, but has meat, fish, or other animal products on the menu.">
                                    <img src="{{cdn}}/category/category_veg-friendly.svg" class="label__img" alt="Vegfriendly">{{ type|capcase}}
                                </span>
                            {{else}}
                                {{ type|capcase }}
                            {{/if}}
                        {{/if}}
                    {{/if}}
                </li>
                <li class="list__item">
                    {{rating}}
                        <i class="rating__icon fa fa-star" aria-hidden="true"></i>
                    {{/rating}}
                    {{if hasDecimal}}
                        <i class="rating__icon fa fa-star-half-o" aria-hidden="true"></i>
                    {{/if}}
                    {{remainingStars}}
                        <i class="rating__icon fa fa-star-o" aria-hidden="true"></i>
                    {{/remainingStars}}
                </li>
                <li class="list__item">
                    {{if review_count|more>0}}
                    <a href="/reviews/{{ pretty_url }}#reviews">
                        <p class="text--muted">{{review_count}} reviews</p>
                    </a>
                    {{else}}
                        <span class="mgray-def-color">
                            <a href="/reviews/{{pretty_url}}/write" class="link--green">Be the first to review</a>
                        </span>
                    {{/if}}
                </li>
            </ul>

            {{if review_count|more>0}}
            <div class="geo__locate__review">
                <div class="media__left">
                    {{if latestReview.avatarUrl|notempty}}
                    <img src="{{latestReview.avatarUrl}}" alt="Profile picture of member">
                    {{/if}}
                </div>
                <div class="media__body">
                    <p class="review__desc ellipsis">{{latestReview.review|chop>120}}</p>
                </div>
            </div>
            {{/if}}
        </div>
    </div>
    <hr class="line--basic {{if #|last}} line--last {{/if}}">
    {{/data}}

    <div class="geo__locate__footer">
        <div class="locate__refresh">
            <a class="link--green refresh-geo" aria-label="Refresh nearby vegan option results"><i class="fa fa-refresh" aria-hidden="true"></i></a>
        </div>
        <div class="locate__more">
            <a href="/searchmap?lat={{lat}}&amp;lng={{lng}}&amp;distance=15" class="link--green"><b>View more listings</b></a>
        </div>
    </div>
</script>

                
