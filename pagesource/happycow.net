                });
                var montserrat800 = new FontFaceObserver('Montserrat', {
                    weight: 800
                });

                var mitr300 = new FontFaceObserver('Mitr', {
                    weight: 300
                });
                var mitr400 = new FontFaceObserver('Mitr', {
                    weight: 400
                });
                var mitr500 = new FontFaceObserver('Mitr', {
                    weight: 500
                });
                var mitr600 = new FontFaceObserver('Mitr', {
                    weight: 600
                });
                var mitr700 = new FontFaceObserver('Mitr', {
                    weight: 700
                });

                var html = document.documentElement;
                if (sessionStorage.fontsLoaded) {
                    html.classList.add('fonts--loaded');
                } else {
                    html.classList.add('fonts--loading');
                    Promise.race([
                        timer(1000),
                        montserrat300.load(),
                        montserrat400.load(),
                        montserrat500.load(),
                        montserrat600.load(),
                        montserrat700.load(),
                        montserrat800.load(),
                        mitr300.load(),
                        mitr400.load(),
                        mitr500.load(),
                        mitr600.load(),
                        mitr700.load()
                    ]).then(function() {
                        html.classList.remove('fonts--loading');
                        html.classList.add('fonts--loaded');
                        sessionStorage.fontsLoaded = true;
                    }).catch(function() {
                        html.classList.remove('fonts--loading');
                        html.classList.add('fonts--failed');
                        sessionStorage.fontsLoaded = false;
                    });
                }

                function timer(time) {
                    return new Promise(function(resolve, reject) {
                        setTimeout(reject, time);
                    });
                }

            }());
