	function tog_features(el_id,foot_id,type_name) {
		if (tb_set[el_id] == 'show') {
			tb_set[el_id] = 'hide';
			document.getElementById(el_id).style.display='none';
			document.getElementById(foot_id).innerHTML='Klicken Sie hier um sich alle ' + type_name + ' anzeigen zu lassen';
		} else {
			tb_set[el_id] = 'show';
			document.getElementById(el_id).style.display='block';
			document.getElementById(el_id).style.display='table-row-group';	 
			document.getElementById(foot_id).innerHTML='Klicken Sie hier um die ' + type_name + ' auszublenden';
		}
	}
	function tog_help(el_id) {
		if (tr_set[el_id] == 'show') {
			tr_set[el_id] = 'hide';
			document.getElementById(el_id).style.display='none';
		} else {
			tr_set[el_id] = 'show';
			document.getElementById(el_id).style.display='block';
			document.getElementById(el_id).style.display='table-row';	 
		}
	}
</script> 
