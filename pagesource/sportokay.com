<script type="text/javascript">
	!(function($, window) {
		$(document).ready(function() {
			var urlPathArray = window.location.pathname.replace(/^[\/]+|[\/]+$/g, "").split( '/'),
				currentStore = urlPathArray.first() || "de_de",
				currentCountry = currentStore.split('_').first(),
				currentBaseUrl = window.location.protocol + "//" + window.location.host + "/" + currentStore + "/",
				preferredCountry = (Mage.Cookies.get("CARTWARE_IP2COUNTRY_PREFERRED_COUNTRYCODE") || "").toLowerCase(),
				semaphore = (Mage.Cookies.get("CARTWARE_IP2COUNTRY_MISMATCH_SEMAPHORE") || "").toLowerCase(),
				modalNode;

			if (preferredCountry) {
				+function(){
					$('.header-container .select-store').each(function() {
						var select = $(this),
							targetOption = select.find('option[data="' + preferredCountry + '"]');

						if (targetOption.length) {
							select.val(targetOption.val());
							if (select.data("uniformed")) {
								select.data("uniformed").update();
							}
						}
					});
				}();
			}

			if (semaphore && semaphore == currentCountry) return;
			if (preferredCountry && preferredCountry == currentCountry) return;

			modalNode = $('#countryModal');

			var switchBlock = modalNode.find(".page-switch-modal"),
				countrySelect = switchBlock.find(".country-select"),
				storeViewContainer = switchBlock.find(".storeview-container"),
				activeStoreViewContainer;

			countrySelect.on('change', function() {
				var select = $(this),
					country = select.val();

				activeStoreViewContainer = storeViewContainer.filter('[data-country="' + country + '"]');

				storeViewContainer.removeClass("active");
				activeStoreViewContainer.addClass("active");
			});


			$.ajax(currentBaseUrl + 'cartware_ip2country/ajax/getCustomerData', {
				type: "POST",
				data: {store: currentStore}
			}).done(function(data, textStatus, jqXHR) {
				if (data.mismatch) {
					modalNode.find("#located-store").append(data.store);
					modalNode.find(".located-country").text(data.countryName);
					var option = countrySelect.find("option[value='" + data.countryCode.toLowerCase() + "']"),
						selectedCountry,
						mismatchCountry,
						redirectUrl;

					if ('undefined' !== typeof option) {
						option.attr('selected', true);
						triggerEvent(countrySelect.get(0), 'change');
					}

					modalNode.modal('show');

					$(".set-country-button").click(function(e) {
						selectedCountry = countrySelect.find(":selected").val();

						if (selectedCountry && selectedCountry != currentCountry) {
							redirectUrl = activeStoreViewContainer.find(":selected").val();
						}

						modalNode.modal('hide');
					});

					modalNode.on('hide', function () {
						if (!selectedCountry) {
							mismatchCountry = currentCountry;
						}
						$.ajax({
							url: currentBaseUrl + 'cartware_ip2country/ajax/setPreferences',
							type: "POST",
							data: {mismatch: mismatchCountry, preference: selectedCountry}
						}).done(function(){
							if (redirectUrl) {
								location.href = redirectUrl;
							}
						});
						//Mage.Cookies.set("CARTWARE_IP2COUNTRY_MISMATCH_SEMAPHORE", countryMap[preferredCountry]);
					});
				}
			});

			function triggerEvent(element, eventName) {
				if ("dispatchEvent" in element) {
					var evt = document.createEvent("HTMLEvents");
					evt.initEvent(eventName, false, true);
					element.dispatchEvent(evt);
				} else if ("fireEvent" in element) {
					element.fireEvent("on" + eventName);
				}
			}
		});
	})(jQuery, this);
</script>
