<script type="text/javascript">
  function initbrandmenu()
  {
    for(x = 0; x < columns.length; x++)
    {
      thiscolumn = columns[x];
      if ((thiscolumn >= 0 || columns[x+1] > 0) && thiscolumn < (0+brandnumberofcolumnsdisplayed))
      {
        $('#brand_dropdown_alpha_letter_'+thiscolumn).addClass('brand_dropdown_alpha_active');

      }
      else
      {
        $('#brand_dropdown_alpha_letter_'+thiscolumn).removeClass('brand_dropdown_alpha_active');
      }
      if (thiscolumn == 0)
      {
        $('#menu-title_'+thiscolumn).addClass('menu-title-active');
      }
      else
      {
        $('#menu-title_'+thiscolumn).removeClass('menu-title-active');
      }
    }
    $('.menu_items_key_more').hide();
    $('#menu_items_key_more_'+(0+brandnumberofcolumnsdisplayed-1)).show();
  }
  function movetocolumn(column,highlightletter)
  {
    if (highlightletter)
    {
      for(x = 0; x < columns.length; x++)
      {
        thiscolumn = columns[x];
        if (column == thiscolumn)
        {
          $('#menu-title_'+thiscolumn).addClass('menu-title-active');
        }
        else
        {
          $('#menu-title_'+thiscolumn).removeClass('menu-title-active');
        }
      }
    }

    if (lastcolumn < column)
    {
      column = lastcolumn;
    }
    if (column < firstcolumn)
    {
      column = firstcolumn;
    }

    var thiscolumn = 0;
    var perfectmatch = false;
    var minimumcolumn = -1;
    var minimumcolumndiff = 0;

    if (column > currentcolumn)
    {
      //going up
      for(x = 0; x < columns.length; x++)
      {
        thiscolumn = columns[x];
        if (thiscolumn == column)
        {
          perfectmatch = true;
        }
        if (minimumcolumn == -1 || thiscolumn <= column)
        {
          minimumcolumn = thiscolumn;
          minimumcolumndiff = (columns[x-1]-minimumcolumn);
        }
      }
      if (!perfectmatch)
      {
        if (minimumcolumndiff < brandnumberofcolumnsdisplayed && currentcolumn < minimumcolumn)
        {
          column = minimumcolumn;
        }
      }
    }
    else
    {
      //going down
      for(x = columns.length-1; x >= 0; x--)
      {
        thiscolumn = columns[x];
        if (thiscolumn == column)
        {
          perfectmatch = true;
        }
        if (minimumcolumn == -1 || thiscolumn >= column)
        {
          minimumcolumn = thiscolumn;
          minimumcolumndiff = (columns[x+1]-minimumcolumn);
        }
      }
      if (!perfectmatch)
      {
        if (minimumcolumndiff < brandnumberofcolumnsdisplayed && currentcolumn > minimumcolumn)
        {
          column = minimumcolumn;
        }
      }
    }

    if (!highlightletter)
    {
      for(x = 0; x < columns.length; x++)
      {
        thiscolumn = columns[x];
        if (column == thiscolumn)
        {
          $('#menu-title_'+thiscolumn).addClass('menu-title-active');
        }
        else
        {
          $('#menu-title_'+thiscolumn).removeClass('menu-title-active');
        }
      }
    }

    position = $('#menu_items_key_'+column).position();
    if (position.left != undefined)
    {
      $('#brands_dropdown_table').animate({right:position.left+'px'}, brandanimationspeed, 'linear');
      currentcolumn = column;
      for(x = 0; x < columns.length; x++)
      {
        thiscolumn = columns[x];
        if ((thiscolumn >= column || columns[x+1] > column) && thiscolumn < (column+brandnumberofcolumnsdisplayed))
        {
          $('#brand_dropdown_alpha_letter_'+thiscolumn).addClass('brand_dropdown_alpha_active');
        }
        else
        {
          $('#brand_dropdown_alpha_letter_'+thiscolumn).removeClass('brand_dropdown_alpha_active');
        }
      }
      $('.menu_items_key_more').hide();
      $('#menu_items_key_more_'+(column+brandnumberofcolumnsdisplayed-1)).show();
    }
  }
</script>

