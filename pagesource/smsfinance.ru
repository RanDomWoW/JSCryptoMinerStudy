    function daysToString(days) {
        if (!days) return 0;
        mod = days % 10;
        if ((days % 100) >= 10 && (days % 100) <= 19) return 'дней';
        if (mod == 1) return 'день';
        if (mod >= 2 && mod <= 4) return 'дня';
        return 'дней';
    }

    function monthToString(months) {
        switch (months) {
            case 1:
                return "месяц";
            case 2:
            case 3:
            case 4:
                return "месяца";
            default :
                return "месяцев";

