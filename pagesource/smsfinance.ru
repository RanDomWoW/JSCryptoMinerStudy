<script type="text/javascript">
    if (window.funnelCore) {
        $(document).ready(function () {
            funnelCore.assignCallback(function() {
                funnelCore.pageVisitedEvent(environment.eventSources.PAGE_INDEX_LOADED);
            });
        });
    }

    //убираем кнопку "сегодня" во всех календарях
    $('<style type="text/css"> .ui-datepicker-current { display: none; } </style>').appendTo("head");
    $('<style type="text/css"> .ui-widget-header { background: rgb(28, 148, 196); border: none;  } </style>').appendTo("head");
    $('<style type="text/css"> .ui_tpicker_hour_slider { border: 2px solid rgb(150, 150, 150);  } </style>').appendTo("head");
    $('<style type="text/css"> .ui_tpicker_minute_slider { border: 2px solid rgb(150, 150, 150);  } </style>').appendTo("head");
    $('<style type="text/css"> .ui-slider-handle { border: 2px solid rgb(150, 150, 150);  } </style>').appendTo("head");

    $("#inputDayFrom").mask("99.99.9999");
    $("#inputTimeFrom").mask("99:99");
    $("#inputTimeTo").mask("99:99");

    var PERIOD_MS = 2 * 60 * 60 * 1000;

    var inputFromSrc = {
        timeOnlyTitle: 'Выберите время',
        timeText: 'Время',
        hourText: 'Часы',
        minuteText: 'Минуты',
        secondText: 'Секунды',
        closeText: 'Закрыть',
        timeOnly: true,

        onSelect: function (datetimeText, datePicker) {
            updateTimeTo();
        }
    };

    function getMoscowCurrentDate() {
        var moscowUtcOffset = 3;
        var date = new Date();
        var utcTime = date.getTime() + (date.getTimezoneOffset() * 60000);
        return new Date(utcTime + (3600000 * moscowUtcOffset));
    }

    function copyHours(srcDate, destDate) {
        destDate.setHours(srcDate.getHours(), srcDate.getMinutes(), 0, 0);
    }

    function updateTimeLimits() {
        var currentDate = getMoscowCurrentDate();

        var selectedDay = $('#inputDayFrom').datetimepicker("getDate");
        var inputTimeFrom = $('#inputTimeFrom');
        var timeFrom = inputTimeFrom.datetimepicker("getDate");

        if (currentDate.getFullYear() == selectedDay.getFullYear() &&
                currentDate.getMonth() == selectedDay.getMonth() &&
                currentDate.getDay() == selectedDay.getDay()) {
            inputFromSrc.minDateTime = new Date(2050, 1, 1, currentDate.getHours(), currentDate.getMinutes());
            if ((timeFrom.getHours() < currentDate.getHours())
                    || (timeFrom.getHours() == currentDate.getHours() && timeFrom.getMinutes() < currentDate.getMinutes())) {
                copyHours(currentDate, timeFrom);
            }
        }
        else {
            inputFromSrc.minDateTime = new Date(2050, 1, 1);
        }
        inputTimeFrom.removeClass("hasDatepicker");
        inputTimeFrom.datetimepicker(inputFromSrc).datetimepicker("setDate", timeFrom).keypress(updateTimeToIfValid);
        updateTimeTo();
    }

    function updateTimeTo() {
        var inputTimeFrom = $('#inputTimeFrom');
        var inputTimeTo = $('#inputTimeTo');
        // высчитываем дату-время с по
        var thisDate = $('#inputDayFrom').datepicker('getDate');
        var srcTimeFrom = inputTimeFrom.datetimepicker('getDate');
        if (srcTimeFrom == null) {
            //вводили дату с самого начала, не было инициализации input'а датой, только ввели текст
            var textValue = inputTimeFrom.val();
            if (/\d\d:\d\d/.test(textValue)) {
                var splitTimeText = textValue.split(":");
                var emptyDate = new Date();
                emptyDate.setHours(+splitTimeText[0], +splitTimeText[1]);
                inputTimeFrom.datetimepicker('setDate', emptyDate);

                srcTimeFrom = inputTimeFrom.datetimepicker('getDate');
            } else {
                return;
            }
        }
        var datetimeFrom = new Date(thisDate.getTime());
        copyHours(srcTimeFrom, datetimeFrom);
        var datetimeTo = new Date(datetimeFrom.getTime() + PERIOD_MS);

        // записываем в поля с результатом
        $("#inputDateFrom").datetimepicker('setDate', datetimeFrom);
        $("#inputDateTo").datetimepicker('setDate', datetimeTo);

        //отображаем
        var time = inputTimeFrom.datetimepicker('getDate');
        copyHours(datetimeFrom, time);
        inputTimeFrom.datetimepicker('setDate', time);

        time = inputTimeFrom.datetimepicker('getDate');
        copyHours(datetimeTo, time);
        inputTimeTo.datetimepicker('setDate', time);
    }

    function updateTimeToIfValid() {
        var value = $('#inputTimeFrom').val();
        if (/\d\d:\d\d/.test(value)) {
            updateTimeTo();
        }
    }

    function hideFields() {
        if ($('#inputAnyDate').prop("checked")) {
            $("#fieldDayFrom").hide();
            $("#fieldTimeFrom").hide();
            $("#fieldTimeTo").hide();
        } else {
            $("#fieldDayFrom").show();
            $("#fieldTimeFrom").show();
            $("#fieldTimeTo").show();
        }
    }

    jQuery(document).ready(function () {
        var startDate = getMoscowCurrentDate();

        $("#inputPhone").mask("8(999)9999999");

        $('#inputDayFrom').datepicker({
            timeOnlyTitle: 'Выберите время',
            closeText: 'Закрыть',
            dateFormat: 'dd.mm.yy',
            showTime: false,
            showHour: false,
            showMinute: false,
            gotoCurrent: true,
            showButtonPanel: true,
            minDate: startDate,

            onSelect: function (datetimeText, datePicker) {
