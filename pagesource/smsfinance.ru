                            loanStatus = 'Old'
                        } else {
                            loanStatus = 'New'
                        }
                        dataLayer.push({
                            'event': 'loan_application',
                            'applicationId': data.msg,
                            'applicationAmount': $('#amountSum').val(),
                            'applicationType': 'MAIN',
                            'clientType': loanStatus,
                            'country': 'Russia',
                            'customerNumber': '',
                            'product': 'SMSFINANCERU'
                        });

                    } else {
                        CustomModal.showError(data.msg);
                    }
                    $('#firstStepButton').removeAttr('disabled');
                })
            });
        } else {
            CustomModal.showError('Введена неверная сумма');
        }
    }

    // Инициализация селектбокса с выбором суммы займа
    function amountSumInit() {

        if (parseInt(0) == 1) {
            return false;
        }

        var i = 0;
        var amounts = [1000, 1500, 2000, 2500, 3000, 3500, 4000, 5000, 6000, 7000, 8000, 9000, 10000, 11000, 12000,
            13000, 14000, 15000, 16000, 17000, 18000, 19000, 20000, 21000, 22000, 23000, 24000, 25000, 26000, 27000,
            28000, 29000, 30000];

        $("#receiveForm")[0].reset();
        $("#amountSum").children().remove();

        while (amounts[i] < "0" && i < amounts.length) {
            $("#amountSum").append($("<option></option>").attr("value", amounts[i]).text(amounts[i]));
            i++;
        }
        $("#amountSum").append($("<option></option>").attr("value", 0).text(0));
        $("#amountSum").val(0);

        $("#amountSum").trigger("change");
    }

    $("#amountSum").on("change", function () {
        var pattern = /[0-9]+/;
        if ($('#amountSum').val() > 15000 && $('#amountSum').val() <= 30000) {
            $('#amountToReturnLabel').html('К возврату каждый месяц:');
            $('#amountToReturn').val(countPay(amountStartLong, amountStepLong, amountsLong));
            $('#paymentType').html('Способ получения - ');
        } else if ($('#amountSum').val() >= 1000 && $('#amountSum').val() <= 15000) {
            $('#amountToReturnLabel').html('К возврату через 30 дней: ');
            $('#amountToReturn').val(countPay(amountStart, amountStep, amounts));
            $('#paymentType').html('Способ получения - ');
        } else {
            if (!pattern.test($('#amountSum').val()) && $('#amountSum').val() != "") {
                CustomModal.showError('Вы ввели недопустимый символ.');
            }
        }
    });

    // Вернет платеж
    function countPay(amountStart, amountStep, amounts) {
        var amount = Number($("#amountSum").val());
        var index = Math.floor((amount - amountStart) / amountStep);
        return amounts[index];
    }

    $("#modal4700").on("show.bs.modal", amountSumInit);
