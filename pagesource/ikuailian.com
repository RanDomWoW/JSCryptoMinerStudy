<script>


    //设置banner
    var mySwiper = new Swiper('.swiper-container',{
        autoplay:0,
        speed:500,
        mode : 'vertical',
        pagination:'.pagination',
        paginationClickable :true,
        mousewheelControl:true,
        slidesPerView:1,
        onSlideChangeStart:function(swiper){
            console.log(swiper);
            if(swiper.activeIndex == 1){
                $(".index .page2 .left").css("left",0);
                $(".index .page2 .right").css("right",0);
            }
            if(swiper.activeIndex >= 1){
                var lazys = $(".swiper-slide").eq(1+swiper.activeIndex).find(".swiper-lazy");
                for(var i = 0 ;i<lazys.length;i++){
                    if(lazys.eq(i).attr("data-background")){
                        lazys.eq(i).css({"background-image":"url("+lazys.eq(i).attr("data-background")+")"});
                        lazys.eq(i).attr("data-background","");
                    }else if(lazys.eq(i).attr("data-src")){
                        lazys.eq(i).attr("src",lazys.eq(i).attr("data-src"));
                        lazys.eq(i).attr("data-src","");
                    }else{return;}
                }
            }
        }
    });
</script>
