        <script type="text/javascript">

          $(function () {          
          
            //Load quick claims for autoflex if URL contains autoflex=claims
            var current = window.location.href;
            if (current.split("?")[1] == "autoflex=claims") {
                $("#claimsModal").modal();
            }

            // This is not the real function
            $("span#setupAcct").on("click", function (e) {
                e.preventDefault();
				
                $("#setupModal").modal("show");

                if ($("span#setupAccount2").data("uid") != "") {
                    $("div#setupForm").hide();
                    $("div#securityQuestion").show();
                } else {
                    $("div#setupForm").show();
                    $("div#securityQuestion").hide();
                }
            });

            $("#confpassword").on("blur", function (e) {
                PasswordMatch()
            });

            function PasswordMatch() {
                var pass = $("#npassword").val();
                if (pass != undefined || pass != "") {
                    var confpass = $("#confpassword").val();
                    if (confpass != undefined || confpass != "") {
                        if (confpass != pass) {
                            $("#confpassword").addClass("invalid");
                            $("#npassword").addClass("invalid");
                            return false;
                        }
                        else {
                            $("#confpassword").addClass("valid");
                            $("#npassword").addClass("valid");
                            return true;
                        }
                    }
                    else {
                        $("#confpassword").addClass("invalid");
                        $("#npassword").addClass("invalid");
                        return false;
                    }
                }
                else {
                    $("#confpassword").addClass("invalid");
                    $("#npassword").addClass("invalid");
                    return false;
                }
            }

            // First level
            $("span#setupAccount").on("click", function (e) {

                e.preventDefault();
				
                var clientno = 0;
                var custno = 0;
                var enrolleeno = 0;
                var rsapin = "";
                var match = PasswordMatch();
                if (!match) {
                    iAlert("Password do not match");
                    return false;
                }

                var a = form_validate($('div#setupForm'));
                if (a) {

                    $("#loader").show();

                    var name = $("#fName").val() + " " + $("#lName").val();
                    var phone = $("#phoneno").val();
                    var email = $("#email").val();
                    var password = $("#npassword").val();

                    if ($.trim($("#clientno").val()) != "" && !isNaN($("#clientno").val())) {
                        clientno = parseInt($("#clientno").val());
                    }

                    if ($.trim($("#custno").val()) != "" && !isNaN($("#custno").val())) {
                        custno = parseInt($("#custno").val());
                    }

                    if ($.trim($("#enrolleeno").val()) != "" && !isNaN($("#enrolleeno").val())) {
                        enrolleeno = parseInt($("#enrolleeno").val());
                    }

                    if ($.trim($("#rsapin").val()) != "") {
                        rsapin = $("#rsapin").val();
                    }

                    var values = {
                        data: {
                            FullName: name,
                            PhoneNumber: phone,
                            CustomerNumber: custno,
                            Email: email,
                            EnrolleeNumber: enrolleeno,
                            RSAPIN: rsapin,
                            Password: password,
                            ClientNo: clientno
                        }
                    };

                    $.ajax({
                        url: "myaxa/index.aspx/processData",
                        type: "POST",
                        contentType: "application/json",
                        data: JSON.stringify(values),
                        dataType: "json",
                        success: function (result) {
                            console.log(result);
                            var res = result.d;

                            if (res.IsSuccessful) {
                                $("div.securitypanel").html(res.message);

                                $("div#setupForm").hide();
                                $("div#securityQuestion").show();

                                $("#setupAccount2").attr("data-uid", res.ReturnCode).show();
                                $("#setupAccount").hide();

                            } else {
                                $("#setupAccount2").hide();
                                $("#setupAccount").show();
                                $("#setupModal").modal("hide");
                                $("div#securityQuestion").hide();
                                iAlert(res.message);
                                //iAlert("Unsuccessful Sign-up");
                            }

                            $("#loader").hide();
                        },
                        error: function (xhr, status) {
                            iAlert("An error occured while adding your accounts. Please try again.");
                            defaultAct();
                            $("#loader").hide();
                        }
                    });
                }

            });

            // Second level            
            $("span#setupAccount2").on("click", function (e) {
                e.preventDefault();
                console.log(parseInt($(this).data("uid")));
                var a = form_validate($('div#securityQuestion'));
                if (a) {

                    $("#loader").show();

                    var answers = [];
                    $("div.securitypanel input").each(function () {
                        answers.push({
                            Answer: $(this).val(),
                            CustomerID: $(this).data("cid"),
                            Question: "",
                            SubsidiaryID: $(this).data("sid")
                        });
                    });

                   var values = { data: answers, userid: parseInt($(this).data("uid")) };

                    $.ajax({
                        url: "myaxa/index.aspx/processAnswers",
                        type: "POST",
                        contentType: "application/json",
                        data: JSON.stringify(values),
                        dataType: "json",
                        success: function (result) {
                            // console.log(result);
                            var res = result.d;

                            if (res.IsSuccessful) {
                                if (res.ReturnCode == "1") {
                                    $("#setupModal").modal('hide');
                                    $("#setupAccount2").hide();
                                    $("#setupAccount").show();
                                    iAlert(res.message);
                                } else {
                                    $("#setupModal").modal('hide');
                                    $("#setupAccount2").hide();
                                    $("#setupAccount").show();
                                    iAlert("A confirmation link has been sent to your email.\n\nWe could not verify your <strong>" + res.message + "</strong> account(s)");
                                }
                            } else {
                                $("#setupModal").modal('hide');
                                iAlert(res.message);
                                //iAlert("Unsuccessful Sign-up");
                                defaultAct();
                            }

                            $("#loader").hide();
                        },
                        error: function (xhr, status) {
                            $("#setupModal").modal('hide');
                            iAlert("An error occured while adding your accounts. Please try again.");
                            defaultAct();
                            $("#loader").hide();
                        }
                    });
                }
            });

            function defaultAct() {
                $("div.securitypanel").html("");

                $("div#setupForm").show();

                $("div#securityQuestion").hide();

                $("#setupAccount2").attr("data-uid", "").hide();
                $("#setupAccount").show();

                $("#setupModal").modal("hide");
            }

        });

        </script>


</body>

</html>