                var windowTop = $(window).scrollTop();
                
                if ( windowTop >= widgetTop ) {
                    if ( isPlay ){
                        $videoFrameBox.addClass('affix');
                    }

                } else{
                    $videoFrameBox.removeClass('affix');
                }
                //
                if ( windowTop >= widgetTop ) {
                    $backToNews.fadeIn();
                } else{
                    $backToNews.fadeOut();
                }

            });
            //close and stop video
            $('.js-video-close').click(function(){
                player.stopVideo();
                isPlay = false;
                $videoFrameBox.removeClass('affix');
                $backToNews.fadeIn();

            });
            //play video
            $backToNews.click(function(){
                player.playVideo();
                isPlay = true;
                $videoFrameBox.addClass('affix');
                $backToNews.fadeOut();
            });
        });
</script>
