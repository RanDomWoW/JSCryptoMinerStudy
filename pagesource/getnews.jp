        var r = Math.floor(Math.random()*5);
        var url = 'http://getnews.jp/archives/tag/'+tag+'/feed';
        $.ajax({
            url: url,
            dataType:'xml',
            success: function(xml){
                var tagt = $($(xml).find('title')[0]).text();
                tagt = tagt.split('» ').pop().replace('ガジェ通','');
                var item = $(xml).find('item')[r];
                var id = $(item).find('link').text().split('/').pop();
                var t = $($(item).find('title')[0]).text();
                var txt = '<div class="widget-header"><i class="mark-name"> '+tagt+' </i><a href="/archives/tag/'+tag+'"><span class="more-link"></span></a></div>';
                txt += '<div class="bg-box"><a href="'+$(item).find('link').text()+'">';
                txt += '<img src="http://px1img.getnews.jp/thumb/tag_Thumb/'+id+'.jpg" alt="'+t+'">';
                txt += '<p class="layer_1">'+t+'</p>';
                txt += '</a></div>';
                $(v).empty().append(txt);
            }
        });
    });
});
//-->
</script>
