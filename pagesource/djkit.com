<script type="text/javascript">
$(function () {
	$('#subscribeAjaxForm').submit(function (event) {
		event.preventDefault();
		var $form = $(this);
		var data = $form.serialize();
		var $inputs = $('input', this).prop('disabled', true).addClass('disabled');

		$.ajax({
			url: this.action,
			type: this.method,
			data: data
		}).done(function (json) {
			$inputs.prop('disabled', false).removeClass('disabled');

			if (json.error) {
				alert('Error: '+json.message);
				return;
			}
			$form.remove();
			$('#subscribedMsg').show();
		});
	});
});
</script>
