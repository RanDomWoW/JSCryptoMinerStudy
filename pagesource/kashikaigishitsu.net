<script>
    $(function () {
        var nowTemp = new Date();
        var now = new Date(nowTemp.getFullYear(), nowTemp.getMonth(), nowTemp.getDate(), 0, 0, 0, 0);
        $('#form_search_space [name="sch_use_date"]').datepicker({
            locale: 'ja',
            format: 'yyyy/mm/dd',
            onRender: function(date) {
                return date.valueOf() < now.valueOf() ? 'disabled' : '';
            }
        }).on("changeDate", function() {
			$(this).data('datepicker').hide();
			$('#serchMenu [name=sch_use_date]').val($('#form_search_space [name="sch_use_date"]').val().replace(/\//g,'-'));
        }).on("change", function(){
			$('#serchMenu [name=sch_use_date]').val($('#form_search_space [name="sch_use_date"]').val().replace(/\//g,'-'));
		});

        var sch_use_time_from = $('#form_search_space [name="sch_use_time_from"]');
        sch_use_time_from.timepicker({
            defaultTime: false,
            snapToStep: true,
            minuteStep: 15,
            showMeridian: false,
//            showInputs: false,
//            disableFocus: true,
        });
		var usetime_change_val="";
        sch_use_time_from.timepicker().on('show.timepicker', function(e) {
            if(!sch_use_time_from.val()) 
                sch_use_time_from.val('09:00');
        });
        sch_use_time_from.timepicker().on('hide.timepicker', function(e) {
			if (usetime_change_val==""){
				sch_use_time_from.val(usetime_change_val);
			}
        });
        sch_use_time_from.timepicker().on('changeTime.timepicker', function(e) {
            usetime_change_val = sch_use_time_from.val()
            $('#serchMenu [name="sch_use_time_from"]').val(usetime_change_val);
            var h = i = '';
            if(usetime_change_val!='') {
                split_hi = usetime_change_val.split(":");
                h=split_hi[0];
                i=split_hi[1];
            }
            $('#serchMenu [name=sch_use_time_from_hour]').val(h);
            $('#serchMenu [name=sch_use_time_from_minute]').val(i);
        });
        $('#serchMenu [name=sch_use_time_from_hour]').change(function(){
            var h = $(this).val();
            var i = $('#serchMenu [name=sch_use_time_from_minute]').val();
            var hi = h+":"+i;
            if (h=="" || i=="") {
				hi = "";
				$('#serchMenu [name=sch_use_time_from_minute]').val('');
			}
            $('#serchMenu [name="sch_use_time_from"]').val(hi);
            $('#form_search_space [name="sch_use_time_from"]').val(hi);
        });
        $('#serchMenu [name=sch_use_time_from_minute]').change(function(){
            var i = $(this).val();
            var h = $('#serchMenu [name=sch_use_time_from_hour]').val();
            var hi = h+":"+i;
            if (h=="" || i=="") {
				hi = "";
				$('#serchMenu [name=sch_use_time_from_hour]').val('');
			}
            $('#serchMenu [name="sch_use_time_from"]').val(hi);
            $('#form_search_space [name="sch_use_time_from"]').val(hi);
        });

        $('#sub-search-collapse').on('show.bs.collapse', function() {

        });
        // 利用時間
        $('#form_search_space [name=sch_use_time]').change(function(){
            $('#serchMenu [name=sch_use_time]').val($(this).val());
        });
        $('#serchMenu [name=sch_use_time]').change(function(){
            $('#form_search_space [name=sch_use_time]').val($(this).val());
        });
        // 人数
        $('#form_search_space [name=sch_occupancy_from]').change(function(){
            $('#serchMenu [name=sch_occupancy_from]').val($(this).val());
        });
        $('#serchMenu [name=sch_occupancy_from]').change(function(){
            $('#form_search_space [name=sch_occupancy_from]').val($(this).val());
        });
        // エリア
        $('#form_search_space [name=sch_area]').change(function(){
            $('#serchMenu [name=sch_area]').val($(this).val());
        });
        $('#serchMenu [name=sch_area]').change(function(){
            $('#form_search_space [name=sch_area]').val($(this).val());
        });
        // 利用用途
        $('#form_search_space [name=sch_use_applications]').change(function(){
            $('#serchMenu [name=sch_use_applications]').val($(this).val());
        });
        $('#serchMenu [name=sch_use_applications]').change(function(){
            $('#form_search_space [name=sch_use_applications]').val($(this).val());
        });
        // 会場タイプ
        $('#form_search_space [name=sch_venue_type]').change(function(){
            $('#serchMenu [name=sch_venue_type]').val($(this).val());
        });
        $('#serchMenu [name=sch_venue_type]').change(function(){
            $('#form_search_space [name=sch_venue_type]').val($(this).val());
        });
        
        
    });
</script>

