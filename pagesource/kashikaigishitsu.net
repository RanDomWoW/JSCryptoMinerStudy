				$("form.kashikaigishitsu_search").attr("action","/search/");
				break;
			case '2':
				$("form.kashikaigishitsu_search").attr("action","/search-rooms/");
				break;
		}
	});
	getAreaCity();

});
function getAreaCity(){
	var obj = $("select[name=sch_area_region_id]");
	var url = '/get_area_city.php';
	var data = {
			'area_region_id': obj.val()
		}
	$.ajax({
		type: 'GET',
		url: url,
		async: false,
		data: data,
		dataType: 'json'
	}).done(function(res){
		if (res['code'] == "SUCCESS"){
			$("select[name=sch_area_city_id]").html(res['data']['html']);
		} else {
			alert(res['msg']);
		}
	}).fail(function(){
		alert("通信エラー");
	}).always(function(){
	});
}
</script>
