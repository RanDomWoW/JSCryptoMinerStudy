              questionSelect.appendChild(defaultOption);
              questionSelect.appendChild(questionOption1);
              questionSelect.appendChild(questionOption2);
              questionSelect.appendChild(questionOption3);
              questionSelect.appendChild(otherOption);

              questionDiv.appendChild(questionSelect);

