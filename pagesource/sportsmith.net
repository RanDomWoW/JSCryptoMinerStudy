              } else if (element.id === "chatSerialSelect") {
                option.textContent = "Select Serial No.";
              }
              element.innerHTML = "";
              element.appendChild(option);
            }


            function QuestionAnswer(questionId) {
              var chat = questionModal.querySelector("#chatDiv");
              var question = Chat.questions.find(function (element) {
                return element.id === parseInt(questionId);
