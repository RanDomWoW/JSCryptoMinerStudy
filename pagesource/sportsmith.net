<script type="text/javascript">

  document.getElementById('brand').addEventListener('change', function (e) {
    ResetEquipmentSelectors(e.target);
  });

  document.getElementById('make').addEventListener('change', function (e) {
    ResetEquipmentSelectors(e.target);
  });
  document.getElementById('model').addEventListener('change', function (e) {
    ResetEquipmentSelectors(e.target);
  });
  document.getElementById('serial').addEventListener('change', function (e) {
    ResetEquipmentSelectors(e.target);
  });

  function ResetEquipmentSelectors(element) {
    console.log("Function.ResetEquipmentSelectors");
    switch (element.id) {
      case "brand":
        ClearDropdowns(document.getElementById("make"));
        ClearDropdowns(document.getElementById("model"));
        ClearDropdowns(document.getElementById("serial"));
        break;
      case "make":
        ClearDropdowns(document.getElementById("model"));
        ClearDropdowns(document.getElementById("serial"));
        break;
      case "model":
        ClearDropdowns(document.getElementById("serial"));
        break;
      case "serial":
        break;
      default:
        console.log("ResetDropdowns.Switch.Default");
        break;
    }

  }

  function ClearDropdowns(element) {
    element.disabled = true;
    var option = document.createElement("option");
    if (element.id === "brand") {
      option.textContent = "Select Brand";
    } else if (element.id === "make") {
