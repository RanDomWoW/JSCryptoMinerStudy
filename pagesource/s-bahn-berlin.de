    function setMobileRedirectCookie() {
        var expireDays = 1;
        var d = new Date();
        d.setTime(d.getTime() + (expireDays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();
        var path = 'path=/';
        document.cookie = 'use_version=www; ' + expires + ';' + path;
    }

    // Set cookie if query paremeter version is set to "www"
    if (getParameterByName('version') === 'www') {
        setMobileRedirectCookie();
