					// Protokoll bestimmen
					if (typeof protocol == 'undefined' || ('http' !== protocol && 'https' !== protocol)) {
						if (newUrl != '') {
							protocol = window.location.protocol.replace(/\:/, '');
						}
					}

					var newUrl = '';

					// Ist das Base Element vorhanden, wenn ja, dieses nutzen
					if (typeof $('base').attr('href') !== 'undefined') {
						newUrl += $('base').attr('href');
					} else {
						newUrl += 'http://www.s-bahn-berlin.de/';
					}

					// Ist ein Protokoll definiert, wenn ja, dieses durch das andere ersetzen
					if (typeof protocol !== 'undefined'
						&& ('http' == protocol || 'https' == protocol)) {
						newUrl = newUrl.replace(/^(?:http(s?)\:)?/, protocol+':');
					}

                    // trim BaseUrl aus url
                    var reg = new RegExp('^http://www.s-bahn-berlin.de');
                    url = url.replace(reg, '');
                    var reg = new RegExp('^/');
                    url = url.replace(reg, '');

					// trim trailing slash aus newUrl
					newUrl = newUrl.replace(/\/$/g, '') + '/';

					// trim leading slash aus url
					newUrl += url.replace(/^\//g, '');

					return newUrl;
				}
				return false;
			}
