  <script type="text/javascript">
    $(document).ready(function(){

      //deter larger images on load
      function init() {
      $('body').addClass('filterAll');
      var imgDefer = document.getElementsByTagName('img');
      for (var i=0; i<imgDefer.length; i++) {
      if(imgDefer[i].getAttribute('data-src')) {
      imgDefer[i].setAttribute('src',imgDefer[i].getAttribute('data-src'));
      } } }

      window.onload = init;

      //rotate through large images
      var counter = 2;

      function imageLoop () {           
        setTimeout(function () {    
          var currentPic = ('.pic-'+(counter-1));
          var nextPic = ('.pic-'+counter);

          $(currentPic).removeClass('fadeInImage');
          $(currentPic).addClass('fadeOutImage');
          $(nextPic).removeClass('fadeOutImage');
          $(nextPic).addClass('fadeInImage');
         
          counter++;                    
          if (counter < 7) {            
             imageLoop();             
          }
          else {
            counter = 2;
            $('.pic-1').removeClass('fadeOutImage');
            $('.pic-1').addClass('fadeInImage');
            imageLoop();
          }                        
        }, 8500)
      }

      imageLoop(); 

      //scroll to and highlight form for later ctas
      $('.formFocus').click(function(){
        if ($(window).width() <= 600) {
          $('html, body').animate({
            scrollTop: $('.registration').offset().top
          }, 500);
        }
        else {
          $('html, body').animate({
            scrollTop: $('body').offset().top
          }, 500);
          $('#id_first_name').focus();
        }
        $('#id_first_name').addClass('highlight');
        setTimeout(function(){
          $('#id_first_name').removeClass('highlight');
        }, 2000);
      });


    });
 </script>



