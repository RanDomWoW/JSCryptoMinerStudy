    <script>
    $(function() {

      var pageWidth = $(window).width();

      $('.top_bar #nav-close').click(function() {
       $('nav').animate({'right':'-100%'}, 'slow');
       $('html').removeClass('noscroll');
      });

      $('.top_bar #mystuff').click(function() {
        $('#mystuff_drop').slideToggle();
      });

      $('.top_bar .menu-collapse').click(function() {
       $('nav').animate({'right':'0'}, 'slow');
       $('html').addClass('noscroll');
      });

      $('.top_bar div.active').click(function() {
        if ($(this).hasClass('isDown') ) {
            $('#mystuff_drop').animate({'right':'-100%'}, 'slow');          
            $(this).removeClass('isDown');
        } else {
            $('#mystuff_drop').animate({'right':'0'}, 'slow');  
            $(this).addClass('isDown');
        }
      });

      if(typeof(Modernizr) != "undefined" && Modernizr.input.placeholder){
        $("#body").addClass("has-placeholder");
      }

      // messages
      $("div.error_container").delay(1000).animate({
        left: '+=380'
      }).delay(5000).animate({
        left: '-=380'
      });
      
      // slide up message
      $(window).scroll(function () {
        if ($(this).scrollTop() > 200) {
          if ($.cookie('fb-out') != 'true' ) { 
            $(".slide_up").slideDown(800);
            $(".close_banner").click(function() {
              $(".slide_up").slideUp(500);
              var date = new Date();
              var minutes = 120;
              date.setTime(date.getTime() + (minutes * 60 * 1000));
              $.cookie('fb-out', 'true', { expires: date, path: '/' });
              return false;
            });
          }
        }
      });

      // slide up message logged in
      $(window).scroll(function () {
        if ($(this).scrollTop() > 200) {
          if ($.cookie('fb-in') != 'true' ) { 
            $(".slide_up_in").slideDown(800);
            $(".close_banner").click(function() {
              $(".slide_up_in").slideUp(500);
              $.cookie('fb-in', 'true', { expires: 7, path: '/'});
              return false;
            });
          }
        }
      });

      // exit intent 10k cta on desktop
      $(document).on('mouseleave', leaveFromTop);
      function leaveFromTop(e){
          if ($.cookie('exit-intent') != 'true' ) {
            if( e.clientY < 0 ) {// less than 60px is close enough to the top
              $(".overlay-cta").fadeIn();
            }
          }
      }

      // hide exit intent 
      $(".overlay-cta, .overlay-cta .content .close_exitIntent").click(function() {
        $(".overlay-cta").hide();
        $.cookie('exit-intent', 'true', { expires: 3, path: '/'});
      });


      if ($(window).width() <= 479) {
        // Hide Header on on scroll down
        var didScroll;
        var lastScrollTop = 0;
        var delta = 5;
        var navbarHeight = $('.top_bar').outerHeight();

        $(window).scroll(function(event){
            didScroll = true;
        });

        setInterval(function() {
            if (didScroll) {
                hasScrolled();
                didScroll = false;
            }
        }, 250);

        function hasScrolled() {
            var st = $(this).scrollTop();
            
            // Make sure they scroll more than delta
            if(Math.abs(lastScrollTop - st) <= delta)
                return;
            
            // If they scrolled down and are past the navbar, add class .nav-up.
            // This is necessary so you never see what is "behind" the navbar.
            if (st > lastScrollTop && st > navbarHeight){
                // Scroll Down
                $('.top_bar').removeClass('nav-down').addClass('nav-up');
                $('.mobile_details .banner').addClass('pt-0');
            } else {
                // Scroll Up
                if(st + $(window).height() < $(document).height()) {
                    $('.top_bar').removeClass('nav-up').addClass('nav-down');
                    $('.mobile_details .banner').removeClass('pt-0');
                }
            }
            
            lastScrollTop = st;
        }
      }

      // dropdown menus
      var megaConfig = {
             interval: 100,
             sensitivity: 7,
             over: function(){
          $(this).find("ul").show();
         },
             timeout: 300,
             out: function(){
          $(this).find("ul").hide();
         }
        };

      $(".content_category").hoverIntent(megaConfig);

      $("a.tracked, button.tracked").click(function(){
        $(this).removeClass("tracked");
        try{
          
  
        }
        catch(e){}
        return true;
      });

      //accordion for footer links
      $('.accordion').click(function() {
        $(this).next('ul').slideToggle();
        $(this).children('.toggle').toggleClass('rotate');
        return false;
      });

      //offsets for inline anchors
      $('a[href*="#"]:not([href="#"])').click(function() {
        if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
          var target = $(this.hash);
          target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
          if (target.length) {
            $('html, body').animate({
              scrollTop: target.offset().top - 150
            }, 1000);
            return false;
          }
        }
      });

    });

  </script>

  


  

