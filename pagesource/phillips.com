    <script>
      $('a\[data-segment\]').on('click', function(e) {
        ga('send', {
          hitType: 'event',
          eventCategory: 'Homepage / Footer',
          eventAction: 'footer link clicked',
          eventLabel: $(e.currentTarget).data('segment')
        });
      });
      $("#email-signup").on('submit', function(e) {
        e.preventDefault();
        e.stopPropagation();
        var data = $(e.target).serialize();
        var emailVal = $(this).find('input\[type=email\]').val();
        //if error message is not hidden hide it
        if ( !$('#email-signup .error').hasClass('hidden') ) {
          $('#email-signup .error').addClass('hidden');
        }
        ga('send', {
          hitType: 'event',
          eventCategory: 'newsletter subscribe homepage footer',
          eventAction: 'newsletter subscribe',
          eventLabel: 'Email ' + emailVal
        });
        $.ajax({
          type: 'POST',
          url: '/SaveEmailSignUp',
          data: data,
          success: function(response) {
            if (response.Result === "Failure") {
              $('#email-signup .error').text(response.Message).removeClass('hidden')
            } else {
              $(e.target).addClass('hidden');
              $('.form-response').removeClass('hidden');
            }
          }.bind(this)
        });
      });
      
      function toggleWeChatEvents(useHover) {
        if (useHover.matches) {
          $('.we-chat, #qr-code').on('mouseenter', function (e) {
            $('#qr-code').toggleClass('show', true);
          });
          $('.we-chat, #qr-code').on('mouseleave', function (e) {
            $('#qr-code').toggleClass('show', false);
          });
        } else {
          $('body').on('click', function (e) {
            if ($(e.target).hasClass('we-chat-btn')) {
              var qrHasShow = $('#qr-code').hasClass('show');
              $('#qr-code').toggleClass('show', !qrHasShow);
            } else {
              $('#qr-code').removeClass('show');
            }
        
          });
          
          $('#qr-code-close').on('click', function (e) {
            $('#qr-code').removeClass('show');
          });
        }
      }

     handleMql("(min-width: 769px) and (hover: hover)", toggleWeChatEvents);

