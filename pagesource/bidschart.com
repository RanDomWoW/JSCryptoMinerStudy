    <script>

        $(document).ready(function () {
            $(".timeago").timeago();
        });

        $('.tooltipHoon').easyTicker({
            height: '42px',
            interval: 10000
        });

        $(document).on("click", ".head-symbol-change", function (e) {
            e.preventDefault();

            $('.head-symbol-change').removeClass('active');
            $(e.currentTarget).addClass('active');

            var group = $(this).attr("group");
            console.log('group', group);

            $('.rakaHoonAboveSelectDiv').hide();
            $('.rakaHoonAboveSelectDiv[group=' + group + ']').fadeIn();

            $('.rakaHoonAboveDiv').hide();
            $('.rakaHoonAboveDiv[group=' + group + ']').fadeIn();

            $('.head-symbol-change-detail').removeClass('active');
            $('.head-symbol-change-detail[group2=0]').addClass('active');

            $('.rakaHoonAboveDetailDiv').hide();
            $('.rakaHoonAboveDetailDiv[group2=0]').fadeIn();
        });

        $(document).on('click', '.head-symbol-change-detail', function () {

            var group2 = $(this).attr('group2');

            $('.head-symbol-change-detail').removeClass('active');
            $(this).addClass('active');

            $('.rakaHoonAboveDetailDiv').hide();
            $(".rakaHoonAboveDetailDiv[group2=" + group2 + "]").fadeIn();

        });

        $(document).ready(function () {
            $('#selectNewsSortByRead').on('click', function (e) {
                e.preventDefault();

                $(e.currentTarget).addClass('active');
                $('#selectNewsSortByComment').removeClass('active');

                $('#newsSortByRead').fadeIn();
                $('#newsSortByComment').hide();
            });

            $('#selectNewsSortByComment').on('click', function (e) {
                e.preventDefault();

                $(e.currentTarget).addClass('active');
                $('#selectNewsSortByRead').removeClass('active');

                $('#newsSortByComment').fadeIn();
                $('#newsSortByRead').hide();
            });
        });


        $(document).on('click', '.menuAll li', function () {
            $('.menuAll li').removeClass('active');
            $(this).addClass('active');
        });


        function showTabs(data) {
            removeTabs();
            $('#tabsTopTen_' + data).removeClass('hide');
        }

        function removeTabs() {
            $('.tabsTopTen').addClass('hide');
        }

        var stxx = new STXChart({
            container: $$$(".chartContainer-header"),
            layout: {
                "chartType": "mountain",
                "crosshair": true
            }
        });

        var listChart = [];
        $('.chartContainer-header').each(function () {

            var symbol = $(this).attr('symbol');

            $.get('/api/chart/getBySymbolMain/' + symbol, function (data) {

//                console.log('list:',data.list);

                var stxx = new STXChart({
                    container: $$$(".chartContainer-header[symbol='" + symbol + "']"),
                    allowZoom: false,
                    layout: {
                        "chartType": "mountain",
                        "crosshair": true
                    }
                });
                stxx.setCandleWidth(stxx.layout.candleWidth * 0.5);
                stxx.preferences.whitespace = 0;
                stxx.chart.yAxis.textStyle = "#AAA";

                stxx.newChart(symbol.replace('.', ''), data.list, stxx.chart, function () {
                    stxx.setStyle('background', 'color', 'black');
                    var inputs = {};
                    var outputs = {"Up Volume": "#486884", "Down Volume": "#255078"};
                    STX.Studies.quickAddStudy(stxx, "vol undr", inputs, outputs);
                });

                listChart.push(stxx);
//                console.log(listChart);

            });

        });
    </script>


