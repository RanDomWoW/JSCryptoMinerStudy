<script>

    var width = $(window).width();

    $(window).resize(function () {
        width = $(window).width();
        if (width <= 1300) {
            $('.sidebarRight_rev').css('right', '-205px');
        } else {
            $('.sidebarRight_rev').css('right', '0px');
        }
    });

    $(document).on('click', 'a.close-alert', function () {
        $(this).parent().parent().parent().hide();
    })


    $(document).ready(function () {
//            console.log('start', width);
        if (width <= 1300) {
            $('.sidebarRight_rev').css('right', '-205px');
        }

        $('.sidebarRight_rev').hover(function (e) {
//                console.log('in', width);
            $(e.currentTarget).css('right', '0px');
        }, function (e) {
//                console.log('out', width);
            if (width <= 1300) {
                $(e.currentTarget).css('right', '-205px');
            }
        });

        var showMsg = $('#showAlert').attr('showMsg');
        if (showMsg == 1) {//show
            $('body').css('padding-top', '30px');
//            $('#navbarCustomWithShowMsg').css('margin-top', '36px');
        } else {//hide
            $('body').css('padding-top', '0px');
//            $('#navbarCustomWithShowMsg').css('margin-top', '0px');
        }

        $('#closeShowMsgBtn').click(function (e) {
            e.preventDefault();
            $('#showMsgContentDiv').slideUp();
            $('body').css('padding-top', '0px');
//            $('#navbarCustomWithShowMsg').css('margin-top', '0px');
        })

        // remove By Au 2016-05-25
//        setTimeout(function () {
//            $('#showMsgContentDiv').slideUp();
//            $('body').css('padding-top', '0px');
//        }, 7000);
    });

    $(document).on('click', '#social-tab-table td', function (e) {
        e.preventDefault();
        var rel = $(e.currentTarget).attr('data-rel');
        $('#social-tab-table td').removeClass('active');
        $('.social-tab-content').hide();
        $(e.currentTarget).addClass('active');
        if (rel === 'facebook') {
            $('#facebookTab').show();
        } else if (rel === 'pantip') {
            $('#pantipTab').show();
        } else if (rel === 'chat') {
            $('#chatTab').show();
        } else if (rel === 'set') {
            $('#setTab').show();
        }
    });

    // start change theme
    $(document).on('click', '.select_theme li', function () {
        var select_theme = $(this).html();
        localStorage.setItem('df_theme', select_theme);

        $('#select_display_theme').html(select_theme);
//                theme_v2 & Light_theme
        if (select_theme == 'Dark') {
            $('body').removeClass('Light_theme');
            $('body').addClass('theme_v2');
        } else if (select_theme == 'Light') {
            $('body').removeClass('theme_v2');
            $('body').addClass('Light_theme');
        }

    });

    // check theme onLoad
    function checkTheme() {
        $('body').removeClass('Light_theme');
        $('body').addClass('theme_v2');

        // remove
//        var local_theme = localStorage.getItem('df_theme');
//        if(local_theme){
//           var select_theme = local_theme;
//        }else{
//            // Df
//            var select_theme = 'Dark';
//        }
//
//        $('#select_display_theme').html(select_theme);
//        if (select_theme == 'Dark') {
//            $('body').removeClass('Light_theme');
//            $('body').addClass('theme_v2');
//        } else if (select_theme == 'Light') {
//            $('body').removeClass('theme_v2');
//            $('body').addClass('Light_theme');
//        }
    }


    // right menu facebook , pantip (ON/OFF)
    //    localStorage.removeItem("check_menu_right");
    var check_menu_right = localStorage.getItem("check_menu_right");
    rightMenuAction(check_menu_right);

    $(document).on('click', '.bt-slide2', function () {
        var check_menu_right_now = $(this).css('right');

        if (check_menu_right_now == '0px') {
            rightMenuAction('ON');
            localStorage.setItem("check_menu_right", 'ON');
        } else if (check_menu_right_now == '226px') {
            rightMenuAction('OFF');
            localStorage.setItem("check_menu_right", 'OFF');
        }

    });


    function rightMenuAction(check_menu_right) {

        if (check_menu_right == 'ON') {
            $('.sidebarRight').removeClass('hide');
            $("#icon_right_menu").removeClass('fa-chevron-left');
            $("#icon_right_menu").addClass('fa-chevron-right');
            $('.bt-slide2').css('right', '226px');

        } else if (check_menu_right == 'OFF') {
            $('.sidebarRight').addClass('hide');
            $("#icon_right_menu").removeClass('fa-chevron-right');
            $("#icon_right_menu").addClass('fa-chevron-left');
            $('.bt-slide2').css('right', '0px');

        }

    }

    // end change theme


    //    Ps.initialize(document.getElementById('sidebarSymbol'));

</script>


