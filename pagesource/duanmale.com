        getGoods({counter_id:id})
    })

    var getGoods = function(params){
        var goods = $('#goods-tmpl')
        $.getJSON("/counter",{counter_id:params.counter_id},function(res){
            if(res.res_code===0){
                $('#tab-goods-ul').html("")
                goods.tmpl(res.data.goods).appendTo($('#tab-goods-ul'))
            }
        })
    }
    getGoods({counter_id:'Z'})
})
</script>
<script type="text/javascript">
//加入购物车
$(function() {

    var goLogin = function(){
        var e = dialog({
            content:"请登录",
            skin:"message-dialog",
        });
        e.show();
        setTimeout(function(){
            e.close();
            window.location.href="/auth/login/";
        },1000);
    };
     $("body").on("click",".J-fast-add-cart", function(e){
        e.stopPropagation();
        var that = $(this);
        var goods_size_id = $(that).attr("data-id");
        var quantity  = 1;
        if(!current_user_id){
            //$.showMessage("请先登录");
            goLogin();
            return
        }
        loading.open();
        $.post("/cart/goods/add?_rand="+Math.random(),{"goods_size_id":goods_size_id, "quantity":quantity}, function(res) {
            if(res.res_code == 0) {
                var img   = that.find('.J-pic').val()
                var title = that.find('.J-title').val()
                var size = that.find('.J-size').val()

                var data = {"img":img, "title":title, "size":size}
                var d2 = dialog({
                        title:false,
                        content:$("#cart-success-tpl").tmpl(data).html(),
                        padding:"14px"
                    })
                d2.showModal();
                $(".J_close_cart_sucuss").on("click",function(){
                    d2.close().remove();
                })
                $(".ui-popup-backdrop").on("click", function(){
                    d2.close().remove();
                })
            } else {
                $.showMessage(res.message, that[0]);
            }
        });
        loading.close();
     });
});
</script>
