		<script>
		$(function() {
			//$("img.lazy").lazyload();
		});

		$( document ).ready(function() {

			var offset              = 1;
			var loading             = false;
			var bigBrowser 		 = 0;


			$('#news_Modal').on('hidden.bs.modal', function (e) {
				$("#postsUnikal").html('');
				//alert(1);
				offset    = 1;
			});
			if ($(window).height() > 1000){
				bigBrowser =  1;
			}

			$("#big,#big1,#loadnew").click(function(){
				call();
			});


			$('.parent-body').bind('scroll', function()
			{

				if($(this).scrollTop() + $(this).innerHeight()>=$(this)[0].scrollHeight)
				{
					call();
				}
			});
			function call()
			{

				if(loading==false) //there's more data to load
				{
					loading = true;
					$('#ajaxloader').addClass("show1");


					$.post('/site/ajaxcategory',{'page': offset,'bigbrowser':bigBrowser}, function(data){
						if (data.length != 0)
						{

							$("#postsUnikal").append(data); //append received data into the element

						}
						$('#ajaxloader').removeClass("show1");

						offset++;
						loading = false;

					}).fail(function(xhr, ajaxOptions, thrownError) {

						$('#ajaxloader').removeClass("show1");

						loading = false;
						//offset++;
					});

				}

			}


		});


	</script>


