        <script>
                            function play() {
                                if (!proc) {
                                    if (player.paused) {
                                        proc = true;
                                        player.play();
                                        jQuery("#play-btn").removeClass('play').removeClass('stop').addClass('load');
                                        jQuery("#listen h1").html("CARREGANDO");
                                        jQuery('.link-site').attr('target', '_BLANK');
                                        //jQuery("#listen h1").html("São Paulo - 106.3 FM");
                                    } else {
                                        player.pause();
                                        jQuery("#play-btn").removeClass('load').removeClass('stop').addClass('play');
                                        jQuery("#listen h1").html("OUÇA A MIX São Paulo!");
                                        jQuery('.link-site').attr('target', '_SELF');
                                    }
                                }
                            }
                            function playingCallBack() {
                                if (is_android && is_chrome) {
                                    setTimeout(playingFix, 15000);
                                } else {
                                    playingFix();
                                }
                            }
                            function playingFix() {
                                playing = true;
                                proc = false;
                                jQuery("#play-btn").removeClass('play').removeClass('load').addClass('stop');
                                jQuery("#listen h1").html("São Paulo - 106.3 FM");
                                jQuery('.link-site').attr('target', '_BLANK');
                            }
                            if ("" === window.location.search || (window.location.pathname === '/' && window.location.href.indexOf('?') < 0)) {
                                var wf = true, ww = false, te = false, tp = true, dt = 'JUNTOS NO MELHOR MIX!', ld = false;
                                function nP() {
                                    if (wf && !ww) {
                                        ww = true;
                                        var aT = 10000;
                                        $.ajax({url: '../aovivo/m.php', dataType: 'json', success: function (d) {
                                                if (d.i == 'MIX' && d.t == dt) {
                                                    te.html(dt);
                                                } else {
                                                    te.html(d.i + ' - ' + d.t);
                                                }
                                                tp = false;
                                                if (false !== ld && (ld.i !== d.i || ld.t !== d.t)) {
                                                    aT = d.d * 1000 - 25000;
                                                }
                                                ld = d;
                                            }, error: function () {
                                                tp = true;
                                                te.html(dt);
                                                aT = 120000;
                                            }, complete: function () {
                                                ww = false;
                                                setTimeout(nP, aT);
                                            }});
                                    } else {
                                        ld = false;
                                        if (!tp) {
                                            tp = true;
                                            te.html(dt);
                                        }
                                        setTimeout(nP, 1000);
                                    }
                                }
                                $(window).ready(function () {
                                    te = $('div.buttons h2');
                                    $(window).focus(function () {
                                        wf = true;
                                    }).blur(function () {
                                        wf = false;
                                    });
                                    nP();
                                });
                            }
//script nestle
        </script>
