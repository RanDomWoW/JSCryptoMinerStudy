<script>
  
  $(function() {
    /**
     * Accordion
     */ 
    $('.accordion-content:not(:first)').slideUp('slow');
    
    $('.accordion-trigger').on('click focus', function(){
      var $this = $(this);
          audience = $this.attr('data-audience'),
          url = $this.attr('data-url'),
          $img = $("#instant_frame-img");
      // change image source
      $img.attr('src', url);
      // if link is set via previous click, remove it
      if($img.parent().is('a')) {
        $img.unwrap();
      }
      // set link on image if Roostery
      if(audience == 'interior_designers') {
        $img.wrap('<a href="https://www.roostery.com" target="_blank" rel="noopener" onclick="dataLayer.push({\'event\': \'welcome-bottom-actions\', \'eventCategory\':\'Welcome Page\', \'eventAction\':\'click\', \'eventLabel\': \'Audience - Roostery - linked image\');"></a>');
      }
      
      if(!$this.hasClass('active')) {
        // change up chevron to down and hide visible content
        $('.accordion-trigger').removeClass('active');
        $('.accordion-content').slideUp('slow');
      }
      
      // show content
      $this.addClass('active');
      $this.next().slideDown('slow');
      
    });
    
    /**
     * detect touch capability and add class to html element ala Modernizr, 
     * needed to style products to handle hover event
     */ 
    if ("ontouchstart" in document.documentElement) {
        document.documentElement.className += " touch";
    } else {
        document.documentElement.className += " no-touch";
        // while we're at it, go ahead and set the product attribution to hidden
        $('.attribution').css('visibility', 'hidden');
    }
    
    /**
     * detect flexbox capability and add class no-flexbox so we can use old way to center Welcome hero text
     * 
     */ 
     
    var documentStyles = document.documentElement.style;
    if (('boxDirection' in documentStyles) || ('WebkitBoxDirection' in documentStyles) || ('msBoxDirection' in documentStyles)){
      // has flexbox box-direction
    } else {
      document.documentElement.className += " no-flexbox";
    }

    
    /**
     * if .product receives focus then show .attribution
     */ 
    $('.product').focus(function() {
      $('.attribution').css('visibility', 'hidden');
      $(this).next().css('visibility', 'visible');
    });
     
    $('.attribution').on('mouseover focus', function() {
      $(this).prev().addClass('active');
    }).on('mouseout blur', function() {
      $(this).css('visibility', 'hidden');
      $(this).prev().removeClass('active');
    });
     
    /**
     * if .polaroid a receives focus then show .attribution
     */ 
    $('.polaroid_design a').on('mouseover focus', function() {
      $('.details').css('visibility', 'hidden');
      $('.polaroid_design_container').attr('style', '');
      $(this).parent().parent().css('background-color', '#efefef');
      $(this).parent().next().css('visibility', 'visible');
    }).on('mouseout blur', function() {
      $('.details').css('visibility', 'hidden');
