    function sousuo() {
        var param = "";
        if ($("#txtkey2").val() != "")
            param = "?strkey=" + $("#txtkey2").val();
        location.href = "/search.aspx" + param;
    }
    function robotsearch() {
        var yy = $("#ddlYY option:selected").val();
        var fz = $("#ddlFZ option:selected").val();
        var dzfw = $("#ddlDZFW option:selected").val();
        var kzg = $("#ddlKZG option:selected").val();
        var key = $("#txtSar").val();
        var param = "";
        if (yy != "") {
            param = "?yy=" + yy;
        }
        if (fz != "") {
            if (param != "")
                param += "&fz=" + fz;
            else
                param = "?fz=" + fz;
        }
        if (dzfw != "") {
            if (param != "")
                param += "&dzfw=" + dzfw;
            else
                param = "?dzfw=" + dzfw;
        }
        if (kzg != "") {
            if (param != "")
                param += "&kzg=" + kzg;
            else
                param = "?kzg=" + kzg;
        }
        if (key != "") {
            if (param != "")
                param += "&key=" + key;
            else
                param = "?key=" + key;
        }
        location.href = "/product/product2.aspx" + param;
    }
    document.onkeydown = function (e) {
        if (!e) e = window.event; //火狐中是 window.event
        if ((e.keyCode || e.which) == 13) {
            if ($("#hidkey").val() == "1")
                sousuo();
            if ($("#hidkey").val() == "3")
                asech();
            if ($("#hidkey").val() == "4")
                robotsearch()
        }
    }

</script>

