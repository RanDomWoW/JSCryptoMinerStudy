<script id="tpl-main-menu" type="text/x-handlebars-template">
	<div class="pl-mobile__wrap">
		<br>
		<p class="text-center">You are Viewing :</p>
		<hr class="pl-hr pl-spacer pl-spacer--out--small pl-spacer--cut-top">
		<p class="pl-h4 text-center primarysite_mobile"></p>
