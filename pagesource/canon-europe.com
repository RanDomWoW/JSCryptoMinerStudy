		<ul class="pl-mobile__menu">
			{{#each items}}
				<li class="pl-mobile__item{{#if hasSubmenu}} pl-mobile__item--has-submenu{{/if}} pl-color pl-color--gray-darkest{{#if isLast}} pl-mobile__item--inline{{/if}}">
					<a {{#if hasSubmenu}}href="#{{link}}"{{else}}href="{{link}}"{{/if}}{{#if target}} target="{{target}}"{{/if}} class="pl-mobile__link {{hasClass}}">{{label}}<span class="hidden headeren">{{headeren}}</span></a>
				</li>
			{{/each}}
		</ul>
	</div>
</script>
<script id="tpl-sub-menu" type="text/x-handlebars-template">
	<div class="pl-mobile__submenus">
		{{#each submenus}}
			<ul id="{{id}}" class="pl-mobile__submenu">
				{{#each items}}
					<li class="pl-mobile__item pl-color pl-color--gray-darkest">
						<a href="{{link}}"{{#if target}} target="{{target}}"{{/if}}{{#if popup}} data-popup-open="{{popup}}"{{/if}}{{#if url}} data-url="{{url}}"{{/if}} class="pl-mobile__link">{{label}}<span class="hidden headeren">{{headeren}}</span></a>
					</li>
				{{/each}}
			</ul>
		{{/each}}
	</div>
</script><!-- Mobile menu templates -->
