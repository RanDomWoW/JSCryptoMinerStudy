<script>
function resizePrograms()
{
	var h = $('#programs').height() ;

	var lineH = Math.floor(h * 0.8 / 14) ;
	
	$('.line_hour').height(lineH) ;
	$('.line_day_hour').height(lineH - 2) ;
	$('.line_day_hour').css('margin-bottom', 2) ;

	var half = Math.ceil((lineH - 2) / 2) ;
	var half2 = (lineH - 2) - half ;
	$('.line_day_hour_half').height(half) ;
	$('.line_day_hour_half').css('margin-bottom', 2) ;
	$('.line_day_hour_half_prev').height(half2) ;
	$('.line_day_hour_half_next').height(half2) ;
	$('.line_day_hour_half_next').css('margin-bottom', 2) ;
	
	$('.line_day_hour_1').height(lineH - 2) ;
	$('.line_day_hour_1').css('margin-bottom', 2) ;
	$('.line_day_hour_2').height(lineH) ;
	$('.line_day_hour_2').css('padding-top', half) ;
	$('.line_day_hour_2').css('padding-bottom', half2) ;
	$('.line_day_hour_2').css('margin-bottom', 2) ;

	$('.line_day_hour_nomargin').css('margin-bottom', 0) ;
	$('.line_day_hour_half_titleonly').css('line-height', half + 'px') ;
	$('.line_day_hour_1_titleonly').css('line-height', (lineH - 2) + 'px') ;

	$('.programs_today').width(Math.floor($('.programs_days_hours').width() - ($('.programs_days_hours').width() * .14 * 6 + 14))) ;
	
}
resizePrograms() ;
</script>
