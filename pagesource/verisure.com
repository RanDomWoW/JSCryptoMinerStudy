<script type="text/javascript">
jQuery(document).ready(function(){
   var country = jQuery('#banner-country .consumer-drop-country');
   var label = jQuery('#banner-country .consumer-drop-label');
   var selected = jQuery('#banner-country .consumer-drop-country a');
   country.hide();   
   country.mCustomScrollbar({
      theme:"3d-thick",
      autoHideScrollbar:true
   });
   function selectCountry(elem, target){
      elem.each(function(){
         jQuery(this).removeClass('selected');
      });
      target.addClass('selected');
      var text = target.text();
      label.html(text);
      country.hide();
      label.removeClass('active');
   }
   function goToCountry(targetUrl){
      targetUrl.each(function(){
         if (jQuery(this).hasClass('selected')) {
             var url = jQuery(this).attr('href');
             window.open(url, '_blank');
         }
      })
   }
   label.on('click', function(){      
      jQuery(this).toggleClass('active');
      country.slideToggle(200);  
   });
   
   jQuery(document).click(function(event){
      var trigger = jQuery('.consumer-drop');
      if (trigger !== event.target && !trigger.has(event.target).length){
         country.hide();
         label.removeClass('active');
      }
   });

 
   selected.click(function(event){
      event.preventDefault();
      console.log("hej");
 	   selectCountry(selected, jQuery(this));
   });
   jQuery('.quote-bar').on('click', '.round-button', function(){
      goToCountry(selected);
   });
});
