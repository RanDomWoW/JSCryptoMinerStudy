<script type="text/javascript">
$(document).ready(function(){
naviclose=null
vno=0
vrolling=null

	$(".b_head span img").click(function(){
		b_tab=$(".b_head span img").index($(this))
		$(".b_head span img").each(function(){
			$(this).attr("src",$(this).attr("src").replace("o.png",".png"))
		})
		$(this).attr("src",$(this).attr("src").replace(".png","o.png"))
		$(".con3 .c_box .b_list ul").hide()
		$(".con3 .c_box .b_list ul:eq("+b_tab+")").show()
		$(".con3 .c_box .b_head .fr").hide()
		$(".con3 .c_box .b_head .fr:eq("+b_tab+")").show()
	})

	$(".dep1 img").hover(function(){
		clearTimeout(naviclose)
		over_index=$(".dep1 img").index($(this))
		if (over_index==0){
			deq1over=18
		}else if(over_index==1){
			deq1over=123
		}else if(over_index==2){
			deq1over=216
		}else if(over_index==3){
			deq1over=607
		}else if(over_index==4){
			deq1over=702
		}else if(over_index==5){
			deq1over=794
		}
		$(".dep2").slideDown(300)
		$(this).attr("src",$(this).attr("src").replace(".png","o.png"))
		$(".dep1over").show().css("left",deq1over+"px")
	},function(){
		$(this).attr("src",$(this).attr("src").replace("o.png",".png"))
		$(".dep1over").hide()
		naviclose=setTimeout("$('.dep2').slideUp(300)",4000)
	})
	
	$(".dep2").hover(function(){
		clearTimeout(naviclose)
	},function(){
		naviclose=setTimeout("$('.dep2').slideUp(300)",4000)
	})

	$(".dep2 img").hover(function(){
		$(this).attr("src",$(this).attr("src").replace(".png","o.png"))
		$(this).parents("span").css("background","url(/img/bg_deq2_over.png) no-repeat "+(58-$(this).width())/2+"px top")
	},function(){
		$(this).attr("src",$(this).attr("src").replace("o.png",".png"))
		$(this).parents("span").css("background","none")
	})

	$(".con1 .c_box .control img").click(function(){
		clearTimeout(vrolling)
		vno=$(".con1 .c_box .control img").index($(this))
		$(".con1 .c_box .phtbox img").hide()
		$(".con1 .c_box .control img").each(function(){
			$(this).attr("src",$(this).attr("src").replace("o.png",".png"))
		})
		$(this).attr("src",$(this).attr("src").replace(".png","o.png"))
		$(".con1 .c_box .phtbox img:eq("+vno+")").fadeIn(500)
		vrolling=setTimeout("vroll()",10000)
	})

	vroll=function(){
		if (vno<$(".con1 .c_box .phtbox img").length-1){
			vno++
		}else{
			vno=0
		}
		$(".con1 .c_box .phtbox img").hide()
		$(".con1 .c_box .control img").each(function(){
			$(this).attr("src",$(this).attr("src").replace("o.png",".png"))
		})
		$(".con1 .c_box .control img:eq("+vno+")").attr("src",$(".con1 .c_box .control img:eq("+vno+")").attr("src").replace(".png","o.png"))
		$(".con1 .c_box .phtbox img:eq("+vno+")").fadeIn(500)
		vrolling=setTimeout("vroll()",10000)
	}
	
	
		setTimeout("vroll()",10000)
	
})
</script>
