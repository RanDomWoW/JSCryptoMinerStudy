			if (typeof $.fn.velocity == 'function') {
				// nur letztes thumb animiert entfernen - die anderen werden eh von den neuen ueberdeckt!
				if (direction == 'prev' && i_this < (settings.anz_thumnails-1) ) {
					img.remove();
				} else {
					img.velocity( options, function() { 
						$(this).remove(); 
					});
				}
				$( getThumbnailHtml.call( that, data.src,data.text,data.link,style) )
				.appendTo(box)
				.velocity(options,function() {
					endSlide.call( that, i_target );
				});
			}
		}

		var endSlide = function( i_target ) {
			unlock(); // auf verdacht unlocken wenn einer sich fertig bewegt hat
			this.i_current = i_target;
		}

		// private functions via var fn

		// thumbnailbox nehmen, in bildliste nach passendem thumb suchen und das reinplatzieren bzw -animieren
		var  updateThumbnail = function( box_thumbnail, i_target, direction ) {

			var that = this;

			if (i_target !== 0 && !i_target ) {
				return false;
			}

			var i_this = box_thumbnail.data('thumb-index');


			var i_img = i_target + i_this ; // + statt -
			if (i_img >= this.count_img ) {
				debug(this.count_img + ' / ' + i_target + ' / ' + i_this + ' / ' + i_img);

				i_img = i_img - this.count_img;

				debug(this.count_img + ' / ' + i_target + ' / ' + i_this + ' / ' + i_img);
			}


			var $img_thumb = $( elem ).find('[data-image-index=' + i_img + ']' );
			var src_thumb = $img_thumb.data( 'image-thumb' );
			if (src_thumb == undefined) { // wenn kein thumb als data angegeben src nehmen 
				var src_thumb = $img_thumb.attr( 'src' );
				// debug(src_thumb);
			} else {
				// debug(src_thumb);
				// return false; // master interessiert uns net, ist im big
			}

			var $item = $( elem ).find('[data-item-index=' + i_img + ']' );
			var $titel = $item.find( settings.selector_title );
			var $link = $item.find( settings.selector_link );

			var text = false;
			if ( $titel.length > 0 ) {
				text = $titel.html();
			}
			var link = false;
			if ( $link.length > 0 ) {
				link = $link.attr('href');
			}

			// wenn direction mitgegeben: animieren - sonst einfach anzeigen ...
			if ( direction) {

				var data_thumbnail = {
					src: src_thumb,
					text: text,
					link: link
				};
				doSlide.call(that, i_target, direction, box_thumbnail, data_thumbnail);
			} else {
				// wie oben angedroht: keine animation ...
				$( getThumbnailHtml.call( that, src_thumb, text, link) ).appendTo(box_thumbnail);
				// unlock(); // auf verdacht unlocken wenn einer sich fertig bewegt hat
			}
		} // ubdade thumbnail

		// html-block fuer's slidin' thumbnail
		var getThumbnailHtml = function( src_thumb, text, link, style ) {
			if (style && style.length > 0) {
				style = 'style="'+style+'"';
			} else {
				style = '';
			}
			if (text && text.length > 0) {
				text = '<div class="media-box__article">' + text + '</div>';
			}
			var link_0, link_1 = false;
			if (link && link.length > 0) {
				link_0 = '<a href="'+link+'">';
				link_1 = '</a>';
			}
			var html = '<figure class="media-box slider-box__item__img-holder" '+style+'>'
						+ link_0
						+ '<div class="media-box__el-holder media-box__el-holder--noOrder"><img class="media-box__el-holder__image" src="' + src_thumb + '" alt="" /></div>'
						+ text
						+ link_1
						+ '</figure>';
			return html;
		}

		// lock'n'unlock
		var lock = function() {
			debug('LOCK');
			isLocked = true;
		}

		var unlock = function() {
			debug('UNLOCK');
			isLocked = false;
		}

		// debugger
		var debug = function( bug ) {
			if ( settings.debug && typeof console == 'object' && typeof console.debug == 'function' ) {
				console.debug( bug );
			}
		}
	};

	// Jquery Wrapper For Init Handling
    $.fn.thumbSlider = function(options, callback) {

    	if (typeof options == 'function') {
    		callback = options;
    		options = {};
    	}

    	if (typeof callback == 'function') {
    		options.callback = callback;
    	}

		// pass options to plugin constructor
		var thumbSlider = new ThumbSlider(this, options);

		// prepare dom, bind listeners
		thumbSlider.init();

		// iterate through given dom nodes and append click handling
    	return this.each(function() {
    		var $elem = $(this);

    		// Return early if this element already has a plugin instance
			if ( $elem.data('thumbSlider') ) return;

			// Store plugin object in this element's data
			$elem.data('thumbSlider', thumbSlider);
		});

	}
})( jQuery );

