			if ( direction == 'next' ) {
				i_target = this.i_current + 1;
			} else {
				i_target = this.i_current - 1;
			}

			if (i_target > i_max) {
				i_target = 0;
			} else if (i_target < 0) {
				i_target = i_max;
			}


			debug( 'current ' + this.i_current );
			debug( 'target ' + i_target );
			debug( 'max ' + i_max ); 
			debug( 'direction (pre) ' + direction );

			// wenn anhand von aktuellem i, maximalem i und ziel-i checken obs nach vorne oder zurueck geht ...
			if ( !( this.i_current == 0 && i_target == i_max ) && (this.i_current < (i_target) || ( this.i_current == i_max && i_target == 0 ) ) ) {
				direction = 'next';
			} else {
				direction = 'prev';
			}
			debug( 'direction (post) ' + direction );

