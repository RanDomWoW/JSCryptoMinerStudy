<script type="text/javascript">

    var winVersion = "https://secure.logmein.com/hamachi.msi";
    var macVersion = "https://secure.logmein.com/LogMeInHamachi.zip";
    var linuxVersion = "/linux";

    function downloadWindowsRedirect() {

        window.location.href = winVersion;
    }

    function downloadWindows() {

        gaHamachi('create', 'UA-47855802-1', 'logmein.com');
        gaHamachi('send', 'pageview');
        setTimeout(downloadWindowsRedirect, 250);
    }

    function checkDownloadOs() {

        if (BrowserDetect.OS == "Windows") {
            downloadWindows();
        }
        else if (BrowserDetect.OS == "Mac") {
            window.location.href = macVersion;
        }
        else {
            window.location.href = linuxVersion;
        }
    }
</script>

