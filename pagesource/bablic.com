                    <!-- ko if:sku == 'credits' -->
                        <td>
                            Translation Credits <input class="hidden-text-box" type="number" step="1" min="1" max="100000" data-bind="value: $parent.changeCredits" />
                        </td>
                    <!-- /ko -->
                    <td data-bind="currency: totalNoDiscount,showSymbol:true"></td>
                </tr>
                <!-- ko if: discount || discountText -->
                <tr>
                    <td data-bind="text: discountText" style="color: #004d71;text-decoration: underline;"></td>
                    <td>(-<span data-bind="currency: discount,showSymbol:true"></span>)</td>
                </tr>
                <!-- /ko -->
                <!-- /ko -->
                <!-- ko if: tax () || taxDeducted() -->
                    <tr data-bind="css:{taxDeducted:taxDeducted}">
                        <td>
                            <span data-bind="text: 'VAT' + (taxDeducted() ? '' : ' (' + Math.round(taxRate()*100) + '%)')"></span>
                            <!-- ko if:countryCode() != 'IL' -->
                                <span class="enter_vatId" data-bind="visible:!showVat()">
                                    <a data-bind="click:function(){showVat(true)}">Enter VAT ID</a> to remove tax
                                </span>
                                <span data-bind="visible:showVat" class="vat_box">
                                    <input type="text" class="input_field vat_input" name="vatID" placeholder="VAT ID" data-bind="value: vatId"/>
                                    <i class="fa fa-chevron-right right_triangle"></i>
                                </span>
                            <!-- /ko -->
                        </td>
                        <td data-bind="currency: tax,showSymbol:true"></td>
                    </tr>
                <!-- /ko -->
                <!-- ko if: items().length > 1 || tax() || taxDeducted() || hasDiscount() -->
                <tr>
                    <td>Total</td>
                    <td data-bind="currency: totalWithTax,showSymbol:true"></td>
                </tr>
                <!-- /ko -->
                </tbody>
            </table>

            <div class="bottom_line checkout_seperator"></div>
        </div>
        <p class="error" data-bind="visible: cart.error, text: cart.error"></p>
        <div><p class="input_head center">choose country</p>
            <div class="choose_country_wrapper select_field_wrapper">
                <select class="select_field select_country_field"
                    data-bind="value:cart.countryCode,options: countries,optionsValue:'id',optionsText:'name'"></select>
                <i class="fa fa-angle-down select_arrow_down"></i>
            </div>
        </div>
        <label class="country_declare" data-bind="css:{active:cart.needSelfDeclare,error: cart.attemptSubmit() && !cart.selfDeclare()}"><input type="checkbox"
                                                                                                        data-bind="checked:cart.selfDeclare">I
            confirm that my/my company's address is in <span data-bind="text: countryName"></span></label>
        <div  data-bind="visible: !cart.items().length && (cart.tax () || cart.taxDeducted()) && cart.countryCode() != 'IL'" class="only_vat"><p class="input_head center">VAT ID</p>
            <div >
                <input class="input_field main_input" type="text" name="vatId" data-bind="value:cart.vatId">
            </div>
        </div>
        <div class="billing-details" data-bind="with:billingDetails">
            <div class="external_billing_gateway" data-bind="attr:{id:'externalBillingGateway'},visible:editBilling">

                <form name="YaadPay" action="https://icom.yaad.net/cgi-bin/yaadpay/yaadpay3new.pl" method="post"
                      style="display:none;"
                      data-bind="attr:{target:'externalGatewayFrame', id:'yaadForm'}">
                    <INPUT TYPE="hidden" id="termianlId" value="4500205422" NAME="Masof">
                    <INPUT TYPE="hidden" value="pay" NAME="action">
                    <INPUT TYPE="hidden" value="0.01" NAME="Amount">
                    <INPUT TYPE="hidden" value="Set" NAME="Order">
                    <INPUT TYPE="hidden" value="Billing Details" NAME="Info">
                    <INPUT TYPE="hidden" value="True" NAME="MoreData">
                    <INPUT TYPE="hidden" value="ENG" NAME="PageLang">
                    <INPUT TYPE="hidden" value="2" NAME="Coin">
                    <INPUT TYPE="hidden" value="000000000" NAME="UserId">
                    <INPUT TYPE="hidden" value="True" NAME="J5">
                    <INPUT TYPE="hidden" id="firstName" NAME="ClientName" data-bind="value: firstName">
                    <INPUT TYPE="hidden" id="lastName" NAME="ClientLName" data-bind="value: lastName">
                    <INPUT TYPE="hidden" value="True" NAME="UTF8">
                    <INPUT TYPE="hidden" value="True" NAME="UTF8out">
                    <INPUT TYPE="hidden" value="1" NAME="Tash">
                </form>
            </div>
            <a href='#' class="use_credit" data-bind="visible: editBilling() && last4(), click: $parent.cancelEditBillingDetails">Use saved credit card</a>
            <!-- ko if:Checkout.paypalShouldAppear -->
                <p data-bind="visible: editBilling" class="supported_cards" style="margin-bottom: 25px">Or <a data-bind="click: function() {$parent.approve('paypal');}" href="#" style="opacity:1"><span style="text-decoration: underline;">pay with</span>&nbsp;<img style="vertical-align: -8px; width: auto; height: auto;margin-left:0" src="https://www.paypalobjects.com/webstatic/en_US/i/buttons/pp-acceptance-small.png" alt="Buy now with PayPal" /></a></p>
            <!-- /ko -->
            <p data-bind="visible: editBilling" class="supported_cards">We accept Visa/Mastercard using our secure SSL PCI compliant checkout<img style="vertical-align: -8px;" src="https://s.bablic.com/images/5/paymentMethods.png"></p>

            <!-- ko if:!editBilling() && last4() -->
            <div class="existing_details">
                <p class="checkout_headline">Saved credit card</p>
                <p class="credit_card">
                    <span data-bind="text: '•••• •••• •••• ' + last4() + '&nbsp;&nbsp;&nbsp;&nbsp;' + expiration()"></span>
                    <a class="update_details" href="#" data-bind="click: $parent.editBillingDetails">change</a>
                </p>

            </div>
            <!-- ko if:Checkout.paypalShouldAppear -->
                <p class="supported_cards" style="margin-bottom: 10px">Or <a data-bind="click: function() {$parent.approve('paypal');}" href="#" style="opacity:1"><span style="text-decoration: underline;">pay with</span>&nbsp;<img style="vertical-align: -8px; width: auto; height: auto;margin-left:0" src="https://www.paypalobjects.com/webstatic/en_US/i/buttons/pp-acceptance-small.png" alt="Buy now with PayPal" /></a></p>
            <!-- /ko -->
            <div class="button_wrapper checkout_button_wrapper">
                <button type="button" class="bablic_button yaad_button" data-bind="click:$parent.approve" >DONE</button>
            </div>
            <!-- /ko -->
        </div>
    </div>
</div>


        
