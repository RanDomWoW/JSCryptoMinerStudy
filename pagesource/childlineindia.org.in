                var btn = document.getElementById("myBtn");
                if (typeof document.getElementsByClassName !== 'undefined') {
                    var span = document.getElementsByClassName("close")[0];
                    // When the user clicks on <span> (x), close the modal
                    span.onclick = function () {
                        modal.style.display = "none";
                    }
                }

                // When the user clicks anywhere outside of the modal, close it
                window.onclick = function (event) {
                    if (event.target == modal) {
                        modal.style.display = "none";
                    }
                }

                var nVer = navigator.appVersion;
                var nAgt = navigator.userAgent;
                var browserName = navigator.appName;
                var fullVersion = '' + parseFloat(navigator.appVersion);
                var majorVersion = parseInt(navigator.appVersion, 10);
                var nameOffset, verOffset, ix;

                // In Opera, the true version is after "brwedge" or after "Version"
                if ((verOffset = nAgt.indexOf("Edge")) != -1) {
                    var brwedge = document.getElementById('brwedge');
                    browserName = "Edge";
                    brwedge.style.display = "block";
                }
                    // In Opera, the true version is after "Opera" or after "Version"
                else if ((verOffset = nAgt.indexOf("Opera")) != -1) {
                    browserName = "Opera";
                }
                else if ((verOffset = nAgt.indexOf("OPR")) != -1) {
                    browserName = "Opera";
                    var brwopera = document.getElementById('brwopera');
                    brwopera.style.display = "block";
                }
                    // In MSIE, the true version is after "MSIE" in userAgent
                else if ((verOffset = nAgt.indexOf("MSIE")) != -1) {
                    browserName = "Microsoft Internet Explorer";
                    
