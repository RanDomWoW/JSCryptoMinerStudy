          var proceed = false;
        }
        if (company == '') {
          jQuery('#company').after('<p class="error-msg">Enter Company.</p>');
          var proceed = false;
        }
        if (proceed == false) {
          alert("Please enter all required fields.");
          return false;
        }
      });

    </script></br>
