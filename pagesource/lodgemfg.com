                                property:'min-height'
                            });
                            
                            
                            var notfound = 0;
                            var countchild = 0;
                            while (notfound < 500) {
                                
                                
                                if ($('.match-child'+countchild).length > 0) {
                                    
                                    $('.match-child'+countchild).matchHeight({
                                        target:$('.target-child'+countchild),
                                        property:'min-height'
                                    });
                                    
                                }else{
                                    notfound++;  
                                }
                                countchild++;
                            }
                            
                            /*
                            $('.match-child1').matchHeight({
                            target:$('.target-child1'),
                            property:'min-height'
                            });
                            $('.match-child2').matchHeight({
                            target:$('.target-child2'),
                            property:'min-height'
                            });
                            $('.match-child3').matchHeight({
                            target:$('.target-child3'),
                            property:'min-height'
                            });
                            $('.match-child4').matchHeight({
                            target:$('.target-child4'),
                            property:'min-height'
                            });
                            $('.match-child5').matchHeight({
                            target:$('.target-child5'),
                            property:'min-height'
                            });
                            */
                            
                            
                            $('.match-1').matchHeight({
                                target:$('.target-1')
                            });
                            $('.same-sib').matchHeight();
                            $('.same-sib2').matchHeight();
                            
                            $.fn.matchHeight._throttle = 80;
                        });
                        
                        
                        document.addEventListener("DOMContentLoaded", function(event) {
                            var s = skrollr.init({ 
                                smoothScrolling: true, 
                                forceHeight: false, 
                                mobileCheck: function(){return false} 
                            });
                            skrollr.get().refresh();
                        });
                        
                        
                        $('[data-scroll-to^="#"]').on('click', function(event) {
                            var target = $(this.getAttribute('data-scroll-to'));
                            if( target.length ) {
                                event.preventDefault();
                                $('html, body').stop().animate({
                                    scrollTop: target.offset().top
                                }, 1000);
                            }
                        });
                        
                        function openSearchDrawer(){
                        	$('.search-drawer').slideDown('fast');
                            $( ".search-drawer input" ).focus();
                        };
                        function closeSearchDrawer(){
                        	$('.search-drawer').slideUp('fast');
                        };
                        $('.nav-search a').click(function(event){
                            event.preventDefault();
                        	openSearchDrawer();
                        });
                        $('.search-drawer .icon-exit').click(function(event){
                             event.preventDefault();
                        	closeSearchDrawer();
                        });
                        $(window).resize(function(){
                        	closeSearchDrawer();
                        });
                    </script>
                    
                </div>
            </div>
        </div>
    
</body>
</html>