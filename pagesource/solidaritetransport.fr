    <script>
        function changeActu(next)
        {
            $(".loading").show();
            if (next) {
                var number = 1;
            } else {
                var number = -1;
            }
            var url = '/home/post_PLACEHOLDER';
            url = url.replace("PLACEHOLDER", number);
            var DATA = 'number=' + number;
            $.ajax({
                type: "POST",
                url: url,
                data: DATA,
                cache: false,
                success: function(data){
                   $('#actu').html(data);
                   $(".loading").hide();
