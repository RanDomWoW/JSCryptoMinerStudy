            <script type="text/javascript">
                var isWelcomeModalActive = 0;

                function showWelcomeModal() {
                    if(!getCookie('shownwelcomemodal')) {
                        var sWelcomeModalOverlay = document.getElementById('modal-welcome-overlay');
                        var sWelcomeModalContainer = document.getElementById('modal-welcome-container');

                        detectBrowserName();

                        document.body.toggleClassName('modal--open');

						GAEventTracking('TmWelcomeScreen', 'Welcome modal', 'modal opening');

                        if (browserName == "Safari") {
                            sWelcomeModalOverlay.toggleClassName('modal-safari');
                            sWelcomeModalContainer.toggleClassName('modal-safari');
                        } else {
                            sWelcomeModalOverlay.toggleClassName('modal-show');
                            sWelcomeModalContainer.toggleClassName('modal-show');
                        }
                        isWelcomeModalActive = 1;

                        setCookie("shownwelcomemodal",true,365);
                    }
                }


                function hideWelcomeModal(sClickedArea) {
                    var sWelcomeModalOverlay = document.getElementById('modal-welcome-overlay');
                    var sWelcomeModalContainer = document.getElementById('modal-welcome-container');

                    detectBrowserName();

                    document.body.toggleClassName('modal--open');

                    if(typeof sClickedArea != 'undefined')
                        GAEventTracking('TmWelcomeScreen', 'Welcome modal', 'Clicked area - "'+sClickedArea+'"');

                    if (browserName == "Safari") {
                        sWelcomeModalOverlay.toggleClassName('modal-safari');
                        sWelcomeModalContainer.toggleClassName('modal-safari');
                    } else {
                        sWelcomeModalOverlay.toggleClassName('modal-show');
                        sWelcomeModalContainer.toggleClassName('modal-show');
                    }

                    isWelcomeModalActive = 0;

                }

