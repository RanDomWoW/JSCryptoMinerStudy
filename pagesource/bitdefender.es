                $(this).toggleClass('active');
                $('.translate-banner .left-one').addClass('active');
                $('.translate-banner .main-text').addClass('gone-right');
                $('.translate-banner .right-one').addClass('inactive');
            });
            $('.backNL').on('click', function () {
                $(this).toggleClass('active');
                $('.translate-banner .left-one').removeClass('active');
                $('.translate-banner .main-text').removeClass('gone-right');
                $('.translate-banner .right-one').removeClass('inactive');
            });
            $('.translate-banner .dropdown').on('click', function () {
                $(this).next().slideToggle();
            });
            $('.backN').on('click', function () {
                $(this).toggleClass('active');
                $('.translate-banner .right-one').removeClass('active');
                $('.translate-banner .main-text').removeClass('gone-left');
                $('.translate-banner .left-one').removeClass('inactive');
