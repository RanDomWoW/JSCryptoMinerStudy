<script>
/////全ページ用スクリプト/////
$(function(){
	//スマホメニューボタン用
	$(".open-tgl,#black-box,.global-nav ul a").click(function(){
		$('#header-sp .global-nav').slideToggle("fast");
		$("#black-box").fadeToggle("linear");
	});

	//読み込み時に画面内に入っていたら、要素の順番にアニメーション表示
	Position = $(window).height() + $(window).scrollTop();
	$('.scroll').each(function(i) {
	if(Position > $(this).offset().top) {
		$(this).delay(30*i).queue(function() {
			$(this).addClass("on");
		});
	}
	});

	//ホバートグル(ヘッダー用)
    $(".dropdown-toggle").hover(function(){
        $(this).children(".dropdown-contents").slideToggle("fast");
    });

	//高さを揃える
	$('.height-box').matchHeight();
});

//スクロールして、画面内にきたらclass付与
$(window).scroll(function(){
	scrollPosition = $(window).height() + $(window).scrollTop();
	$('.scroll').each(function() {
	if(scrollPosition - 150 > $(this).offset().top) {
		$(this).addClass("on");
	}
	});
});
</script>

