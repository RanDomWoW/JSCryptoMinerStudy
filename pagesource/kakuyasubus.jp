	// 選択された出発地に対する
	// 有効な目的地都道府県リストを更新する
	function updateArrPrefs(dep_pref_id, callback){
		// arrPrefs = somedata;
		if(callback){
			callback();
		}
	}

	// 出発地のセレクトボックスから、有効な出発地都道府県リストを作る
	var depPrefIDs = [];
	$depPref.find('option').each(function(){
		var pref_id = $(this).attr('value');
		depPrefIDs.push(pref_id);
	});

	// エリア選択要素を出発地用に更新する
	function updateDepMap(){
		$areaButtonLi.addClass('disable');
		for(var key in depPrefIDs){
			var pref_id = depPrefIDs[key];

			$areaButton.find('img[data-pref-id="'+pref_id+'"]').closest('li').removeClass('disable');
		}
	}
	$depPref.val(0);
	updateArrSelect(0);
	updateDepMap();	// 出発地として有効な都道府県を表示
	changeState();

	// 出発地が選択された
	$depPref.change(function(){
		var pref_id = parseInt($(this).val());

		if(pref_id){
			// 都道府県が選択された

			$depPref.attr('disabled', 'disabled');	// 出発地をロック
			$mapOverlay.stop().fadeTo(100, 0.7);			// オーバーレイON

			// 出発地に対する目的地都道府県リストを更新する
			updateArrPrefs(pref_id, function(){
				$depPref.removeAttr('disabled');	// 出発地のロック解除
				$mapOverlay.stop().fadeOut(100); // オーバーレイOFF

				// 地図およびセレクトボックスの更新
				updateArrSelect(pref_id);
				updateArrMap(pref_id);
				changeState();
			});

		}else{
			// 未選択状態

			updateArrSelect(0);
			updateDepMap();
			changeState();
		}

	});
	// 目的地が選択された
	$arrPref.change(function(){
		changeState();
	});

	// エリアボタンがクリックされた
	$areaButton.click(function(){
		var dep_val = parseInt($depPref.val());
		var arr_val = parseInt($arrPref.val());

		var pref_id = $(this).find('img').data('pref-id');

		if($(this).closest('li').hasClass('disable')){
			// 無効な都道府県
			return false;
		}

		if(dep_val){
			// 出発地が選択されている
			$arrPref.val(pref_id);	// 目的地を選択
			$arrPref.change();
		}else{
			// 出発地が未選択
			$depPref.val(pref_id);	// 出発地を選択
			$depPref.change();
		}

		return false;
	});

	// リセットボタンが押された
	$resetBtn.click(function(){

		// 出発地・目的地をクリア
		$depPref.val(0);
		$arrPref.val(0);

		updateDepMap();
		updateArrSelect(0);
		changeState();

		return false;
	});

	function changeState(anim){
		// console.log('changeState');
		if(typeof anim === 'undefined'){
			anim = true;
		}

		var trans_duration = 0;
		var pin_duration = 0;
		var title_duration = 0;
		var pin_move_duration = 0;
		if(anim){
			trans_duration = transDuration;
			pin_duration = pinShowDuration;
			title_duration = titleShowDuration;
			pin_move_duration = pinMoveDuration;
		}

		var dep_val = parseInt($depPref.val());
		var arr_val = parseInt($arrPref.val());

		// シーンの切り替え //////////////////////////////
		if(dep_val && arr_val){
			// 出発地と目的地の両方が選択されている

			// ラベルの設定
			var dep_src = '/img/destination'+dep_val+'.png';
			var arr_src = '/img/destination'+arr_val+'.png';
			$depLabel.attr('src', dep_src);
			$arrLabel.attr('src', arr_src);

			// 日付選択シーンへの切り替え
			$sceneArea.fadeOut(trans_duration);
			$sceneDate.fadeIn(trans_duration);

			// $sceneArea.hide();
			// $sceneDate.show();
		}else{
			// 出発地、目的地のいずれか、もしくは両方が選択されていない

			// エリア選択シーンへの切り替え
			$sceneDate.fadeOut(trans_duration);
			$sceneArea.fadeIn(trans_duration);

			// $sceneDate.hide();
			// $sceneArea.show();
		}

		// ピンの設定 ///////////////////////////////////
		if(dep_val){
			// 出発地が選択されている
			var cls = 'point'+dep_val;

			// ピンの移動と表示
			$depPoint.attr('style', ''); // 同一エリアを選択したときにピンの位置をずらしている場合があるので位置をクリア

			$depPoint.attr('class', '');
			$depPoint.addClass('dep');
			$depPoint.addClass(cls);
			$depPoint.fadeIn(pin_duration);
		}else{
			$depPoint.fadeOut(pin_duration);
		}
		if(arr_val){
			// 目的地が選択されている

			var cls = 'point'+arr_val;

			// ピンの移動と表示
			$arrPoint.attr('style', ''); // 同一エリアを選択したときにピンの位置をずらしている場合があるので位置をクリア

			$arrPoint.attr('class', '');
			$arrPoint.addClass('arr');
			$arrPoint.addClass(cls);
			$arrPoint.fadeIn(pin_duration);
		}else{
			$arrPoint.fadeOut(pin_duration);
		}

		if(arr_val!=0 && arr_val==dep_val){
			// 出発地と目的地が同一
			var left = $depPoint.position().left;
			var dep_left = left - samePinDiff;
			var arr_left = left + samePinDiff;

			// $depPoint.css({left: dep_left+'px'});
			// $arrPoint.css({left: arr_left+'px'});
			$depPoint.animate({left: dep_left+'px'}, pin_move_duration);
			$arrPoint.animate({left: arr_left+'px'}, pin_move_duration);
		}

		// 吹き出しの設定
		$('.flow').hide();
		$titleLabel.hide();
		$searchRow.removeClass('on');
		if(dep_val && arr_val){
			$flow3.show();
			$searchRow3.addClass('on');
		}else if(dep_val){
			$flow2.show();
			$arrTitleLabel.fadeIn(title_duration);
			$searchRow2.addClass('on');
		}else{
			$flow1.show();
			$depTitleLabel.fadeIn(title_duration);
			$searchRow1.addClass('on');
		}

	}
	changeState(false);

	// 出発地に対する有効な都道府県を取得し
	// セレクトボックスを更新する
	var loading_arr = false;
	function updateArrSelect(dep_pref_id){
		var error = false;

		if(!dep_pref_id) error = true;
		if(!arrPrefs[dep_pref_id]) error = true;

		var prefs = arrPrefs[dep_pref_id];

		if(error){
			prefs = null;
		}

		createSelectbox($arrPref, prefs, 0, '都道府県を選択', 1);

	}

	// 目的地用の地図を更新する
	function updateArrMap(dep_pref_id){
		var error = false;

		if(!dep_pref_id) error = true;
		if(!arrPrefs[dep_pref_id]) error = true;

		var prefs = arrPrefs[dep_pref_id];
		$areaButtonLi.addClass('disable');
		if(!error){
			// console.log(prefs);
			var caption = 1;
			for(var key in prefs){
				var pref_id = 0;
				if(caption){
					for(var id in prefs[key]){
						pref_id = id;
						$areaButton.find('img[data-pref-id="'+pref_id+'"]').closest('li').removeClass('disable');
					}

				}else{
					pref_id = key;
					$areaButton.find('img[data-pref-id="'+pref_id+'"]').closest('li').removeClass('disable');
				}
			}

		}
	}

	// キャンペーンスライドショー

	// スライドの高さを計算する
	$slideContainer = $('.campaignArea ul');
	var slide_heigth = 0;
	$slideContainer.find('li').each(function(){
		slide_heigth = Math.max(slide_heigth, $(this).height());
	});
	$('.campaignArea ul').carouFredSel({
		auto: 3000,
		width: 915,
		height: slide_heigth,
		scroll: 1,
		prev: '.campaign_inner .btn_right',
		next: '.campaign_inner .btn_left'
	});

	// 主要路線から探すのリンク領域を拡大
	$('.mainroute li').boxTarget();

	// datepicker
	var $dpMon = $('#ServiceMonth');
	var $dpDay = $('#ServiceDay');
	var $dpField = $('#datepicker-field');
	var dpYear = 2018;
	var currYear = 2018;
	var currMon = 3;
	var currDay = 21;
	$dpField.datepicker({
		showOn: 'button',
		buttonImage: "/img/icon_calendar.png",
		buttonImageOnly: true,
		dateFormat: "yy-mm-dd",
		numberOfMonths: 2,
		minDate: new Date(currYear, currMon-1, currDay),
		maxDate: new Date(2018, 4, 31)
	}).change(function(){
		// 選択された日付をセレクトボックスに反映
		var date = $(this).val();
		var arr = date.split('-');

		dpYear = arr[0];
		$dpMon.val(parseInt(arr[1]));
		$dpDay.val(parseInt(arr[2]));
	});

	$dpMon.change(function(){
		updateDpField();
	});
	$dpDay.change(function(){
		updateDpField();
	});
	function updateDpField(){
		var mon = $dpMon.val();
		var day = $dpDay.val();
		if(mon==0 || day==0) return;
		$dpField.val(dpYear+'-'+mon+'-'+day);
	}


});
</script>


