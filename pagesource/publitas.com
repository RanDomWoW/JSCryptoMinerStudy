      (function(){
        var banner = document.getElementById('banner');
        var header = document.getElementById('header');
        var altHeader = true;

        function setAltHeader() {
          if (!altHeader) {
            header.classList.add('alt');
            altHeader = true
          }
        }
        function removeAltHeader() {
          if (altHeader) {
            header.classList.remove('alt');
            altHeader = false;
          }
        }
        function updateHeader() {
          if (!banner) {
            removeAltHeader();
            return;
          }
          var bannerHeight = height(banner);
          var headerHeight = height(header);
          var pastBanner = window.scrollY >= (bannerHeight - headerHeight);
          if (pastBanner) {
            removeAltHeader();
          } else {
            setAltHeader();
          }
        }
        function height(el) {
          var rect = el.getBoundingClientRect();
          return rect.bottom - rect.top;
        }
        document.addEventListener('scroll', function() {
          requestAnimationFrame(updateHeader);
        })
        updateHeader();
      }());
    </script> <script src="javascripts/all-0e621091.js" async=true></script> <script charset=ISO-8859-1 src="//fast.wistia.com/assets/external/popover-v1.js" async></script> <script type="application/ld+json">
