<script>

	$(document).ready(function()
	{

		$('#q').keydown(function(){
			$('#q').tooltip('destroy');
		});

		$('#ln').keydown(function(){
			$('#ln').tooltip('destroy');
		});

		$('#r').keydown(function(){
			$('#r').tooltip('destroy');
		});

		if ($('#placeholder-js').length == 1) {
			$('input[placeholder]').placeholder();
		}

		$('#l').typeahead({
			source: function (query, process) {
				return $.get('/suggest/location/', { query: query }, function (data) {
					return process(data);
				});
			},
			matcher: function (item) {
				return true;
			},
			sorter: typeaheadLocationsSorter,
			highlighter: typeaheadLocationsHighlighter,
			updater: typeaheadLocationsUpdater,
			minLength: 2
		});

		$('#cz').typeahead({
			source: function (query, process) {
				return $.get('/suggest/location/', { query: query }, function (data) {
					return process(data);
				});
			},
			matcher: function (item) {
				return true;
			},
			sorter: typeaheadLocationsSorter,
			highlighter: typeaheadLocationsHighlighter,
			updater: typeaheadLocationsUpdater,
			minLength: 2
		});

		var hideCurrentLocationTimeout;
		$('#l').blur(function() {
			hideCurrentLocationTimeout = setTimeout(function(){ $('#current-location').hide(); }, 1000);
		});
		$('#l').focus(function() {
			if (hideCurrentLocationTimeout) {
				clearTimeout(hideCurrentLocationTimeout);
			}
			if (($(this).val() == '') && supportedGeoLocation()) {
				$('#current-location').show();
			} else {
				$('#current-location').hide();
			}
		});
		$('#l').keyup(function() {
			if (($(this).val() == '') && supportedGeoLocation()) {
				$('#current-location').show();
			} else {
				$('#current-location').hide();
			}
		});

		$('#current-location').click(function() {
			$('#current-location').hide();
		});

		focusOnInput('q');	initClearButtons();
	
	});

</script>

