	function carregaCarrosselShopping() {
	    $.ajax({
	        type: "GET",
	        url: "https://www.moto.com.br/img/Lay2011/xml/ofertas-home-motocom-buscape.xml",
	        dataType: "xml",
	        success: function(xml) {
	            /* descobrindo o tamanho do xml */
	            var tamanhoXml = 0;
	            $(xml).find('produto').each(function(index, value) {
	                tamanhoXml++;
	            });
	            /* gerando numeros aleatorios com base no tamanho do xml*/
	            var random1 = Math.floor(Math.random() * tamanhoXml);
	            var random2 = Math.floor(Math.random() * tamanhoXml);
	            var random3 = Math.floor(Math.random() * tamanhoXml);
	            var random4 = Math.floor(Math.random() * tamanhoXml);
	            var random5 = Math.floor(Math.random() * tamanhoXml);

	            var i = 1;
	            $(xml).find('produto').each(function(index, value) {
	                /* filtrando os itens escolhidos aleatoriamente */
	                if (index == random1 ||
	                    index == random2 ||
	                    index == random3 ||
	                    index == random4 ||
	                    index == random5) {
	                    /* adicionando os itens do shopping na home do moto */
	                    var sProduto = $(this).find('id_produto').text();
	                    var sLproduto = $(this).find('link_produto').text();
	                    var sTitulo = $(this).find('titulo').text();
	                    var imagem = $(this).find('imagem').text().replace("http://", "https://");
	                    var preco = $(this).find('preco').text();
	                    var preco_desc = $(this).find('parcel').text();

	                    $('<ul id="seg">').html(
	                            '<div id="seg_' + i + '_conteudo"><h1><a href="' + sLproduto + '"target="_blank">' +
	                            '<img src="' +
	                            imagem +
	                            '" alt="" title="" style="margin: 0 0 0 0px !important"  width="147" height="146"  /></a>' +
	                            '<h2><p style="font: 12px Arial; color:#304b83; text-align:center;">' +
	                            sTitulo +
	                            ' </p><h3><p style="font:14px Arial, Helvetica, sans-serif; color:#666; margin-top:-1px;">' +
	                            'R$ ' + preco +
	                            ' <p style="font:10px Arial, Helvetica, sans-serif; color:#666;margin-top:-1px; "> ' +
	                            preco_desc +
	                            '</p></div></ul></h3>')
	                        .appendTo(
	                            "#shoppingmoto");
	                    i++;
	                    if (index == 4) {
	                        return false;
	                    }
	                }
	            });
	        },
	        error: function() {
	            jQuery.error = console.error;
	        }
	    });
	}
	var slides = {
		0 : {
			image : 'https://www.moto.com.br/img/2018/03/12/img127099-1520889191-v653x354.jpg',
			title : 'Honda CB 500X supera a rotina da cidade e viagens mais longas',
