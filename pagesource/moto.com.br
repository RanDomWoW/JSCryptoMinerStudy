	function mudaModelos() {
		var idMar = document.getElementById('motoMarca')[document.getElementById('motoMarca').selectedIndex].value.split('_ID_')[1]
		preencheDadosModelos(idMar, -1);
	}
	function preencheDadosModelos(idMar, idSelected){
		$('#motoModelo').find('option').remove();
		$('#motoModelo').append($('<option></option>').val('').html('Modelo: Todos'));
		$.getJSON('/comprar/json/modelosJson.json?idMarca=' + idMar, function(data) {
			$.each(data, function(i, modelo) {
				if(idSelected == modelo.id){
					$('#motoModelo').append($('<option></option>').val(modelo.modelo + '_ID_' + modelo.id).html(modelo.modelo).attr('selected','selected'));
				}else{
					$('#motoModelo').append($('<option></option>').val(modelo.modelo + '_ID_' + modelo.id).html(modelo.modelo));
				}
			});
		});
	}
	
	function mudaCidades() {
		var idEst = document.getElementById('estado')[document.getElementById('estado').selectedIndex].value.split('_ID_')[1]
		$('#cidade').find('option').remove();
		//$('#cidade').append($('<option></option>').val(cidade[1]+'_ID_'+cidade[0]).html(cidade[1]));

		var cidades = document.getElementById("cidades");
		if (cidades != null) {
			cidades.parentNode.removeChild(cidades);
		}

		var capitais = document.getElementById("capital");
		if (capitais != null) {
			capitais.parentNode.removeChild(capitais);
		}
		preencheDadosCidade(idEst, -1);
	}
	
	function preencheDadosCidade(idEst, idSelected){
		
		var capitalEstado = document.createElement("optgroup");
		capitalEstado.label = "Capital";
		capitalEstado.id = "capital";
		var cidadesEstado = document.createElement("optgroup");
		cidadesEstado.label = "Todas as Cidades";
		cidadesEstado.id = "cidades";

		$('#cidade').append($('<option></option>').val('').html('Cidade: Todas'));
		$.getJSON('/comprar/json/cidadesJson.json?idEstado=' + idEst, 
		function(data) {
			$.each(data, function(i, cidade) {

				var opt = document.createElement("option");
				opt.value = cidade[1] + '_ID_' + cidade[0];
				var tmp = document.createTextNode(cidade[1]);
				opt.appendChild(tmp);

				//cloneNode - appendChild não pode estar em dois lugares do documento ao mesmo tempo 
				var op = document.createElement("option");
				op.value = cidade[1] + '_ID_' + cidade[0];
				var clone = tmp.cloneNode(true);
				op.appendChild(clone);
				
				if(idSelected == cidade[0]){
					op.setAttribute('selected', 'selected');
				} 
				
				cidadesEstado.appendChild(op);

				if (cidade[2] == true) {
					document.getElementById("cidade").appendChild(capitalEstado);
					capitalEstado.appendChild(opt);
					document.getElementById("cidade").appendChild(cidadesEstado);
				}
			});
		});

	}
</script>

