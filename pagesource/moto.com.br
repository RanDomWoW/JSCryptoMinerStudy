		2 : {
			image : 'https://www.moto.com.br/img/2018/03/01/img126828-1519958700-v653x354.jpg',
			title : 'Nova Harley-Davidson Fat Bob é imponente e tem força de sobra',
			alt : 'Nova Harley-Davidson Fat Bob é imponente e tem força de sobra',
			url : '/testes/conteudo/nova-harleydavidson-fat-bob-e-imponente-e-tem-forca-de-sobra-126804.html'
		},
		3 : {
			image : 'https://www.moto.com.br/img/2018/03/08/img126954-1520554414-v653x354.jpg',
			title : 'Especial: Tudo sobre a nova Yamaha Ténéré 700',
			alt : 'Especial: Tudo sobre a nova Yamaha Ténéré 700',
			url : '/acontece/conteudo/especial-tudo-sobre-a-nova-yamaha-tenere-700-126953.html'
		},
		4 : {
			image : 'https://www.moto.com.br/img//shopping-natal.jpg',
			title : '',
			alt : '',
			url : 'https://shopping.moto.com.br/'
		}
	};
	$(function() {
		for ( var slideNum in slides) {
			$('#slider')
					.append(
							'<div><a href="'+slides[slideNum].url+'"><img src="'+slides[slideNum].image+'" alt="'+slides[slideNum].alt+'" title="'+slides[slideNum].alt+'" width="654" height="354" /></a></div>');
			$('#slider img').hide();//hide all the images on the page
			numSlides++;
		}
		var i = 0;//initialize
		var int = 0;
		function doThis() {
			var imgs = $('#slider img').length;//count the number of images on the page
			if (i >= imgs) {// Loop the images
				clearInterval(int);//When it reaches the last image the loop ends
			}
			$('#slider img:hidden').eq(0).fadeIn(500);//fades in the hidden images one by one
			i++;//add 1 to the count
		}
		$('#paginacao-slider li').click(function(ev) {
			var slideID = $("#paginacao-slider li").index(this);
			switchToSlide(slideID);
		});
		switchToSlide(0);
	});
	var currentSlide = 0;
	var numSlides = 0;
	function switchToSlide(selectedSlide) {
		for ( var slideNum in slides) {
			var childNum = parseInt(slideNum) + 2;

			if (slideNum == selectedSlide) {
				$('#slider h1').html(slides[slideNum].title);
				$('#slider #figcaption').attr('href', slides[slideNum].url);
				$('#slider div:nth-child(' + childNum + ')').fadeIn('slow')
						.addClass('active');
				$('#slider div:nth-child(' + childNum + ') img').fadeIn()
						.addClass('active');
			} else {
				$('#slider div:nth-child(' + childNum + ')').fadeOut('slow')
						.removeClass('active');
			}
		}

		$('#paginacao-slider p').html((currentSlide + 1) + " / " + numSlides);

		if (isPlaying) {
			startSlideTimer();
		}
	}
	var slideTimer = null;
	var slideDelay = 4000;
	var isPlaying = true;
	function playPause(self) {
		if (isPlaying) {
			clearTimeout(slideTimer);
			isPlaying = false;
			$(self).find('span').removeClass('bt-stop').addClass('bt-play');
		} else {
			isPlaying = true;
			startSlideTimer();
			$(self).find('span').removeClass('bt-play').addClass('bt-stop');
		}
	}
	function startSlideTimer() {
		if (slideTimer != null) {
			clearTimeout(slideTimer);
		}
		slideTimer = setTimeout(nextSlide, slideDelay);
	}
	function nextSlide() {
		if (currentSlide < (numSlides - 1)) {
			currentSlide++;
		} else {
			currentSlide = 0;
		}
		switchToSlide(currentSlide);
	}
	function prevSlide() {
		if (currentSlide > 0) {
			currentSlide--;
		} else {
			currentSlide = (numSlides - 1);
		}
		switchToSlide(currentSlide);
	}

	var tabAtual = 1
	mudarTab = function(numeroTab) {
		$("#tab_" + tabAtual).toggle();
		$("#tab_" + numeroTab).toggle();
		$("#br_" + numeroTab).addClass("active");
		$("#br_" + tabAtual).removeClass("active").addClass("cor-busca-rapida");
		tabAtual = numeroTab;
	}
	function enviarBuscaAvancada() {
		var url = "motoMarca=" + document.getElementById('motoMarca').value;
		url += "&motoModelo=" + document.getElementById('motoModelo').value;
		url += "&estado=" + document.getElementById('estado').value;
		url += "&cidade=" + document.getElementById('cidade').value;
		if (document.getElementById("apenas_lojista").checked == true) {
			url += "&apenas_lojista="
					+ document.getElementById('apenas_lojista').value;
		}
		if (document.getElementById("apenas_particular").checked == true) {
			url += "&apenas_particular="
					+ document.getElementById('apenas_particular').value;
		}
		url += "&ano_inicial=" + document.getElementById('anoInicial').value;
		url += "&ano_final=" + document.getElementById('anoFinal').value;
		url += "&preco_inicial=" + document.getElementById('precoMinimo').value;
		url += "&preco_final=" + document.getElementById('precoMaximo').value;
		document.getElementById("busca-avancada-new").href = "/anunciosv2/buscaAnuncioMoto.form?"
				+ url;
	}
	// final script
</script>
<script>
	function inicializa() {
		carregaMontadoras();
		carregaTopOfertas();
		carregaTopOfertasOrizontal();
	}
	
	function carregaMontadoras() {
	    $.getJSON('https://www.moto.com.br/img/json/motoMarca.json',
	        function(data) {
	            $.each(data.marcas, function(i, marca) {
	                var urlMarca = marca.marca.replace(" ", "_");
	                $("#montadoras .clearfix").append('<a href="/' + urlMarca + '/historia.html"><img src="https://www.moto.com.br' + marca.imagemPrincipalPath3 + '"/></a>');
	            });
	            $('#montadoras .clearfix').carouFredSel({
	                auto: false,
	                prev: "#paginacao-montadoras-prev",
	                next: "#paginacao-montadoras-next",
	                items: 11,
	                scroll: 1
	            });
	        });
	}

	function carregaTopOfertas(){
		$("#top-fertas-botao")
		.append(
				"<div class='paginacao-limita-newhome'><a href='' class='top-ofertas-prev-newhome'><span class='ico-trans-topofertas-newhome'></span></a>  <a href='' class='top-ofertas-next-newhome'><span class='ico-trans-topofertas-newhome'></span></a></div>");
		$(".lista-top-ofertas").carouFredSel({
			auto : false,
			circular : false,
			prev : ".top-ofertas-prev-newhome",
			next : ".top-ofertas-next-newhome",
			direction : "up",
			items : 10,
			visible : 10,
			minimum : 10,
			scroll : 1,
		});
	}
	
	function carregaTopOfertasOrizontal(){
		$.getJSON('https://www.moto.com.br/img/json/getAnunciosSDPF.json', function(data) {
			$.each(data.anuncios, function(i, anuncio) {
				$("#anuncRelacHori").append(
					'<section class="topofertashorizontal">'
						+ '<div class="bloco-oferta">'
						+ '<a href="/comprar/'+anuncio.linkDescription+'-'+anuncio.id+'.html" title='+anuncio.descricao+'">'
						+ '<div class="anuncio-relacionado-horizontal">'
						+ '<img src="https://www.moto.com.br/'
						+ anuncio.imagem
						+ '" class="anuncio-descricao" alt='
						+ anuncio.descricao
						+ '" />'
						+ '<p class="marcaModelo">'
						+ anuncio.marca
						+ '  '
						+ anuncio.modelo
						+ ' <br></p> <p> '
						+ anuncio.ano
						+ '</p>'
						+ '<p class="preco">'
						+ anuncio.moeda
						+ ' '
						+ anuncio.preco
						+ '</p>'
						+ '</div>'
						+ '</a>'
						+ '</div>'
						+ '</section> ');
				if (i <= 8) {
					$("#anuncRelac").append(
									'<div class="hr"></div>');
				}
			});
		});
	}

	function formatReal( int ){
	        var tmp = int+'';
	        if( tmp.length >= 4 && tmp.length <= 6  ){
	      		tmp = tmp.replace(/([0-9]{3})$/g, ".$1");
	      		return tmp;
	        }
	        if( tmp.length >= 7 && tmp.length <= 9){
	        	tmp = tmp.replace(/([0-9]{6})$/g, ".$1");
	            tmp = tmp.replace(/([0-9]{3})$/g, ".$1");
	            
	            return tmp;
	        }
	        if( tmp.length >= 10 && tmp.length <= 12){
	        	tmp = tmp.replace(/([0-9]{9})$/g, ".$1");
	        	tmp = tmp.replace(/([0-9]{6})$/g, ".$1");
	            tmp = tmp.replace(/([0-9]{3})$/g, ".$1");
	            return tmp;
	        }

	        return tmp;
	}
	
