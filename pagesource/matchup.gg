  <script>
    $.getJSON("http://ddragon.leagueoflegends.com/cdn/8.1.1/data/en_US/champion.json", function(data) {
      var champ_data = [];
      var champ_name_to_id_map = {};
      var champ_names = []
      $.each(data["data"], function(key, val) {
        champ_data.push({"label": val["name"], "value": key})
        champ_name_to_id_map[val["name"].toUpperCase()] = key;
        champ_names.push(key.toUpperCase())
      });

      $('#search-form').submit(
        function () {
          if ($('#main-search-container').hasClass('champion')) {
            // If searching for champions
            var ally_champion = $('#main-search-champion-input').val();
            var opponent_champion = $('#main-search-opponent-input').val();
            if (champ_name_to_id_map[ally_champion.toUpperCase()]) {
              ally_champion = champ_name_to_id_map[ally_champion.toUpperCase()];
            }
            if (champ_name_to_id_map[opponent_champion.toUpperCase()]) {
              opponent_champion = champ_name_to_id_map[opponent_champion.toUpperCase()];
            }
            if (ally_champion && opponent_champion) {
              window.location.href = "/matchup/" + ally_champion + "/" + opponent_champion
            }
          } else {
            // Searching by summoner
            var potential_champion_name;
            var summoner_or_champion_name = $('#main-search-summoner-input').val();
            if (summoner_or_champion_name.toUpperCase() in champ_name_to_id_map) {
              potential_champion_name = champ_name_to_id_map[summoner_or_champion_name.toUpperCase()]
            } else if (champ_names.indexOf(summoner_or_champion_name.toUpperCase()) != -1) {
              potential_champion_name = summoner_or_champion_name;
            }
            if (!potential_champion_name) {
              setCookie("summoner", summoner_or_champion_name);
              window.location.href = "/" + $('.search-region-selector').attr('data-region') + "/summoner/" + summoner_or_champion_name
            } else {
              setCookie("summoner", summoner_or_champion_name);
              window.location.href = "/champion/" + potential_champion_name;
            }
          }
          return false;
        }
      );

      new Awesomplete(document.getElementById("main-search-champion-input"), {list: champ_data});
      new Awesomplete(document.getElementById("main-search-opponent-input"), {list: champ_data});
      new Awesomplete(document.getElementById("main-search-summoner-input"), {list: champ_data});

      function addAutocompleteImage(e) {
        var new_obj = $(e.target);

        if (new_obj.is('li')) {
          var champ_name = champ_name_to_id_map[new_obj.text().toUpperCase()];
          var img_div = $('<img>',
            {src: "//ddragon.leagueoflegends.com/cdn/" + 8.1 +".1/img/champion/" + champ_name + ".png",
              width: "25px", height: "25px"
            }).load(function () {});
          new_obj.prepend($('<div>', {class: "autocomplete-image"}).append(img_div));
        }
      }

      $('#main-search-champion-input').parent().on("DOMNodeInserted", addAutocompleteImage);
      $('#main-search-opponent-input').parent().on("DOMNodeInserted", addAutocompleteImage);
      $('#main-search-summoner-input').parent().on("DOMNodeInserted", addAutocompleteImage);
    });
  </script>


