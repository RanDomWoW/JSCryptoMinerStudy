<script type="text/javascript">
var endtimes = {};
var starttimes = {};
var colonhidden = false;
jQuery(document).ready(function() {
jQuery('.daily_box .daily_header').each(function(key , value) {
var endtime = jQuery(value).attr('data-endtime');
if(endtime.length > 0) {
endtimes[key] = new Date(endtime);
} else {
endtimes[key] = null;
}
var starttime = jQuery(value).attr('data-starttime');
if(starttime.length > 0) {
starttimes[key] = new Date(starttime);
} else {
starttimes[key] = null;
}
});
setInterval(function() {
webtech_calc_timeline();
} , 1000 * 60 * 5);
webtech_calc_timeline();
});
function webtech_calc_timeline() {
var nowDate = new Date(new Date(new Date().toUTCString()).valueOf() + 3600 * 8 * 1000);
jQuery('.daily_box .daily_header').each(function(key , value) {
var startDate = starttimes[key];
var endDate = endtimes[key];
if(startDate != null) {
var _diff = (nowDate.valueOf() - startDate.valueOf()) / 1000;
var _absdiff = Math.abs(_diff);
if(_diff > 0) {
if(endDate != null) {
var _diff = (endDate.valueOf() - nowDate.valueOf()) / 1000;
var _absdiff = Math.abs(_diff);
var _day = Math.floor(_absdiff / (24 * 3600));
var _hour = Math.floor((_absdiff % (24 * 3600)) / 3600);
var _minutes = Math.floor((_absdiff % (24 * 3600)) / (24 * 60));
jQuery(value).find('.daily_clock > p').html((_day ? _day + '日' : '') + (_hour ? pad(_hour , 2) + ':' : '00') + (_minutes ? pad(_minutes,2) : ''));
}
} else {
var _day = Math.floor(_absdiff / (24 * 3600));
jQuery(value).find('.daily_clock > p').html(_day + '日後開始');
}
}
});	
}

function pad (str, max) {
 str = str.toString();
 return str.length < max ? pad("0" + str, max) : str;
}

</script>
