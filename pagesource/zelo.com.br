        function productRememberLoadSku()
        {
         
            $.ajax({
                    type: "POST",
                    url: "http://www2.zelo.com.br/services/VerticoServices.asmx/GetProductSku",
                    data: "{idProduct:'" + $('#dvProductRemember').attr("idProduct") + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function(msg) {
                        var obj;
                        eval("obj = " + msg.d);
                        if (obj.isError == 0) {
                            var sku = obj.message.split(",");
                            var ddl = "<option value='0'>Selecione</option>";
                            for(i = 0; i < sku.length; i++)
                            {
                                ddl += "<option value='" + sku[i] + "'>" + sku[i].split("-")[1] + "</option> \n";
                            }
                            $("#ddlRememberSku").html(ddl);
                            $("#trProductRememberSku").show();
                        }
                    },
                    error: function(httpRequest, status, data) {
                        
                    }
                });   
        }

      function productRememberSend() {
            
            var idProduct = $('#dvProductRemember').attr("idProduct");
            
            if(!Page_ClientValidate("ProductRemember"))
            {
                $("#ctl00_lblRemeberError").html("");
                return;
            }
           
            if($("#trProductRememberSku").is(":visible"))
            {
                if($("#ddlRememberSku").val() == "0")
                {
                    $("#ctl00_lblRemeberError").html("Por favor, selecione um tamanho");
                    return;
                }
                idProduct = $("#ddlRememberSku").val();
            }
            $("#dvProductRememberLoading").height($('#ctl00_dvProductRememberForm').height());
            $('#ctl00_dvProductRememberForm').fadeOut("fast", function() {
                $("#dvProductRememberLoading").fadeIn("fast", function() {
                    $.ajax({
                        type: "POST",
                        url: "http://www2.zelo.com.br/services/VerticoServices.asmx/CallProductRememberInsert",
                        data: "{idProduct:'" + idProduct + "', dsName: '" + $("#ctl00_iptProductRememberName").val() + "', dsEmail: '" + $("#ctl00_iptProductRememberEmail").val() + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function(msg) {
                            var obj;
                            eval("obj = " + msg.d);
                            if (obj.isError == 0) {
                                $("#ctl00_iptProductRememberName").val("");
                                $("#ctl00_iptProductRememberEmail").val("");
                            }
                            $("#ctl00_lblRemeberError").html(obj.message);
                            
                            $("#dvProductRememberLoading").hide();
                            $('#ctl00_dvProductRememberForm').show();
                        },
                        error: function(httpRequest, status, data) {
                            $("#ctl00_lblRemeberError").html(data);
                            
                            $("#dvProductRememberLoading").hide();
                            $('#ctl00_dvProductRememberForm').show();
                        }
                    });
                });
            });
        }

