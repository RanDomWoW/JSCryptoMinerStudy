      $( '.blog-posts' ).append( '<div class="col-md-4"><div class="card"><a target="_blank" href="' + post.link + '"><img class="card-img-top img-responsive lazy" src="' + post.meta_fields.post_image[0] + '"></a><div class="card-block"><h2 class="bold text-center card-title">' + post.title.rendered + '</h2><div class="text-center dots">&#9679;&#9679;&#9679;</div>' + post.excerpt.rendered + '<a target="_blank" href="' + post.link + '">READ MORE</a></div></div>' );
    });
  }
});

$.ajax({
