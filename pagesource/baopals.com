<script>

$('.topPicks .responsive-slider, .themedSlider .responsive-slider, .deptSlider .responsive-slider, .recentOrders .responsive-slider').on('init', function(event, slick, currentSlide, nextSlide) {
  // Randomize the slides and then run slick slider
  $.fn.randomize = function(selector) {
      var $elems = selector ? $(this).find(selector) : $(this).children(),
      $parents = $elems.parent();

      $parents.each(function() {
          $(this).children(selector).sort(function(){
              return Math.round(Math.random()) - 0.5;
          }).detach().appendTo(this);
      });

      return this;
  };

  $('.topPicks .responsive-slider, .themedSlider .responsive-slider, .deptSlider .responsive-slider, .recentOrders .responsive-slider').find('.slick-track').randomize('.slick-slide');
});

$.ajax({
  type: "GET",
