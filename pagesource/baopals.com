    // Search dropdown
    $('.SearchInput').keyup(function(e) { //when key is pressed in search bar
      if (e.keyCode != 13 && e.keyCode != 32 && e.keyCode != 40 && e.keyCode != 38) {
        var searchTerm = $(this).val(); //val of search bar
        var myExp = new RegExp(searchTerm, "i"); //regular experation
        window.isUsingRecSearch = false;
        window.displayBoxIndex = -1;

        $.getJSON('/catalog/view/theme/lexus_megashop/searchResults.json', function(data) { //get the json file

          var output = "<ul>";
          var loopCount = 0;
          $.each(data, function(key, val) {
            if (val.title.search(myExp) != -1) { //search for the data in the json file
              loopCount += 1;
              if (loopCount >= 10) {
                return false;
              }
              output += '<li>';
              output += '<a href="' + val.url + '">' + val.title + '</a>';
              output += '</li>';
            }
          }); //end each
          output += "</ul>";
          $('.updateResults').html(output); //output result to the update div
        });
        if (!$(this).val()) {
          $('.updateResults').hide()
        } else if ($(this).val()) {
          $('.updateResults').show()
        }
      }
    });
    $("body").click(function(event) {
    	if (!$(event.target).is(".updateResults")) {
    		$(".updateResults").hide();
        window.displayBoxIndex = -1;
    	}
    	if ($(event.target).is("#txtkey")) {
    		if (!$('#txtkey').val()) {
    			$(".updateResults").hide();
    		} else {
    			$(".updateResults").show();
    		}
    	}
    	if (!$(".updateResults ul").has("li").length) {
    		$(".updateResults").hide()
        window.displayBoxIndex = -1;
    	}
    });
    </script>

