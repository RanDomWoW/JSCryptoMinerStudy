      const subst = `//img.alicdn.com/`;
      const result = str.replace(regex, subst);
      $('#dealsContent').append('<div class="col-md-3 daily-deal"><div class="deal-pic"><a href="' + post.meta_fields.product_url[0] + '"><img class="img-responsive lazy" src="' + result + '"></a></div><span class="h2 bold text-center">' + post.title.rendered + '</span><div class="price clearfix"><div class="pull-left"><span class="active-price">¥' + post.meta_fields.product_price[0] + '</span><span class="old-price">¥' + post.meta_fields.product_price_old + '</span></div><div class="pull-right"><a href="' + post.meta_fields.product_url[0] + '">Buy Now</a></div></div></div>');
    });

    $('#dealsContent:first-child').addClass('col-md-offset-1');
  }
});

