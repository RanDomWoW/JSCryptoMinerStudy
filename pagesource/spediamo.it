	<script type="text/javascript">
		/* (custom) dynamic answer to 'segui la spedizione' */
		var urlCallTrackShipmentHistoryRequest = "http://spediamo.it/ajax/call-track-shipment-history-request";
		function openWarn(html) {
			$('#tracking_dialog').dialog('option', 'title', 'Errore');
			$('#tracking_dialog').html(html);
			$('#tracking_dialog').dialog('open');
		}
		function trim(str) {
			str = str.replace(/^\s+/, '');
			for (var i = str.length - 1; i >= 0; i--) { if (/\S/.test(str.charAt(i))) { str = str.substring(0, i + 1); break; } }
			return str;
		}
		$(document).ready(function()
		{
			$('#tracking_dialog').dialog({
				autoOpen: false, 
				modal: true, 
				width: 850, 
				resizable: false, 
				draggable: false, 
				buttons: { 'Chiudi': function() { $(this).dialog('close'); } }
			});

			$("#click_for_tracking").live('click', function(e) {

				var tracking_number = trim($('#tracking_number').val());

				if (tracking_number == '') return false;

				if ( (tracking_number.match('^[0-9]{13}$') != null) || (tracking_number.match('^1[0-9]{11}$') != null) || (tracking_number.match('^[A-Za-z]{1}[0-9]{3}-[0-9]{3}-[0-9]{3}-[0-9]{3}$') != null) ) {
					$.ajax({
						type: 'POST', 
						url: urlCallTrackShipmentHistoryRequest,
						data: 'publicId='+$('#tracking_number').val(),
						dataType: 'json',
						success: function(o) {

							$('#tracking_dialog').dialog('option', 'title', 'Numero di tracking: ' + tracking_number);

							if (o.level && o.level == 'ERROR') {
								$('#tracking_dialog').html(o.message);
							}else	$('#tracking_dialog').html(o.html);

							$('#tracking_dialog').dialog('open');
						},
						error: function(err) { openWarn('C\'&egrave; un problema nella connessione. Riprova pi&ugrave; tardi.'); }
					});
				} else	openWarn('Nessuna informazione disponibile.<br>Controlla di avere digitato il codice riferimento spedizione correttamente.');
				return false;
			});
		});
	</script>
