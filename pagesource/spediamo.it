								<script type="text/javascript">
					var urlLogin = "http://spediamo.it/ajax/session/login";
					var urlLogout = "http://spediamo.it/ajax/session/logout";
					var urlRegistrati = "http://spediamo.it/registrati";
					var urlForgotPassword = "http://spediamo.it/forgot-password";
					var urlRetrievePassword = "http://spediamo.it/ajax/session/retrieve-password";
					function isErrInAjax(o) {
						if (o.message)
							$('#login_err').html('<h4>'+o.message+'</h4>');
						if (o.level && o.level == 'ERROR') return true; else return false;
					}
					function listen() {
						$(document).ready(function() {
							$('#click_for_login').click(function() {
								if ($(this).attr('src') == 'images/button-entra.png')
									$.ajax({
										type: 'post',
										url: urlLogin,
										data: 'username='+encodeURIComponent($('#username').val()) + '&passwd='+encodeURIComponent($('#password').val()),
										dataType: 'json',
										success: function(o) {
											if (isErrInAjax(o)) return false;
											if (o.ok == 'false') {
												$('#password').val('');
												$('#login_err').html('<h4>email e/o password errata!</h4>');
											}else	window.location = o.url;
										},
										error: function(err) { $('#login_err').html('<h4>problemi di rete!</h4>'); }
									});
								else
									$.ajax({
										type: 'post',
										url: urlRetrievePassword,
										data: 'email='+$('#username').val(),
										dataType: 'json',
										success: function(o) {
											if (o.message) $('#login_err').html('<h4>'+o.message+'</h4>');
											//if (o.ok == 'yes')
												redrawLogin('checkMail');
											//else
											//	$('#login_err').html('<h4>email non registrata!</h4>');
										},
										error: function(err) { $('#login_err').html('<h4>problemi di rete!</h4>'); }
									});
							});
							$('#click_for_logout').click(function() {
								$.ajax({
									type: 'post',
									url: urlLogout,
									dataType: 'json',
									success: function(o) {
										if (o.message) $('#login_err').html('<h4>'+o.message+'</h4>');
										if (o.ok == 'yes')
											redrawLogin();
										else	$('#login_err').html('<h4>problemi di rete!</h4>');
									},
									error: function(err) { $('#login_err').html('<h4>problemi di rete!</h4>'); }
								});
							});
							$('#click_for_forgotten_password').click(function() {
								$('#password_block').hide();
								$('#click_for_forgotten_password').hide();
								$('#click_for_login').attr('src', 'images/button-invia.png');
								$('#login_err').html('');
							});
						});
					}
					function redrawLogin(optMail) {
						$('#login').html(''+
							'<h2>Login</h2>' +
							'<form name="loginform" action="javascript:void(0);" method="post">' +
								'<label for="username">username</label>' +
								'<input type="text" name="username" id="username" class="field100"/>' +
								'<label for="password">password</label>' +
								'<input type="password" name="password" id="password" class="field100" />' +
								((optMail) ? '' : '<ul><li><a title="Hai dimenticato la password?" href="'+urlForgotPassword+'" alt="dimenticato password spedizioni" title="dimenticato password spedizioni">Hai dimenticato la password?</a></li></ul>') +
								'<input id="click_for_login" type="image" src="images/button-entra.png" name="submit" class="imagebutton"/>' +
							'</form>' +
							'<br class="clear"/>' +
							'<div id="login_err"></div>' +
							((optMail) ? '<h4 style="color:#FBC717">password inviata,<br/>controlla la tua email</h4>' : '<h3><a href="'+urlRegistrati+'"> <img src="images/arrow1.png" alt="registrati per spedire" title="registrati per spedire" /> Registrati ora</a></h3>'));
						listen();
					}
					listen();
				</script>
