        jQuery(document).ready(function() {

            $(".js-modal-btn").modalVideo({autoplay:1,controls:0,showinfo:0});

            $(".btn").click(function(){
                var type = $(this).attr('data-click');
                if(type=="standard"){ $("#plan").val("m"); }
                if(type=="premium"){ $("#plan").val("pm");}
                if(type=="free"){ $("#plan").val("free");}
            });
            $("#submit").click(function(){
                var query = $("#register-form").serialize();
                $('#submit').prop('disabled', true);
                $.ajax({
                    type: 'POST',
                    url: '/2016/inc/functions.php',
                    data: query,
                    cache: false,
                    success: function (response) {
                        console.info(response);
                        var res = response.split("|");
                        //error
                        if(res[0]=='0'){
                            $(".alert").addClass('alert-warning').html(res[1]).removeClass('hide');
                            $('#submit').prop('disabled', false);
                        }else if(res[0]=='1'){
                            $(".alert").addClass('alert-success').html(res[1]).removeClass('hide');
                            if(res[2]!='0'){
                                window.setTimeout(function(){  
                                    // Move to a new location or you can do something else
                                    window.location.href = 'p?' + res[2];
                                }, 3000);
                            }
                        }else{
                             $(".alert").addClass('alert-danger').html('Sorry there was a problem with your sign up, please contact us').removeClass('hide');
                        }
                    }
                });
                return false;
            });
        });
    </script>
