		<!-- multidomain access -->
		<script type="text/javascript">
    (function(global, doc) {
        'use strict';

        global.addEventListener('message', function (event) {
            var acceptableSources = ["https:\/\/www.fn.no","https:\/\/www.globalis.fi","https:\/\/www.globalis.se","https:\/\/www.globalis.dk","https:\/\/www.globalis.is","https:\/\/eng.fn.dev.seeds.no"],
                eventId = 'eZEvent',
                isKnownSource = acceptableSources.some(function (source) {
                    return source === event.origin;
                }),
                setBlockContent = function (config) {
                    var block = doc.querySelector(config.selector);

                    if (block) {
                        block.innerHTML = config.content;
                    }
                },
                setBlockAttribute = function (config) {
                    var zoneNode = doc.querySelector(config.selectorZone),
                        blockNodes;

                    if (!zoneNode) {
                        return;
                    }
                    blockNodes = [].slice.call(zoneNode.querySelectorAll(config.selectorBlock));

                    blockNodes[config.index].setAttribute(config.attributeName, config.attributeValue);
                },
                response = {
                    height: Math.max(
                        doc.body.scrollHeight,
                        doc.body.offsetHeight,
                        doc.body.clientHeight
                    ),
                    pathName: global.location.pathname,
                    href: global.location.href,
                    id: eventId
                };

            if (!isKnownSource || event.data.id !== eventId) {
                return;
            }

            if (event.data.content) {
                setBlockContent(event.data.content);
            }

            if (event.data.blockAttribute) {
                setBlockAttribute(event.data.blockAttribute);

                return;
            }

            event.source.postMessage(response, event.origin);
        }, false);
    })(window, document);
</script>


