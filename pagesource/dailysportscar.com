					 * -------------------------------------------------------------------
					 * This is our true of false function for when we have a response.
					 * @param 		string data the response from the ajax call
					 * @return 		bool
					 * -------------------------------------------------------------------
					 */
					function parseResults(data) {
						try{
							data = JSON.parse(data);
							return true;
