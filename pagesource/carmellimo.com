			});
		
			<!-- ***** Banner Slider ***** --->
			revapi = jQuery('.tp-banner').revolution({
											delay:6000,
											hideThumbs:10,
											minFullScreenHeight:436,
											fullScreen:'on',
											fullWidth:'on',
