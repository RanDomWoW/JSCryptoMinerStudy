	 	function setCampaignParams(){
			var campaignSource = $.cookie('campaignSource');
			var campaignMedium = $.cookie('campaignMedium');
			var campaignName = $.cookie('campaignName');
			var campaignId = $.cookie('campaignId');
			if(campaignSource && campaignMedium && campaignName && campaignId){
				 ga('set', 'campaignSource', campaignSource); // source from which this page is being clicked or referrer from
		         ga('set', 'campaignMedium', campaignMedium); // medium like email or facebook or google ad or google search.. etc.,
		         ga('set', 'campaignName', campaignName);   // unnique campaign name
		         ga('set', 'campaignId', campaignId);   // any unique random id we can generate for each campaign
			}
		}
	 	
	 	function setBookedTripParams(){
			if(typeof carmelTripId != "undefined"){
				 ga('set', 'dimension1', 'Trip Id -- ' + carmelTripId); 
			}
		}
	 	
		var isExecAdmin = false;
		$(function(){
			$.ajax({
				url:"/WebApp/Ajax?action=checkSessionCookie",
		 		async:false,
		 		dataType:"text",
		 		success:function(data) {
		 			if('' != data.trim()) {
			 			var loginVars = data.trim().split(',');
			 			if(loginVars[1].substring(loginVars[1].indexOf(':') + 1) == "false") {
			 				$.removeCookie('logincookie', { path: '/' });
			 			} else if(loginVars[0].substring(loginVars[0].indexOf(':') + 1) == "true" ) {
			 				isExecAdmin = true;
			 			} 
		 			}
		 		}
			});
			configureLoginMenus();
			configureSideLoginMenu();
		  	var execCookie = $.cookie("execAcctParams");
		  	if (execCookie !== undefined && execCookie != "") {
				var startIndex = execCookie.indexOf(":")+2;
			  	var endIndex = execCookie.length-2;
			  	var acctId = execCookie.substring(startIndex, endIndex);
				$('#AboutService').html('Proud to Service ' + acctId + ' Members');
		  	}
		  	else{
		  		$('#AboutService').html('Need a Ride? Just Carmel it!');
		  	}
		  	
		  	var affLinkCookie = $.cookie('webAffLink');
		  	if (affLinkCookie !== undefined && affLinkCookie != "") {
		  		$('#affLinkDesc').prepend('<!--' + 'Affiliate Link: ' + affLinkCookie + '-->');
		  		$('#affLinkAux').unwrap();
		  		$('#affLinkAux').remove();
		  	}
		  	setFooterCopyRight();
		});
		
