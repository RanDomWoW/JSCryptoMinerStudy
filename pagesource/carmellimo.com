			$('.bquotebox,.hstop').on('mouseover', function(e) { 
				if($(window).width()>950) {
					$('.tp-banner').revpause();
				} else {
					$('.tp-banner').revresume();
				}
			});
			// When resume button is clicked...
			$('.bquotebox,.hstop').on('mouseout', function(e){ 
				$('.tp-banner').revresume();
			});
			<!-- ^^^^^ Banner Slider End ^^^^^ --->
			
			<!-- ***** Flight Slider ***** -->
			revap = jQuery('.sb-banner').revolution({
											delay:6000,
