                    document.forms[0].submit();
                }
            });

            $("div#menu ul li:last-child").css("margin-right", "0px");

            $('#q').focus(function () {
                $(this).val('');
            });

            $('#q').blur(function () {
                if ($(this).val() == '') {
                    $(this).val($(this).attr('data-tooltip'));
                }
            });



            $('#menu-main>li').hover(function () {
                $('ul:first', this).show();
            }, function () {
                $('ul:first', this).hide();
            });

            $('#menu-main li>ul li').hover(function () {
                var current = $('ul', this);
                var parent = current.parent().parent();
                parentHeight = parent.height();

                if (current.height() > parent.height()) {
                    parent.height(current.height());
                }
                if (current.height() < parent.height()) {
                    current.height(parent.height());
                }
                current.fadeIn(300);
            }, function () {
                var current = $('ul', this)
                current.hide();
                if (!!parentHeight) {
                    var parent = current.parent().parent().height(parentHeight);
                }
            });

        });


