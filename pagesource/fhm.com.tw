        <script>

var count = 0;
var countGirlIcon = 0;

$(document).ready(function() {
    carouselInit();
    girlIconBarInit();
});

function carouselInit() {
    $(".carousel-box").each(function(){
        var t = $(this);
        var width = t.width();
        var height = t.find('img').height();
        var length = t.find('li').length;
        if(length <= 1){
            return;
        }
        //console.log(length);
        var delay = 5000;
        setInterval(carouselInterval, delay,t);
        t.find('.carousel-list li').css('height' ,height );
        t.find('.carousel-list li').css('width' ,width );
        t.find('.carousel-list').css('width', length * width );
    });

}

function carouselInterval(t){
    var width = t.width();
    var height = t.find('img').height();
    var length = t.find('li').length;
    var now = parseFloat(t.find('.carousel-list').css('margin-left'));
    margin = 0 - width+now;
    margin = (margin*-1 < length * width) ? margin : 0 ;
    //console.log(margin , length * width);

    t.find('.carousel-list').css({marginLeft: margin});
    t.find('.carousel-list li').css('height' ,height );
    t.find('.carousel-list li').css('width' ,width );
    t.find('.carousel-list').css('width', length * width );
}

function girlIconBarInit() {
    var width = 95;
    var jump = 3;
    var length = $('.gril-icon-list li').length;
    var totalWidth = length * width;
    var boxWidth = $('.girl-icon-bar').width();
    var paddingRight = 20;
    $('.gril-icon-list').width( totalWidth );

    $('.girl-icon-bar .girl-icon-control-right').click(function() {
        var marginLeft = parseInt($('.gril-icon-list').css('margin-left'), 10);
        marginLeft = marginLeft - jump * width;
        marginLeft = Math.max(marginLeft, 0 - totalWidth + boxWidth - paddingRight);
        $('.gril-icon-list').css({marginLeft: marginLeft});
    });

    $('.girl-icon-bar .girl-icon-control-left').click(function() {
        var marginLeft = parseInt($('.gril-icon-list').css('margin-left'), 10);
        marginLeft = marginLeft + jump * width;
        marginLeft = Math.min(marginLeft, 0);
        $('.gril-icon-list').css({marginLeft: marginLeft});
    });
}
</script>
