<script>

    function showPriceChart(fromcity,tocity,fromLb,toLb){





        if(fromcity &&  tocity && (fromcity != tocity) ) {
            $('#flightPriceWeek').addClass('hide');

            $.ajax({
                url: 'flight/showPriceChart',
                data: {'fromCity': fromcity, 'toCity': tocity},
                method: 'POST',
                beforeSend: function () {
                    $("#loader-date").fadeIn();
                    $('.grid-price').html('<i class="fa fa-spin fa-spinner"></i>');
                },
                success: function (html) {
                    $("#loader-date").fadeOut();
                    $('#prices-graphs').html(html);
                    $('#prices-graphs').show();
                    $('#flightPriceWeek').removeClass('hide');
                    $('#fromCityText').text(fromLb);
                    $('#toCityText').text(toLb);

                }
            });
        }
    }
    function internalflight_exchange(from,to) {
        var source=$('select[name="'+from+'"]').val();  var dest=$('select[name="'+to+'"]').val();
        $('select[name="'+from+'"]').val(dest);
        $('select[name="'+to+'"]').val(source);
        $('select[name="'+from+'"]').trigger("chosen:updated");
        $('select[name="'+to+'"]').trigger("chosen:updated");
        var fromLb=$('#selectfromcity').find(":selected").text().split('-')[2];
        var toLb=$('#selecttocity').find(":selected").text().split('-')[2];
        showPriceChart($('#selectfromcity').val(),$('#selecttocity').val(),fromLb,toLb)
    }
</script>
