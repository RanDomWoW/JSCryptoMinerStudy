		}
	window.addEventListener('resize',function() {
		if (window.innerWidth < minRes) {
			marketingMenu.insertBefore(editionMenu,marketingMenu.firstChild);
		} else { 
			leftSideMenu.appendChild(editionMenu);
		}
	});
	editionMenu.addEventListener('click',function() {
		if (window.innerWidth < minRes) {
			editionMenu.children[1].classList.toggle("hide");
		}
	},false);
</script>
<!-- end edition menu responsiveness + click reveal -->

<!-- smooth scrolling for anchor links -->
<script>
$(document).ready(function() {
$('a[href^="#"]').click(function () {
    $('html, body').animate({
        scrollTop: $('[name="' + $.attr(this, 'href').substr(1) + '"]').offset().top
    }, 500);

    return false;
});
});
</script>
<!-- end smooth scropping for anchor links -->

