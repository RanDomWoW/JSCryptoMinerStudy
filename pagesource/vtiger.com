<script>
if(document.contains(document.getElementById("complete-pricing-edition-header"))) {
window.addEventListener('scroll', myFunction);
var pricingTableHeader = document.getElementById("complete-pricing-edition-header");
var pricingTableHeaderHeight = document.getElementById("complete-pricing-edition-header").offsetHeight;
var footerMenu = document.getElementById("footer");
var mainMenuHeight = document.getElementById("main-nav").offsetHeight;
var table = document.getElementById("pricing-table");
var stickyAt = table.getBoundingClientRect().top;
var unstickyAt = footerMenu.getBoundingClientRect().top - window.innerHeight;

function myFunction() {
	if (window.pageYOffset >= table.getBoundingClientRect().top + mainMenuHeight + pricingTableHeaderHeight + 250) {
		pricingTableHeader.classList.add("stickytop");
	} else {
		pricingTableHeader.classList.remove("stickytop");
	}
}
}
</script>
<!-- end complete pricing table sticky top-bar -->

<!-- left menu sticky -->
<script>
if(document.contains(document.getElementById("left-menu-main-container"))) {
window.addEventListener('load', myFunction);
window.addEventListener('scroll', myFunction);

var leftMenu = document.getElementById("left-menu-main-container");
var introHeader = document.getElementsByTagName("header")[0] || 0;
var introHeaderHeight = 0;
	
if (introHeader != 0) {
	introHeaderHeight = introHeader.offsetHeight
}	
	
var mainContent = document.getElementById("main-content");
var footerMenu = document.getElementById("footer");
var footerHeight = footerMenu.offsetHeight;
var stickyAt = introHeaderHeight + leftMenu.offsetHeight;
var unstickyAt = footerMenu.getBoundingClientRect().top - window.innerHeight;

function myFunction() {
	if (window.pageYOffset + window.innerHeight >= stickyAt && window.pageYOffset <= document.body.scrollHeight - window.innerHeight - footerHeight) {
		leftMenu.classList.add("sticky");
		leftMenu.classList.remove("stickyfooter")
		mainContent.classList.add("col-md-push-2");
	} else {
		leftMenu.classList.remove("sticky");
