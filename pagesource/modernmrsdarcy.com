<script src="//my.hellobar.com/616025e7546c67e2106f98671e27ca4af9bdc609.js" type="text/javascript" async="async"></script><script>
// Create a "check" button and append it to each list item
var myNodelist = document.querySelectorAll('.printselect .mbt-book');
var i;
for (i = 0; i < myNodelist.length; i++) {
  var span = document.createElement('SPAN');
  var txt = document.createTextNode('\u2713');
  span.className = 'printcheck';
  span.appendChild(txt);
  myNodelist[i].appendChild(span);
}

// Click on a check button to select the current list item
var check = document.querySelectorAll('.printselect .printcheck');
var i;
for (i = 0; i < check.length; i++) {
  check[i].onclick = function() {
    var div = this.parentElement;
    div.classList.toggle('printselected')
  }
}

// Click on a email button to email custom list
var printemaillink = document.getElementById('printemail');
printemaillink.onclick = function() {
    this.href = "mailto:?subject=MMD%20Custom%20Reading%20List&body=";
    this.href += getPrintListBody();
};

function getPrintListBody() {
    var selected = document.querySelectorAll('.printselected');
    var i;
    var printselected = encodeURI("Voila! Here is your customized reading list from the 2017 Modern Mrs Darcy Summer Reading Guide. We hope you find a book you absolutely love on this list. Tell us all about it on the blog, by email, or on social media—make sure to use the hashtag #summerstooshort (because summer's too short to read bad books, am I right?) and tag Anne @annebogel on instagram or twitter. We can't wait to see what you choose to read this summer.");
    printselected += '%0A%0AHappy%20reading!%0A';
    for (i = 0; i < selected.length; i++) {
         printselected += '%0A';
         printselected += encodeURI(selected[i].querySelectorAll('.mbt-book-title a')[0].innerHTML);
         printselected += '%0ABY%20';
         printselected += encodeURIComponent(selected[i].querySelectorAll('.mbt-meta-mbt_author a')[0].innerHTML);
         printselected += '%0A';
    }
    return printselected;
}
