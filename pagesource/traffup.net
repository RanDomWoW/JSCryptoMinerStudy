<script>

$(document).ready(function()
{
	tot_tm = $("#box_tm").find("div.tm").length;
	curr_tm = 0;
	
	navs = '';
	a = 0;
	while (a < tot_tm)
	{
		navs = navs + "<span rel='"+a+"' class='tm_dots'></span>";
		a = a+1;
	}
	
	$("#tm_nav").html(navs);
	
	// initiate
	show_tm(0);
	
	counter = setInterval(function()
	{
		show_tm('next');
		
	},4000);
		
	
	$(".tm_dots").click(function()
	{
		// stop auto scroll
		clearInterval(counter);
		
		pointer = $(this).attr("rel");
		show_tm(pointer);
	});
	
	
	function show_tm(num)
	{
		
		if (num == 'next')
		{
			// show next
			num = curr_tm + 1;
			if (num >= tot_tm) num = 0;
		}
		
			
	
		// reset
		$("#box_tm").find("div.tm").hide();
		$(".tm_dots").removeClass("tm_dot_sel");
		
		// show
		$("#box_tm").find("div.tm:eq("+num+")").animate({opacity: "show"},"slow");
		$(".tm_dots:eq("+num+")").addClass("tm_dot_sel");
		
		curr_tm = num;
		
		
		
	}
	
	
});


</script>




