    <script src="/js/hd/jquery.fullPage.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#app').fullpage({
                'css3': true,       
                'navigation': true,
                'navigationPosition': 'right',
                'loopBottom': true,
                'scrollingSpeed': 1000,
                afterLoad: function(anchorLink, index) {
                    if (index === 1) {
                        $('.section-1').addClass('active2');
                    }
                    ga('send', "pageview", "/main/section"+index);
                },
                onLeave: function(index, nextIndex, direction) {
                    if(nextIndex !== 1) {
                        $('.section-1').removeClass('active2');
                    }
                }
            });
            $('.section').removeClass('hide');
            $('.next').on('click', function(event) {
                var target = event.currentTarget;
                var section = $(target).data('section');
                $.fn.fullpage.moveSectionDown();
                ga('send', 'event','main','在' + section +'点击了next');
            })
            $('.download-pc').on('click', function(event) {
                var target = event.target;
                var section = $(target).data('download');
                ga('send', 'event','main','在' + section +'下载');
            })
            $('.section-menu > li').hover(function(){
               $(this).find('.section-subMenu').stop().show()
            }, function(){
               $(this).find('.section-subMenu').stop().hide()
            })
        });
    </script>
