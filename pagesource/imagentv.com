  <section class="item_carousel item_carousel_{{key}} overflow-hidden mt40 m-mt20">
    <h1 class="title-bloque proxima uppercase c-ecec h-fff pl10 pb-28 pt-48 pr10">
        {{#if header.name}}
            <a class="c-00a fs45 m-fs26 title_{{{getClassName header.name}}}" href="{{getLinkSection header.name}}">{{header.name}}</a>
        {{else}}
            <a class="c-00a fs45 m-fs26 title_{{{getClassName name}}}" href="{{getLinkSection header.name}}">{{name}}</a>
        {{/if}}
    </h1>
    <div class="slider responsive">
        {{#each items}}
            <div>
                <div class="content-item" itemscope="" itemtype="http://schema.org/TVSeries">
                    <a itemprop="url" href="{{this.url_desktop}}" class="dblock none-decoration prelative">
                        {{#if this.kaltura}}
                            <span class="icn-play-s sprite-play sprite icon-center"></span>
                        {{/if}}
                        {{#if this.thumbnail}}
                            <img width="335" height="182" itemprop="thumbnailUrl" src="data:image/png;base64," data-lazy="{{change_image_style this.thumbnail "medium" "carousel"}}" title="{{this.title}}" />
                        {{else}}
                            <img width="335" height="182" itemprop="thumbnailUrl" src="http://cdn.imagentv.com/resources/defaults/v2/imagen_default300.png" title="{{this.title}}" />
                        {{/if}}
                        {{#ifCond this.progress_percentage '>' 0}}
                            <span class="dblock percentageWrapper">
                                <span class="percentageBar" style="width: {{this.progress_percentage}}%"></span>
                            </span>
                        {{/ifCond}}
                    </a>
                    <div class="content-title relative dblock c-333 pt10">
                        <h1 itemprop="name" class="fs20 m-fs18 lh24">
                            <a itemprop="url" href="{{this.url_desktop}}" class="dblock none-decoration">
                                {{#if this.emission_description}}
                                    <span class="dblock fs16 c-158">{{this.emission_description}}</span>
                                {{/if}}
                                {{trimString this.title 0 90 "..."}}
                            </a>
                        </h1>
                        {{#if this.program}}
                            {{#if this.url_program}}
                                <a href="{{this.url_program}}" class="absolute title-program fs16 lh20">
                                    <span itemprop="alternateName">{{this.program}}</span>
                                </a>
                            {{else}}
                                {{#if this.url_desktop}}
                                    <a href="{{this.url_desktop}}" class="absolute title-program fs16 lh20">
                                        <span itemprop="alternateName">{{this.program}}</span>
                                    </a>
                                {{else}}
                                    <span itemprop="alternateName" class="absolute title-program fs16 lh20">{{this.program}}</span>
                                {{/if}}
                            {{/if}}
                        {{/if}}
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
</section>

