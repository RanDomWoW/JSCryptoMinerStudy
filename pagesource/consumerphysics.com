$('a').click(function() {
    var href = $(this).attr('href');
    var text = $(this).text(); 
    
    var element = $(this).find("span:contains('Read More')").parent();
    var loc = window.location.pathname;
    console.log(loc);
    //alert(element.text());
    if(element.length > 0 || (text.length > 0 && text.indexOf('Read') > -1)){
        //alert('a');
        if(loc.indexOf('business') !== -1){
        dataLayer.push({'event': 'cp-business', 'eventInfo': {'category':'Read more', 'action': "'"+ element.text() + "'", 'label': "'"+ href +"'", 'value': 0}});
            
        }else{
            if(loc.indexOf('press') !== -1){
                
                dataLayer.push({'event': 'cp-consumer', 'eventInfo': {'category':'Press', 'action': 'Click on read more', 'label': "'"+href +"'", 'value': 0}});
                console.log("dataLayer.push({'event': 'cp-consumer', 'eventInfo': {'category':'Press', 'action': 'Click on read more', 'label': '"+href +"', 'value': 0}});");
                
            }else{
                dataLayer.push({'event': 'cp-business', 'eventInfo': {'category':'Forms', 'action': "'"+ element.text() + "'", 'label': "'"+ href +"'", 'value': 0}});
            }
        }
    }
    
    if(href.indexOf('pdf') !== -1){
        var category = $(this).attr('data-cat');
        if(typeof category !== "undefined"){ //download gated file
            text = 'Content form - '+category;
            label = $(this).text();
        
            if (this.getAttribute("href").charAt(0) !== "#") {
                text = 'Download popup - '+category;
            }
            dataLayer.push({'event': 'cp-business', 'eventInfo': {'category':'Forms', 'action': "'"+text+"'", 'label':"'"+label+"'" , 'value': 0}});
            console.log("dataLayer.push({'event': 'cp-business', 'eventInfo': {'category':'Forms', 'action': '"+text+"', 'label':'"+label+"' , 'value': 0}});");
        }
    }
    console.log(element.text());
});





