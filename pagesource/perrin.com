<!-- GOOGLE REMARKETING -->
<script>
window.addEventListener('load',function(){
  var id = '';
  var price = 0;
  var page = '';

  //cart page
  if (window.location.pathname == '/shop/cart') {
    var page = 'cart';
    var id = new Array();
    var price = new Array();

    jQuery('.sku span').each(function() {
      id.push(jQuery(this).text());
    });
    price = jQuery('.totalPrice:last').text().split("$").reverse()[0].replace(/[^0-9.]/g, '');
    price = parseFloat(price);

  } else if (jQuery('.category-list').length > 0) {
    var page = 'category';

  } else if (jQuery('.product-detail').length > 0) {
    var page = 'product';
    var id = new Array();
    var price = new Array();

    id = variantSkus;
    price = jQuery('.price-group .price:first').text().replace(/[^0-9.]/g, '');
    if (price == '' || price == undefined || price == NaN) {
      price = jQuery('.price-group .origin:first').text().replace(/[^0-9.]/g, '');
    }
    price = parseFloat(price);

  } else if (window.location.href.indexOf('checkout_success') != -1) {
    var page = 'purchase';

  } else if (window.location.pathname == '/') {
    var page = 'home';

  } else {
    page = 'others';

  }

  var google_tag_params = {
    ecomm_prodid: id,
    ecomm_pagetype: page,
    ecomm_totalvalue: price
  };


   try {
        ga('set', 'dimension1', google_tag_params.ecomm_prodid.toString()); // Please make sure that Dimension 1 is set as the Custom Dimension for Product ID
    } catch (e) {}
    try {
        ga('set', 'dimension2', google_tag_params.ecomm_pagetype.toString()); // Please make sure that Dimension 2 is set as the Custom Dimension for Page Type
    } catch (e) {}
    try {
        ga('set', 'dimension3', google_tag_params.ecomm_totalvalue.toString()); // Please make sure that Dimension 3 is set as the Custom Dimension for Total Value
    } catch (e) {}
    ga('send', 'event', 'page', 'visit', google_tag_params.ecomm_pagetype.toString(), {
        'nonInteraction': 1
    });
    });
</script>

	
	
