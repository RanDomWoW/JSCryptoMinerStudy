<script>
	(function () {
		var toggleFeatureItem = function(event) {
			event.preventDefault();
			// show only chosen feature item
			var items = document.getElementsByClassName("features__item");
			window.app.showElementInArray(items, this.parentElement, 'features__item--collapsed');
			if (window.smoothscroll && window.innerWidth < 768) {
				window.smoothscroll.animateScroll(this.parentElement, this, { offset: 180, header: '[data-scroll-header]'});
			}
			// show image in browser-frame according to selected feature
			// formula => image_id === feature_item_id + 'picture' <- must be set in all items and their pictures
			var pictures = document.getElementsByClassName("front-frame__picture");
			var shownImage = document.getElementById(this.parentElement.id + '-picture');
			window.app.showElementInArray(pictures, shownImage, 'front-frame__picture--hidden');
		};

		var items_titles = document.getElementsByClassName("features__item-title");
		for (var i = 0; i < items_titles.length; i++) {
			items_titles[i].addEventListener('click', toggleFeatureItem);
		}
	}())
