<script>


    function linkToContactPage(hashVal) {

        if (window.location.href.indexOf("contact/mailto.aspx") < 0) {
            window.location.href = "https://www.b144.co.il/contact/mailto.aspx?_bezeq=1" + hashVal;
        } else {
            window.location.hash = hashVal;
            window.location.reload();
            return false;
        }
    }

    var linkRewriter = function (a, b) {
        $('a[href*="' + a + '"]').each(function () {
            $(this).attr('href', $(this).attr('href').replace(a, b));
        });
    };

    function switch_search_type(tabObject) {

        $(".main-header-tab").removeClass("header-tab--active");
        $(".sub-header-tab").removeClass("header-tab--active");

        $(".business-category").hide();
        $(".img_main").hide();


        $(tabObject).addClass("header-tab--active");

        var targetTabId = $(tabObject).data("target-tab");

        if ($(tabObject).data("target-tab") == "div_Sbusiness") {

            if (document.URL.indexOf("private") > -1) {
                linkRewriter("private", "www");
            }

            $("#div_Sbusiness").show();
            $("#div_Speople").hide();
            localStorage.setItem("searchType", "BusinessSearch");
        } else {
            $("#div_Sbusiness").hide();
            $("#div_Speople").show();
            localStorage.setItem("searchType", "PrivateSearch");
        }
    }
</script>
