    <script type="text/javascript">

        var bMainContentShown = false;
        var nativeAppProxy = undefined;

        $(window).load(function () {
            $("#stripe").show();
            ShowMainContent();
        });

        setTimeout(ShowMainContent, 1000);

        function ShowMainContent() {
            if (bMainContentShown == false) {
                $('.mainContent').css({ opacity: 0.0 }).animate({ opacity: 1.0 }, 2);
                bMainContentShown = true;
            }
        }


