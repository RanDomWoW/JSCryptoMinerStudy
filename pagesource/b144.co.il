        function setCookie(cvalue) {
            var cookieName = "isCoockieAllowed";
            var d = new Date();
            d.setTime(d.getTime() + (100 * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();

            document.cookie = cookieName + "=" + cvalue + ";" + expires + ";path=/";

            if (!checkCookie()) {
                popup.open();
            }
        }

        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1);
                if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
            }
            return "";
        }

        function checkCookie() {
            var isCoockieAllowed = getCookie("isCoockieAllowed");
            if (!(typeof isCoockieAllowed !== "undefined" && isCoockieAllowed == "true")) {
                return false;
            } else {
                return true;
            }
        }

        $(function () {
            var popup = (function () {
                var popupBar = document.querySelector('.popup-bar');
                popupBar.style.display = 'none';
                popupBar.style.height = '0px';

                var open = function () {
                    setTimeout(function () {
                        popupBar.style.display = 'block';
                        $('.popup-bar').animate({
                            height: '+=40',
                        }, 800, function () { }); // end of animate
                        $('.cookie-popup-msg').focus();
                    }, 1000);
                };

                var close = function () {
                    $('.popup-bar').animate({
                        height: 'toggle'
                    }, 800, function () { }); //end of animation
                    setTimeout(function () { // timeout for clearing the popup
                        popupBar.style.display = 'none';
                        popupBar.style.height = '0px';
                        console.log('popup display: ' + popupBar.style.display);
                    }, 1000); // end of setTimeout
                }; // end of close function

                $('.popup-close').click(function () {
                    return close();
                })
                return {
                    open: open
                };
            })();

            if (!checkCookie()) {
                popup.open();
            }

            var panel1 = new tabpanel("tabpanel1", "main-header-tab", "main-header-search", "header-tab--active");

        });


    </script>

