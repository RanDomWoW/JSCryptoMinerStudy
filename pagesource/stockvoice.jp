<script>
$(function () {
  function timestamp(str) {
    return moment(str).format('YYYY/MM/DD HH:mm');
  }

  var formatter = {
    qksNews: function (alert) {
      return _.template($('#qks-news-template').html())({alert: alert, timestamp: timestamp});
    },
    stockPrice: function (alert) {
      return _.template($('#stock-price-template').html())({alert: alert, timestamp: timestamp});
    },
    movingAverageCrossover: function (alert) {
      return _.template($('#moving-average-crossover-template').html())({alert: alert, timestamp: timestamp});
    }
  };

  function hasAlert(id) {
    return $(sprintf('.tab-pane_alert ul li[data-id="%s"]', id)).length > 0;
  }

  function updateCorpAlerts(alerts) {
    var duration = 1500;
    var max = 50;

    $.each(alerts, function (_, alert) {
      // 一覧に存在するアラートはスキップ
      if (hasAlert(alert.id))
        return true;

      var $li;

      // アラートデータをベースにDOMの部分木を生成
      switch (alert.type) {
      case 'qks_news':
        $li = $(formatter.qksNews(alert));
        break;
      case 'stock_price':
        $li = $(formatter.stockPrice(alert));
        break;
      case 'moving_average_crossover':
        $li = $(formatter.movingAverageCrossover(alert));
        break;
      }

      // アニメーション表示用に隠しておく
      $li.hide();

      // リストの先頭に追加
      $li.prependTo('.tab-pane_alert ul');
    });

    // 追加したアラートを表示
    $('.tab-pane_alert ul li:hidden').slideDown(duration);

    // 不要になったアラートを非表示
    var selector = sprintf('.tab-pane_alert ul li:gt(%d)', max);

    $(selector).fadeOut(duration, function () {
      // アラートが完全に非表示になったらDOMツリーから削除
      $(selector).remove();
    });
  }

  function fetchCorpAlerts(fn) {
    var url = $('#corp_alerts_url').val();

    $.get({url: url, dataType: 'json'})
      .done(function (data) {
        fn(data);
      });
  }

  var interval = 30 * 1000;

  // 初期表示
  fetchCorpAlerts(function (data) {
    updateCorpAlerts(data);
  });

  // タイマ登録
  setInterval(function (data) {
    fetchCorpAlerts(function (data) {
      updateCorpAlerts(data);
    });
  }, interval);
});
</script>

