    $('#questionForm #uname').attr('class','font_it_gr_12 inputborder');
    $('#questionForm #email').addClass('font_12_black inputborder');
    $('#questionForm #email').addClass('font_it_gr_12 inputborder');
    $('#questionForm #email').addClass('font_it_gr_12 inputborder');
    $('#questionForm #topic').val(str1);
    $('#questionForm #body').val(str2);
    $('#questionForm #uname').val(name);
    $('#questionForm #email').val(email);
    $('#questionForm #button').attr('disabled','disabled');

    $('#additional_question_form').hide();

}

function hideAdditional()
{
    if ($('#questionForm #topic').val() == '')
    {
        //clearQuestionForm();
        $('#questionForm #topic').val(str1);
        $('#questionForm #topic').attr('class','font_it_gr_12 inputborder');
    }
}

function selectExperience()
{
    str1='Тема вашего опыта (вкратце)';
    str2='Полный текст вашего опыта';
    clearQuestionForm();
}

function selectQuestion()
{
    str1='Тема вашего вопроса (вкратце)';
    str2='Полный текст вопроса';
    clearQuestionForm();
}

function checkQuestionForm()
{
    checked = true;
    if($("#questionForm #topic").val()==str1)
    {
        $('#modal').html('Не заполнено поле "'+str1+'"');
        checked = false;
    }
    else if($("#questionForm #body").val()==str2)
    {
        $('#modal').html('Не заполнено поле "'+str2+'"');
        checked = false;
    }
    else if($('#questionForm #subscribe').is(':checked'))
    {
        if ($('#questionForm #email').val()=='Email')
        {
            $('#modal').html('Не заполнено поле "Email"');
            checked = false;
        }
		else if(!regex_email.test($('#questionForm #email').val()))
		{
            $('#modal').html('Email некорректного формата');
            checked = false;
		}
        else if($('#questionForm #uname').val()=='Имя')
        {
            $('#modal').html('Не заполнено поле "Имя"');
            checked = false;
        }
    }
    else if($('#questionForm #uname').val()=='Имя') $('#questionForm #uname').val('Гость');
    if(checked)
    {
        name = $('#questionForm #uname').val();
		email = $('#questionForm #email').val();
        $.post("/files/QComments/post_comment.php",$('#questionForm').serialize(), function(data){
            if($("#questionForm #q").is(':checked')) msg = "Ваш вопрос успешно добавлен!<br>После проверки модератором, он появится на сайте.";
            else if($("#questionForm #exp").is(':checked')) msg = "Ваш опыт успешно добавлен!<br>После проверки модератором, он появится на сайте.";
            if($('#questionForm #subscribe').is(':checked')) msg += "<br>Вы успешно подписаны на обновление ответов и комментариев!";
            $('#modal').html(msg);

            $('#modal').modal({overlayClose:true});
            selectQuestion();
        });
    }
    else
    {
        $('#modal').modal({overlayClose:true});
    }
}
function openQFrom()
{
    closeAllQuestions();
    $("#openQFrom").html("");
    $('#questionFormDiv').show('slow');
}

