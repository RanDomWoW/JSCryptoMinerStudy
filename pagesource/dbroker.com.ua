    $(".answerFormDest").html('');
    if(q_id!=opened)
    openQuestion(q_id);
    question.next(".answerFormDest").html($('#answerFormSource').html());
    if(question.attr("class")=="answer")
    {
        vtype=1;
        question.next(".answerFormDest").find("#answ_id").val(question.attr("a_id"));
        astr = "Ваш комментарий к ответу";
        question.next(".answerFormDest").css("padding-left","40px");
    }
    else if(section!=3)
    {
        vtype=0;
        astr = "Ваш к ответу к вопросу";
        question.next(".answerFormDest").find("#answ_id").val('');
    }
	else
	{
        vtype=1;
        //question.next(".answerFormDest").find("#answ_id").val(question.attr("a_id"));
        astr = "Ваш комментарий к опыту";
        question.next(".answerFormDest").css("padding-left","40px");
		
	}
    question.next(".answerFormDest").find("#type").val(vtype);
    question.next(".answerFormDest").find("#q_id").val(q_id);
    question.next(".answerFormDest").find("#uname").val(name);
    question.next(".answerFormDest").find("#email").val(email);
    $(".answerFormDest").find("#body").val(astr);
}

function checkAnswerForm()
{
    $checked = true;
    if($("#answerForm #body").val()==astr)
    {
        $('#modal').html('Не заполнено поле "'+astr+'"');
        $checked = false;
    }
    else if($('#answerForm #subscribe').is(':checked'))
    {
        if ($('#answerForm #email').val()=='Email')
        {
            $('#modal').html('Не заполнено поле "Email"');
            $checked = false;
        }
		else if(!regex_email.test($('#answerForm #email').val()))
		{
            $('#modal').html('Email некорректного формата');
            $checked = false;
		}
        else if($('#answerForm #uname').val()=='Имя')
        {
            $('#modal').html('Не заполнено поле "Имя"');
            $checked = false;
        }

    }
    else if($('#answerForm #uname').val()=='Имя') $('#answerForm #uname').val('Гость');
	regex_dbroker = /.*dbroker.*/;
	if(regex_dbroker.test($('#answerForm #uname').val().toLowerCase()))
	{
		$('#modal').html('Поле "Имя" не может содержать "dbroker"');
		$checked = false;
	}

	if($checked)
	{
        name = $('#answerForm #uname').val();
		email = $('#answerForm #email').val();

		$.post("/files/QComments/check_captcha.php",{s:$('#answerForm #captcha').val()}, function(data){
			if(data!='1')
			{
				$('#modal').html('Неверно введены цифры на картинке');
				$checked = false;
			}
		});

		window.setTimeout('postComment()', 1000);
	}
    else
    {
        $('#modal').modal({overlayClose:true});
    }


}

function postComment()
{
    if($checked)
    {

        $.post("/files/QComments/post_comment.php",$('#answerForm').serialize(), function(data){
            q_id = $('.answerFormDest').find("#q_id").val();
            if($("#answerForm #answ_id").val()=="")
            {
                $n = $("#q"+q_id+" .num_answers").text();
                $("#q"+q_id+" .num_answers").text(parseInt($n)+1);
            }

            if($("#answerForm #type").val()==0)
            {
                msg = "Ваш ответ успешно добавлен!";
            }
            else if($("#answerForm #type").val()==1) msg = "Ваш комментарий успешно добавлен!";
            if($('#answerForm #subscribe').is(':checked')) msg += "<br>Вы успешно подписаны на обновление ответов и комментариев!";
            $('#modal').html(msg);

            $('#modal').modal({overlayClose:true});
            $('.answerFormDest').html('');
            opened=0;
            openQuestion(q_id);
        });
    }
    else
    {
        $('#modal').modal({overlayClose:true});
    }

}

// sections
function selectSection(n)
{
    if(n==section) return false;
    section = n;
    $("#list").hide();
    $('#questionFormDiv').show();
    $("#openQFrom").html("");
    opened=0;
    $(".secq").addClass("section");
	$("#sec"+3).addClass("section");
    $(".secq").removeClass("sec_active");
    $("#sec"+n).removeClass("section");
    $("#sec"+n).addClass("sec_active");
    $.post("/files/QComments/get_list.php",{ f: n, from: 0,page:'/'} , function(data){
        $("#list").html(data);
		if(n==3)
		{
			$("#border").attr("class","expborder");
			type=1;
			option = 1;
			astr = "Ваш комментарий к опыту";
		}
		else
		{
			$("#border").attr("class","qborder");
			type=0;
			option=0;
			astr = "Ваш ответ к вопросу";
		}
		$("#list").fadeIn(800);//show("slow");
    });

    /*if(n==3) $("#sec"+n).attr("bgcolor","#FEEEDB");
    else $("#sec"+n).attr("bgcolor","#FEF6DD");*/
}

function openQuestion(q_id)
{
    str = $(".questionOpened").find(".bodyquest").text();
    $(".questionOpened").find(".bodyquest").html(str.substr(0,200)+"...");
    closeAllQuestions();

    if(opened!=q_id)
    {
        opened=q_id;
        //console.log($(".questionOpened").siblings(".questionAnswers").first());
        //$(".questionOpened").next().html('');
        //$(".questionOpened").next().next().html('');
        $("#q"+q_id).next().next().hide();
        $("#q"+q_id).removeClass("question");
        $("#q"+q_id).addClass("questionOpened");
	$("#q"+q_id).find(".view").html('');
        $.post("/files/QComments/get_question_text.php",{ q_id: q_id} , function(data){
            $("#q"+q_id).find(".bodyquest").html(data);
        });

        $.post("/files/QComments/get_list.php",{ q_id: q_id} , function(data){
            $("#q"+q_id).next().next().html(data);
            $("#q"+q_id).next().next().fadeIn(800);
            if($("#q"+q_id).find(".view").attr('href')!=undefined )
                window.location = $("#q"+q_id).find(".view").attr('href');
        });
    }
	else opened = 0;
}

