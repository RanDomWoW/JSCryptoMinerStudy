<script language="javascript">
var str1 = 'Тема вашего вопроса (вкратце)';
var str2 = 'Полный текст вопроса';
var astr = 'Ваш ответ к вопросу';
var str = '';
var type = 0;
var option = 0;
var opened = 0;
var section = 2;
var regex_email = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
var name = "Имя";
var email = "Email";


// questionForm

function showAdditional()
{
    if ($('#questionForm #topic').val() == str1)
    {
        $('#additional_question_form').show();
        $('#questionForm #button').attr('disabled',false);
    }
}

function clearQuestionForm()
{
