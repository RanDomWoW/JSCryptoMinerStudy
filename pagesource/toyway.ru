<script>
$(document).ready(function(){
    resizeCenterBannersToMainPage();
})
$(window).resize(function(){
    resizeCenterBannersToMainPage();
})
function resizeCenterBannersToMainPage() {
    var arrSize = [];
    $('[data-image-width]').each(function(i,elem) {
        arrSize.push($(this).data("imageWidth"));
    });
    //console.log (arrSize);
    var sizeScreen = parseFloat($(".main_bottom .main_banners").css("width"));
    var summ = 0; var count = 0; var start = 0;
    jQuery.each(arrSize, function( i, val ) {
        summ = summ + (val + 6);
        count = count + 1;
        if (sizeScreen <= 1024) {
            if (sizeScreen <= summ){    
                for (var i=start; i<count; i++){
                    var tempWidth = (arrSize[i]*100)/summ; //В %
                    $('[data-image-width]').eq(i).css("width",(((tempWidth*sizeScreen)/100)-6) + "px"); //В px
                }
                start = count;
                summ = 0;
            }
        } else {
            $('[data-image-width]').children("img").css("width","100%"); //В px
            $('[data-image-width]').css("width","auto"); //В px
        }
    })
}
</script>		
