                                                <script type="text/template" id="tpl-match-ticker-row" data-type="desktop">
    <div class="matchticker__item">
        <div class="match match--desktop">
            <header class="match__title">
                <a href="<%-match.stage.uri%>" class="flex-box align-items--stretch inverse-color--black-01">
                    <div class="align-self--flex-end match__icon">
                        <i class="icon-games icon-games--15 icon-games--<%-match.discipline.slug%>"></i>
                    </div>
                    <div class="align-self--flex-end match__name">
                        <strong class="text-overflow"><%-match.reportage.title%></strong>
                                            </div>
                </a>
            </header>
            <section class="flex-box align-items--stretch match__inner">
                <div class="align-self--center match__teams">
                    <a href="<%-match.uri%>" class="match__link-in-teams"></a>
                    <div class="team team--first">
                        <% if (match.team1) { %>
                            <a href="<%-match.team1.uri%>" class="inverse-color--black-01 team__name">
                                <%-match.team1.abbr.trim() ? match.team1.abbr : match.team1.title %>
                            </a>
                        <% } %>
                        <% if (match.player1) { %>
                            <a href="<%-match.player1.uri%>" class="inverse-color--black-01 team__name">
                                <%-match.player1.nickname%>
                            </a>
                        <% } %>
                    </div>
                    <div class="team team--second team--looser">
                        <% if (match.team2) { %>
                            <a href="<%-match.team2.uri%>" class="inverse-color--black-01 team__name">
                                <%-match.team2.abbr.trim() ? match.team2.abbr : match.team2.title %>
                            </a>
                        <% } %>
                        <% if (match.player2) { %>
                            <a href="<%-match.player2.uri%>" class="inverse-color--black-01 team__name">
                                <%-match.player2.nickname%>
                            </a>
                        <% } %>
                    </div>
                </div>
                <aside class="align-self--center match__info" onclick="window.location='<%-match.uri%>'" style="cursor: pointer">
                    <% if (match.status == 'past' || (match.status == 'active' && (match.score1 || match.score2))) { %>
                        <div><%-match.score1%></div>
                        <div><%-match.score2%></div>
                    <% } %>
                </aside>
            </section>
            <div class="match__meta">
                <% if (match.status == 'future') { %>
                <time datetime="<%-moment.unix(match.timestamp).format('YYYY-MM-DD') %>">
                    <%-moment.unix(match.timestamp).format('D MMM') %>
                    <%-moment.unix(match.timestamp).format('HH:mm') %>
                </time>
                <% } %>
                <% if (match.status == 'past') { %>
                <p class="end color--gray-01">Завершен</p>
                <% } %>
                <% if (match.status == 'active') { %>
                <p class="online online--live color--red-02">
                    <span>Live</span>
                    <% if (match.stream) { %>
                        <svg viewBox="0 0 50 50" class="icon-play icon--20">
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-play" class="icon-fill--red-02"></use>
                        </svg>
                    <% } %>
                </p>
                <% } %>
            </div>
        </div>
        <div class="matchticker__links">
            <a class="inverse-color--black-01" href="<%-match.stage.uri%>">
                <span>Турнир</span>
            </a>
            <a class="inverse-color--black-01" href="<%-match.uri%>">
                <span>Матч</span>
            </a>
        </div>
    </div>
</script>

