<script type="text/javascript">
try{
    SLI = window.SLI || {};
    SLI.searchSubmit = function(event, caller) {
        event.preventDefault ? event.preventDefault() : event.returnValue = false;
        var keyword = '';
        var target = event.target;
        if (caller.nodeName.match(/form/i)) {
            keyword = target.w.value;
        } else if (caller.nodeName.match(/input/i)) {
            keyword = target.form.w.value;
        } else {
            var parent = target.parentElement;
            while (parent && !parent.nodeName.match(/html/i)) {
                var input = parent.querySelector('input[name="w"]');
                if (!input) {
                    parent = parent.parentElement;
                    continue;
                }
                keyword = input.value;
                break;
            }
        }
        keyword = keyword.replace(/^\s+|\s+$/g, '');
        if (keyword == '') {
            return false;
        }
        keyword = encodeURIComponent(keyword);
        window.location = location.protocol+"//shop.mizunousa.com/search?ts=custom&w=" + keyword;
        return false;
    };
} catch(error){
	console.log("Error creating SLI searchSubmit function");
}
</script>
