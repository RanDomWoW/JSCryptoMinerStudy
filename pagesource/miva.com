<script type="text/javascript">
// event listener for prompt text click
jQuery(document).ready(function() {
jQuery('#ecPromptText').on("click", function(e) {
e.preventDefault();
pmOpenContact();
});
});
// function to show form/overlay and hide prompt
function pmOpenContact() {
var timestart = Math.round(new Date().getTime() / 1000);
jQuery('#pmGo').val(timestart);
jQuery('#ecContact').css("display", "block");
jQuery('#ecPromptLocation').css("display", "none");
jQuery('#ecOverlay').css("display", "block");
}
// event listener for send contact button-click
jQuery(document).ready(function() {
if ( jQuery('#ecSendContact') ) {
jQuery('#ecSendContact').on("click", function(e) {
validateMessage();
validateEmail();
var isError = false;
var errorMessage = '';
if ($('#ecContactMessage').hasClass("invalid")) {
errorMessage += 'Message missing. ';
isError = true;
}
if ($('#ecContactEmail').hasClass("invalid")) {
errorMessage += 'Email missing/invalid. ';
isError = true;
}
if (isError){
$('#ecShowError').html(errorMessage);
e.preventDefault(); }
else{
$('#ecShowError').html('');
pmSendMessage();
}	});
}
});
// function to send message
function pmSendMessage() {
var timestop = Math.round(new Date().getTime() / 1000);
jQuery('#pmStop').val(timestop);
jQuery('#ecContact').css("display", "block");
jQuery('#ecOverlay').css("display", "block");
jQuery("#ecContactForm").submit();
}
// event listener for contact form submit
jQuery(document).ready(function() {
jQuery('#ecContactForm').on("submit", function(e) {
jQuery('#ecContact').css("display", "none");
});
});
// event listener for close contact link-click
jQuery(document).ready(function() {
jQuery('#ecCloseContact').on("click", function(e) {
pmCloseContact();
});
});
// event listener for close button-click
jQuery(document).ready(function() {
jQuery('#ecClose').on("click", function(e) {
pmCloseContact();
});
});
// function to 'close' Contact window
function pmCloseContact() {
jQuery('#ecContact').css("display", "none");
jQuery('#ecPromptLocation').css("display", "block");
jQuery('#ecOverlay').css("display", "none");
return false;
}
// validate email field
function validateEmail() {
var input = jQuery('#ecContactEmail');
var re = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/;
var is_email = re.test(input.val());
if (is_email) {
input.removeClass("invalid");
}
else {
input.addClass("invalid");
}
}
// validate message field
function validateMessage() {
var input = jQuery('#ecContactMessage');
var is_message = input.val();
if (is_message) {
input.removeClass("invalid");
}
else {
input.addClass("invalid");
}
}
</script>
