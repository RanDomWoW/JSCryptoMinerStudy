    var x=$('input[name=slide_image]:checked').val();
    if(x=="1")
    {
        x=number;
      
    }
    else{
        x--;
    }
    
    $("#cbx_"+x).prop("checked", true); 
    $("#slt_slide").find("span.active").removeClass('active');
    $("#s_"+x).addClass('active');
    //var y=(x-1)*10;
    load_slide_ajax(x);
});
function load_slide_ajax(id)
{
    id=id-1;
    $.ajax({
            url:"Pages/load_slide",
            type:"POST",
            data: {
				id: id,arrId:arrId
			},
			success:function(data){
			 if($.trim(data)!="NO")
             {
               $('#slideshow').html(data);	
             }
            } 
        });
}
function check_slide(id){
    $("#slt_slide").find("span.active").removeClass('active');
    $("#s_"+id).addClass('active');
  //  var y=(id-1)*10;
    load_slide_ajax(id);
}

