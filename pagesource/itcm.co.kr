<script>
		/* scrollUp Minimum setup */
	jQuery(function($){
			$.scrollUp();
		});
	/* 헤더 스크롤 */
	jQuery(function($){
		var $bh = $(".bottom_header").clone(),
			hd_top = $(".bottom_header").offset().top,
			scrollTop = prev_scrollTop = 0;
		$bh.appendTo($(".in_header"));
		var $rb = $(".fix_right .fix_banner"),
			rb_top = $rb.offset() ? $rb.offset().top : null;
		var $lb = $(".fix_left .fix_banner"),
			lb_top = $lb.offset() ? $lb.offset().top : null;
		
		$(document).ready(function(){
			hd_top += $(".wrap_header_banner:visible").length ? $(".wrap_header_banner").height() : 0;
		});
			
		$(window).scroll(function(){
			scrollTop = $(this).scrollTop();
			
			if(scrollTop > hd_top)
			{
				if(scrollTop < prev_scrollTop)
				{
					$bh.css({
						"display" : "block",
						"position" : "fixed",
					    "width" : "100%",
					    "z-index" : "998",
					    "background" : "#252229",
					    "top" : 0
					});
				}
				else if(scrollTop != prev_scrollTop) /* ie11 scroll same bug -_- .. */
				{
					$bh.css({
						"display" : "none"
					});
				}
			}
			else
			{
				$bh.css({
						"display" : "none"
					});
			}
			if((scrollTop) > (rb_top-92))
			{
				$rb.css({
					"position" : "fixed",
					"top" : "50px"
				});
			}
			else
			{
				$rb.css({
					"position" : "absolute",
					"top" : 0
				});
			}
			if(scrollTop > (lb_top-92))
			{
				$lb.css({
					"position" : "fixed",
					"top" : "50px"
				});
			}
			else
			{
				$lb.css({
					"position" : "absolute",
					"top" : 0
				});
			}
			prev_scrollTop = scrollTop;
		});
		$(window).scroll();
	});
	</script>
