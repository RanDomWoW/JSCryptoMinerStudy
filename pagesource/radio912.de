<script type="text/javascript">
  
  var newsplayer = function(id) {
    
    var make_timer = function(n) {
      var minutes = Math.floor(n/60);
      var seconds = Math.round(n-(minutes*60));
      if ( minutes < 10 ) {
        minutes = "0"+minutes;
      }
      if ( seconds < 10 ) {
        seconds = "0"+seconds;
      }
      return minutes+":"+seconds;
    }

    var playercontainer = jQuery("#"+id);
    var player = document.getElementById(id+"player");
    var zeit = playercontainer.find("DIV.zeit:eq(0)");
    var stop = playercontainer.find("A.stopbutton:eq(0)");
    var play = playercontainer.find("A.nachrichtenhoeren:eq(0)");
    var dauer = 0;
    var playing = false;
        
    window.setInterval(function(){
      if ( dauer == 0 ) {
        dauerneu = document.getElementById(id+"player").duration;
        if ( !isNaN(dauerneu) ) {
          dauer = dauerneu;
          zeit.removeClass("loading");          
        }
      }
      zeit.html(dauer==0?"":make_timer(dauer-player.currentTime));
    },1000);
    
    play.click(function(){
      if ( playing === false ) {
        player.play();
        if ( dauer == 0 ) { zeit.addClass("loading"); }
        play.addClass("pausebutton");
        playing = true;
        zeit.html(dauer==0?"":make_timer(dauer-player.currentTime));  
      } else {
        player.pause();
        play.removeClass("pausebutton");
        playing = false;
      }
      return false;
    });
    
    stop.click(function(){
        playing = false;
        play.removeClass("pausebutton");
        player.pause();
        if ( player.currentTime != 0 ) { player.currentTime = 0; }
        zeit.html(dauer==0?"":make_timer(dauer));  
        zeit.removeClass("loading");          
        return false;
    });
    
  }
  
  newsplayer("nrwnews");
  newsplayer("lokalnews");
  
</script><div id="aktuellertrack">
