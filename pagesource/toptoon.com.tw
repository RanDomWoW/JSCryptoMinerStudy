var g_this;
function createBannerSlider(element){
    
    var _this = this;
    g_this = _this;
    
    this.animating = false;
    
    this.init = function(){
        this.el = document.querySelector(element);
        this.pagination = $(_this.el).next().find('ul');        

            $(_this.el).children().each(function(){
                var index = this.getAttribute('data-index');
                $(_this.pagination).append('<li data-index="'+index+'"></li>');
            });
            
        _this.bind();
        _this.setBackground();
        _this.interval = setInterval(function(){
            _this.next('r',1);
        }, 3000);
        _this.alignArrows();
    }
    this.bind = function(){
        $(_this.el).prev().find('.prev').on('click',function(){
            _this.resetInterval();
            _this.prev();
        });
        $(_this.el).prev().find('.next').on('click',function(){
            _this.resetInterval();
            _this.next();
        });
        $(_this.pagination).on('click','li',function(){
            var index = parseInt(this.getAttribute('data-index'));
            var currPosition = parseInt(_this.pagination.find('.active').index());
            var gap = currPosition - index;

            if(gap>0)   _this.setDisplay('l',gap);
            else if(gap<0)  _this.setDisplay('r',Math.abs(gap));
        });
        $(_this.el).parent().parent().on('mouseenter',function(){
            _this.resetInterval();
        });
        $(_this.el).parent().parent().on('mouseleave',function(){
            _this.interval = setInterval(function(){
                _this.next('r',1);
            }, 3000);
        });
        $(window).resize(function(){
            _this.alignArrows();
        });
    }
    this.resetInterval = function() {
        clearInterval(_this.interval);
    }
    this.alignArrows = function(){
        if($(window).width()<$(_this.el).width())
        {
            $(_this.el).prev().width(Math.min(1700,$(window).width()));
        }
    }
