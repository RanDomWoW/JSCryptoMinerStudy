        }
    }
    this.setBackground = function() {
        var midValue = this.getMidValue();
        
        
        var bg = $(_this.el).children().eq(2).attr('data-bg-src');
        var bg_url = $(_this.el).children().eq(2).attr('data-url');
        
        $('.list_center').fadeOut(200, function() {
            $('.list_center').attr('src',bg);
            $('.list_center').css('width','');
        	$('.list_center').css('height','');
        }).fadeIn(200);
        
        $('.list_center_url').attr('href',bg_url);
        //var bg = $(_this.el).children().eq(2).attr('data-bg-src');
        //$(_this.el).parent().parent().css('background-image','url('+bg+')');
        $(_this.pagination).children().removeClass('active');
        var currPosition = $(_this.el).children().eq(2).attr('data-index');
        $(_this.pagination).children().eq(currPosition).addClass('active');

    }
    this.getMidValue = function(){
        //return (Math.round($(_this.el).children().length/2) - 1);
        //alert(Math.round($(_this.el).children().length));
        return (Math.round($(_this.el).children().length/3) - 1);
    }
    this.prev = function(){
        _this.setDisplay('l',1);
    }
    this.next = function(){
    	//_this.children().eq(2).css('opacity','1');
        _this.setDisplay('r',1);
    }

    if(this.el===null)  return false;
    else this.init();
}

