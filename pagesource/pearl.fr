            if(isColorAttribute || isImageAttribute){
                %><div class="variation_bloc"><span class="<%= variationsClass %>_variations variations"><%
                    _.each(variableAttribute.variations, function(variation,key,list){

                        var href  = !_.isUndefined(variation.targetArticle) ? 'href="' + Routing.generate('pearl_front_article_page_slug', { arcod: variation.targetArticle.arcod, slug: variation.targetArticle.slug }) + '"' : "";
                        var style = isColorAttribute ? 'style="background-color:' + variation.color + ';"' : '';
                        var title = isTextAttribute ? 'title="' + variation.label + '"' : '';
                    %><a <%= style %><%= href %><%= title %>><% if(isTextAttribute){ %><%= variation.label %><% } else if(isImageAttribute && !_.isUndefined(variation.targetArticle)){ %><% var image = !_.isUndefined(variation.targetArticle.image) ? variation.targetArticle.image : 'https://content.pearl.fr/media/cache/default/article_teeny_tiny_high_nocrop/bundles/pearlfront/images/general/not_available.png' %><img src="<%= image %>" alt="<%= variation.targetArticle.name %>"/><% } %></a><%
                    });
                    %></span></div><%
            }
            %><%
        }
        %></div></script><script type="text/template" id="js-cart-item"><div class="cart_item" data-arcod="<%= item.article.arcod %>"><div class="row"><a class="picture_container" href="<%= Routing.generate('pearl_front_article_page_slug', {arcod: item.article.arcod, slug: item.article.slug}) %>"><img class="picture" src="<%= item.article.image %>" alt="<%= item.article.cart_name %>" /></a><span class="info"><span class="general"><a class="title" href="<%= Routing.generate('pearl_front_article_page_slug', {arcod: item.article.arcod, slug: item.article.slug}) %>"><%= item.article.arcod %> - <%= item.article.cart_name %></a><a class="delete confirm-action" href="<%= Routing.generate('pearl_cart_item_remove', {id: item.id}) %>" data-confirm-message="Voulez-vous vraiment retirer cet article ?"><i class="fa fa-trash"></i> Supprimer</a><span class="pipe delete">|</span><a class="favorite favorite-trigger" data-arcod="<%= item.article.arcod %>" href="<%= Routing.generate('pearl_front_article_page_slug', {arcod: item.article.arcod, slug: item.article.slug}) %>"><i class="fa fa-heart-o"></i> Ajouter aux favoris
                    </a></span><span class="stock stock_state <%= item.article.stock.status %>"><%= item.article.stock.text %></span></span><div class="quantity_calc"><div class="row"><span class="unit_price"><%= item.unit_price %><span class="currency">&nbsp;<%= item.article.price.currency.symbol %></span></span><span class="quantity"><div><input type="number" name="<%= form_name %>[items][<%= index %>][quantity]" required="required" novalidate="novalidate" <% if(item.article.maxOrderableQuantity == 1){ %>disabled="disabled"<% } %> class="quantity-spinner" size="3" min="1" max="99" step="1" value="<%= item.quantity %>"></div></span><span class="total"><span><%= item.total %></span><span class="currency">&nbsp;<%= item.article.price.currency.symbol %></span></span></div></div></div><% if(item.adjustments.length > 0){ %><% _.each(item.adjustments, function(adjustment, index, list){ %><div class="row item_adjustment"><span class="info"><span class="general"><%= adjustment.description %></span></span><div class="quantity_calc"><div class="row"><span class="total"><span><%= adjustment.amount %></span><span class="currency">&nbsp;<%= item.article.price.currency.symbol %></span></span></div></div></div><% }); %><% } %></div></script><script type="text/template" id="js-stars"><span class="stars"><%

            var starRow = '';

            for(i = 1; i <= 5; i++) {

                var starClass = 'fa fa-star-o';

                if(score >= i){
                    starClass = 'fa fa-star active';
                }
                else if(score > i-1){
                    starClass = score > i-0.5 ? 'fa fa-star active' : 'fa fa-star-half-o active';
                }

                starRow += '<i class="'+starClass+'"></i>';

            }

        %><%= starRow %></span></script><script type="text/template" id="js-cart-detail"><div class="cart_detail_header"><% _.each(cart.items, function(item, index, list){ %><div class="cart_detail_row"><div class="detail_picture"><a href="<%= Routing.generate('pearl_front_article_page_slug', {arcod: item.article.arcod, slug: item.article.slug}) %>"><img src="<%= item.article.image %>" alt="<%= item.article.cart_name %>"/></a></div><div class="detail_infos"><a href="<%= Routing.generate('pearl_front_article_page_slug', {arcod: item.article.arcod, slug: item.article.slug}) %>"><%= item.article.cart_name.length > 30 ? item.article.cart_name.substr(0, 27) + '...' : item.article.cart_name %></a><br />
                    
                    Qté : <%= item.quantity %><br /><%= item.total %>&nbsp;<%= cart.currency.symbol %></div><div class="detail_remove"><a href="<%= Routing.generate('pearl_cart_item_remove', {id: item.id}) %>" class="button with-icon simple"><i class="icon-cross small"></i></a></div></div><% }); %><div class="cart_detail_resume"><div class="line bigger bold"><span class="fl">Total TTC</span><span class="fr txt-red" ><span class="cart_detail_items_total" rv-text="cart.items_total"><%= cart.items_total %></span>&nbsp;<%= cart.currency.symbol %></span></div><div class="line mts"><a href="/panier" class="button with-icon green medium block"><i class="icon-panier big fl"></i><span class="small uppercase">Accéder au panier</span></a></div></div></div></script><script type="text/template" id="js-account-tooltip"><div class="tooltip_account_container"><div class="pas"><div class="title">Déjà client</div><form name="form" method="post" action="/connexion_check"><div><label for="_username" class="required">Votre adresse email</label><input type="text" id="_username" name="_username" required="required" autocomplete="off"></div><div class="mbs"><label for="_password" class="required">Votre mot de passe</label><input type="password" id="_password" name="_password" required="required"></div><div class="txtright"><a class="underline" href="/mot-de-passe-oublie/?track%5Bresetting_place%5D=classic">Mot de passe oublié ?</a></div><input type="hidden" id="_target_path" name="_target_path" value="pearl_front_account_index" /><div class="mts center w80 medium-w100 small-w100 tiny-w100"><button type="submit" class="button with-icon green medium block"><i class="icon-cadenas_plein big fl"></i><span class="uppercase">Se connecter</span></button></div></form></div><hr class="light mts mbn"/><div class="pas"><div class="title">Nouveau client</div><div class="mts center w80 medium-w100 small-w100 tiny-w100"><a href="/connexion" class="button with-icon green medium block"><i class="icon-compte big fl"></i><span class="uppercase">Créer un compte</span></a></div></div><hr class="light mts mbn"/><div class="pas"><p><a href="/compte/commandes"><i class="icon-chevron-thick-right txt-pearl_blue"></i> Suivi de commandes</a></p><p><a href="/favoris/"><i class="icon-chevron-thick-right txt-pearl_blue"></i> Vos favoris</a></p></div></div></script>


