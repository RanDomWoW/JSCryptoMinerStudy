    <script language="javascript">
	// Restituisce un Oggetto Ajax in modalità  cross browser
        function GetAjObject() {
            var oAjax = null;
            var UserBrowser = navigator.userAgent.toUpperCase();

            if (typeof (XMLHttpRequest) === "function" || typeof (XMLHttpRequest) === "object") {
                oAjax = new XMLHttpRequest();
            } else if (window.ActiveXObject && UserBrowser.indexOf("MSIE 4") < 0) {
                if (UserBrowser.indexOf("MSIE 5") < 0) {
                    oAjax = new ActiveXObject("Msxml2.XMLHTTP");
                } else {
                    oAjax = new ActiveXObject("Microsoft.XMLHTTP");
                }
            }
            
          
            return oAjax;
        }

        // Restituisce un TimeStamp: server per evitare che il Browser restituisca la risposta precedente in cash
        // Cambiando ad ogni GET il Browser interpreta ogni chiamata come nuova e forza sul Server
        function GetTimeStamp() {
            var Digital = new Date();
            var year = Digital.getFullYear();	
			var month = Digital.getMonth();
            var day = Digital.getDay();
            var hours = Digital.getHours();
            var minutes = Digital.getMinutes();
            var seconds = Digital.getSeconds();
            var ReturnValue = year + '-' + month + '-' + day + '-' + hours + '-' + minutes + '-' + seconds;
            return ReturnValue;
        }
        var page = 0;
        
        function closeMe(){
          page = 0;
          document.getElementById('AltreNotizie').innerHTML = "";
          document.getElementById('ricerca').focus();
          document.getElementById('BtnChiudi').innerHTML = "Altre Notizie";
          document.getElementById('linkChiudi').style.visibility = "collapse";
        }
        
        function GetNotizieList(Path) {
 
               
            /*if(document.getElementById('NotizieStorico').innerHTML == "Mostra Meno Notizie"){
             document.getElementById('AltreNotizie').innerHTML = "";
             page = 0;
             document.getElementById('NotizieStorico').innerHTML = "Mostra Altre Notizie"
             document.getElementById('ricerca').focus();
             document.getElementById("freccia").className = "fa fa-caret-down";
            }
            */
            
            //else{
           
            if(Path>0){
            page++;
            }
            else{
            if ((page-1)<= 0){ 
              page = 1; 
              }
            else{
            page--;
            }
            
            }
            document.getElementById('BtnChiudi').innerHTML = "<em class='fa fa-minus' id='meno'></em>";
            document.getElementById('linkChiudi').style.visibility = "visible";
            // Ottengo l'Oggeto Ajax
            var oAjax = GetAjObject();

            var URL = window.location.toString()
            
         
            if (URL.indexOf('?') > -1) { URL = URL.substring(0, URL.indexOf('?')) }
            URL = URL + '?Op=GetNotizieList&Path=' + Path + '&page='+ page +'&TimeStamp=' + GetTimeStamp();
            
            // Preparo la funzione destinata a gestire l'evento di ricezione
            oAjax.onreadystatechange = function() {
                if (oAjax.readyState == 4) {
                    if (oAjax.status == 200) {
                    
                       
                        if(oAjax.responseText!=""){
                        document.getElementById('AltreNotizie').innerHTML += oAjax.responseText;
                        }
                        else{
                       // document.getElementById('NotizieStorico').innerHTML = "Mostra Meno Notizie";
                       // document.getElementById("freccia").className = "fa fa-caret-up";
                       // page = 0;
                            closeMe();
                        }
                        
                    }
                }
            }

            oAjax.open('get', URL, true);
            oAjax.send(null);
            
           //}
        }
    
        
        function GetNotizieMove(Path) {

            // Ottengo l'Oggeto Ajax
            var oAjax = GetAjObject();
            
            if(Path>0){
            page++;
            }
            else{
            if ((page-1)<= 0){ 
              page = 1; 
              }
            else{
            page--;
            }
            
            }
         
            
            
            var URL = window.location.toString()
            if (URL.indexOf('?') > -1) { URL = URL.substring(0, URL.indexOf('?')) }
            URL = URL + '?Op=GetNotizieMove&Path=' + Path + '&page='+page+'&TimeStamp=' + GetTimeStamp() ;
          
           
               oAjax.onreadystatechange = function() {
                if (oAjax.readyState == 4) {
                    if (oAjax.status == 200) {
                        document.getElementById('AltreNotizie').innerHTML = oAjax.responseText;
                       
                              
                    }
                }
            }

            oAjax.open('get', URL, true);
            oAjax.send(null);
        }
	</script> 
