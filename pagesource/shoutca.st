        $("label.show-client-plans").addClass("active");
        $("label.show-reseller-plans").removeClass("active");
        $("#plans-slider").carousel(0);
    });
    $(document).on("click", ".show-reseller-plans", function() {
        $("label.show-reseller-plans").addClass("active");
        $("label.show-client-plans").removeClass("active");
        $("#plans-slider").carousel(1);
    });
    $('#plans-slider').carousel().on('slide.bs.carousel', function (e) {
        var nextH = $(e.relatedTarget).height();
        $(this).find('.active').parent().animate({ height: nextH }, 650);
    });

    var planPricesInGBP = {
        clientFree: 0,
        clientStandard: 2,
        clientPremium: 3,
        clientPro: 5,
        resellerStarter: 5,
        resellerStandard: 10,
        resellerPremium: 20,
        resellerPro: 30,
    };
    var getDecimalPart = function (number) {
        return (number - Math.floor(number)).toFixed(2).slice(2);
    };
    var updatePrices = function (currency, rate) {
        for (var plan in planPricesInGBP) {
            if (!planPricesInGBP.hasOwnProperty(plan)) {
                return;
            }
            updateDom(plan, currency, planPricesInGBP[plan] * rate);
        }
        $(".change-currency").removeClass("active");
        $(".change-currency[data-target-currency='" + currency + "']").addClass("active");
    };
    var updateDom = function (plan, currency, price) {
        var whole = $(".plan-" + plan + ".price span");
        whole.attr("style", "");
        whole.text(currency + Math.floor(price));
        if (Math.floor(price).toString().length > 1) {
            whole.attr("style", "font-size: 36px;");
        }
        var decimal = getDecimalPart(price);
        var separator = ".";
        if (currency === "€") {
            separator = ",";
        }
        if (decimal.length === 1) {
            decimal = decimal + "0";
        }
        if (decimal === "00") {
            decimal = "";
            separator = "";
        }
        $(".plan-" + plan + ".price small").text(separator + decimal);
    };
    var changeCurrency = function (currency) {
        $.ajax({
            cache: true,
            url: "https://itframe.innovatete.ch/internal/get-currency-rate/" + currency,
            dataType: "json",
            success: function (data) {
                updatePrices(currency, data.rate);
            },
        });
    };
    $(document).on("click", ".change-currency", function () {
        var currency = $(this).data("target-currency");
        changeCurrency(currency);
    });
    $.ajax({
        url: "https://itframe.innovatete.ch/intern/geoip/location-for-me",
        success: function (response) {
            if (!response.countryCode) {
                return changeCurrency("$");
            }
            var eurCountries = ['AL', 'AD', 'AM', 'AT', 'BY', 'BE', 'BA', 'BG', 'CH', 'CY', 'CZ',
                'DE', 'DK', 'EE', 'ES', 'FO', 'FI', 'FR', 'GE', 'GI', 'GR', 'HU', 'HR',
                'IE', 'IS', 'IT', 'LT', 'LU', 'LV', 'MC', 'MK', 'MT', 'NO', 'NL', 'PL',
                'PT', 'RO', 'RU', 'SE', 'SI', 'SK', 'SM', 'TR', 'UA', 'VA'];
            if (eurCountries.indexOf(response.countryCode) !== -1) {
                return changeCurrency("€");
            }
            if (response.countryCode === "GB") {
                return changeCurrency("£");
            }
            changeCurrency("$");
        },
        error: function () {
            changeCurrency("$");
        },
        dataType: "json",
    });
});})</script>
