				}
				var $ps_albums 		= $('#ps_albums');
				var elems			= $ps_albums.children().length;
				
				var $ps_container 	= jQuery('#ps_container');
				var $ps_overlay 	= jQuery('#ps_overlay');
				var $ps_close		= jQuery('#ps_close');
				
				$('#ps_albums div a#Preview').bind('click',function(){
				
					var $elem = $('#ps_albums div');
					var eventId = $(this).attr("rel");
					
					var $loading 	= jQuery('<div />',{className:'loading'});
					$elem.append($loading);
					$ps_container.find('img').remove();
					
					$.ajax({
					//url: "photostack.php",
					url:"albumview.php",
					data:{eventid:eventId} , 
					dataType:"json",
					
					success:function(data) {
					
					function simple_tooltip(target_items, name){
						
 $(target_items).each(function(i){
		
		$("body").append("<div class='"+name+"' id='"+name+i+"'><p>"+$(this).attr('title')+"</p></div>");
		var my_tooltip = $("#"+name+i);

		$(this).removeAttr("title").mouseover(function(){
				my_tooltip.css({opacity:0.8, display:"none"}).fadeIn(400);
		}).mousemove(function(kmouse){
				
				my_tooltip.css({left:kmouse.pageX+15, top:kmouse.pageY+15});
		}).mouseout(function(){
				my_tooltip.fadeOut(400);
		});
	});
}
					
					var items_count	= data.length;
					
					//$ps_container.append("<div class='capcontainer'>");
						for(var i = 0; i < items_count; ++i){
    						var datasource=data[i].split("[[]]");
							
							var item_source = datasource[0];
							var imgtitle = datasource[1];
							
							
							var cnt 		= 0;
							
							$("<img />").load(function(){
								var $image = jQuery(this);
								++cnt;
								resizeCenterImage($image,datasource[1]);
								$ps_container.append($image);
								var r = Math.floor(Math.random()*41)-20;
								if(cnt < items_count){
									$image.css({
										'-moz-transform'	:'rotate('+r+'deg)',
										'-webkit-transform'	:'rotate('+r+'deg)',
										'transform'			:'rotate('+r+'deg)'
									});
								}
								
								if(cnt == items_count){
									$ps_container.append("</div>");
									ajustimagePosition($ps_container);
									//$loading.remove();
									$ps_container.show();
									$ps_close.show();
									$ps_overlay.show();
									
								}
							}).attr({'src':item_source,'title':imgtitle,'class':'someClass1'});
						}
						
						//$(".someClass1").tipTip({maxWidth: "250", edgeOffset:0});
					},
					error: function(x,y,z){
								//alert('An error has occurred:\n' + x + '\n' + y + '\n' + z);
						}
					});
				});
				
				function ajustimagePosition($objContainer)
				{
					var maskHeight = jQuery(document).height();
					var maskWidth = jQuery(document).width();
					var scrollpos =jQuery(window).scrollTop();
					var scrollposleft =jQuery(window).scrollLeft();
					var windowHeight = jQuery(window).height();
					var windowWidth = jQuery(window).width();
					var dialogHeight = jQuery($objContainer).height();
					var dialogTop =  (scrollpos+(windowHeight/2)) - (jQuery('#dialog-box').height()/2);
					
					jQuery($objContainer).stop().animate({
						top:dialogTop+'px'},'fast');
				}
				
				$ps_container.live('mouseenter',function(){
					imgCount = $("#ps_container img").length;
					if(imgCount==1)
						jQuery.preventDefault();
					$('#ps_next_photo').show();

				}).live('mouseleave',function(){
					$('#ps_next_photo').hide();
				});
				
				
				jQuery(window).scroll(function () {
						ajustimagePosition($ps_container);		
				});
				$('#ps_next_photo').bind('click',function(){
					var $current 	= $ps_container.find('img:last');
					var r			= Math.floor(Math.random()*41)-20;
					//alert(r);
					var currentPositions = {
						marginLeft	: $current.css('margin-left'),
						marginTop	: $current.css('margin-top')
					}
					var $new_current = $current.prev();
					$($current).stop(true,true).animate({
						'marginLeft':'250px',
						'marginTop':'-385px'
					},50,function(){
						$(this).insertBefore($ps_container.find('img:first'))
							   .css({
									'-moz-transform'	:'rotate('+r+'deg)',
									'-webkit-transform'	:'rotate('+r+'deg)',
									'transform'			:'rotate('+r+'deg)'
								})
							   .animate({
									'marginLeft':currentPositions.marginLeft,
									'marginTop'	:currentPositions.marginTop
									},250,function(){
										$new_current.css({
											'-moz-transform'	:'rotate(0deg)',
											'-webkit-transform'	:'rotate(0deg)',
											'transform'			:'rotate(0deg)'
										});
							   });
					});
				});
				
				
				
				$('#ps_close').bind('click',function(){
					$ps_container.hide();
					$ps_container.next().nextAll().empty();
					$ps_close.hide();
					$ps_overlay.fadeOut(400);
				});
				
				function resizeCenterImage($image,$ImgTitle){
				//alert($image);
					var theImage 	= new Image();
					theImage.src 	= $image.attr("src");
					theImage.title 	= $image.attr("title");
					var imgwidth 	= theImage.width;
					var imgheight 	= theImage.height;
					
					var containerwidth  = 460;
					var containerheight = 330;

					
					if(imgwidth	> containerwidth){
						var newwidth = containerwidth;
						var ratio = imgwidth / containerwidth;
						var newheight = imgheight / ratio;
						if(newheight > containerheight){
							var newnewheight = containerheight;
							var newratio = newheight/containerheight;
							var newnewwidth =newwidth/newratio;
							theImage.width = newnewwidth;
							theImage.height= newnewheight;
						}
						else{
							theImage.width = newwidth;
							theImage.height= newheight;
						}
					}
					else if(imgheight > containerheight){
						var newheight = containerheight;
						var ratio = imgheight / containerheight;
						var newwidth = imgwidth / ratio;
						if(newwidth > containerwidth){
							var newnewwidth = containerwidth;
							var newratio = newwidth/containerwidth;
							var newnewheight =newheight/newratio;
							theImage.height = newnewheight;
							theImage.width= newnewwidth;
						}
						else{
							theImage.width = newwidth;
							theImage.height= newheight;
						}
					}
					$image.css({
						'width'			:theImage.width,
						'height'		:theImage.height,
						'margin-top'	:-(theImage.height/2)-10+'px',
						'margin-left'	:-(theImage.width/2)-10+'px'	
					});
				}
				 //simple_tooltip(".someClass1","tooltip");
				
},(jQuery));
</script>



