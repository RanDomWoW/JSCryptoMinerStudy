function getParameterByName(name) {
    name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
        results = regex.exec(location.search);
    return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}
var utmSource = getParameterByName('utm_source');
var utmMedium = getParameterByName('utm_medium');
var utmCampaign = getParameterByName('utm_campaign');
var utmTerm = getParameterByName('utm_term');
var utmContent = getParameterByName('utm_content');
var xuid = getParameterByName('xuid');
var gclid = getParameterByName('gclid');
if(utmSource != ""){
	Cookies.set('utm_source_cookie', utmSource, { expires: 90 });
	Cookies.set('utm_medium_cookie', utmMedium, { expires: 90 });
	Cookies.set('utm_campaign_cookie', utmCampaign, { expires: 90 });
	Cookies.set('utm_term_cookie', utmTerm, { expires: 90 });
	Cookies.set('utm_content_cookie', utmContent, { expires: 90 });
	Cookies.set('xuid_cookie', xuid, { expires: 90 });
	Cookies.set('gclid_cookie', gclid, { expires: 90 });
	Cookies.set('checkVisited_jtr', '1');
}
}
function contactValidate()
 {
	var frm = document.contactForm;
	var emailID=document.contactForm.email;
	var x = document.contactForm.pno;
	var sub_button = document.getElementById("sub_button");
    var submitting = document.getElementById("submitting");
	   if (GenValidation(frm.fullname,'Name','','Name') == 0)
		return false;
       if ((emailID.value==null)||(emailID.value=="")||(emailID.value=="Email"))
		{
		alert("Please enter valid 'Email ID'")
		emailID.focus()
		return false
		}
		if (echeck(emailID.value)==false)
		{
		emailID.value=""
		emailID.focus()
		return false
		}
		if ((x.value==null)||(x.value=="")||(x.value=="Contact No"))
		{
		alert("Please Enter Valid 'Contact Number'")
		x.focus()
		return false
		}
		if (validate(x.value)==false)
		{
		x.value=""
		x.focus()
		return false
		}		
        if (GenValidation(frm.city,'City','','City') == 0)
		return false;
		if (SelectValidation(frm.state,"'State'") == 0)
		return false;
		if (SelectValidation(frm.message,"'product for'") == 0)
		return false;
		if (GenValidation(frm.verificationcode,'Verification Code','','') == 0)
		return false;
		frm.utm_source.value = Cookies.get('utm_source_cookie');
		frm.utm_medium.value = Cookies.get('utm_medium_cookie');
		frm.utm_campaign.value = Cookies.get('utm_campaign_cookie');
		frm.utm_term.value = Cookies.get('utm_term_cookie');
		frm.utm_content.value = Cookies.get('utm_content_cookie');
		frm.xuid.value = Cookies.get('xuid_cookie');
		frm.gclid.value = Cookies.get('gclid_cookie');
		sub_button.style.display = "none";
        submitting.style.display = "block";
}
function validate(x)
{
        //var x = document.frm.pno.value;
       
        if (x==null || x=="")
 	 {
 		 alert("Phone no. cannot be left blank");
 		 return false;
 	 }       

       	 if(isNaN(x)|| x.indexOf(" ")!=-1)
	{
              			alert("Enter numeric value");
			return false;
                }
       			 if (x.length > 12)
			{
                			alert("enter 10 characters"); 
				return false;
          			 }
       
}

function echeck(str) 
{
		var at="@"
		var dot="."
		var lat=str.indexOf(at)
		var lstr=str.length
		var ldot=str.indexOf(dot)
		if (str.indexOf(at)==-1){
		   alert("Invalid E-mail ID")
		   return false
		}
		if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr){
		   alert("Invalid E-mail ID")
		   return false
		}
		if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr){
		    alert("Invalid E-mail ID")
		    return false
		}
		 if (str.indexOf(at,(lat+1))!=-1){
		    alert("Invalid E-mail ID")
		    return false
		 }
		 if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){
		    alert("Invalid E-mail ID")
		    return false
		 }
		 if (str.indexOf(dot,(lat+2))==-1){
		    alert("Invalid E-mail ID")
		    return false
		 }		
		 if (str.indexOf(" ")!=-1){
		    alert("Invalid E-mail ID")
		    return false
		 }
 		 return true				
	}
	
</script>
<script type="text/javascript">
$(document).ready(function() {	
//Captcha image reload	
  $('#reload').click(function(){
    $("#captcha").attr("src", "captcha.php?"+(new Date()).getTime());
  });
  // Floating box to top while scrolling down
	$(window).scroll(function(){
	  if($(this).scrollTop() + $("#layer1").innerHeight()<$(document).height() && $(window).scrollTop() > 550 && window.innerWidth >= 800 && window.innerHeight >= 600 && $(window).scrollTop() < $(".desc_layer").height()) {
	       $("#layer1")
	        .stop()
	        .animate({"marginTop": ($(window).scrollTop() - 550) + "px"}, "normal" );
	  } else if($(this).scrollTop() < 150) {
	    $("#layer1").stop().animate({"marginTop": "00px"}, "normal");
	  }
	  var scrollAmount = $(window).scrollTop();
		if(scrollAmount > 150) {
			$("#up-mover").show('300');		
		} else {
			$("#up-mover").hide('100');
		}
	});
	$("#up-mover").on('click tab', function(){
       $('html, body').animate({
		    scrollTop: $("body").offset().top
		}, 500);
    });
  
  $("a.vid").click(function() {
	$.fancybox({
			'padding'		: 0,
			'autoScale'		: false,
			'transitionIn'	: 'none',
			'transitionOut'	: 'none',
			'width'		    : 640,
			'height'		: 360,
			'title'			: null,
			'href'			: this.href.replace(new RegExp("watch\\?v=", "i"), 'v/') + '&autoplay=1',
			'type'			: 'swf',
			'swf'			: {
			'wmode'		: 'transparent',
			'allowfullscreen'	: 'true'
			},
			helpers: {
				overlay: {
					speedIn: 0,
					speedOut: 300,
					opacity: 0.98,
					css: {
						cursor: 'pointer'
					},
					closeClick: true
				}
			}
		});

	return false;
});
});
</script>
