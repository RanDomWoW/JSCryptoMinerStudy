	//김성철 슬라이드
	//이사님 요청사항 맞출려다보니 어쩔수없이 코드가 복잡해짐
	//페이지 로드시 첫 배너는 0번임
	//그이후로 1~10번이 계쏙 돌아감, 0번은 없어짐
	var ready 	= 0;
	var NowNum 	= 1;
	var MaxNum	= 0;
	var First	= 0;
	
	$(function(){
		MaxNum = $(".imgSet").length;
		
		if(MaxNum == 0){
			$("#main_banner").css("display","none");
			return false;
		}
		
		MaxNum = MaxNum - 1;	
		
		if(MaxNum == "0"){
			$("#main_banner").css("display","none");
		}
		else if(MaxNum == "1"){
			$("#left").css("display","none");
			$("#right").css("display","none");
			$(".btn").css("display","none");
		}
		else{
			$("#btnArea2").css("width",18 * MaxNum);
	
			for(var i=0; i<=MaxNum; i++){
				$("#btn" + i).css("display","block");
			}
			
			$("#right").click(function(){
				Sright(Number(NowNum) + 1);
			});
		   
			$("#left").click(function(){
				Sleft(Number(NowNum) - 1);
			});
			
			$(".btn").click(function(){
				var NextNum = $(this).attr("data-num");
				if(NowNum < NextNum){
					Sright(NextNum);
				}
				else if(NowNum > NextNum){
					Sleft(NextNum);
				}
			});
			
			var myVar = setInterval(function(){
				Sright(Number(NowNum) + 1);
			}, 10000);	//배너 스크롤링 초수 조정하는 곳
		}
	});
	
	function Sright(NextNum){
		$(".imgSet").css("z-index","1");
		
		if(ready == 0){
			ready = 1;
			
			if(NextNum > MaxNum){
				NextNum = 1;
			}
			
			$(".btn").attr("src","/images/2014_new/slide_over.png");
			$("#btn" + NextNum).attr("src","/images/2014_new/slide_on.png");
			
			//페이지 로드시 첫배너는 0번임
			//슬라이드 일어나면 첫배너는 1번부터 시작함
			if(First == 0){
				NowNum = 0;
			}
			
			$("#imgSet"+NowNum).css("z-index","2");
			$("#imgSet"+NextNum).css("display","block");
			$("#imgSet"+NowNum).fadeOut("slow", function() {
				NowNum 	= NextNum
				ready 	= 0;
				
				var FIdx = $("#imgSet" + NowNum).attr("data-FIdx");
				var BIdx = $("#imgSet" + NowNum).attr("data-BIdx");
				
				//window.open("/Library/Banner/Slide_Ajax.asp?FIdx="+FIdx+"&BIdx="+BIdx,"","");
				$.post("/Library/Banner/Slide_Ajax.asp", {FIdx:FIdx, BIdx:BIdx}, function(data){});
			});
			
			First = 1;
		}
	}
	
	function Sleft(NextNum){
		$(".imgSet").css("z-index","1");
		
		if(ready == 0){
			ready = 1;
			
			if(NextNum < 1){
				NextNum = MaxNum;
			}
			
			$(".btn").attr("src","/images/2014_new/slide_over.png");
			$("#btn" + NextNum).attr("src","/images/2014_new/slide_on.png");
			
			//페이지 로드시 첫배너는 0번임
			//슬라이드 일어나면 첫배너는 1번부터 시작함
			if(First == 0){
				NowNum = 0;
			}
			
			$("#imgSet"+NowNum).css("z-index","2");
			$("#imgSet"+NextNum).css("display","block");
			$("#imgSet"+NowNum).fadeOut("slow", function() {
				NowNum 	= NextNum
				ready 	= 0;
				
				var FIdx = $("#imgSet" + NowNum).attr("data-FIdx");
				var BIdx = $("#imgSet" + NowNum).attr("data-BIdx");
				
				//window.open("/Library/Banner/Slide_Ajax.asp?FIdx="+FIdx+"&BIdx="+BIdx,"","");
				$.post("/Library/Banner/Slide_Ajax.asp", {FIdx:FIdx, BIdx:BIdx}, function(data){});
			});
			
			First = 1;
		}
	}
	
