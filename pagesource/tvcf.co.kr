	//LinkOpen페이지 Ajax로 호출해서 카운트만 올리고 아무행동안하도록 조취
	function AjaxCount(FIdx, G_USER_ID, Link, Flag, BIdx){
		if(Flag == "open"){
			if(Link == "about:blank"){
				$.post("/Library/Banner/LinkOpen.asp", { FIdx:FIdx, G_USER_ID:G_USER_ID, BIdx:BIdx}, function(data){});
			}
			else{
				window.open("/Library/Banner/LinkOpen.asp?FIdx="+FIdx+"&G_USER_ID="+G_USER_ID+"&Link="+Link+"&BIdx"+BIdx,"area","");
			}
		}
		else{
			if(Link == "about:blank"){
				$.post("/Library/Banner/LinkOpen.asp", { FIdx:FIdx, G_USER_ID:G_USER_ID, BIdx:BIdx}, function(data){});
			}
			else{
				location.href="/Library/Banner/LinkOpen.asp?FIdx="+FIdx+"&G_USER_ID="+G_USER_ID+"&Link="+Link+"&BIdx"+BIdx;
			}
		}
	}
