<!-- 스폰서기업 배너 -->
<div id="bottomArea">
	<h3 class="hidden">스폰서기업 배너</h3>
	<script type="text/javascript">
	$(document).ready(function(){
		$("#sponsor").attr("motion", "easeOutBounce");
		$("#sponsor").attr("top", "0");
		$("#sponsor").attr("height", $("#sponsor>div").eq(0).height());
		$("#sponsor").attr("total", $("#sponsor>div").size());

		$("#btnSponsorSideToggle").bind("click", function() { 
			fnSponsorSlideToggle(); 
		});

		$("#sponsorArea").bind("mouseover", function() {
			var h = $("#sponsorArea").css("height").replace("px", "");
			if(h<100) {
				fnSponsorShow();
			}
		});

		$("#sponsor>div").eq(0).clone().appendTo($("#sponsor"));
		$("#sponsor").attr("timer", setInterval(sponsor_rotate, 3000));
	});


	function sponsor_rotate(){
		$("#btnSponsorSideToggle").unbind("click");
		$("#sponsorArea").unbind("mouseover");;

		if ($("#sponsor:animated").size()) return false;

		var height = $("#sponsor").attr("height");
		var pos = (parseInt($("#sponsor").attr("top"))+1);
		var px = pos * height;

		$("#sponsor").attr("top", pos);
		$("#sponsor").animate({ 
			top: ((px * -1) + "px")
		}, 1000, $("#sponsor").attr("motion"), 
			function() {
				var pos = parseInt($("#sponsor").attr("top"));
				var total = parseInt($("#sponsor").attr("total"));
				if (pos>=total) {
					$("#sponsor").attr("top", 0);
					$("#sponsor").css("top", "0px");
				}

				$("#btnSponsorSideToggle").bind("click", function() { 
					fnSponsorSlideToggle(); 
				});

				$("#sponsorArea").bind("mouseover", function() {
					var h = $("#sponsorArea").css("height").replace("px", "");
					if(h<150) {
						fnSponsorShow();
					}
				});

			}
		);


	}

	function fnSponsorSlideToggle() {
		var x = $("#btnSponsorSideToggle").attr("title");

		if(x == "펼치기") {
			fnSponsorShow();
		}else{
			fnSponsorHide();
		}
	}

	function fnSponsorShow() {
		$("#btnSponsorSideToggle").attr("title", "닫기");
		clearInterval($("#sponsor").attr("timer"));

		var DivCnt = $("#sponsor>div").length - 1;
		$("#sponsor>div").eq(DivCnt).remove();					//neko: 맨 마지막것 지우기

		var SponHeight = $("#SponCnt").val();
		
		$("#sponsorArea").css("height", SponHeight+"px");
		$("#sponsor").css("height", SponHeight+"px");
		$("#sponsor").css("top", "0px");

		$("#sponsorBtnArea").hide();
		
		$("#sponShow").css("display","block");
		$("#sponsorArea").css("display","none");
	}

	function fnSponsorHide() {
		$("#btnSponsorSideToggle").attr("title", "펼치기");

		$("#sponsor>div").eq(0).clone().appendTo($("#sponsor"));

		
		$("#sponsor").css("height", "100px");
		$("#sponsorArea").css("height", "105px");
		$("#sponsor").attr("top", "0");

		$("#sponsor").attr("timer", setInterval(sponsor_rotate, 3000));

		$("#sponsorBtnArea").show();
		
		$("#sponShow").css("display","none");
		$("#sponsorArea").css("display","block");
	}

	function fnSponsorSlideUp() {
		clearInterval($("#sponsor").attr("timer"));

		var idx = parseInt($("#sponsor").attr("top"));
		if(idx>0) {
			clearInterval($("#sponsor").attr("timer"));
			$("#sponsor").attr("top", idx-2);
			sponsor_rotate();
		}
		$("#sponsor").attr("timer", setInterval(sponsor_rotate, 3000));
	}

	function fnSponsorSlideDown() {
		clearInterval($("#sponsor").attr("timer"));
		
		var idx = parseInt($("#sponsor").attr("top"));
		var DivCnt = $("#sponsor>div").length - 2;
		
		if(idx<DivCnt) {										//neko: 맨 마지막것-1 까지 가능
			sponsor_rotate();
		}
		$("#sponsor").attr("timer", setInterval(sponsor_rotate, 3000));
	}

</script>

