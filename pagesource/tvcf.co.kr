	function fnSide_Today_Tab(pNum) {
		$(".Div_Side_TodayNew").hide();
		$("#Div_Side_Today"+pNum).show();

		for(var i=1; i<=4; i++) {
			if(parseInt(i)==parseInt(pNum)) {
				$("#new_todayTab0" + i).addClass("selected");
