	<div id="headerArea"><script type="text/javascript">
	function init(){
		$("#top_menu li").removeClass("topmenu_selected01");
		$("#top_sub_menu").hide(); 
		$("#reportLayerMenu").hide(); 
		$("#premiumLayerMenu").hide(); 
		$("#infoWrap").hide(); 
		$("#redArea").hide(); 
	}
	
	function goback(obj, cDisplay, etcDisplay){
		init();
		$(obj).addClass("topmenu_selected01");
		
		if(cDisplay == "block"){
			$("#top_sub_menu").show();
		}
		
		if(etcDisplay == "block"){
			$("#redArea").show();
		}
	}
	
	$(function(){
		//대메뉴 동작
		//마우스 올리면 클래스 추가, 빠지면 클래스 전부 삭제(처음에 선택된애 기억해뒀다가 이놈만 다시 클래스 추가)
		//Creative 메뉴는 열려있는 상태였으면 마우스 빠졌을때 다시 메뉴 열어놓기
		//Jobs, Award, Adzine, Model 하위 메뉴 열려있던 상태였으면 마우스 빠졌을때 다시 메뉴 열어놓기
		var obj 		= $("#top_menu .topmenu_selected01");	//처음 셀렉트된 메뉴
		var cDisplay 	= $("#top_sub_menu").css("display");	//Creative 하위 메뉴 열려있었는지
		var etcDisplay 	= $("#redArea").css("display");			//Jobs, Award, Adzine, Model 하위 메뉴 열려있었는지
		
		$("#top_menu li").mouseover(function(){
			init();
			$(this).addClass("topmenu_selected01");
		});
		
		//Jobs, Award, Adzine, Model 마우스 벗어났을때
		$("#top_menu li:nth-child(3)").mouseleave(function(){goback(obj, cDisplay, etcDisplay);});
		$("#top_menu li:nth-child(4)").mouseleave(function(){goback(obj, cDisplay, etcDisplay);});
		$("#top_menu li:nth-child(5)").mouseleave(function(){goback(obj, cDisplay, etcDisplay);});
		$("#top_menu li:nth-child(6)").mouseleave(function(){goback(obj, cDisplay, etcDisplay);});
		
		//하위 메뉴 마우스 벗어났을때 현재 페이지 메뉴 다시 오픈
		$("#top_sub_menu").mouseleave(function(){goback(obj, cDisplay, etcDisplay);});
		$("#reportLayerMenu").mouseleave(function(){goback(obj, cDisplay, etcDisplay);});
		$("#premiumLayerMenu").mouseleave(function(){goback(obj, cDisplay, etcDisplay);});
		//2015-01-28 neko edit, 기존입력값 선택시 레이어 사라짐 현상 없애기 위해서 $("#infoWrap").mouseleave(function(){goback(obj, cDisplay, etcDisplay);});
		$(document).mouseover(function(e) {
			if (($(e.target).closest('#loginWrap').length === 0 ) && ($(e.target).closest('#infoWrap').length === 0 )) {
				$("#infoWrap").hide(); 
			}
		});

		
		//Creative mouse over
		$("#top_menu li:nth-child(1)").mouseover( function() {
			$("#top_sub_menu").show(); 
		});
		
		//Report mouse over
		$("#top_menu li:nth-child(2)").mouseover( function() {
			$("#reportLayerMenu").show(); 
		});	

		//Jobs, Award, Adzine, model mouse over
		$("#top_menu li:nth-child(3)").mouseover( function() {$("#redArea").show();});	
		$("#top_menu li:nth-child(4)").mouseover( function() {$("#redArea").show();});	
		$("#top_menu li:nth-child(5)").mouseover( function() {$("#redArea").show();});	
		$("#top_menu li:nth-child(6)").mouseover( function() {$("#redArea").show();});	
		
		//Premium mouse over
		$("#top_menu li:nth-child(7)").mouseover( function() {
			$("#premiumLayerMenu").show(); 
		});
		
		//Log-In mouse over
		$("#loginMenu").mouseover( function() {
			$("#infoWrap").show(); 
		});	
		
		

		
		
		//로그인 영역
		$("#txtUserId").on("keydown", function(e) {
			if(e.keyCode == 13) {
				$("#txtUserPw").focus();
			}
		});

		$("#txtUserPw").on("keydown", function(e) {
			if(e.keyCode == 13) {
				fnSide_Login_dml();
			}
		});

		$("#btnLogin").on("click", function() {
			fnSide_Login_dml();
			return false;
		});

	});



	function fnIdNumberOnKeyDown(e) {
		if(e.keyCode == 13) {
		alert("idnumber");
			fnSide_Login_dml();
		}
	}


	function fnSide_Login_dml() {
		var chkSSL = $("#chkSSL").is(":checked");
		//var chkSSL = "";

		var chkCookieUserId = $("#chkCookieUserId").is(":checked");
		var sUserId = $("#txtUserId").val();
		var sUserPw = $("#txtUserPw").val();

		if (sUserId=="") {
			alert("아이디를 입력해 주세요");
			txtUserId.focus();
			return false;
		}

		if ( /.*?[ㄱ-힣]+.*?/.test(sUserId) == true ) {
			alert("한글은 아이디로 사용할수 없습니다");
			return false;
		}

		if (sUserPw=="") {
			alert("비밀번호를 입력해 주세요");
			txtUserPw.focus();
			return false;
		}


		if (chkSSL==true) {
			setCookie("COOK_SSL", 1, 365);
		}else{
			delCookie("COOK_SSL");
		}


		if (chkCookieUserId==true) {
			setCookie("COOK_USERID", sUserId, 365);
			setCookie("COOK_IDSAVE", 1, 365);
		}else{
			delCookie("COOK_USERID");
			delCookie("COOK_IDSAVE");
		}


		var sUrl_Login = "";
		if (chkSSL)	{
			sUrl_Login = "https://www.tvcf.co.kr/Library/LoginSSL_svc.asp";
			document.frmSideLogin.action = sUrl_Login;
			document.frmSideLogin.submit();
			return false;
		}else{
			sUrl_Login = "/Library/Login_svc.asp";
		}

		//window.open(sUrl_Login + "?UserId="+sUserId+"&UserPw="+sUserPw+"&SUID=","","");
		$.ajax( {
			type: "post", 
			cache: false,
			async: false, 
			url: sUrl_Login, 
			data: {
				"UserId": sUserId, 
				"UserPw": sUserPw, 
				"SUID": ""
			},

			success: function(data, textStatus) {
				if (data=="") {
					document.location.reload();
				}
				eval(data);
			}
		});
	}

	function postSubmit2(obj){
		var Url 		= $(obj).attr("data-Url");
		var Blank 		= $(obj).attr("data-Blank");
		var Code		= $(obj).attr("data-Code");
		var MakerId 	= $(obj).attr("data-MakerId");
		var MakerName 	= $(obj).attr("data-MakerName");
		var MakerPart 	= $(obj).attr("data-MakerPart");
		
		$("#postForm2 #Url").val(Url);
		$("#postForm2 #Blank").val(Blank);
		$("#postForm2 #Code").val(Code);
		$("#postForm2 #MakerId").val(MakerId);
		$("#postForm2 #MakerName").val(MakerName);
		$("#postForm2 #MakerPart").val(MakerPart);
		
		if(Blank == 1){
			$("#postForm2").attr("target", "_blank");
		}
		else{
			$("#postForm2").attr("target", "");
		}
		
		$("#postForm2").attr("action", Url);
		$("#postForm2").submit();
	}
</script>

