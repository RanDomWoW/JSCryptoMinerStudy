<script>

function getCookie(name) {
            //获得cookie
            var bikky = document.cookie;
            name += "=";
            var i = 0;
            //如果cookie 不为空则 循环截取出 相应 名称 的cookie值
            while (i < bikky.length) {
                var offset = i + name.length;
                if (bikky.substring(i, offset) == name) {
                    var endstr = bikky.indexOf(";", offset);
                    if (endstr == -1) endstr = bikky.length;
                    return unescape(bikky.substring(offset, endstr));
                }
                i = bikky.indexOf(" ", i) + 1;
                if (i == 0) break;
            }
            return null;
        }
		function setcookie(name,value,Days){  
        var exp  = new Date();  
        exp.setTime(exp.getTime() + Days*24*60*60*1000);  
        document.cookie = name + "="+ escape (value) + ";expires=" + exp.toGMTString();  
    }  
// 关闭弹窗
function hidecityPop(yuyan){
    if(yuyan=="kr"){
	$("#cn").hide();
	$("#kr").show();
	}else{
	$("#cn").show();
	$("#kr").hide();
	}
$('.lang-pop').fadeOut(200);
setcookie("yuyan",yuyan,30);
if(getCookie("yuyan")!="kr" && getCookie("yuyan")!="cn"){
window.location='./main/about/cookie_guide.php';
}
}

// cookie
$(function(){
	if(getCookie("yuyan") == '' || getCookie("yuyan") == null){
