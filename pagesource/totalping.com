<script type="text/javascript"><!--
var RepetirCada = 1000;
var AnimacionDegradado = 50;
var AnimacionDegradadoSalto = 3;
var AnimacionDegradadoColor = 220;
var ListaUltimosPings = [];
var IsMouseInControl = false;
function Mouse_Entrada()
{
IsMouseInControl = true;
}
function Mouse_Salida()
{
IsMouseInControl = false;
}
function M_E()
{
return Mouse_Entrada();
}
function M_S()
{
return Mouse_Salida();
}
function DegradadoOcultacionItemListaPings(Item, Color)
{
CambiarColorListaPings(Item, Color, false);
Color = Color + AnimacionDegradadoSalto;
if (Color < AnimacionDegradadoColor)
{
if (IsMouseInControl)
{
CambiarColorListaPings(Item, 0, false);
setTimeout("AnimacionListaPings("+Item+")", RepetirCada);
return false;
}
setTimeout("DegradadoOcultacionItemListaPings("+Item+", "+ Color+")", AnimacionDegradado);
}
else
{
MostrarOcultarItemListaPings(Item, false);
CambiarColorListaPings(Item, AnimacionDegradadoColor, false);
Item = Item + 1;
if (Item> ListaUltimosPingsTotal -1)
Item = 0;
MostrarOcultarItemListaPings(Item, true);
CambiarColorListaPings(Item, AnimacionDegradadoColor, false);
setTimeout("DegradadoMostrarItemListaPings("+(Item)+", "+ AnimacionDegradadoColor +")", AnimacionDegradado);
}
}
function DegradadoMostrarItemListaPings(Item, Color)
{
CambiarColorListaPings(Item, Color, false);
Color = Color - AnimacionDegradadoSalto;
if (Color> 0)
setTimeout("DegradadoMostrarItemListaPings("+Item+", "+ Color+")", AnimacionDegradado);
else
{
CambiarColorListaPings(Item, 0, false);
setTimeout("AnimacionListaPings("+(Item)+")", RepetirCada);
}
}
function arrayToColorListaPings(Color)
{
return "rgb(" + Color +
", " + Color +
", " + Color + ")";
}
function CambiarColorListaPings(Item, Color, Recuperar)
{
var Enlace = document.getElementById('IR_' + Item);
if (Enlace)
{
Enlace.style.color = arrayToColorListaPings(Color);
ListaUltimosPings[Item].style.color = arrayToColorListaPings(Color);
}
}
function GetListaPings(ID)
{
var lista = document.getElementById(ID);
if (lista)
{
var hijo;
for (hijo = lista.firstChild; hijo; hijo = hijo.nextSibling)
if (hijo.nodeType == 1 && hijo.nodeName == "DIV")
ListaUltimosPings[ListaUltimosPings.length] = hijo;
}
ListaUltimosPingsTotal = ListaUltimosPings.length;
}
function MostrarOcultarItemListaPings(Item, Mostrar)
{
if (Mostrar)
ListaUltimosPings[Item].style.display = "block";
else
ListaUltimosPings[Item].style.display = "none";
}
function OcultarTodosListaPings()
{
var y;
for (y=0; y<ListaUltimosPingsTotal; y++)
MostrarOcultarItemListaPings(y, false);
}
function AnimacionListaPings(Item)
{
if (IsMouseInControl)
{
setTimeout("AnimacionListaPings("+Item+")", RepetirCada);
return false;
}
DegradadoOcultacionItemListaPings(Item, 0);
}
