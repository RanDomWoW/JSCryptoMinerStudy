<script type="text/javascript">
 $(function(){

    $('#current-time').text(show());
    $('#week').text(showWeek());
  function show(){
   var mydate = new Date();
   var str = "" + mydate.getFullYear() + "年";
   str += (mydate.getMonth()+1) + "月";
   str += mydate.getDate() + "日";
   return str;
  }
  function showWeek(){
var dayNames = new Array("星期日","星期一","星期二","星期三","星期四","星期五","星期六"); 
Stamp = new Date();  
return dayNames[Stamp.getDay()];
  }


 });
