      <script >
    function social_media_tracker(link_location, social_platform){                   
        mixpanel.track("Social Share", {
            "Link Location": link_location,
            "Social Platform": social_platform
            });
        }

    var excludeIPS = ['65.213.72.30', '185.142.16.9', '203.176.115.9', '208.68.197.6',
                      '208.68.197.9', '208.68.199.6', '208.68.199.9', '208.85.160.9',
                      '208.85.161.9'];

    $.getJSON('//freegeoip.net/json/?callback=?', function(data) {                   
        try{
            if(ignoreMix && excludeIPS.indexOf(data.ip) > -1){
               
                console.log('Mixpanel ignore events set: NO events tracked.');
                mixpanel.register({"$ignore":true});

            }else{
               
                mixpanel.unregister("$ignore");
     
                mixpanel.track("View Page", {
                    "Page Name": document.querySelector('.dotted-underline') ? document.querySelector('.dotted-underline').innerText : location.href,
                    "Careers Site": false
                    });                                
                
                mixpanel.people.set_once({
                    // 'UTM (first-touch)' : new Date().toISOString(), 
                    'First Visited P72 Site' : new Date().toISOString()
                });
             
                mixpanel.people.set({
                    // 'UTM (last-touch)' : new Date().toISOString(),
                    'Last Visited P72 Site' : new Date().toISOString()
                });      
                                        
                (function(){
                    var links = document.querySelectorAll('a');
                    [].forEach.call(links, function(link) {
                        link.addEventListener("click", function (e) {                           
                            mixpanel.track("Click Link", {
                                "Link Name": link.text,                                
                                "Link Location": link.getAttribute('link-location') == null ? 'Body' : link.getAttribute('link-location'),
                                "Link Type": link.getAttribute('link-type') == null ? '' : link.getAttribute('link-type'),
                                "Link Destination URL" : link.href
                            });
                        });                    
                    });                       
                })();      

                // Wistia Video Tracking   
                window._wq = window._wq || [];
                _wq.push({ id: "_all", onReady: function(video) {
                   video.bind("secondchange", function() {
                      if (video.secondsWatched() === 1) {
                        mixpanel.track('Start Video', {'Video Length': video.duration(), 'Video Name': video.name()});
                      }
                      if (video.secondsWatched() === 10) {
                        mixpanel.track('Watch Video 10s', {'Video Length': video.duration(), 'Video Name': video.name()});
                      }
                      if (video.secondsWatched() === 30) {
                        mixpanel.track('Watch Video 30s', {'Video Length': video.duration(), 'Video Name': video.name()});
                      }
                      if (video.secondsWatched() === 60) {
                        mixpanel.track('Watch Video 60s', {'Video Length': video.duration(), 'Video Name': video.name()});
                      }
                    });
                     
                    video.bind("end", function(t) {
                      mixpanel.track('End Video', {'Video Length': video.duration(), 'Video Name': video.name()})
                    });
                     
