					//$fancypanel.fadeOut("fast");
						//$searcharrow.removeClass("arrow-down").addClass("arrow-up");
				//});
			$searchtext.blur(function () {
				if ($.trim(this.value) === "") {
					this.value = ("Pesquisar...");
				}
				$fancypanel.fadeOut("fast");
				//$searcharrow.removeClass("arrow-down").addClass("arrow-up");
			});
			$("#search-bar form").submit(function () {
				var b = $searchtext.val();
				if (b === "Pesquisar..." || b === "") {
					$("#search-bar div.fancy-panel:hidden").fadeIn("fast");
					return false;
			}
			return true;
			});
		});	// fim do document ready
	</script>	
	
