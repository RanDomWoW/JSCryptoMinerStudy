			$searchradio.click(function() {
				$searchtext.focus();
				var valor = $searchtext.val();
				$searchtext.val("");
				$searchtext.val(valor);
			});

			$searchtext.focus(function () {
				if (this.value === this.defaultValue) {
					this.value = ""
				} else {
					this.select()
				}
				$fancypanel.fadeIn("fast");
				//$searcharrow.removeClass("arrow-up").addClass("arrow-down");
