<script>
	jQuery(document).ready(function(){
		jQuery(".langbar select").bind("change",function(){
			var _val = jQuery(this).val();
			//alert(_val);
			
			jQuery(".langbar a").eq(_val).trigger("click");
		});
		jQuery(".left2 a.lang").bind("click",function(){
			var idx = jQuery(".left2 a.lang").index(this);
			jQuery(".langbar a").eq(idx).trigger("click");
		});
		$(".langbar a.lang").click(function () {
			if($(this).hasClass('on')){
			   return false;
			}
			var lang = $(this).data('type');
			if(lang!='zh-tw' && lang!='en-us'){
			   return false;
			}
			document.cookie=COOKIE_PREFIX+"think_language="+lang + "; path=/";
			if(window.location.pathname != '/hmv/tw/index.html' && (window.location.href.indexOf('/tw/') > 0 || window.location.href.indexOf('/en/') > 0)){
			   if(lang == 'zh-tw'){
				   window.location.href = window.location.href.replace('/en/','/tw/');
			   }else{
				   window.location.href = window.location.href.replace('/tw/','/en/');
			   }
			}else if(window.location.pathname == '/' || window.location.pathname == '/index.html'){
				//首页跳转
				window.location.href = '/hmv/tw/index.html';
			}else if(window.location.pathname == '/hmv/tw/index.html'){
				//首页跳转
				window.location.href = '/';
			}else{
			   window.location.reload();
			}
		});
	});
</script>
