	function shuffle(arr) {
		var _c = arr.length, temp, index;
		while (_c > 0) {
			index = Math.floor(Math.random() * _c);
			_c--;
			temp = arr[_c];
			arr[_c] = arr[index];
			arr[index] = temp;
		}
		return arr;
	}
