<script>
	$(function(){
		$('.cate-list li:not(.current)').click(function(){
			$('.cate-list li.current>span').text($(this).text());
			$('#cate').val($(this).data('value'));
			$('.cate-list li:not(.current)').hide();
		});
		$('.cate-list').hover(function(){
			$('.cate-list li:not(.current)').show();
		},function(){
			$('.cate-list li:not(.current)').hide();
		});
		$('.search-input').keydown(function(){
			$(this).siblings('.placeholder').hide();
		});
		$('.search-input').keyup(function(){
			if($(this).val()!==""){
				$(this).siblings('.placeholder').hide();	
			}else{
				$(this).siblings('.placeholder').show();
			}
		});
		$('.search-input').mouseup(function(){
			if($(this).val()!==""){
				$(this).siblings('.placeholder').hide();	
			}else{
				$(this).siblings('.placeholder').show();
			}
		});
		$('.search-input').blur(function(){
			if($(this).val()!==""){
				$(this).siblings('.placeholder').hide();	
			}else{
				$(this).siblings('.placeholder').show();
			}
		});
		if($('.search-input').val()==""){
			$('.search-input').siblings('.placeholder').show();
		}
	});
</script>
