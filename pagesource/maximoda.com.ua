<script>
	(function($) {
		$(function() {

			// Проверим, есть ли запись в куках о посещении посетителя
			// Если запись есть - ничего не делаем
			if (!$.cookie('was')) {
				$(window).load(function() {
					if (screen.width>768){
						/*проверка на пути*/
						var path=window.location;
						/*найти вхождение строки*/
						var find=path.pathname.indexOf("detail") + 1;


						if(path.pathname!='/login/' && path.pathname!='/auth/' && path.pathname!='/auth/index.php' && path.pathname!='/login/index.php' && path.pathname!='/personal/cart/'&& path.pathname!='/personal/order/make/'&& path.pathname!='/personal/order/make/index.php'&& path.pathname!='/personal/order/' && path.pathname!='/register/' && find==0) {
							p = $('.overlay-welcome');
							p.css('display', 'block');
							p.click(function (event) {
								e = event || window.event;
								if (e.target == this) {
									$(p).css('display', 'none');
								}
							});
							$('.close-modal-welcome').click(function () {
								p.css('display', 'none');
							});
							$(this).keydown(function (eventObject) {
								if (eventObject.which == 27) {
									$('.modal-welcome').hide();
									$('.overlay-welcome').fadeOut(400);
								}
							});
						}
					}
				});


			}

			// Запомним в куках, что посетитель к нам уже заходил
			$.cookie('was', true, {
				expires: 365,
				path: '/'
			});

		})
	})(jQuery)
</script>
