    // 7 : 보도자료
    // 8 : 정책자료실
    // 9 : 부동산통계현황 - 지가변동률-년도별지역별
    // 10: 부동산통계현황 - 토지거래현황-년도별행정구역별
    // 11: 부동산통계현황 - 건축물거래현황-년도별행정구역별
    // 12: 부동산통계현황 - 아파트거래현황-년도별행정구역별
    // 13: 제도안내 - 토지투기지역
    // 14: 제도안내 - 주택투기지역
    // 15: 부동산민원-부동산정보조회-부동산정보통합열람
    // 16: 부동산민원-부동산정보조회-토지이용계획
    // 17: 모바일앱 이벤트 공지사항
    // 18: 모바일앱 앱어워드코리아2016 대상수상 공지사항
    // 19: 2016년 하반기 설문조사 공지사항
    // 1020493 : 2016년 하반기 설문조사 이벤트 결과

    if(menuParam=="1"){
        callContents('main', '/stat/stat.do');
    }else if(menuParam=="2"){
        callContents('/vm/lotdetailinfo/lotdetailinfo','/goEstateInfo.do');
    }else if(menuParam=="3"){
        callContents('/vm/prcsrch/prcsrch','/goEstateInfo.do');
    }else if(menuParam=="4"){
        callContents('bbs', '/cop/bbs/selectBoardList.do?bbsId=BBSMSTR_000000000002');
