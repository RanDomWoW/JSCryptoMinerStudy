function fn_searchMainBbsList(bbsId){
	$.ajax({
			url:"/main/selBbsList.do",
			type:'GET', 
			data: '&bbsId='+bbsId,
			dataType:'json', 
			contentType: 'text/xml; charset=UTF-8',
			success:function(args){
				var html = '';
				$.each( args, function(index, data) {
					html += '<input type="hidden" id="bbsId'+index+'" name="bbsId'+index+'" value="'+data.bbsId+'" />';
					html += '<input type="hidden" id="nttId'+index+'" name="nttId'+index+'" value="'+data.nttId+'" />';
					html += '<li><a href="#title" onclick="formSubmitIn('+index+');">'+data.nttSj+'</a></li>';
				});
				if(bbsId == 'BBSMSTR_000000000001'){
					$('#bbsDiv').empty();
					$('#bbsDiv2').empty();
		        	$('#bbsDiv').html(html);					
				}else{
					$('#bbsDiv').empty();
					$('#bbsDiv2').empty();
		        	$('#bbsDiv2').html(html);
				}
			},
			error: function(e){
	
				onnaraAlert('시스템 통신오류가 발생했습니다. \n다시 시도해주세요.');
			}
	});
}

function formSubmitIn(idx){
	var bbsId = $('#bbsId'+idx).val();
	var nttId = $('#nttId'+idx).val();
	callContents('bbs', '/cop/bbs/selectBoardArticle.do?bbsId='+bbsId+'&nttId='+nttId);
}
