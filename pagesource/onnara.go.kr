/* !!!!!!!!!!!!!!!!!!!! 여기서 부터 복사 */
/* background:url("images/... ") - 경로 변경해서 사용 */
/* wrap_update ============================== */
.wrap_update {}
.wrap_update.open {}
.wrap_update.close {}
.wrap_update.close .contents_update {display:none;}
/* btn ========== */
.wrap_update .btn_update {position:absolute; z-index:1; left:50%; top:0; height:30px; margin-left:-49px; font-weight:bold; color:#fcc700; background-color:#2b4159;}
/* close */
.wrap_update.close .btn_update {}
.wrap_update.close .btn_update:before {content:""; display:inline-block; width:14px; height:100%; margin-left:-14px; padding-left:12px; vertical-align:middle; background:url("/images/common/btn_update_open_bg_lf.png") no-repeat 0 0;}
.wrap_update.close .btn_update:after  {content:""; display:inline-block; width:14px; height:100%; margin-right:-14px; padding-right:12px; vertical-align:middle; background:url("/images/common/btn_update_open_bg_rg.png") no-repeat right 0;}
/* open */
.wrap_update.open .btn_update {}
.wrap_update.open .btn_update:before {content:""; display:inline-block; width:14px; height:100%; margin-left:-14px; padding-left:12px; vertical-align:middle; background:url("/images/common/btn_update_close_bg_lf.png") no-repeat 0 0;}
.wrap_update.open .btn_update:after  {content:""; display:inline-block; width:14px; height:100%; margin-right:-14px; padding-right:12px; vertical-align:middle; background:url("/images/common/btn_update_close_bg_rg.png") no-repeat right 0;}
/* contents ========== */
.wrap_update .contents_update {overflow:hidden; position:absolute; z-index:1; top:-120px; left:0; width:100%; height:120px; background:url("/images/common/update_bg.gif") center 0;}
.wrap_update .contents_update .inner {position:relative; width:1000px; margin:0 auto;}
.wrap_update .contents_update .inner:after {content:""; display:block; clear:both;}
/* btn_list_all */
.wrap_update .contents_update .btn_list_all {position:absolute; left:50%; top:19px; width:81px; height:81px; margin-left:-40px; background:url("/images/common/btn_all_bg.png") no-repeat 0 0;}
.wrap_update .contents_update .btn_list_all a {display:block; height:100%; font-weight:bold; color:#d20b0b; text-align:center;}
.wrap_update .contents_update .btn_list_all a span {display:block; padding:29px 0 0px; font-size:10px;}
.wrap_update .contents_update .btn_list_all a em {display:block; margin-top:-2px; font-size:14px; font-style:normal;}
/* group_lft */
.wrap_update .contents_update .group_lft {float:left; width:420px; padding-right:10px; text-align:right;}
.wrap_update .contents_update .group_lft .tit {position:relative; display:inline-block; margin:21px 0 9px; font-size:24px; font-weight:bold; color:#FFF;}
.wrap_update .contents_update .group_lft .tit:before {content:""; position:absolute; left:-20px; top:-5px; width:19px; height:19px; background:url("/images/common/ico_new.png") center 0;}
.wrap_update .contents_update .group_lft .txt {line-height:1.4; font-size:12px; font-weight:bold; color:#85bfff;}
/* group_rgt */
.wrap_update .contents_update .group_rgt {float:right; position:relative; width:424px; padding:17px 0 0 6px;}
.wrap_update .contents_update .group_rgt ol {float:left;}
.wrap_update .contents_update .group_rgt ol li {margin-bottom:-15px; padding-left:15px; background:url("/images/common/bul_ylw.png") no-repeat 0 4px;}
.wrap_update .contents_update .group_rgt ol li .tit {margin-right:5px;  font-size:14px; font-weight:bold; color:#FFF;}
.wrap_update .contents_update .group_rgt ol li .date {font-size:12px; font-weight:bold; color:#ffd800;}
.wrap_update .contents_update .group_rgt .btn_more {float:left; height:19px; line-height:19px; margin:-2px 0 0 0; padding:0 12px; border-radius:19px; font-size:12px; font-weight:bold; color:#FFF; background:#ff471c;}
