        callContents('/vm/offer/offerCalendar','/goPlotNotification.do');
    }else if(menuParam=="6"){
        callContents('/vm/offer/offerApt','/goPlotNotification.do');
    }else if(menuParam=="7"){
        callContents('bbs', '/cop/bbs/selectBoardList.do?bbsId=BBSMSTR_000000000005');
    }else if(menuParam=="8"){
        callContents('bbs', '/cop/bbs/selectBoardList.do?bbsId=BBSMSTR_000000000003');
    }else if(menuParam=="9"){
        callContents('statView', '/stat/stat.do?statsCd=ST00000077');
    }else if(menuParam=="10"){
        callContents('statView', '/stat/stat.do?statsCd=ST00000003');
    }else if(menuParam=="11"){
        callContents('statView', '/stat/stat.do?statsCd=ST00000030');
    }else if(menuParam=="12"){
        callContents('statView', '/stat/stat.do?statsCd=ST00000070');
    }else if(menuParam=="13"){
        callContents('/vm/onnaraguide/systemGuidance','/goOnnaraGuide.do?systemGuidanceIndex=08');
    }else if(menuParam=="14"){
        callContents('/vm/onnaraguide/systemGuidance','/goOnnaraGuide.do?systemGuidanceIndex=07');
    }else if(menuParam=="15"){
        callContents('', '/km/cmm/kmCommon/PKMCMCM0010.do?tabClIndex=3&kmUrl=/km/estateinfo/info/PKMESIF1000.do?searchCvplClCode=60');
    }else if(menuParam=="16"){
        callContents('', '/km/cmm/kmCommon/PKMCMCM0010.do?tabClIndex=3&kmUrl=/km/estateinfo/info/PKMESIF1010.do?searchCvplClCode=60');
    }else if(menuParam=="17"){
        callContents('bbs', '/cop/bbs/selectBoardArticle.do?bbsId=BBSMSTR_000000000001&nttId=20402');
    }else if(menuParam=="18"){
        callContents('bbs', '/cop/bbs/selectBoardArticle.do?bbsId=BBSMSTR_000000000001&nttId=20452');
    }else if(menuParam=="19"){
        callContents('bbs', '/cop/bbs/selectBoardArticle.do?bbsId=BBSMSTR_000000000001&nttId=20471');
    }else if(menuParam=="1020493"){
        callContents('bbs', '/cop/bbs/selectBoardArticle.do?bbsId=BBSMSTR_000000000001&nttId=20493');
    }

    if(!isNullString(gMenu)){

        if( (gMenu=="bbs" && !isNullString(forwardUrl ))){
            callContents('bbs', forwardUrl);
        }
    }
}
