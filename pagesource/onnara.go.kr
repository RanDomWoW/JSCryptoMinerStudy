$(document).ready(function(){
	$(".wrap_update .btn_update").on("click", function(){
		if ($(".wrap_update").hasClass("close")){
			
			$(".wrap_update").removeClass("close");
			$(".wrap_update").addClass("open");
			$(this).text("업데이트내역 닫기");
			$("#header").css({"top" : "120px"});
			$("#container").css({"padding-top" : "270px"});
		} else if ($(".wrap_update").hasClass("open")){
			$(".wrap_update").removeClass("open");
			$(".wrap_update").addClass("close");
			$(this).text("업데이트내역 보기");
			$("#header").css({"top" : "0px"});
			$("#container").css({"padding-top" : "150px"});
		};
	});
});		

