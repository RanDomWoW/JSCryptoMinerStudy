function DateTime()
{
y = new Date
a = y.getFullYear()
b = y.getMonth()+1
c = y.getDate()
d = y.getHours()
if (d < 10)
d =("0"+d)
e = y.getMinutes()
if (e < 10)
e =("0"+e)
s = y.getSeconds()
if (s < 10)
s =("0"+s)
f = y.getDay()

switch(b) {
case 1 :
	b=("&nbsp;&nbsp;" + c + ". Január " + a);
break;
case 2 :
	b=("&nbsp;&nbsp;" + c + ". Február " + a);
break;
case 3 :
	b=("&nbsp;&nbsp;" + c + ". Marec " + a);
break;
case 4 :
	b=("&nbsp;&nbsp;" + c + ". Apríl " + a);
break;
case 5 :
	b=("&nbsp;&nbsp;" + c + ". Máj " + a);
break;
case 6 :
	b=("&nbsp;&nbsp;" + c + ". Jún " + a);
break;
case 7 :
	b=("&nbsp;&nbsp;" + c + ". Júl " + a);
break;
case 8 :
	b=("&nbsp;&nbsp;" + c + ". August " + a);
break;
case 9 :
	b=("&nbsp;&nbsp;" + c + ". September " + a);
break;
case 10 :
	b=("&nbsp;&nbsp;" + c + ". Október " + a);
break;
case 11 :
	b=("&nbsp;&nbsp;" + c + ". November " + a);
break;
case 12 :
	b=("&nbsp;&nbsp;" + c + ". December " + a);
break;
}
switch(f) {
case 1 :
	f=("Dnes je Pondelok, ");
break;
case 2 :
	f=("Dnes je Utorok, ");
break;
case 3 :
