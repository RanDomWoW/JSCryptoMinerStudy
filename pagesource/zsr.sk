case 6 :
	f=("Dnes je Sobota, ");
break;
case 7 :
	f=("Dnes je Nedeľa, ");
break;
}
id_span_klient_cas.innerHTML = f + b + "&nbsp;&nbsp;" + d + ":" + e + ":" + s;
window.setTimeout("DateTime();", 1000);
}
