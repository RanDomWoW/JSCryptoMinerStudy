    <script>
            function SetLeg(leg) {
                $J("#leg").val(leg);
            }
            function ShowFlightSearch() {
                $J("#home-flight-clear").hide();
                $J("#home-flight-button").show();
            }
            function HideFlightSearch() {
                $J("#home-flight-button").hide();
                $J("#home-flight-clear").show();
            }
            function ClearFlightSearch() {
                $J("#home-flight-input").val("");
                $J("#home-flight-clear").hide();
                $J("#home-flight-button").show();
            }
            //Autocomplete
            $J("#home-flight-input").autocompleteHighlight({
                    //Fetch data from server
                    source: function(request, response) {
                        $J.post("/ajax/AIA/Flight/FlightAutoCompleteComponentAsync", $J("#home-flight-search").serializeObject(), function(data) {
                            response(data);
                        });
                    },
                    //Min number of characters to initiate autocomplete
                    minLength: 2,
                    //When item is selected
                    select: function(e, ui) {
                        e.preventDefault();
                        this.value = ui.item.flightNumber;
                        window.location.href = "/flights" + ui.item.value;
                    },
                    //When item is focused by arrow keys
                    focus: function(e, ui) {
                        e.preventDefault();
                        this.value = ui.item.flightNumber;
                    },
                    //Highlight the search term within
                    highlightClass: "ui-autocomplete-highlight"
            });

            //Show and hide input clear button when typing/deleting
            $J("#home-flight-input").on("focus", function (e) {
                e.preventDefault();
                ShowFlightSearch();
            });
            $J('main').click(function (e) {
                var input = $J("#home-flight-input");

                if (input.length) {
                    if ($J(e.target).closest(input).length || $J(e.target)[0] === $J("#home-flight-button")[0])
                        return;

                    if (input.val().length > 0) {
                        HideFlightSearch();
                    }
                }
            });
        </script>



