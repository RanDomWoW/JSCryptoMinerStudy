<script type='text/javascript'>
  /*<![CDATA[*/
    var numPages=6;
    var firstText ='الاول';
    var lastText ='الاخير';
    var prevText ='« السابق';
    var nextText ='التالي »';
    var urlactivepage=location.href;
    var home_page="/";
  /*]]>*/
