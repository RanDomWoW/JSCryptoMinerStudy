                     '<a href="' + ad.targetUrl + '" onclick="trackClick(\''+ad.label+'\')" target="_blank" style="text-decoration: none; color:black;">' + ad.content + '</a>' +
                  '</div>';
        $(item).html(html);
        trackImpression(ad);
    }
    function trackClick(label) {
        var img = document.createElement("img");
        img.setAttribute("src", "/tracking/bn?label=" + label + "&action=addClick&t="+Date.now());
        document.body.appendChild(img);
    }
    function trackImpression(ad) {
        var time = Date.now();
        var img = document.createElement("img");
        img.setAttribute("src", "/tracking/bn?label=" + ad.label  + "&action=addImpression&t="+time);
        document.body.appendChild(img);
        if(ad.externalTracking) {
            var imgTracking = document.createElement("img");
            imgTracking.setAttribute("src", ad.externalTracking + "t="+time);
            document.body.appendChild(imgTracking);
        }
    }
</script>

    
