    <script type="text/javascript">
	//console.log(navigator.userAgent);

function checkWidth() {
	//alert("hi");

	var windowSize = $(window).width();
	var windowHeight = $(window).height();
	//console.log(windowSize);

	if ( windowHeight > 1110) { 
    	//console.log(windowHeight);
    	//alert(windowHeight);
    	$('.fullscreen-bg, .fullscreen-bg__video').css({
    		//'border':'3px solid red',
    		'max-height':'820px'
    	});
    	$('.fullscreen-bg, .fullscreen-bg__video').removeAttr('height');
    	

    } 

	else if ( windowSize > 960) {
		

	//console.log('more'); 
		
	    $(".desktopBanner").css({
	      	'display':'block'
	    });
	    $(".mobileBanner").css({
	      	'display':'none'
	    });
     }
     else if ( windowSize <= 959 && windowSize > 640) { 
     	//console.log('less');

     	$(".mobileBanner").css({
	      	'display':'block'
	    });
	    $(".desktopBanner").css({
	      	'display':'none'
	    });


	    $('.fullscreen-bg__video').css({
				//'top': 'inherit',
				'width': '200%',
				'left': '-50%',
				'min-height':'450px',
				//'border':'2px solid green'

		});
	    $('.fullscreen-bg').css({
	    	'min-height':'450px',
	    	//'border':'2px solid red'
	    });  

    }
    else if ( windowSize <= 641 && windowSize > 320) { 
     	console.log('less');

     	//alert('mobile');
     	$(".mobileBanner").css({
	      	'display':'block'
	    });
	    $(".desktopBanner").css({
	      	'display':'none'
	    });


	    $('.fullscreen-bg__video').css({
				//'top': 'inherit',
				'width': '200%',
				'left': '-50%',
				'min-height':'260px',
				//'border':'2px solid pink'

		});
	    $('.fullscreen-bg').css({
	    	'min-height':'260px',
	    	//'border':'2px solid yellow'
	    });

    }
    else if ( windowSize == 320) { 
     	console.log('less');

     	//alert('mobile');
     	$(".mobileBanner").css({
	      	'display':'block'
	    });
	    $(".desktopBanner").css({
	      	'display':'none'
	    });


	    $('.fullscreen-bg__video').css({
				//'top': 'inherit',
				'width': '200%',
				'left': '-50%',
				'min-height':'200px',
				//'border':'2px solid red'

		});
	    $('.fullscreen-bg').css({
	    	'min-height':'200px',
	    	//'border':'2px solid red'
	    });
    }

         
}

  	// Execute on load
    checkWidth();
    // Bind event listener
    $(window).resize(checkWidth);

$(document).ready(function() {

		function homeVideofull() {

			var winH = $(window).height();

			$('.fullscreen-bg, .fullscreen-bg__video').height(winH - 130 );
			$('.fullscreen-bg__video').css({
				'top': 'initial',
				'width': '200%',
				'left': '-49%'
			});

			/*------------------to target IE 11 Banner------------------------*/
	 		if (document.documentMode || /Edge/.test(navigator.userAgent)) {
			    //alert('Hello Microsoft User!');

			    $('.fullscreen-bg__video').css({
					'top': '0',
					'width': 'auto',
					'left': '-10% ',
					'right': '-10%',
					'margin': '0 auto'
					//'border':'3px solid red'
				});
			}

			/*------------------to target Firefox banner------------------------*/
			if (navigator.userAgent.indexOf("Firefox") != -1) {
                    //alert('Firefox');
                    //alert(winH);
                    $('.fullscreen-bg').height(winH - 130);
                    $('.fullscreen-bg').css({
                        //'border': '3px solid green'
                    });

                }
		}

		/*------------PCs having height more than 1130-----------*/
		


		var winW = $(window).width();
		if(winW >= 960) {
			//alert("hi");
			homeVideofull(); 
		}
		
		$(window).resize(function() {
			var winW = $(window).width();
			if(winW >= 960) {
				homeVideofull();
			}

		});

		$('.fullscreen-bg__video').parent().click(function () {
			//alert('hi');
		    if($(this).children(".fullscreen-bg__video").get(0).paused){
		        $(this).children(".fullscreen-bg__video").get(0).play();
		        $(this).children(".playpause").fadeOut();
		    }else{
		       $(this).children(".fullscreen-bg__video").get(0).pause();
		        $(this).children(".playpause").fadeIn();
		    }
		});
	});



