    <!-- TOP NAV -->
    <header id="topNav" class="topHead rtl" ng-controller="menusControllers">
        <!-- remove class="topHead" if no topHead used! -->
        <div class="container">

            <!-- Top Nav -->
            <!-- Logo text or image -->
            <a class="logo" href="#/">
                <img src="/Content/images/logo.png" alt="بانک حکمت ایرانیان" />
            </a>
            <!-- Mobile Menu Button -->
            <button class="btn btn-mobile pull-left" data-toggle="collapse" data-target=".nav-main-collapse">
                <i class="fa fa-bars"></i>
            </button>
            <p ng-if="app_direction=='rtl'" class="yekan pull-left hidden-xs" style="color: #d3d3d3; margin-top: 6px; margin-right: 10px;  font-weight: bold">
                <span class="fa fa-calendar" style="color: #a9a9a9;"></span>
                {{persianDate}}
            </p>
            

            <div id="menu-container" ng-class="{'ltr-font ltr': app_direction=='ltr' , 'yekan rtl': app_direction=='rtl'}" class="navbar-collapse nav-main-collapse collapse pull-right" style=" position: absolute;height: 35px !important;padding: 0 ;width: calc(100% - 30px);">
                <nav class="nav-main mega-menu" style="position: relative;">
                    <ul class="nav nav-pills nav-main scroll-menu" id="topMain">
                        <li class="dropdown mega-menu-item mega-menu-fullwidth" ng-if="!menuLoaded">
                            <a class="dropdown-toggle">
                                <b style="font-size: 20px;" translate="LOADING"></b>
                            </a>
                        </li>
                        <script type="text/ng-template" id="menuTree">
                            <li ng-if="menuItem.Type=='5'" class="divider"></li>

                            <a ng-if="menuItem.ParentId==null && menuItem.Type!='5'" class="dropdown-toggle" ng-class="{'no-sub-item ':menuItem.Children.length<=0}" ng-href='{{(menuItem.Url) ? menuItem.Url : "#/lang/"+menuItem.LanguageCultureNames+"/page/"+menuItem.PageId}}' ng-attr-target="{{(menuItem.Url && menuItem.Url.startWith('#')) ? '_blank' : '_self'}}">
                                <span ng-if="menuItem.Url == '#/'" class="fa fa-home" style="font-size:20px;top: 7px; color: black;position:absolute"></span>
                                <b ng-if="menuItem.Url != '#/'" style="">{{ menuItem.Name }}</b>
                                <b ng-if="menuItem.Url == '#/'" ng-style="app_direction=='ltr' && {'padding-left':'22px'} || {'padding-right':'22px'}">{{ menuItem.Name }}</b>
                            </a>

                            <a ng-if="menuItem.ParentId!=null && menuItem.Type!='5'" class="dropdown-toggle" ng-href='{{(menuItem.Url) ? menuItem.Url : "#/lang/"+menuItem.LanguageCultureNames+"/page/"+menuItem.PageId}}' ng-attr-target="{{(menuItem.Url) ? '_blank' : '_self'}}">
                                <span>{{ menuItem.Name}}</span>
                            </a>

                            <ul class="dropdown-menu fade-in four" ng-if="menuItem.Children.length>1 && menuItem.Type != 4 && menuItem.Type != 5 ">

                                <li ng-repeat="menuItem in menuItem.Children | orderBy:'OrderNumber'" ng-class="{'dropdown-submenu ':menuItem.Children.length>0}" ng-include="'menuTree'">
                                </li>
                            </ul>

                            <ul class="dropdown-menu  fade-in four" ng-if="menuItem.Children.length>1 && menuItem.Type == 4 && menuItem.ParentId==null">
                                <li>
                                    <div class="mega-menu-content">
                                        <div class="row">
                                            <div class="col-md-3" ng-repeat="menuItem in menuItem.Children | orderBy:'OrderNumber'">
                                                <ul class="sub-menu">
                                                    <li>
                                                        <span class="mega-menu-sub-title">{{ menuItem.Name}}</span>
                                                        <ul class="sub-menu">
                                                            <li ng-repeat="menuItem in menuItem.Children  | orderBy:'OrderNumber'" ng-include="'menuTree'"></li>
                                                        </ul>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            </ul>

                            <ul class="dropdown-menu  fade-in four" ng-if="menuItem.Children.length>1 && menuItem.Type == 2 && menuItem.ParentId==null">
                                <li>
                                    <div class="mega-menu-content">
                                        <div class="row">
                                            <div class="col-md-6" ng-repeat="menuItem in menuItem.Children | orderBy:'OrderNumber'">
                                                <ul class="sub-menu">
                                                    <li>
                                                        <ul class="sub-menu">
                                                            <li ng-repeat="menuItem in menuItem.Children  | orderBy:'OrderNumber'" ng-include="'menuTree'" ng-class="{ 'dropdown-submenu ' : menuItem.Children.length > 0}"></li>
                                                        </ul>
                                                    </li>
                                                </ul>
                                            </div>

                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </script>



                        <li class="dropdown" ng-repeat="menuItem in menuItems  | orderBy:'OrderNumber'" ng-include="'menuTree'" ng-class="{'mega-menu-item mega-menu-fullwidth' : menuItem.Type == 4,'mega-menu-item mega-menu-two-columns' : menuItem.Type == 2 }"></li>




                    </ul>
                </nav>
            </div>
            <!-- /Top Nav -->

        </div>
