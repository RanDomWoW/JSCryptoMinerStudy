                        var going = 0;
                        var num = 0;
                        var c = 0;

                        var dowhile = true;

                        while (dowhile) {

                            num = (going == 0 || going == 2) ? m : n;

                            for (i = 1; i <= num; i++) {

                                order[el.id][c] = x + '' + y;
                                c++;

                                if (i != num) {
                                    switch (going) {
                                        case 0 :
                                            y++;
                                            break;
                                        case 1 :
                                            x++;
                                            break;
                                        case 2 :
                                            y--;
                                            break;
                                        case 3 :
                                            x--;
                                            break;

                                    }
                                }
                            }

                            going = (going + 1) % 4;

                            switch (going) {
                                case 0 :
                                    m--;
                                    y++;
                                    break;
                                case 1 :
                                    n--;
                                    x++;
                                    break;
                                case 2 :
                                    m--;
                                    y--;
                                    break;
                                case 3 :
                                    n--;
