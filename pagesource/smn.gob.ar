                            mapa.removeLayer(zoom2);

                        }
                        if(currentZoom>10){
                            mapa.addLayer(zoom0);
                            mapa.addLayer(zoom1);
                            mapa.addLayer(zoom2);

                        }







                    });
                }
                catch (e) {
                    console.log('Se produjo un error al inicializar el mapa. Revise la configuración.', e);
                }

            }
            else {
                console.log('Missing configuration smn.map.config');
            }



        }
    };
})();
