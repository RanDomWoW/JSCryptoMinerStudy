<script type="text/javascript">
	// xử lý đặc biệt cho chia sẻ background và banner trôi 2 ngày 11+12/12/2014
	if (v_cat_id  == 45 && typeof(arr_body_background)!="undefined" && arr_body_background.length > 0 && (ADS_105_15s_1.aNodes.length > 0 || ADS_105_15s_2.aNodes.length > 0)){
		// chỉ thực hiện ở trang chủ khi có background
		var d = new Date();
		var now_date = d.getDate();
		var now_month = d.getMonth();
		var now_year = d.getFullYear();
		if ((now_date == 11 || now_date == 12 || now_date == 13) && now_month == 11 && now_year == 2014){
			// chỉ thực hiện nếu là 11+12/12/2014
			if (arr_body_background.length == 1){
				// tự nhân background nếu chỉ có 1 cái để hiển thị được
				arr_body_background[1] = arr_body_background[0];
			}
			
			if (tmp = getCookie('background_or_troi')){
				background_or_troi = parseInt(tmp);
				background_or_troi = background_or_troi % 2;
			} else {
				background_or_troi = 0; //0: background / 1: trôi
			}
			if (background_or_troi == 0){
				// set banner trôi ko hiển thị
				ADS_105_15s_1.aNodes = [];
				ADS_105_15s_2.aNodes = [];
			} else {
				arr_body_background = [];// set background ko hiển thị
			}
			// set lại cookie lượt hiển thị background hay banner trôi
			setCookie('background_or_troi', 1 - background_or_troi, 720, '/', '', '');
		}
	}
	// test chia sẻ banner
	change_body_background();
	// test chia sẻ banner
	
	// xử lý đặc biệt cho chiều cao 2 box tin tức trong ngày - bóng đá thể thao đầu trang
	if (document.getElementById('spec_box_tin_tuc_trong_ngay_bong_da_the_thao_2') && document.getElementById('spec_box_tin_tuc_trong_ngay_bong_da_the_thao_3')){
		v_height_2 = document.getElementById('spec_box_tin_tuc_trong_ngay_bong_da_the_thao_2').offsetHeight;
		v_height_3 = document.getElementById('spec_box_tin_tuc_trong_ngay_bong_da_the_thao_3').offsetHeight;
		// set chiều cao box thấp hơn theo chiều cao box cao hơn
		if (v_height_2 != v_height_3){
			if (v_height_2 > v_height_3){
				document.getElementById('spec_box_tin_tuc_trong_ngay_bong_da_the_thao_3').style.height = (v_height_2 - 2) + 'px';
