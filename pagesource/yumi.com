    function assess_domain() {
        var domain = $("#domain").val();
        var url = '';
        var $loginTips = $("#wttip");
        var $loginTip_s = $("#loginTip_s");
        var data = "domain=" + domain;
        $.ajax({
            url: '/yumi/welcome_controller/domain_assess',
            cache: false,
            dataType: "json",
            data: data,
            type: "POST",
            success: function (json) {
                if (json.state == 1) {
                    var price = json.price;
                    var prices = json.prices;
                    var domain_name = json.domain_name;
                    $loginTips.hide();
                    $("#assess_res").hide();
                    $("#domain").val(domain_name);
                    $("#assessPrice").html(price);
                    $("#assess_price").val(prices);
                    $("#assess_dl").show();
                } else if (json.state == 2) {
                    if (json.sell_form == 'Auction') {
                        url = '<a class="orange f14 f-b" title="' + json.domain_name + '" href="' + json.domain_url + '" target="_blank">域名竞价中，立即去出价</a>'
                    } else if (json.sell_form == 'WxAuction') {
                        url = '<a class="orange f14 f-b" title="' + json.domain_name + '" href="' + json.domain_url + '" target="_blank">微信拍卖中，立即出价</a>'
                    } else if (json.sell_form == 'FixedPrice') {
                        url = '<a class="orange f14 f-b" title="' + json.domain_name + '" href="' + json.domain_url + '" target="_blank">一口价出售中，立即去购买</a>'
                    } else if (json.sell_form == 'backorder') {
                        url = '<a class="orange f14 f-b" title="' + json.domain_name + '" href="' + json.domain_url + '" target="_blank">预订出售中，立即预订</a>'
                    } else {
                        url = '<span class="orange f14 f-b">域名交易中,你可以询价其他域名</span>'
                    }
                    $loginTips.hide();
                    $("#assess_dl").hide();
                    $("#assess_res").html(url);
                    $("#assess_res").show();

                } else {
                    $loginTip_s.html("请输入正确的域名");
                    $loginTips.show();
                }
            }
        });
    }
