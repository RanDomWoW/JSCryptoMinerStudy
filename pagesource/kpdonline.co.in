        function DisplayAgeDetails() {

            /*

            ************************************************

            GETS THE CURRENT YEAR, MONTH, DAY

            ************************************************

            */
            var _today = new Date();
            var _day = _today.getDate();
            var _mon = _today.getMonth() + 1;
            var _yr = _today.getFullYear();

            /*

            ************************************************

            GETS THE DoB YEAR, MONTH, DAY

            ************************************************

            */
            var _dob = new Date();
            _dob = new Date(ddtedtCasteCertificateDt.GetValue());
            var _dobday = _dob.getDate();
            var _dobmon = _dob.getMonth() + 1;
            var _dobyr = _dob.getFullYear();

            /*

            *********************************************************************

            GETS THE AGE IN YEAR, MONTH, DAYS FOR AS ON TODAY

            *********************************************************************

            */
            var _ageyrtoday = _yr - _dobyr;
            var _agemontoday = _mon - _dobmon;
            var _agedaytoday = _day - _dobday;

            if (_agedaytoday < 0) {
                _agemontoday--;
                _agedaytoday = 30 + _agedaytoday;

            }

            if (_agemontoday < 0) {
                _ageyrtoday--;
                _agemontoday = 12 + (_agemontoday);

            }

            /*

            ************************************************

            DISPL. THE AGE IN YEAR, MONTH, DAYS FOR AS ON TODAY

            ************************************************

            */
            dlblFinalAge1.SetValue('Age consider as on date (Today date is :' + _day + '/' + _mon + '/' + _yr + ' ) <br> Your Age is ' + _ageyrtoday + ' Years ' + _agemontoday + ' Months ' + _agedaytoday + ' Days')


            var _baseDt = new Date(2017, 12, 09);
            var _baseday = _baseDt.getDate();
            var _basemon = _baseDt.getMonth() + 1;
            var _baseyr = _baseDt.getFullYear();
            /*

            *********************************************************************

            GETS THE AGE IN YEAR, MONTH, DAYS FOR AS ON BASE DATE

            *********************************************************************

            */
            var _ageyrbase = _baseyr - _dobyr;
            var _agemonbase = _basemon - _dobmon;
            var _agedaybase = _baseday - _dobday;

            if (_agedaybase < 0) {
                _agemonbase--;
                _agedaybase = 30 + _agedaybase;

            }

            if (_agemonbase < 0) {
                _ageyrbase--;
                _agemonbase = 12 + (_agemonbase);

            }

            /*

            *********************************************************************

            DISPL. THE AGE IN YEAR, MONTH, DAYS AS ON BASE DATE

            *********************************************************************

            */
            dlblFinalAge.SetValue('Age consider as on date (Cut Off date   is :' + _baseday + '/' + _basemon + '/' + _baseyr + ' ) <br> Your Age as on base date is ' + _ageyrbase + ' Years ' + _agemonbase + ' Months ' + _agedaybase + ' Days')

        }

