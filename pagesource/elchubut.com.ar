			<script>
				$(document).ready(function() {
			
					$(".encuestas_grafico").each(function() {
						var width = (100 * $(this).attr('data-cont') / $(this).attr('data-votos')).toFixed(1) + "%";
						if (width == 'NaN%')
							width = '0%';
						$(this).css('width', width);
						$(this).parent().siblings().text(width + " - " + $(this).parent().siblings().text());
				    });
			
				    $(".encuesta_enviar").click(function() {
				    	var boton = $(this);
			
				    	$(".novoto." + boton.attr('id')).each(function() {
				    		if ($(".encuesta_checked." + boton.attr('id') + ":checked").length == 1) {
				    			$(this).hide();
				    			var vot = $(".encuesta_checked." + boton.attr('id') + ":checked").val();
				    			var cookie = $(this).attr("data-cookie");
				    			if (cookie == "")
				    				cookie = boton.attr('id');
			    				$.post( "/get-encuestas/vot", { vot: vot, encuesta_id: boton.attr('id'), cookie: cookie }, function(resp) {
			    						getEncuesta(boton.attr('id'));
				    			});
				    		}
				    	});
				    });
			
				    var cookie = get_cookies();
			
				    $(".encuestas.novoto").each(function() {
				    	if (cookie["voto_"+$(this).attr('data-cookie')]) {
				    		$(".encuestas.voto[data-cookie="+$(this).attr('data-cookie')+"]").show();
				    		$(this).remove();
				    	}
				    })
			
				   // iconos_compartir();
			
				});
			
			 function dibujarEncuestas(id) {
			 		$(".encuestas_grafico." + id).each(function() {
						var width = Math.round(100 * $(this).attr('data-cont') / $(this).attr('data-votos') * 100)/100 + "%";
						if (width == 'NaN%')
							width = '0%';
						$(this).css('width', width);
						$(this).parent().siblings().text(width + " - " + $(this).parent().siblings().text());
				    });
			
				    $(".voto." + id).fadeIn('normal');
			 }
			
			 function getEncuesta(id) {
			    $.ajax({
			          url: "/get-encuesta",
			          type: "POST",
			          dataType: "json",
			          data: {id_encuesta: id}
			    }).done(function(encuesta) {
			    	$(".encuestas.voto."+id).remove();
			      	var string = '<div style="display: none" class="encuestas voto ' + encuesta._id + '">';
			
				  	for (var j=0; j< encuesta.r.length; j++ ) {
			  			string = string +
			  			'<div class="encuestas_pregunta">' +
							'<div class="grafico">' +
								'<div class="encuestas_grafico ' + encuesta._id + '" data-votos="' + encuesta.votos + '" data-cont="' + encuesta.r[j].contador + '" data-valor="' + encuesta.r[j].valor + '">'+
									'<label class="encuestas_label">' + encuesta.r[j].label + '</label>' +
									'<label class="encuestas_porcentaje"></label>'+
								'</div>' +
							'</div>' +
						'</div>';
					}
			
					string+= "<span style='display:block; text-align: center; font-size: 13pt; margin-top: 20px'>Cantidad de votos:" +
						  	"<span style='font-weight: bold'>" + encuesta.votos + "</span>" +
						  "</span></div>";
			
					$(".encuestas_container." + id).append(string);
			
					dibujarEncuestas(id);
			    });
			
			}
			
			</script>
			
