    var sliderFgaleria = $('.fgaleria_container').bxSlider({
		minSlides: 1,
		maxSlides: 1,
		moveSlides: 1,
		slideMargin: 20,
		preloadImages: true,
		pagerCustom : "#bxpager_fgaleria",
		//pager: false,
	    onSliderLoad: function (){
			if (typeof $container != 'undefined') $container.packery()
				$('.fgaleria_container > div').each(function(i){
					if(!$(this).hasClass('bx-clone')){
						$("#bxpager_fgaleria").append('<a data-slide-index="'+(i - 1)+'" href=""><li> '+i+'</li></a>');
						
						var img_src = $(this).find('img').attr('src');
						$("#bxfotos_fgaleria").append('<li><a data-slide-index="'+(i - 1)+'" class="pager-img"><img src="'+img_src+'"></a></li>');
					}
				});
		}
    });

    $(document).on("click","a.pager-img",function(e){
    	e.preventDefault();
    	e.stopPropagation();
    	var i_galeria = $(this).data('slide-index');
    	sliderFgaleria.goToSlide(i_galeria);
    	return false;
    });

    $('a.pager-prev').click(function (e) {
    	e.preventDefault();
	    var current = sliderFgaleria.getCurrentSlide();
	    sliderFgaleria.goToPrevSlide(current) - 1;
	});

	$('a.pager-next').click(function (e) {
		e.preventDefault();
	    var current = sliderFgaleria.getCurrentSlide();
	    sliderFgaleria.goToNextSlide(current) + 1;
	});
});
</script>



