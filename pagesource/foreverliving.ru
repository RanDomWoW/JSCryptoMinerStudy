<script>
    function getAuth(good_id) {
      
        $('#wrong_data').hide();
        
        sendParams['user'] = $('#username').val();
        sendParams['password'] = $('#password').val();
        sendParams['action'] = 'authCheck';
        
        $.ajax({
            type: 'POST',
            url: sendParams['URL_AJAX_REQUEST'],
            data: sendParams,
            type: "POST",
            success: function (data) {
                
                var d = JSON.parse(data);
                
                if(d.status==true) {
                    document.location.reload();
                }else{
                    $('#wrong_data').text(d.message);
                    $('#wrong_data').show();
                }
            },
            async:true,
        });
          
    }    
    
    
    function restorePassword() {
      
        sendParams['email'] = $('#email').val();
        
        if(sendParams['email']!='') {
            
            $('#info_data').hide();
            
            sendParams['action'] = 'restorePassword';
            
            $.ajax({
                type: 'POST',
                url: sendParams['URL_AJAX_REQUEST'],
                data: sendParams,
                success: function (data) {
                    $('#info_data').text(data);
                    $('#info_data').show();
                },
                async:true,
            });
        } 
    } 
    
    
    function restoreID() {
      
        sendParams['id'] = $('#idnum').val();
        sendParams['email'] = $('#email_new').val();

        if(sendParams['id']!='') {
            
            $('#info_data2').hide();
            $('#info_data3').hide();
            
            $('#modal-2').removeClass('remind-big');
            
            sendParams['action'] = 'restoreID';
            
            $.ajax({
                type: 'POST',
                url: sendParams['URL_AJAX_REQUEST'],
                data: sendParams,
                success: function (data) {
                    var restore = JSON.parse(data);
                    
                    if(restore.status == 'no_distributor' || restore.status == 'send_email') {
                        $('#info_data2').text(restore.message);
                        $('#info_data2').show();
                    }
                    
                    if(restore.status == 'add_email') {
                        $('#info_data2').text(restore.message);
                        $('#info_data2').show();
                        $('#email_new').show();
                        
                        $('#modal-2').addClass('remind-big');
                    }
                    
                    if(restore.status == 'wrong_distributor' || restore.status == 'wrong_email' || restore.status == 'dublicate_email' || restore.status == 'user_added') {
                        $('#info_data3').text(restore.message);
                        $('#info_data3').show();
                        
                        $('#modal-2').addClass('remind-big');
                    }
                    
                },
                async:true,
            });
        } 
    }
</script>    
