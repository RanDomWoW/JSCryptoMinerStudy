<script language="JavaScript" type="text/javascript">
function hiddenPnl(objId){
     document.getElementById(objId).style.display='none';
}
function showPnl(objId){
     document.getElementById(objId).style.display='';
}

function hiddenBgImg(objId){
    document.getElementById(objId).className=objId;
}
function showBgImg(objId){
    document.getElementById(objId).className=objId+"2";
}

function showImgOne(num,objId1,objId2,objId3){
   switch(num){
           case 1:
		      showBgImg(objId1);
			  hiddenBgImg(objId2);
			  hiddenBgImg(objId3);
			  break;
		   case 2:
		      hiddenBgImg(objId1);
			  showBgImg(objId2);
			  hiddenBgImg(objId3);
		      break;
		   case 3:
		      hiddenBgImg(objId1);
			  hiddenBgImg(objId2);
			  showBgImg(objId3);
		      break;
   } 
}

function showOne(num,objId1,objId2,objId3){
   switch(num){
           case 1:
		      showPnl(objId1);
			  hiddenPnl(objId2);
			  hiddenPnl(objId3);
			  break;
		   case 2:
		      hiddenPnl(objId1);
			  showPnl(objId2);
			  hiddenPnl(objId3);
		      break;
		   case 3:
		      hiddenPnl(objId1);
			  hiddenPnl(objId2);
			  showPnl(objId3);
		      break;
   } 
}

</script>
