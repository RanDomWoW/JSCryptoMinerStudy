<script>
// POST SCRIPT - actions to take after the html is loaded.

	// for header hide/show on scroll
	var dy_old			= 0;
	var scrolldiff		= 50;
	var headobjid		= 'promos';
	var headhideclass	= 'promos-up';
	var headshowclass	= 'promos-down';
//	var headerHeight	= document.getElementById(headobjid).offsetHeight;
	var promoobj		= document.getElementById(headobjid);
	if (promoobj !== null) {
		headerHeight = promoobj.offsetHeight;
	} else {
		headerHeight = 100;
	}

// search validation
function checksearch(f) {
	if (f.q.value == "") {f.q.focus(); return false;} else {return true;}
}

// gmap assist buttons
window.addEventListener('scroll', function(e){
	/* header change */
	var dy = window.pageYOffset || document.documentElement.scrollTop;
	var op = document.getElementById('header');
	if (dy < 1700) {
		if (op.className.search(/\bscrolled\b/gi) !== -1) {
			op.className = op.className.replace(/\bscrolled\b/gi, ' ');
		}
	} else {
		if (op.className.search(/\bscrolled\b/gi) == -1) {
			op.className += ' scrolled';
		}
	}
	/* /header change */
	var mapobj	= document.getElementById('map_canvas');
	if (mapobj !== null) {

		var headh	= 50;
		var mapb		= document.getElementById('gmap_buttons');
		if (mapb == null) return;

		var pagey = window.pageYOffset || document.documentElement.scrollTop;

		var mapy = mapobj.getBoundingClientRect().top + pagey;
		var maph	= mapobj.offsetHeight;

		pagey += headh;

		if (pagey > mapy && pagey < (mapy + (maph - 100) - headh)) {
			mapb.style.position	= 'fixed';
			mapb.style.top			= (headh + 10) +'px';
		} else {
			mapb.style.position	= 'relative';
			mapb.style.top			= '0px';
		}

	}
});

// close menu when clicked away
window.addEventListener('click', function(event){
	if (menu_up || searchshown) {
		if (!event) event = window.event;
		var o = event.target || event.srcElement;
		// if clicked anywhere but 'menu' button and within dropmenu, close the menu.
		if ($(o).closest('#header').length == 0 && $(o).closest('#dropmenu').length == 0) {
			menu_hide();
		}
		// if clicked anywhere but in the search box, close the search box.
		if ($(o).closest('#searchinput').length == 0 && $(o).closest('#header').length == 0) {
			search_hide();
		}
	}
});
</script>





