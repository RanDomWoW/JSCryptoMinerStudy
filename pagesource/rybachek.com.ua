    $(function() {

        $('body').on('click','.tobasket',function(e){
            var iid = $(this).data('iid');
            var price =  $(this).data('price');
            var param = '';
            if( $('#item'+iid+' select.params').length > 0 ){
                $('#item'+iid+' select.params').each( function( index ){
                    param += $(this).val() + '_';
                } );
            }

            
            var qty = 1;
            var send = ko.toJSON( { 'iid':iid, 'qty':qty, 'price':price, 'param':param } );

            $.ajax({
                url:'/tocart/'+iid+'/'+qty,
                type: 'post', cache: false, async: false,
                data : send,
                contentType: 'application/json',
                success: function ( data ) {

                    LocalProccess( data );
                    setLabelCartBtnTimeout(iid, data.cart.lbBasket,'add');
                }
            });
            $('#qty'+iid).remove();

            $('#orderQuick').modal('show');
        });

        $('.viewbasket').live('click', function(){
            $('#orderQuick').modal('show');
        });
    });
    function InitCart()
    {
        $.ajaxSetup( { cache: false });
        $.getJSON("/getcart/", { 't': (new Date).getTime() + Math.random() }, function( allData ) {
            if( clientPhoneTpl.length > 0 && checkPhoneMask == 1 ) $("#quickordertel").mask( clientPhoneTpl );
            cart = new Cart( allData.cart );
            ko.applyBindings(cart, document.getElementById('orderQuick'));
            cart.user( new User( { 'phone':'', 'name':'', 'email':'', 'comment':'', 'address':'' } ) );
        });
    }
</script>

