    <script type="text/javascript">

        function adjustCartHeight() {
            var winHeight = $(window).height();
            var modalTopOffset = parseInt(winHeight * .1);
            var $modalBody = $('#orderQuick').find('.modal-body');

            //        var mHeaderHeight = $('#orderQuick').find('.modal-header').outerHeight();
            //        var mFooterHeight = $('#orderQuick').find('.modal-footer').outerHeight();
            //        var modalHeight = $('#orderQuick').height();

            var modalMaxHeight = winHeight - ( 260 + modalTopOffset );
            modalMaxHeight = Math.min(modalMaxHeight, 400);
            modalMaxHeight = Math.max(modalMaxHeight, 100);
            $modalBody.css({ 'max-height': modalMaxHeight + 'px'})
        }

        $(document).ready(function () {
            adjustCartHeight();
        });

        $(window).on('resize', function () {
            adjustCartHeight();
        });
    </script>
