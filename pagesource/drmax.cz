			$('[data-local-storage-val="kv"]').click();
		} else {
			$('[data-local-storage-val="standard"]').click();
		}
		
		$(document).on('click', '[data-local-storage-val="kv"]', function() {
			$('[for="customer-login"]').html('ID karty');
			$('[for="customer-password"]').html('PIN');
		});
		
		$(document).on('click', '[data-local-storage-val="standard"]', function() {
			$('[for="customer-login"]').html('E-mail');
			$('[for="customer-password"]').html('Heslo');
		});
		
		$('form').submit(function(){
			$('button[type=submit]', this).attr('disabled', 'disabled');
		});
		
		$(document).on('click', '.custom-toggler a', function() {
			var localStorageVal = $(this).data('local-storage-val'); 
			
			$('.custom-toggler a').removeClass('selected');
			$('[data-local-storage-val="' + localStorageVal + '"]').addClass('selected');
			
			localStorage.setItem('loginTab', localStorageVal);
		})
		
		function bindShowPassword() {
			$('.showPasswordToggle').on('click',function(){
				var toggle = $(this);
				toggle.addClass('inited');		
				if ( !toggle.hasClass('hidePassword')) {
					toggle.removeClass('showPassword').addClass('hidePassword');
					toggle.children('i').removeClass('fa-eye').addClass('fa-eye-slash');
					toggle.prev('input').each(function(){
						$(this).prop('type','text').addClass('hidePassword');
						$(this).attr('autocomplete','off');
					});
				} else {
					toggle.removeClass('hidePassword').addClass('showPassword');
					toggle.children('i').addClass('fa-eye').removeClass('fa-eye-slash');
					toggle.prev('input').each(function(){
						$(this).prop('type','password').removeClass('hidePassword');				
					});
				}
				return false;		
			});
		}
		
		
		var re = /(?!:\/\/)([a-zA-Z0-9-]+\.){0,5}[a-zA-Z0-9-][a-zA-Z0-9-]+\.[a-zA-Z]{2,64}?/;
		
	    $(".quote-balloon").not('.pharmacist-quote-balloon').each(function() { 
	        var $this = $(this);
	        var bodyText = $(this).text();
	        $this.html(urlsToLinks(bodyText));
	    });
		
		function tokenize(string) {
		    return string.split(" ");
		}
		
		function linkify(string) {
			var str = string.replace('(', '').replace(')', '');
			
			if (str.substring(str.length-1) == ".") {
		        str = str.substring(0, str.length-1);
		    }
		    
		    if (str == 'dr.max' || str == 'Dr.Max' || str == 'Dr.max') {
		    	return str;
		    }
			
			if (str.indexOf('@') > -1) {
				return '<a href="mailto:' + str + '">' + str + '</a>';
			} else {
		    	return '<a href="' + str + '">' + str + '</a>';
		    }
		}
		
		function linkIfUrl(string) {
		  return !!(re.exec(string)) ? linkify(string) : string;
		}
		
		function urlsToLinks(string) {
		    return tokenize(string).map(linkIfUrl).join(" ");
		}
		
		bindShowPassword();
		
		$(document).on('click', 'circle[class="human-body-part-indicator"]', function() {
			var $lnk = $('#lnk-' + $(this).attr('id'));
			TDI.Ajax.send($lnk);
		});
		
		$(document).on('click', '#js-switch-gender', function() {
			dataLayerEtn.push({
