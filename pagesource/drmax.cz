				"category" : $(this).data("target-gender")
			});
		});
	
		$(document).on('click', 'a.body-parts', function() {
			dataLayerEtn.push({
				"event"    : "body_part",
				"category" : ""
			});
		});
		
		$(function(){
			$('.js-popup').each(function(){
				var $this = $(this);
				
				$this.magnificPopup({
					items: {
		  				type: 'inline',
		  				src: $this.data('js-popup-target')
					}
				});
			});
		});
	});
	
