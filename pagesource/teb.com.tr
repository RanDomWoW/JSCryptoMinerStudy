        } else {

            $('#cokieMain').removeClass('reveal');
            $('body').removeClass('ckPp');
        }
    }

    function closeCookie() {
        $('#cokieContent3 img').click(function () {
            $('#cokieMain').removeClass('reveal');
            setTimeout(function () {
                $('#cokieMain').removeClass('revealCSSPosition');
                $('body').removeClass('ckPp');
            }, 130);
        });
    }

