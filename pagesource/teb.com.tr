    // Cookie
    function initCookie() {

        if (Cookies.get().view != 'tebcomtrV2') {

            Cookies.set('view', 'tebcomtrV2', { expires: 60 });
            $('#cokieMain').addClass('revealCSSPosition').addClass('reveal');
