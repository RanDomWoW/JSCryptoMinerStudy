<script type="text/javascript">
var main_image_current=1;
var max_main_image=7;
var main_image_smooth=7000;
var main_image_timer=null;
var navigator_margin=80;
function auto_main_image_change(){
main_image_current++;
if(max_main_image<main_image_current)main_image_current=1;
change_main_image();
change_navigator();
}
function click_navigator(index){
main_image_current=index;
clearInterval(main_image_timer);
change_main_image();
change_navigator();
main_image_timer=setInterval("auto_main_image_change()", main_image_smooth);
}
function change_main_image(){
$('.main_image_data').stop();
$('.main_image_data').hide();
$('.main_image_data').css('z-index',10);
$('.main_image'+main_image_current).css('opacity',1);
$('.main_image'+main_image_current).css('z-index',20);
$('.main_image'+main_image_current).fadeIn('1500');
}
function change_navigator(){
$('.main_image_navigator').stop();
$('.main_image_navigator_data').each(function(){
var _class=$(this).attr('class');
_class=_class.replace('main_image_navigator_data main_image_navigator','');
var _img=$(this).children('img');
_img.css('z-index',30);
_img.unbind('mouseover');
_img.unbind('mouseout');
var _src=_img.attr('src');
if(_class==main_image_current){
_img.attr('src',_src.replace('_roll_off','_roll_on'));
}else{
_img.attr('src',_src.replace('_roll_on','_roll_off'));
_img.bind('mouseover',function(){
var __src=_img.attr('src');
_img.attr('src',__src.replace('_roll_off','_roll_on'));
});
_img.bind('mouseout',function(){
var __src=_img.attr('src');
_img.attr('src',__src.replace('_roll_on','_roll_off'));
});
}
});
var _margin=0;
if(max_main_image>3){
if(main_image_current>2 && max_main_image>main_image_current){
_margin=main_image_current-2;
}else if(main_image_current>2 && max_main_image==main_image_current){
_margin=main_image_current-3;
}
}
$('.main_image_navigator').animate({marginTop: '-'+(_margin*navigator_margin)+'px'},1000,'easeOutCubic');
}
</script>
