			var num = $('.swiper-main .swiper-container .swiper-wrapper .swiper-slide').find('img').length;
			if (num == 1) {} else {
				var main_swiper = new Swiper('.swiper-main .swiper-container', {
					loop: true,
					autoplay: 4000,
					pagination: '.swiper-pagination',
				});
			}

			//pickup回転
			var picup_swiper = new Swiper('.forPC .pickupArea .swiper-container', {
				slidesPerView: 5,
				slidesPerColumn: 2,
				spaceBetween: 19,
				nextButton: '.swiper-button-next',
				prevButton: '.swiper-button-prev',
				loop: true,
				slidesPerView: 5,
				slidesPerColumn: 2,
				onImagesReady: function() {
					rotate_slide('left');
				},
				onSlideNextStart: rotate_slide('left'),
				onSlidePrevStart: rotate_slide('right')
			});
			var picup_swiper = new Swiper('.forSP .pickupArea .swiper-container', {
				slidesPerView: 4,
				slidesPerColumn: 2,
				spaceBetween: 8,
				nextButton: '.swiper-button-next',
				prevButton: '.swiper-button-prev',
				loop: true,
				loopedSlides: 4,
				onImagesReady: function() {
					rotate_slide('left');
				},
				onSlideNextStart: rotate_slide('left'),
				onSlidePrevStart: rotate_slide('right')
			});
			var mySwiper = new Swiper('.swiper-container-calendar', {
				nextButton: '.swiper-button-next-calendar',
				prevButton: '.swiper-button-prev-calendar',
				initialSlide: 9,
				speed: 400,
				spaceBetween: 100,
				effect: 'fade'
			});
			var mySwiper = new Swiper('.swiper-container-calendar_sp', {
				nextButton: '.swiper-button-next-calendar_sp',
				prevButton: '.swiper-button-prev-calendar_sp',
				initialSlide: 3,
				speed: 400,
				spaceBetween: 100,
				effect: 'fade'
			});
		});

		function rotate_slide(vector) {
			var target = -720;
			if (vector == 'right') {
				target = 720;
			}
			$({
				deg: 0
			}).animate({
				deg: target
			}, {
				duration: 1000,
				// 途中経過
				progress: function() {
					$('.pickupArea .swiper-slide img').css({
						transform: 'rotate(' + this.deg + 'deg)'
					});
				},
			});
		}

	</script>
