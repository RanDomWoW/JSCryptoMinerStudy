                var info = SYMBOLS_UTIL.symbolStr[0];
                if(arrs.length==3){
                    if(coincommonjs41==arrs[1]){
                        newTitle = info+":"+cnyOrUsdSymbol+last+"-"+arrs[2];
                    }else{
                        newTitle = info+":"+cnyOrUsdSymbol+last+"-"+arrs[1]+"-"+arrs[2];
                    }
                }else if(arrs.length==4){
                    newTitle = info+":"+cnyOrUsdSymbol+last+"-"+arrs[2]+"-"+arrs[3];
                }else if(arrs.length==2){
                    newTitle = info+":"+cnyOrUsdSymbol+last+"-"+arrs[0]+"-"+arrs[1];
                }else if(arrs.length==1){
                    newTitle = info+":"+cnyOrUsdSymbol+last+"-"+arrs[0];
                }
