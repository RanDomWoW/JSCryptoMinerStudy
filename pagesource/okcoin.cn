<script>
    var flag = false;
    var site_flag = 1;
    var if_offline=1;

    // 标记首页
    var isExIndexPage = true;
    var indexLangType = '1';
    var IndexLang = {
        quotes: ' Quotes'
    };

    // 资源文件URL
    var PRE_URL = "https://img.bafang.com/v_20180209174940/okcoin" + '/js3.0';

    var siteName = site_flag == 1 ? 'cn' : 'com';

    if (PRE_URL.indexOf("local.") > -1) {
        PRE_URL = 'http://local.okcoin.' + siteName + '/okcoin' + '/js3.0';
    }
    var BASE_URL_AJAX = location.protocol + '//'+ location.host + '/v2';
    if (location.host.indexOf("local") > -1) {
        BASE_URL_AJAX = 'http://localtest.intra.okcoin.' + siteName + '/v2';
    }
    // 全局对象
    var ok = {
        symbol: '',
        currencySymbol: {
            btc: '฿',
            ltc: 'Ł',
            eth: 'E',
            cny: '￥',
            etc: '',
            bch: '',
            bcs: '',
            money: site_flag == 1 ? '￥' : '$'
        },
        allTickersObj: {},
        productsObj: {},
        productsOnlineObj: {},
        symbolToUpper: function (symbol) {
            if (!symbol) {
                return '';
            }
            var arr = symbol.split("_");
            if (arr.length === 1) {
                return arr[0].toUpperCase();
            }
            return arr[0].toUpperCase() + "/" + arr[1].toUpperCase();
        },
        // 价格截位
        priceTruncate: function (symbol) {
            if (ok.marketsProducts[symbol]) {
                return ok.marketsProducts[symbol]['maxPriceDigit']
            }
            return 8;
        },
        // 成交量截位
        amountTruncate: function (symbol) {
            if (ok.marketsProducts[symbol]) {
                return ok.marketsProducts[symbol]['maxSizeDigit']
            }
            return 8;
        },
        // 最小交易单位
        minTradeTruncate: function (symbol) {
            if (ok.marketsProducts[symbol]) {
                return ok.marketsProducts[symbol]['minTradeSize']
            }
            return 0.001;
        },
        getSignByCurrency: function (currency) {
            for (var currencyId in ok.currenciesObj) {
                var currencyObj = ok.currenciesObj[currencyId];
                if (currencyObj && currencyObj.symbol.toLowerCase() === currency && currencyObj.sign) {
                    return currencyObj.sign;
                }
            }
            return '';
        }
    };

    // 更新BTC LTC ETH行情信息
    function updateTickerInfo(data, symbolName) {

        var $section = jQuery('#' + symbolName + 'SpotSection');

        $section.find('.ticker-last').text(formatValue(data.last,site_flag == 1 ? 2 : 3));
        if (data.change > 0) {
            $section.find('.ticker-data').removeClass('red').addClass('green');
            $section.find('.ticker-arrow').removeClass('ticker-arrow-down').addClass('ticker-arrow-up');
        } else {
            $section.find('.ticker-data').removeClass('green').addClass('red');
            $section.find('.ticker-arrow').removeClass('ticker-arrow-up').addClass('ticker-arrow-down');
        }

        if ($section.hasClass('active')) {
            jQuery('#24hrHigh').text(ok.currencySymbol.money + formatValue(data.dayHigh,2));
            jQuery('#24hrLow').text(ok.currencySymbol.money + formatValue(data.dayLow,2));
            if (data.change > 0) {
                jQuery('#24percent').text("+"+formatValue(data.change,2) + '%');
            } else {
                jQuery('#24percent').text(formatValue(data.change,2) + '%');
            }
            jQuery('#24volume').text(ok.currencySymbol[symbolName] + formatValue(data.vol,2));
        }
    }

    // 请求BTC LTC ETH行情信息
    function loadTickerInfo() {
        var random = new Date().getTime();
        var url_ticker = "/real/ticker.do?random=" + random;
        jQuery.post(url_ticker, {symbol: 0}, function (data) {
            if (!data) {
                return;
            }

            updateTickerInfo(data, 'btc');

            updateTitle(data);
        }, "JSON");

        jQuery.post(url_ticker, {symbol: 1}, function (data) {
            if (!data) {
                return;
            }

            updateTickerInfo(data, 'ltc');

        }, "JSON");

        jQuery.post(url_ticker, {symbol: 2}, function (data) {
            if (!data) {
                return;
            }

            updateTickerInfo(data, 'eth');

        }, "JSON");
    }

    loadTickerInfo();

</script>
