<script type="text/javascript">
	var custcallback = function(data) {
		if (data.login) {
			$("#bar_logout").show();
			$("#bar_login").hide();
			$("#bar_entry").hide();
			$("#footer_logout").show();
			$("#footer_login").hide();
		} else {
			$("#bar_logout").hide();
			$("#bar_login").show();
			$("#bar_entry").show();
			$("#footer_logout").hide();
			$("#footer_login").show();
		}
		if (data.ejs) {
			var script = document.createElement('script');
			script.innerHTML = data.ejs;
			document.body.appendChild(script);
		}
		if (data.uasp) {
			$("#toSpBtn").show();
		} else {
			$("#toSpBtn").hide();
		}
		if (typeof(custinfoMethods) !== "undefined") {
			for (var i = 0; i < custinfoMethods.length; i++) {
				custinfoMethods[i](data);
			}
		}
	}

	var script = document.createElement('script');
	script.src = 'https://reebok.jp/pc/secure/cust/custinfo.cgi?type=pc&p=custcallback&t=' + (new Date()).getTime() + (typeof(custinfoParam) === "undefined" ? "" : ("&" + custinfoParam));
	document.body.appendChild(script);
</script>


