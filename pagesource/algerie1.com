  });

} else {

  $.simpleWeather({
    location: 'Alger',
    woeid: '',
    unit: 'c',
    success: function(weather) {
      html = '<span><strong>Alger </strong></span>';
      html += '<span class="weather-deg reg-cold"><i class="icon-'+weather.code+'"></i> '+weather.temp+'&deg;'+weather.units.temp+'</span>';
  
      $("#weather").html(html);
    },
    error: function(error) {
     
    }
  });
}

			});
