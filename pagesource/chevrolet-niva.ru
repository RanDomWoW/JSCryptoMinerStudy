<script type="text/ng-template" id="modal/widget-service-hint.html">
    <div class="modal ">
    <div class="modal-dialog modal-lg">
        <div class="modal-content close">
            
        
    <div class="modal-header">
        <button type="button" class="btn-close close modal-close">
                <svg role="img" class="svg-icon svg-icon_correct svg-icon_inverted modal-close__svg"><use xmlns: xlink="http://www.w3.org/1999/xlink" xlink:href="#close"></use></svg>
        </button>
