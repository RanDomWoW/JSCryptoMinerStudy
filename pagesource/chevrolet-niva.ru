                           href="/uploads/ba741e3e94ba5e4c32c13383f827d529.pdf"
                           target="_blank">
                                <span class="btn__text">Скачать листовку Chevrolet NIVA</span>    <svg role="img" class="svg-icon svg-icon_correct complectations__file-icon"><use xmlns: xlink="http://www.w3.org/1999/xlink" xlink:href="#book"></use></svg></a>
                                    </div> <!-- slice__control -->
                    </div>

        <div class="complectations__list">
            <slick
                class="complectations__tabs"
                settings="tabsSlick.config"
                ng-if="tabsSlick.ready"
                ng-cloak
            >
                <div
                    class="complectations__item"
                    ng-class="{'complectations__item_active' : activeComplectation.Id == complectation.Id}"
                    ng-repeat="complectation in ::complectations"
                    ng-click="setActiveComplectation(complectation)"
                >
                    <div class="complectations__title">[[complectation.Title]]</div>
                    <div class="complectations__price">от [[complectation.getMinPrice() | number:0 ]] <span class="rub">a</span>
                    </div>
                    <div class="complectations__photo-wrap" ng-if="complectation.getPhoto()">
                        <img class="complectations__photo lazyload" 
                             data-src="[[complectation.getPhoto(color, 'smaller')]]"
                             alt="[[ complectation.getTitle() ]]"
                        />
                    </div>

                    <div class="complectations__photo-wrap" ng-if="!complectation.getPhoto(color)">
                                <svg role="img" class="svg-icon svg-icon_correct car-card__svg-nophoto svg-icon_default"><use xmlns: xlink="http://www.w3.org/1999/xlink" xlink:href="#nophoto"></use></svg>
<div class="car-card__text-nophoto">нет фото </div>
                    </div>

                    <div class="complectation__item-arrow"></div>
                </div>
            </slick>
        </div>
    </div>
    <div
        class="complectations__content complectations-review complectations-review_loading loading loading_inverted"
        ng-class="{'complectations-review_loading loading loading_inverted': !tabsSlick.ready}"
    >
        <div class="container has-animations fading animation_slow animation_no-leave" ng-animate-swap="activeComplectation" ng-cloak>
            <div class="complectations-review__head">
                <div class="complectations-review__title">
                    <span class="complectations-review__model-title">[[ activeComplectation.getShortTitle() ]]</span>
                    [[ activeComplectation.Title ]]
                </div>
                <div class="complectations-review__price">от [[ activeComplectation.getMinPrice() | number:0 ]] <span
                        class="rub">a</span></div>
            </div>

            <div class="complectations-review__body">
                <div class="complectations-review__info">
                    <div class="complectations-review__stats complectation-stats">
                        <div class="complectation-stats__list">
                            <div class="complectation-stats__row complectation-stats__row_first">
                                <div class="complectation-stats__title">
                                    Привод
                                </div>
                                <div class="complectation-stats__value">
                                    [[ activeComplectation.Modification.Drivetrain.Id | uppercase ]]
                                </div>
                            </div>

                            <div class="complectation-stats__row complectation-stats__row_even">
                                <div class="complectation-stats__title">
                                    Двигатель
                                </div>
                                <div class="complectation-stats__value">[[
                                    activeComplectation.Modification.Engine.Volume ]] л
                                </div>
                            </div>

                            <div class="complectation-stats__row">
                                <div class="complectation-stats__title">
                                    Трансмиссия
                                </div>
                                <div class="complectation-stats__value">[[
                                    activeComplectation.Modification.Transmission.Title ]]
                                </div>
                            </div>

                            <div class="complectation-stats__row complectation-stats__row_even">
                                <div class="complectation-stats__title">
                                    Мощность
                                </div>
                                <div class="complectation-stats__value">[[ activeComplectation.Modification.Engine.Power
                                    ]] л.с.
                                </div>
                            </div>
                        </div>

                        <div class="complectation-stats__descr-wrap">
                            <div class="complectation-stats__descr">
                                [[ :: description[activeComplectation.Title] ]]
                            </div>
                            <a
                                    target="_self"
                                    href="/chevrolet-niva/trim_levels_and_prices"
                                    class="complectation-stats__btn btn btn_reverse"
                                    ng-if="activeComplectation.Title !== 'SE'"
                            >Подробнее</a>
                            <a
                                    target="_self"
                                    href="/about/news/news-list/vypusk-iubileinoi-chevrolet-niva-special-edition-2017"
                                    class="complectation-stats__btn btn btn_reverse"
                                    ng-if="activeComplectation.Title == 'SE'"
                            >Подробнее</a>
                        </div> <!-- / complectation-stats__descr-wrap -->

                    </div> <!-- / complectations-review__stats complectation-stats -->
                </div> <!-- / complectations-review__info -->

                <media class="complectations-review__media">
                    <div class="complectation-media">
                        <div class="complectation-media__pic">
                            <img
                                class="complectation-media__car has-animations fading"
                                ng-src="[[ getMainPhoto(activeComplectation, '/bundles/chevroletniva2017/images/complectations/') ]]"
                                width="680" height="430"
                            />
                        </div>

                        <div class="complectation-media__tips">
                            <button
                                type="button"
                                class="complectation-media__tip feature-tip"
                                ng-class="{'feature-tip_special': feature.isInterior}"
                                ng-style="featurePos[feature.Id]"
                                ng-if="activeComplectation"
                                ng-repeat="feature in getKillerFeatures(activeComplectation)"
                                uib-popover-html="feature.Title"
                                popover-trigger="mouseenter"
                                popover-placement="auto top"
                                popover-append-to-body="true"
                                popover-class="feature-tip__popover"
                            ></button>
                        </div>
                    </div>
                </media>
            </div> <!-- / row -->
        </div>
    </div>
</section>

    

    
    
