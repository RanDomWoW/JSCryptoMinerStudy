        $pro_first = $('.new-project-list > li:first').addClass('pactive');
        $pro_last = $('.new-project-list > li:last');
        $bul_first = $('.bullet-pane > li:first').addClass('bactive');
        $bul_last = $('.bullet-pane > li:last');
        $pro_first.fadeIn().addClass('pactive');
        $('.bullet-pane > li > a').click(function () {
            $(this).parents('ul').children('li').not($(this).parent()).removeClass('bactive');
            $(this).parent().addClass('bactive')
            $rel = $(this).attr('rel');
            $('.new-project-list').children('li').removeClass('pactive').css("display", "none");
            $('.new-project-list').children('li').eq($rel).fadeIn().addClass('pactive');
            return false;
        })
        /*------------------------------*/
        setInterval(function () {
            $pactive = $('.new-project-list > li.pactive');
            $bactive = $('.bullet-pane  > li.bactive');
            if ($pro_last.hasClass('pactive')) {
                $pactive.removeClass('pactive').css("display", "none");
                $pro_first.fadeIn().addClass('pactive');
                $bactive.removeClass('bactive');
                $bul_first.addClass('bactive');
            } else {
                $pactive.removeClass('pactive').css("display", "none").next().fadeIn().addClass('pactive');
                $bactive.removeClass('bactive').next().addClass('bactive');
            }
        }, 7000)
