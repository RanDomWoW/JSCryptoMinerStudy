    <script type="text/javascript">
        var duration = 0;
        var delay = - 9000;

        setInterval(function () {
            var d = new Date(1397 , 01 , 04 , 07 , 10 , 24 , 592);
                d.setSeconds(d.getSeconds() + duration + delay);

                duration = duration + 1;
                var time = document.getElementById('lbltime')
                var hh = d.getHours();
                var m = d.getMinutes();
                var s = d.getSeconds();
                var dd = "AM";
                var h = hh;
                if (h >= 12) {
                    h = hh - 12;
                    dd = "PM";
                }
                if (h == 0) {
                    h = 12;
                }
                m = m < 10 ? "0" + m : m;
                s = s < 10 ? "0" + s : s;
                h = h < 10 ? "0" + h : h;
                time.innerHTML = h + ':' + m + ':' + s + ' ' + dd;


            }, 1000);
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('.farsi-num').each(function () {
                $(this).html(Convertnumber2farsi($(this).html()));
            });
        });

        function Convertnumber2farsi($srting) {
            $num0 = "۰";
            $num1 = "۱";
            $num2 = "۲";
            $num3 = "۳";
            $num4 = "۴";
            $num5 = "۵";
            $num6 = "۶";
            $num7 = "۷";
            $num8 = "۸";
            $num9 = "۹";

            $stringtemp = "";
            $len = $srting.length;
            for ($sub = 0; $sub < $len; $sub++) {
                if ($srting.substr($sub, 1) == "0")
                    $stringtemp = $stringtemp + $num0;
                else if ($srting.substr($sub, 1) == "1")
                    $stringtemp = $stringtemp + $num1;
                else if ($srting.substr($sub, 1) == "2")
                    $stringtemp = $stringtemp + $num2;
                else if ($srting.substr($sub, 1) == "3")
                    $stringtemp = $stringtemp + $num3;
                else if ($srting.substr($sub, 1) == "4")
                    $stringtemp = $stringtemp + $num4;
                else if ($srting.substr($sub, 1) == "5")
                    $stringtemp = $stringtemp + $num5;
                else if ($srting.substr($sub, 1) == "6")
                    $stringtemp = $stringtemp + $num6;
                else if ($srting.substr($sub, 1) == "7")
                    $stringtemp = $stringtemp + $num7;
                else if ($srting.substr($sub, 1) == "8")
                    $stringtemp = $stringtemp + $num8;
                else if ($srting.substr($sub, 1) == "9")
                    $stringtemp = $stringtemp + $num9;
                else $stringtemp = $stringtemp + $srting.substr($sub, 1);
            }
            return $stringtemp;
        }
    </script>
