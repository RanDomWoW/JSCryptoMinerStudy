<meta content='https://2.bp.blogspot.com/-2P5TUyhpcFQ/WqJ39e-yr3I/AAAAAAABqo8/8tlg8HQt2ykCuQ2KaNF-UkrmsLzm9SkUwCLcBGAs/s1600/Afghanistan_Flag.jpg' itemprop='image_url'/>
<meta content='364573257508940376' itemprop='blogId'/>
