                                                <div ng-show="user_registration.user_phone.$error.pattern">Questo non è un numero valido.</div>
                                            </div>
                                        </div>
                                    </div>
                                </div> <!-- row-tablet -->
                                <div class="row-tablet">
                                    <div class="cell-tablet">
