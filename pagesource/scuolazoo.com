                                            <div class="error" ng-show="!user_registration.user_lastname.$untouched && !user_registration.user_lastname.$valid">
                                                <div ng-show="user_registration.user_lastname.$error.required">Questo campo è richiesto.</div>
                                            </div>
                                        </div>
                                    </div>
                                </div> <!-- row-tablet -->
                                <div class="row-tablet">
                                    <div class="cell-tablet">
                                        <div class="campo-input">
                                            <input type="email" placeholder="Email*" name="user_email" ng-model="user.email" required>
                                            <div class="error" ng-show="!user_registration.user_email.$untouched && !user_registration.user_email.$valid">
