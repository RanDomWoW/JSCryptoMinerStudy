    //从头执行倒计时，将结束时间写入localStorage
    function mtgSmsCountdown(obj, type) {
        //300代表倒计时有300秒
        switch (type)
        {
            case 'reg':
                localStorage.smsExpireDate1 = Date.parse(new Date()) / 1000 + 120;
                break;
            case 'forget':
                localStorage.sms = Date.parse(new Date()) / 1000 + 120;
                break;
        }


        $(obj).attr("disabled", "disabled");
        mtgSmsCompareDate(obj, type);
        mtgSmsInterval = window.setInterval(function () {
            mtgSmsCompareDate(obj, type)
        }, 1000);
    }

    //每秒更新倒计时时间
    function mtgSmsCompareDate(obj, type) {
        switch (type)
        {
            case 'reg':
                var mtgSmsRemainTime = parseInt(localStorage.smsExpireDate1) - Date.parse(new Date()) / 1000;
                break;
            case 'forget':
                var mtgSmsRemainTime = parseInt(localStorage.sms) - Date.parse(new Date()) / 1000;
                break;
        }

        $(obj).text(mtgSmsRemainTime + "秒");

        if (mtgSmsRemainTime < 1) {
            $(obj).removeAttr("disabled");
            $(obj).text("重新发送");
            localStorage.clear();
            clearInterval(mtgSmsInterval);
        }
    }

    //初始化检查，如果localStorage中已经存在倒计时，则继续倒计时
    function mtgSmsInitCheck(obj, type) {
        switch (type)
        {
            case 'reg':
                var remaining = localStorage.smsExpireDate1;
                break;
            case 'forget':
                var remaining = localStorage.sms;
                break;
        }
        if (remaining) {
            if (parseInt(remaining) - Date.parse(new Date()) / 1000 > 1) {
                mtgSmsCompareDate(obj, type);
                mtgSmsInterval = window.setInterval(function () {
                    mtgSmsCompareDate(obj, type)
                }, 1000);
                $(obj).attr("disabled", true);
            }
        }
    }

    $(function () {
        //倒计时初始化检查
        mtgSmsInitCheck(document.getElementById('regCode'), 'reg');
        mtgSmsInitCheck(document.getElementById('forgetCode'), 'forget');
    })


    //获取cookie
    function getCookie(cookieName) {
        var cookieValue = "";
        if (document.cookie && document.cookie != '') {
            var cookies = document.cookie.split(';');
            for (var i = 0; i < cookies.length; i++) {
                var cookie = cookies[i].trim();

                if (cookie.substring(0, cookieName.length + 1).trim() == cookieName.trim() + "=") {
                    cookieValue = cookie.substring(cookieName.length + 1, cookie.length);
                    break;
                }
            }
        }
        return cookieValue;
    }

    // 时间错转日期
    function formatDateTime(timeStamp, format) {
        var date = new Date();
        date.setTime(timeStamp * 1000);
        var y = date.getFullYear();
        var m = date.getMonth() + 1;
        // m = m < 10 ? ('0' + m) : m;
        var d = date.getDate();
        // d = d < 10 ? ('0' + d) : d;
        var h = date.getHours();
        h = h < 10 ? ('0' + h) : h;
        var minute = date.getMinutes();
        var second = date.getSeconds();
        minute = minute < 10 ? ('0' + minute) : minute;
        second = second < 10 ? ('0' + second) : second;
        if (format == 'y-m-d') {
            return y + '-' + m + '-' + d;
        } else if (format == 'm-d-h-m') {
            return m + '-' + d + ' ' + h + ':' + minute;
        } else if (format == 'h-m') {
            return h + ':' + minute;
        } else if (format == 'cn-m-d') {
            return m + '月' + d + '日';
        } else {
            return m + '-' + d;
        }
    };

    // 根据日期计算本年第几周
    function weekOfYear(date){
        var dateArray = date.split('-');
        var year = dateArray[0];
        var month = dateArray[1];
        var day = dateArray[2];
        var date1 = new Date(year, 0, 1);
        var date2 = new Date(year, month - 1, day, 1);
        var dayMS = 24 * 60 * 60 * 1000;
        var firstDay = (8 - date1.getDay()) * dayMS;
        var weekMS = 7 * dayMS;
        date1 = date1.getTime();
        date2 = date2.getTime();
        return Math.ceil((date2 - date1 - firstDay)/weekMS) + 1;
    }

    // 计算日期是周几
    function dayOfWeek(dateStr) {
        var weekDay = ["周日", "周一", "周二", "周三", "周四", "周五", "周六"];   
        var myDate = new Date(Date.parse(dateStr.replace(/-/g, "/")));  
        return weekDay[myDate.getDay()];
    }
    
    // 根据时间返回本周还是下周
    function week(time, obj) {
        var now_date = formatDateTime(Math.round(new Date() / 1000), 'y-m-d');
        var forecast_date = formatDateTime(time, 'y-m-d');
        var now_week = dayOfWeek(now_date);// 本地时间周几
        var forecast_week = dayOfWeek(forecast_date);// 时间戳周几
        var now_n_week = weekOfYear(now_date);// 本地时间第几周
        var forecast_n_week = weekOfYear(forecast_date);// 时间戳第几周
        if (forecast_n_week + 1 == now_n_week) {//上周
            if (now_week == forecast_week + 1) {
                return '昨天';
            } else {
                return formatDateTime(time, 'cn-m-d');
            }
        } else if (forecast_n_week == now_n_week) {//本周
            if (now_week == forecast_week) {
                return '今天';
            } else if(now_week == forecast_week + 1) {
                return '昨天';
            } else if(now_week > forecast_week + 1) {
                return formatDateTime(time, 'cn-m-d');
            } else {
                return '本' + forecast_week;
            }
        } else if(forecast_n_week == now_n_week + 1) {//下周
            return '下' + forecast_week;
        } else {
            // 如果显示日期，则去掉首播前的日期
            $(obj).siblings('.txt').find('.formatTime').remove();
            return formatDateTime(time, 'cn-m-d');
        }
    }

    // 把时间戳替换成日期
    $(".formatTime").each(function () {
        var time = $(this).text();
        var formatdate = $(this).attr("data-format");
        $(this).text(formatDateTime(time, formatdate));
    });

    // 把时间戳替换成星期
    $(".formatWeek").each(function(index,element) {
        $(this).text(week($(this).text(), element));
    });
    
    function everyDay (name, fn) {
        if (name === undefined) {
            throw new Error ('name does not exist!');
        }

        function getTodayTimeStamp () {
            return new Date().setHours(0, 0, 0, 0)
        }

        function setCache () {
            localforage.setItem(name, getTodayTimeStamp(), function (err, val) {
                if (err) {
                console.error(err);
                }
            })
        }

        var onOff = false;
            localforage.getItem(name, function (err, val) {
        if (err) {
            console.error(err);
        } else {
            if (val) {
                if (val < getTodayTimeStamp()) {
                    onOff = true;
                    setCache()
                }
            } else {
                onOff = true;
                setCache()
            }

            fn(onOff);
        }
        })
    }
</script>

