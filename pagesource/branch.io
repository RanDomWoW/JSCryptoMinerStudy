			<script type="text/javascript">

	const project = "main-website";

	$(function(){

		// Tracks every pageview
		trackEvent('pageview');

		self.scrolledFlags = new Array();

		$("[data-event-type='click'],a[data-element-tag]").each(function(index) {
			$(this).on("click", function(){
				trackEvent("click", $(this));
			});
		});

		$("[data-event-type='form-submit']").each(function(index) {
			$(this).on("submit", function(){
				trackEvent("form-submit", $(this));
			});
		});
	});

	$(window).scroll(function() {
		$("[data-event-type='scroll'],div[data-element-tag],section[data-element-tag]").each(function(index) {
			var hT = $(this).offset().top,
				hH = $(this).outerHeight(),
				wH = $(window).height(),
				wS = $(self).scrollTop();
			if (wS > (hT+hH-wH)){
					if (self.scrolledFlags[index] === undefined) {
						trackEvent("scroll",$(this));
						self.scrolledFlags[index] = true;
					}
			}
		});
	});

	function trackEvent(actionType, element){

		var mpTrackingOptions = {
			"project":project,
			"page":"/"
		};

		var gaTrackingOptions = {
			"hitType":"event",
			"eventAction":actionType
		};

		if (actionType != "pageview") {
			var parent = getParentType(element);
			mpTrackingOptions["parent"] = parent;
			if (element.data('element-type') == null) {
				if (element.is("a")) mpTrackingOptions["type"] = 'link';
				if (element.is("section") || element.is("div")) mpTrackingOptions["type"] = 'content-section';
			} else {
				mpTrackingOptions["type"] = element.data('element-type');
			}
			gaTrackingOptions["eventCategory"] = mpTrackingOptions["type"];
		}

		if (element && element.data('element-tag')) {
			let tagString = element.data('element-tag');
			mpTrackingOptions["tags"] = tagString;
			gaTrackingOptions["eventLabel"] = tagString;
		}

		if (actionType == "click") {
			var innerText = element.text();
			if(innerText.length > 180) innerText = innerText.substring(0,180);
			mpTrackingOptions["label"] = innerText;
			ga('set', 'dimension6', innerText);
		}

		var mpaction = 'mw-' + actionType;
		mixpanel.track( mpaction, mpTrackingOptions);

		if (actionType != "pageview") {
			ga('send', gaTrackingOptions);
  		}	
	}

	/*
	- Checks whether this is in nav or a footer
	- Checks whether it is a section.
		- If has ID, parent=id
	- Checks whether it has a section for a parent
		- If has ID, parent=id
	*/
	function getParentType(element) {
		var parent = "body";
		if (element.parents('nav.navbar').length > 0){parent = "nav"}
		else if (element.parents('footer').length > 0){parent = "footer"}
		else if (element.is('section')){
			if (element[0].id) { parent = element[0].id; }
		}
		else if (element.parents('section').length > 0){
			let parentSection = element.parents('section')[0];
			if (parentSection.id) { parent = parentSection.id; }
		}
		return parent;
	}

</script>
		
