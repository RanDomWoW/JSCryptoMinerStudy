<script type="text/javascript">
  $('.copyright-year').html(new Date().getFullYear());

  $(function() {
  //Accordion Menu Manager
    bdsAccordianMenu = {
      model : {
        animationInProgress : false
      },

      init : function() {
        var flag = false;
        $('.bds-accordian-heading').bind('touchstart click', function(){
          if (!flag) {
            flag = true;
            setTimeout(function(){ flag = false; }, 100);

            if($(window).width() <=991){
              // console.log("calling expand collapse");
              bdsAccordianMenu.expandCollapse($(this));
            }
          }
          return false
        });
      },

      expandCollapse : function(obj) {
        if (bdsAccordianMenu.model.animationInProgress) return false;
        bdsAccordianMenu.model.animationInProgress = true;

        var targetDiv = obj.next('.bds-accordian-list');

        if (obj.closest('.bds-accordian-menu').hasClass('show-menu')) {
          // have collapse animation happen first, then remove show menu
          targetDiv.slideUp(250,'linear',function() {
          targetDiv.attr('style','');
            bdsAccordianMenu.model.animationInProgress = false;
            obj.closest('.bds-accordian-menu').removeClass('show-menu');
          });
        }
        else {
          // console.log('does not have class show-menu. adding...');
          obj.closest('.bds-accordian-menu').addClass('show-menu');
          targetDiv.attr('style','display:none;');
          targetDiv.slideDown(250,'swing');

          $('html, body').animate({scrollTop: obj.offset().top - 62}, 500, 'swing', function() {
            bdsAccordianMenu.model.animationInProgress = false;
          });

        }
      }

    }; /*end bdsAccordianMenu obj*/

    var bds_accordian_initiated=false;

    function bds_initialize_accordian(){
      if($(window).width() <=991){
        if(bds_accordian_initiated==false){
          bds_accordian_initiated=true;
          bdsAccordianMenu.init();
        }
        else{
          return 0;
        }
      }
    }

    /* fixes menu when switching orientation on tablet */
    $(window).bind("orientationchange", function(evt){
      bds_initialize_accordian();
    });

    bds_initialize_accordian();

  });
</script>

