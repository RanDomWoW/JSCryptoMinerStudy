var meetupsShown = 0;
$('.branch-events').each(function(index){
    var dateLabel = $(this).find('.short-date');
    var mydate = $(this).find('.event-date');
    var eventDate = Date.parse(dateLabel.text());
    var today = Date.today();
    var diff = (eventDate-today)/86400000;

    if(diff<0 || meetupsShown >= 4)
      $(this).remove();
    else{
      if(diff>=0){
        if(diff<=3)
          $(mydate).addClass('bds-txt-red');
        if(diff>3 && diff<=7)
          $(mydate).addClass('bds-txt-orange');
        if(diff>7)
          $(mydate).addClass('bds-txt-green');
        meetupsShown++;
      }
    }
});


