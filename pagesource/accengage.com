#content .avada-row { max-width:1200px!important }

.acc-hp-section-space { margin-top:30px; }

#nico .hs-input[type=submit]{
	width:90%!important;
	padding:10px 30px 10px 30px!important;
	border-radius:4px;
	color:#FFF;
	font-weight:200;
	background:#3FA9F5;
	background-image:linear-gradient(to left, #3FA9F5, #1d71b8)!important;
	text-shadow:none!important;
	height:auto!important;
	font-family:'Open Sans Condensed', Arial, Helvetica, sans-serif !important;
	margin-top:15px;
	margin-bottom:15px;
	font-size:20px!important;
}
#nico .hs-input[type=submit]:hover {
	border:1px solid #1d71b8!important;
	color:#1d71b8!important;
	font-weight:normal!important;
	background-image:none!important;
	background:none!important;
	padding:9px 29px 9px 29px!important;
}

#nico .hs-label { 
	display:none!important;
}

#nico .hs-input[type=text], #nico .hs-input[type=email], #nico .hs-input[type=tel] {
	height:29px;
	padding-left:15px;
}
#nico select {
	height:29px!important;
	width:90%!important;
	padding-left:5px;
}
#nico .hs-textarea {
	padding-left:5px;
	width:90%!important;
}
#nico .hs-input {
	margin-bottom:8px;
}
#nico .hs-input.invalid.error {
	border-color:#c87872;
}
#nico .hs-error-msgs.inputs-list {
	display:none !important;
}
#nico .hs-button[type=submit] {
	border:none !important;
}
#nico .hs-button[type=submit]:hover {
	cursor:pointer !important;
}
#nico .hbspt-form {
	padding-top:10px !important;
}

#nico .hs-form-field {
	width:90%!important;
	margin-left: auto;
	margin-right: auto;
}

#nico select, #nico .hs-input[type=text], #nico .hs-input[type=email], #nico .hs-input[type=tel] {
    height:29px;
    width:100%!important;
	-ms-box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box; 
	box-sizing:border-box;
}

#nico select { padding-left:0px!important; }
#nico .hs-input[type=text], #nico .hs-input[type=email], #nico .hs-input[type=tel] {padding-left:5px!important;}


.acc-hp-push-card {
	margin-top:40px;
}

.acc-hp-push-card img {
	max-width:122px;
	height:auto;
	margin-bottom:15px;
}

.acc-hp-push-card a {
	position:relative;
	top:0px;
	-webkit-transition:all 0.3s ease;
	transition:all 0.3s ease;
	text-shadow: 0px 0px 3px black !important;
	color:#FFFFFF;
}

.acc-hp-push-card a:visited {
	color:#FFFFFF;
}

.acc-hp-push-card a:hover {
	color:#0076bd;
	top:-10px;
}


/* ******** PICTOS ******** **/
@media only screen and (max-width: 350px) {
	.acc-hp-push-card {
		width: 100%;
		margin-left: none;
		margin-right: none;
	}
}

@media only screen and  (min-width: 350px) and (max-width: 600px) {
	.acc-hp-push-card {
		width: 40%;
		margin-left: 5%;
		margin-right: 5%;
	}
}

@media only screen and (max-width: 800px) and (min-width: 600px) {
	.acc-hp-push-card {
		width: 20%;
		margin-left: 2%;
		margin-right: 3%;
	}
}





/* ******** NUMBERS ******** **/
@media only screen and (max-width: 800px) {
	.counters-box .counter-box-wrapper {
	    margin: 0 0 0px!important;
	    padding: 20px;
	    width: 100%;
	}
}


/* ******** FREE DEMO ******** **/
@media only screen and (max-width: 440px) {
	#disp > a.freedemo-big::before {
		content: '';
	}
}

@media only screen and (min-width: 440px) {
	#disp > a.freedemo-big::before {
		content: 'REQUEST A ';
	}
}


