<script type="text/javascript">
	var rolloverImages = new Array();
	rolloverImages.push(selected_menu);
	
	$(function(){
		$('#search_information_tv .tab_content div').eq(1).hide();
		if($.cookie('search_tab')){
			$('#search_information_tv .tab_content div').hide();
			$($.cookie('search_tab')).show();
		}
		$('#search_information_tv .tab_content ul a').click(function(){
			$('#search_information_tv .tab_content div').hide();
			$($(this).attr('href')).show();
			$.cookie('search_tab', $(this).attr('href'));
			
			(function(){
				$(this.weekday).val('');
				$(this.date).val('');
				$('.datepicker:first', this).val('');
			}).apply($('#program_search_form').get(0));
			
			return false;
		});
		$.timepicker.regional['ja']['timeOnlyTitle'] = "時刻を選択";
		$.timepicker.setDefaults($.timepicker.regional['ja']);
		$('#search_information_tv .datepicker').datepicker({
			dateFormat: "yy-mm-dd"
		});
		$('#search_information_tv .timepicker').timepicker({
			timeFormat: "hh:mm"
		});
	});
	
	$(function(){
		restore_cond($('#program_search_form').get(0));
		$('#program_search_form').submit(function(){
			save_cond(this);
		});
		$('#btn_clear_program_search_form').click(function(){
			clear_cond(this);
		});

		restore_keyword($('#keyword_search_form').get(0));
		$('#keyword_search_form').submit(function(){
			save_keyword(this);
		});

	});
	
	function save_cond( form ){
		$.cookie('search_pref', $(form.pref).val(), { path: "/" });
		$.cookie('search_date', $(form.date).val(), { path: "/" });
		$.cookie('search_weekday', $(form.weekday).val(), { path: "/" });
		$.cookie('search_time', $(form.time).val(), { path: "/" });
		$.cookie('search_genre', $(form.genre).val(), { path: "/" });
	}
	function restore_cond( form ){
		$(form.pref).val($.cookie('search_pref')||"");
		$(form.date).val($.cookie('search_date')||"");
		$(form.weekday).val($.cookie('search_weekday')||"");
		$(form.time).val($.cookie('search_time')||"");
		$(form.genre).val($.cookie('search_genre')||"");
	}
	function clear_cond( form ){
		$.cookie('search_pref', '');
		$.cookie('search_date', '');
		$.cookie('search_weekday', '');
		$.cookie('search_time', '');
		$.cookie('search_genre', '');
	}
	function save_keyword( form ){
		$.cookie('search_keyword', $(form.keyword).val(), { path: '/' });
	}
	function restore_keyword( form ){
		$(form.keyword).val($.cookie('search_keyword')||"");
	}
	function clear_keyword( form ){
		$.cookie('search_keyword', '', { expires: -1 });
	}
	
	$(function(){ $(document).bind("contextmenu",function(e){ return false; }); });
	
</script>

