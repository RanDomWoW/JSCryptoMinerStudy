<script type="text/javascript">
    $j(document).ready(function() {
        newsletterObj = {
            VIEW_COOKIE: 'newsletter_viewed',
            PAGES_VIEWED_COOKIE: 'unique_page_view_list',
            EURO_PAGE_VIEW_TRIGGER: 3,

            run: function() {
                this.incrementUniquePageCount();

                if(this.canDisplay()) {
                    this.display();
                }
            },

            display: function() {
                $j('body').append('<section class="pop-overlay news-overlay" />');

                if(!isMobileMode()) {
                    $j('.news-overlay').show();
                }

                $j('.newsletter-popup').show();
                this.markNewsletterAsSeen();
            },

            close: function() {
                if(!isMobileMode()) {
                    $j('.news-overlay').remove();
                }
                $j('.newsletter-popup').hide();
            },

            isEuroSite: function() {
                // Global variable set in page/html/head.phtml
                return SITE_IS_EUROPEAN_NOT_FRENCH;
            },

            incrementUniquePageCount: function() {
                /*if(!this.isEuroSite()) {
                    return;
                }*/

                var pagesSeen = Mage.Cookies.get(this.PAGES_VIEWED_COOKIE);

                if(!pagesSeen) {
                    pagesSeen = [];
                } else {
                    // Convert to an array
                    pagesSeen = decodeURI(pagesSeen);
                    pagesSeen = pagesSeen.split(',');
                }

                if(pagesSeen.indexOf(window.location.pathname) !== -1) {
                    // Page already seen so nothing else to do here
                    return;
                }

                if(pagesSeen.length >= this.EURO_PAGE_VIEW_TRIGGER) {
                    // We are at our trigger point, no need to store any more in the cookie
                    return;
                }

                pagesSeen.push(window.location.pathname);

                // Convert to string
                pagesSeen = pagesSeen.join(',');
                pagesSeen = encodeURI(pagesSeen);

                Mage.Cookies.set(this.PAGES_VIEWED_COOKIE, pagesSeen);
            },

            markNewsletterAsSeen: function() {
                var expireAt = new Date();
                expireAt.setDate(expireAt.getDate() + 360);
                Mage.Cookies.set(this.VIEW_COOKIE, true, expireAt);
            },

            hasNewsletterBeenSeen: function() {
                return (Mage.Cookies.get(this.VIEW_COOKIE) == 'true');
            },

            isNPageVisit: function(n) {
                var pagesSeen = Mage.Cookies.get(this.PAGES_VIEWED_COOKIE);

                if(pagesSeen) {
                    pagesSeen = pagesSeen.split(',');
                    return (pagesSeen.length === n);
                } else {
                    return false;
                }
            },

            canDisplay: function() {
                /*if(this.isEuroSite()) {
                    if(this.isNPageVisit(this.EURO_PAGE_VIEW_TRIGGER)) {
                        // If 4th unique page visit && newsletter hasn't been seen
                        return !this.hasNewsletterBeenSeen();
                    } else {
                        return false;
                    }
                } else {
                    return !this.hasNewsletterBeenSeen();
                }*/
                if(this.isNPageVisit(this.EURO_PAGE_VIEW_TRIGGER)) {
                    // If 4th unique page visit && newsletter hasn't been seen
                    return !this.hasNewsletterBeenSeen();
                } else {
                    return false;
                }
            }
        };

        var nlForm = new VarienForm('newsletter-form-id', true);
        $j('#newsletter-form-id .form-btn').on('click', function(){
            if(nlForm.validator.validate()){
                $j.ajax({
                    type: "POST",
                    url: "https://www.diptyqueparis.com/newsletter/subscriber/newPopupAjax/",
                    dataType: "json",
                    data: $j('#newsletter-form-id').serialize(),
                    success: function(_result){
                        if(_result.success) {
                            if(_result.floodlight_url){
                                var a = axel * 10000000000000;
                                $j('body').prepend('<iframe src="' + _result.floodlight_url + 'ord=' + a + '?" width="1" height="1" frameborder="0" style="display:none"></iframe>');
                            }
                            newsletterObj.close();
                        } else {
                            alert(_result.message);
                        }
                    }
                });
            }
            return false;
        });

        $j('.newsletter-popup .close-popup').on('click', function() {
            newsletterObj.close();
        });

        newsletterObj.run();
    });
</script>

