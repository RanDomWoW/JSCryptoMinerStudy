	//Arranca Validación Header Form Ofertas por Mail
	//Detectamos si hacen clic en "¡Registrate!"
	$("#navbar #register_header #register_button, .registrar_email input[type='submit']").on( 'click' , function(){
		
		//Creamos la función que valida si un input tiene o no un email
		function validateEmail(email) { 	  
	    var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
	    return re.test(email);
		}

		//Creamos la función con el if que determina qué hacemos cuando sepamos si es o no un email.
		function validate(){
		  $("#navbar #register_header #register_button").text("");
		  
		  var email = $("#email_header").val();
		  
		  if (validateEmail(email)) {
		    window.location = "https://www.supermercadosdia.com.ar/recibi-ofertas-en-tu-email/";
		  } else {
		  	$( "#tg_validate_error" ).html( "<span style='color:white;'>Ingrese una dirección de email válida.</span>" );
		  }

		  return false;
		}

		//Lo mismo para el form de ofertas
		function validate_ofer(){
		  $(".registrar_email input[type='submit']").text("");

		  
		  ofer_email = $(".registrar_email input[type='submit']").parent().siblings("span[class*='email']").children('input');
		  var email = $(ofer_email).val();
		  
		  if (validateEmail(email)) {
		    window.location = "https://www.supermercadosdia.com.ar/recibi-ofertas-en-tu-email/";
		  } else {
		  	$( ".registrar_email h6" ).html( "<span style='color:#d52b1e; text-align:right; padding-left:10px;'>Ingrese una dirección de email válida.</span>" );
		  }

		  return false;
		}


		$("#masthead form").bind("submit", validate);
		$(".registrar_email form").bind("submit", validate_ofer);
		
	});
