              mensaje = "Longitud Incorrecta";
          }
      }

      //Validación: debe comenzar con 280
      if (resultado) {
          if (codigoIngresado.substring(0, 3) != "280") {
              resultado = false;
              mensaje = "Prefijo Incorrecto";
          }
      }

      //Validación: primer dígito verificador
      if (resultado) {
          val4ponderado = parseInt(codigoIngresado[3]) * 2;
          val5ponderado = parseInt(codigoIngresado[4]) * 3;
          val6ponderado = parseInt(codigoIngresado[5]);
          val7ponderado = parseInt(codigoIngresado[6]) * 2;
          val8ponderado = parseInt(codigoIngresado[7]) * 3;
          val9ponderado = parseInt(codigoIngresado[8]);
          val10ponderado = parseInt(codigoIngresado[9]) * 2;
          val11ponderado = parseInt(codigoIngresado[10]) * 3;

          totalponderado = val4ponderado + val5ponderado + val6ponderado +
                       val7ponderado + val8ponderado + val9ponderado +
                       val10ponderado + val11ponderado;

          totalredondeado = Math.floor((totalponderado + 9) / 10) * 10;

          digitoverificador = totalredondeado - totalponderado;

          if (digitoverificador != parseInt(codigoIngresado[11])) {
              resultado = false;
              mensaje = "Verificador incorrecto";
          }
      }

      //Validación: segundo dígito verificador
      if (resultado) {
          val1ponderado = parseInt(codigoIngresado[0]);
          val2ponderado = parseInt(codigoIngresado[1]) * 3;
          val3ponderado = parseInt(codigoIngresado[2]);
          val4ponderado = parseInt(codigoIngresado[3]) * 3;
          val5ponderado = parseInt(codigoIngresado[4]);
          val6ponderado = parseInt(codigoIngresado[5]) * 3;
          val7ponderado = parseInt(codigoIngresado[6])
          val8ponderado = parseInt(codigoIngresado[7]) * 3;
          val9ponderado = parseInt(codigoIngresado[8]);
          val10ponderado = parseInt(codigoIngresado[9]) * 3;
          val11ponderado = parseInt(codigoIngresado[10]);
          val12ponderado = parseInt(codigoIngresado[11]) * 3;

          totalponderado = val1ponderado + val2ponderado + val3ponderado +
                       val4ponderado + val5ponderado + val6ponderado +
                       val7ponderado + val8ponderado + val9ponderado +
                       val10ponderado + val11ponderado + val12ponderado;

          totalredondeado = Math.floor((totalponderado + 9) / 10) * 10;

          digitoverificador = totalredondeado - totalponderado;

          if (digitoverificador != parseInt(codigoIngresado[12])) {
              resultado = false;
              mensaje = "Verificador Incorrecto";
          }

      }

      return resultado;
  	}

