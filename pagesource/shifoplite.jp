<script>

$(function(){
	//スライダー
	$('.bxslider').bxSlider({
	  auto: true,//自動切り替えの有無
	  pause:3500,//停止時間※デフォルトは4000
	  speed:1000,//動くスピード※デフォルトは500
	  minSlides: 1,//一度に表示させる画像の最小値
	  maxSlides: 4,//一度に表示させる画像の数
		moveSlides: 1,
	  slideWidth: 218,
	  slideMargin: 10,
	  pager: true,
		infiniteLoop: true, // true
	  prevText: '＜',
	  nextText: '＞',
	});

	$("#footerData").load("./footer.html");
	$('#page a, #header nav a').on('click', function(){
		var target = String(this.href).split('#')[1];
		if(target){
			var ypos = $("#"+target).offset().top - 50;
			$('html, body').animate({scrollTop:ypos}, 500, 'easeInOutQuart');
			return false;
		}
	});
	$(window).on('load resize', function(e){
		if($(window).innerWidth() > 640){
			setAutoHeight($('.charmPoint .autoHeight'));
			setAutoHeight($('.stepList .autoHeight'));
			setAutoHeight($('.functionList li:nth-child(-n+3)').find('.autoHeight'));
			setAutoHeight($('.functionList li:nth-child(n+4):nth-child(-n+6)').find('.autoHeight'));
			setAutoHeight($('.functionList li:nth-child(n+7):nth-child(-n+9)').find('.autoHeight'));
			$('#pri_li').css('display',"block");
			$('#contact_li').css('display',"block");
		}else{
			$('.charmPoint .autoHeight').removeAttr('style');
			$('.stepList .autoHeight').removeAttr('style');
			$('.functionList .autoHeight').removeAttr('style');
			$('#pri_li').css('display',"none");
			$('#contact_li').css('display',"none");
		}
		if(e.type == 'load'){
			$('html, body').scrollTop(0);
			setTimeout(function(){
				var href = location.hash;
				if(String(href).indexOf('#') != -1){
					var p = $(href).offset().top - 40;
					$('html,body').animate({ scrollTop: p }, 500, 'easeInOutQuart');
				}
			},100);
		}
	});
	function setAutoHeight($trg){
		$($trg).removeAttr('style');
		var h = 0;
		$trg.each(function(){
			h = ($(this).innerHeight() > h)?$(this).innerHeight():h;
		});
		$trg.css('height', h + 'px');
	}
	//
	$('.btnMenu').on('click', function(){
		$('body').toggleClass('open');
		return false;
	});
	var listenEvents = ['webkitTransitionEnd','oTransitionEnd','otransitionend','transitionend'];
	$('#spMenu a').on('click', function(){
		var href = $(this).attr('href');
		if(String(href).indexOf('#') != -1){
			$('body').removeClass('open');
			$('#page').on(listenEvents.join(' '), function() {
				$('#page').off(listenEvents.join(' '));
				var p = $(href).offset().top - 40;
				$('html,body').animate({ scrollTop: p }, 500, 'easeInOutQuart');
				return false;
			});
			return false;
		}
	});
});
</script>
