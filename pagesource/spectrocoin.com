<script>
			// trim leading spaces from inputs, except password input
			var form = $('form')
			form.submit(function() {
				var arr = $(this).serializeArray();
				for(i in arr) {
					var fieldName = arr[i]['name'];
					if(fieldName !== "password" && fieldName !== "csrfToken" && document.getElementById(fieldName) !== null){
						document.getElementById(fieldName).value = trimmer(arr[i]['value']);
					}
				}
			});
			function trimmer(x) {
				return x.trim();
			}
			// prevent a href double click
			 $(".dblclk").one('click', function (e) {
				   $(this).bind('click', false);
			 });
		</script>
<script>
			// prevent not found page error and submitting forms more than once
			$.fn.preventDoubleSubmit = function() {
				var lastClicked;
				var timeSinceClicked;
				$(this).bind('submit', function(event) {
					if(lastClicked) {
						timeSinceClicked = $.now() - lastClicked;
					};
					lastClicked = $.now()
					if(timeSinceClicked < 2000) {
						e.preventDefault();
					};
					return true;
				});
			};
		</script>
