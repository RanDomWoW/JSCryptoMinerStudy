					<script class="mybooks-template" type="text/x-handlebars-template">
						<section class="mybooks-box">
							<h2>
Mine Smartbøker ({{user.first_name}} {{user.last_name}})</h2>
							<ul class="books">
							{{#each products}}
								{{#if isbn}}
								<li>
									<div class="book-placeholder" data-isbn="{{isbn}}" data-title="{{title}}">
										<div class="placeholder">
										</div>
										<strong>{{title}}</strong>
									</div> 
								</li>
								{{/if}}
							{{/each}}
							</ul>
						</section>
					</script>
					<script class="choose-user-template" type="text/x-handlebars-template">
						<section class="lightbox-modale">
							<div class="lightbox-header">
								<h2>{{data.title}}</h2>
								Kjøp tilgang
							</div>
							<div class="lightbox-content">
								<nav class="option-picker">
									<a href="{{order_link}}" class="ico-company-option">
										<strong>Skole</strong>
									</a>
									{{#if products}}
									<a href="#" class="ico-private-option js-private-buy">
										<strong>Privatperson</strong>
									</a>
									{{/if}}
								</nav>
							</div>
						</section>
					</script>
					
					
<script class="private-buy-template" type="text/x-handlebars-template">
	<section class="lightbox-modale">
		<div class="lightbox-header">
			<h2>{{data.title}}</h2>
		</div>
		<div class="lightbox-content">
			<form action="#" class="private-buy-form js-private-buy-form">
				{{#if data.img}}
				<img src="{{data.img}}" alt="{{data.title}}" />
				{{/if}}
				{{#if products}}
				<label>Velg utgave</label>
				<div class="select-list" id="pick-product" name="product_list">
					{{#each products}}
						{{#if price_alternatives}}
						<div>
							<input type="radio" name="product_list" id="product_list_{{id}}" data-price-id="{{price_alternatives.0.id}}" data-tibet-id="{{id}}" value="{{id}}" />
							<label for="product_list_{{id}}">
								<strong>{{title}}{{#if language}} <span>({{language}})</span>{{/if}}</strong>
								Pris kr: {{price_alternatives.0.formatted_unit_price}} inkl mva
							</label>
						</div>
						{{/if}}
					{{/each}}
				</div>
				{{/if}}
				<input type="submit" class="order-button" value="Legg i handlekurv »" />
				<p>Pris er inklusiv mva. Varighet 1 år.</p>
				<p>Du vil få mulighet til å legge flere produkter i handlekurven før du går til betaling.</p>
			</form>
		</div>
	</section>
</script>
					
					<script class="cart-template" type="text/x-handlebars-template">
						<div class="shoping-cart">
							<div class="meta">Etter kjøp vil du få tilsendt en e-post med bekreftelse. <a href="#" class="js-empty-cart">Tøm handlekurv</a></div>
							<div class="holder">
								<div class="cart-header">
									<strong class="title">Din handlekurv</strong>
									<dl>
										{{#if total_discounted_price}}
										<dt>Rabatt:</dt>
										<dd>Kr {{total_discount}}</dd>
										{{/if}}
										<dt class="sum">Sum:</dt>
										<dd class="sum">Kr {{total_regular_price}}</dd>

									</dl>
								</div>
								<div class="cart-content">
									{{#each order_lines}}
									<article class="cart-item">
										<div>
											<div class="img-placeholder" data-isbn="{{product.isbn}}"></div>
											<strong class="title">{{product.title}}</strong>
											{{#if product.subtitle}}
											<span>{{product.subtitle}}</span>
											{{/if}}
											<span>Kr {{price_alternative.unit_price}} / {{price_alternative.billing_identifier}}</span>
											<a href="#" class="remove js-remove-product" data-product-id="{{product.id}}" data-price-alternative="{{price_alternative.id}}">x</a>
										</div>
									</article>
									{{/each}}
								</div>
								<div class="cart-footer">
									<a href="#" class="cart-button js-cart-checkout"><span>Gå til betaling</span></a>
								</div>
							</div>
						</div>
					</script>
					
