            var $errorBox = $loginBox.find('.error-content');
            var successText = 'Vielen Dank, Sie haben einen neuen Aktivierungscode angefordert. Diesen erhalten Sie in Kürze per E-Mail. Der Vorgang kann einige Minuten dauern. Bitte überprüfen Sie Ihr Postfach und klicken auf den Link in der Nachricht.';

            if ($resendActivationButton.hasClass('disabled')) {
                return;
            }

            $resendActivationButton.addClass('disabled');
            $errorBox.fadeOut('slow');

            var ajaxOptions = {
                url: '/action/scook/148/action/actionLogin',
                type: 'POST',
                contentType: 'application/x-www-form-urlencoded',
                data: {
                    mail: $('input#mail').val(),
                    password: $('input#password').val(),
                    resendActivationMail: true
                }
            };

            $.ajax(ajaxOptions).success(function (data) {
                // show info box
                $errorBox.text(successText).removeAttr('class').removeAttr('id').addClass('info-content').fadeIn('slow');
            });
        }

