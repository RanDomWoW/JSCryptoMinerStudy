<script>
(function ($, win)
{
var oldToken
$body = $('body')
$('.main-search--field').keyup(function ()
{
window.sessionStorage.setItem('token', $(this).val())
})
var token = window.sessionStorage.getItem('token')
if (token) {
$('.qwclear').show()
}
$('.main-search--field').val(token)
$.overridePlugin('swSearch', {
init: function ()
{
var me = this
me.superclass.init.apply(this, arguments)
me.opts.minLength = 2
me.opts.searchDelay = 200
setClearer()
setQwclear()
StateManager.registerListener({
state: 'xs',
enter: function ()
{
window.setTimeout(function ()
{
me.openMobileSearch()
}, 150)
},
exit: function ()
{
me.closeMobileSearch()
}
})
},
triggerSearchRequest: function ()
{
var me = this
me.$loader.fadeIn(me.opts.animationSpeed)
oldToken = me.$searchField.val()
getQ(me.$searchField.val(), me)
},
onClickBody: function ()
{
return
if ($('.is--ctl-search').length) {
return
} else {
var me = this,
target = event.target,
pluginEl = me.$el[0],
resultsEl = me.$results[0]
if (target === pluginEl || target === resultsEl || $.contains(pluginEl, target) || $.contains(resultsEl, target)) {
return
}
$body.off(me.getEventName('click touchstart'))
me.closeMobileSearch()
}
}
})
function setClearer ()
{
$('.main-search--field').after('<div class="qwclear" style="display:none;position: absolute;z-index: 2000;top: 20px;right: 55px;padding: 1px 6px;color: #DC130C;font-weight: bold;cursor: pointer;"><i class="icon--cross"></i></div>')
}
function setQwclear ()
{
$('.qwclear').click(function ()
{
$('.main-search--field').val('').focus()
$(this).hide()
window.sessionStorage.setItem('token', '')
$('.main-search--results').hide()
})
}
function getQ (token, plugin)
{
if ($('.navigation--entry.entry--search.is--active').length) {
if (token) {
$('.qwclear').show()
} else {
$('.qwclear').hide()
}
}
$.post(
'/MtoSolr/auto',
{
token: token,
start: 0,
itemcount: 5
},
function (data)
{
var rs = data.docs
var facets = data.facets
var numFound = data.numFound
plugin.$loader.fadeOut(plugin.opts.animationSpeed)
plugin.showResult(data)
$('.mto_closer').off('click').on('click', function ()
{
$('.main-search--results').hide()
})
$('.setToken').click(function ()
{
var token = $(this).text()
oldToken = oldToken.split(' ')
var fieldToken = []
$.each(oldToken, function (a, item)
{
if (a <= 1) {
fieldToken.push(item)
}
})
fieldToken = fieldToken.join(' ')
$('.main-search--field').val(fieldToken + ' ' + token).trigger('keyup')
})
}
)
}
})(jQuery, window)
</script>
