					<script>
					  // Currency rates
						var conn = new WebSocket('ws://zenrus.ru:8888');
						var lastValues = [0, 0, 0];

						conn.onmessage = function (e) {
							lastValues = updateValues(e.data.split(';'));
						};

						function updateValues(newValues) {
							if (typeof newValues[0] != 'undefined') {
								updateVal(jQuery('.vc-rates.usd'), newValues[0], lastValues[0]);
							}

							if (typeof newValues[1] != 'undefined') {
								updateVal(jQuery('.vc-rates.eur'), newValues[1], lastValues[1]);
							}

							if (typeof newValues[2] != 'undefined') {
								updateVal(jQuery('.vc-rates.oil'), newValues[2], lastValues[2]);
							}

							return newValues;
						}

						function updateVal(element, value, oldValue) {
							element.find('.num').text(Math.round(value*100)/100);
							var direction = Math.round(value*100)/100 > Math.round(oldValue*100)/100 ? 'up' : 'down';

							element.find('.direction')
								.removeClass('vc-rates-up, vc-rates-down')
								.addClass('vc-rates-' + direction);
						}
					</script>
