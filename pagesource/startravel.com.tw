<script>
    _bnSwiper = function () {
        var bnswiper = new Swiper('.wrap-bn', {
            loop: true,
            paginationClickable: true,
            speed: 1000,
            //loop: true,
            autoplay: {
                delay: 3000,
                disableOnInteraction: false,
            },
            pagination: {
            el: '.swiper-pagination',
            },
        });
    }

    var winWidth = $(window).width();

    _Swiper = function () {
        var randNo = getRandomInt(0, $(".swiper-slide").length);

        if (winWidth >= 768) {
            var swiper = new Swiper('.swiper-container', {
                lazy: true,
                initialSlide: randNo,
                paginationClickable: true,
                speed: 2500,
                //loop: true,
                autoplay: {
                    delay: 3000,
                    disableOnInteraction: true,
                },
                pagination: {
                    el: '.swiper-pagination',
                    clickable: true,
                    renderBullet: function (index, className) {
                        var names = [];
                        $(".swiper-wrapper .swiper-slide").each(function(i) {
                          names.push($(this).attr("data-name"));
                        });
                        return '<span class="' + className + '">' + names[index] + '</span>';
                    },
                },
            });

        } else {
            var swiper = new Swiper('.swiper-container', {
                lazy: true,
                initialSlide: randNo,
                paginationClickable: true,
                speed: 2500,
                // loop: true,
                autoplay: {
                    delay: 3000,
                    disableOnInteraction: true,
                },
                pagination: {
                    el: '.swiper-pagination',
                    clickable: true,
                },
            });
        }
    }

    $(function () {
        _Swiper();
        _bnSwiper();
        loadNavnew();
    });
    $( window ).resize(function() {
        _Swiper();
    });

    function getRandomInt(min, max) {
        return Math.floor(Math.random() * (max - min + 1)) + min;
    }

	$(function () {
		$('div[data-eventcategory]').on('click', 'a', function(event){
			var $parents = $(this).parents('[data-eventcategory]');
			var eventcategory = $parents.data('eventcategory');
			var eventaction = $parents.data('eventaction');
			var eventlabel = $parents.data('eventlabel');

			ga('send', 'event', {
				eventCategory: eventcategory,
				eventAction: eventaction,
				eventLabel: eval(eventlabel)
			});
		});
	});
</script>

