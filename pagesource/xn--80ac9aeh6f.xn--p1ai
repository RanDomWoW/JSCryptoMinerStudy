    var win = $(window);
      win.scroll(function() {
        if($('.part-list').hasClass('loading')) {
          return;
        }
        
        if($('.part-list').hasClass('continue-parts')) {
          return;
        }
     
        if ($(document).height() - win.height() - 150 < win.scrollTop()) {
          $('.part-list').addClass('loading');
          
          $.get('/v1/book/parts-main/', { page: $('#marker-end').data('page'), format: "html" }, function (response) {
            if(response.status == 200) {
              if(response.result.length) {
                $('#marker-end').data('page', $('#marker-end').data('page') + 1);
                $.when($('#infinite').append(response.result))
                .then(function() {
                  $('time.timeago').timeago();
                });
              } else {
                 $('#marker-end').remove();
                 $('.part-list').addClass('continue-parts');
              }
            } else {
              console.log('error', response.error);
              $('.part-list').addClass('continue-parts');
            }
            $('.part-list').removeClass('loading');
          });
        }
      });
  });

