    <script>
        
        function normalizeCarousels(){
            $(".carousel").each(function(n,e){
                var items = $(this).find('.carousel-item'), heights = [], tallest, holder = $(this).find('.carousel-inner').height();
                if (items.length) {
                    $(this).find('.carousel-item').css("height","auto");
                    items.each(function() {
                        heights.push($(this).outerHeight());
                    });
                    tallest = Math.max.apply(null, heights);
                    $(this).find('.carousel-item').css("height",tallest);
                }
            });
        }
        
        $(document).ready(function() {
            var hash = window.location.hash;
            // SIGNUP SLIM VIEW
            if (hash === '#signup') {
                $('#hs_cos_wrapper_translation_module_header').hide()
                $('#slim-header').removeClass("hide")
                $('.hide-on-signup').hide()
                $('.show-on-signup').removeClass("hide")
                $('footer').hide();
            }
            // HIDE CREATE ACCOUNT CTA
            if (window.location.pathname.indexOf("cad-pricing") > -1) {
                $('#header_create-account').hide();
            }
            
            // NORMALIZE CAROUSELS
            normalizeCarousels();
            $(window).on('resize', function() {
                normalizeCarousels();
            });
            
        });
    </script>


    
