<script>
jQuery(function($){
	// CSSのCDNフェールオーバー
	$(window).on('load', function(){
		$('link.fallback').each(function(){
			var element=$(this);
			element.attr('href', element.data('fallback'));
		});

		window.autotrack_js || $.getScript('/assets/js/autotrack.js');
	});

	// 発火は、#で始まるアンカーがクリックされた時
	$('a[href^=#]').click(function(){

		// アンカー名を取得する
		var href = $(this).attr("href");
		if (href == "#" || href == "") {
			return false;
		} else if (href == "#top") {
			// ページトップに戻るなら、targetは<html>
			target = $('html');
		} else {
			// アンカーの指す要素をtargetとする
			target = $(href);
		}

		// ダイソーWebの上部ナビバーの高さを取得
		var heightCorpNavBar = $('#globalHeader.navbar-fixed-top').outerHeight();
		var headerHight = (heightCorpNavBar || 0);

		// オフセットを算出
		var position = target.offset().top - headerHight;

		// スムーズスクロール
		$('html, body').animate({scrollTop : position}, 800);
		return false;
   });
});
</script>

	<!-- 話題の新商品のスライダー用 -->
