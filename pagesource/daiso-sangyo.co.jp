	<script>$.fn.imagesLoaded || document.write('<script src="/assets/js/imagesloaded.pkgd.min.js"><\/script>');</script>

	<script>
		// カルーセル表示
		$( '#img-slide-0' ).on('load', function(){			// 1枚目の画像をロードしたらスライダーなしで起動
			$( '#slider' ).removeClass('slider-init');		// 初期スタイルを無効化
			$( '#slider' ).sliderPro({
				aspectRatio: 1.0,			// アスペクト比
				arrows: false,				// 矢印無し
				buttons: false,				// ページャー無し
				autoplay: false,			// 自動スライド無し
				loop: false,				// ループ再生無し
				visibleSize: '100%',		// 前後のスライドを表示
				autoplayDelay: 5000			// 5秒間隔で
			});
			// chromeでカルーセル画像をクリックできない問題を修正
			if($(window).width() > 767) {
				$( '#slider .sp-slide a' ).each(function(){
					$(this).addClass('sp-selectable').css('cursor','pointer');
				});
			}
		});

		$(window).on('load', function(){					// すべての画像をロードしたらスライダーありに変更
			$( '#slider' ).removeClass('slider-init');		// 初期スタイルを無効化
			$( '#slider' ).sliderPro({
				//width: '100%',
				aspectRatio: 1.0,			// アスペクト比
				arrows: true,				// 矢印あり
				buttons: true,				// ページャーあり
				autoplay: true,				// 自動スライドあり
				loop: true,					// ループ再生あり
				visibleSize: '100%',		// 前後のスライドを表示
				//forceSize: 'fullWidth',	// 幅いっぱいに表示
				autoplayDelay: 5000			// 5秒間隔で
			});
			// chromeでカルーセル画像をクリックできない問題を修正
			if($(window).width() > 767) {
				$( '#slider .sp-slide a' ).each(function(){
					$(this).addClass('sp-selectable').css('cursor','pointer');
				});
			}
		});

		// アニメーション付きグリッド表示
		$(window).on('load', function(){
			$( '#newItemsGrid' ).removeClass('grid-init');		// 初期スタイルを無効化
			var $grid = $('.grid').masonry({
							itemSelector: '.grid-item',
							columnWidth: '.grid-sizer',
							percentPosition: true,
						});

			// レスポンシブではこの画像ローダーが必要
			$grid.imagesLoaded().progress(function() {
				$grid.masonry('layout');
			});
		});
	</script>

