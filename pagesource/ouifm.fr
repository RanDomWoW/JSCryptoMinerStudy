<script>
jQuery(document).ready(function ($) {
    // tracking Analytics
    $('.global').on('click', '.swipe-wrap li', function (e) {
        ga('send', 'event', {
            eventCategory: 'slider',
            eventAction: 'click',
            eventLabel: $(this).find('a').attr('href'),
            eventValue: $(this).data('index'),
            transport: 'beacon'
        });
    });
    setTimeout(function () {
        $.getJSON('/slider.json', function (data) {
            $.each(data, function (slot, arr) {
                var rnd = Math.floor(Math.random() * arr.length);
                $.each(arr, function (key, slider) {
                    if (key !== rnd) {
                        return true;
                    }
                    if (document.location.protocol === 'https:') {
                        slider.img = slider.img.replace('http:', 'https:');
                    } else {
                        slider.img = slider.img.replace('https:', 'http:');
                    }
                    if (slot > 0) {
                        $('.swipe-wrap').append(
                            $('<li/>').append(
                                $('<a/>', { 'href': slider.link }).append(
                                    $('<img/>', {
                                        src: slider.img,
                                        title: slider.title,
                                        alt: ''
                                    })
                                )
                            )
                        );
                    }
                    $('.swipe-pagination').append(
                        $('<li/>', { 'class': slot === 0 ? 'on' : '', 'data-index': slot }).append(
                            $('<a/>', { 'href': slider.link })
                        )
                    );
                });
            });
            var swipe = Swipe(document.getElementById('swipe'), {
                startSlide: 0,
                speed: 100,
                auto: 3000,
                continuous: true,
                disableScroll: false,
                stopPropagation: false,
                callback: function (idx, elem) {
                    $('.swipe-pagination li')
                        .removeClass('on')
                        .filter('[data-index="' + idx + '"]')
                        .addClass('on');
                }
            });
            $('.swipe-pagination li').on('mouseenter click', function () {
                swipe.slide($(this).data('index'));
            });
        });
    }, 5000);
});
</script>
