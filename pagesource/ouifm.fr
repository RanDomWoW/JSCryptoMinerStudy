<script>
var cnil = {

  wording: "En poursuivant votre navigation sur ce site, vous acceptez que des cookies soient utilisés afin d’améliorer votre expérience d’utilisateur et de vous offrir des contenus personnalisés.",
  more_url: "https://www.ouifm.fr/mentions-legales/",
  domain: "www.ouifm.fr",

  init: function () {
    var self = this;
    document.addEventListener("DOMContentLoaded", function () {
      if (!self.getCookie('cnil')) {
        document.body.appendChild(self.getBanner());
        self.showBanner();
        document.getElementById('cnil-btn-ok').addEventListener('click', function () {
          self.setCookie('cnil', 'ok');
          self.hideBanner();
        });
        document.getElementById('cnil-btn-more').addEventListener('click', function () {
          document.location = self.more_url;
        });
      }
    });
  },
  showBanner: function () {
    document.getElementById('cnil-banner').style.display = "block";
  },
  hideBanner: function () {
    document.getElementById('cnil-banner').style.display = "none";
  },
  getBanner: function () {
    var banner = document.createElement("div");
    banner.id = "cnil-banner";

    var label = document.createTextNode(this.wording);
    banner.appendChild(label);

    var btn_ok = document.createElement("button");
    btn_ok.id = "cnil-btn-ok";
    btn_ok.appendChild(document.createTextNode("OK"));
    banner.appendChild(btn_ok);

    var btn_more = document.createElement("button");
    btn_more.id = "cnil-btn-more";
    btn_more.appendChild(document.createTextNode("En savoir plus"));
    banner.appendChild(btn_more);

    return banner;
  },
  getCookie: function (name) {
    var oRegex = new RegExp("(?:; )?" + name + "=([^;]*);?");
    if (oRegex.test(document.cookie)) {
      return decodeURIComponent(RegExp["$1"]);
    }
  },
  setCookie: function(name, value) {
    document.cookie = name + '=' + encodeURIComponent(value) + '; expires=0; path=/; domain=' + this.domain;
  }
}

cnil.init();

</script>
