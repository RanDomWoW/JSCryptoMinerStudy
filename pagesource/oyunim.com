var hangiSirada=0;
var myVar = setInterval(function(){myTimer()}, 3000);
var populerSayfa=1;
var yeniSayfa=1;

function myTimer() {
hangiSirada++;
if(hangiSirada>3)hangiSirada=0
$(".featured-thumb-item").removeClass("active");
$(".featured-thumb-item:eq("+hangiSirada+")").addClass("active");
slayter(hangiSirada);
}

$(".featured-games").hover(function(){clearInterval(myVar);},function(){myVar = setInterval(function(){myTimer()}, 3000);});

$(".featured-thumb-item").mouseover(function(){
$(".featured-thumb-item").removeClass("active");
$(this).addClass("active");
slayter($("li.featured-thumb-item").index($(".featured-thumb-item.active")));
});

function slayter(a)
{
hangiSirada=a;
$(".featured-info").children("a").attr("href", ($("li.featured-thumb-item:eq("+a+")").children("a").attr("href")));
$("img.featured-game-thumb").attr("src", $(".featured-thumb-item:eq("+a+")").children("a").children("img").attr("src"));
$(".featured-game-title").html($(".featured-thumb-item:eq("+a+")").children("a").attr("title"));
$(".featured-game-description").html($(".featured-thumb-item:eq("+a+")").children("a").children("img").attr("alt"));	
}


$("#yn-next").click(function(){
if(yeniSayfa<5)
{
yeniSayfa++;
getir("yeni","id",yeniSayfa,".new-results",".yn-pages");
}
});


$("#yn-prev").click(function(){
if(yeniSayfa>1)
{
yeniSayfa--;
getir("yeni","id",yeniSayfa,".new-results",".yn-pages");
}
});

$("#po-prev").click(function(){
if(populerSayfa>1)
{
populerSayfa--;
getir("yeni","oy",populerSayfa,".popular-results",".po-pages");
}
});
$("#po-next").click(function(){
if(populerSayfa<5)
{
populerSayfa++;
getir("yeni","oy",populerSayfa,".popular-results",".po-pages");
}
});

function getir(ne,order,yeniSayfa,hangialan,secici)
{
  $.post("inc/anasayfaslayt.php",
  {
    ne:ne,order:order,s:yeniSayfa
  },
  function(data,status){
    if(status=="success") $(hangialan).html(data);
	$(secici+" li").removeClass("active");
	$(secici+" li.sayfa"+yeniSayfa).addClass("active");
  });
}


</script>
