    e.preventDefault();
    var valido = true;

    $('.frmAscendente input, .frmAscendente select').each(function(){
      var elem = $(this);
      elem.removeClass('disabled');
    });

    setTimeout(function(){
      $('.frmAscendente input, .frmAscendente select').each(function(){
        var elem = $(this);
        if (elem.val() == null || elem.val()=='') {
          elem.addClass('disabled');
          valido = false;
        } else {
          elem.removeClass('disabled');
        }
      });

      if (valido) {
        $('.frmAscendente').submit();
      }
    }, 100);

  });

  $('.frmAscendente .frmEstado').on('change', function(){
