background: #B4E0F8 url(http://themes.googleusercontent.com/image?id=1wLTWkU_6PD30roBJNfOzJkbDSVdh23FfYsvrDIfh8emJnpH0BAOKT4kTP4ytiOfdJE2g) repeat scroll top center /* Credit:  */;
}
html body .content-outer {
