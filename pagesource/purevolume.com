<script type="text/javascript">

// invoked once for each individual image that loads 
function itemImagesLoaded(el)
{ 
    var margin_left = Math.floor( (parseInt($(el).width()) / 2) + 2) * -1;
    $(el).css('margin-left', margin_left);
} 

// invoked only once: when all images loaded
function selectorImagesLoaded($selector)
{ 
    return true;
} 

function playlistItemMenuClick()
{
	// all player tracks
	$('.track_options_menu').click(function(e) {
        e.stopPropagation();
    });


	// find all track buttons
    $(".track_options_menu_button").each(function(){
 	    
 	    $(this).unbind('click');
 	    
 	    // attach the onclick to them
    	$(this).click(function(){
    		
    		$options_div = $(this).parents('.playlist_item').find('.track_options_menu');
    	                	
            if ($options_div.is(':visible'))
            {
                $options_div.hide();
            }
            else
            {
            	$('.track_options_menu').hide();

                $options_div.show();
                
                showCompletely($options_div);     
            }
            return false;
        });
        
    });
}

function processPlaylistSongs()
{
		var song_info = [];
	
		// find all songs on page
		$('.track_like').each(function()
		{
			song_info.push($(this).parent('.track_options').parent('div').attr('id'));
			
		});
		
		// check via ajax if any song on the page was liked
		if (song_info.length > 0)
		{
			showLiked('Song', song_info, 'heart');
			showDeleteSongs();
		}
}

function showDeleteSongs()
{
	// check via ajax if any song on the page was liked
	$('.playlist_item').each(function()
	{
	   
	
		if ($(this).attr('owner'))
		{
			var pieces = $(this).attr('owner').split(':');
				
    		var user = userLoginMeta();
    				
    		if (pieces[0] == user.type && pieces[1] == user.id)
    		{		
    			$(this).find('.track_remove').show();
    		}
        }
		
	});
}

function selectPlaylist(name, id, ownerType, ownerId, extra)
{   
	var $selectedPlaylist  = $('div#playlist_container_' + id);
	var $playlistContainer = $selectedPlaylist.parent('div.playlist_container');
	var $playerWrapper     = $playlistContainer.parent('div.player_wrapper');
                
	$playlistContainer.find('div.playlist_container_inner').hide(); 
	$selectedPlaylist.show();
	$playerWrapper.find('.playlist_select_link').html(name);
	
	$playerWrapper.find('.playlist_dropdown').hide();
		
	playlist_name   = id;
				                	                	
	if ($selectedPlaylist.find('div.playlist_item').length == 0)
	{   
		getMoreTracks(id, ownerType, ownerId, id, extra);	
	}
	                            	
	return false;
}

function playlistLoaded()
{
	$('div.playlist_item').hide();
	$('div.playlist_item').show(); 
	
	var $jScrollClass  = $('div.scroll_jScroll');
	var $overflowClass = $('div.scroll_overflow');
	
	// make sure we have loaded tracks before calling jScroll
	if ($jScrollClass.find('div.playlist_item').length != 0)
	{
		$('div.playlist_container').removeClass('playlist_loading');
		
		if ($jScrollClass.find('div.playlist_item').length > 3)
		{
			$jScrollClass.append('<div style="z-index: 99; display: block;" owner="none:0" other="0" class="playlist_item" id="empty_1"></div>');
			$jScrollClass.append('<div style="z-index: 98; display: block;" owner="none:0" other="0" class="playlist_item" id="empty_2"></div>');
		}
		$jScrollClass.jScrollPane({showArrows:true, scrollbarWidth:0, scrollbarMargin:0});
		
		// crappy ie hack		
		setTimeout("var $liked = $('div.scroll_jScroll').find('a.track_like'); $liked.toggle(); $liked.show();", 10);
	}  
	
	// make sure we have tracks loaded 
	else if ($overflowClass.find('div.playlist_item').length != 0)
	{
		$('.playlist_container').removeClass('playlist_loading');
	}
	
	// no ajax will be called; no tracks means no tracks
	else if ($jScrollClass.length == 0 && $overflowClass.length == 0)
	{
		$('div.playlist_container').removeClass('playlist_loading');
	}
}
          
function createOverflowScroll(moduleId, playlist_name, ownerType, ownerId, extra)
{
	//console.log('moduleId ' + moduleId + ' p ' + playlist_name);

	$('#' + moduleId + ' .scroll_overflow').scroll(function(){
								
		if (canScroll == true)
		{				
			var lastHeight = $('div#playlist_container_'+ playlist_name + '.playlist_container_inner .playlist_item:last').offset().top + $('div#playlist_container_' + playlist_name + '.playlist_container_inner .playlist_item:last').height();
			
			var totalHeight = $('div#playlist_container_'+ playlist_name + '.playlist_container_inner').offset().top + $('div#playlist_container_'+ playlist_name + '.playlist_container_inner').height();
			
			if (lastHeight <= totalHeight)
			{
				getMoreTracks(playlist_name, ownerType, ownerId, playlist_name, extra);
			}
		}
	});
}	

