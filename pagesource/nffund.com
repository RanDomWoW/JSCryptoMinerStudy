<script id="template_index_fhb"  type="text/x-handlebars-template">

	<thead>
	<tr>
		<th scope="col" disable_sort="true" class="default_cursor">基金名称</th>
		<th scope="col" disable_sort="true" class="default_cursor"> 基金代码</th>
		<th scope="col" disable_sort="true" class="default_cursor">净值日期</th>
		<th scope="col"><span class=''> 基金净值</span> </th>
		<th scope="col"><span class=''> 累计净值</span></th>
		<th scope="col"><span class=''> 日涨幅</span></th>
		<th scope="col"><span class=''> 最近半年</span></th>
		<th scope="col"><span class=''> 最近一年</span></th>
		<th scope="col"><span class=''> 今年以来</span></th>
		<th scope="col"><span class=''> 成立以来</span></th>
		<th scope="col">网上直销</th>
	</tr>
	</thead>
	<tbody>
	{{#each this}}
	<tr>
		<td align="left" class="fund_name">
			<div class="fund_bg">
				<a target='_blank' href="/new/fundproduct/?action=searchjjjz&fundcode={{fundcode}}">{{fundname}}</a>
			</div>
		</td>
		<td>{{fundcode}}</td>
		<td>{{fmtdate fdate}}</td>
		<td>{{fmtfhbnav nav}}</td>
		<td>{{fmtfhbnav ljnav}}</td>

		{{#isnull upRatio}}
		<td>--</td>
		{{else}}
		{{#upratio upRatio}}
		<td><span class="red">{{upRatio}}%</span></td>
		{{else}}
		<td><span class="green">{{upRatio}}%</span></td>
		{{/upratio}}

		{{/isnull}}

		{{#isnull recentHalfYear}}
		<td>--</td>
		{{else}}
		<td>{{recentHalfYear}}%</td>
		{{/isnull}}

		{{#isnull recentOneYear}}
		<td>--</td>
		{{else}}
		<td>{{recentOneYear}}%</td>
		{{/isnull}}

		{{#isnull thisYear}}
		<td>--</td>
		{{else}}
		<td>{{thisYear}}%</td>
		{{/isnull}}

		{{#isnull since}}
		<td>--</td>
		{{else}}
		<td>{{since}}%</td>
		{{/isnull}}

		{{#isrgq transactionState}}
		{{#canrg status}}
		<td><a href="https://trade.southernfund.com/new/account/login/init?url=%2Fnew%2Ftrade%2Fdeclare%2Finit%3FmenuId%3D30500%26fundcode%3D{{fundcode}}" class="buy_bt">认购</a></td>
		{{else}}
		<td><a class="buy_bt_hui">认购</a></td>
		{{/canrg}}
		{{else}}
		{{#cansg zx_status}}
		<td><a href="https://trade.southernfund.com/new/account/login/init?url=%2Fnew%2Ftrade%2Fdeclare%2Finit%3FmenuId%3D30500%26fundcode%3D{{fundcode}}" class="buy_bt">申购</a></td>
		{{else}}
		<td><a class="buy_bt_hui">申购</a></td>
		{{/cansg}}
		{{/isrgq}}
	</tr>

	{{/each}}
	</tbody>

</script>
