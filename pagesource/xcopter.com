;(function(){
var callBackList = {};
var config = {
glue: "/",
deli: "_",
prefix : "sdk_basket",
appid:null,
unique_id:null,
req_seqs:null,
product_data:{},
action_url:null,
main_data : {},
main: [],
sub: [],
delimg_url: "//img.echosting.cafe24.com/design/skin/default/product/btn_price_delete.gif"
};
//set callback
$.setSdkBasketCallback = function (req_appid, callback) {
if (req_appid && typeof callback === "function") {
callBackList[req_appid] = callback;
}
};
//check box all
$(document).delegate('input[id^="' +getIdClassPrefix("chkall")+ '"]:visible', "change", function(){
setUniqueId($(this).attr("id"));
var val = $(this).attr("checked");
$("."+getIdClass("chk")).each(function(){
$(this).attr("checked", val);
});
});
//set submit evt
$(document).delegate('a[id^="' + getIdClassPrefix("submit")+ '"]:visible', "click", function(){
setUniqueId($(this).attr("id"));
if (typeof callBackList[config.appid] == "function") {
submitFire(callBackList[config.appid]);
} else {
submitFire();
}
});
//set delvtype change evt
$(document).delegate('input[class^="' + getIdClassPrefix("delvtype")+ '"]:visible', "change", function(){
var tmp_class = $(this).attr("name");
if (tmp_class) {
var delv = $(this).val();
if (delv == "A" || delv == "B") {
$("[."+tmp_class+"][rel='delvtype']").val(delv);
}
}
});
//add item
$(document).delegate('select[class^="' + getIdClassPrefix("select") + '"]:visible', "change", function(){
setUniqueId($(this).attr("class"));
var seq = $(this).attr("rel");
if (!config.product_data[seq]) {
setDataFromHidden();
}
var prd_data = config.product_data[seq];
var opt_code = $(this).val();
var opt_name = $(this).find("option:selected").html();
if (prd_data["option_type"] == "F") {
createItemTypeF(prd_data, opt_name, opt_code);
} else if (prd_data["option_type"] == "T") {
if (prd_data["item_listing_type"] == "S") {
createItemTypeTS(prd_data, opt_name, opt_code);
} else if (prd_data["item_listing_type"] == "C") {
createItemTypeTC(prd_data, opt_name, opt_code);
}
}
});
//del item
$(document).delegate('a[class^="' + getIdClassPrefix("itemdel") + '"]:visible', "click", function(){
setUniqueId($(this).attr("class"));
var seq = $(this).attr("rel");
if (seq) {
$(this).closest("."+getIdClass("itembox_"+seq)).remove();
}
});
//opt type F
function createItemTypeF(prd_data, opt_name, opt_code)
{
setItemTag(prd_data, opt_name, opt_code)
}
//opt type TC
function createItemTypeTC(prd_data, opt_name, opt_code)
{
setItemTag(prd_data, opt_name, opt_code)
}
//opt type TS
function createItemTypeTS(prd_data, opt_name, opt_code)
{
var sel_val = [];
$("."+getIdClass("select_"+prd_data['product_no'])).each(function(){
var selopt = $(this).find(":selected").val();
if (selopt) {
sel_val.push(selopt);
}
});
opt_name = sel_val.join(config.glue);
var opt_name_key = encodeURIComponent(opt_name);
opt_code = prd_data['opt_data'][opt_name_key];
setItemTag(prd_data, opt_name, opt_code)
}
//get added price
function getPrice(prd_data, opt_code)
{
var opt_price = prd_data['opt_price'];
if (!opt_price.hasOwnProperty(opt_code)) {
return false;
}
var price = parseInt(prd_data['product_price'], 10) + parseInt(opt_price[opt_code], 10);
if (isNaN(price)) {
return false;
}
return addCommas(price) + "원";
}
function createItemTag(prd_data, code, name, price)
{
var quan = parseInt(prd_data['product_min'], 10);
quan = (isNaN(quan) || quan < 1) ? 1 : quan;
var html = '<ul class="'+getIdClass("itembox_"+prd_data['product_no'])+'">';
html += '<li><input type="hidden" class="'+getIdClass("itemcode_"+prd_data['product_no'])+'" value="'+code+'"/>'+name+'</li>';
html += '<li><input type="text" class="'+getIdClass("quan_"+prd_data['product_no'])+'" value="'+quan+'"> '+__('개')+'</li>';
html += '<li>'+price+'</li>';
html += '<li><a href="javascript:;" class="'+getIdClass("itemdel_"+prd_data['product_no'])+'" rel="'+prd_data['product_no']+'"><img src="'+config.delimg_url+'" alt=""></a></li>';
html += '</ul>';
return html;
}
function setItemTag(prd_data, opt_name, opt_code)
{
if (isNewItemTag(prd_data['product_no'], opt_code) == false) {
alert(__("이미 선택되어 있는 옵션입니다."));
return;
}
var price = getPrice(prd_data, opt_code);
if (price !== false) {
var html = createItemTag(prd_data, opt_code, opt_name, price);
addItemTag("itembox_"+prd_data['product_no'], html);
}
}
function isNewItemTag(seq, code)
{
var item_tag = $('#'+getIdClass("itembox_"+seq)).find("input[value="+code+"]");
if (item_tag.length > 0) {
return false;
}
return true;
}
function addItemTag(target, html)
{
var box = $('#'+getIdClass(target));
if (box.length > 0) {
box.append(html);
}
}
//set req app id
function setUniqueId(id)
{
var idset = id.split(config.deli);
var app_frag = idset[idset.length-1];
var app_id = idset[idset.length-2];
if (!app_id || !app_frag) {
return false;
}
config.unique_id = app_frag;
config.appid = app_id;
}
function submitFire (callback) {
clearConfig();
setDataFromHidden();
product_submit(callback);
}
function clearConfig()
{
config.main_data = {};
config.main = [];
config.sub = [];
config.product_data = {};
config.action_url = $('#'+getIdClass("form")).attr("rel");
}
function setDataFromHidden(){
var seqs = $('#'+getIdClass("form")).attr("seqs");
config.req_seqs = seqs.split(",");
for (var i=0; i<config.req_seqs.length; i++) {
var seq = config.req_seqs[i];
var pid = getIdClass("tmpdata_"+seq);
config.product_data[seq] = {};
$("."+pid).each(function(){
var key = $(this).attr("rel");
var value = $(this).attr("value");
if (key == "opt_price" && value) {
value = $.parseJSON(value);
}
if (key == "opt_data" && value) {
value = $.parseJSON(value);
}
config.product_data[seq][key] = value;
});
}
}
function product_submit(callback)
{
if (checkSelectedItem() == false) return;
if (checkOptionRequired() == false) return;
if (checkAddOption() == false) return;
if (checkQuantity() == false) return;
initSubmitData();
var frm = $('#'+getIdClass("form"));
var main = config.main_data;
frm.find(":hidden").remove();
frm.append(getInputHidden('product_no', main.product_no));
frm.append(getInputHidden('product_name', main.product_name));
frm.append(getInputHidden('main_cate_no', main.main_cate_no));
frm.append(getInputHidden('display_group', main.display_group));
frm.append(getInputHidden('option_type', main.option_type));
frm.append(getInputHidden('product_price', main.product_price));
frm.append(getInputHidden('product_min', main.product_min));
frm.append(getInputHidden('command', main.command));
frm.append(getInputHidden('has_option', main.has_option));
frm.append(getInputHidden('basket_type', main.basket_type));
frm.append(getInputHidden('multi_option_schema', main.multi_option_schema));
frm.append(getInputHidden('multi_option_data', main.multi_option_data));
frm.append(getInputHidden('quantity', main.quantity));
frm.append(getInputHidden('delvType', main.delvtype));
frm.append(getInputHidden('redirect', main.redirect));
frm.append(getInputHidden('prd_detail_ship_type', main.prd_detail_ship_type));
frm.append(getInputHidden('product_max_type', main.product_max_type));
frm.append(getInputHidden('product_max', main.product_max));
frm.append(getInputHidden('relation_product', main.relation_product));
// check option and get option value
$('select[class="' + getIdClass("select_"+main.product_no) + '"]:visible').each(function()
{
frm.append(getInputHidden('optionids[]', $(this).attr('name')));
if ($(this).attr('required') == true) {
frm.append(getInputHidden('needed[]', $(this).attr('name')));
}
var iSelectedIndex = $(this).get(0).selectedIndex;
if (iSelectedIndex > 0) {
var sValue = $(this).val();
if (sValue) {
frm.append(getInputHidden($(this).attr('name'), sValue));
}
}
});
//check user add option and get value
var addopt = $("."+getIdClass("addopt_"+main.product_no));
if (addopt.length > 0) {
var aName = [];
for (var j=0; j<addopt.length; j++) {
frm.append(getInputHidden('option_add[]', $(addopt[j]).val()));
aName.push($(addopt[j]).attr("title_option"));
}
frm.append(getInputHidden('add_option_name', aName.join(';')));
}
// get data when multi product submitted
var sel_main = config.main;
for (var i=0; i<sel_main.length; i++)
{
var main_info = sel_main[i];
frm.append(getInputHidden('selected_item[]', main_info));
}
var sub = config.sub;
for (var i=0; i<sub.length; i++)
{
var sub_info = sub[i];
frm.append(getInputHidden('relation_item[]', sub_info));
}
var items = getSubmitProduct();
for (var i=0; i<items.length; i++) {
var seq = items[i];
if (!seq || seq == main.product_no) {
continue;
}
if ( $("."+getIdClass("addopt_"+seq)).length > 0 ) {
var nameJoin = [];
var valueJoin = [];
$("."+getIdClass("addopt_"+seq)).each(function(){
nameJoin.push( $(this).attr("title_option"));
valueJoin.push( $(this).val());
})
frm.append(getInputHidden('user_option_name_' + seq, nameJoin.join(',@,')));
frm.append(getInputHidden('user_option_' + seq, valueJoin.join(',@,')));
}
}
// submit using ajax
frm.attr("action", "/"+config.action_url);
var sParam = frm.serialize();
$.post(config.action_url, sParam, function(data)
{
if (typeof callback == "function") {
callback();
} else {
if (confirm(__("장바구니에 추가되었습니다. \n장바구니를 확인하시겠습니까?"))) {
makePop();
}
}
}, 'json');
}
function makePop()
{
var settings = 'toolbar=0, status=0, menubar=0, scrollbars=yes, height=960, width=1280';
var target_default = '/order/basket.html';
var target = "";
if ($.trim(target) == "") {
target = target_default;
}
window.open(target, __('장바구니'), settings);
}
function checkSelectedItem()
{
var reqseqs = getSubmitProduct();
if (reqseqs.length < 1) {
alert(__("선택된 상품이 없습니다."));
return false;
}
return true;
}
function checkOptionRequired()
{
var items = getSubmitProduct();
for (var i=0; i<items.length; i++) {
var seq = items[i];
var flag = false;
if (!seq) {
return false;
}
$("."+getIdClass("select_"+seq)).each(function(){
if (flag == true) {
return;
}
if ($(this).attr('required')) {
if ($('option:selected', this).val().indexOf('*') > -1) {
$(this).focus();
flag = true;
}
}
});
if (flag == true) {
alert(__("선택안된 필수 옵션이 있습니다."));
return false;
}
}
return true;
}
function checkAddOption()
{
var items = getSubmitProduct();
for (var i=0; i<items.length; i++) {
var seq = items[i];
if (!seq) {
return false;
}
var addopt = $("."+getIdClass("addopt_"+seq));
if (addopt.length < 1) {
continue;
} else {
for (var j=0; j<addopt.length; j++) {
var $add = $(addopt[j]);
if ($add.attr('required')) {
if ($.trim($add.val()) == "") {
$add.focus();
alert(__("추가 옵션을 입력해 주세요."));
return false;
} else {
if ($add.val().length > 100) {
$add.focus();
alert(__("추가 옵션은 100 자 이상 적을수 없습니다."));
return false;
}
}
}
}
}
}
return true;
}
function getSubmitProduct()
{
var chkbox = $('input[class="'+getIdClass("chk")+'"]');
if (chkbox.length < 1) {
return config.req_seqs;
} else {
var items = $('input[class="'+getIdClass("chk")+'"]:checked');
var selProduct = [];
for (var i=0; i<items.length; i++) {
var item = items[i];
var seq = $(item).val();
selProduct.push(seq);
}
return selProduct;
}
}
function checkQuantity(seq, min)
{
var items = getSubmitProduct();
for (var i=0; i<items.length; i++) {
var seq = items[i];
if (!seq) {
return false;
}
var data = config.product_data[seq];
var min = parseInt(data['product_min'],10);
var max = parseInt(data['product_max'],10);
var targetInput = $("."+getIdClass("quan_"+seq));
$("."+getIdClass("quan_"+seq)).each(function(){
targetInput = $(this);
var quan = $.trim(targetInput.val());
quan = parseInt(quan, 10);
min = ( min < 1 ) ? 1 : min;
if (isNaN(quan)) {
targetInput.focus();
alert(__("정확한 수량을 입력해 주세요."));
return false;
}
if (quan < min) {
targetInput.focus();
alert(__("최소 주문 갯수를 확인해 주세요."));
return false;
}
if (max !== 0 && quan > max) {
targetInput.focus();
alert(sprintf(__("최대 %s개 주문 할 수 있습니다."), max));
return false;
}
});
}
return true;
}
function initSubmitData()
{
var mainFlag = true;
var items = getSubmitProduct();
