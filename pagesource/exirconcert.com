        var popup_status = Cookies.get('telegram_channel_popup');

        setTimeout(function () {
            if(popup_status == undefined){
                popup_status = 0;
            }
            popup_status = parseInt(popup_status);
            if(popup_status < 2 ){
                $('#telegram_channel_popup').fadeIn(100, function () {
                    $("#wrapper").addClass('blur');
                });
            }
        }, 1000);

        var $telegram_chanel_popup = $('#telegram_channel_popup');

        $telegram_chanel_popup.find('.close-pop').click(function (e) {
            e.preventDefault();
            $('#telegram_channel_popup').fadeOut(100, function () {
                $("#wrapper").removeClass('blur');
            });
            Cookies.set('telegram_channel_popup', popup_status + 1 , { expires: 10 , path: '/' })
        });

        $telegram_chanel_popup.find('.join').click(function (e) {
