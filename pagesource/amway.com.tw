	<script>
		var TodayDate = new Date( "2018/03/19" );
		$(document).ready(function () {
	        var $slideBlockTemplate = $(
			'<li class="banner-item">' +
			'	<a href="#" class="banner-cover-btn ajax-link" onclick="">' +
			'		<img src="" class="banner-pic ajax-image" width="960" height="408" alt="">' +
			'	</a>' +
			'</li>');
			$.ajaxSetup({ cache: false });
	        $.ajax({
	            url: '/res/slides.js',
	            dataType: 'json'
	        }).done(function (data) {
				$.each(data.slides, function (key, item) {
					var isHidden = false;
					if ( item.begin != "" ) {
						var beginDate = new Date( item.begin );
						if ( TodayDate < beginDate ) isHidden = true;
					}
					if ( item.expire != "" ) {
						var expireDate = new Date( item.expire );
						if ( TodayDate > expireDate ) isHidden = true;
					}
					if ( ! isHidden ) {
						var $temp = $slideBlockTemplate.clone();
						$temp.find('.ajax-image').attr('src', item.image);
						$temp.find('.ajax-link').attr('href', item.link);
						$temp.find('.ajax-link').attr('onclick', "javascript:_gaq.push([ '_trackEvent' , 'banner' , 'clicked' , '"+item.link+"' ]);");
						$('.slide-ajax-list').append($temp);
					}
				});
				var mySwiper = $('.banner').swiper({
					wrapperClass: 'banner-list',
					slideClass: 'banner-item',
					pagination: '.banner-pager',
					paginationClickable: true,
					autoplayDisableOnInteraction: false,
					calculateHeight: true,
					mode: 'horizontal',
					autoplay: data.delay,
					speed: 500,
					loop: true,
					initialSlide: (data.begin_index - 1),
					onSlideChangeStart: function(swiper){
						$('.slide-ajax-list').find("li a img").each(function( index ) {
							$(this).attr("src", $(this).attr("src") );
						});
					}
				});
	        });
		});
	</script>

