		/** GENERAL **/
.tinymce h1, .tinymce h2, .tinymce h3, .tinymce h4, .tinymce h5, .tinymce h6 {margin-bottom:10px;}

[data-module="cta"].module.expand-width .wrap {max-width:850px;}

[data-module="product"].tinymce h2 {font-size:39px;}

.text-align-center h2, .text-align-center p {text-align:center;}

.panel-video-cta {display:none !important;}  /*** REMOVE THIS ONCE WE HAVE PRODUCT VIDEOS IN PLACE ***/

.page-template-contact .button-group, .page-template-contact hr {display:none;}

/*** FORMS ***/
.wrap .nav-resources select {border:1px solid #eee;}

.bounds [data-module="product"].module .product-cta-panel {background-color: #f4f7fa; left:-9px;}

.slug-agency-partners p {font-size:16px;}
.slug-agency-partners [data-module="logos"] p {font-size:18px;}



/** BACKGROUNDS **/

@media only screen and (max-width: 990px) {
.bounds [data-module="banner"].is-tall .banner-content {margin:0;}}

@media only screen and (max-width: 990px) {
.home .bounds [data-module="banner"].is-tall .banner-content {background-image: none !important; background-color: #2D3E50; }
body.home [data-module="banner"] .banner-content h1.banner-title {padding-top:0;}
}

.home [data-module="banner"].is-tall {max-height:800px; height:520px;}
.home [data-module="banner"].is-tall .banner-content .banner-content-inner {height:80%; }
.home [data-module="banner"] .banner-content {background-position: bottom; background-repeat:no-repeat;}
body.home [data-module="banner"] h1.banner-title {color:#fff; padding-top:70px;}
body.home [data-module="banner"] p {color:#fff; }

.bg-circles1flip {background-image: url("/wp-content/uploads/2018/03/circles1flip.png"); background-size: 100%;}

[data-module="panel"].module.is-announcement.bg-white {background-color:#fff;}

.bounds .module.bg-red {background-color:#df2f4d;}
.bg-red .button {background-color: transparent; border:1px solid #fff; color: #fff; }
.bg-red .button:hover {background-color: #fff; border:1px solid #fff; color: #df2f4d; opacity:1;}


/** PANELS & VISUAL EDITORS **/

.bounds [data-module="panel"].module.is-announcement .panel-cols .panel-col img {box-shadow:0 2px 7px 0 rgba(0, 0, 0, 0.3);}

.bounds [data-module="ve"].module.is-announcement {font-size:16px; background-color:#f4f7fa;}
.bounds [data-module="ve"]#brand-video .wrap {max-width:1000px;}
.bounds [data-module="ve"].module .cols.cols-2 .col {padding:0;}
.bounds [data-module="ve"].module .cols.cols-2 .col:last-child {margin-left:5%; width:45%;}
.bounds [data-module="ve"].module.is-announcement .panel-subtitle {font-size:16px;}
.bounds [data-module="ve"].module.is-announcement .panel-title {font-family: "AvenirNextLTW01-Medium", Helvetica, Arial, sans-serif; margin-top: 15px;margin-bottom: 9px;}
.bounds [data-module="ve"].module.is-announcement p {margin-top:0;}

@media only screen and (max-width: 800px) {
.bounds [data-module="ve"].module .cols.cols-2 .col:last-child { float: none; margin-left:0; margin-top: 30px; overflow: hidden; width: 100%; }
}


/** REDUCE PADDING & MARGINS **/

.tinymce.less-spacing h2 {margin-bottom:0;}
.tinymce.less-spacing h2 + p {margin-top:7px; padding-top;0;}
.tinymce.less-spacing .cols.margin-top {margin-top:4%;}
.bounds [data-module="panel"].module.is-announcement {padding-top:80px; padding-bottom:80px;}
.tinymce.padding-very-min {padding-top:55px !important; padding-bottom:50px !important;}

.tinymce.is-announcement h3 {margin-bottom:10px;}
.tinymce.is-announcement h2.panel-title {margin-top:15px; margin-bottom:9px;}
.tinymce.is-announcement h2.panel-title + p {margin-top:9px;}

.tinymce.together p {font-size: 16px; margin-bottom: 15px;}
.banner-content-inner h1  {line-height:1.2;}


/** NAV **/
header .nav-primary ul {margin-right:10px;}
header .nav-primary ul > li {font-family: "AvenirNextLTW01-Regular", Helvetica, Arial, sans-serif;}
.nav-submenu .nav-cta {margin-bottom:3px;}

/** STACK HARMONY **/

.home .shortcode-cards .shortcode-card {background-color:transparent; margin: 0 1.5%; width:30%}
@media only screen and (max-width: 700px) { .home .shortcode-cards .shortcode-card {margin: 5% 2.5%; width:95%}
}

.home .shortcode-cards .shortcode-card:hover {background-color:transparent;}
.home .tinymce .stack-harmony h2 { margin-bottom: 0; }
.home .shortcode-cards.margin-top {margin-top:2%;}
.home .shortcode-cards article.shortcode-card p {font-size:15px; margin:0;}
.home .shortcode-cards article.shortcode-card p.shortcode-caption { margin:9px 0 25px;}
.shortcode-cards a.button { font-size: 15px; margin: 0 auto; padding: 10px; width: 150px; }

#platform-stack .shortcode-cards .shortcode-card:hover {background-color:transparent;}
#platform-stack .tinymce .stack-harmony h2 { margin-bottom: 0; }
#platform-stack .shortcode-cards.margin-top {margin-top:2%;}
#platform-stack .shortcode-cards article.shortcode-card p {font-size:15px; margin:0;}
#platform-stack .shortcode-cards article.shortcode-card p.shortcode-caption { margin:9px 0 25px;}

.stack-harmony h2 {margin-bottom:10px;}
.stack-harmony p {font-size: 16px; margin-top:11px;}
.shortcode-cards .shortcode-card:first-child { border-top-left-radius: 7px; border-bottom-left-radius: 7px;}
.shortcode-cards .shortcode-card:last-child { border-top-right-radius: 7px; border-bottom-right-radius: 7px;}
.shortcode-cards .shortcode-card {height:180px;}
.shortcode-cards article.shortcode-card .shortcode-caption{font-size:17px;}

img.plus {height:20px; margin:22px 0; width:20px; }

/** BLOG **/
.resources-card .resources-card-thumbnail img {border-bottom: 1px solid #eee;}


/** PRODUCT MODULES (PRODUCT PAGE HEROS) **/

.product-wrap .wrap:first-of-type .wrap-inner {max-width:1000px;}
.product-wrap .wrap:first-of-type .wrap-inner h2 {margin-bottom:0;}
.product-wrap .wrap:first-of-type .wrap-inner p {margin-top:11px;}

/** BUTTONS **/

.bounds .button {font-family: "AvenirNextLTW01-Regular", Helvetica, Arial, sans-serif; font-size:17px; padding: 15px 35px;}
a.button:hover {opacity:.8;}
.outline-cta.tinymce .button {background-color: transparent; border:1px solid #df2f4d; color: #df2f4d; }
.outline-cta.tinymce .button:hover {background-color: #df2f4d; border:1px solid #df2f4d; color: #fff; }

#stack .button-forensiq {border-color:#e40046; color:#e40046; margin-right:10px;}
#stack .button-altitude {border-color:#a7b3bc; color:#a7b3bc; margin-right:10px;}
#stack .button-radius {border-color:#2d3e50; color:#2d3e50; margin-right:10px;}

#stack .button-forensiq:hover {background-color:#e40046; color:#fff;}
#stack .button-altitude:hover {background-color:#a7b3bc; color:#fff;}
#stack .button-radius:hover {background-color:#2d3e50; color:#fff;}



/** FOOTER **/

footer .nav-gutter li a {font-size: 12px; color: #bbb;}
footer .nav-gutter li:after {margin:0 8px 0 10px;}
	</style>

