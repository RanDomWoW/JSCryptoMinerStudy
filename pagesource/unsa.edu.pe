  /**Funcion que muestra el slider segun la categoria seleccionada
   * id: identificador de la categoria
   */
  function mostrar(identificador){

    var id = document.getElementById(identificador);

    id1 = document.getElementById('slider1');
    id2 = document.getElementById('slider2');
    id3 = document.getElementById('slider3');
    id4 = document.getElementById('slider4');
    id5 = document.getElementById('slider5');
    id6 = document.getElementById('slider6');

    efecto1 = document.getElementsByClassName('efecto1')[0];
