    efecto4 = document.getElementsByClassName('efecto4')[0];
    efecto5 = document.getElementsByClassName('efecto5')[0];
    efecto6 = document.getElementsByClassName('efecto6')[0];

    for (var i = 1; i <= 6; i++) 
    {
      if(eval('id' + i)) 
      {
        if(id==eval('id' + i))
        {
          id.style.display = 'block';
        }
        else
        {
          eval('id' + i).style.display = 'none';
          eval('efecto' + i).style.right = '-294px';
        }
      }
    }
    marcarCategoriaSeleccionada(identificador);
  }
  /**Funcion que resalta la categoria seleccionada en el Menu derecho
   * id: identificador de la categoria
   */
  function marcarCategoriaSeleccionada(id) {
    for (var y = 1; y <= 6; y++) {
      var name= "#men_cate"+y;
      if($(name).length > 0) {
        $(name).removeClass("unsaback");
      }
      var z = id.substr(id.length - 1); 
      name= "#men_cate"+z;
      $(name).addClass("unsaback");
    }
  }
  
