    var pid = prx_pid||"0";
    var category = prx_categoryname||"";     
    fbq('track','ViewContent',{content_ids:[pid], content_type:'product'});   
    $(window).on('cart:itemAdded', function(e, itemData){
        fbq('track', 'AddToCart', {content_ids: pid,content_type: 'product'});        
    });
}           
else if(/\/(dog|cat|pharmacy)\/\S+\/\S+/.test(location.pathname)){ 
    var pids = $('[data-pid]').map(function(idx,el){return $(el).data('pid');}).toArray();       
    var arr  = $('#breadcrumbs').text().trim().replace(/\s{2,}/g,' ').replace(/\s?\/\s?/g,'/').split('/');
    arr.shift();
    var category = arr.pop();
    var category_path = arr.join(' > ');              
