	font-size: 8pt;
	font-weight: 700;
	color: #555;
	width: 323px;
	height: 14px;
	text-align: left;
	white-space: nowrap;
	overflow: hidden;
}
 
.progressBarInProgress,
.progressBarComplete,
.progressBarError {
	font-size: 0;
	width: 0%;
	height: 2px;
	background-color: blue;
	margin-top: 2px;
}
 
.progressBarComplete {
	width: 100%;
	background-color: green;
	visibility: hidden;
}
 
.progressBarError {
	width: 100%;
	background-color: red;
	visibility: hidden;
}
 
.progressBarStatus {
	margin-top: 2px;
	width: 337px;
	font-size: 7pt;
	font-family: Arial;
	text-align: left;
	white-space: nowrap;
}
 
a.progressCancel {
	font-size: 0;
	display: block;
	height: 14px;
	width: 14px;
	float: right;
}
 
a.progressCancel:hover {
	background-position: 0px 0px;
}
 
 
.swfupload {
	vertical-align: top;
