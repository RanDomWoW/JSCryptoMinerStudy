				            slideBy: 2,
				            center:false,
				            loop:true
				        },
				        767:{
				            items:3,
				            nav:true,
				            slideBy: 3,
				            center:true,
				            loop:true
				        },
				        991:{
				            items:4,
				            slideBy: 4,
				            nav:true,
				            loop:true
				        }
				    }
				});

				$('.owl-carousel.review-carousel').owlCarousel({
				    items:1,
				    center: false,
					navText: [
					   "<img src='"+theme_path+"/images/side-arrow-left.png'>",
					   "<img src='"+theme_path+"/images/side-arrow-right.png'>"
					],
				    responsive:{
				        0:{
				        	items:2,
				            margin:10,
				            loop:true,
    						autoWidth:true
				        },
				        575:{
				            items:1,
				            nav:true,
				            center:true,
				            loop:true,
				            margin:10
				        },
				        991:{
				            items:2,
				            nav:true,
				            loop:true,
				            margin:30
				        }
				    }
				});
