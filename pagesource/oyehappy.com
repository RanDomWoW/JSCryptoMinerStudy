			$(document).on('click', '.accordion-toggl', function(event){
				event.preventDefault();
	            $('.collapse').removeClass('show');
	            $(this).addClass('show');
	        });	        
			function display_customer_story(){
				$('.cust-comment').css('display', 'none');
				var rel = $('.avatar-carousel').find('.center .item').attr('rel');
				$('#'+rel).css('display', 'block');
			}
			
			var a_owl = $('.avatar-carousel');
			a_owl.children().each( function( index ) {
			  	$(this).attr( 'data-position', index ); // NB: .attr() instead of .data()
			});
			a_owl.owlCarousel({
			    center: true,
			    items:5,
			    loop:true,
			    margin:0,
			    nav:false,
			    autoWidth:true,
			    responsive:{
			        600:{
			            items:4
			        }
			    }
			});
			$(document).on('click', '.owl-item>div', function() {
			  	a_owl.trigger('to.owl.carousel', $(this).data( 'position' ) ); 
			  	display_customer_story();
			});
			a_owl.on('changed.owl.carousel', function(event) {
			    display_customer_story();
			});			
			display_customer_story();


	      	$(document).ready(function(){
	      		//Desktop menu
		        $('.hidden-menu-items').hide();
		        $(document).on('click', '.menu-toggl a', function(event){
		        	//event.stopPropagation();
		        	event.preventDefault();
		        	$('.hidden-menu-items').hide();
		            if (!$(this).closest('li').hasClass('current-menu-item')) {
						$($(this).attr('rel')).toggle();
						$('#menu-primary-menu .nav-item.current-menu-item').removeClass('current-menu-item');
						$($(this).closest('li').addClass('current-menu-item'));
					}else{
						$('#menu-primary-menu .nav-item.current-menu-item').removeClass('current-menu-item');
					}
					$('#hidden-filters').hide();
					$('.filter-options').removeClass('active');
		        });

		        

		        $(document).on("click", function () {
		        	$('#menu-primary-menu .nav-item.current-menu-item').removeClass('current-menu-item');
    				$(".hidden-menu-items").hide();
    				$("#hidden-filters").hide();

    				//For mobile menu
    				$('.mobile-main-menu').hide();
    				$('body').removeClass('noScroll');
				});
				
				//Open the hamburger menu
		        $('.mobile-main-menu ').hide();
		        $(document).on('click', '.mobile-hamburger .hamburgerIcon', function(event){
		            $('.mobile-main-menu ').show();
		            $('body').addClass('noScroll');
		            event.stopPropagation();
		        });

		        $(document).on('click', '#menu-primary-menu, .hidden-menu-items', function(event){
			        event.stopPropagation();
			    });

			    $(document).on('click', '#hidden-filters, .filter-options', function(event){
			        event.stopPropagation();
			    });

			    $(document).on('click', '.mobile-main-menu', function(event){
			        event.stopPropagation();
			    });
			    
				
				$(document).on('click', '.closemenu', function(event){
					event.preventDefault();
					$('.mobile-main-menu').hide();
					$('body').removeClass('noScroll');
				});

				//Main menu items
				$(document).on('click', '.mobile-main-menu .navbar-nav .nav-link', function(event){
					event.preventDefault();
					if ($(this).attr('rel') == 1) {
			            window.location.replace($(this).attr('href'));
			        }else{
			        	$('.mobile-main-menu .navbar-nav, .login-btn-group, #main-title, .menu-options-collapse').hide();
			            $('.return-menu, '+$(this).attr('rel')).show();
			        }
		        });

		        //Back link
		        $(document).on('click', '.back-to-menu', function(event){
		        	event.preventDefault();
		        	$('.mobile-main-menu .navbar-nav').show();
		        	$('.menu-options-collapse').hide();
		        	$('#main-title').show();
		        	$('.login-btn-group').show();
		        	$('.return-menu').hide();
		        });

		        //Sub categories
		        $(document).on('click', '.menu-list-heading', function(event){
		        	event.preventDefault();
		        	var $this = $(this);
		        	var isActive = $this.hasClass('active');
				    $(".menu-list-heading").removeClass("active");
				    isActive ? $this.removeClass("active") : $this.addClass("active");
		        });

		        //Changing the menu name
		        $('.mobile-main-menu .nav-item.link-names .nav-link').click(function(){
				 	var $menuItemName = $(this).text();
				 	$('#clicked-name').text($menuItemName);
				});

				//Search toggle
				$('#main-header .sprite.sprite-search').on('click', function(event){
					event.preventDefault();
					$('#menu-primary-menu .nav-item.action').removeClass('action');
					$('.hidden-menu-items').hide();
		          	$('#searchbar').slideToggle(300, function(){
			            // callback after search bar animation
			        });
				});
	 		});


	      	//For mobile footer links
	      	$(document).on('click', '.down-arrow-icon', function(event){
	      		event.preventDefault();
                var $this = $(this);
                var isActive = $this.hasClass('active');
                $(".down-arrow-icon").removeClass("active");
                isActive ? $this.removeClass("active") : $this.addClass("active");
            });
            

            $('.add-wishlist, .add-wishlist-btn').click(function(event){
            	event.preventDefault();
            	var loc = window.location;
            	var wishlist_count = $('.wishlist_count').html();
            	var the = $(this);
            	var rel = $(this).attr('rel');
				var request = $.ajax({
					url: "https://www.oyehappy.com/add-wishlist/",
					method: "POST",
					data: { id : rel }
				});
				//console.log(request); 
				request.done(function( msg ) {
					switch(msg){
						case 'notlogin':
							swal({
							  title: "Oops!",
							  text: "Please login to add surprises to wishlist.<br> <a href='https://www.oyehappy.com/my-account/?redirect="+loc+"'>Click here to login</a>.",
							  html: true
							});
							break;
						case 'added':
							$(the).find('i').addClass('heart-active');
							$('.wishlist_count').html( parseInt(wishlist_count) + 1);
							sweetAlert("Awesome!", "Surprise added to wishlist. Wish more!", "success");
							break;
						case 'deleted':
							$(the).find('i').removeClass('heart-active');
							$('.wishlist_count').html( parseInt(wishlist_count) - 1);
							sweetAlert("Removed!", "Surprise is off your wishlist.", "info");
							break;
						case 'idmissing':
							sweetAlert("Oops...", "Product id is missing", "error");
							break;
						default:
							//sweetAlert("Success", "Product added to Wishlist", "success");
					}				  	
				});			 
				request.fail(function( jqXHR, textStatus ) {
					sweetAlert("Oops...", "Something went wrong please try again.", "error");
				});
            });

            /**
            * If "redirect" element exist in 'payment-confirmation.php' redirect to 'thank-you.php' file
            */
            var redirect_link = $(document).find('#redirect').attr('rel');
			if(redirect_link != null){
				window.setTimeout(function(){ window.location = redirect_link; },2000);
			}
			if (navigator.userAgent.indexOf('Safari') != -1 && navigator.userAgent.indexOf('Chrome') == -1)  {
		   		$("#main-header").css({"padding-bottom": "0"});
			    $(".hamburgerIcon").css({"right": "90%"});
			    $(".menuDesktop").css({"width": "86%"});
			}
		})(jQuery);		
