                .css({ zIndex: 0, cursor: 'pointer' })
                .on('DOMNodeInserted', 'iframe', function () {
                    /**
                     * Prevent default iframe click and share event.
                     * Parent: z-index: 0
                     * iFrame: z-index: -1
                     */
                    $(this).css({ 'z-index': -1 });
                })
                .on('click', function(event) {
                    /**
