	function showHappeningNow(){
		if ($("#BCLASS-HouseToday").hasClass("sitting")) {
			$(".BCLASS-Sitting").show()
		} else if ($("#BCLASS-HouseToday").hasClass("nonsitting")) {
			$(".BCLASS-Non-Sitting").show()
		}
	}
	function setBoxSize() {
	if (!$("#ie8").length) {
		var homeBoxHeight = [];
		var i = 0;
		$("#BCLASS-HomeBanner-Content .panel").each(function() { 
		homeBoxHeight.push($(this).height())
		i++
		});
		if (homeBoxHeight[0] > homeBoxHeight[1]) {
		$("#BCLASS-HomeBanner-Content .panel").eq(0).height(homeBoxHeight[0])
		$("#BCLASS-HomeBanner-Content .panel").eq(1).height(homeBoxHeight[0])
		} else {
		$("#BCLASS-HomeBanner-Content .panel").eq(0).height(homeBoxHeight[1])
		$("#BCLASS-HomeBanner-Content .panel").eq(1).height(homeBoxHeight[1])
		}
		if (homeBoxHeight[2] > homeBoxHeight[3]) {
		$("#BCLASS-HomeBanner-Content .panel").eq(2).height(homeBoxHeight[2])
		$("#BCLASS-HomeBanner-Content .panel").eq(3).height(homeBoxHeight[2])
		} else {
		$("#BCLASS-HomeBanner-Content .panel").eq(2).height(homeBoxHeight[3])
		$("#BCLASS-HomeBanner-Content .panel").eq(3).height(homeBoxHeight[4])
		}
	}
	}
	_spBodyOnLoadFunctionNames.push("showHappeningNow");
	if ($(window).width()>768) {
		_spBodyOnLoadFunctionNames.push("setBoxSize");
	}
	
	</script>




