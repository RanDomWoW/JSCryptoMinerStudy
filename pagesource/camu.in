<!DOCTYPE html>
<html lang="en" ng-app="camu">
	<head>
		<meta charset="utf-8">
		<title>Login Page - Camu</title>
        
		<meta name="description" content="User login page">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">

		<!-- <link rel="stylesheet" href="assets/css/bootstrap.css"  /> -->
		<!-- <link rel="stylesheet" href="assets/css/bootstrap-responsive.min.css"  /> -->

		<!-- <link rel="stylesheet" href="assets/css/font-awesome.css" />

		<link rel="stylesheet" href="assets/css/ace.css" />
		<link rel="stylesheet" href="assets/css/ace-fonts.css" />
		<link rel="stylesheet" href="assets/css/ace-skins.min.css" />
		<link rel="stylesheet" href="assets/css/ace-responsive.min.css" />
		
		<link rel='stylesheet' href='assets/css/loading-bar.min.css'  /> -->
		<link rel="stylesheet" href="assets/css/vendor.min.css?v=7542763">
	</head>
    
	<body class="login-layout" ng-controller="LoginCtrl">
		<div class="main-container">
			<div class="main-content">
				<div class="row">
					<div class="col-md-10 col-md-offset-1">
						<div class="login-container">
							<div class="row">
								<div class="center">
									<h1>
										<i class="green ace-icon fa fa-leaf"></i>
										<span class="red">{{"CAMU."|translate}}</span>
										<span class="white">{{"YOUR_CAMPUS."|translate}}</span>
									</h1>
									<h4 class="blue">{{"Â©_OCTOZE_TECHNOLOGIES._COPYRIGHT_2016-2017."|translate}}</h4>
								</div>
							</div>
                            
							<div class="space-6"></div>

							<div class="row">
								<div class="position-relative">
									<div id="login-box" class="login-box visible widget-box no-border">
										<div class="widget-body">
											<div class="widget-main">
												<h4 class="header blue lighter bigger">
													<i class="red ace-icon fa fa-key"></i>{{"PLEASE_ENTER_YOUR_INFORMATION"|translate}}</h4>

												<div class="space-6"></div>

												<form autocomplete="off">
														<div style="display:none;">
															<input type="password">
														</div>
													<fieldset>
														<label class="block clearfix">
															<span class="block input-icon-right input-icon">
																<input id="uname" type="text" class="form-control" placeholder="{{&quot;USER_NAME&quot;|translate}}" ng-model="login.name" ng-change="clearError()" autofocus>
																<i class="ace-icon fa fa-user"></i>
															</span>
														</label>

														<label class="block clearfix">
															<span class="block input-icon-right input-icon">
																<input type="password" class="form-control" placeholder="{{&quot;PASSWORD&quot;|translate}}" ng-model="login.pwd" id="pwd" ng-change="clearError()">
																<i class="ace-icon fa fa-lock"></i>
															</span>
														</label>

														<div class="space"></div>
                                                        <div ng-show="login.error" class="alert alert-danger" ng-cloak="">
                                                            <strong>{{"WARNING!"|translate}}</strong> {{login.error}}
                                                        </div>

														<div class="clearfix">
															<label class="inline">
																<input type="checkbox" class="ace" disabled>
																<span class="lbl">{{"REMEMBER_ME"|translate}}</span>
															</label>
															<button class="width-35 pull-right btn btn-primary btn-sm" ng-click="doLogin()">
																<i class="ace-icon fa fa-key"></i>{{"LOGIN"|translate}}</button>
														</div>
														<div class="space-4"></div>
													</fieldset>
												</form>
                                                
												<div class="social-or-login center">
													<span class="bigger-110">{{"OR_LOGIN_USING"|translate}}</span>
												</div>
                                                
												<div class="social-login center">
													<a class="btn btn-primary">
														<i class="ace-icon fa fa-facebook"></i>
													</a>
                                                    
													<a class="btn btn-info">
														<i class="ace-icon fa fa-twitter"></i>
													</a>
                                                    
													<a class="btn btn-danger">
														<i class="ace-icon fa fa-google-plus"></i>
													</a>
												</div>
											</div><!--/widget-main-->
                                            
											<div class="toolbar clearfix">
												<div>
													<a href="#" onclick="show_box(&apos;forgot-box&apos;); return false;" class="forgot-password-link">
														<i class="ace-icon fa fa-arrow-left"></i>{{"I_FORGOT_MY_PASSWORD"|translate}}</a>
												</div>
                                                
												<div>
													<a href="#" onclick="show_box(&apos;signup-box&apos;); return false;" class="user-signup-link">{{"I_WANT_TO_REGISTER"|translate}}<i class="ace-icon fa fa-arrow-right"></i>
													</a>
												</div>
											</div>
										</div><!--/widget-body-->
									</div><!--/login-box-->
                                    
									<div id="forgot-box" class="forgot-box widget-box no-border">
										<div class="widget-body">
											<div class="widget-main">
												<h4 class="header red lighter bigger">
													<i class="ace-icon fa fa-key"></i>{{"RETRIEVE_PASSWORD"|translate}}</h4>
                                                
												<div class="space-6"></div>
												<p>{{"ENTER_YOUR_EMAIL_AND_TO_RECEIVE_INSTRUCTIONS"|translate}}</p>
                                                
												<form>
													<fieldset>
														<label class="block clearfix">
															<span class="block input-icon-right input-icon">
																<input type="email" class="col-md-12" placeholder="{{&quot;EMAIL&quot;|translate}}">
																<i class="ace-icon fa fa-envelope"></i>
															</span>
														</label>
                                                        
														<div class="clearfix">
															<button onclick="return false;" class="width-35 pull-right btn btn-danger btn-sm">
																<i class="ace-icon fa fa-lightbulb-o"></i>
																<span class="bigger-110">{{"SEND_ME!"|translate}}</span>
															</button>
														</div>
													</fieldset>
												</form>
											</div><!--/widget-main-->
                                            
											<div class="toolbar center">
												<a href="#" onclick="show_box(&apos;login-box&apos;); return false;" class="back-to-login-link">{{"BACK_TO_LOGIN"|translate}}<i class="ace-icon fa fa-arrow-right"></i>
												</a>
											</div>
										</div><!--/widget-body-->
									</div><!--/forgot-box-->
                                    
									<div id="signup-box" class="signup-box widget-box no-border">
										<div class="widget-body">
											<div class="widget-main">
												<h4 class="header green lighter bigger">
													<i class="blue ace-icon fa fa-users"></i>{{"NEW_USER_REGISTRATION"|translate}}</h4>
                                                
												<div class="space-6"></div>
												<p>{{"ENTER_YOUR_DETAILS_TO_BEGIN:"|translate}}</p>
                                                
												<form>
													<fieldset>
														<label class="block clearfix">
															<span class="block input-icon-right input-icon">
																<input type="email" class="col-md-12" placeholder="{{&quot;EMAIL&quot;|translate}}">
																<i class="ace-icon fa fa-envelope"></i>
															</span>
														</label>
                                                        
														<label class="block clearfix">
															<span class="block input-icon-right input-icon">
																<input type="text" class="col-md-12" placeholder="{{&quot;USERNAME&quot;|translate}}">
																<i class="ace-icon fa fa-user"></i>
															</span>
														</label>
                                                        
														<label class="block clearfix">
															<span class="block input-icon-right input-icon">
																<input type="password" class="col-md-12" placeholder="{{&quot;PASSWORD&quot;|translate}}">
																<i class="ace-icon fa fa-lock"></i>
															</span>
														</label>
                                                        
														<label class="block clearfix">
															<span class="block input-icon-right input-icon">
																<input type="password" class="col-md-12" placeholder="{{&quot;REPEAT_PASSWORD&quot;|translate}}">
																<i class="ace-icon fa fa-retweet"></i>
															</span>
														</label>
                                                        
														<label class="block clearfix">
															<input type="checkbox" class="ace">
															<span class="lbl">{{"I_ACCEPT_THE"|translate}}<a href="#">{{"USER_AGREEMENT"|translate}}</a>
															</span>
														</label>
                                                        
														<div class="space-24"></div>
                                                        
														<div class="clearfix">
															<button type="reset" class="width-30 pull-left btn btn-sm">
																<i class="ace-icon fa fa-refresh"></i>{{"RESET"|translate}}</button>
                                                            
															<button onclick="return false;" class="width-65 pull-right btn btn-success btn-sm">{{"REGISTER"|translate}}<i class="ace-icon fa fa-arrow-right fa-on-right"></i>
															</button>
														</div>
													</fieldset>
												</form>
											</div>
                                            
											<div class="toolbar center">
												<a href="#" onclick="show_box(&apos;login-box&apos;); return false;" class="back-to-login-link">
													<i class="ace-icon fa fa-arrow-left"></i>{{"BACK_TO_LOGIN"|translate}}</a>
											</div>
										</div><!--/widget-body-->
									</div><!--/signup-box-->
								</div><!--/position-relative-->
							</div>
						</div>
					</div><!--/.span-->
				</div><!--/.row-fluid-->
			</div>
		</div><!--/.main-container-->
        
		<!--basic scripts-->
        
		<script type="text/javascript">
//			window.jQuery || document.write("<script src='assets/js/jquery-2.0.3.min.js'>"+"<"+"/script>");
            </script>
        
		<!--<![endif]-->

		<!--[if IE]>
         <script type="text/javascript">
         window.jQuery || document.write("<script src='assets/js/jquery-1.10.2.min.js'>"+"<"+"/script>");
         </script>
         <![endif]-->
		<script type="text/javascript">
//			if("ontouchend" in document) document.write("<script src='assets/js/jquery.mobile.custom.min.js'>"+"<"+"/script>");
            </script>
		<script src="/assets/js/vendor.min.js?v=7542763"></script>
		
		<script src="/app/app.min.js?v=11602"></script>	

		<script type="text/javascript">
			function show_box(id) {
                jQuery('.widget-box.visible').removeClass('visible');
                jQuery('#'+id).addClass('visible');
			}
            </script>
	</body>
</html>