    <script type="text/javascript">
        $(document).ready(function(){ 
        	$("#search-input").bind("keypress", {}, keypressInBox);
        
            $('#send-search').click( function(e) {
                e.preventDefault;
                var q = $('#box-cerca .search').val();
                window.location.href = "https://www.bicego.it/it/risultati-ricerca?q=" + q;
            });
            var q_text;
            $('#search-input').bind("cut copy paste",function(e) {
             e.preventDefault();
             });
            $('#search-input').bind("cut copy paste",function(e) {
             e.preventDefault();
             });
            $('#search-input').on('input', function() {
                  $(this).val($(this).val().replace(/[^a-z0-9 àèéùìò]/gi, ''));
            });
           $('#search-input').autocomplete({     
                groupBy: 'category',
                serviceUrl: 'https://www.bicego.it/search.php?action=get_suggests',
                minChars: 2,
                triggerSelectOnValidInput: false,  
                formatResult: function (suggestion, currentValue) {
                     text = suggestion.value;
                     var Search = currentValue.charAt(0).toUpperCase() + currentValue.slice(1);;
                     text = text.replace(new RegExp(currentValue, 'g'), '<strong>'+currentValue+'</strong>');
                     text = text.replace(new RegExp(Search, 'g'), '<strong>'+Search+'</strong>');
                     //text.replace('/'+currentValue+'/gi','<strong>'+currentValue+'</strong>');
                     
                     return text;
                },
                onSearchComplete: function (query, suggestions) {
                    q_text = query;
                    var text = $('.autocomplete-group:first-child strong').text();
                    if(text == 'lista') {
                        $('.autocomplete-group:first-child').remove();
                    } 
                }, 
                onSelect: function (suggestion) {   
                    var value = suggestion.data.q; 
                    $('#box-cerca .search').val(value);
                    if(suggestion.data.tipo == 'offerta') {       
                        window.location.href = "https://www.bicego.it/offertelavoro/" + suggestion.data.tipo_url;
                    }
                    else if(suggestion.data.tipo == 'categoria') {       
                        window.location.href = "https://www.bicego.it/offertelavoro/" + suggestion.data.tipo_url;
                    }
                    else if(suggestion.data.tipo == 'strumento') {       
                        window.location.href = "https://www.bicego.it/it/" + suggestion.data.tipo_url;
                    }
                    else if (suggestion.data.tipo == 'lista_completa') {
                        window.location.href = "https://www.bicego.it/it/risultati-ricerca?q=" + q_text;
                    }
                    else {    
                        window.location.href = "https://www.bicego.it/it/risultati-ricerca?q=" + value;
                    }
                }   
            });	
		});	
		
		function keypressInBox(e) { 
		    var code = (e.keyCode ? e.keyCode : e.which);
		    if (code == 13) { //Enter keycode                        
		        e.preventDefault();
		
		        $("#form_search").submit();
		    }
		};	 
	</script>







