			if (!isPreview)
				$('.normal.owl-carousel .teaser').appendTo('.mobile.owl-carousel');
			else
				$('.normal.owl-carousel .teaser').parent().appendTo('.mobile.owl-carousel');

			$('.img-normal').hide();
			$('.img-mobile').show();
			$('.img-mobile img').each(function () {
				url = $(this).data('url');
				$(this).attr('src', url);
			});

			if (!isPreview) {
				$('.mobile.owl-carousel').owlCarousel({
					margin: 10,
					loop: true,
					responsiveClass: true,
					dots: true,
					autoplay: false,
					nav: false,
					lazyLoad: true,
					responsive: {
						0: {
							items: 1
						},
						600: {
							items: 1
						}
					}
				});
			} else {
				$('.mobile.owl-carousel').show();
			}
		} else {
			$('.img-normal img').each(function () {
				url = $(this).data('url');
				$(this).attr('src', url);
			});
			//var size = $('.normal.owl-carousel').children('div').length;
			//var scripts = $('.normal.owl-carousel').children('script').length;
			//if(size === scripts) {
			//    size = 0;
