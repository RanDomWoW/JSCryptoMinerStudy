			var owl = $('.normal.owl-carousel');
			if (!isPreview) {
                owl.owlCarousel({
					margin: 10,
					autoHeight: true,
					loop: owl.children('div').length > 1,
					responsiveClass: true,
					dots: owl.children('div').length > 1,
					navText: ["<i class='fa fa-angle-left'></i>", "<i class='fa fa-angle-right'></i>"],
					autoplay: false,
					items: 1,
					lazyLoad: true,
					nav: owl.children('div').length > 1
				});
			} else {
				$('.normal.owl-carousel').show();
			}
		}
	})();

