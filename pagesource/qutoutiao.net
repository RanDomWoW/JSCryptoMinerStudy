<script>
    var str = location.pathname.split('/').pop().split('.')[0];
    $(".p_head .nav li").removeClass('active');
    if(new RegExp('report').test(str)){
        str = 'report';
    }
    switch (str) {
        case 'home':
            $(".p_head .nav .j_home").addClass('active');
            break;
        case 'about':
            $(".p_head .nav .j_about").addClass('active');
            break;
        case 'media':
            $(".p_head .nav .j_media").addClass('active');
            break;
        case 'report':
            $(".p_head .nav .j_about").addClass('active');
            break;
        case 'platform':
            $(".p_head .nav .j_platform").addClass('active');
            break;
        default:
            $(".p_head .nav .j_home").addClass('active');
            break;
    }
    $('.header .logo').on('click',function(){location.href='home.html'});
    window.onscroll = function(){
        var scrolltop = document.documentElement.scrollTop || document.body.scrollTop;
        if(scrolltop > 1600) {
            $('.w_top').show();
        }else {
            $('.w_top').hide();
        }
    }
    $('.w_top').on('click', function(event) {
        $('body,html').animate({scrollTop:0},500);
    });
</script>
