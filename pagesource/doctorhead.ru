						});
						$(".qs_item:first a").css("background","#3dbcd4");
						$(".qs_item:first a").addClass("active");
						$("#q0 a").css("background","#3dbcd4");
						$("#q0 a").addClass("active");
						CURRENT = -1;
					},1000);
				}
			}
			


			
		});
		clicked = 0;
		$(".qs_item").live("click", function(){
			clicked = 1;
			$('#SEARCH_SUGGEST').parent().hide();
		});
		
		$('#SEARCH_QUERY').focusout(function(){
