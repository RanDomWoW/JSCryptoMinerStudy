			})
		}
		//种类切换
		var class_url = "/index.php?c=ajax&a=changeListById";
		function changeListById(obj,catid,tid,num,url,id,tname,label,more){
			$.ajax({
				type:"post",
				url:class_url,
				dataType: "json",
				data:{catid:catid,tid:tid,page:1,pageSize:12},
				success:function(data) {
					var html = spliceClassHtml(data);
					$("#list"+num).html(html);
					var lia=$(obj).parent().children("a");
					lia.each(function(){
						$(this).find("li").eq(0).removeClass("on");
					})
					$(obj).find("li").eq(0).addClass("on");
					$("#"+id).html(tname);
					$("#"+url).attr("href","/top/"+label+".html");
					if(tid==0 && catid==1 && num==1){
						$("#"+more).attr("href","/android/");
					}else if(tid==0 && catid==1 && num==2){
						$("#"+more).attr("href","/android/10.html");
					}else if(tid==0 && catid==2 && num==3){
						$("#"+more).attr("href","/iphone/");
					}else if(tid==0 && catid==2 && num==4){
						$("#"+more).attr("href","/iphone/10.html");
					}else if(tid!=0 && catid==1){
						$("#"+more).attr("href","/android/"+tid+".html");
					}else if(tid!=0 && catid==2){
						$("#"+more).attr("href","/iphone/"+tid+".html");
					}
				}
			})
		}

		function spliceClassHtml(dts){

			var html = "";
			for (var key in dts) {
				html+='<a href="'+dts[key]['url_link']+'" target="_blank">';
				html+='			<li>';
				html+='			<img src="'+dts[key]['img_logo']+'">';
				html+='		<p>'+dts[key]['title']+'</p>';
				html+='</li>';
				html+='</a>';
			}

			return html;
		}
	</script>
