    function heibgYahoo(remove) {
        var docu = document;
        if (remove && docu.getElementById("heidiv")) {
            docu.body.removeChild(docu.getElementById("heidiv"));
        } else if (!docu.getElementById("heidiv")) {
            if (remove) return;
