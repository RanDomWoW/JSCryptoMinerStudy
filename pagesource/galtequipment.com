<script type="text/javascript">
	//header menu btn click
	var menu_open = true;
	var opened = false;
	var ele_Menu = document.getElementById("hd-menu-btn");
	var menu_list = document.getElementById("hd-menu-list");
	var ele_cover = document.getElementById("hd-menu-list");
	ele_Menu.onclick = function(){
		if(menu_open){ 
			if(opened){
				return false;
			}else{
				ele_Menu.className += " hd-menu-open";
				opened = true;
				setTimeout(function(){
					menu_list.className += " hd-mlist-open";
					menu_open = false;
					opened = false;
					
				})
			}
		}else{ 
			if(opened){
				return false;
			}else{
				ele_Menu.className = "hd-menu-btn";
				menu_list.className += " hd-mlist-closed";
				opened = true;
				setTimeout(function(){
					menu_list.className = "hd-menu-list";
					menu_open = true;
					opened = false;
				});
			}
		}
	}
	ele_cover.onclick = function() {
		ele_Menu.className = "hd-menu-btn";
		menu_list.className += " hd-mlist-closed";
		opened = true;
		setTimeout(function(){
			menu_list.className = "hd-menu-list";
			menu_open = true;
			opened = false;
		});
	}
</script>