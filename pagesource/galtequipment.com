<script>
var _pa = 1;
var is_can_next = 1;
var cur_url = "ajax";

function loaddata(){
    totalheight = parseFloat(jQuery(window).height()) + parseFloat(jQuery(window).scrollTop());     
 
    if (jQuery(document).height() <= totalheight)     
    {
        if(is_can_next == 1)
            {
                is_can_next = 0;
                nextpage();
            }
    }
}
 
function nextpage(){
    var best_url = cur_url+'?page='+_pa+'/';
   
    jQuery('#center_pullup').show();
    
    jQuery.ajax({
        url: best_url,
        dataType: 'json',
        success: function(result){
            var str = "";
            var length = 0;
            jQuery.each(result,function(){
                length++;
            });
            if(length!=0){
              jQuery.each(result.arr,function(index,array){

                str = "<li>\
                        <a href=\"/"+array['slug']+"/\"><img src=\""+array['cover_img']+"\"></a>\
                        <h3>\
                          <a href=\"/"+array['slug']+"/\">"+array['title']+"</a>\
                        </h3>\
                      </li>";
                  jQuery(".list-ul").append(str);
                  // jQuery(".fade").animate({opacity: 1},1000);
              })
            _pa++;
            is_can_next = 1;
            jQuery('#center_pullup').fadeOut(100);
          }else{
            jQuery('#center_pullup img').attr("src","/Public/Images/end.png");
             setTimeout(function(){
              jQuery('#center_pullup').fadeOut(100);
            }, 1000);
          };
        }
      }
    );
}
 
jQuery(function(){
    jQuery(window).scroll( function() {
        loaddata();
    });
})
 
</script>
