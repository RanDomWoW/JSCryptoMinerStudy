	<script type="text/javascript">
		/* Sticky Ads */
		document.addEventListener("DOMContentLoaded", function(){

			var stickyAds = document.getElementsByClassName('ad-unit-sticky');

			if (stickyAds.length != 0) {
		        var last_known_scroll_position = 0;
		        var ticking = false;

		        function manangeScrollPos(scroll_pos, ad) {
		            var content = ad.getElementsByClassName('textwidget')[0],
		                disclaimer = ad.getElementsByClassName('disclaimer')[0];

		            if (scroll_pos > ad.offsetTop) {
		                content.style.position = 'fixed';
		                content.style.top = '107px';
		                disclaimer.style.position = 'fixed';
		                disclaimer.style.top = '90px';
		            } else {
		                content.style.position = 'relative';
		                content.style.top = '0';
		                disclaimer.style.position = 'relative';
		                disclaimer.style.top = '0';
		            }
		        }

		        window.addEventListener('scroll', function (e) {
		            last_known_scroll_position = window.scrollY;
		            if (!ticking) {
		                window.requestAnimationFrame(function () {
		                    for (var i = 0; i < stickyAds.length; i++) {
		                        manangeScrollPos(last_known_scroll_position, stickyAds[i]);
		                    }
		                    ticking = false;
		                });
		            }
		            ticking = true;
		        });
		    }

		});
	</script>

