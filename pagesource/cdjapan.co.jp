      function GetCookies() {
        var result = new Array();
        var ck = document.cookie;
        if ( ck != '' ) {
          var cookies = ck.split( '; ' );
          for ( var i = 0; i < cookies.length; i++ ) {
            var cookie = cookies[ i ].split( '=' );
            result[ cookie[ 0 ] ] = decodeURIComponent( cookie[ 1 ] );
          }
        }
        return result;
      }
