<script charset="utf-8" type="text/javascript">
$(function() {
  RR.jsonCallback = function() {
    var dt = RR.data.JSON.placements || [];

    for (i = 0; i < 3; i++) {
    $.ajax({
      url: 'http://www.cdjapan.co.jp/api/rr/1',
      type: 'POST',
      data: {'rcdata' : JSON.stringify(dt[i]), sid : i + 1, rpp : i ? 20 : 10},
      timeout: 30000,
      success: function(ds) {
        if (ds && ds.length) {
          var d = ds.split( "\t" );
          if (d[0]) {
          $('#rrrecom' + d[2]).html(
            '<div id="third-menu-center' + (d[2] == 1 ? '' : d[2]) + '" class="col4 colo cal_list">' +
            '<h4 class="spec-hl">' + d[1] + '</h4>' +
            '<ul class="items item-thumb small" id="detail-recommend-item' + d[2] + '"></ul></div>' + 
            '<div class="rdum" style="height:2.2em;"></div>');
          $('#detail-recommend-item' + d[2]).append(d[0]);
          if (d[2] == 1) {
            $('#rr-side-data').append(d[0]);
            $('#rr-side-title').html(d[1]);
          }
          nwapi.transCurrency('.price-jp-yen', '.price-exchange');

          save_width2 = slwidth('.cal_list');
          rrload_stop[d[2]] = 1;
          rrsliders[d[2]] = $('#detail-recommend-item' + d[2]).bxSlider(
            { responsive: false, maxSlides: 5, hideControlOnEnd: true, infiniteLoop: true, slideWidth: save_width2, onSliderLoad: function (obj) { rrload_stop[d[2]]=0;$('.rdum').hide(); } }
          );
          }
        }
      },
      error:  function(req, st) {
        console.log('err:', st);
      }
    });
    }
  };
});

$(window).resize(function(){
  var new_width2 = slwidth();
  if (save_width2 != new_width2) {
    save_width2 = new_width2;
    var nwidth2 = slwidth('.cal_list');
    if (rrsliders[1] && !rrload_stop[1]) {
      rrload_stop[1] = 1;
      rrsliders[1].reloadSlider(
        { responsive: false, maxSlides: 5, hideControlOnEnd: true, infiniteLoop: true, slideWidth: nwidth2, onSliderLoad: function (obj) { rrload_stop[1]=0; } }
      );
    }
    if (rrsliders[2] && !rrload_stop[2]) {
      rrload_stop[2] = 1;
      rrsliders[2].reloadSlider(
        { responsive: false, maxSlides: 5, hideControlOnEnd: true, infiniteLoop: true, slideWidth: nwidth2, onSliderLoad: function (obj) { rrload_stop[2]=0; } }
      );
    }
    if (rrsliders[3] && !rrload_stop[3]) {
      rrload_stop[3] = 1;
      rrsliders[3].reloadSlider(
        { responsive: false, maxSlides: 5, hideControlOnEnd: true, infiniteLoop: true, slideWidth: nwidth2, onSliderLoad: function (obj) { rrload_stop[3]=0; } }
      );
    }
  }
});
</script>
