  load_stop[1] = 1;
  sliders[1] = $('#detail-recommend-item1').bxSlider(
    { responsive: false, maxSlides: 5, hideControlOnEnd: true, infiniteLoop: true, slideWidth: save_width, onSliderLoad: function (obj) { load_stop[1]=0; } }
  );
});

$(window).resize(function(){
  var new_width = slwidth();
  if (save_width != new_width) {
    save_width = new_width;
    var nwidth = slwidth('.cal_list');
    if (sliders[0] && !load_stop[0]) {
      load_stop[0] = 1;
      sliders[0].reloadSlider(
        { responsive: false, maxSlides: 5, hideControlOnEnd: true, infiniteLoop: false, slideWidth: nwidth, onSliderLoad: function (obj) { load_stop[0]=0; } }
      );
    }
    if (sliders[1] && !load_stop[1]) {
      load_stop[1] = 1;
      sliders[1].reloadSlider(
        { responsive: false, maxSlides: 5, hideControlOnEnd: true, infiniteLoop: true, slideWidth: nwidth, onSliderLoad: function (obj) { load_stop[1]=0; } }
      );
    }
  }
});
</script>

