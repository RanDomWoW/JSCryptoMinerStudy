<script type="text/javascript">
function slwidth(n) {
  var w = $(n || '#cal_list').width();
  var c = Math.floor(w / 100);
  if (c <= 0) { return 0; }
  if (c > 5) { c = 5; }
  return Math.floor(w / c);
}
var save_width = 0;
var save_width2 = 0;
var sliders = new Array();
var load_stop = new Array();
var rrsliders = new Array();
var rrload_stop = new Array();

$(document).ready(function() {
  $('#calendar').fullCalendar({
    header: {
      right: 'prev,next',
      left: 'title'
    },
    height: 'auto',
    defaultDate: '2018-03-18'
  });
  var hday = function(hd) {
    hd = hd || [];
    $.each(hd, function(i, value) {
      $("td[data-date='" + value + "']").addClass("fc-holiday");
    });
  };
  var arr;
  $.getJSON("http://st.cdjapan.co.jp/assets/js/holiday.json" , function(data) {
    arr = data;
    var dt = new Date();
    dt.setMonth(dt.getMonth() - 6);
    var yy1 = dt.getUTCFullYear();
    var yy2 = yy1 + 1;
    arr.push(yy1+'-12-30',yy1+'-12-31',yy2+'-01-01',yy2+'-01-02',yy2+'-01-03');
    hday(arr);
  });
  $('.fc-button').click(function () {
    hday(arr);
  });
});
</script>

