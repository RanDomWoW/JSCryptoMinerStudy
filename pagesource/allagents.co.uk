<script>
    jQuery(document).ready(function () {

        jQuery('#frontPageTabs [data-toggle="tab"] a').click(function (e) {
            e.preventDefault();
            jQuery(this).tab('show');
        });


        jQuery(document).ready(function () {
            jQuery('#searchLeagueTableForm').on('submit', function () {
                jQuery('#searchLeagueTableForm').attr('action', '/league-tables/' + $('#lt_agent_type').val() + '/' + $('#ltCity').val());
            });

            var a = jQuery('#ltCity').autocomplete({
                serviceUrl: '/properties/ajax/postcode-town',
                minChars: 2,
                maxHeight: 400,
                width: 300,
                zIndex: 9999,
                deferRequestBy: 0, //miliseconds
                noCache: false, //default is false, set to true to disable caching
                onSelect: function (value, data) {
                    var agent_type = $('#lt_agent_type').val();
                    window.location.href = "/league-tables/" + agent_type + "/" + data + '/';
                }
            });
        });


        jQuery('#searchAgentForm').on('submit', function () {
            var name = $('#search_name').val();
            name = name.toLowerCase();
            name = name.replace(/\s/g, "-");
            name = name.replace(/\+/g, "-");

            var region = $('#search_region').val().toLowerCase().replace(' ', '+');
            var x = region;
            //var type = $('#agent_type').val();
            var regex = new RegExp("\\+", "g");
            x = x.replace(regex, '-');
            if (name) {
                // Fix for single branch agents - display single branch page instead of a single branch link
                $.ajax({
                    type: "POST",
                    dataType: "json",
                    url: '/properties/ajax/get-agentby-link',
                    data: {agent_name: name},
                    success: function (data) {
                        window.location.href = '/' + name + '/';
                    },
                    error: function (xhr, status, error) {
                        window.location.href = '/search/' + name + '/';
                    }
                });
            } else {
                if (region) {
                    if (hasNumber(region)) {
                        window.location.href = "/find-agent/postcode/" + x + '/';
                    } else {
                        window.location.href = "/find-agent/" + x + '/';
                    }
                }
            }
            return false;
        });

        var a = jQuery('#search_name').autocomplete({
            serviceUrl: '/properties/agents/autocomplete',
            minChars: 2,
            maxHeight: 400,
            width: 300,
            zIndex: 9999,
            deferRequestBy: 0, //miliseconds
            noCache: false, //default is false, set to true to disable caching
            onSelect: function (value, data) {
                $("#searchButton").prop("disabled", true); //Disable form to avoid another search being sent

                // Fix for single branch agents - display single branch page instead of a single branch link
                $.ajax({
                    type: "POST",
                    dataType: "json",
                    url: '/properties/ajax/get-agentby-link',
                    data: {agent_name: data},
                    success: function (data) {
                        window.location.href = '/' + data + '/';
                    },
                    error: function (xhr, status, error) {
                        data = data.toLowerCase();
                        data = data.replace(/\s/g, "-");
                        data = data.replace(/\+/g, "-");
                        window.location.href = '/search/' + data + '/';
                    }
                });
            }
        });

        var a = jQuery('#search_region').autocomplete({
            serviceUrl: '/properties/ajax/postcode-town',
            minChars: 2,
            maxHeight: 400,
            width: 300,
            zIndex: 9999,
            deferRequestBy: 0, //miliseconds
            noCache: false, //default is false, set to true to disable caching
            onSelect: function (value, data) {
                var type = $('#agent_type').val();
                var x = data;
                var regex = new RegExp("\\+", "g");
                x = x.replace(regex, '-');
                //is it a postcode?
                if (data.match('([A-PR-UWYZa-pr-uwyz]([0-9]{1,2}|([A-HK-Ya-hk-y][0-9]|[A-HK-Ya-hk-y][0-9]([0-9]|[ABEHMNPRV-Yabehmnprv-y]))|[0-9][A-HJKS-UWa-hjks-uw]))')) {
                    $("#searchButton").prop("disabled", true); //Disable form to avoid another search being sent
                    window.location.href = "/find-agent/postcode/" + x + '/';
                }
                else {
                    window.onerror = function (msg, url, linenumber) {
                        console.log('Error message: ' + msg + '\nURL: ' + url + '\nLine Number: ' + linenumber);
                        return true;
                    }

                    // we need to use regexp as normal replcae will not work on chrome
                    var regex = new RegExp("\\+", "g");
                    x = x.replace(regex, '-');

                    $("#searchButton").prop("disabled", true); //Disable form to avoid another search being sent
                    window.location.href = "/find-agent/" + x;

                }
            }
        });

        var a = jQuery('#searchForSaleForm #area').autocomplete({
            serviceUrl: '/properties/ajax/dis-tow-cou-pos',
            minChars: 2,
            maxHeight: 400,
            width: 300,
            zIndex: 9999,
            deferRequestBy: 0, //miliseconds
            noCache: false, //default is false, set to true to disable caching
        });

        var a = jQuery('#searchToRentForm #area').autocomplete({
            serviceUrl: '/properties/ajax/dis-tow-cou-pos',
            minChars: 2,
            maxHeight: 400,
            width: 300,
            zIndex: 9999,
            deferRequestBy: 0, //miliseconds
            noCache: false, //default is false, set to true to disable caching
        });

        jQuery('#searchForSaleForm, #searchToRentForm').submit(function () {
            var someForm = $(this);
            var search = jQuery("#area", someForm).val().toLowerCase();
            var url = jQuery('#url', someForm).val();
            var str = "";
            var actionUrl = url + search + "/";

//            alert('here');
//            alert(search);
//            alert(url);
//            alert(actionUrl);

            jQuery.each(someForm[0].elements, function (index, elem) {
                if (elem.name !== "search" && elem.name !== "forsale" && elem.name !== "url") {
                    if (elem.value !== "") {
                        str += "&" + encodeURIComponent(elem.name) + "=" + encodeURIComponent(elem.value);
                    }
                }
            });
            if (str !== "") {
                actionUrl += "?" + str;
            }
            //alert(actionUrl);
            someForm.attr('action', actionUrl);
            //event.preventDefault();
        });
    });

    function hasNumber(elem) {
        var matches = elem.match(/\d+/g);
        if (matches != null) {
            return true;
        }
        return false;
    }
</script>


