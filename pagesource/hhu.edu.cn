            height_list.push(height);
            $(this).hover(function(){
                $(this).addClass("hover");
            },function(){
                $(this).removeClass("hover");
            });
            $(this).find("li").hover(function(){
                $(this).addClass("hover");
            },function(){
                $(this).removeClass("hover");
            })
           		
        });
        $(navli).find('ul').height(height_list.max()+10);
    });    
    Array.prototype.max = function(){   //最大值
        return Math.max.apply({},this)
    } 
