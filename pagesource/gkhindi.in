padding:0 0;
text-transform:uppercase;
display:inline-block;
max-width:764px;
width:100%;
}
#nav ul#menu {
float:left;
margin:0px 0px 0px 18px;
padding:0px 0px;
height:34px;
}
#nav li {
margin:0px 0px;
padding:0px 0px;
float:left;
display:inline;
list-style:none;
position:relative;
}
#nav li > a {
display:block;
padding:0 10px;
margin:3px 0;
line-height:28px;
