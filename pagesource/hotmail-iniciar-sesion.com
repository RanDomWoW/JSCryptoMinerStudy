margin: 0 1em;
}
.post {
margin: 0 0 45px 0;
}
h3.post-title, .comments h4 {
font: normal normal 20px 'Trebuchet MS', Trebuchet, sans-serif;
margin: .75em 0 0;
}
.emailMarketingFormTitle {
margin-bottom: -18px;
}
.post-body {
