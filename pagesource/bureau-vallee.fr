        if (Mage.Cookies.get('bv_cart')) {
            var dataCart = Mage.Cookies.get('bv_cart').evalJSON(true);
            if (dataCart.qty < 2){
                var libelle = 'article';
            } else {
                var libelle = 'articles';
            }
            miniCartQty.update(dataCart.qty);
            miniCartQtyLabel.update(libelle);
            var price= dataCart.amount;
            price = price.replace('€','€')
            miniCartSubtotal.update(price);
        } else {
            miniCartQty.update(0);
            miniCartQtyLabel.update('article');
            var montant = "0,00"
            if(currencySymbol=="FCFP"){
                montant = "0";
            }
            miniCartSubtotal.update('<span class="price">'+montant+'&nbsp;€</span>');
        }
    }
}

document.observe("dom:loaded", function() {
