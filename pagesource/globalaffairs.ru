		if (_urlA.length) urlArray.push(_urlA);

		for (i = 0; i < urlArray.length; i++) {
			$.ajax({
				type: 'GET',
				url: "https://disqus.com/api/3.0/threads/set.jsonp",
				data: { api_key: disqusPublicKey, forum : disqusShortname, thread : urlArray[i] }, // URL method
				cache: false,
				dataType: "jsonp",
				success: function (result) {
					//console.log(result.response);
					for (var i in result.response) {
						var count = result.response[i].posts;
						var h = $('i[data-disqus-url="' + result.response[i].link + '"]').html();
						if (!h) {
							h = 0;
						} else {
							h = h.replace('(', '').replace(')', '')*1; 
						}

						h = h + count;

						if (h) h = '(' + h + ')';
						else h = '';
						
						$('i[data-disqus-url="' + result.response[i].link + '"]').html(h);
					}
				}
			});
		}
	});
</script>
 	

<div class="modal-auth"></div>

