    <script type="text/javascript">
	$(document).ready(function(e) {
		$("select").chosen({
			width: "100%",
			disable_search_threshold: 10
		});
		var formId = '#flagForm';
		var button = '#flagSubmitBtn';
		var validate = ajaxValidation();
		$(button).click(function(e) {
			$(this).attr("disabled", "disabled");
			$(this).after("<span class='submit-indicator'></span>");
			var self = this;
			var url = $(formId).attr('action');
			var element = $(formId);
			validate.doPost({
				url: url,
				buttonRef: self,
				element: element,
				callback: function(message) {
					$(self).removeAttr("disabled");
					if (message == 'error') {
						$('.submit-indicator').remove();
						$(self).unbind();
					} else {
						$("#UserContactReason").trigger("chosen:updated");
						$("select").chosen('destroy');
						$("select").prop('selectedIndex', 0);
						$("select").chosen({
							width: "100%",
							disable_search_threshold: 10
						});
						$('#ajaxValidationMsgText').html('Thank you for flagging this definition');
						$('#ajaxValidationMsg').show();
						$('#flagDefinition').modal('hide');
					}
				}
			});
			return false;
		});

	});
	$(document).on("click", ".flagClick", function() {
		var defID = this.id;
		$(".modal-dialog #UserContactLink").val('http://definedterm.com/a/definition/' + defID);
		$(".modal-dialog #UserContactId").val(defID);
	});
</script>

