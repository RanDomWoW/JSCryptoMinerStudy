<script type="text/javascript">

var ready_to_send = 0;
var imei_input = $('#id_imei');

function remove_not_digits(imei) {
    return imei.replace(/[^0-9]/, '');
}

function cd_correct_check() {
    var imei = imei_input.val();
    var imei_js_error = $('#imei_js_error');
    var bad_words = imei.length - imei.replace(/[^0-9]/g,"").length;
    var correct_cd, insert_cd;

    for (var i=0; i<bad_words; i++)
    {
        imei = remove_not_digits(imei);
    }

    imei_input.val(imei);
    correct_cd = gen_cd(imei);
    insert_cd = imei.charAt(imei.length - 1);

    // IMEI invalid
    if ((imei.length > 0 && imei.length < 15 || imei.length > 15) || correct_cd != insert_cd ){
        imei_js_error.addClass('visible');
        ready_to_send = 0;
    }
    // IMEI valid
    else {
        imei_js_error.removeClass('visible');
        ready_to_send = 1;
    }

}

$(document).ready(function() {
    if (imei_input.val()) {
        cd_correct_check();
    }

    $('form#imei_form').submit(function(e){
        if (ready_to_send == 1){
            $('button#szukaj').attr('disabled', true).html('Checking...');
            e.returnValue = true;
            return true;
        } else {
            e.returnValue = false;
            return false;
        }
    });

    if (window.location.hash) {
        window.location.hash = '';
    }

});

imei_input.bind('input propertychange', cd_correct_check);

</script>

