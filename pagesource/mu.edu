<script>
// Change menu based on screen resize for non-mobile browsers
$( window ).resize(function(e) {
	//reset back to normal screen menu
	if ($(window).width()  > 700) {
		//return nav menu position if alert is visible
		//if ($('#website-alert').length() { $('nav #menu').css('margin-top', 73);}
		$('nav #menu').css('margin-top', 10);
		//return menu icon, hide screen overlay, reset activemenu height
		//$('nav #menu').css('margin-top', 10);
    	$('div.overlay').remove();
		$('#menuButton').html('<img src="/_global-images/menu-icon.png" alt="Menu" />'); 
		$('.activemenu').css('min-height', '0');
		$('nav #menu').css('left', '0px');
		$('nav #menu').css('opacity', 1);
		// if nav menu and subnav menu are both open
		if (navDisplay == true && subnavDisplay == true) {
			$( '.activemenu' ).css( 'left' , '-303px' );
			$( '.activemenu' ).removeClass('activemenu');
			$('nav #menu').css('left', '0px');
			navDisplay = false;
			subnavDisplay = false;
			e.stopPropagation();
		// if nav menu is open but subnav menu is closed
		} else if (navDisplay == true  && subnavDisplay == false) {
			$('nav #menu').css('left', '0px');
			navDisplay = false;
			e.stopPropagation();
		//default return menu to normal position
		} else {
			$('nav #menu').css('left', '0px');
			e.stopPropagation();
		}
	//set nav position for slider
	} else if ($(window).width()  <= 700 && navDisplay == false) {
		
		//adjust menu top position based on alert window height
		
		if ($('#website-alert').length ){
	alertHeight = $( '#website-alert .container' ).height() + 24;
	menuTopPosition = alertHeight + 73;
	$('nav #menu').css('margin-top', menuTopPosition);	
	} else { $('nav #menu').css('margin-top', 71);	}
	//end alert window adjustment
		
		$('nav #menu').css('left', '-303px');
		e.stopPropagation();
		
	}
})
