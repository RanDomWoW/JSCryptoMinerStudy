<script type="text/javascript">
    jQuery(function(jQuery){
    	var slider = jQuery('#films-list-header');
    	var slider_container = jQuery('.film-list-conteiner', slider);
    	var prev = jQuery('.films-list-nav-prev', slider);
    	var next = jQuery('.films-list-nav-next', slider);
    	var this_slider = 0;
    	var count_active_sliders = 4;
    	var time_out = 0;
    	var	slider_index = 0;
    	var	slider_index_prev = count_active_sliders - 1;
    	var action_first = true;
    	
    	// Next
    	function slider_items_action(type){
    		if(type == 'hide'){
    			jQuery('.film-list-item a', slider).eq(slider_index).fadeOut(300);
    		}else{
    			jQuery('.film-list-item', slider).eq(slider_index).show();
    			jQuery('.film-list-item a', slider).eq(slider_index).fadeIn(300);
    		}
    		
    		if(slider_index < count_active_sliders - 1){
    			setTimeout(function() {
    				slider_items_action(type);
    			}, 50);
    			slider_index = slider_index + 1;
    		}else{
    			slider_index = 0;
    		}
    	}
    	
    	function slider_action_next(){
    		var sliders = jQuery('.film-list-item', slider);
    		var sliders_active = jQuery('.active-item', slider);
    
    		if(!action_first){
    			slider_items_action('hide');
    			setTimeout(function() {
    				jQuery('.film-list-item', slider).hide();
    				sliders.eq(0).appendTo(slider_container);
    				sliders = jQuery('.film-list-item', slider);
    			}, 600);
    		}		
    		
    		setTimeout(function() {
    			slider_items_action('show');
    		}, 600);
    		
    		
    		
    		action_first = false;
    		return false;
    	}
    	next.click(function(){
    		slider_action_next('');
    	});
    	
    	// Prev
    	function slider_items_action_prev(type){
    		if(type == 'hide'){
    			jQuery('.film-list-item a', slider).eq(slider_index_prev).fadeOut(300);
    		}else{
    			jQuery('.film-list-item a', slider).eq(slider_index_prev).fadeIn(300);
    		}
    		
    		if(slider_index_prev > 0){
    			setTimeout(function() {
    				slider_items_action_prev(type);
    			}, 50);
    			slider_index_prev = slider_index_prev - 1;
    		}else{
    			slider_index_prev = count_active_sliders - 1;
    		}
    	}
    	
    	function slider_action_prev(){
    		var sliders = jQuery('.film-list-item', slider);
    		var sliders_active = jQuery('.active-item', slider);
    
    		if(!action_first){
    			slider_items_action_prev('hide');
    			setTimeout(function() {
    				jQuery('.film-list-item', slider).hide();
    				sliders.last().prependTo(slider_container).show().find('a').fadeOut(0);
    				sliders = jQuery('.film-list-item', slider);
    			}, 600);
    		}		
    		
    		setTimeout(function() {
    			sliders.slice(0, 4).show();
    			slider_items_action_prev('show');
    		}, 600);
    		
    		
    		
    		action_first = false;
    		return false;
    	}
    	prev.click(function(){
    		slider_action_prev('');
    	});
    	
    	
    	jQuery('.film-list-item', slider).hide().removeClass('active-item');
    	jQuery('.film-list-item a', slider).fadeOut(0);
    	slider_action_next();
    	
    
    	// Слайдер в сайтбар
    	jQuery('.sidebar-list-films').each(function(){
    		var sid_slider = this;
    		var sid_slider_container = jQuery('.film-list-conteiner', sid_slider);
    		var sid_prev = jQuery('.films-list-nav-prev', sid_slider);
    		var sid_next = jQuery('.films-list-nav-next', sid_slider);
    		jQuery('.film-list-item', sid_slider).fadeOut(0);
    		jQuery('.film-list-item:first', sid_slider).fadeIn(300);
    		
    		sid_prev.click(function(){
    			jQuery('.film-list-item:first', sid_slider).fadeOut(300, function(){
    				jQuery('.film-list-item:last', sid_slider).prependTo(sid_slider_container);
    				jQuery('.film-list-item:first', sid_slider).fadeIn(300);
    			});
    		});
    		sid_next.click(function(){
    			jQuery('.film-list-item:first', sid_slider).fadeOut(300, function(){
    				jQuery(this).appendTo(sid_slider_container);
    				jQuery('.film-list-item:first', sid_slider).fadeIn(300);
    			});
    		});
    	});
    	
    });
</script>

