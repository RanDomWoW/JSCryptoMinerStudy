    <script type="text/javascript">
        $(function() {
            $(window).scroll(function() {
                var scroll = $(window).scrollTop();

                // 當卷軸超過70px,.gototop就淡入，如果小於就淡出
                if (scroll >= 70) {
                    $(".gototop").fadeIn();
                } else {
                    $(".gototop").fadeOut();
                }
            });

            // 當我按下.gototop時，添加動畫讓卷軸跑道最上面
            $('.gototop').click(function() {
                $('html,body').animate({
                    scrollTop: $('html').offset().top
                });
            });
        });
    </script>

    

