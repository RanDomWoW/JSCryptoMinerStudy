}
function check_all(a_fld, a_sel)
{
	var obj, n;
	eval("obj = document." + a_fld);
	if (obj.length == null)
	{
		obj.checked = a_sel;
	}
	else
	{
		n = obj.length;
		for (i=0; i<n; i ++)
		{
			obj[i].checked = a_sel;
		}
	}

}
