<script>
    $("#alphabet > ul").find("li > a").on("click", function (e) {

        if (!$(this).hasClass("active")) {
            /*условие чтобы потом не добавлять active */
            var thisis = $(this)
        }
        $("#alphabet > ul").find("li > a").each(function (index, element) {
            $(this).removeClass("active")
        });
        if (thisis) {
            thisis.addClass("active")
        }
        highlight_city($(this).html())
    })

    function highlight_city(i) {
        if ($("#alphabet > ul").find("li > a.active").is("a")) {
            /*если есть нажатая буква*/
            $("#change_city_list").find("a").each(function (index, element) {

                if ($(this).data("letter") == i) {
                    $(this).removeClass("un_highlighted")
                }
                else $(this).addClass("un_highlighted")
            });
        }
        else {
            $("#change_city_list").find("a").each(function (index, element) {
                $(this).removeClass("un_highlighted")
            });
        }

    }

    function ch_c_close() {
        $("#ch_c_bg").fadeOut("fast")
        $("#bl_ch_city").fadeOut("fast")
    }
</script><!--/bl_ch_city-->

