//BASIC FUNCTIONS
    //simple debouncing function
	function debounce(func, wait, immediate) {
		var timeout;
		return function() {
			var context = this, args = arguments;
			var later = function() {
				timeout = null;
				if (!immediate) func.apply(context, args);
			};
			var callNow = immediate && !timeout;
			clearTimeout(timeout);
			timeout = setTimeout(later, wait);
			if (callNow) func.apply(context, args);
		};
	};
	
	//to make equal height elements
	function matchElementHeights(element){
		element.height('auto');
			var maxHeight = Math.max.apply(null, element.map(function (){
			return $(this).height();
		}).get());
				
		element.height(maxHeight);
	}

//SPECIFIC HOME PAGE

//Match take the next step boxes
	var panelText = $("#homePageBoxes .panel .panelText");
	var homeButton = $("#homePageBoxes .panel .homeButton");
	function matchHomeCTA(){
		if (!$('body').hasClass('mobile')){
			matchElementHeights(panelText);
			matchElementHeights(homeButton);
		}
	}
		
	$(document).ready(function(){
		matchHomeCTA();
	});
	
	var resizeMatchHeights = debounce(function() {
		panelText.height('auto');
		homeButton.height('auto');
		matchHomeCTA();
	}, 150);
	
	if(window.addEventListener)
		window.addEventListener('resize', resizeMatchHeights);
	else
		window.attachEvent('onresize', resizeMatchHeights);
//]]>
</script>

