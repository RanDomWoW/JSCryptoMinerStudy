			strprice = strprice.replace(',','');

		strprice=strprice.replace(" " , "");
		strprice=strprice.replace("&nbsp;" , "");
		if(strprice.charCodeAt(1)>128)
			strprice= strprice[0] + strprice.substr(2);
			

		retprice = parseFloat(strprice);
		
		return retprice;
	}
