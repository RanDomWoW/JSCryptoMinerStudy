function changeCurrency()
{

	var sourceCurrency = 1;
	var minship = 100;
	var targetCurrency = $("#selmodalcurr").val();
	var strDestCurrency = $("#selmodalcurr option:selected").text();

	$.ajax({
		type: "GET",
		url: "/app/site/hosting/scriptlet.nl?script=202&deploy=1&currency="+sourceCurrency+"&destcurrency="+targetCurrency+"&namedestcurrency="+strDestCurrency+"&total="+minship,
		async: true,
		cache: false,
		processData: false,
		success: function(msgam)
		{
			$("#spanshipmin").html(msgam);
			$("#spanshipmin2").html(msgam);
		}
	});
	
}
function selectCountryCurr()
{
	//if currency is different, redirect
	var shipCount = $("#selmodalcountry").val();
	var lang = $("#selmodallang").val();
	Set_Cookie("shipcountry", shipCount, 1, '/', '', '');
	var siteCur = 1;
	var siteLang = "en";
	 
	if(document.getElementById('shopperlanguage'))
		siteLang = $("#shopperlanguage").val();
	
	if(document.getElementById('currency'))
		siteCur = $("#currency").val();
	else
		siteCur = "";
	var selCur = $("#selmodalcurr").val();
	
	if(siteLang!=lang)
	{	
		if(siteCur!=selCur)
		{
			$.ajax({
				type: "GET",
				url: "/app/site/backend/setshopperlanguagelocale.nl?c=3374976&n=1&selshopperlanguage="+lang,
				async: true,
				cache: false,
				processData: false,
				success: function(msg)
				{
					window.location.href='/app/site/backend/setshoppercurrency.nl?n=1&selcurrency=' + selCur;
				}
			});
		}
		else
		{
			window.location.href = "/app/site/backend/setshopperlanguagelocale.nl?c=3374976&n=1&selshopperlanguage="+lang;
		}
	}
	else
	{	
		if(siteCur!=selCur)
			window.location.href='/app/site/backend/setshoppercurrency.nl?n=1&selcurrency=' + selCur;
		else
			$("#closegeoip").click();
	}
}

function getCountryCurrency(geoCount)
{
	var geoCurrency = "1"; //usd
	switch(geoCount)
	{
		case "US":
			geoCurrency = "1"; //USD
			break;
		case "GB":
			geoCurrency = "2"; //GBP
			break;
		case "CA":
			geoCurrency = "3"; //CANADA
