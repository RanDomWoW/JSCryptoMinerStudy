<script language="javascript">
	$(document).ready(function(){
		getCartItems();
	});
	
	function getCartItems()
	{
		$.ajax({
			type: "GET",
			url: "/mini_cart/mini_cart.ss",
			async: true,
			cache: false,
			processData: true,
			success: function(msg)
			{
				if(msg.indexOf("DOCTYPE")!=-1)
					return true;
				else
				{
					var itemsMini = JSON.parse(msg);
					var minicarthtml = buildMiniCart(itemsMini);
					$("#ulcart").html(minicarthtml);
					$("#ulcartfr").html(minicarthtml);
					return true;
				}
			}
		});	
	}
	function buildMiniCart(cartItems)
	{  	
		//$("#spancartcount").html("("+cartItems.summary.itemcount+")");
		var carthtml = '';
		var cartqtyitems = 0;
		if(cartItems.items)
		{		
			for(var i=0; i<cartItems.items.length; i++)
			{
				var myItem = cartItems.items[i];
				carthtml += '<li><div class="col-xs-12 pdr-40"><div class="row"><div class="col-xs-4"><a href="'+myItem.storeurl+'"><img src="'+myItem.storedisplaythumbnail+'" class="img-responsive" alt="'+myItem.storedisplayname2+'"></a></div><div class="col-xs-8"><h5>'+myItem.storedisplayname2+'</h5>';
				var itemOptions = myItem.options;
				var optionsHTML = "";
				if(itemOptions)
				{
					for(var j=0; j<itemOptions.length;j++)
					{
						optionsHTML += '<span class="selected-size">'+itemOptions[j].name+' '+itemOptions[j].displayvalue+'</span>';
					}
				}
				var strcolor = "";
				if(myItem.itemid && myItem.itemid.indexOf('-')!=-1)
				{
					strcolor = myItem.itemid.split('-')[1];
				}
				cartqtyitems+=myItem.quantity;
				var htmlcolor = "<span></span>";
				if(strcolor!="")
					htmlcolor = "<span>Color "+strcolor+"</span>";
				carthtml += '<span class="selected-color">'+htmlcolor+'</span>'+optionsHTML+'<span class="price">'+myItem.rate_formatted+'</span></div></div></div><button class="header-icons remove-item" style="display:none;" onclick="javascript:removeItem('+myItem.internalid+','+myItem.quantity+');"></button></li>';
			}
			carthtml += '</ul><ul><li class="subtotal">Sub Total <span class="pull-right">'+cartItems.summary.subtotal_formatted+'</span></li>';
			carthtml += '<li class="text-center"><a class="btn-primary" href="/checkout-2-05-0/cart.ssp?sc=3#cart">VIEW FULL CART</a></li>';
			carthtml += '<li class="text-center"><a class="btn-primary" href="/checkout-2-05-0/cart.ssp?sc=3#cart">CHECKOUT</a></li>';
		    carthtml += '<li class="close-cart hidden-lg hidden-md hidden-xs hidden-sm"><h4>Cart</h4><button type="button" class="close"><span aria-hidden="true">&times;</span></button></li></ul>';
			if(cartqtyitems!=parseInt("0"))
				$("#spancartqty").html(cartqtyitems);
		}
		else
		{
			carthtml = '<span class="empty-cart">Your Shopping Cart is empty.</span>';
		}
		return carthtml;
	}
	
	function removeItem(itemidrem, qtyrem)
	{
		var addtocarturl = "/app/site/backend/additemtocart.nl?c=3374976&n=1&buyid=" + itemidrem + "&qty=-" + qtyrem;
		$.ajax({
			type: "GET",
			url: addtocarturl,
			async: true,
			cache: false,
			processData: false,
			success: function(msg2)
			{
				getCartItems();
			},
			error: function(msg2)
			{
				getCartItems();
			},
			warning: function(msg2)
			{
				getCartItems();
			}
		});
	}
</script>
