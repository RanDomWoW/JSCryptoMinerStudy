strprice = strprice.replace("руб","");
		while(!isnumber)
		{
			for(var i=0; i<=9; i++)
			{
				if(strprice[0]==arrnum[i])
				{
					isnumber = true;
					break;
				}
			}
			if(!isnumber)
			{
				strprice = strprice.substr(1);
			}
		}
		
		if(strprice.indexOf(',')!=-1 && strprice.indexOf('.')==-1) 
			strprice = strprice.replace(',','.');

