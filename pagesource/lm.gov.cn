						$("#middle"+i).removeClass('out');
						$("#middle"+i).addClass('over');
						$('#middle_right_ul'+i).css('display','block');
					}else{
						$("#middle"+i).removeClass('over');
						$("#middle"+i).addClass('out');
						$('#middle_right_ul'+i).css('display','none');
					}
				}
			}
			if( m>=16 && m<=20 ){
				for(i=12;i<=16;i++){
