						$("#middle"+i).removeClass('out');
						$("#middle"+i).addClass('over');
						$('#employment_bottom_middle_ul'+i).css('display','block');
					}else{
						$("#middle"+i).removeClass('over');
						$("#middle"+i).addClass('out');
						$('#employment_bottom_middle_ul'+i).css('display','none');
					}
				}
			}
			if( m>=21 && m<=23 ){
				for(i=17;i<=19;i++){
					if(i==(m-4)){
						$("#middle"+i).removeClass('out');
						$("#middle"+i).addClass('over');
						$('#training_middle_ul'+i).css('display','block');
					}else{
						$("#middle"+i).removeClass('over');
						$("#middle"+i).addClass('out');
						$('#training_middle_ul'+i).css('display','none');
					}
				}
			}
			if( m>=24 && m<=27 ){
