				$('#news_img_bottom_images').css('display','none');
				$('#hot_bottom_ul').css('display','none');
			}
			if( m==2 ){
				$("#disdiv1").removeClass('over');
				$("#disdiv1").addClass('no-over');
				$("#disdiv5").removeClass('over');
				$("#disdiv5").addClass('no-over');
				$("#disdiv2").removeClass('no-over');
				$("#disdiv2").addClass('over');
				$('#news_img_bottom_ul').css('display','none');
				$('#news_img_bottom_images').css('display','block');
				$('#hot_bottom_ul').css('display','none');
			}
			if( m==5 ){
				$("#disdiv1").removeClass('over');
				$("#disdiv1").addClass('no-over');
				$("#disdiv2").removeClass('over');
				$("#disdiv2").addClass('no-over');
				$("#disdiv5").removeClass('no-over');
				$("#disdiv5").addClass('over');
				$('#news_img_bottom_ul').css('display','none');
				$('#news_img_bottom_images').css('display','none');
