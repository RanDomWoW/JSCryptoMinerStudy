			    $('#hot_active').css('display', 'none');
			    $('#hot_menu_ul').css('display', 'block');
			}
			if (m >= 30 && m <= 36) {
				for(i=26;i<=32;i++){
					if(i==(m-4)){
						$("#middle"+i).removeClass('out');
						$("#middle"+i).addClass('over');
						$('#middle_left_ul'+i).css('display','block');
					}else{
						$("#middle"+i).removeClass('over');
