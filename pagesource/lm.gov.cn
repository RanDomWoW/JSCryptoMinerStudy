						$("#middle"+i).removeClass('out');
						$("#middle"+i).addClass('over');
						$('#middle_middle_ul'+i).css('display','block');
					}else{
						$("#middle"+i).removeClass('over');
						$("#middle"+i).addClass('out');
						$('#middle_middle_ul'+i).css('display','none');
					}
				}
			}
			if( m>=14 && m<=15 ){
