		function disdiv(m){
			//12为热点信息热点图片
			if( m==1 ){
				$("#disdiv1").removeClass('no-over');
				$("#disdiv1").addClass('over');
				$("#disdiv2").removeClass('over');
				$("#disdiv2").addClass('no-over');
				$("#disdiv5").removeClass('over');
