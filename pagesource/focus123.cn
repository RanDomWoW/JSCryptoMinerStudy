			/*新闻轮播*/
			var oimg = document.getElementById('active_img').getElementsByTagName('img');
			var oli = document.getElementById('news_title').getElementsByTagName('li');

			for(i = 0; i < oli.length; i++) {
				oli[i].index = i
				oli[i].onmousemove = function() {

					clearInterval(imgTimes);

					imgEvert.style.display = 'none';
					liEvert.className = 'news_blue';
					imgEvert = oimg[this.index];
					liEvert = this;
					oimg[this.index].style.display = 'block';
					this.className = 'imgColor';
				}

			}

			document.getElementById('news_title').onmouseout = function() {
				imgTimes = setInterval(
					'imgTime()', 1000)
			}
			var imgNum = 0;
			var imgEvert = null;
			var liEvert = null;
			imgEvert = oimg[0];
			liEvert = oli[0];
			imgEvert.style.display = 'block'
			liEvert.className = 'imgColor';
			//定时器

			var imgTimes = setInterval(
				'imgTime()', 1000)

			function imgTime() {
				imgEvert.style.display = 'none';
				liEvert.className = 'news_blue';
				imgNum++;
				if(imgNum >= oli.length) {

					imgNum = 0;
				}

