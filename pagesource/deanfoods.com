        });
        $('.main-header .navigation-mobile').find('.menu-item.with-submenu').click(function(){
            $('.main-header .navigation-mobile').find('.menu-item.with-submenu').find('.sub-menu-items').removeClass('open');
            $(this).find('.sub-menu-items').addClass('open');
        });
        $('#back-to-top').click(function(){
            $('html, body').animate({scrollTop : 0},800);
            return false;
        });
        $('.main-header .navigation-mobile .menu-box .menu .menu-item').click(function () {
            $('.main-header .navigation-mobile').find('.menu-item').removeClass('disable');
            $('.main-header .navigation-mobile').find('.menu-item').not($(this)).addClass('disable');
        });

        $('.main-header .navigation-desktop .search-btn .mark').click(function () {
            $('.main-header .navigation-desktop .search-btn').find('.sub-menu-box').toggleClass('show');
        });
       
        $('.main-header .navigation-desktop .search-btn .sub-menu-box #search-form').submit(function (event ) {
            event.preventDefault(); 
            if($('.main-header .navigation-desktop .search-btn #keyword').val().length >= 3){
                $.ajax({
                    url: 'http://www.deanfoods.com/wp-admin/admin-ajax.php',
                    type: 'post',
                    data: { action: 'data_fetch_search', keyword: $('.main-header .navigation-desktop .search-btn #keyword').val() },
                    success: function(data) {
                        $('.main-header .navigation-desktop .bottom .search-btn .resault-list').addClass( 'show' );
                        $('.main-header .navigation-desktop .bottom .search-btn .resault-list #datafetch').html( data );
                    }
                });
            }else{
                if($('.main-header .navigation-desktop .search-btn #keyword').val().length <= 0){
                    $('.main-header .navigation-desktop .search-btn #keyword').attr("placeholder", "Type something");
                }else{
                    $('.main-header .navigation-desktop .search-btn #keyword').attr("placeholder", "\"" + $('.main-header .navigation-desktop .search-btn #keyword').val() + "\"" + " is too short");
                    $('.main-header .navigation-desktop .search-btn #keyword').val("");
                }
            }
            
        });

        $('.main-header .navigation-desktop .burger .mark').click(function () {
            $('.main-header .navigation-desktop .burger').find('.sub-menu-box').toggleClass('show');
            $(this).toggleClass('close');
        });

    })(jQuery);
</script>

