<script type='text/javascript'>
    (function ($) {
        var SwiperBrands = new Swiper ('.swiper-brands', {
            speed: 600,
            slidesPerView: 'auto',
            navigation: {
                nextEl: '.brands-slider-arrow-next',
                prevEl: '.brands-slider-arrow-prev'
            },
            on: {
                init: function () {
                    hideItems(this.activeIndex, this.slides);
                    setTimeout(function() {
                        $('.swiper-brands').css("overflow", "initial");  
                    }, 300);
                },
                slideChange: function () {
                    hideItems(this.activeIndex, this.slides);
                },
            }
        });
        function hideItems(index, items){
            for(i = 0; i < items.length; i++){
                if(i > (index + 5) || (i < index)) {
                    $(items[i]).addClass('hide');
                }else{
                    $(items[i]).removeClass('hide');
                }
            }
        }
    })(jQuery);
