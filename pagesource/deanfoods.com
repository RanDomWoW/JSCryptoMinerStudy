					var canvas, stage, exportRoot, animDone = false;
					function initFillUp() {
						if(!animDone){
							canvas = document.getElementById("fill-up-canvas");
							images = images||{};
							ss = ss||{};
							var loader = new createjs2.LoadQueue(false);
							loader.addEventListener("fileload", handleFileLoad);
							loader.addEventListener("complete", handleComplete);
							loader.loadFile({src:"http://www.deanfoods.com/wp-content/themes/deanfoods/assets/animations/fill-up/images/animation_fill_up_atlas_.json", type:"spritesheet", id:"animation_fill_up_atlas_"}, true);
							loader.loadManifest(lib2.properties.manifest);
							animDone = true
						}
					}
					function handleFileLoad(evt) {
						if (evt.item.type == "image") { images[evt.item.id] = evt.result; }
					}
					function handleComplete(evt) {
						var queue = evt.target;
						ss["animation_fill_up_atlas_"] = queue.getResult("animation_fill_up_atlas_");
						exportRoot = new lib2.animationfillup();
						stage = new createjs2.Stage(canvas);
						stage.addChild(exportRoot);
						stage.update();
						createjs2.Ticker.setFPS(lib2.properties.fps);
						createjs2.Ticker.addEventListener("tick", stage);
					}
				});
				})(jQuery);
			</script>
