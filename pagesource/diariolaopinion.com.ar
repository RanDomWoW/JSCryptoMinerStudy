        <script type="text/javascript">
$j = jQuery.noConflict();

$j(document).ready(function() {
    $j("a.votar-opcion").click(function(event) {
        event.preventDefault();
        if (!$j(this).hasClass("disabled")) {
            var opcion = $j(this).data("opcion");
            var encuesta = $j(this).data("encuesta");
            var mensaje = $j("#mensaje-encuesta-" + encuesta);
            mensaje.hide("fast");
            $j("#opcion-" + opcion).addClass("procesando");
            $j.ajax("http://diariolaopinion.com.ar/load/_votar", {
                type:"POST",
                data:{"opcion":opcion},
                error:function() { alert("Disculpa, hubo un error al procesar la acción") }
            }).done(function(result) {
                $j("#opcion-" + opcion).removeClass("procesando");
                if (result.success) {
                    $j.each(result.resultados.opciones, function() {
                        var barra_progreso = $j("#opcion-" + this.id + " .progress-bar");
                        barra_progreso.html(this.porcentaje + "%");
                        barra_progreso.css("width", this.porcentaje + "%");
                    });
                    mensaje.html("Gracias por participar!<br>Los resultados se actualizarán en breve");
                    mensaje.show("slow");
                }
                else if (result.message) {
                    mensaje.html(result.message);
                    mensaje.show("slow");
                }
                $j("#encuesta-" + encuesta + " a.votar-opcion").addClass("disabled");
                $j("#encuesta-" + encuesta + " .progress").show("fast");
                $j("#encuesta-" + encuesta + " .ver-resultados").hide("fast");
            });
        }
    });


    $j("a.ver-resultados").click(function(event) {
        event.preventDefault();
        var encuesta = $j(this).data("encuesta");
        if ($j(this).data("resultados") === "visibles") {
            $j("#encuesta-" + encuesta + " a.votar-opcion").removeClass("disabled");
            $j("#encuesta-" + encuesta + " .progress").hide("fast");
            $j(this).data("resultados", "").html("Ver resultados");
        } else {
            $j("#encuesta-" + encuesta + " a.votar-opcion").addClass("disabled");
            $j("#encuesta-" + encuesta + " .progress").show("fast");
            $j(this).data("resultados", "visibles");
            $j(this).html("Ocultar resultados");
        }
    });
});
</script>

    

    
