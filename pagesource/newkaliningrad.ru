<script type="text/javascript">
function NKForgotPswd(btn) {
    if($('#forgotpswd-form').length == 0) return false;
    var actionURI = $('#forgotpswd-form').attr('action');
    if(!actionURI) return false;
    var params = new Object;
    var inputs = $('#forgotpswd-form').find('input');
    for(var i=0;i<inputs.length;i++) {
        var iName = $(inputs[i]).attr('name');
        if($(inputs[i]).attr('type') == 'checkbox') {
            if($(inputs[i]).is(":checked")) {
                params[iName] = $(inputs[i]).val();
            } else {
                params[iName] = '';
            }
        } else {
            params[iName] = $(inputs[i]).val(); 
        }
    }
    if(btn) {
        $(btn).addClass('disabled');
    }
    $.post(actionURI, params, function(data){
    	console.log($(data).find('#nk-auth-result').html());
        var authResult = $(data).find('#nk-auth-result').data('status');
        if(authResult == 0) {
            var errorMsg = $(data).find('#nk-auth-result').html();
            $('#forgotpswd-error').html(errorMsg).show();
            if(btn) {
                $(btn).removeClass('disabled');
            }
        } else {
             var Msg = $(data).find('#nk-auth-result').html();
             $('#forgotpswd-message').show();
             $('#forgotpswd-message').html(Msg);
             $('#forgotpswd-form').hide();
        }
    });
}
</script>
