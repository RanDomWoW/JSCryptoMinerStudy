    <script>
        $(function(){
            //Platform and Browser Detection for Close Button style
            var ua = window.navigator.userAgent,
                platform = window.navigator.platform,
                mac = platform.toUpperCase().indexOf('MAC') >= 0 ? true : false,
                win = platform.toUpperCase().indexOf('WIN') >= 0 ? true : false,
                msie = ua.indexOf('MSIE ') >= 0 ? true : false,
                trident = ua.indexOf('Trident/') >= 0 ? true : false,
                edge = ua.indexOf('Edge/') >= 0 ? true : false,
                firefox = ua.indexOf('Firefox') >= 0 ? true : false;
            if ( mac ) { 
                $('body').addClass('mac');
            }
            if ( win ) { 
                $('body').addClass('win');
            }
            if ( msie || trident || edge ){
                $('body').addClass('ie');
            }
            if ( firefox ) { 
                $('body').addClass('firefox');
            }
            
            //Login Lightbox
            $('.login_wrapper, .nav_login a').on('click', function(e){
                e.preventDefault();
                $('.overlay').show();
            });
            $(".overlay:not(), .close")
            .on("click", function () {
                $('.overlay').hide();
            })
            .on("click", ".lightbox", function (e) {
                e.stopPropagation();
            });
            
          
            //Slider
            var duration = 10000,
                slider = function(){
                    var $activeSlide = $('.slides .bg.active').parent(),
                        $ind = $activeSlide.index(),
                        $next = ($ind + 1 === 3) ? 0 : $ind + 1,
                        $slideNow = $('.slides .bg:eq('+$ind+'), ol li:eq('+$ind+')'),
                        $slideNext = $('.slides .bg:eq('+$next+'), ol li:eq('+$next+')');             
                    $slideNow.removeClass('active').siblings().removeClass('active');
                    $slideNext.addClass('active');
                },
                startCycle = function() {
                    runSlides  = setInterval(slider, duration);
                };
             
            $('ol li').on('click', function(){
                var index= $(this).index();    
                $('.slides .bg').not(':eq('+index+')').removeClass('active');
                $('.slides .bg').eq(index).addClass('active');
                $(this).addClass('active').siblings().removeClass('active');
                clearInterval(runSlides);
                setTimeout(startCycle, 0);
            });
              
            startCycle();
          
            
            
            //Shuffle Member Cells
            var $member_cell = $('.member_cell'),
                $allMemberCells = $member_cell.get(),
                getRandom = function(max) {
                    return Math.floor(Math.random() * max)
                },
                $shuffled = $.map($allMemberCells, function(){
                    var random = getRandom($allMemberCells.length),
                        $randMemberCells = $($allMemberCells[random]).clone(true)[0];
                        $allMemberCells.splice(random, 1);
                    return $randMemberCells;
                });              
            $member_cell.each(function(i){          
                if(i>7){            
                    $(this).replaceWith();
                }else{
                    $(this).replaceWith($($shuffled[i]));
                }
            });   
          
          
            //Language Selection
            var FFN = FFN || {};
            FFN.footer_lang = (function($) {
                var $obj = {
                  lang_content: $('.suspended ul')
                },
            setLang = function(e) {
              var lang = $(this).data('lang') || 'english',
                  newloc = self.location.href.replace(/[\?&](m|show_lang|lang)=(\w+|\d+)/gi, '');

              if (!lang) {
                return false;
              }

              if (newloc.indexOf('#') > 0) {
                newloc = newloc.replace('#', '&lang=' + lang + '&show_lang=' + lang + '#').replace(/[\?&]/, '?');
                // for registration, changing language sends people back to page 1, to make certain their browser has all the info when they reach the final registration page
                if (newloc.indexOf('#/regpage') > 0) {
                  newloc = newloc.replace(/\#\/regpage.*/, '');
                }
              } else {
                newloc = (newloc + '&lang=' + lang + '&show_lang=' + lang).replace(/[\?&]/, '?');
              }

              self.location.href = newloc + "";
            },
            init = function() {
              $obj.lang_content.on('click', 'div', setLang);
            };

            return {
              init: init
            };

            })(jQuery);

            FFN.footer_lang.init();
        });        
    </script>
</body>
