	if(imgw*1>width){ 
		imgh = imgh/imgw*width; 
		imgw=width;
	}
	if(imgh*1>height){
		imgw = imgw/imgh*height;
		imgh=height;
	}
	ImgD.width = imgw;    
    ImgD.height = imgh;    
}  
</script>

