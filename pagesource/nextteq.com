<script>
$(document).ready( function(){
    resets = 10; 
    slideWidth = -956;
    slideSpeed = 2000;
    slideDelay = 3000;
    slideQuantity = 5;
    holder = "#slideHolder";
    
    loop(0);

    function loop(i){
        for ( i; i < slideQuantity; i++ ) {
            var distance = slideWidth*i;
            $( holder ).animate({ marginLeft: distance }, slideSpeed).delay(slideDelay);
        }
        resets--;
        reset(resets);
    }
    
    function reset(resets){ if(!resets){return;} $( holder ).animate({ marginLeft: "0" }, 0); loop(0); }
    
});
</script>
