<style>.counter-digit{font-size:0em}</style>
<script type="text/javascript">
	$(function(){
		//MEMBER COUNTER
		var member_counter_flip_startCnt = 0;

		$("#member_counter_flip").flipCounter("init",{number:0, duration:1000,imagePath:"https://cdn.auctown.jp/image/counter/flipCounter-medium.png",formatNumberOptions:{format:"##,###,###,###,###",locale:"jp"}});
		$("#member_counter_flip").flipCounter("startAnimation",{
	        number:131421, // the initial number the counter should display, overrides the hidden field 
	        numIntegralDigits:1, // number of places left of the decimal point to maintain 
	        numFractionalDigits:0, // number of places right of the decimal point to maintain 
	        digitClass:"counter-digit", // class of the counter digits 
	        counterFieldName:"counter-value", // name of the hidden field 
	        digitHeight:40, // the height of each digit in the flipCounter-medium.png sprite image 
	        digitWidth:30, // the width of each digit in the flipCounter-medium.png sprite image 
	        //imagePath:"/image/counter/flipCounter-medium.png", // the path to the sprite image relative to your html document 
	        easing: jQuery.easing.easeOutCubic, // the easing function to apply to animations, you can override this with a jQuery.easing method 
	        duration:1000, // duration of animations 
	        onAnimationStarted:false, // call back for animation upon starting 
	        onAnimationStopped:setUpdateMemberCounter, // call back for animation upon stopping 
	        onAnimationPaused:false // call back for animation upon pausing 
		});

		function readMemberCnt(){
			$.ajax({	
				type: "post"
				,url: "/statistics/main_counter.php?kind=M"
				,async: false 
				,success: function(data){
				  	if(typeof data !== 'undefined'){
						var counterVal = $('#member_counter_flip').flipCounter('getNumber').toString();
						counterVal = (isNaN($.trim(counterVal)))? 0 : parseInt($.trim(counterVal),10);
						var	rData = (isNaN($.trim(data)))? 0 : parseInt($.trim(data),10);

						member_counter_flip_startCnt++;
						try{console.log("readMemberCnt("+member_counter_flip_startCnt+")"+counterVal+":"+rData)}catch(e){} 
						if(counterVal < rData){
							$("#member_counter_flip").flipCounter("pauseAnimation");
							$("#member_counter_flip").flipCounter("startAnimation",{number:rData,duration:5000,formatNumberOptions:{format:"##,###,###,###,###",locale:"jp"}});
						}
				  	}
				}
				,error: function(xhr, status, error) {
					try{console.log("Member XML Error!!!! : "+error);}catch(e){} 
				}
			});
		}

		function setUpdateMemberCounter(){
			if(member_counter_flip_startCnt == 0) setInterval(readMemberCnt, 50000);
		}

		//GOODS COUNTER
		var goods_counter_flip_startCnt = 0;

		$("#goods_counter_flip").flipCounter("init",{number:0, duration:1000,imagePath:"https://cdn.auctown.jp/image/counter/flipCounter-medium.png",formatNumberOptions:{format:"##,###,###,###,###",locale:"jp"}});
		$("#goods_counter_flip").flipCounter("startAnimation",{
	        number:713131598, // the initial number the counter should display, overrides the hidden field 
	        numIntegralDigits:1, // number of places left of the decimal point to maintain 
	        numFractionalDigits:0, // number of places right of the decimal point to maintain 
	        digitClass:"counter-digit", // class of the counter digits 
	        counterFieldName:"counter-value", // name of the hidden field 
	        digitHeight:40, // the height of each digit in the flipCounter-medium.png sprite image 
	        digitWidth:30, // the width of each digit in the flipCounter-medium.png sprite image 
	        //imagePath:"/image/counter/flipCounter-medium.png", // the path to the sprite image relative to your html document 
	        easing: jQuery.easing.easeOutCubic, // the easing function to apply to animations, you can override this with a jQuery.easing method 
	        duration:1000, // duration of animations 
	        onAnimationStarted:false, // call back for animation upon starting 
	        onAnimationStopped:setUpdateGoodsCounter, // call back for animation upon stopping 
	        onAnimationPaused:false // call back for animation upon pausing 
		});

		function readGoodsCnt(){
			$.ajax({	
				type: "post"
				,url: "/statistics/main_counter.php?kind=S"
				,async: false 
				,success: function(data){
				  	if(typeof data !== 'undefined'){
						var counterVal = $('#goods_counter_flip').flipCounter('getNumber').toString();
						counterVal = (isNaN($.trim(counterVal)))? 0 : parseInt($.trim(counterVal),10);
						var	rData = (isNaN($.trim(data)))? 0 : parseInt($.trim(data),10);
						
						goods_counter_flip_startCnt++;
						try{console.log("readGoodsCnt("+goods_counter_flip_startCnt+")"+counterVal+":"+rData)}catch(e){} 
						if(counterVal < rData){
							$("#goods_counter_flip").flipCounter("pauseAnimation");
							$("#goods_counter_flip").flipCounter("startAnimation",{number:rData,duration:5000,formatNumberOptions:{format:"##,###,###,###,###",locale:"jp"}});
						}
				  	}
				}
				,error: function(xhr, status, error) {
					try{console.log("XML Error!!!! : "+error);}catch(e){} 
				}
			});
		}

		function setUpdateGoodsCounter(){
			if(goods_counter_flip_startCnt == 0) setInterval(readGoodsCnt, 45000);
		}

	});
</script>
