                                       "Июль",
                                       "Август",
                                       "Сентябрь",
                                       "Октябрь",
                                       "Ноябрь",
                                       "Декабрь"],
                        dow_labels: ["Пн", "Вт", "Ср", "Чт", "Пт", "Сб", "Вск"]
                    };
                    break;

                case 'se':
                    return {
