                $('#top-link-block').removeClass('hidden').affix({
                    offset: { top: 100 }
                });
            }

            $('#speechSwitcher').trigger('change');

            var feedbackModal = $("#feedbackModal");
            var url = window.location.href;
            feedbackModal.ajaxComplete(function () {
                if (selectedString) {
                    $(this).find("#Note").val(selectedString);
                    $(this).find("#Url").val(url);
                }
                selectedString = null;
            });

