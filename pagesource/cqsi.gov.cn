<script type="text/javascript">
  $.ajax({
    url:'http://wthrcdn.etouch.cn/weather_mini?citykey=101040100',
    type:'get',
    dataType:'json',
    success:function(res){
      var d = new Date();
      var str = d.getFullYear()+"-"+(d.getMonth()+1)+"-"+d.getDate();
      var type = res.data.forecast[0].type;
      var high = res.data.forecast[0].high.substring(2,res.data.forecast[0].high.length);
      $("#weatherLi").html(d.getFullYear()+"年"+(d.getMonth()+1)+"月"+d.getDate()+"日"+" "+type+" "+high);
      
    }
  });
  //全文检索
  function bq() {
    var num1 = $('#search1').val();
    if(num1) {
      location.href="/zcms/ZSIDMInstitution/ContenSerchPC.zhtml?TITLE="+num1;
    } 
  }
  $(function() {
    // Slideshow 
    $("#slider").responsiveSlides({
      auto: true,
      pager: false,
      nav: true,
      speed: 1000,
      timeout: 4000,
      pager: true,
      pauseControls: true,
      namespace: "callbacks"
    });
    
    var storage = window.sessionStorage;
     var user = JSON.parse(storage.getItem("user"));
        if(user!=null&&user!="undefined"){
          if(user.name!=null&&user.name!=""&&user.name!="undefined"){
      $("#successText").html("欢迎您！<br />"+(user.mobile.substring(0,3)+"***"+user.mobile.substring(7,user.mobile.length)));
      $("#loginLi").hide();
      $(".suLi").show();
	    }
        }
  });
  
  function quitUser(){
var storage = window.sessionStorage;
storage.removeItem('user');
$.ajax({
	url:"/zcms/reservation/delSeeSionUserid",
	type:"get",
	dataType:"json",
	success:function(data){
	}
});

location.href="/bsdl/grdl.html?regFlag=3";
}
