          <script language="JavaScript">

    function creaArray(x) {
       this.length = x
       return this
    }

    mes = new creaArray(12)
    mes[1]  = "enero"
    mes[2]  = "febrero"
    mes[3]  = "marzo"
    mes[4]  = "abril"
    mes[5]  = "mayo"
