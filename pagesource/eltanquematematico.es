    mes[7]  = "julio"
    mes[8]  = "agosto"
    mes[9]  = "septiembre"
    mes[10] = "octubre"
    mes[11] = "noviembre"
    mes[12] = "diciembre"

    dia = new creaArray(7)
    dia[1] = "domingo"
    dia[2] = "lunes"
    dia[3] = "martes"
    dia[4] = "miércoles"
    dia[5] = "jueves"
    dia[6] = "viernes"
    dia[7] = "sábado"

    function muestra(fecha) {
       var nomdia = dia[fecha.getDay() + 1]
       var nommes = mes[fecha.getMonth() + 1]
       return nomdia + " " + fecha.getDate() + " de " + nommes + "."
    }

    document.write("<font color=\"black\" size=\"2\" face=\"arial\"><i><b>"+muestra(new Date())+"</b></i></font>")

    
