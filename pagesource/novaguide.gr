        $(".newsletter button").on("click", function() {
            var $this = $(this);
            var emailSelector = $this.siblings("input[name='newsletter']");

            var valid = validateEmail(emailSelector.val());

            if (emailSelector.val() === "") {
                $this.siblings("span").text("Παρακαλούμε συμπληρώστε το e-mail σας.");
            }
            else if (!valid) {
                $this.siblings("span").text("Μη έγκυρη διεύθυνση e-mail.");
            } else {
                $.ajax({
                    method: "POST",
                    url: "/Sys/Newsletter/Subscribe",
                    data: {
