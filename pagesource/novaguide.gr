                                            type: "POST",
                                            url: 'Sys/SaveFile/UploadFiles',
                                            data: { facebookId: response.id, fileName: response.id + ".jpg" },
                                            success: function (data) {

                                                localStorage.setItem("Photo", data);

                                                //  window.location.replace("/?pid=31&la=" + NvPage.LanguageId)
                                                $('#PhotoProfile').val(data.replace('"', ''));
                                            },
                                        }).done(
                                        function () {
                                            window.location.replace($("#UrlRedirectLogin").html().trim())
                                        }

                                        );

                                       
                                    }
                                    $(".login-mobile:visible .loader").hide()
                                }
                            });
                        });
                    }
                }, {
                    scope: 'public_profile,email',
                    return_scopes: true
                });
            }

    







