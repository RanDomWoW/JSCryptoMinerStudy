        var productstab = document.getElementById("productstab");
        var literaturetab = document.getElementById("literaturetab");
        var faqstab = document.getElementById("faqstab");
        var newstab = document.getElementById("newstab");
        var showalltab = document.getElementById("showalltab");
        //alert(folderid);
        if (folderid=="588"){ //product details/product models
            //alert('setting product tab');
