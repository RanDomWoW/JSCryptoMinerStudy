        }
        if (folderid=="350"){ //faq
            //alert('setting faq tab');
		    if (faqfilterlink != null){
			    faqfilterlink.style.textDecoration="none";
			    faqfilterlink.style.color="#c0c0c0";
		    }
		    if (faqstab != null){
                //faqstab.style.backgroundColor="#67772C";
                faqstab.style.backgroundColor="#000000";
            }
        }
        if (folderid=="579"){ //news
            //alert('setting news tab');
		    if (newsfilterlink != null){
			    newsfilterlink.style.textDecoration="none";
			    newsfilterlink.style.color="#c0c0c0";
		    }
		    if (newstab != null){
                //newstab.style.backgroundColor="#67772C";
                newstab.style.backgroundColor="#000000";
            }
        }
        if (folderid=="0"){ //all
            //alert('setting showall tab');
		    if (showallfilterlink != null){
			    showallfilterlink.style.textDecoration="none";
			    showallfilterlink.style.color="#c0c0c0";
		    }
		    if (showalltab != null){
                //showalltab.style.backgroundColor="#67772C";
                showalltab.style.backgroundColor="#000000";
            }
        }
    }

    function getCookieVal(offset) {
      var endstr = document.cookie.indexOf (";", offset);
      if (endstr == -1) { endstr = document.cookie.length; }
      return unescape(document.cookie.substring(offset, endstr));
    }

    function GetCookie(name) {
      var arg = name + "=";
      var alen = arg.length;
      var clen = document.cookie.length;
      var i = 0;
      while (i < clen) {
        var j = i + alen;
        if (document.cookie.substring(i, j) == arg) {
          return getCookieVal (j);
          }
        i = document.cookie.indexOf(" ", i) + 1;
        if (i == 0) break;
        }
      return null;
    }

    //cookies
    function createCookie(name,value,days) {
        if (days) {
	        var date = new Date();
	        date.setTime(date.getTime()+(days*24*60*60*1000));
	        var expires = "; expires="+date.toGMTString();
        }
        else var expires = "";
        document.cookie = name+"="+value+expires+"; path=/";
    }

    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
	        var c = ca[i];
	        while (c.charAt(0)==' ') c = c.substring(1,c.length);
	        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
        }
        return null;
    }

    function eraseCookie(name) {
        createCookie(name,"",-1);
    }

    function searchRedirect(url)
    {
        //alert('in search redirect');
        document.cookie="postbackcookie=";
        window.location.href=url;
    }

    //cart-related scripts
    function getCurrentCartCount()
    {
        var currentCount = readCookie("cartcount");
        //alert("currentCount: " + currentCount);
        var countText = document.getElementById("cartCount");
        if (currentCount=="NaN" || currentCount==null)
        {
            countText.innerHTML = 0;
        }
        else
        {
            countText.innerHTML = currentCount;
        }

    }

    function addToCart()
    {
        //cartCount - id of count span
        //commented out temporarily 7/16/2012
        //var countText = document.getElementById("cartCount");
        var currentCount = readCookie("cartcount");
        //alert("currentCount=" + currentCount);
        var newCount = 0;
        if (currentCount=="NaN"||currentCount==null)
        {
            newCount=1;
        }
        else
        {
            newCount=parseInt(currentCount) + 1;
        }

        //alert("newCount=" + newCount);
        createCookie("cartcount",newCount,0);
        //commented out temporarily 7/16/2012
        //countText.innerHtml = newCount;
    }

    function removeFromCart()
    {
        //
    }

    function emptyCart()
    {
        //
    }
