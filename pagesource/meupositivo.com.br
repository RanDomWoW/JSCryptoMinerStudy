<script type="text/javascript">
	  $(function() {
	      function busca(chave) {
	          $.ajax({
	              type: "POST",
	              url: '/ajax/busca.php',
	              data: {chave: chave},
	              success: function(data) {
	                  $('#conteudo').html(data);
	              }
	          });
	      }
	      $('#chave').keyup(function() {
	          busca(this.value);
	      });
	  });
	</script>
