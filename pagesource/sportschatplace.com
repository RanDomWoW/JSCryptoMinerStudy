<script>
$(document).ready( function(){

	if(typeof touch_device !== "undefined"){
		if(touch_device){
 			console.log("touch device");
			//mainVideo
			$.get( "//sportschatplace.com/video-home-mobile", function( data ) {
  				$( "#mainVideo" ).append( data );

			});
			console.log("mobile video loaded");
		} else {
		 	console.log("not touch device");
			//mainVideo
			$.get( "//sportschatplace.com/video-home-desktop", function( data ) {
  				$( "#mainVideo" ).append( data );

			});
			console.log("desktop video loaded");
		}
		setTimeout(function(){
			var vids = $("#mainVideo").find("script");
			//console.log(vids);
			$.each(vids, function (index){
				$(document).load($(this)[0]["src"]);
				eval($(this)[0]["text"]);
				//console.log($(this)[0]["src"]);
				console.log($(this)[0]["text"]);
			});
		}, 100);
	}

});
</script>

