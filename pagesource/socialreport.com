<script>

    setTimeout(function() {
        $(".home-page-alert").slideDown(200);
    }, 1000);


    var showingTrail = false;

    var hideSideCta = false;
    var hideBottomCta = false;


    function hideSideWidget(obj) {
        $(obj).closest('.widget').fadeOut();

        hideSideCta = true;
    }

    function hideBottomWidget(obj) {
        $(obj).closest('.widget').fadeOut();

        hideBottomCta = true;
    }

    window.onresize = function(event) {
        var cta_side = $(".widget-sidebar-cta");

        if ($(document).width() < 1000) {
            cta_side.css("left",  "1400px");
        } else {
            cta_side.css("left", ($(document).width() -700) + "px");
        }

    };

    $(window).scroll(function(e){
        var progress = $(this).scrollTop() / $(document).height();

        var cta_side = $(".widget-sidebar-cta");
        var cta_buttom = $(".widget_trail");

        if ($(document).width() < 1000) {
            cta_side.css("left",  "1400px");
        } else {
            cta_side.css("left", ($(document).width() - 700) + "px");
        }


        if (cta_side.length > 0 && cta_buttom.length > 0) {
            if (progress > 0.75) {
                if (!showingTrail) {
                    showingTrail = true;
                    if (!hideBottomCta) {
                        cta_buttom.css({"left":"-400px"}).animate({"left":"0px"}, "slow");
                    }
                    cta_side.fadeOut();
                }
            }
            else if (progress > 0.05) {
                if (!hideSideCta) {
                    cta_side.fadeIn();
                }
                cta_buttom.css({"left":"-400px"});
                showingTrail = false;
            }
            else {
                cta_side.fadeOut();
                cta_buttom.css({"left":"-400px"});
                showingTrail = false;
            }
        }


    });

</script>
<script type="text/javascript">

    jQuery(window).scroll(function() {
        if(jQuery( window ).width()>767){
            if (jQuery(this).scrollTop() > 1){
                jQuery('header').addClass("sticky");
            }else{
                jQuery('header').removeClass("sticky");
            }

            if (jQuery(this).scrollTop() > 230){
                jQuery('header .register').show();
            }else{
                jQuery('header .register').hide();
            }

        }else{
            jQuery('header .register').hide();
            jQuery('header').removeClass("sticky");
        }
    });

</script>

