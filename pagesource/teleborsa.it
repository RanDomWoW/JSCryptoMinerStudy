        function showPrimoPiano(idx, autoPlay) {
            var oldIdx = ppIdx;
            if (idx != null) { 
                ppIdx = idx; 
                if (ppIdx < 0) ppIdx = ppTot - 1;
                else if (ppIdx >= ppTot) ppIdx = 0;
                }

            var items = $('.pp-item');
            items.fadeOut(200, 'linear');
            items.find('.pw').hide();
            var item = $('.pp-item').eq(ppIdx);

            item.css({ "visibility":"hidden", "display":"block" });
            var title = item.find('.title');
            var text = item.find('.text');
            var contents = item.find('.contents');
            var titleMaxHeight = parseInt(title.css('line-height'))*3;
            title.adapText({ maxHeight:titleMaxHeight, minFontSize:8, minWordSpacing:-0.03, minLetterSpacing:-0.03, useEllipsis:false });
            var contentsMaxHeight = 176;
            text.adapText({ maxHeight:contentsMaxHeight, container:$(contents) });
            item.css({ "visibility":"visible", "display":"none" });
           
            clearTimeout(ppTimeout);
            if (autoPlay == null && ppIdx == oldIdx) { autoPlay = !ppAutoPlay; }
            
            if (autoPlay) { 
                if (autoPlay != ppAutoPlay || oldIdx == -1) {
                    /*$('.primopiano .pager a.selected').removeClass("pause").addClass("play");*/
                }
                ppAutoPlay = true;
                ppTimeout = setTimeout(function() { showPrimoPiano(ppIdx+1, ppAutoPlay) }, 10000); 
            }
            else {
                if (autoPlay != ppAutoPlay || oldIdx == -1) {
                    /*$('.primopiano .pager a.selected').removeClass("play").addClass("pause"); */
                }
                ppAutoPlay = false;
            }
            item.fadeIn(400, 'linear', function() { 
                item.find('.pw').slideDown(300); 
            });
            return false;
        }       

