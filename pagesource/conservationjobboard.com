				    var ca = decodedCookie.split(';');
				    for(var i = 0; i <ca.length; i++) {
				        var c = ca[i];
				        while (c.charAt(0) == ' ') {
				            c = c.substring(1);
				        }
				        if (c.indexOf(name) == 0) {
				            return c.substring(name.length, c.length);
				        }
				    }
				    return '';
				}

				// Tracking related functions
				/* to drop */function alterSource(newSource) {
					argBox = newSource;
					console.log(argBox);
				}

				// user parameters
				var closeDuration = 7;
				var subDuration = 7;
				var okPopDuration = 5000;
				var closingTimer = null;

				var post_url = baseUrl + 'api/subscriber/new';
				var email_url = baseUrl + 'api/subscriber/mail';

				// user functions

				// Cookie check
				function checkCookie() {
				    var isSub = getCookie('is_sub');
				    var isClose = getCookie('is_close');
				    if ((isSub != '' && isSub == '1') || (isClose != '' && isClose == '1')) {
				        return true;
				    } else {
				       return false;
				    }
				}

				// Slider loader functions
				// Opening
				function resetSlider() {
					window.clearTimeout(closingTimer);
					$('#subbox .sub-pop-2').css("display", "none");
		    		$('#subbox .sub-pop-1').css("display", "block");
		    		$('#subbox .sub-pop-1').find('.sub-loading-gif').remove();
        			$('#subbox .sub-pop-1').find('input[name="email"]').val(''); 
        			$('#subbox .sub-pop-1').find('button[type="submit"]').show();

				}
				function loadSub(timer) {
					if (timer === undefined || timer === null ) {
						setTimeout('$("#subbox").css({"display": "block"});', 4900);
	                	setTimeout('$("#subbox").animate({"left": "0px","opacity": "0.92"}, "very slow");', 5000);
					} else {
						setTimeout('$("#subbox").css({"display": "block"});', timer - 100);
	                	setTimeout('$("#subbox").animate({"left": "0px","opacity": "0.92"}, "very slow");', timer);
					}
	               
	            }

	            // Closing
	            function closeSub(e) {
	            	
	            	e.preventDefault();
	            	$('#slider-input').attr('data-code', 'em-1');
	                $('#subbox').animate({
	                    'left': '-600px',
	                    'opacity': '0.2'
	                }, 'very slow');

	                setCookie('is_close', 1, closeDuration );

	               
	            }

	            function closeSuccess(e) {

	            	e.preventDefault();
	            	$('#subbox').animate({
	                    'left': '-600px',
	                    'opacity': '0.2'
	                }, 'very slow');

	                
	            }


	            // Utilities functions
	            function validateEmail(mail) 
			    {
			        if (/^[a-zA-Z0-9.!#$%&'*+\/=?^_`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$/.test(mail))
			        {
			        return (true)
			        }
			        return (false)
			    }

			    function displayMessage(message, accessor) {
	            	$(accessor).html(message);
	           	}

	           	function displayLoadingGif() {
	           		$(subInput.form).find('button[type="submit"]').hide();
	           		$(subInput.form).find(subInput.errorAccessor).empty();
	           		var target_input = $(subInput.form).find('input[name="email"]');
                    $('<img src="' + baseUrl + 'theme/img/sub-loading.gif" class="sub-loading-gif">').insertAfter(target_input);
	           	}

	           	function hideLoadingGif() {
	           		$(subInput.form).find('.sub-loading-gif').remove();
        			$(subInput.form).find('button[type="submit"]').show(); 
	           	}

	           	function displayOkPopupFade() {
	           		$('.sub-pop-1').fadeOut(800, function() {
			            $('.sub-pop-2').fadeIn(1000, function(){
			                closingTimer = setTimeout(function() {
			                	$('#subbox').animate({
				                    'left': '-600px',
				                    'opacity': '0.2'
				                }, 'very slow');
			                }, okPopDuration);
			            });
			        });
	           	}

			    function displayOkPopupExternal() {
		    		$('#subbox .sub-pop-1').hide();
		    		$('#subbox .sub-pop-2').show();
		    		$("#subbox").animate({"left": "0px","opacity": "0.92"}, "very slow");
		    		setTimeout(function() {
	                	$('#subbox').animate({
		                    'left': '-600px',
		                    'opacity': '0.2'
		                }, 'very slow');
	                }, okPopDuration);
			    }


			    // Main subsription function
			    function subscribe() {
			    	$.ajax({
                        type: "POST",
                        url: post_url,
                        data: {email : subInput.email},
                        success: function(response) {
                            
                            var data = JSON.parse(response);
                           
                            if (data.type == "success") {
                            	
                                $.ajax({
                                    type: "POST",
                                    url: email_url,
                                    data: {
                                        email : subInput.email,
                                        token : data.response 
                                    },
                                    success: function(response) {
                                        var emailData = JSON.parse(response);
                                       
                                        if (emailData.type == "success") {
                                        	setCookie('is_sub', 1, subDuration );
                                        	gtag('config', 'UA-10225560-2', {'page_path': '/subscription-success?from=' + subInput.code +''});
                                        	gtag('event', 'page_view', { 'send_to': 'UA-10225560-2' });
                                        	
                                        	subOutput = {
												type : subInput.type,
												result : 1,
												errorAccessor : subInput.errorAccessor,
												successAccessor : subInput.successAccessor,
												message : 'Please check your inbox for a confirmation email.'
											};

											postSubscribe();

                                        } else {
                            				
                                            subOutput = {
												type : subInput.type,
												result : -1,
												errorAccessor : subInput.errorAccessor,
												successAccessor : subInput.successAccessor,
												message : 'System error please try again later'
											};

											postSubscribe();

                                        }
                                    }
                                });
                            } else {
                            	
                            	subOutput =  {
									type : subInput.type,
									result : 0,
									errorAccessor : subInput.errorAccessor,
									successAccessor : subInput.successAccessor,
									message : data.response
								};

								postSubscribe();
                            }
                        }
                    });
			    }

			    function postSubscribe() {
			    	switch (subOutput.type) {
			    		case "slider" :
			    			switch (subOutput.result) {
			    				case 1: 
			    					displayOkPopupFade();
			    				break;
			    				case 0:
			    					displayMessage(subOutput.message, $(subInput.form).find(subOutput.errorAccessor));
			    					hideLoadingGif();
			    				break;
			    				case -1:
			    					displayMessage(subOutput.message, $(subInput.form).find(subOutput.errorAccessor));
			    					hideLoadingGif();
			    				break;
			    			}
			    		break;
			    		case "block_desktop" :
			    			switch (subOutput.result) {
			    				case 1: 
			    					displayOkPopupExternal();
			    					hideLoadingGif();
			    				break;
			    				case 0:
			    					displayMessage(subOutput.message, $(subInput.form).find(subOutput.errorAccessor));
			    					hideLoadingGif();
			    				break;
			    				case -1:
			    					displayMessage(subOutput.message, $(subInput.form).find(subOutput.errorAccessor));
			    					hideLoadingGif();
			    				break;
			    			}
			    		break;
			    		case "block_mobile" :
			    			switch (subOutput.result) {
			    				case 1: 
			    					displayMessage('You need to activate your subscription:<br>Please check your inbox for a confirmation email.', $(subInput.form).find(subOutput.successAccessor));
			    					hideLoadingGif();
			    				break;
			    				case 0:
			    					displayMessage(subOutput.message, $(subInput.form).find(subOutput.errorAccessor));
			    					
			    					hideLoadingGif();
			    				break;
			    				case -1:
			    					displayMessage(subOutput.message, $(subInput.form).find(subOutput.errorAccessor));
			    					
			    					hideLoadingGif();
			    				break;
			    			}
			    		break;
			    	}
			    }


	            // Exec
	            if (!checkCookie()) {
	            	loadSub(10000);
	            }

            	$('form.subscribtion-form').on('submit', this, function(event) {
            		event.preventDefault();
            		console.log('form submited');
            		var emailInput = $(this).find('input[name="email"]');


            		subInput.form = $(this);
            		subInput.type = $(emailInput).attr('data-type');
            		subInput.errorAccessor = $(emailInput).attr('data-error-accessor');
            		subInput.successAccessor = $(emailInput).attr('data-success-accessor');
            		subInput.email = $(emailInput).val();
					subInput.code = $(emailInput).attr('data-code');
            		

            		console.log(subInput);
            		if (subInput.email != '') {
            			if (validateEmail(subInput.email)) {
            				displayLoadingGif();
            				subscribe();
            			} else {
            				displayMessage('Please insert a valid email address !', $(subInput.form).find(subInput.errorAccessor));
            			}
            		} else {
            			displayMessage('Please insert a valid email address !', $(subInput.form).find(subInput.errorAccessor));
            		}
            	});
	            
			</script>
<script type="text/javascript">
			$(document).ready(function() {
				$('.sub-trigger').on('click', this, function(event) {
					$('#slider-input').attr('data-code', $(this).attr('data-sub'));
					event.preventDefault();
					resetSlider();
					loadSub(100);
				});
			});
		</script>
