//click img
function setCountries(obj){

	var src=$(obj).attr("data-source");

	$("#national_flag").attr("src","https://img-s.mlo.me/cosplay/images/national_flag/big/"+src+".png");//显示大国旗

	var country_name=$(obj).attr("title");
	$("#country_name").html(country_name);//显示国家名

	var language=$(obj).attr("lang");
		

	//选择国家后对应的语言显示
	if(language!=''){
		language=language.split(",");
		$("#languageList a").hide();
	
		$("#languageList a").each(function(){

			for(var i=0;i<language.length;i++){
				if( $(this).attr("language") == language[i] ){
					$(this).show();
				}else{
					//$(this).show();
				}
			}
			

			$("#languageList .more").show();

		});
	}else{
		$("#languageList a").show();
		$("#languageList .more").hide();
	}


	$("#countryid").val($(obj).attr("id"));


}
;(function($){
	
	$(function(){

		/*
