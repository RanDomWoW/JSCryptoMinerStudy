<script type="text/javascript">

/* *********LEFT BAR ********* */
jQuery('.left_nav_color').mouseover(function () {
    jQuery(this).stop().animate({ backgroundPosition: "(0 0)" }, { duration: 270 });
    this.style.color = '#ffffff';

});

jQuery('.left_nav_color').mouseout(function () {
    var tempObjClassNames = this.className;
    if (tempObjClassNames.search('current') == -1) {
        jQuery(this).stop().animate({ backgroundPosition: "(-140px 0)" }, { duration: 150 });
        this.style.color = '#A4A4A4';
    }
});

jQuery('#moreheader').click(function () {
    jQuery('#moreitems').slideToggle(170);
    jQuery('#upperArrow').toggleClass('arrow_pos_up');
});

</script>