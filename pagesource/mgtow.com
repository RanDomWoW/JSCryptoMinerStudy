<script>
$(document).ready(function(){
	$("#back2top").hide();
	$(function () {
		$(window).scroll(function () {
			if ($(this).scrollTop() > 200) {
				$('#back2top').fadeIn();
				var offset = $(window).scrollTop();
				$("#spinner").css("margin-top", offset - 60);
			} else {
				$('#back2top').fadeOut();
			}
		});
		$('#back2top').click(function () {	
			//alert($("#back2top").offset().top);
			var dist = $("#back2top").offset().top;
			var scrolltime = dist/5;
			$('body,html').animate({ scrollTop: 0 }, scrolltime);
			return false;
		});
	});
});
