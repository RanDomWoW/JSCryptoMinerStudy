<script>
function SP_Random_Mix(target,mode,Random){
	if(mode == undefined || mode == '') mode = 'individual';
	if(SP$(target).length <= 0) return false;
	if(SP$(target).length > 1){
		if(mode == 'relative'){
			var _target = SP$(target)[0];
			var Total_Child = SP$(_target).children().length;
			var Random = Math.floor((Math.random() * Total_Child) + 1);
		}
		SP$(target).each(function(){
			var _this = SP$(this);
			SP_Random_Mix(_this,mode,Random);
		});
		return;
	}
	var target = SP$(target);
	if(mode == 'individual'){
		//console.log(4444444);
		var Total_Child = target.children().length;
		var Random = Math.floor((Math.random() * Total_Child) + 1);
		//console.log(Random);
	}
	target.children().eq(Random).prependTo(target);
	//console.log(Random);
}
SP_Random_Mix('#SP_slider_thumb_pager, .main-slider-container .SP_sliderStarter','relative');
SP_Random_Mix('#SP_rightSlide .SP_sliderStarter','individual');
function SP_Rebuild(){
	SP$('#SP_slider_thumb_pager > a').each(function(i,v){
		var _this = SP$(this);
		_this.attr('data-slide-index',i);
	});
}
SP_Rebuild();
function SP_Main_Slider_Start(){
	SP$('#SP_slider_thumb .main-slider-container .SP_sliderStarter').SP_sliderPackage({
		SP_packages:'bxslider',
		mode:'horizontal',
		auto:true,
		autoControls:false,
		controls:true,
		pager:true,
		pause:4000,
		minSlides:1,
		maxSlides:1,
		slideWidth:1200,
		slideMargin:0,
		speed:800,
		pagerCustom:'#SP_slider_thumb_pager',
		useCSS:false,
		easing:'easeInOutCubic'
	});
	SP$('#SP_rightSlide .SP_sliderStarter').SP_sliderPackage({
		SP_packages:'bxslider',
		mode:'horizontal',
		auto:true,
		autoControls:false,
		controls:true,
		pager:true,
		pause:4000,
		minSlides:1,
		maxSlides:1,
		//slideWidth:1200,
		//slideMargin:0,
		speed:800,
		pagerCustom:null,
		useCSS:false,
		easing:'easeInOutCubic'
	});
}
SP_Main_Slider_Start();
