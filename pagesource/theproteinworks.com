        <div class="row rowFlex pageColour">
            <div class="col w25 facets" id="algolia-left-container">
                <div id="refine-toggle" class="visible-xs visible-sm">+ Refine</div>
                <div class="hidden-xs hidden-sm" id="instant-search-facets-container">
                    <div id="current-refinements"></div>
                </div>
            </div>

            <div class="col w75 search-results" id="algolia-right-container">
                <div class="row">
                    <div class="col-md-12">
                        <div>
                            {{#second_bar}}
                            <div id="instant-search-bar-container">
                                <div id="instant-search-box">
                                    <div class="instant-search-bar-label">
                                        <h1>Your Search Results...</h1>
                                    </div>
                                    <div class="instant-search-bar-wrapper">
                                        <label for="instant-search-bar">
                                            Search:                                        </label>

                                        <input placeholder="Search for products"
                                               id="instant-search-bar" type="text" autocomplete="off" spellcheck="false"
                                               autocorrect="off" autocapitalize="off"/>

                                        <span class="clear-cross clear-query-instant"></span>
                                    </div>
                                </div>
                            </div>
                            {{/second_bar}}
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="row toolbar result-header ">
                        <div class="col sorts sorter styled" id="algolia-sorts"></div>
                        <div class="col sorts limiter styled" ><label>Items per page:</label><span id="algolia-hitperpage"></span></div>
                        <div class="col stats" id="algolia-stats"></div>

                    </div>
                </div>
                <div class="row">
                    <div>
                        <div class="hits">

                            <div id="instant-search-results-container" class="product-list" itemscope itemtype="http://schema.org/ItemList"></div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>

                <div class="text-center">
                    <div id="instant-search-pagination-container" >

                    </div>
                </div>
            </div>
        </div>

    </div>
</script>
<!-- Product hit template -->
<script type="text/template" id="instant-hit-template">
    {{#hits}}
        <div class="product" data-product-id="{{objectID}}" data-product-sku="{{sku}}">
            <div class='inner'>
                <meta itemprop="itemListElement" content="{{{ _highlightResult.name.value }}}"/>
                <div class="box thumbnail">
                    <a href="{{url}}">
                        {{#image_url}}<img src="{{{ image_url }}}" class="image" />{{/image_url}}
                        {{^image_url}}<span class="no-image" class="image"></span>{{/image_url}}
                    </a>
                </div>

                <div class="box description">

                    <h3 class="name"><a href="{{url}}">{{{ _highlightResult.name.value }}}</a></h3>

                    <div class="ratings cell product-rating">
                        {{#rating_summary}}
                        <div class="star-rating-wrapper">
                            <div class="star-rating" style="width:{{rating_summary}}%"></div>
                        </div>
                        <div class="value">({{rating_count}})</div>
                        {{/rating_summary}}
                        {{^rating_summary}}
                        <div class="star-rating-wrapper empty">
                        </div>
                        {{/rating_summary}}
                    </div>

                    <div class="summary">{{{short_description}}}</div>
                    
                    <!--- product ICONS here --->

                </div>

                <div class="box price-offer-button">

                    <div class="offer">
                        {{{offer_html}}}
                    </div>

                    <div class="buttons">
                        <div class="button-container">


                            {{#isAddToCartEnabled}}
                            {{#in_stock}}
                            <button title="Add to Basket" class="button btn-cart"
                                    onclick="setLocation('https://www.theproteinworks.com//checkout/cart/add/product/{{objectID}}')">
                                <span>Quick Buy</span>
                            </button>

                            {{/in_stock}}
                            {{/isAddToCartEnabled}}

                            <button title="{{name}}" class="button btn-info"
                                    onclick="window.document.location='{{url}}'">
                                <span class="more-info">More info</span></a>
                            </button>

                        </div></div>

                    <div class="pricing">
                        {{#price.GBP.default_original_formated}}
                        <div class="price old"><span class="label">was</span>{{price.GBP.default_formated}}</div>
                        <div class="price"><span class="label">from <span>{{price.GBP.default_original_formated}}</div>
                        {{/price.GBP.default_original_formated}}

                        {{^price.GBP.default_original_formated}}
                        <div class="price"><span class="label">from </span>{{price.GBP.default_formated}}</div>
                        {{/price.GBP.default_original_formated}}
                    </div>


                    <span class="stretch">&nbsp;</span>

                </div>


                </div>
        </div>
    {{/hits}}
</script>
<!-- Search statistics template (used for displaying hits' count and time of query) -->
<script type="text/template" id="instant-stats-template">
    {{#hasOneResult}}
        <strong>1</strong> result found    {{/hasOneResult}}

    {{#hasManyResults}}
        {{^hasNoResults}}
            {{first}}-{{last}} out of        {{/hasNoResults}}
        <strong>{{#helpers.formatNumber}}{{nbHits}}{{/helpers.formatNumber}} results found</strong>
    {{/hasManyResults}}

    in {{seconds}} seconds</script><!-- Refinements lists item template -->
<script type="text/template" id="refinements-lists-item-template">
    <label class="{{cssClasses.label}}">
        <input type="checkbox" class="{{cssClasses.checkbox}}" value="{{name}}" {{#isRefined}}checked{{/isRefined}} />{{name}}
        {{#isRefined}}<span class="cross-circle"></span>{{/isRefined}}
        <span class="{{cssClasses.count}}">
            {{#helpers.formatNumber}}{{count}}{{/helpers.formatNumber}}
        </span>
    </label>
</script><!-- Current refinements template -->
<script type="text/template" id="current-refinements-template">
    <div class="cross-wrapper">
        <span class="clear-cross clear-refinement"></span>
    </div>
    <div class="current-refinement-wrapper">
        {{#label}}
            <span class="current-refinement-label">{{label}}{{^operator}}:{{/operator}}</span>
        {{/label}}

        {{#operator}}
            {{{displayOperator}}}
        {{/operator}}

        {{#exclude}}-{{/exclude}}

        <span class="current-refinement-name">{{name}}</span>
    </div>
</script>        <!--- before_body_end END -->
    </div>
</div>
