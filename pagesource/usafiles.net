    var uploadComplete = true;
    $(document).ready(function() {
        document.domain = 'usafiles.net';
        // figure out if we should use 'chunking'
        var maxChunkSize = 0;
        var uploaderMaxSize = 2097152;
                if(browserXHR2Support() == true)
        {
            maxChunkSize = 2092152;
            var uploaderMaxSize = 104857600;
        }
        
        // Initialize the jQuery File Upload widget:
        $('#fileUpload #uploader').fileupload({
            sequentialUploads: true,
            url: 'http://usafiles.net/core/page/ajax/file_upload_handler.ajax.php?r=usafiles.net&p=http&csaKey1=9fd869c7be49840d26cf2579324f3e1d0bba07dc77543e684704b9d6e845c159&csaKey2=2f373d075761bda7fd2b7bac9830aae64fd83b8f70711d9d0f93af09f7c94796',
            maxFileSize: uploaderMaxSize,
            formData: {},
