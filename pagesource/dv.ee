<script>
$(document).on('submit', 'form#send-hint', function(e){
	(e.preventDefault) ? e.preventDefault() : e.returnValue = false;
	var errorField = $(this).find("#hint-error").empty();
	var errorEmailField = $(this).find("#hint-error-email").empty();
	var text = $.trim($(this).find("#hint-message").val());
	var email = $(this).find("#hint-email").val();
	var hadError = false;
	if(!validateEmail(email)){
		errorEmailField.text("Ё-майл с ошибкой");
		hadError = true;
	}if(text.length < 1){
		errorField.text("ѕожалуйста, заполните строку");
		hadError = true;
	}
	if(hadError){
		return;
	}
	console.log($(this).serialize());
	var str = $(this).find('#hint-message').serialize();
	var byteArray = [];
	for (var i = 0; i < str.length; ++i)
	{
		byteArray.push(str.charCodeAt(i));
	}
	console.log(byteArray);
	$.ajax({
		url: '/apps/pbcs.dll/exec?name=sendTip.php5',
		data: $(this).serialize(),
		method: 'post'
	}).done( function(data){
		$("#send-hint").html('<h3>—пасибо!</h3><button class="submit-button" data-dismiss="modal">«акрыть</button>');
	}).fail( function(data){
		alert("hint sending failed");
		console.log(data);
		//TODO veateade kasutajale
	});
});
function validateEmail(email) {
		var regexString = /[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?/;
	return regexString.test(email);
}
</script>

