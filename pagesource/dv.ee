<script type="text/javascript">
	$(document).ready(function() {
		 $.ajax({
	        timeout: 10000,
	        method: "GET",
	        url:"//investorapi.aripaev.ee/odata/v4/SelectedInstruments?$filter=SiteCode eq 'DV' and Order gt -1&$orderby=Order&$select=Name,LastPriceNominal,Change",
	        cache: true
	    }).done(function(data){
	        if (typeof data.value === "undefined" || data.value.length < 1)
	            return false;
	            html = '';
	        $.each(data.value, function() {
	            var obj = this,
	                title = obj.Name.replace("Kuld", "«олото").replace("Brent Oil Future 1st Front Month", "—ыра€ нефть").replace("Rubla", "EUR/RUB").replace("USA dollar", "EUR/USD").replace("Dow Jones Industrial Average", "Dow Jones"),
	                change = obj.Change ? truncateInt(obj.Change) : 0,
	                value = obj.LastPriceNominal ? truncateInt(obj.LastPriceNominal) : 0,
	                progress = (change < 0) ? "negative" : "positive";
	            html +=  '<a class="stock-index">';
	            html +=     '<img src="/img/'+progress+'_big.gif" height="14" width="11" alt="">';
	            html +=     title;
	            html +=     '<span class="stock-index-'+progress+'">&nbsp;&nbsp;'+change+'</span>';
	            html +=  '&nbsp;&nbsp;'+value+'</a>';
	        });
	        if (data.value.length > 0) {
				if (window.innerWidth < 768)
					$('#mobile-placeholder-stockticker').html(html);
				else
					$('div.stock-indexes').html(html);
			}
	    })
	    function truncateInt(number) {
	        return (Math.round(number * 100) / 100).toFixed(2);
	    }
	});
</script>

