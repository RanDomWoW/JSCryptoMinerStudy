					mmUl += '<li><a class="default" href="#default">' + app.resources.USD_lang_ca + '</button></li>';
				}
			
			mmUl += '</ul>';
			mmDiv += mmUl;
			mmDiv += '</div>'

			mmLangli += mmDiv;
			mmLangli += '</li>';

		var spanShip = '';
			
               if (currentLocale != 'en_US') {
                                         spanShip = '<span class="mobile-shipto">' + app.resources.USD_ship_to + '<span class="shipto-country-canada">'+ ' ' + currentCountry +  '</span></span>';
