		}
		else {
			mmenuFooterLocale = "<a class='nav-account icon-user-2' href='https://www.additionelle.com/on/demandware.store/Sites-Additionelle_CA-Site/en_US/Account-Show'>" + mmenuAccountText +"</a><a class='nav-help icon-help' href='https://www.additionelle.com/en-us/FAQ.html'>" + mmenuHelpText +"</a>" + mmenuLangBtn
		}
	}

	var $menu = jqrecent.query("#navigation nav").clone();
		$menu.find(".html-slot-container,.navigation-header,span.arrow").remove();
		$menu.find(".menu-wrapper").unwrap();
		$menu.find("ul,ul li,ul a,ul li a").removeAttr("class");
		$menu.find("ul li,ul li a").removeAttr("style");
		$menu.find("ul a").removeAttr("id");
		$menu.attr("id", "mobile-navigation");

		$menu.mmenu({
