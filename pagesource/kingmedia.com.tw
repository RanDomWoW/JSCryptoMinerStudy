function login(){
  REG=new http();
  REG.setFn(processREG);
  REG.setURL("cgi-bin/login.cgi");
  REG.post(loginFD());
}
function loginFD(){
  var formData = new FormData();
  formData.append("account", document.getElementById("account").value);
  formData.append("pass", document.getElementById("password").value);
  return formData;
}

function processREG(){
  var ret=REG.getResponse().trim();
  var data=ret.split("*");
  if(data[0]=="Success"){

    alert( Url.decode(data[2])+"您好!")
    localStorage.setItem("url", data[1]);
    location.href=data[1];
  }else
    alert("對不起, 您輸入的帳號密碼有誤, 請再輸入一次");
}

