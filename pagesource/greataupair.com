						getShowcaseListing = getShowcaseListing + '&currentRow=' + showCurrentRow;
					}
					
					$.getJSON(getShowcaseListing, function(data) {
						showcaseSlider.slick('slickAdd', data[0].HTML);
						showCurrentRow = data[0].CURRENTROW;
					});
				}
				
				if ($(window).width() < 768) {resizeSlidesForMobile();}

			});
			
			if ($(window).width() < 768) {
				resizeSlidesForMobile();
			}
			
			function resizeSlidesForMobile() {
				var screenWidth = $(window).width();
				var slideWidth;
				var slideMargin = 20;
				var slideHeight = $('#showcaseSlider .items').height();
				
				slideWidth = screenWidth - slideMargin*2;
				
				$('#showcaseSlider .slide').css({
					width: slideWidth,
					margin: '0 ' + slideMargin + 'px'
				})
				
				$('#showcaseSlider').css({
					height: slideHeight
				})
			}
		
		});
