			var s2=document.getElementsByTagName("script")[0]; s2.parentNode.insertBefore(s,s2);
				
			var tcEAcquisitionJsUrl = "//cdn.tagcommander.com/967/tc_AirFranceWEBeacquisition_2.js";
			var t=document.createElement("script"); t.async=true; t.src=tcEAcquisitionJsUrl ;    
