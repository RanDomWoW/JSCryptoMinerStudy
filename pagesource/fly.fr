<script type="text/javascript">//<![CDATA[
function validateEmail(email){var re=/^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;return re.test(email);}jQuery(document).ready(function(){jQuery.noConflict();jQuery('body').append('<div id="overlay_load" class="overlay"><div id="loading-img"></div></div>');jQuery('#email_newsletter').keypress(function(e){if(e.which==13){newsletterSubscriberFormDetail.submit();}});});var newsletterSubscriberFormDetail=new VarienForm('newsletter_form_footer');newsletterSubscriberFormDetail.submit=function(button,url){if(this.validator.validate()){var url=jQuery('#newsletter_form_footer').attr('action');var formData=jQuery('#newsletter_form_footer').serialize();var htmlSucess='<i class="icon-succes"></i><h5>Merci ! Votre inscription a bien été prise en compte.</h5>';var htmlError='<h3>Oups !</h3><h5>Une erreur technique est survenue.</h5><p> Veuillez nous excusez pour ce désagrément, nous vous invitons à recharger la page et renouveler votre demande.</p>';var htmlExist='<i class="icon-succes"></i><h5>Vous êtes déjà inscrit à notre newsletter</h5>';var htmlValidateEmail="<h3>Oups!</h3><h5>Format d'email non valide</h5>";try{jQuery('#overlay_load').show();if(!validateEmail(jQuery('#email_newsletter').val())){jQuery('#content_message').html(htmlValidateEmail);jQuery('#overlay_load').hide();jQuery('#newsletter2').modal('show');}else{jQuery.ajax({url:url,type:'post',data:formData,success:function(data){var obj=jQuery.parseJSON(data);if(obj.status=='success'){jQuery('#content_message').html(htmlSucess);jQuery(obj.messageSuppl).appendTo('#content_message');jQuery('#email_newsletter').val('');}else if(obj.status=='already'){jQuery('#content_message').html(htmlExist);jQuery('#email_newsletter').val('');}else{jQuery('#content_message').html(htmlError);}jQuery('#overlay_load').hide();jQuery('#newsletter2').modal('show');},error:function(){jQuery('#overlay_load').hide();jQuery('#content_message').html(htmlError);jQuery('#newsletter2').modal('show');}});}}catch(e){}if(button&&button!='undefined'){}}}.bind(newsletterSubscriberFormDetail);
//]]></script>
<script type="text/javascript">//<![CDATA[
var loginForm=new VarienForm('login-form',true);if($('login-email')&&$('login-password')){$('login-email').observe('keypress',bindLoginPost);$('login-password').observe('keypress',bindLoginPost);}function bindLoginPost(evt){if(evt.keyCode==Event.KEY_RETURN){loginForm.submit();}}loginForm.submit=function(button,url){if(this.validator.validate()){jQuery('#error_message').hide();var form=this.form;var oldUrl=form.action;if(url){form.action=url;}var e=null;if(!url){url=jQuery('#login-form').attr('action');}var data=jQuery('#login-form').serialize();data+='&isAjax=1';jQuery('#ajax_loader_login').show();jQuery('#btn_login').prop('disabled',true);try{jQuery.ajax({url:url,dataType:'json',type:'post',data:data,success:function(data){jQuery('#ajax_loader_login').hide();if(data.status=='SUCCESS'){window.location.replace(data.url);}else{jQuery('#error_message').html(data.message);jQuery('#btn_login').prop('disabled',false);jQuery('#error_message').show();}}});}catch(e){}this.form.action=oldUrl;if(e){throw e;}}}.bind(loginForm);
//]]></script>
<script type="text/javascript">jQuery('.show-filter-mobile').click(function(){var block_filter=jQuery('.block-filter');var icon_plus=jQuery(this).find('.icon-plus');if(block_filter.hasClass("in")){icon_plus.html('+');block_filter.removeClass("in");}else{icon_plus.html('-');block_filter.addClass("in");}});</script>
<script type="text/javascript">//<![CDATA[
jQuery(document).ready(function(){jQuery('#password').keypress(function(event){if(event.keyCode==13){loginPostForm.submit();}});jQuery('#user-email').keypress(function(event){if(event.keyCode==13){loginPostForm.submit();}});});var loginPostForm=new VarienForm('login-form-connect');loginPostForm.submit=function(button,url){if(this.validator.validate()){jQuery('#error_message_connect').hide();jQuery('#btn_login').prop('disabled',true);var form=this.form;var oldUrl=form.action;if(url){form.action=url;}var e=null;if(!url){url=jQuery('#login-form-connect').attr('action');}var data=jQuery('#login-form-connect').serialize();data+='&isAjax=1';jQuery('#ajax_loader_login_connect').show();try{jQuery.ajax({url:url,dataType:'json',type:'post',data:data,success:function(data){jQuery('#ajax_loader_login_connect').hide();if(data.status=='SUCCESS'){window.location.replace(data.url);}else{jQuery('#error_message_connect').html(data.message);jQuery('#error_message_connect').show();jQuery('#btn_login').prop('disabled',false);}},error:function(data){console.log(data);}});}catch(e){}this.form.action=oldUrl;if(e){throw e;}}}.bind(loginPostForm);
//]]></script>
