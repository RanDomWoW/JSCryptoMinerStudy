    <script>
        $(document).ready(function () {

            $('#drphomeCity').dropdown();
            $('#drphomeStreet').dropdown();

            //$('#development').modal('show');

            //$('#developmentClose').click(function () {
            //    $('#development').modal('hide');
            //});


            var hasFrom = window.location.href.includes('login');
            if (hasFrom) {
                //$('#LoginModal').modal('show');
                $('#LoginModal')
                .modal({
                    closable: false
                }).modal('show');
            }

            $('#btnLogin').click(function () {
                $("#btnLogin").addClass("loading");

                if ($('#newusername').val() == '' || $('#newusername').val() == undefined || $('#newpassword').val() == '' || $('#newpassword').val() == undefined) {
                    toastr.error('نام کاربری و کلمه عبور اجباری می باشد.');
                }
                var data = { "UserName": $('#newusername').val(), "Password": $('#newpassword').val(), "IsDevelop": false };
                $.ajax({
                    type: "POST",
                    url: '/api/Login/UserLogin',
                    data: data,
                    dataType: 'json',
                    success: function (data) {
                        if (data == 1) {
                            window.location.href = '/Profile';
                        }
                        else if (data == -1) {
                            $("#btnLogin").removeClass("loading");
                            toastr.error('برای ورود باید ابتدا حساب کاربری خود را از طریق لینک ارسالی که به ایمیل شما فرستاده شده است، فعال نمایید');
                        }
                        else if (data == -2) {
                            window.location.href = "/Account/Activation?userName=" + $('#newusername').val() + "&password=" + $('#newpassword').val();
                        }
                        else if (data == -3) {
                            $("#btnLogin").removeClass("loading");
                            toastr.error('متاسفانه حساب کاربری شما مسدود شده است. لطفا جهت بررسی مشکل با پشتیبانی تماس بگیرید');
                        }
                        else if (data == -4) {
                            $("#btnLogin").removeClass("loading");
                            toastr.error('نام کاربری یا کلمه عبور اشتباه میباشد');
                        }
                    },
                    error: function () {
                        toastr.error('خطای ناخواسته رخ داده است.');
                    }
                });
            });
        });
    </script>
 
