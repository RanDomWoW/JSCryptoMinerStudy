<script type="text/javascript">
    var clearClasses = function (items, index) {
        items.removeClass('cloudTextMoveToTopRight');
        items.removeClass('cloudTextMoveToTopLeft');
        items.removeClass('resetCloudTextPositionFromRight');
        items.removeClass('resetCloudTextPositionFromLeft');
    };

    var setClasses = function (items, index) {
        items.eq(index).addClass('cloudTextMoveToTopRight');
        items.eq(index + 1).addClass('cloudTextMoveToTopLeft');
    };

    var resetClasses = function (items, index) {
        items.eq(index - 1).addClass('resetCloudTextPositionFromRight');
        items.eq(index).addClass('resetCloudTextPositionFromLeft');
    };
</script>

