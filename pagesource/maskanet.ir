    <script type="text/javascript">
        var focusedInput;

        function openNav() {
            $('#mn-Sidenav').animate({ right: '0px' }, 200);
            $('#backMenuLayout').fadeIn();
            $(window).bind("onscroll touchmove", function () {
                window.scrollTo(0, 0);
            });
        }

        function closeNav() {
            $('#mn-Sidenav').animate({ right: '-80%' }, 200);
            $('#backMenuLayout').fadeOut();
            $(window).unbind("onscroll touchmove");
        }

        $(document).ready(function () {

            $('#modalClose').click(function () {
                $('#LoginModal').modal('hide');
            });

            $(document).click(function (e) {
                if (!$(e.target).hasClass('mn-user-panel-btn')) {
                    $('[data-toggle="popover"]').popover('hide');
                }
            });

            var width = $(document).width();

            if (width >= 992) {
                if ($('div').hasClass("mn-gen-top-toolbar-wp")) {
                    $('body').removeClass('body-padding70');
                    $('body').addClass('body-padding130');
                }
                else {
                    $('body').removeClass('body-padding130');
                    $('body').addClass('body-padding70');
                }

                if (window.location.href == "http://maskanet.ir/" || window.location.href == "http://www.maskanet.ir/" || window.location.href == "http://dev.maskanet.ir/")
                    setTimeout(function () {
                        $('[data-toggle="popover"]').popover('show');
                    }, 500);
            }
            else {

                if ($('div').hasClass("mn-gen-top-toolbar-wp")) {
                    $('body').removeClass('body-padding70');
                    $('body').addClass('body-padding130');
                }
                else {
                    $('body').addClass('body-padding35');
                    $('body').removeClass('body-padding70');
                    $('body').removeClass('body-padding130');
                }

                $('input[class=search]').each(function () {

                    if ($(this).parent().parent().parent().parent().parent().prop("id") != "locationSearchDiv" && $(this).parent().parent().parent().parent().prop("id") != "locationSearchDiv") {
                        $(this).remove();
                    }

                });

                $('.search').click(function (e) {
                    if (!$(e.target).hasClass('item')) {

                        if (!$('#agentUpgrade').is(':visible') && !$('#locationSearchDiv').is(':visible')) {
                            $(this).find('i.dropdown.icon').trigger('click');
                        }
                    }
                });
            }

            $(document).ajaxComplete(function (event, request, settings) {
                if (request != null && request.responseText != null && request.responseText == "Unconfirmed User" && request.status == "403") {
                    $("#unAuthorizedModal").modal('show');
                }
            });

            $(".unconfirmedUser").on('click', function () {
                $(".ui.modal").modal('hide');
                $("#unAuthorizedModal").modal({
                    onHide: function () {
                        $(".ui.modal:not(#unAuthorizedModal)").modal('hide')
                    }
                }).modal('show');
            });


            $('.sidebar li').on('click', function () {
                $('li.active').removeClass('active');
                $(this).addClass('active');
            });

            $("#menu-userpanel").dropdown();

            $('.ui.sidebar').sidebar('attach events', '.toc.item');

            $('#mn-js-user-panel > .trigger').popover({
                html: true,
                title: function () {
                    return $(this).parent().find('.head').html();
                },
                content: function () {
                    return $(this).parent().find('.content').html();
                }
            });

            $('#backMenuLayout').click(function (e) {
                closeNav();
            });

            $('.mn-imgmenu').on('click', function () {
                openNav();
            });

            $('input[type="text"], input[type="password"]').focus(function () {

                if (window.location.href.indexOf("/Search") == -1) {
                    focusedInput = $(this);
                    //$('html,body').removeClass('overflowScrolling-touch');
                    //$('html,body').addClass('overflowScrolling-auto');
                }

            }).blur(function () {
                if (window.location.href.indexOf("/Search") == -1) {
                    //$('html,body').removeClass('overflowScrolling-auto');
                    //$('html,body').addClass('overflowScrolling-touch');
                    focusedInput = undefined;
                }
            });

            $(window).bind("onscroll touchmove", function () {
                if (window.location.href.indexOf("/Search") == -1 && focusedInput !== undefined) {
                    focusedInput.blur();
                }

                //$('*').addClass('cursor-default');
            })

            var interVal = setInterval(function () {
                if ($("iframe[name='jivo_container']").contents().find("head").length > 0) {
                    var $head = $("iframe[name='jivo_container']").contents().find("head");
                    $head.append($("<link/>", { rel: "stylesheet", href: window.location.origin + "/Content/LiveChat/livechat.css", type: "text/css" }));

                    var width = $(document).width();

                    if (width < 992) {
                        $('jdiv').remove();
                    }

                    clearInterval(interVal);
                }
            }, 100);
        });

