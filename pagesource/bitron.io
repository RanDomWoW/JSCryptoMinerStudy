$(document).ready(function() {
			var lang ='';
			var image ='';
			setlang(lang,image);
});		
function setlang(lang='',image=''){
 	if(lang!="" ){
		$.post( site_url+"home/setlanguage", { lang:lang,image:image }).done(function( data ) {
				var obj = JSON.parse(data);
				setl(obj);
			});
	}else {
		$.post( site_url+"home/setlanguage", { lang:'',image:'' }).done(function( data ) {
			var obj = JSON.parse(data);
				setl(obj);
			});
	}
	
	
}
function setl(obj){
	var lang =  obj.lang;
	var image =  obj.image;
	$('#datalanguage').attr('data-lang',lang);
	$('.translation-links a').show();
	$('#'+lang).hide();
	$('#langImage').attr('src',image); 
	var $frame = $('.goog-te-menu-frame:first');
	if (!$frame.size()) {
	// alert("Error: Could not find Google translate frame.");
	return false;
	}
	$frame.contents().find('.goog-te-menu2-item span.text:contains('+lang+')').get(0).click();
	return false;
}
   
</script>
  <script type="text/javascript">
    $('.translation-links a').click(function() {

		var lang = $(this).data('lang');
		var image = $(this).find('img').attr('src');
