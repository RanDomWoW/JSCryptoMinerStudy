    <script>

        $(document).ready(function () {

            $('img.caption').captionjs({
                'mode': 'stacked',
                'inherit_styles': true,
                'force_dimensions': true
            });
            $('#searchText').bind('keydown', function (e) {
                var $search = $(this);
                //on keydown for all textboxes
                if (e.target.className == "searchtextbox") {
                    if (e.keyCode == 13) { //if this is enter key
                        e.preventDefault();

                        if ($search.val().length > 0)
                            Search($search.val());

                        return false;
                    }
                    else
                        return true;
                }
                else
                    return true;
            });

            function Search(v) {
                $('#status').fadeIn();
                $('#preloader').fadeIn();
                $.ajax({
                    type: 'post',
                    contentType: 'application/json; charset=utf-8',
                    data: JSON.stringify({ Text: v }),
                    dataType: 'json',
                    url: "WebService.asmx/FarsiSearch",
                    timeout: 300000,

                    success: function success(msg) {
                        $('#status').delay(300).fadeOut();
                        $('#preloader').delay(300).fadeOut('slow');

                        var result = msg.d;
                        var output = [];
                        if (result.length > 0) {
                            $('#mainplaceholder').html('');
                            output.push("<div class='col-md-14 col-sm-11 text-right pull-right'>");
                            output.push("<div class='row'>");

                            $.each(result, function (index, item) {
                                output.push("<div class='sec-topic col-sm-16'>");
                                output.push("<a href='Article.aspx?a=" + item.ID + '&c=' + item.CategoryID + "'><div class='row'><div class='col-md-11 col-sm-16 col-lg-11 col-xs-16'> <div class='sec-info' style='direction:rtl;'>");
                                output.push('<h3>' + item.Title + '</h3></div>');
                                output.push('<p>' + item.Summary + '</p><div class="text-primary">');
                                output.push('<span>' + item.Date + '</span>');
                                output.push('<span class="fa fa-clock-o pull-right" style="margin-left:5px;"></span></div></div>');
                                output.push('<div class="col-md-5">');
                                output.push('<img class="img-thumbnail" src="' + item.BannerPath + '" style="height:170px; width:100%; "/>');
                                output.push('</div></div>');
                                output.push('</a><hr/></div>');
                            });

                            output.push("</div></div>");
                            $('#mainplaceholder').html(output.join(""));
                            $("#mainplaceholder").highlight(v.split(" "));
                        }

                        else {
                            output.push("<div style='direction: rtl; font-size: 20px; margin-top: 10px; text-align: center;'>");
                            output.push("Ù…ÙˆØ±Ø¯ Ù¾ÙŠØ¯Ø§ Ù†Ø´Ø¯");
                            output.push("</div>");
                            $('mainplaceholder').html(output.join(""));

                        }
                    }
                });
            }
        });

    </script>
