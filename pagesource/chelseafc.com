<script>
$(function() {

    (function($, undefined){

        var $PlayerCarousel = $("#PlayerCarousel"),
            $items = $PlayerCarousel.find('.item'),
            $fadedWrapper = $PlayerCarousel.closest('.faded-carousel'),
            PlayerCarousel;
    
        var extendLazyLoad = function(){
            
            var $prevImg = $PlayerCarousel.find('.owl-item.active').first().prev().find('img.lazyOwl'),
                $nextImg  = $PlayerCarousel.find('.owl-item.active').last().next().find('img.lazyOwl');

            $prevImg.attr('src', $prevImg.data('src')).show().css('opacity', 1);
            $nextImg.attr('src', $nextImg.data('src')).show().css('opacity', 1);
        };

        var $playerNext, $playerPrev;

        PlayerCarousel = $PlayerCarousel.owlCarousel({
            nav : false,
            dots : true,
            loop:true,
            items : 3,
            responsive: {
                0: {
                    items: 1
                },
                330: {
                    items: 2
                },
                440: {
                    items: 3
                },
                550: {
                    items: 2
                },
                770: {
                    items: 3
                },
                975: {
                    items: 3
                }
            },
            slideBy: 4,
            startPosition: Math.floor(Math.random() * ($items.length + 1)), 
            lazyLoad : true,
            onInitialized: extendLazyLoad
        });

        PlayerCarousel.on('loaded.owl.lazy', extendLazyLoad);

        $playerNext = $('<div class="slider-arrow slider-arrow-right"><span class="sr-only">&gt;<\/span><\/div>')
            .css('height', '100%')
            .click(function(){
                PlayerCarousel.trigger('next.owl.carousel');
            }), 
        $playerPrev = $('<div class="slider-arrow slider-arrow-left"><span class="sr-only">&lt;<\/span><\/div>')
            .css('height', '100%')
            .click(function(){
                PlayerCarousel.trigger('prev.owl.carousel');
            });

        $fadedWrapper.append($playerNext).append($playerPrev);
	
    })($);
 
});
</script>
