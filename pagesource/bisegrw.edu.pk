        if (e.which == 13 || e.data.which == 13) {
            e.preventDefault();
            var txtSearch=obj("#s").val();
            var list = new Array();
            var searchResult = "<div id=\"search-result\" class=\"col-md-12\"><div class=\"widget-main\"><div class=\"widget-inner\"><h2>Search Results</h2>";
            obj('a').each(function(){
                var mHref = obj(this).attr("href");
                if(mHref.indexOf(txtSearch)>=0){
                    var mTitle = obj(this).text();
                    if(obj.inArray(mTitle, list)<0){
                        list.push(mTitle);
                        searchResult += "<p><a href='"+mHref+"'>"+mTitle+"</a></p>";
                    }
                }
            });
            if(list.length==0){
                searchResult += "<p>Could not find any result.</p>";
            }
            searchResult += "</div></div></div>";
            obj('#search-result').remove();
            obj("body > .container > .row").prepend(searchResult);
        }
    };
    obj("#s").keypress(function(e){return searchNow(e)});
    obj("#sbtn").click({which: 13},function(e){return searchNow(e)});
