<script type="text/javascript">
  window.addEventListener('DOMContentLoaded', function(){
    var w = window.innerHeight,
      nl = document.querySelector('.news-letter'),
      wrapper = nl.querySelector('.wrapper'),
      wrapper_class = wrapper.className,
      minimize = nl.querySelector('.minimize'),
      show_nl = document.querySelector('.show-nl'),
      show_nl_class = show_nl.className,
      show_nl_flag = true,
      nl_submit = nl.querySelector('.nl-btn'),
      nl_input = nl.querySelector('.nl-input'),
      num_regex = /^[0-9]{1,10}$/,
      email_regex = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/,
      nl_storage = localStorage.getItem("nl_storage"),
      nl_min = localStorage.getItem("nl_min");

    //Load
    if(nl_storage ==  null){
      localStorage.setItem("nl_storage", true);
    }
    else{
      if(nl_storage == 'false'){
        nl.style.display = "none";
        if(nl_min == 'true'){
          show_nl.className = show_nl_class +" show"
        }else{
          show_nl.className = show_nl_class +" hide"
        }
      }else{
        setTimeout(function(){
          nl.style.display = "block";
        }, 5000)
      }
      ga('send', {
        hitType: 'event',
        eventCategory: 'Popup - Newsletter Subscription',
        eventAction: 'Impression',
        eventLabel: 'Newsletter Module'
      });
    }

    //Scroll
    window.addEventListener('scroll', function(){
      var footer =document.querySelector('.footer-new'),
        p = footer.getBoundingClientRect().top;

      if(show_nl_flag) {
        if (p <= (w - 180)) {
          wrapper.className = wrapper_class + " stay";
        } else {
          wrapper.className = wrapper_class;
        }
      }
      if (p <= w) {
        show_nl.style.opacity = 0;
      } else {
        show_nl.style.opacity = 1;
      }
    })
    //Submit
    nl_submit.addEventListener('click', function(){
      var val = nl_input.value;
      if(val == null || val == ''){
        Materialize.toast("Please provide an Email or Phone Number", 3000);
        nl_input.value = ""
      }else{
        if(num_regex.test(val)){
          if(val.charAt(0) < 7 ){
            Materialize.toast("Mobile number should start with 7/8/9 and have 10 digits only", 3000);
            nl_input.value = ""
          }else{
            fn_home.ajaxForm({'key':'phone','value':val},'POST','/home_page/subscribe-nl')
              .done(function(resp){
                var res = JSON.parse(resp);
                if(res.status == "success"){
                  nl_input.value = "";
                  nl.style.display = "none";
                  Materialize.toast(res.message, 3000);
                  localStorage.setItem("nl_storage", false);
                  ga('send', {
                    hitType: 'event',
                    eventCategory: 'Popup - Newsletter Subscription',
                    eventAction: 'Click',
                    eventLabel: 'EnterTextSection-SubmitButton',
                    eventvalue: 'Web'
                  });
                  clevertap.event.push("Newsletter Subscribers", {
                    "All Subscribers - SMS": val
                  });
                }
              })
          }
        }else{
          if(email_regex.test(val)){
            fn_home.ajaxForm({'key':'email','value':val},'POST','/home_page/subscribe-nl')
              .done(function(resp){
                var res = JSON.parse(resp);
                if(res.status == "success"){
                  nl_input.value = "";
                  nl.style.display = "none";
                  Materialize.toast(res.message, 3000);
                  localStorage.setItem("nl_storage", false);
                  ga('send', {
                    hitType: 'event',
                    eventCategory: 'Popup - Newsletter Subscription',
                    eventAction: 'Click',
                    eventLabel: 'EnterTextSection-SubmitButton',
                    eventvalue: 'Web'
                  });
                  clevertap.event.push("Newsletter Subscribers", {
                    "All Subscribers - Email": val
                  });
                }
              })
          }
          else{
            Materialize.toast("Please provide valid Email / Phone Number", 3000);
            nl_input.value = "";
          }
        }
      }
    })

    //Minimize
    minimize.addEventListener('click', function () {
      wrapper.className = wrapper_class+" stay";
      show_nl.className = show_nl_class+" show";
      show_nl_flag = false;
      localStorage.setItem("nl_storage", false);
      localStorage.setItem("nl_min", true);
      ga('send', {
        hitType: 'event',
        eventCategory: 'Popup - Newsletter Subscription',
        eventAction: 'Click',
        eventLabel: 'NewsletterMinimizeIcon',
        eventvalue: 'Web'
      });

      ga('send', {
        hitType: 'event',
        eventCategory: 'Popup - Newsletter Subscription',
        eventAction: 'Impression',
        eventLabel: 'NewsletterIconModule'
      });
    })

    //Maximize
    show_nl.addEventListener('click', function () {
      nl.style.display = "block";
      wrapper.className = wrapper_class;
      show_nl.className = show_nl_class+" hide";
      show_nl_flag = true;
      localStorage.setItem("nl_min", false);
      ga('send', {
        hitType: 'event',
        eventCategory: 'Popup - Newsletter Subscription',
        eventAction: 'Click',
        eventLabel: 'NewsletterSubscriptionIcon',
        eventvalue: 'Web'
      });
    })

    $(document).ready(function() {
      $.ajaxSetup({ cache: true });
      $.getScript('//connect.facebook.net/en_US/sdk.js', function(){
        FB.init({
          appId: '495228424012009',
          version: 'v2.7'
        });
      });

      $('.fb-login-button').click(function () {
        FB.login(function(response) {
          if (response.status === 'connected') {
            fn_home.ajaxForm({'key':'fb_id','value':response.authResponse.userID},'POST','/home_page/subscribe-nl')
              .done(function(resp){
                var res = JSON.parse(resp);
                if(res.status == "success"){
                  nl_input.value = "";
                  nl.style.display = "none";
                  Materialize.toast(res.message, 3000);
                  localStorage.setItem("nl_storage", false);
                  ga('send', {
                    hitType: 'event',
                    eventCategory: 'Popup - Newsletter Subscription',
                    eventAction: 'Click',
                    eventLabel: 'FacebookIcon',
                    eventvalue: 'Web'
                  });
                }
              })
          } else {
            Materialize.toast("Please try again", 3000)
          }
        });
      });
    });

    var googleUser = {};
    var startApp = function() {
      gapi.load('auth2', function(){
        // Retrieve the singleton for the GoogleAuth library and set up the client.
        auth2 = gapi.auth2.init({
          client_id: '1093634637875-0o2amcs7p5a8hb3rmg4foocn2a5qeqe8.apps.googleusercontent.com',
          cookiepolicy: 'single_host_origin',
          ux_mode: 'popup'
        });
        attachSignin(document.getElementById('google_login'));
      });
    };

    function attachSignin(element) {
//        console.log(element.id);
      auth2.attachClickHandler(element, {},
        function(googleUser) {
          var e = googleUser.getBasicProfile()
          fn_home.ajaxForm({'key':'google_id','value':e.Eea},'POST','/home_page/subscribe-nl')
            .done(function(resp){
              var res = JSON.parse(resp);
              if(res.status == "success"){
                nl_input.value = "";
                nl.style.display = "none";
                Materialize.toast(res.message, 3000);
                localStorage.setItem("nl_storage", false);
                ga('send', {
                  hitType: 'event',
                  eventCategory: 'Popup - Newsletter Subscription',
                  eventAction: 'Click',
                  eventLabel: 'GoogleIcon',
                  eventvalue: 'Web'
                });
              }
            })
        }, function(error) {
          Materialize.toast("Could not subscribe, check your browser cookie settings", 4000);
        });
    }
    startApp();
  })
</script>
