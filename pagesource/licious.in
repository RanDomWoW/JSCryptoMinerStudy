            $('.search-cart').find('.cart').removeClass("loaded");
          }

          $('.search-cart').find('.cart').find('i').html(cart)
          if (q < 2) {
            $(this).parent().parent().removeClass("slide")
            qty.html(parseInt(0))
            return;
          } else {
            qty.html(parseInt(q - 1))
          }

        })


        $('.in-news ul li').on('click', function () {
          $('.in-news ul li').removeClass("active");
