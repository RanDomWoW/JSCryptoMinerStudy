  document.addEventListener('DOMContentLoaded', function () {
    var page = $('.page_slug').val();
    page = page.replace("/","")
    var ls = localStorage.getItem("showLocation");
      if (ls != "hide") {
        $('.location-search-pop-wrapper').removeClass("hide");
        $('.location-search-pop-wrapper').addClass("show");
        //praveen added
        $('.main-sublocation-wrapper').addClass("show");
        $('.loc-screen').addClass("show");
      } else {
        //praveen added
        $('.main-sublocation-wrapper').removeClass("show");
        $('.loc-screen').removeClass("show");
      }

    // Praveen Scripts Starts.....................................
    $('.location-search-pop-wrapper').removeClass("hide");
    $('.location-search-pop-wrapper').addClass("show");
    function hidelocpop() {
      document.addEventListener('click', function (e) {
//        console.log(e.target)
        if (e.target.classList.contains('loc-screen')) {
          $('.main-sublocation-wrapper').removeClass("show");
          $('.loc-screen').removeClass("show");
          $('.sublocation-old-popup').show();
          $('.loc-error, .loc-error-addr').hide();
          $('.recent-searches').addClass('hide');

          if(!$('.location-search-pop-wrapper').hasClass("guest")){
            $('.location-search-pop-wrapper').removeClass("show");
          }
          if ($('.addresses-list').find('.address').length > 0) {
            $('.location-search-pop-wrapper').addClass("hide");
            $('.location-search-pop-wrapper').removeClass("show");
            $('.sublocation-popup-wrapper').removeClass("hide");
            $('.sublocation-popup-wrapper').addClass("show");
          }
          else {
