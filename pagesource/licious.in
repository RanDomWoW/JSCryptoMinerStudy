      document.addEventListener('DOMContentLoaded', function () {

        var path = document.querySelector("path");
        var length = path.getTotalLength();
        path.style.strokeDasharray = length;
        path.style.strokeDashoffset = length;
        window.addEventListener("scroll", myFunction);

        function myFunction() {
          var scrollpercent = (document.body.scrollTop + document.documentElement.scrollTop) / (document.documentElement.scrollHeight - document.documentElement.clientHeight);
          var draw = length * scrollpercent * 4.5;
          path.style.strokeDashoffset = length - draw;
//          console.log(draw);
          if (draw > 58) {
            $('.what-wedo.one').addClass("active");
            $('.bullets .one').addClass("active");
          }
          else {
            $('.what-wedo.one').removeClass("active");
            $('.bullets .one').removeClass("active");
          }
          if (draw > 200) {
            $('.what-wedo.two').addClass("active");
            $('.bullets .two').addClass("active");
          }
          else {
            $('.what-wedo.two').removeClass("active");
            $('.bullets .two').removeClass("active");
          }
          if (draw > 415) {
            $('.what-wedo.three').addClass("active");
            $('.bullets .three').addClass("active");
          }
          else {
            $('.what-wedo.three').removeClass("active");
            $('.bullets .three').removeClass("active");
          }
          if (draw > 655) {
            $('.what-wedo.four').addClass("active");
            $('.bullets .four').addClass("active");
          }
          else {
            $('.what-wedo.four').removeClass("active");
            $('.bullets .four').removeClass("active");
          }
          if (draw > 920) {
            $('.what-wedo.five').addClass("active");
            $('.bullets .five').addClass("active");
          }
          else {
            $('.what-wedo.five').removeClass("active");
            $('.bullets .five').removeClass("active");
          }

        }

