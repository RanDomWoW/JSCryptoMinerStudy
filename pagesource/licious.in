<script type="text/javascript">
  //Maps
  function initMap(bounds_c) {
    var defaultBounds = bounds_c;
    var popUpInput = document.getElementById('location_pop');
    var placesFormLanding = document.getElementById('placesFormLanding');
    var usecurrloc = document.querySelector('.use-my-loc');
    var lng = document.querySelector('.lngpop');
    var lat = document.querySelector('.latpop');
    var map;
    var marker;
    var loc_loader = document.querySelector('.loader-img');
    var loc_loader_class = loc_loader.getAttribute('class');
    var locerr = document.querySelector('.loc-error');
    var locerr_class = locerr.getAttribute("class");
    var locerr_error = locerr.getAttribute("data-gp");
    var usecurrloc_class = usecurrloc.getAttribute("class");
    var options = {componentRestrictions: {country: 'in'}, bounds: defaultBounds, zoom: 13};
    var autocompletePop = new google.maps.places.Autocomplete(popUpInput, options);
    var click_go = document.querySelector('.go-class')

    //Maps
    autocompletePop.addListener('place_changed', function () {
      var placePop = autocompletePop.getPlace();
      placesFormLanding.dataset.lat = placePop.geometry.location.lat();
      placesFormLanding.dataset.lng = placePop.geometry.location.lng();
      get_location(placesFormLanding, {
        lat: placePop.geometry.location.lat(),
        lng: placePop.geometry.location.lng(),
        loc: popUpInput.value
      })
    });

    click_go.addEventListener('click', function (e) {
      e.preventDefault();
      if (popUpInput.value == "") {
        show_error("Please enter your location to view our catalogue");
      }
      else {
        locerr.className = locerr_class;
        get_location(placesFormLanding)
      }
    });

    function get_location(form, pos) {
      $('.use-my-loc').hide();
      $('.loader-img').show();
//      click_go.value = "";
      $.ajax({
        url: form.getAttribute("data-url"),
        type: 'POST',
        data: {
          lat: form.getAttribute("data-lat"),
          lng: form.getAttribute("data-lng"),
          location: popUpInput.value,
          clearcart: 'true'
        },
        timeout: 20000,
        headers: {
          'X-CSRF-TOKEN': $('meta[name="_token"]').attr('content')
        }
      })
        .done(function (response) {
          if (response.result == "success") {
            localStorage.setItem("showLocation", "hide")
            window.location.reload();
            $('.location-name').html(popUpInput.value)
            $('.location-search-pop-wrapper').removeClass("show")
            $('.main-sublocation-wrapper').removeClass("show")
            $('.location-search-pop-wrapper').find('.loc-error').hide();
            localStorage.setItem("hub_id", response.hub_id)

//            clevertap.event.push("Location Changed", {
//              "Lat": form.getAttribute("data-lat"),
//              "Lon": form.getAttribute("data-lng"),
//              "Location": popUpInput.value
//            });
            popUpInput.value = "";
            // Praveen added....
            var recentAddresses = JSON.parse(localStorage.getItem('addresses'));
            if (recentAddresses == null||recentAddresses.length==0) {
              recentAddresses = [];
                recentAddresses.unshift({
                    "lat": pos.lat,
                    "lng": pos.lng,
                    "location": pos.loc
                });
                localStorage.setItem("addresses", JSON.stringify(recentAddresses));
            }
            else{
               var sortRecentAddr = recentAddresses.slice(0,3);
                var flag = false;
                $.map(sortRecentAddr,function (data,i) {
                    if((parseFloat(pos.lat)==parseFloat(data.lat))&&(parseFloat(pos.lng)==parseFloat(data.lng))){
                        flag = true;
                      var cutAddr = recentAddresses.splice(i, 1);
                      recentAddresses.unshift(cutAddr[0]);
                      localStorage.setItem("addresses", JSON.stringify(recentAddresses));
                    }
                })
                if(flag==false){
                    recentAddresses.unshift({
                        "lat": pos.lat,
                        "lng": pos.lng,
                        "location": pos.loc
                    });
                    localStorage.setItem("addresses", JSON.stringify(recentAddresses));
                }
            }
            localStorage.setItem("default_addr_id","")
            localStorage.setItem("default_addr_lat", pos.lat)
            localStorage.setItem("default_addr_lng", pos.lng)
            var pageName = $('.pageName').val();
//                 console.log(pageName);
            var category = '';
            var eventLabel = 'Loc:' + pos.loc + ' Lat:' + pos.lat + ' Lon:' + pos.lng;
            if(pageName!=undefined){
              if(pageName=='homepage')
                category = 'SL-Homepage - Change Address';
              else if(pageName=='plp')
                category = 'SL-Product listing page - Change Address';
              else if(pageName=='pdp')
                category = 'SL-Product detail page - Change Address';
              fn_sublocation.gaSublocation(category,'Click',eventLabel)
            }
            // Praveen added....

          }
          else {
//            localStorage.setItem("showLocation", "show")
            var err_msg = '<img src="https://d2407na1z3fc0t.cloudfront.net/Banner/loc-err.png" alt="">' +
              'Thank you for stopping by, We unfortunately do not serve your locality'
            $('.location-search-pop-wrapper').find('.loc-error').html(err_msg).show();
            popUpInput.value = "";
            $('.use-my-loc').show();
            $('.loader-img').hide();
            ga('send', {
              hitType: 'event',
              eventCategory: 'LocationError',
              eventAction: 'Error',
              eventLabel: 'Loc:' + pos.loc + ' Lat:' + pos.lat + ' Lon:' + pos.lng + ' - NoService'
            });
//            console.log({
//              hitType: 'event',
//              eventCategory: 'LocationError',
//              eventAction: 'Error',
//              eventLabel: 'Loc:' + pos.loc + ' Lat:' + pos.lat + ' Lon:' + pos.lng + ' - NoService'
//            })
          }
        })
        .fail(function (error) {
//          console.log(error);
          $('.use-my-loc').show();
          $('.loader-img').hide();
        });
    }

//Geo Location
    usecurrloc.addEventListener('click', function (e) {
      e.preventDefault();
      $('.use-my-loc').hide();
      $('.loader-img').show();
      // geolocation.
      if (navigator.geolocation) {
        navigator.geolocation.getCurrentPosition(function (position) {
          var pos = {
            lat: position.coords.latitude,
            lng: position.coords.longitude
          };
          placesFormLanding.dataset.lat = pos.lat;
          placesFormLanding.dataset.lng = pos.lng;

          //Send response
          $.ajax({
            url: "https://maps.googleapis.com/maps/api/geocode/json?latlng=" + position.coords.latitude + "," + position.coords.longitude + "&key=AIzaSyAxCUFRpqhv_ddrIDYagEAEPe-Q_bx6KZs",
            type: 'POST'
          })
            .done(function (response) {
              var addr = response.results[0].formatted_address;
              $.ajax({
                url: placesFormLanding.getAttribute("data-url"),
                type: 'POST',
                data: {
                  lat: pos.lat,
                  lng: pos.lng,
                  location: response.results[0].formatted_address,
                  clearcart: 'true'
                },
                timeout: 20000,
                headers: {
                  'X-CSRF-TOKEN': $('meta[name="_token"]').attr('content')
                }
              })
                .done(function (response) {
                  if (response.result == "success") {
//                    $('.use-my-loc').show();
//                    $('.loader-img').hide();
                    localStorage.setItem("showLocation", "hide")
                    localStorage.setItem("hub_id", response.hub_id)
//                    console.log({
//                      "Lat": pos.lat,
//                      "Lon": pos.lng,
//                      "Location": addr
//                    })
                    clevertap.event.push("Location Changed", {
                      "Lat": pos.lat,
                      "Lon": pos.lng,
                      "Location": addr
                    });
                    window.location.reload();
                    localStorage.setItem("default_addr_id","")

                    localStorage.setItem("default_addr_lat", pos.lat)
                    localStorage.setItem("default_addr_lng", pos.lng)
                  }
                  else {
//                    localStorage.setItem("showLocation", "show")
                    var err_msg = '<img src="https://d2407na1z3fc0t.cloudfront.net/Banner/loc-err.png" alt="">' +
                      'Thank you for stopping by, We unfortunately do not serve your locality'
                    $('.location-search-pop-wrapper').find('.loc-error').html(err_msg).show();
                    $('.use-my-loc').show();
                    $('.loader-img').hide();
                    ga('send', {
                      hitType: 'event',
                      eventCategory: 'LocationError',
                      eventAction: 'Error',
                      eventLabel: 'Loc:' + addr + ' Lat:' + pos.lat + ' Lon:' + pos.lon + ' - NoService'
                    });
                  }
                })
                .fail(function (error) {
//                  console.log(error);
                  $('.use-my-loc').show();
                  $('.loader-img').hide();
                });
            });

        }, function () {
          $('.use-my-loc').show();
          $('.loader-img').hide();
          var err_msg = '<img src="https://d2407na1z3fc0t.cloudfront.net/Banner/loc-err.png" alt="">' +
            'Unable to get your location, Please try again.'
          $('.location-search-pop-wrapper').find('.loc-error').html(err_msg).show();
          ga('send', {
            hitType: 'event',
            eventCategory: 'LocationError',
            eventAction: 'Error',
            eventLabel: 'Loc: Lat:' + pos.lat + ' Lon:' + pos.lon + ' - NoService'
          });
        });
      } else {
        $('.use-my-loc').show();
        $('.loader-img').hide();
        var err_msg = '<img src="https://d2407na1z3fc0t.cloudfront.net/Banner/loc-err.png" alt="">' +
          'Your browser does not support this feature.'
        $('.location-search-pop-wrapper').find('.loc-error').html(err_msg).show();
      }
    });

    popUpInput.addEventListener('focus', function () {
      usecurrloc.className = usecurrloc_class + " show";
    })
    popUpInput.addEventListener('blur', function () {
      usecurrloc.className = usecurrloc_class;
    })

    if (typeof showMap === "function") {
      showMap();
    }
  }

