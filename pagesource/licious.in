          $('.search-screen').hide();
          $('.search .search-field').hide();
          $('.profile .profile-drop').hide();
        })

        $(document).on('keydown', function (e) {
          if (e.keyCode === 27) {
            $('.search-screen').hide();
            $('.search .search-field').hide();
            $('.profile .profile-drop').hide();
          }
        })
        var cart = 0;
        $('.add-to-cart').on('click', function () {
          $(this).parent().addClass("slide");
          cart++;
          $(this).parent().find('.item-qty').html("1")
          $('.search-cart').find('.cart').addClass("loaded")
          $('.search-cart').find('.cart').find('i').html(cart)
        })
        $('.add-one').on('click', function () {
          cart++;
          var qty = $(this).parent().find('.item-qty');
          qty.html(parseInt(qty.html()) + 1)
          $('.search-cart').find('.cart').addClass("loaded")
          $('.search-cart').find('.cart').find('i').html(cart)
        })
        $('.remove-one').on('click', function () {
          cart--;
