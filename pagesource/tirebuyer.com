                    $(function() {
                        // set dataStore items from querystring else check to see if cookie exists
                        var queryString = getQueryStrings();

                        // cartId
                        if (queryString.cartId !="" && queryString.cartId != undefined){
                            dataStore.cart.cartId = queryString.cartId;
                        }else if( $.cookie('cartId') != "" && $.cookie('cartId') != undefined){
                            dataStore.cart.cartId = $.cookie('cartId');
                        }

                        // Get Akamai geo info
                        
                            $.extend(dataStore.zip.geo,{"country":"US","state":"VA","county":"LOUDOUN","city":"ASHBURN","zipCodes":["20146","20147","20148","20149"],"dma":"511","ip":"54.81.128.172"});
                        $.extend(dataStore.fitmentData.zip.geo,{"country":"US","state":"VA","county":"LOUDOUN","city":"ASHBURN","zipCodes":["20146","20147","20148","20149"],"dma":"511","ip":"54.81.128.172"});
                        

                        // zipcode
                        if (dataStore.zip != "" && dataStore.zip != undefined && dataStore.zip.geo.length > 1){
                            dataStore.zip.zipCode = dataStore.zip.geo.zipCodes[0];
                            dataStore.fitmentData.zip.zipCode = dataStore.zip.geo.zipCodes[0];
                        }else if (queryString.zipCode !="" && queryString.zipCode != undefined){
                            dataStore.zip.zipCode = queryString.zipCode;
                            dataStore.fitmentData.zip.zipCode = queryString.zipCode;
                        }else if( $.cookie('TBZIP') != "" && $.cookie('TBZIP') != undefined){
                            if (dataStore.zip === "" || dataStore.zip === undefined) {
                                dataStore.zip = {};
                            }
                            dataStore.zip.zipCode = $.cookie('TBZIP');
                            dataStore.fitmentData.zip.zipCode = $.cookie('TBZIP');
                        }

                        // site device type
                        if ( $('body').hasClass('device-desktop') ){
                            dataStore.site.device = 'desktop';
                        }else if ( $('body').hasClass('device-tablet') ){
                            dataStore.site.device = 'tablet';
                        }else if ( $('body').hasClass('device-mobile') ){
                            dataStore.site.device = 'mobile';
                        }else{
                            dataStore.site.device = 'desktop';
                        }

                        // site device orientation
                        if ( $('body').hasClass('device-orientation-landscape') ){
                            dataStore.site.orientation = 'landscape';
                        }else if ( $('body').hasClass('device-orientation-portrait') ){
                            dataStore.site.orientation = 'portrait';
                        }else{
                            dataStore.site.orientation = 'landscape';
                        }

                    });
