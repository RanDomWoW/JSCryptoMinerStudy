<script src="//maps.google.com/maps/api/js?sensor=true"></script>
<script>
//googleMapの埋め込み
function initialize() {
var latlng = new google.maps.LatLng(34.732884, 135.733921);
var myOptions = {
zoom: 13, /*拡大比率*/
center: latlng, /*表示枠内の中心点*/
mapTypeId: google.maps.MapTypeId.ROADMAP,/*表示タイプの指定*/
mapTypeControl: false,
zoomControl: false,
streetViewControl: false,
scrollwheel: false/*スクロールで拡大縮小の禁止*/
};
var map = new google.maps.Map(document.getElementById('mapCanvas'), myOptions);
/*アイコン設定▼*/
var icon = new google.maps.MarkerImage('/img/index/pin.png',
new google.maps.Size(80,84),/*アイコンサイズ設定*/
new google.maps.Point(0,0)/*アイコン位置設定*/
);
var markerOptions = {
position: latlng,
map: map,
icon: icon,
title: '奈良先端科学技術大学院大学'
};
var marker = new google.maps.Marker(markerOptions);
}
(function fixInfoWindow() {
var set = google.maps.InfoWindow.prototype.set;
google.maps.InfoWindow.prototype.set = function(key, val) {
if (key === "map") {
if (! this.get("noSupress")) {
return;
}
}
set.apply(this, arguments);
}
})();
// IFrame Player API の読み込み
var tag = document.createElement('script');
tag.src = "https://www.youtube.com/iframe_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
// YouTubeの埋め込み
function onYouTubeIframeAPIReady() {
ytPlayer = new YT.Player(
'movieArea', // 埋め込む場所の指定
{
width: '100%', // プレーヤーの幅
height: 600, // プレーヤーの高さ
videoId: 'pv3p9lIulZM', // YouTubeのID
playerVars: {
wmode: 'transparent',
rel: 0, // 再生終了後に関連動画を表示するかどうか設定
loop: 1, //動画を繰り返し再生するかどうか設定
playlist: 'pv3p9lIulZM',
controls: 0, //コントロールバーの表示を設定
autoplay: 0, // 自動再生するかどうか設定
modestbranding: 1, //YouTubeロゴの表示を設定
showinfo: 0 //動画再生前にタイトルなどを表示するどうか設定
}
}
);
}
