  clear: both;
}
.container {
	width:1170px;
	margin:0 auto;
	padding:0px 12px;
}
.show_767 {
	display:none;
}
/* common style */
.orange_btn {
	font-size:12px;
	text-transform:uppercase;
	padding:7px 10px;
	border:1px solid #f48033;
	color:#f48033;
	font-family: 'gotham_boldregular';
	transition:all 0.5s ease 0s;
