	color:#3f4443 !important;
}

.resp-content-active, .resp-accordion-active {
    display: block;
}

.resp-tab-content {
   /* border-left: 1px solid #97dddd !important;*/
}

h2.resp-accordion {
    font-size: 13px;
    border: 1px solid #c1c1c1;
    border-top: 0px solid #c1c1c1;
    margin: 0px;
    padding: 10px 15px;
}

