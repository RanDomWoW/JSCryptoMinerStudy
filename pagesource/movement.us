	text-transform:uppercase;
	padding:7px 10px;
	font-family: 'gotham_boldregular';
	transition:all 0.5s ease 0s;
	display:inline-block;
	text-align:center;
}
.cyan_button:hover {
	background:#97dddd;
	color:#3f4443;
	text-decoration:none;
	transition:all 0.5s ease 0s;
}
input[type="submit"],input[type="button"] {
	font-size:18px;
	text-transform:uppercase;
	padding:7px 10px;
	border:1px solid #f48033;
	color:#f48033;
	font-family: 'gotham_boldregular';
	transition:all 0.5s ease 0s;
