			if ( typeof(type) === 'undefined' ) { type = 'Esemény'; }
			if ( typeof(label) === 'undefined' ) { label = 'pontatlan'; }
			
			non_interaction_event = EVE.pageTime >= EVE.interactiveEventAfter ? true : false;
			
			gtag('event', type, {
