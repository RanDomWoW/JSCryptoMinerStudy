				'event_label': label,
				'non_interaction': non_interaction_event
			});
		}
		
		/**
		 * Event bejelentése GA-nak, majd ugrás az adott link-re.
		 * 
		 * @param str url - ugrás Link-je a bejelntés után
		 * @param str category - Link kategóriája
		 * @param str label - Link címkéje pl. title
		 * @returns empty
		 */
		var trackOutboundLink = function( url, category, type, label ) {
			if ( typeof(category) === 'undefined' ) { category = 'outbound'; }
			if ( typeof(type) === 'undefined' ) { type = 'click'; }
			if ( typeof(label) === 'undefined' ) { label = url; }
		
			gtag('event', type, {
				'event_category': category,
				'event_label': label,
				'transport_type': 'beacon',
				'event_callback': function(){ document.location = url; }
			});
		}
		
