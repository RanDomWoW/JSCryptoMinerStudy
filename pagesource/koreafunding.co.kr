                } ).then( cb, function() {} ); break;
                case "confirm": swal(
                    { html: '<b>'+msg.replace( /\\n|\n/g, "<br>\n" )+'</b>', type: 'info', confirmButtonColor: '#3085d6', width: "650px", showCancelButton: true ,confirmButtonText: "확인",cancelButtonText: "취소"}
                ).then( cb, function( dismiss ) { if ( dismiss === 'cancel' ) {}; return false; } ); break;
                default: swal( { html: '<b>'+msg.replace( /\\n|\n/g, "<br>\n" )+'</b>', text: msg, type: 'info', confirmButtonColor: '#3085d6', confirmButtonText: "확인" } ).then( cb ); break;
            }
        }
        return false;
    };

