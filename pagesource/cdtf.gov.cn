    function playPause() {
      var music = document.getElementById('music');
      var music_img = document.getElementById('music_img');
      if (music.paused){
        music.play();
        music_img.src = '/chengdu/xhtml/images/playing.gif';
        $("#music_btn").attr('title','点击暂停');
      }else{
        music.pause();
        music_img.src = '/chengdu/xhtml/images/pausing.png';
        $("#music_btn").attr('title','点击播放');
      }
    }
