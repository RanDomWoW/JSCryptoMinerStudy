		<script type="text/javascript">
			function rennab_shuffle_go() {
				var rennabs = document.getElementsByClassName('rennab shuffle');
				var params = "";	// Параметры для POST запроса
				for (var i=1; i <= rennabs.length; i++) {
					params += 'b' + i + '=' + rennabs[i-1].id + '&w' + i + '=' + rennabs[i-1].clientWidth + '&'; // Параметры для POST запроса
				}
				params = params.slice(0,-1);
				httpRequest(
					"POST",
					"/api/rennab_shuffle",
					params,
					function ( responseText ) {
						try { var response = JSON.parse( responseText ); }
						catch(e) { return; }
						switch ( response.status ) {
							case "OK" :
								for ( var key in response.data ) {
									document.getElementById(key).innerHTML = response.data[key];
								}
								break;
							default :
								console.log(responseText);
								break;
						}
					}
				);
			}
			var rennab_shuffle_block = false;
			rennab_shuffle_go();
			setTimeout(rennab_shuffle_go,25000);
			setTimeout(rennab_shuffle_go,50000);
		</script>

