	});
});

function show_5day(){
	if ($('#wxbox:hover').length>0) {
		if(state5day == "off") {
			$("#wxnow").slideToggle(400);
			$("#wx5day").slideToggle(400);
			state5day = "on";
		}
	}
}

function hide_5day(){
	if(state5day == "on") {
		$("#wxnow").slideToggle(300);
		$("#wx5day").slideToggle(300);
		state5day = "off";
	}
}
