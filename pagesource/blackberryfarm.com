                <script>
            
            
            
            //-------------------------- define functions
            
            
            	function setSlideoutHeight(){
                    var totalHeight = 0;
                    $("#slideout-nav nav").children().each(function(){
                        totalHeight = totalHeight + $(this).outerHeight(true);
                    });
                    var padHeight = totalHeight + 40;
                    var winHeight = $(window).height() - 30;
                    
                   
                    $('#slideout-nav nav').css({'height': padHeight, 'max-height' : winHeight});
                }
                
            	function closeSlideout(){
                    var winWidth = $(window).width();
                    // close slideout menu
                    $('#slideout-nav').removeClass('visible');
                    if(winWidth >= 1200){
                        $('#slideout-nav nav').css('height','40px');
                    }else{
                        $('#slideout-nav nav').css('height','34px');
                    }
                    
                }
                
                function stickUnstickSlideout(){
                    
                    var $el = $('#slideout-nav'); 
                    var isPositionFixed = ($el.css('position') == 'fixed');
                    var winWidth = $(window).width();
                    
                    if(winWidth >= 1200){
                        if ($(window).scrollTop() > 132 && isPositionFixed == false){ 
                            $('#slideout-nav').css({'position': 'fixed', 'top': '30px'}); 
                        }
                        if ($(window).scrollTop() <= 132 )
                        {
                            $('#slideout-nav').css({'position': 'absolute', 'top': '162px'}); 
                        }
                    }else if(winWidth < 1200 && winWidth >= 768){
                        $('#slideout-nav').css({'position': 'absolute', 'top': '74px'}); 
                    }else if(winWidth < 768){
                        $('#slideout-nav').css({'position': 'absolute', 'top': '60px'}); 
                    }
                }
            	
                function setSlideoutTimeout(){               
                    var timeout;
                    
                    timeout = window.setTimeout(hideDeskSlideout, 4500);
                    
                    $("#slideout-nav").mouseenter(function() {
                        window.clearTimeout(timeout);
                    }).mouseleave(function(){
                        timeout = window.setTimeout(hideDeskSlideout, 3000);
                    });
                    
                    function hideDeskSlideout() {
                    	window.clearTimeout(timeout);
                    	if($(window).width() >= 1200){
                        	$('#slideout-nav').removeClass('visible');
                        	$('#slideout-nav nav').css('height','40px');
                        }
                    }
                    $( window ).resize(function() {
                      	hideDeskSlideout();
                    });
                    
                    
                }
            
            
            
         //-------------------------- document ready
            
            $(document).ready(function(){
                
                // assign selected calss to slideout menu
                var currentpath = window.location.pathname;
                
                
                //console.log('currentpath='+currentpath);
