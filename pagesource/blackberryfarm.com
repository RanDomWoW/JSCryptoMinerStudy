                    var thispath = $(this).attr('href');
                    //console.log("h3="+$(this).parent("h3").length);
                    if ($(this).parent("h3").length == 0 && currentpath == thispath){
                        // console.log("it is it! "+currentpath +" / "+ thispath);
                        $(this).addClass( "selected" );
                    }
                });
                $( "#nav_2235223 a" ).each(function( i ) {
                    var thispath = $(this).attr('href');
                    var patharray = thispath.split("/");
                    var curpathsplit = currentpath.split("/");
                    //console.log("paths "+curpathsplit[1]);
                    if (curpathsplit[1] == patharray[1] || curpathsplit[1] == patharray[0]){
                        $(this).parent('li').addClass( "selected" );
                    }
                });
                $( "#nav_849166 a" ).each(function( i ) {
              		var thispath = $(this).attr('href');
                    if(currentpath.indexOf(thispath) >= 0){
                        $(this).parent('li').addClass( "selected" );
                        
                    }
                    if(currentpath.indexOf('/accommodation/')  >= 0 && thispath.indexOf('/accommodations/') >= 0){
                    	$(this).parent('li').addClass( "selected" );  
                	}
                });
                $( "#nav ul li a" ).each(function( i ) {
