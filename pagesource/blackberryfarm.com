            var scrollTimeout;
            
            $(window).scroll(function () {
                
                if (scrollTimeout) {
                    clearTimeout(scrollTimeout);
                    scrollTimeout = null;
                }
                scrollTimeout = setTimeout(scrollHandler, 10);
                
            });
            
            scrollHandler = function () {
                if ($('html').hasClass('no-touchevents')){
                    
                    
                    $( '.parallax .bg-img' ).each(function( index ) {
                        var scrolled = $(window).scrollTop();
                        var fromtop = $(this).parent( ".parallax" ).offset().top;
                        var parentheight = $(this).parent( ".parallax" ).height();
                        var bottomwindowscroll = $( window ).height() + scrolled;
                        var bpttomofparent = parentheight + fromtop;
                        
                        if(bottomwindowscroll>fromtop && scrolled <= bpttomofparent ){
                            var imgheight =$(this).height();
                            var imgwidth =$(this).width();
                            var newtop = 0 - ((imgheight - parentheight)/2);
                            var totalspacetomove = $( window ).height() + parentheight;
                            var currentamount = bpttomofparent-scrolled;
                            var amountpossibletoscroll = 0-newtop;
                            var widthroom = imgwidth/4;
                            var percentmoved = (currentamount / totalspacetomove) * 100;
                            var percentoffinal = amountpossibletoscroll * percentmoved / 100;
                            //var translateX = widthroom * percentmoved / 100;
                            var translateX = 0;
                            if(percentmoved >= 1){
                                var translateY = 0-percentoffinal/1.2;
                                
                                var translateX = 0-translateX/9;
                                $(this).css({ 'transform':'translate3d(' + translateX +'px, ' +   translateY + 'px, 0)'}); 
                                
                            }
                        }
                        $(this).css({ top: newtop+'px',left: '0px' });
                    });
                }
            };
            
