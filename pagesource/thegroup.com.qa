                $("#MyTable .TradesValueWidth").css("border-right", "1px solid transparent !important");
                $("#MyTable .TradesValueWidth").css("border-right-color", "transparent !important"); //alert('ggggggggg');
            }
        }
    }

    function containsArr(el, arr)
    {
        var found = false;
        for (k=0;k<arr.length;k++) 
        {
            if (arr[k] == el)
            {
                found = true;
                break;
            }
        }
        return found;
    }
    function getHiddenColumns(container) {
        var notChecked = '';
        $(container ).find('input[type="checkbox"]').each(function () {
            if ($(this).is(':checked')) {
            }
            else {
                notChecked += $(this).attr('id') + ",";
            }
        });
        if (notChecked.length > 0)
            return notChecked.substring(0, notChecked.length - 1);
        else
            return "";
    }

    function removeItemFromArray(arr, item) {
        for (var i = arr.length; i--;) {
            if (arr[i] === item) {
                arr.splice(i, 1);
            }
        }
    }

    function updateAlertedCompanies(objCheckBox)
    {
        if (objCheckBox.checked) {
            arrSoundAlertedCompanies.push(objCheckBox.id.substr(objCheckBox.id.indexOf('_') + 1));
        }
        else {
            removeItemFromArray(arrSoundAlertedCompanies, objCheckBox.id.substr(objCheckBox.id.indexOf('_') + 1));
        }

        // Save alerted companies in cookies
        var strSoundAlertedCompaniesCookie = "";
        for (var idx = 0; idx < arrSoundAlertedCompanies.length; idx++) {
            strSoundAlertedCompaniesCookie += arrSoundAlertedCompanies[idx] + ","
        }
        strSoundAlertedCompaniesCookie = strSoundAlertedCompaniesCookie.substring(0, strSoundAlertedCompaniesCookie.length - 1);
        setCookie('SoundAlertedCompanies', strSoundAlertedCompaniesCookie, 365);

        if (arrSoundAlertedCompanies.length > 0)
            $('#imgSoundAlertIcon').attr('src', 'images/sound-icon.png');
        else
            $('#imgSoundAlertIcon').attr('src', 'images/sound-icon-Mute.png');
    }
	
