                $(".fulltable-tbl-header").css("width", "auto").css("max-width", "auto");
                $("#MyTable1 th.TradesValueWidth").css("min-height", "37px");
            }
            var found = false;
            var found2 = false;
            var columns = notSelectedColumns.toString().split(",");
            for (j=0;j<columns.length;j++) {
                if (columns[j].toString() == "col_Low52Width") { found = true; }
                if (columns[j].toString() == "col_TradesValueWidth") { found2 = true; }

                var column_class = columns[j].toString().replace("col_", "");
                //alert(column_class);
                $("." + column_class).hide();
            }
            if (found == true && found2 == false) {
                $("#MyTable .TradesValueWidth").css("border-right", "1px solid gray !important");
