            tab_text = tab_text + "<tr style='background-color:#792037;color:#ffffff'>" + htmltr + "</tr>";
        }
        tab = document.getElementById('MyTable'); // id of table
        for (j = 0 ; j < tab.rows.length ; j++) {
            var trHtml = $("#OnTheFlyTable").append("<tr id='tr_"+ j+"'>" +tab.rows[j].innerHTML + "</tr>");  
            
            $("#tr_"+ j).find("div[datanotexported=true],td[datanotexported=true]").remove();
            var tds = new Array();
            var htmltr = "";
            var rowbgColor = $(tab.rows[j]).attr("bgcolor");
            $("#tr_" + j).find("td").each(function () {
                var bgcolor = $(this).css("background-color");
                var tdId = $(this).attr("id");
                try {
                    if (tdId.indexOf("tdLastTradePriceParent") >= 0 || tdId.indexOf("tdNetChangeParent") >= 0 || tdId.indexOf("tdChangePercParent") >= 0 ||  tdId.indexOf("tdAskPriceParent") >= 0 || tdId.indexOf("tdOfferPriceParent") >= 0) {
                        bgcolor = $(this).children(0).css("background-color");
                    }
                } catch (ebgColor) { }
                if (bgcolor == "rgba(0, 0, 0, 0)" || bgcolor == "transparent")
                    if (rowbgColor == "white")
                        tds.push("<td>" + $(this).html() + "</td>");
                    else
                        tds.push("<td bgcolor='" + rowbgColor + "'>" + $(this).html() + "</td>");
                else
                    tds.push("<td bgcolor='" + rgba2hex(bgcolor) + "'>" + $(this).html() + "</td>");
            });
            for (i = 0; i < tds.length; i++) {
                htmltr += tds[i];// "<td>" + tds[i] + "</td>";
            }
            
            tab_text = tab_text + "<tr>" + htmltr + "</tr>";
        }


        tab_text = tab_text + "</table>";
        tab_text = tab_text.replace(/<A[^>]*>|<\/A>/g, "");//remove if u want links in your table
        tab_text = tab_text.replace(/<img[^>]*>/gi, ""); // remove if u want images in your table
        tab_text = tab_text.replace(/<input[^>]*>|<\/input>/gi, ""); // reomves input params
        tab_text += "</html>";

        var file_name = "The Group Prices_" + ($("#divMarket_LastUpdate").html().split(" ").length > 0 ? $("#divMarket_LastUpdate").html().split(" ")[0].toString().replace(/\//g, '-') : getDate('dd_mm_yyyy', new Date()).toString().replace(/\//g, '-')) + ".xls";
        var ua = window.navigator.userAgent;
        var msie = ua.indexOf("MSIE ");

        var is_chrome = ua.indexOf('Chrome') > -1;
        var is_safari = ua.indexOf("Safari") > -1;
        if ((is_chrome) && (is_safari)) { is_safari = false; }
       
        if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./))      // If Internet Explorer
        {
            txtArea1.document.open("xls/html","_blank");
            txtArea1.document.write(tab_text);
            txtArea1.document.close();
            txtArea1.focus();
            sa = txtArea1.document.execCommand("SaveAs", true, file_name);
        }
        else if (is_safari) {
            var uriContent = encodeURIComponent(tab_text);

            var form = $('<form method="POST" action="getExcel.aspx' + '">');
            form.append($('<input type="hidden" name="data' + '" value="' + uriContent + '">'));
            $('body').append(form);
            form.submit();

            return;
            //==========

            var formData = new FormData(); 
            tab_text = tab_text.replace(/\+/g, '___');
            formData.append('contents', escape(tab_text));
            
            $.ajax({
                type: 'POST',
                url: 'FileUploader.ashx',
                data: formData,
                cache: false,
                contentType: false,
                processData: false,
                success: function (filename) {
                   
                        var pom = document.createElement('a');
                        pom.setAttribute('href', 'excel/' + filename);
                        pom.setAttribute('target', "blank");
                        pom.setAttribute('download', filename);
                        ////pom.download = file_name;
                        if (document.createEvent) {
                            var event = document.createEvent('MouseEvents');
                            event.initEvent('click', true, true);
                            pom.dispatchEvent(event);
                        }
                        else {
                            pom.click();
                        }                    
                },
                processData: false,
                contentType: false,
                error: function () {
                     if (lang == "ar")
                        alert("حدث خطأ أثناء تصدير البيانات. برجاء المحاولة لاحقا");
                    else
                        alert("Error occurred in exporting data. Please, try again later.");
                }
            });


            

        }
        else                 //other browser not tested on IE 11
        {

            var uri = 'data:application/vnd.ms-excel,'+ encodeURIComponent(tab_text);
            //debugger;
            var downloadLink = document.createElement("a");
            downloadLink.href = uri;
            downloadLink.download = file_name;

            document.body.appendChild(downloadLink);
            downloadLink.click();
            document.body.removeChild(downloadLink);

            
            //sa = window.open('data:application/vnd.ms-excel,'+ encodeURIComponent(tab_text));

        }
    }


   
    

    function getDate(mode, userdate) {
        var dte = userdate || new Date(),
            d = dte.getDate().toString(),
            m = (dte.getMonth() + 1).toString(),
            yyyy = dte.getFullYear().toString(),
            dd = (d.length < 2) ? '0'+ d : d,
            mm = (m.length < 2) ? '0'+ m : m,
            yy = yyyy.substring(2, 4);
        switch (mode) {
            case 'dd_mm_yyyy': return dd + '_'+ mm + '_'+ yyyy;
            case 'yyyymmdd': return yyyy + mm + dd;
            default: return dte;
        }
    }

   
