                      "col_AvgPriceWidth,col_High52Width,col_Low52Width,col_TradesValueWidth";
                            
        var allColumns = allColumnsStr.toString().split(",");
        for (n = 0; n < allColumns.length; n++) {
            var column_class = allColumns[n].toString().replace("col_", "");
            $("." + column_class).show();
        }

        if (notSelectedColumns != null && notSelectedColumns != "") {
            var ua = window.navigator.userAgent;
            if (ua.indexOf("MSIE 7") >= 0 || ua.indexOf("MSIE 8") >= 0) {
                $(".priceStreamerCell").css("width", "auto").css("max-width", "auto");
