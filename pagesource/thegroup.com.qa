    function affectCheckColumns(container, notSelectedColumns) {
        //alert(notSelectedColumns);
        if (notSelectedColumns != null && notSelectedColumns != "") {
            var columns = notSelectedColumns.toString().split(",");
            $(container).find('input[type="checkbox"]').each(function () {
                var id = $(this).attr('id');
                if (containsArr(id,columns)== true)
                {
                    $("#" + id).prop('checked', false); // Unchecks it
                }
            });
        }
    }

    function affectColumns(notSelectedColumns) {
        var allColumnsStr = "col_CompanyWidth,col_HighPriceWidth,col_AskVolWidth,col_AskPriceWidth,col_BidVolWidth,col_BidPriceWidth,col_LowPriceWidth,col_ClosePriceWidth," +
