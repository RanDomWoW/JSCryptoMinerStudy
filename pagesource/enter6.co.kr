	var view_size = 3;
	var num_ = Math.ceil(history2.length/view_size);
	var total_num_ = history2.length;
	var total_pnum_ = Math.ceil(history2.length/view_size);

	today_history("");

	function today_history(con_num){
		var m_mstring = "";
		var mstring = "";
		if(num_==0) {
			$("#today_history_page_num").html("0");
			$("#today_history_total_num").html("0");
			$("#today_products").html("<p style='width:100%;margin-top:10px;padding:25px 0;border:1px solid #eee;background:#f2f2f2;text-align:center;'>최근 본 상품이 없습니다.</p>");
			$("#today_products_page").hide();
		} else {
			if(con_num!="") {
				var test_num_=num_ + con_num;
				//alert(test_num_);
				if(test_num_ > total_pnum_){
					alert('더이상 품목이 없습니다.');
