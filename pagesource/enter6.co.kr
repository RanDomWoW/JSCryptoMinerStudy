<script>
    $('#search_texts').keyup(function(e) {
        var searchInputDOM = $(this),
            keyboardCode = e.keyCode;

        if (keyboardCode == 38 || keyboardCode == 40) {
            $('.relation_search').hide();
            $('.relation_search2').hide();
        }
        else if (keyboardCode == 13) {
            $('.relation_search').hide();
            $('.relation_search2').hide();
        }
        else {
            if(searchInputDOM.val().length > 0){
                $('.relation_search2').hide();
                $.ajax({
                    url: '/diquest/search.php',
                    type: 'get',
                    dataType: "html",
                    data: ({
                        act: "auto",
                        dq_max_cnt: "",
                        search_text: searchInputDOM.val()
                    }),
                    success: function(result) {
                        if (!result) {
                            $('.relation_search').hide();
                        } else {
                            $('.relation_search').show();

                            var list = JSON.parse(result);

                            var output = "";
                            var output_brand = "";
                            var output_category = "";
                            var search_text = searchInputDOM.val();

                            if(list.keyword) {
                                $.each(list.keyword, function (key, item) {
                                    var keyword = item; // 상품명

                                    //상품명 내 검색어 강조를 위해 치환
                                    keyword = keyword.replace(search_text ,"<strong>"+search_text+"</strong>");
                                    output += '<a href="/shop/search.php?search_text=' + item + '"> ' + keyword + '</a>';
                                });
                            }

                            if(list.brand) {
                                $.each(list.brand, function (key, item) {
                                    var brand_name = item.brand_name;
                                    var b_ix = item.brand_name;

                                    //상품명 내 검색어 강조를 위해 치환
                                    brand_name = brand_name.replace(search_text , "<strong>" + search_text + "</strong>");
                                    output_brand += '<a href="/event/goods_brand.php?b_ix=' + item.b_ix + '"> ' + brand_name + '</a>';
                                    //output += '<span>71</span>';
                                });
                            }

                            if(list.category) {
                                $.each(list.category, function (key, item) {
                                    var cname = item.cname;
                                    var cid = item.cid;
                                    var cpath = item.cpath;

                                    //상품명 내 검색어 강조를 위해 치환
                                    cpath = cpath.replace(search_text , "<strong>" + search_text + "</strong>");
                                    output_category += '<a href="/shop/goods_list.php?cid=' + item.cid + '"> ' + cpath + '</a>';
                                    //output += '<span>71</span>';
                                });
                            }

                            if (output || output_brand || output_category) {
                                if (output) {
                                    $('.auto_area').html(output);
                                }
								else {
                                    $('.auto_area').html('');
                                }

                                if (output_brand) {
                                    $('.auto_area_brand').html(output_brand);
                                    $('.auto_area_brand_title').show();
                                }
                                else {
                                    $('.auto_area_brand').html('');
                                    $('.auto_area_brand_title').hide();
                                }

                                if (output_category) {
                                    $('.auto_area_category').html(output_category);
                                    $('.auto_area_category_title').show();
                                }
                                else {
                                    $('.auto_area_category').html('');
                                    $('.auto_area_category_title').hide();
                                }
                            }
                            else {
                            	$('.relation_search').hide();
                            }

                        }
                    }
                });
            }else{
                $('.relation_search').hide();
            }
        }
    });
    $('#search_texts').click(function() {
        if ($(this).val() == '') {
            $(this).attr('placeholder','');
            if($(".relation_search2").is(':hidden')) {
                $(".relation_search2").show();
            }
            else {
                $(".relation_search2").hide();
            }

            if ($('.auto_area_recent li').length > 0) {
                $('.no-keyword').css('display','none');
            }
            else {
                $('.no-keyword').css('display','block');
            }
		}

        $.ajax({
            url: '/diquest/search.php',
            type: 'get',
            dataType: "html",
            data: ({
                act: "hot"
            }),
            success: function(result) {
                var list_hot = JSON.parse(result);
                var output_hot = "";

                if(list_hot.ResultList) {
                    $.each(list_hot.ResultList, function (key, item) {
                        var keyword = item.keyword;
                        var ranking = item.ranking;

                        output_hot += '<li><strong>' + ranking + '</strong><a href="/shop/search.php?search_text=' + keyword + '"> ' + keyword + '</a></li>';
                    });
                }
                if (output_hot) {
                    if (output_hot) {
                        $('.auto_area_hot').html(output_hot);
                    }
                }
            }
        });
        $.ajax({
            url: '/diquest/search.php',
            type: 'get',
            dataType: "html",
            data: ({
                act: "recent"
            }),
            success: function(result) {
				if (!result) {
                    $('.no-keyword').css('display','block');
                }
				else {
                    var list_recent = JSON.parse(result);
                    var output_recent = "";
                    if(list_recent) {
                        $.each(list_recent, function (key, item) {
                            var recent_keyword = item.keyword;
                            var recent_date = item.date;

                            output_recent += '<li class="recent_keyword" id="id_recent_li'+key+'"><a href="/shop/search.php?search_text=' + recent_keyword + '"> ' + recent_keyword + '</a>';
                            output_recent += '<span class="date">'+recent_date+'<a href="javascript:void(0)" onclick=delete_keyword("'+key+'","part"); class="btn_del btn-del">삭제</a></span></li>';
                        });
                    }
                    if (output_recent) {
                    	$('.auto_area_recent').html(output_recent);
                    }
                    $('.no-keyword').css('display','none');
				}
            }
        });
    });
    function delete_keyword(key,type) {
        if(type == 'all'){
            var url = '/diquest/search.php?act=all_delete_recent_keyword';
        }else{
            var url = '/diquest/search.php?act=delete_recent_keyword&recent_keyword_key='+key;
        }

        $.ajax({
            url : url,
            type : 'GET',
            datatype : 'html',
            async : false,
            error : function(data,error){
                alert(error);
            },
            success : function(data){
				if(type == 'all'){
					$('.auto_area_recent').find('li').remove();
                    $('.no-keyword').css('display','block');
				}else{
				    var li_id = '#id_recent_li'+key+'';
					$(li_id).remove();
                    if ($('.auto_area_recent li').length > 0) {
                        $('.no-keyword').css('display','none');
                    }
                    else {
                        $('.no-keyword').css('display','block');
                    }
				}
            }
        });
	}
	function fn_close() {
        $('.relation_search2').hide();
	}
    function CloseInterBanner() {
        if ( $('input[name="todayBanner"]').is(':checked') ) {
            var todayDate = new Date();
            todayDate.setDate(todayDate.getDate() + 1);
            document.cookie = 'SetCloseInterBanner' + "=" + escape('done') + "; path=/; expires=" + todayDate.toGMTString() + ";"
        }
    }
</script>
