				if($(this).attr('target')){
					that.pause();
				}
			});

			//flash视频流结束，播放下一个
			window.videoFocusStop = function(){
				Index.videoFocus.next();
			};
			
			this.getData(function(){
				var _curUser = that.box.find('li:eq(4)').addClass('on').data('user').split(','),
				_curUserInfo = that.parseUserInfo(_curUser);
			
				that.writeSwf(_curUserInfo);
				that.setUser(_curUser);
			});
		},
		writeSwf: function(_userInfo){
			var _focus = this;
			window.homeFocusReady = function(){
				_focus.focusSwf = swfobject.getObjectById('videoPlayer');
				if(_focus.spamUser){
					_focus.play(_focus.spamUser);
				}
			};
			swfobject.embedSWF(window.__PLAYER, 'videoPlayer', '100%', '100%', '10.1', '/apple/expressInstall.swf', {protocol:location.protocol+'//', ruid:_userInfo.uid, fileName:_userInfo.flv, isPhoneLive:_userInfo.isPhone, bg:_userInfo.bg, toRoomUrl:_userInfo.url, readyFun: 'homeFocusReady', chID:0}, {allowscriptaccess:'always', wmode:'opaque', bgcolor:'#333333'});
		},
		parseUserInfo: function(user){
			var _isPhone = user[8]>1 ? 1 : 0,
				_bg = user[9].replace('_s', '_b'),
				_url = user[6].indexOf('6.cn')>-1 ? user[6] : location.protocol+'//v.6.cn'+ user[6],
				_url = _url + ((window.location.hostname == 'www.6.cn' && jQuery.browser.msie) ? '?_tra_='+VIA_PAGEID+'-ipvafi4f' : '');

			return {
				uid: user[7],
				isPhone: _isPhone,
				bg: _bg,
				flv: user[2],
				url: _url
			}
		},
		setUser: function(_user){
			var _html = this.tplUser.replace(/\{rid\}/g, _user[0])
					.replace(/\{link\}/g, _user[6])
					.replace(/\{alias\}/g, _user[1])
					.replace('{pic}', _user[3])
					.replace('{count}', numF(_user[5]));

			this.boxUser.html(_html);
		},
		curUser:{},
		play: function(user){
			if(this.focusSwf){
				var _userInfo = this.parseUserInfo(user);
				this.focusSwf.JsStartLive(_userInfo.flv, _userInfo.isPhone, _userInfo.bg);
				this.focusSwf.setEnterRoomURL(_userInfo.url);
				this.setUser(user);
			}else{
				this.spamUser = user;
			}
		},
		pause: function(){	
			try{
				if(this.focusSwf){
					this.focusSwf.videoPause();
				}
			}catch(e){}
		},
		reset: function(){
			var _li = this.box.find('.video-item li.on').eq(0);
			_li.click();
		},
		next: function(){
			var _tm_dif = (new Date()).getTime() - this.tm;

			//超过20分钟请求新数据
			if(_tm_dif > 20*60*1000){ 
				this.getData();
			}else{
				var _list = this.box.find('.video-item'),
					_cur_li =  _list.find('li.on'),
					_next_li = _cur_li.next('li'),
					_first_li = _list.find('li:first');

				if(_next_li[0]){ //有下一个
					_next_li.click();
				}else{
					_first_li.click();
				}
			}
		},
		getData: function(callback){
			var that = this;
			this.tm = (new Date()).getTime();

			$.getJSON('/api/getIndexBigLiveListNew.php', function(obj){
				if(obj.flag == '001'){
					var _list = that.box.find('.video-item');
					if( $.type(callback) == 'function'){
						that.parseData(obj.content.list);
						callback();
					}else{
						that.parseData(obj.content.list, 1);
					}
					if(obj.content.chinesenewyear == 1){
						_list.addClass('chinesenewyear');
					}else{
						_list.removeClass('chinesenewyear');
					}
				}else{
					setTimeout(that.getData, 1000);
				}
			});
		},
		// isUpdate 中间更新列表用
		parseData: function(list, isUpdate){
			var that = this,
				_html = [];

			$.each(list, function(i, user){
				var _u = user.rid+','+user.alias+','+user.flvtitle+','+user.picuser
						+','+user.starttime+','+user.count+','+user.link+','+user.uid
						+','+user.recordtype+','+user.pic;

				_html.push(
					that.tpl.replace('{rank}', i+1)
						.replace('{rid}', user.rid)
						.replace(/\{alias\}/g, user.alias)
						.replace('{pic}', user.pic)
						.replace('{user}', _u)
				);
			});

			var _list_box = that.box.find('.video-item ul');
			_list_box.html(_html.join(''));
			if(isUpdate){
				_list_box.find('li:eq(4)').click();
			}
		}
	};


	if(isVerB){
		var _listBox = $('#videoFocus .video-item'),
			_tpl = _listBox.find('ul.template').html().replace('src3', 'src');

		$.getJSON('/api/index/pc.php', {act:'top', uuid:UUID, uid:_puser.uid||0}, function(data){
			if(data.flag=='001'){
				var _html = [];
				window.INDEX_VER = data.content.ver;
				$.each(data.content.data, function(i, user){
					var _u = user.rid+','+user.alias+','+user.flvtitle+','+user.picuser+','
							+user.starttime+','+user.count+','+user.link+','+user.recId;

					var _li = _tpl.replace('{rank}', i+1)
							.replace('{rid}', user.rid)
							.replace(/\{alias\}/g, user.alias)
							.replace('{pic}', user.pic)
							.replace('{user}', _u);

					_li = $(_li).attr('data-recid', user.recId);
					_html.push(_li.prop("outerHTML"));				
				});

				_listBox.find('ul.list').html(_html);
				_listBox.find('ul.list li').eq(4).addClass('on');

				_listBox.find('.text').css('display', 'block');
				_listBox.find('img').on('load', function(){
					$(this).css({
						opacity: 1,
						visibility: 'visible'
					});
				});			
			}else if(data.flag=='403'){
				/* */
				location.href = location.href;
			}else{
				console.log(data.content);

				/* */
				_listBox.find('.text').css('display', 'block');
				_listBox.find('img').css({
					opacity: 1,
					visibility: 'visible'
				});
			}

			//videoDef();
			VideoFocus.init();
		});
	}else{
		//videoDef();
		VideoFocus.init();
	}

})
