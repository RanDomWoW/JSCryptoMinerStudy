                        if ($j(dateCheckInBtnSelector).hasClass('book_cal_button_on')) {
                            $j(dateCheckInBtnSelector).toggleClass('book_cal_button_on');
                            $j(dateCheckInBtnSelector).toggleClass('book_cal_button');
                            $j('#dateCheckInButtonArrow').attr('src', '../../../assets/images/datepicker/cal-arrow-orange.png');
                        }
                    },
                    onSelect: function (dateText, inst) {
                        $j(dateMonthCheckOutBtnSelector).text($j.format.date(new Date(dateText), "MMM"));
                        $j(dateDayCheckOutBtnSelector).text($j.format.date(new Date(dateText), "d"));
                        calendarState.departing.isOpen = false;

                        $j(dateCheckOutBtnSelector).toggleClass('book_cal_button_on');
                        $j(dateCheckOutBtnSelector).toggleClass('book_cal_button');
                    }
                });

                offerCodeSwap = $j(offerCodeSelector).val();
                //$j(offerCodeSelector).datepicker('setDate', $j(checkOutDateSelector).val());
                $j(offerCodeSelector).datepicker('show');

                calendarState.departing.isOpen = true;
            } else {
                calendarState.departing.isOpen = false;
            }

            $j(dateCheckOutBtnSelector).toggleClass('book_cal_button_on');
            $j(dateCheckOutBtnSelector).toggleClass('book_cal_button');

            if ($j(dateCheckOutBtnSelector).hasClass('book_cal_button_on')) {
                $j('#dateCheckOutButtonArrow').attr('src', '../../../assets/images/datepicker/cal-arrow-white.png');
            } else {
                $j('#dateCheckOutButtonArrow').attr('src', '../../../assets/images/datepicker/cal-arrow-orange.png');
            }

            if ($j(dateCheckInBtnSelector).hasClass('book_cal_button_on')) {
                $j(dateCheckInBtnSelector).toggleClass('book_cal_button_on');
                $j(dateCheckInBtnSelector).toggleClass('book_cal_button');
                $j('#dateCheckInButtonArrow').attr('src', '../../../assets/images/datepicker/cal-arrow-orange.png');
            }
        });

