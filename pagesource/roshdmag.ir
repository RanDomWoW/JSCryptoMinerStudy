        function ShowLayer(overObject, targetPanelId) {
            txtSelectedLayer.val(targetPanelId);
            txtSelectedLink.val(overObject.id);

            var panelsIds = new Array('ctl00_cphHeaderBottom_Sampa_Web_View_StoreUI_StoreTopBarControl02cphHeaderBottom_3777_pnlLogin', 'ctl00_cphHeaderBottom_Sampa_Web_View_StoreUI_StoreTopBarControl02cphHeaderBottom_3777_pnlCart', 'ctl00_cphHeaderBottom_Sampa_Web_View_StoreUI_StoreTopBarControl02cphHeaderBottom_3777_pnlFavorites');
