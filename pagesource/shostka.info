<script type="text/javascript">
    jQuery(document).ready(function(){
        //time
        var curDateJS = jQuery('#curDateJS');
        var now = new Date();
        var currentMonth = now.getMonth();
        var currentDay = now.getDay();
          switch (currentMonth+1){        
            case 1: currentMonth='января'; break;
            case 2: currentMonth='февраля'; break;
            case 3: currentMonth='марта'; break;
            case 4: currentMonth='апреля'; break;
            case 5: currentMonth='мая'; break;
            case 6: currentMonth='июня'; break;
            case 7: currentMonth='июля'; break;
            case 8: currentMonth='августа'; break;
            case 9: currentMonth='сентября'; break;
            case 10: currentMonth='октября'; break;
            case 11: currentMonth='ноября'; break;
            case 12: currentMonth='декабря'; break; 
        }
         switch (currentDay){        
            case 0: currentDay='Воскресенье'; break;
            case 1: currentDay='Понедельник'; break;
            case 2: currentDay='Вторник'; break;
            case 3: currentDay='Среда'; break;
            case 4: currentDay='Четверг'; break;
            case 5: currentDay="Пятница"; break;
            case 6: currentDay='Суббота'; break;
        }
        curDateJS.html(currentDay+', '+now.getDate()+' '+currentMonth+', '+now.getHours()+':'+now.getMinutes());
        
        //toggle footer
       var f_btn = jQuery("#f-btn");
       var footer = jQuery("footer");
       f_btn.on( "click", function() {
           if (footer.hasClass('f-down')) footer.removeClass('f-down'); else footer.addClass('f-down');
           if(!footer.hasClass('acposts')){
               jQuery.ajax({
                                        url: 'http://shostka.info/wp-content/themes/PortalTown/other/ajax-calendar.php',
                                        type: 'POST',
                                        beforeSend: function(){
                                            jQuery('body').addClass('sent-r');
                                        },
                                        success: function(html){                                        
                                            jQuery('.f-archive').html(html);
                                            footer.addClass('acposts');
                                            jQuery('body').removeClass('sent-r');
                                        },
                                        error: function(){
                                            jQuery('body').removeClass('sent-r');
                                        }
                                    });
      };});
    });
</script>
