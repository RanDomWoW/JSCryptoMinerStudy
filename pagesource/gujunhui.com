		<script>
			$(function(){
				function imgs (){
					var pmW = $(window).width();
					if ( pmW <= 991 ){
						$('.header .right .search').addClass('ydd');
						$('.header2 .right .search').addClass('ydd');
					 	$('.header .right .search.ydd').hide();
					 	$('.header2 .right .search.ydd').hide();
					 	$('body').removeClass('hover');
					}else{
						$('.header .right .search').removeClass('ydd');
						$('.header2 .right .search').removeClass('ydd');
					 	$('.header .right .search').show();
					 	$('.header2 .right .search').show();
					 	$('body').addClass('hover');
					}
				}
				imgs();	
				$(window).resize(function(){
					imgs();	
				})
				$('.header .search-btn').click(function(){
					if( $('.header .right .search').is(':hidden') ){
						$('.header .right .search').show();
					}else{
						$('.header .right .search').hide();
					}
				})
				$('.header2 .search-btn').click(function(){
					if( $('.header2 .right .search').is(':hidden') ){
						$('.header2 .right .search').show();
					}else{
						$('.header2 .right .search').hide();
					}
				})
				$('.menu-btn').click(function(event){
					if( $('.menu-box').is(':hidden') ){
						$('.menu-btn').addClass('active');
						$('.menu-box').fadeIn();
						$('body').addClass('active');
						$('.ydd-btn').addClass('active');
					}else{
						$('.menu-btn').removeClass('active');
						$('.menu-box').fadeOut();
						$('body').removeClass('active');
						$('.ydd-btn').removeClass('active');
					}
					event.stopPropagation();
				});
				$('.menu-box .menu ul li span b').click(function(){
					if( $(this).parents('li').next('.ul').is(':hidden') ){
						$(this).parents('li').next('.ul').show();
						$(this).html('-');
					}else{
						$(this).parents('li').next('.ul').hide();
						$(this).html('+');
					}
				});
				$('.menu-box .menu').click(function(event){
					$('.menu-box').show();
					event.stopPropagation();
				});
				$(window).click(function(){
					$('.menu-box').fadeOut();
					$('.menu-btn').removeClass('active');
					$('body').removeClass('active');
					$('.menu-box .menu .ul').hide();
					$('.ydd-btn').removeClass('active');
					$('.menu-box .menu ul li span b').html('+');
				});
				$('.header .menu ul li').hover(function(){
					$(this).find('.ul').slideDown();
				},function(){
					$(this).find('.ul').stop().slideUp();
				})
				$('.header2 .menu ul li').hover(function(){
					$(this).find('.ul').slideDown();
				},function(){
					$(this).find('.ul').stop().slideUp();
				})
				$('.header .right .weixin img').eq(0).click(function(){
					if( !$('body').hasClass('hover') ){
						if( $('.header .right .weixin .ewms').is(':hidden') ){
							$('.header .right .weixin .ewms').show();
						}else{
							$('.header .right .weixin .ewms').hide();
						}
					}
				})
				$('.header .right .weixin img').eq(0).hover(function(){
					if( $('body').hasClass('hover') ){
						$('.header .right .weixin .ewms').show();
					}
				},function(){
					if( $('body').hasClass('hover') ){
						$('.header .right .weixin .ewms').hide();
					}
				})
				$('.header2 .right .weixin img').eq(0).click(function(){
					if( !$('body').hasClass('hover') ){
						if( $('.header2 .right .weixin .ewms').is(':hidden') ){
							$('.header2 .right .weixin .ewms').show();
						}else{
							$('.header2 .right .weixin .ewms').hide();
						}
					}
				})
				$('.header2 .right .weixin img').eq(0).hover(function(){
					if( $('body').hasClass('hover') ){
						$('.header2 .right .weixin .ewms').show();
					}
				},function(){
					if( $('body').hasClass('hover') ){
						$('.header2 .right .weixin .ewms').hide();
					}
				})
			})
