    function enter(event) {     
         $('#menu').data({closing: false}).stop(true, false).animate({
            top: '0',
            opacity: 1
        },{
            duration: 600  
        });
    };
    function leave(event) { 
        $('#menu').data({closing: true}).delay(400).animate({
            top: '-'+$loginht+'px',
            opacity: 0
        },{
            duration: 600
        });
    };
    
    $('#menu').hover(
        function (){ 
            if ($(this).data('closing')) {
                $(this).stop(true, true);
            }
        },
        leave          
    );	
	});
