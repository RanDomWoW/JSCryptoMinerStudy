        function md5auth(seed) {
            var username = document.getElementById("txtusrnam").value;
            var rexp = /^\w+$/;
            if (username.length < 5 || username.length > 20) {
                alert("Please enter valid username or password");
                document.getElementById("txtusrnam").focus();
                return (false);
            }
            if (document.getElementById("txtCaptcha").value == "") {
                alert("Please enter correct code");
                document.getElementById("txtusrnam").focus();
                return (false);
            }
            if (username.search(rexp) == -1) {
                alert("Please enter valid username or password");
                document.getElementById("txtusrnam").focus();
                return (false);
            }
            var password = document.getElementById("txtpwd").value;

            if (password.length == 0 || password.length < 5 || password.length > 20) {
                document.getElementById("txtpwd").focus();
                alert("Please enter valid username or password");
                return (false);
            }

            //var pass1=/(?=^.{5,20}$)(?=.*\d)(?=.*[\!\@\#\$\*])((?=.*[A-Z])|(?=.*[a-z])).*$/;
            var pass1 = /(?=^.{5,20}$)(?=.*\d)((?=.*[A-Z])|(?=.*[a-z])).*$/;
            //var password = document.forms[0].txtPassword.value;
            if (password.search(pass1) == -1) {
                alert("Password must contain one letter and one digit in any order and 5-20 character");
                document.getElementById("txtpwd").focus();
                return (false);
            }

            var hash = hex_md5(seed + hex_md5(password));
            //alert(hex_md5(password));


            //document.getElementById("txtusrnam").value=hex_md5(password);
            document.getElementById("txtpwd").value = hash;
            //alert("txtpwd").value);
            // document.forms[0].txtpwd.value=hash; 
            return true;
            //return hash;

        }
        function clickButton(e, buttonid) {
            var evt = e ? e : window.event;
            var bt = document.getElementById("btnlogin");
            if (bt) {
                if (evt.keyCode == 13) {
                    bt.click();
                    return false;
                }
            }
        }
    </script>

    
