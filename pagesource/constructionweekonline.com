	}else{
		playPauseImg.alt = "Play";
		playPauseImg.src="/pictures/furniture/btn_gray_play.gif";
		//document.getElementById("Controller").innerHTML = "Play";
	}
}
window.onload = function() {textticker(0);}
</script>
