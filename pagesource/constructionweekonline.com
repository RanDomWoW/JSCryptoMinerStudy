	char_pos = 0;
	textticker(index);
	playPauseImg.alt = "Pause";
	playPauseImg.src="/pictures/furniture/btn_gray_pause.gif";
}

function stopScroling(){
	playPauseImg = document.getElementById("Controller").childNodes[0];
	
	clearTimeout(t2);clearTimeout(t1);
	var tickcontent = document.getElementById("ticktext");
	tickcontent.innerHTML = ti[index]+"_";
