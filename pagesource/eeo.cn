				useStateClassSkin: true,
				autoBlur: false,
				smoothPlayBar: true,
				keyEnabled: true,
				remainingDuration: false,
				toggleDuration: true
			});
			function jPlayerFn(videoSrc) {
				page2VideoPlayed=true;
				//jplayer
				$("#jquery_jplayer_1").jPlayer("setMedia", {
			      m4v: videoSrc
			    });
			    $("#jquery_jplayer_1").jPlayer("play");
			}

			//var videoSrcArr = ['movie.mp4', 'mov_bbb.mp4','movie.mp4','mov_bbb.mp4','movie.mp4'];


			var $popBoxCover=$(".popBoxCover");

			$('body').on('click', '.img-slider .videoPopUpBox', function(){

				//显示背景层
				$popBoxCover.css("display","block");

				var $videoBox = $(".videoBoxDiv");

				var bannerImgRect=this.getBoundingClientRect();
				var playerWidth1=bannerImgRect.right-bannerImgRect.left;
				var playerHeigth1=bannerImgRect.bottom-bannerImgRect.top;
				var playerMarginLeft1=-playerWidth1/2;
				var playerTop1=bannerImgRect.top;
				var palyerMarginTop1=0;

				var playerWidth2=1031;
				var playerHeight2=570;
				var playerMarginLeft2=-playerWidth2/2;
				var playerTop2='50%';
				var playerMarginTop2=-playerHeight2/2;


				$videoBox.removeClass('hide').css({
					width:playerWidth1,
					height:playerHeigth1,
					left:'50%',
					top:playerTop1,
					'margin-left':playerMarginLeft1,
					'margin-top':palyerMarginTop1
				}).show().animate({
					width:playerWidth2,
					height:playerHeight2,
					left:'50%',
					top:playerTop2,
					'margin-left':playerMarginLeft2,
					'margin-top':playerMarginTop2
				},300);
				var videoSrc=$(this).attr('data-videosrc');
				videoSrc && jPlayerFn(videoSrc);

			});
			function videoPlayerClose(){
				$("#jquery_jplayer_1").jPlayer("stop");
				$(".videoBoxDiv").addClass('hide').removeAttr('style');
				//关闭遮罩层
				$popBoxCover.css("display","none");

				page2VideoPlayed=false;
			}
			$(".videoClose").click(function(){
				videoPlayerClose();
			});

		});
	});
</script>
