	    //向右滑动
		var idx = 0;
		$(".agent_next").click(function() {
			idx++;
			var len = $(".bd ul li").length;
			if(idx + 3 > len) {
				$(".bd ul.img-slider").stop().append($("ul.img-slider").html());
			}
			$(".bd ul.img-slider").stop().animate({
				left: -idx * 362
			}, 300);
		});

		//左滑动
		$(".agent_prev").click(function() {
			if(idx == 0) {
				$("ul.img-slider").prepend($("ul.img-slider").html());
				$("ul.img-slider").css("left", "-800px");
				idx = 4
			}
			idx--;
			$(".bd ul.img-slider").stop().animate({
				left: -idx * 362
			}, 300);
		});

    //视频
	requireLoad.ready(function(){
		var page2VideoPlayed=false;
		require(['js/jplayer/jquery.jplayer.js'], function() {
			$("#jquery_jplayer_1").jPlayer({
				ready: function () {
					$("#jquery_jplayer_1").jPlayer("setMedia", {
				      m4v: ""
				    });
				    /*$("#jquery_jplayer_1").jPlayer("play");*/
