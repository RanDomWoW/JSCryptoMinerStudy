<script type="text/javascript" src="js/colorbox/jquery.colorbox.js"></script>
<link rel="stylesheet" type="text/css" href="js/colorbox/colorbox.css" />
<script>
$(document).ready(function(){

	$(".inline").colorbox({inline:true, width:"440px"});
	$(".inline2").colorbox({inline:true, width:"800px"});
	
});

$('#group2').hover(function(){
			$('#group1').removeClass("active");
			$(this).addClass("active");
			$('.group1').hide();
			$('.group2').show();
	})
$('#group1').hover(function(){
			$('#group2').removeClass("active");
			$(this).addClass("active");
			$('.group2').hide();
			$('.group1').show();
	})	
	
	
	
$(".ad_types_css a").click(function(){
	//	alert($(this).attr('id'));
	$(".ad_types_css a").removeClass("active");
	$(this).addClass("active");
 	$(".ad_type_detail div").css("display","none");
	$("."+$(this).attr('id')).css("display","block");
	


})	
	

</script>



