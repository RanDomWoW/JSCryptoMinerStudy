</script> <script>
        $(function() {
            $('#userLabel').on('click', function() {
                $(this).parents('.dropdown').toggleClass('open');

                return false;
            });

            $(document).on('click', function(e){
                var $target = $(e.target);
                if(!$target.hasClass('no-close-dropdown') && !$target.parents('.no-close-dropdown').length){
                    var $noClose = '';
                    if($target.parents('.dropdown').length)
                        $noClose = ':not(#' + $target.parents('.dropdown').attr('id') + ')';

                    $('.dropdown.open' + $noClose).removeClass('open');
                }
            });

            $('.close-dropdown').on('click', function() {
                $(this).parents('.dropdown.open').removeClass('open');

                return false;
            });

            $('.selectpicker').selectpicker();

            $('#backorderCreditsSelector')
                .on('change', function() {
                    var $this = $(this),
                        value = $this.val();

                    $this.siblings('.bootstrap-select.open').removeClass('open');

                    $('#backorderCredits').html(value);
                    $('#adjustedBackorderCreditsPrice').html(Math.round(100 * value * 0) / 100);
                })
                .trigger('change');
        });
    </script>
