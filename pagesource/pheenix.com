<script>
    $(function() {
        $('.jsLanguageLink').on('click', function() {
            var languageId = $(this).data('id'),
                prevLanguageId = $.cookie('languageId');

            $.cookie('languageId', languageId, {
                expires: 365,
                path: '/'
            });

            if (languageId != prevLanguageId) {
                location.reload();
            }
        });

