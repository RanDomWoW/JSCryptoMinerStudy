<script language="javascript" type="text/javascript">

function onSetLayout(){
	var idIndexleftHeight = (document.getElementById('idIndexleft').offsetHeight);
	var idIndexrightHeight = (document.getElementById('idIndexright').offsetHeight);
	var height = 0;
	
	var idWhosonlineHeight = document.getElementById('idWhosonline').offsetHeight;
	var idLoginHeight = document.getElementById('idLogin').offsetHeight;
	var idBodymenuHeight = document.getElementById('idBody-menu').offsetHeight;
	var idLeftHeight = document.getElementById('idLeft').offsetHeight;
	var idLoginHeight = document.getElementById('idLogin').offsetHeight;
	
	var idIndexrow1 = document.getElementById('idIndexrow1');
	var idIndexrow2 = document.getElementById('idIndexrow2');
	var idIndexright = document.getElementById('idIndexright');
	var idIndexleft = document.getElementById('idIndexleft');
	var idCenter = document.getElementById('idCenter');
	var idLeft = document.getElementById('idLeft');
	var idMenu = document.getElementById('idMenu');
	var idLogin = document.getElementById('idLogin');
	var idWhosonline = document.getElementById('idWhosonline');
	var idRight = document.getElementById('idRight');
	var idIndex = document.getElementById('idIndex');
	
	if(document.getElementById('idFlashbanner').offsetHeight == 0){
		document.getElementById('idDivFlash').style.display = 'none';
	}
	
	if(idBodymenuHeight == 0 || idMenu.offsetHeight == 0){
		idMenu.style.display = "none";
	}
	
	if(idLogin.offsetHeight == 0){
		idLogin.style.display = "none";
	}
	
	if(idWhosonline.offsetHeight == 0){
		idWhosonline.style.display = "none";
	}
	
	if(idRight.offsetHeight == 0){
		idRight.style.display = "none";
	}
	
	if(idIndex){
		var tagH3 = idIndex.getElementsByTagName('H3');
		if(tagH3.length > 0){
			for(i=0;i<tagH3.length;i++){
				tagH3.item(i).innerHTML = "<span class='headIndex'>"+tagH3.item(i).innerHTML+"</span>";
			}
		}
	}
	if(idRight.offsetHeight == 0 && idWhosonline.offsetHeight == 0 && idLogin.offsetHeight == 0 && idBodymenuHeight == 0){
		idIndexright.style.display = "none";
		idIndexrow1.style.width = "960px";
		idCenter.style.width = "780px";
		idIndexleft.style.width= "100%";
		if(idLeftHeight == 0){
			idLeft.style.display = "none";
			idCenter.style.padding = "0";
			idCenter.style.width = "100%";
		}
	}
	else{
		if(idLeftHeight <=25){
			idLeft.style.display = "none";
			idCenter.style.padding = "0";
			idCenter.style.width = "100%";
		}
	}
	
	height = (idIndexrow1.offsetHeight) + (idIndexrow2.offsetHeight);
	
	height = height < 1000 ? 1000 : height;
	
	document.getElementById('idIndex').style.height = height+"px";
}

setTimeout("onSetLayout()",1000);
</script>

