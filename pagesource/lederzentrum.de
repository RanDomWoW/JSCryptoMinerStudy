	var height = (windowWidth/4.266667)-100;
    var unitHeight = parseInt(height) + 'px';
		
	    jQuery('.homepage-hero-module').css('height',unitHeight);
		
		jQuery('.homepage-hero-module .poster').css('height',unitHeight);
	
	    jQuery('.avideo-container .poster img').css("height",parseInt(height)-35+"px");
		jQuery('.avideo-container .poster img').css("width","auto");
		jQuery('.avideo-container .poster img').css("margin","15px 0");
		
		
		if ( windowWidth < 700 ){
			
				var halfwidth = (jQuery('.page').width()/2);
				var halfimgwidth = (jQuery('.avideo-container .poster img').height()/1.4);
				var posleft = halfwidth-halfimgwidth;
				jQuery('.avideo-container .poster img').css("left",posleft+"px");	
				
		} else { 	
		
				var halfwidth = (jQuery('.page').width()/2);
				var halfimgwidth = (jQuery('.avideo-container .poster img').height()/1.4);
				var posleft = halfwidth-halfimgwidth;
				jQuery('.avideo-container .poster img').css("left",posleft+"px");	
				
		}

		
}

jQuery( document ).ready(function() {

			
