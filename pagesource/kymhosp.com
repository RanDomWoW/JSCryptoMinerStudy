<script type="text/javascript">
	$(document).ready(function(){
		//공백제거
		function compactTrim(str){
			return str.replace(/(\s*)/g, "");
		}

		//ESC
		$(document).keydown(function(event){
			if(event.which=='27'){
				$("#layer_background_area").fadeOut(200);
				$("#loadingDiv_iframe").attr("src","");
				$("#loadingDiv").fadeOut(400);
			}
		});
		//레이어닫기
		$(".close_layer_btn").click(function(){
			$("#layer_background_area").fadeOut(200);
			$("#loadingDiv_iframe").attr("src","");
			$("#loadingDiv").fadeOut(400);
		})



		//강의보기
		$("[id^='info_view_']").click(function(){
			var no = $(this).attr("id").split("_")[2];
			var url = $(this).attr("attr");
			
			if(url=="http://"){
				alert("준비중입니다");
				return false;
			}else{
				var width = $(window).width();
				var height = $(window).height();
				var divTop = document.documentElement.scrollTop+document.body.scrollTop+150; //상단 좌표
				$("#layer_background_area").css("width",width);
				$("#layer_background_area").css("height",height*3);
				$("#loadingDiv_iframe").attr("src",url);
				$("#layer_background_area").fadeTo(300,0.3);
					
					
				var loadingDivObj = $("#loadingDiv");
				loadingDivObj.css("top", divTop);
				if (width<=1010){
					loadingDivObj.css("left","0");
				}else if(width<=1300){
					loadingDivObj.css("left",$(window).width()/9);
				}else{
				loadingDivObj.css("left",$(window).width()/4);
				}
				loadingDivObj.fadeIn(500);

			}

		})


	})
</script>
