<script type="text/javascript">
    jQuery(document).ready(function () {
        /*
			Função leitor de cookies
        */

        function cookieForHeader(cb_enabled) {
            var name = cb_enabled + "=";
            var decodedCookie = decodeURIComponent(document.cookie);
            var ca = decodedCookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

        /*
			Fim - Função leitor de cookies
        */

        /*
			Funções assinar
        */

        //Quando clicar em assinar verifica se os termos de uso esta aceito
        //Se aceito - abre popup de redirectionamento
        //Se não - abre popover
        jQuery('.btn_assinar').click(function (event) {

            // Verifica se o checkbox esta assinado
            var check = jQuery(this).parent().find('.check_assinar').is(':checked');
            if (check) {
                //Fecha popover
                jQuery(this).parent().find('.popover__content').hide();

                //Abre popup redirecionamento
                //jQuery('.ll_overlay').fadeIn('fast');

                //Redirect
                //setTimeout(function(){
                //  window.location = link;
                //}, 1500);

            } else {
                //Cria popover de "Aceite os termos"
                jQuery(this).parent().find('.popover__content').attr('style', 'transform: translate(0,10px); background-color: #ff006a; padding: 1.5rem; box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.26); width: auto; z-index: 999999999; margin-bottom: 15px;').fadeIn('fast');

                setTimeout(function () {
                    jQuery(this).parent().find('.popover__content').fadeOut('fast');
                }, 5000);
            }


        });

        //Click no popover - esconde popover "Você precisa aceitar os termos"
        jQuery('.popover__content').click(function () {
            jQuery(this).fadeOut();
        });

        //Habilita os links de assinar (checkbox)
        jQuery(".check_assinar").change(function () {
            //var link = jQuery(this).attr('data-target');
            if (this.checked) {
                jQuery(this).parent().find('.btn_assinar').removeAttr('disabled');
            } else {
                jQuery(this).parent().find('.btn_assinar').attr('disabled', 'disabled');
            }
        });

        //Expandir tabela de preços
        jQuery('.expandir img').click(function () {
            jQuery('.tabela-de-precos').slideToggle();
            jQuery('.expandir img').toggle();
            window.location.href = "#price";
        });

        //Mostrar tabela de preços
        $('#abrirCompar').on('click', function () {
            $('#price').slideToggle('slow');
            $('#abrirCompar > i').toggleClass('fa-chevron-down');
        });

        //Másacara no campo de telefone da popup
        jQuery('input[name=phone]').mask('(00) 00000-0000');
		/*
			Fim - Funções assinar
        */


        //Cookiebar
        jQuery.cookieBar({
            message: 'O nosso website usa cookies para ajudar a melhorar a sua experiência de utilização. Ao utilizar o website, confirma que aceita a sua utilização. ',
            acceptButton: true,
            acceptText: 'Eu entendo',
            acceptFunction: null,
            declineButton: false,
            declineText: 'Desabilitar Cookies',
            declineFunction: null,
            policyButton: true,
            policyText: 'Política de privacidade',
            policyURL: 'http://cliente.leadlover.com/politicas-de-privacidade?_ga=1.261311437.1325221293.1480705520',
            autoEnable: true,
            acceptOnContinue: false,
            acceptOnScroll: false,
            acceptAnyClick: false,
            expireDays: 365,
            renewOnVisit: false,
            forceShow: false,
            effect: 'slide',
            element: 'body',
            append: false,
            fixed: true,
            bottom: true,
            zindex: '99999999',
        });

        /* Margin no site para o cookiebar
	    	Cookie cb-enabled
	    		Sem ativar 'enabled'
	    		Ativo 'accepted'
	    */
        var cookieBar = cookieForHeader('cb-enabled');

        if (cookieBar === 'enabled') {
            var top = jQuery('#cookie-bar').height();
            jQuery('body #wrap').css('margin-top', top);
        } else {
            jQuery('body #wrap').css('margin-top', '0');
        }

        //Quando clicar em concordo remove margin
        jQuery('.cb-enable').on('click', function () {
            jQuery('body #wrap').css('margin-top', '0');
        });
    });

</script>

