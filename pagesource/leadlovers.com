                        if (_form.find('#pid').length == 0) _form.prepend('<input type="hidden" id="pid" name="pid" value="' + _pageID + '" />'); else _form.find('#pid').val(_pageID);
                        // insere ou atualiza o id
                        if (_form.find('#id').length == 0) _form.prepend('<input type="hidden" id="id" name="id" value="' + _machineID + '" />'); else _form.find('#id').val(_machineID);
