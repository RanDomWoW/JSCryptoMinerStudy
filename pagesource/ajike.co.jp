<script>
	//output localstrage 画面ロード時に起動
	function output_count(){
		if(window.localStorage){
			var blog_count = 0;
			var str = localStorage.getItem("date_key");
			//日付データが無い時、過去1週間の記事数=3記事を返す。
            if(!str) {
                return 3;
            }
            else {
                return 0;
            }

			var today = JSON.parse(str);
			var key_date = new Date(today['accessDate']);
			window.console.log(key_date);

			var n = "2018/03/22 15:00:00,2018/03/09 09:15:00,2018/03/08 15:40:00";
			var ar = n.split(",");

			for (i=0; i < ar.length; i++) {
				var n_date = new Date(ar[i]);

				if (n_date >= key_date) {
					blog_count++;
				}
			}
        }
        else{
			//ローカルストレージに対応していない場合、文字出力しない。
			return null;
		}
		return blog_count;
	}

	//save localstrage ニュース記事閲覧時に起動
	function save_count(){
		var today = {accessDate: "2018/03/23 16:00:17"}
		var str = JSON.stringify(today);
		localStorage.setItem("date_key",str);
		//日付を更新し、カウントを消す
	}
</script>

