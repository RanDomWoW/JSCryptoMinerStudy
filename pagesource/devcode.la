<script>
		$("#linkModalLogin").on("click", function(e){
			e.preventDefault();
			$('#registroModal').modal("hide");
			$('#loginModal').modal({
				keyboard: true,
				show: true,
			});
		});
		$("#linkModalRegistro").on("click", function(e){
			e.preventDefault();
			$('#loginModal').modal("hide");
			$('#registroModal').modal({
				keyboard: true,
				show: true,
			});
		});
		$(".modal__close").on('click', function(e){
			e.preventDefault();
			$('#loginModal').modal("hide");
			$('#registroModal').modal("hide");
		});
	</script>
