/* Palette - Add to UI-Platform */
/* THEME */
/* For use with flip-container styles in commonMod */
/* mixin for multiline */
.progressiveHeroMod {
  min-height: 400px;
  text-align: center;
}
.progressiveHeroMod h1 {
  font-size: 72px;
  font-weight: 800;
  color: #ffffff;
  line-height: .95;
  margin-bottom: 26px;
  margin-top: 48px;
}
.progressiveHeroMod .ctaArea {
  margin: 21px 0 30px 0;
}
.progressiveHeroMod .textUnderButton {
  color: #ffffff;
  font-size: 16px;
  text-align: center;
  padding-top: 22px;
  font-weight: normal;
}
.progressiveHeroMod .textUnderButton h2, .progressiveHeroMod h3, .progressiveHeroMod h4, .progressiveHeroMod h5 {
  color: #ffffff;
  font-size: 16px;
  margin-bottom: 12px;
}
.progressiveHeroMod .textUnderButton h5 {
  margin-bottom: 8px;
}
.progressiveHeroMod .textUnderButton ul {
  margin-top: 0;
  margin-bottom: 0;
  list-style-position: inside;
  padding: 0;
}
.progressiveHeroMod .textUnderButton a {
  color: #ffffff;
  text-decoration: underline;
  font-size: 12px;
}
.progressiveHeroMod .btn-hero {
  font-size: 20px;
  padding: 17px 27px;
  max-width: 360px;
  box-shadow: 0 11px 11px rgba(0, 0, 0, 0.16), 0 2px 2px rgba(0, 0, 0, 0.23);
}
@media (max-width: 767px) {
  .progressiveHeroMod {
    min-height: auto;
  }
  .progressiveHeroMod h1 {
    font-size: 30px;
    line-height: 1.3;
    margin-bottom: 8px;
    margin-top: 32px;
  }
  .progressiveHeroMod .ctaArea {
    margin: 21px 0 40px 0;
  }
}




