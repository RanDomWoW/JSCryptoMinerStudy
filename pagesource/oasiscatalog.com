    $(document).on('keydown', function (event) {
        var sel;

        var $selection = {
            getText: function () {
                var text = '';
                if (typeof window.getSelection === 'function') {
                    // Not IE, используем метод getSelection
                    text = window.getSelection().toString();
                } else {
                    // IE, используем объект selection
                    text = document.selection.createRange().text;
                }
                return text;
            },
            getElem: function () {
                var elem;
                if (typeof window.getSelection === 'function') {
                    // Not IE, используем метод getSelection
                    elem = window.getSelection();
                } else {
                    // IE, используем объект selection
                    elem = document.selection;
                }
                return elem;
            }
        };

        if (
            (event.ctrlKey || event.metaKey) && // нажат ctrl
            ((event.keyCode === 0xA) || (event.keyCode === 0xD)) && // нажат enter
            ($selection.getText().trim()) // и не пробелы
        ) {
            $('#mistakeModal').modal('show');
            $('#textmistake').val($selection.getText()); // передача в инпут модалки
            $('.text-mistake').html($selection.getText()); //передача в поле модалки
        }
    });
    $('#mistake-form').on('click', '.send-form', function(e){
        e.preventDefault();
        var form = $('#mistake-form');
        $.ajax({
            type: 'POST',
            url: form.attr('action'),
            data: form.serialize(),
            beforeSend: function () {
                form.parents('.modal-content').append('<div class="preloader"></div>');
                $(this).attr('disabled', 'disabled');
            },
            success: function (data) {
                $.informer(data.message, data.status);
            },
            complete: function () {
                form.parents('.modal-content').find('.preloader').remove();
                $('#mistakeModal').modal('hide');
            }
        });
    });


