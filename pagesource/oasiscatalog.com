        $(document).on('submit', '.modal-subscribe__form', function (e) {
            e.preventDefault();

            var data = $(this).serialize();
            var form = $(this);

            $.ajax({
                method: 'post',
                url: "/site/subscribe",
                data: data,
                success: function (data) {
                    if (data === 'success') {
                        $(form).addClass('hidden');
                        $('.modal-subscribe__success').removeClass('hidden');
                        $('.layout-fixed-panel').hide();
                    } else {
                        $(form).addClass('hidden');
                        $('.modal-subscribe__fail').removeClass('hidden');
                    }
                }
            });
        });
    

