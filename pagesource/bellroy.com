<script type="text/javascript">
(function(win, doc) {
  try {
    var $pageHeaderMenu = doc.querySelector('.pageHeaderMenu');
    var $pageHeaderMenuSpacingHelper = doc.querySelector('.pageHeaderMenu--spacing-helper');
    var $pageHeaderMenuItems = $pageHeaderMenu.querySelectorAll('.topLevel > ul > li > a');
    var $pageHeaderSubMenuItems = doc.querySelectorAll('.subMenu a, .mobileShopReel a');
    var $hamburgerBtn = doc.querySelector('.pageHeaderMenuPreHeader .hamburger');

    var forEachNode = function(nodeList, callback) {
      var i;
      for (i = 0; i < nodeList.length; i++) {
        callback(nodeList[i], i);
      }
    };

    var targetNodeType = function(target, type) {
      while(target.nodeName !== type.toUpperCase() && target.nodeName !== 'body') {
        target = target.parentNode;
      }
      return target;
    };

    var mutateClassDelayed = function(action, $target, className, timeout) {
      setTimeout(function() {
        $target.classList[action](className);
      }, timeout);
    };

    var mutateClass = function(action, $targets, className, timeout) {
      var i;
      for (i = 0; i < $targets.length; i += 1) {
        if (!timeout) {
          $targets[i].classList[action](className);
        } else {
          mutateClassDelayed(action, $targets[i], className, timeout);
        }
      }
    };

    var removeClass = function($targets, className, timeout = false) {
      mutateClass('remove', $targets, className, timeout);
    };

    var addClass = function($targets, className, timeout = false) {
      mutateClass('add', $targets, className, timeout);
    };

		var openMainItem = function($target) {
			var targetIsCurrentlyOpen = $target.classList.contains('open');
      var otherSubmenuIsCurrentlyOpen = (targetIsCurrentlyOpen || $pageHeaderMenu.classList.contains('submenu--open'));

      forEachNode($pageHeaderMenuItems, function($pageHeaderMenuItem) {
        removeClass([$pageHeaderMenuItem], 'open');
      });

      if (targetIsCurrentlyOpen) {
        removeClass([$pageHeaderMenu], 'submenu--open');
        removeClass([$target], 'open')
      } else {
        addClass([$pageHeaderMenu], 'submenu--open');
        addClass([$target], 'open');
      }
		};

    var onClickMainItem = function(event) {
      event.preventDefault();
      var $target = targetNodeType(event.target, 'a');
			openMainItem($target);      
      return false;
    };

    forEachNode($pageHeaderMenuItems, function($pageHeaderMenuItem) {
      $subMenu = $pageHeaderMenuItem.parentNode.querySelector('.subMenu');
      if ($subMenu) {
        addClass([$pageHeaderMenuItem.parentNode], 'has-submenu');
        $pageHeaderMenuItem.addEventListener('click', onClickMainItem);
      }
    });

		var openDrawer = function() {
			addClass([$hamburgerBtn], 'open');
			addClass([$pageHeaderMenu], 'drawer-active');
			addClass([$pageHeaderMenu], 'drawer-open', 16);
		};
		
		var closeDrawer = function() {
			removeClass([$hamburgerBtn], 'open');
      removeClass([$pageHeaderMenu], 'drawer-open',);
      removeClass([$pageHeaderMenu], 'drawer-active', 300);
		};

    $hamburgerBtn.addEventListener('click', function(event) {
      event.preventDefault();
      var isOpen = $hamburgerBtn.classList.contains('open');
      if (isOpen) {
        closeDrawer();
      } else {
        openDrawer();
      }
    });

    var setLineItemCount = function(amount) {
    	var $lineItemCounts = doc.querySelectorAll('.lineItemCount');
    	forEachNode($lineItemCounts, function($lineItemCount) {
    		var wrap = $lineItemCount.classList.contains('wrap');
    		if (amount === 0) {
	    		$lineItemCount.innerHTML = '';
    		} else {
	    		var str_amount = (wrap) ? '('+amount+')' : amount;
	    		$lineItemCount.innerHTML = str_amount;
    		}
    	});
    };

    var updateLineItemCount = function() {
    	var i;
    	var state = win.store__default.getState();
    	var cart = state.cart;

    	if (cart.lineItems === null || cart.lineItems.length === 0) {
    		setLineItemCount(0);
    		return;
    	}

    	var productCount = 0;
    	for (i = 0; i < cart.lineItems.length; i += 1) {
    		if (cart.lineItems[i].itemType !== 'sales_promotion') {
    			productCount += cart.lineItems[i].quantity;
    		}
  		}

    	setLineItemCount(productCount);
    };

    var openCart = function(event) {
    	win.store__default.dispatch({
    		type: 'CART_SHOW_MINICART',
    		event: event,
    		source: 'button',
    	});
    };

    var closeOverlay = function() {
      forEachNode(doc.querySelectorAll('.pageHeaderMenuOverlay'), function($pageHeaderMenuOverlay) {
        removeClass([$pageHeaderMenuOverlay], 'open');
      });
    };

    var openMenuOverlay = function(event, overlayId) {
      if (win.innerWidth >= 768) {
      	if (event) {
      		event.preventDefault();
      	}
        var $overlay = document.getElementById(overlayId);
        closeOverlay();
        addClass([$overlay], 'open');
        $overlay.querySelector('input[type="text"],input[type="email"]').focus();
        return false;
      }
      return true;
    };

    var openSearch = function(event = null) {
      return openMenuOverlay(event, 'search-menu-overlay');
    };

    var openNewsletter = function(event) {
      return openMenuOverlay(event, 'newsletter-menu-overlay');
    };

    doc.addEventListener('DOMContentLoaded', function() {
      if (typeof win.store__default !== 'undefined') {
    		win.store__default.subscribe(function() {
    			updateLineItemCount();
    		});
      }
    });

    forEachNode(doc.querySelectorAll('.js-open-cart'), function($cartButton) {
    	$cartButton.addEventListener('mouseenter', openCart);
    });

    forEachNode(doc.querySelectorAll('.js-open-search'), function($openSearchButton) {
    	$openSearchButton.addEventListener('click', openSearch);
    });

    forEachNode(doc.querySelectorAll('.js-open-newsletter'), function($openSearchButton) {
      $openSearchButton.addEventListener('click', openNewsletter);
    });

    forEachNode(doc.querySelectorAll('.js-close-overlay'), function($closeOverlayButton) {
    	$closeOverlayButton.addEventListener('click', closeOverlay);
    });

		var applySearchLineEffect = function($target) {
      var $searchInputWrapper = $target.querySelector('.searchInputWrapper');
      var $input = $searchInputWrapper.querySelector('input[type="text"]');
      var $searchUnderLine = $searchInputWrapper.querySelector('.searchInputWrapper__underline');
      
      var updateLine = function() {
        $searchUnderLine.innerHTML = $input.value;
      };
      $input.addEventListener('keypress', updateLine);
      $input.addEventListener('keyup', updateLine);
      $input.addEventListener('keydown', updateLine);
      updateLine();
    };
    
    applySearchLineEffect(doc.getElementById('search-menu-overlay'));

    doc.addEventListener('DOMContentLoaded', function() {
      if (typeof win.store__default === 'undefined') { return; }
      var state = win.store__default.getState();
      var shipping = state.shipping;
      var $img = document.createElement('img');
      $img.src= shipping.flag_icon;
      var $span = document.createElement('span');
      $span.innerHTML = shipping.phrases.shipping_notice_message;
      $shippingMessage = document.querySelector('.shippingMessage');
      $shippingMessage.appendChild($img);
      $shippingMessage.appendChild($span);
    });

    var makeSubMenuItemsActiveWhenStringMatchesHref = function(matchString) {
    	forEachNode($pageHeaderSubMenuItems, function($subMenuItem) {
				if (matchString === $subMenuItem.getAttribute('href')) {
					$subMenuItem.classList.add('active');
				} else {
					$subMenuItem.classList.remove('active');
				}
			});
    };
    
    forEachNode($pageHeaderSubMenuItems, function($subMenuItem) {
    	$subMenuItem.addEventListener('click', function() {
    		makeSubMenuItemsActiveWhenStringMatchesHref($subMenuItem.getAttribute('href'));
    	  closeDrawer();
    	});
    })
    
    doc.addEventListener('DOMContentLoaded', function() {
			var location = win.location.pathname;
			var search = win.location.search;

			if (location.match(/^\/products\/category/)) {
				openMainItem($pageHeaderMenu.querySelector('.menuitem-SHOP'));
				doc.querySelector('.mobileShopReel').classList.add('active');
			}

			if (location.match(/^\/search/)) {
				var $pageHeaderMenuSearchUnderlay = doc.querySelector('.pageHeaderMenuSearchUnderlay');
				var $searchInput = $pageHeaderMenuSearchUnderlay.querySelector('.searchInputWrapper input[name="q"]');
				var searchQuery = search.split("?")[1];
				if (searchQuery !== undefined) {
					$searchInput.value = search.split("?")[1].split("&")[0].split('=')[1].replace(/\+/g, ' ');
				}
				applySearchLineEffect($pageHeaderMenuSearchUnderlay);
				$pageHeaderMenuSearchUnderlay.setAttribute('style', '');
				
				var $searchLi = doc.querySelector('#PageMenuPreHeader a[href="/search"]').parentNode;
				$searchLi.parentNode.removeChild($searchLi);
			} else {
				var $pageHeaderMenuSearchUnderlay = doc.querySelector('.pageHeaderMenuSearchUnderlay');
				$pageHeaderMenuSearchUnderlay.parentNode.removeChild($pageHeaderMenuSearchUnderlay);
			}

			makeSubMenuItemsActiveWhenStringMatchesHref(location);
    });

	
    doc.addEventListener('keydown', function(event) {
      if (event.keyCode === 27) {
        closeOverlay();
      }
    });

    doc.addEventListener('keydown', function(event) {
    	var searchIsClosed = (document.getElementById('search-menu-overlay').classList.contains('open') === false);
    	
      if (win.location.pathname !== '/search' && event.keyCode === 191 && searchIsClosed) {
      	event.preventDefault();
        openSearch();
      }
    });

    win.addEventListener('popstate', function() {
    	var location = win.location.pathname;
			makeSubMenuItemsActiveWhenStringMatchesHref(location);
    });
    
  } catch (e) { console.error(e); }
})(window, document);

</script>
