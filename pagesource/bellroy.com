	if (priority === null) { priority = 0 }
	if (style === null) { style = '' }
	if (priority < currentActiveBannerPriority) { return; }
	
	window.currentActiveBannerPriority = priority;
	
	var holder = document.getElementById('bannerHolder');
	holder.innerHTML = '';
	
	var banner = document.createElement('a');
	banner.href = href;
	banner.className = 'topBanner';
	banner.setAttribute('style', style);
	var bannerP = document.createElement('p');
	bannerP.innerHTML = title + '&nbsp;';
	if (cta) {
