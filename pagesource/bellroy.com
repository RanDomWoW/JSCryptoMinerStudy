<script type="text/javascript">
window.activeNewsletterSignupModal = null;

var closeNewsletterSignUpModal = function() {
	if (!window.activeNewsletterSignupModal) { return true; }
	var $modal = window.activeNewsletterSignupModal;
	$modal.classList.remove('newsletterSignUp--show');
	setTimeout(function() {
		if (!window.activeNewsletterSignupModal) { return true; }
		var $modal = window.activeNewsletterSignupModal;
	  $modal.parentNode.removeChild($modal);
	  window.activeNewsletterSignupModal = null;
	}, 400);
};

var openNewsletterSignUpModal = function(title, blurb, identifier) {
	closeNewsletterSignUpModal();
	
	var template = document.getElementById('templateNewsletterSignup').innerHTML;
	template = template.replace('||TITLE||', title);
	template = template.replace('||BLURB||', blurb);
	template = template.replace('||IDENTIFIER||', identifier);

	window.activeNewsletterSignupModal = document.createElement('span');
	window.activeNewsletterSignupModal.innerHTML = template;

	document.body.appendChild(window.activeNewsletterSignupModal);
	WebpackerReact.mount();

	window.activeNewsletterSignupModal.querySelector('.newsletterSignUp__backdrop')
		.addEventListener('click', closeNewsletterSignUpModal);
	window.activeNewsletterSignupModal.querySelector('.newsletterSignUp__modal__close')
		.addEventListener('click', closeNewsletterSignUpModal);

	window.activeNewsletterSignupModal.classList.add('newsletterSignUp--show');
};

var openComingSoonModal = function(target, identifier = 'newsletter') {
	var $productTile = target;	
	var title = $productTile.getAttribute('title');
	var blurb = $productTile.getAttribute('data-blurb');

	openNewsletterSignUpModal(title, blurb, identifier);
};

window.addEventListener('keydown', function(event) {
	if (event.keyCode === 27) {
		closeNewsletterSignUpModal();
	}
});
</script>

