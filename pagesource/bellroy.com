<style type="text/css">
.newsletterSignUp {
	display: block;
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	z-index: 1000;
}

.newsletterSignUp--show .newsletterSignUp__backdrop,
.newsletterSignUp--show .newsletterSignUp__modal {
	opacity: 1;	
}

.newsletterSignUp__backdrop {
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	z-index: 1;
	background-color: rgba(0,0,0,.4);
	opacity: 0;
	transition: opacity .2s ease;
}

.newsletterSignUp__modal {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
	z-index: 2;
	background-color: #dedede;
	color: #464646;
	text-align: center;
	padding: 20px;
	max-width: 80vw;
	max-height: 90vh;
	overflow: hidden;
	overflow-y: auto;
	width: 480px;
	opacity: 0;
	transition: opacity .4s ease;
}

@media (min-width: 768px) {
	.newsletterSignUp__modal {
		padding: 40px 80px 60px 80px;
	}
}
.newsletterSignUp__modal__title {
	font-family: 'Lato', sans-serif;
  -webkit-font-smoothing: antialiased;
  font-size: 14px;
  letter-spacing: 1px;
  font-weight: 600;
  color: #464646;
  text-transform: uppercase;
  line-height: 1.5;
	width: 160px;
  margin: 30px auto 10px auto;
	
}
@media (min-width: 768px) {
	.newsletterSignUp__modal__title {
  	line-height: 57px;
  	width: auto;
	}
}

.newsletterSignUp__modal__icon {
	margin: 20px auto -21px;
	width: 40px;
}

.newsletterSignUp__modal__blurb {
	font-family: 'PT serif', sans-serif;
  font-size: 14px;
  letter-spacing: .5px;
}

.newsletterSignUp__modal__close {
	position: absolute;
	top: 10px;
	right: 10px;
	height: 16px;
	width: 16px;
	cursor: pointer;
}

.newsletterSignUp__modal__close:before,
.newsletterSignUp__modal__close:after {
	display: block;
	content: ' ';
	border-left: 1px solid #464646;
	position: absolute;
	left: 50%;
	top: 0;
	height: 16px;
}

.newsletterSignUp__modal__close:before {
	transform: rotate(45deg);
}

.newsletterSignUp__modal__close:after {
	transform: rotate(-45deg);
}

.newsletterSignUpSignupComponent {
	margin: 0 0 45px 0;
}

.newsletterSignUpSignupComponent form {
	margin: 20px auto;
}
</style>
