$(document).ready(function(){  
  var showChar = 138;  // How many characters are shown by default
    var ellipsestext = " ...";
    var moretext = "Show more >";
    var lesstext = "Show less";
    $('.collection_description').each(function() {
        var content = $(this).html();
        if(content.length > showChar) {
            var c = content.substr(0, showChar);
            var h = content.substr(showChar, content.length - showChar);
            var html = c + '<span class="moreellipses">' + ellipsestext+ '&nbsp;</span></p><span class="morecontent"><span>' + h + '</span><a href="" class="morelink">' + moretext + '</a></span>';
            $(this).html(html);
          $(this).find('p:first-child').css('display', 'inline');
        }
    });
    $(".morelink").click(function(){
        if($(this).hasClass("less")) {
            $(this).removeClass("less");
            $(this).html(moretext);
        } else {
            $(this).addClass("less");
            $(this).html(lesstext);
        }
        $(this).parent().prev().find('.moreellipses').toggle();
        $(this).prev().toggle();
        return false;
    });
});
