<script type='text/javascript'>
function updateNavbarCustomerMenu(loggedIn) {
  var menu = $("#navbar-customer-menu");
  if (loggedIn) {
    menu.html(
      '<li class="menu-item">' +
        '<a href="/dashboard/">Dashboard</a>' +
      '</li>' +
      '<li class="menu-item">' +
        '<a href="/logout/">Sign Out</a>' +
      '</li>'
    );
  } else {
    menu.html(
      '<li class="menu-item">' +
        '<a href="/login/">Login</a>' +
      '</li>' +
      '<li class="menu-item">' +
        '<a href="/login/">Sign Up</a>' +
      '</li>'
    );
  }
}

function updateVerticalMenu(loggedIn, elId, isMobile) {
  var menu = $("#" + elId);
  if (loggedIn) {
    menu.removeClass();
    if (isMobile) {
      menu.addClass('indented-parent-menu');
    } else {
      menu.addClass('parent-menu');
    }
    menu.addClass('menu-item menu-item-type-custom menu-item-object-custom ' +
      'menu-item-has-children parent');
    menu.html(
      '<a id="' + elId + '-opener" href="#">' +
        '<span class="menu-item-text">My Account</span>' +
      '</a>' +
      '<ul id="' + elId + '-submenu" class="sub-menu">' +
        '<li class="menu-item menu-item-type-custom menu-item-object-custom">' +
          '<a href="/dashboard/">Orders</a>' +
        '</li>' +
        '<li class="menu-item menu-item-type-custom menu-item-object-custom">' +
          '<a href="/dashboard/referral/">Give $25 Get $25</a>' +
        '</li>' +
        '<li class="menu-item menu-item-type-custom menu-item-object-custom">' +
          '<a href="/dashboard/products/">Products</a>' +
        '</li>' +
        '<li class="menu-item menu-item-type-custom menu-item-object-custom">' +
          '<a href="/dashboard/cashout/">Balance / Withdraw</a>' +
        '</li>' +
        '<li class="menu-item menu-item-type-custom menu-item-object-custom">' +
          '<a href="/dashboard/account-details/">Account Details</a>' +
        '</li>' +
        '<li class="menu-item menu-item-type-custom menu-item-object-custom">' +
          '<a href="/dashboard/change-password/">Change Password</a>' +
        '</li>' +
        '<li class="menu-item menu-item-type-custom menu-item-object-custom">' +
          '<a href="/logout/">Sign Out</a>' +
        '</li>' +
      '</ul>'
    );

    $("#" + elId + "-opener").click(function(e) {
      if (menu.hasClass('sub-menu-open')) {
        menu.removeClass('sub-menu-open');
        $("#" + elId + "-submenu").slideUp();
        e.preventDefault();
      } else {
        menu.addClass('sub-menu-open');
        $("#" + elId + "-submenu").slideDown();
        e.preventDefault();
      }
    });
  } else {
    menu.removeClass();
    menu.addClass("menu-item menu-item-type-custom menu-item-object-custom");
    menu.html(
      '<a href="/login/">' +
        '<span class="menu-item-text">Login / Sign Up</span>' +
      '</a>'
    );
  }
}

function updateCartItemCounts(numItems) {
  $(".cart-num-items-container").each(function() {
    $(this).text(numItems);
  });
}

function getItemUrl(perma) {
  return FS_BASE_URL + '/' + perma;
}

function updateCart(items, subtotal, numItems) {
  var container = $(".fs-cart-container");
  if (numItems === 0) {
    container.html(
      '<div class="shopping-bag">' +
        '<div class="bag-empty">Your cart is empty.</div>' +
      '</div'
    );
    return;
  }

  var itemsHtml = '';
  for (var label in items) {
    if (!items.hasOwnProperty(label)) {
      continue;
    }

    var item = items[label];
    itemsHtml += '<div class="bag-product clearfix">' +
      '<figure>' +
        '<a class="bag-product-img" href="">' +
          '<img width="180" height="180" src="' + item.photo + '" ' +
            'class="attachment-shop_thumbnail size-shop_thumbnail ' +
              'wp-post-image">' +
        '</a>' +
      '</figure>' +
      '<div class="bag-product-details">' +
        '<div class="bag-product-title">' +
          '<a href="' + getItemUrl(item.perma) + '">' + item.name + '</a>' +
        '</div>' +
        '<div class="bag-product-price">' +
          'Unit Price:' +
          '<span class="woocommerce-Price-amount amount">' +
            '$' + item.price.toFixed(2) +
          '</span>' +
        '</div>' +
        '<div class="bag-product-quantity">' +
          'Quantity: <span class="quantity">' + item.quantity + '</span>' +
        '</div>' +
      '</div>' +
      '<a href="/cart/?remove=' + label + '" ' +
          'class="remove fs-remove-product" title="Remove this item">' +
        '&times;' +
      '</a>' +
    '</div>';
  }

  container.html(
    '<div class="shopping-bag">' +
      '<div class="bag-header">' +
        numItems + ' item in the cart' +
      '</div>' +
      '<div class="bag-contents">' + itemsHtml + '</div>' +
      '<div class="bag-total">' +
        '<span class="total-title">Total</span>' +
        '<span class="total-amount">' +
          '<span class="woocommerce-Price-amount amount">' +
            '$' + subtotal.toFixed(2) +
	        '</span>' +
	      '</span>' +
      '</div>' +
      '<div class="bag-buttons">' +
        '<a class="sf-button standard sf-icon-reveal bag-button" ' +
            'href="/cart/">' +
          '<i class="sf-icon-search-quickview"></i>' +
          '<span class="text">View cart</span>' +
        '</a>' +
        '<a class="sf-button standard sf-icon-reveal checkout-button" ' +
            'href="/checkout/">' +
          '<i class="fa-long-arrow-right"></i>' +
          '<span class="text">Checkout</span>' +
        '</a>' +
      '</div>' +
    '</div>'
  );
}

function showReferralBanner(show) {
  var banner = $("#sf-referral-banner");
  if (!banner) {
    return;
  }

  if (show) {
    banner.removeClass('hide');
    banner.addClass('referral-banner-block');
  } else {
    banner.addClass('hide');
    banner.removeClass('referral-banner-block');
  }
}

$(document).ready(function() {
  $.ajax({
    url: "/ajax/session/",
    method: "GET",
    dataType: "json",
    success: function(resp) {
      if (resp) {
        showReferralBanner(resp.show_referral_banner);
        updateNavbarCustomerMenu(resp.logged_in);
        updateVerticalMenu(resp.logged_in, 'vertical-customer-menu', false);
        updateVerticalMenu(resp.logged_in, 'vertical-customer-menu-mobile',
          true);
        updateCartItemCounts(resp.total_items);
        updateCart(resp.items, resp.subtotal, resp.total_items);
      }
    },
    error: function(resp) {
    }
  });
});

function copyToClipboard(element) {
    $(element).select();
    document.execCommand("copy");
}
