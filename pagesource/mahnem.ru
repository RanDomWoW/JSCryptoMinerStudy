<script type='text/javascript'><!--
$(document).ready(function(){

var tSelected = 'Выбрано: ';
var numCountries = 0;
var numTypes = 0;

$('#placefrom').autocomplete('https://www.mahnem.ru/?module=suggest_city',{
  highlight:false,
  minChars:2,
  scrollHeight:200,
  selectFirst:false
  });
// list of countries
$('#mtext').click(function(){
	$('html, body').animate({
		scrollTop: $('#fSearch').offset().top
		}, 500);
	$('#countrylist').fadeIn(200);
	});
// hide open dialogs
$(document).bind('mouseup touchend', function(e){
	$('#infFund').fadeOut(100);
	$('#infEnroute').fadeOut(100);
	var o = $('#mWhomMenu');
	if (!o.is(e.target) && o.has(e.target).length == 0)
		o.fadeOut(100);
	o = $('#mTravelMenu');
	if (!o.is(e.target) && o.has(e.target).length == 0)
		o.fadeOut(100);
	o = $('#countrylist');
	if (!o.is(e.target) && o.has(e.target).length == 0)
		o.fadeOut(100);
	});
// mWhom new
$('#mWhom').click(function(){
	$('#mWhomMenu').fadeIn(100);
	});
$('div[id^=wi]').click(function(){
	var v = $(this).attr('id').substring(2);
	$('#mwhom').val(v);
	$('#mWhom').html($(this).html());
	$('#mWhomMenu').fadeOut(100);
	});
// Country selector
$('div[id^=cp], div[id^=ca]').click(function(){
	var id = $(this).attr('id').substring(2);
	var val = $('#mtext').val();
	var name = $(this).html();
	var index = val.indexOf(name);
	if (index == -1) {
		if (numCountries < 7) {
			$('#cp'+id).css('font-weight','bold');
			$('#ca'+id).css('font-weight','bold');
			$('#mtext').val(val + ' ' + name);
			numCountries++;
			}
		} 
	else {
		$('#cp'+id).css('font-weight','normal');
		$('#ca'+id).css('font-weight','normal');
		$('#mtext').val(val.substring(0, index) + val.substring(index+name.length+1));
		numCountries--;
		}	
	});
// Popular / Alphabet order switch
$('#bPopular').click(function(){
	$('#cPopular').show();
	$('#cAlphabet').hide();
	$(this).css('background','#EEE');
	$('#bAlphabet').css('background','#FFF');
	});
$('#bAlphabet').click(function(){
	$('#cPopular').hide();
	$('#cAlphabet').show();
	$(this).css('background','#EEE');
	$('#bPopular').css('background','#FFF');
	});
// TravelType new
$('#mTravelType').click(function(){
	$('#mTravelMenu').fadeIn(100);
	});
$('div[id^=ti]').click(function(){
	var l = '.';	// delimiter
	var id = this.id.substring(2);
	var val = $('#tt').val();
	if (val != '') {
		var find = val.indexOf(l+id+l);
		if (find == -1) { // add
			$('#tt').val(val+id+l);
			numTypes += 1;
			$('#mTravelType').html(tSelected+numTypes);
			$(this).css('font-weight','bold');
		} else { // remove
			$('#tt').val(val.substring(0, find) + val.substring(find+id.length+1));
			if ($('tt').val() == l)
				$('#tt').val('');
			numTypes -= 1;
			$('#mTravelType').html(tSelected+numTypes);
			$(this).css('font-weight','normal');
			}		
	} else {
		$('#tt').val(l+id+l);
		numTypes = 1;
		$('#mTravelType').html(tSelected+'1');
		$(this).css('font-weight','bold');
	}
	$('#debug').html($('#tt').val());
	});
});
//-->
</script>

