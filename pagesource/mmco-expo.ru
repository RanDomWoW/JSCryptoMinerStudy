    <script>
		var video = document.getElementById('video');
		video.addEventListener('click',function(){  video.play();},false);

        $('#owl1').owlCarousel({
            dots: false,
            items: 1,
            loop: true,
            autoplay: true,
            autoplayTimeout: 5000,
            slideBy: 1,
            center: true,
            margin: 20,
            dots: true,
			nav:true,
			navText: ["<img src='http://mmco-expo.ru/wp-content/themes/mmco/img/left_arr.png'>", "<img src='http://mmco-expo.ru/wp-content/themes/mmco/img/right_arr.png'>"],			
			smartSpeed: 2000,
            responsive: {
                0: {
                    center: false,
                    margin: 0,
                    items: 1,
                },
                480: {},
                768: {
                    items: 1,
                },
                1024: {
                    items: 1,
                }
            }
        });
		
		var owl_dir = $('#owl2');	
        owl_dir.owlCarousel({
            dots: false,
            items: 4,
            loop: true,
            autoplay: true,
            autoplayTimeout: 3000,
			smartSpeed: 600,
            slideBy: 1,
            center: false,
            margin: 10,
			nav:true,
			navText: ["<img src='http://mmco-expo.ru/wp-content/themes/mmco/img/left_arr.png'>", "<img src='http://mmco-expo.ru/wp-content/themes/mmco/img/right_arr.png'>"],
			responsive: {
                0: {
                    items: 1,
                },
                480: {
                    items: 2,
                },
                768: {
                    items: 3,
                },
                1024: {
                    items: 4,
                }
            }
        });
		
		jQuery("document").ready(function($){
			
			$('a:not([href])').css({'position':'absolute','margin-top':'-200px', 'margin-bottom':'200px'});			
			
			var owl_p = $('#owl4');	
			owl_p.owlCarousel({
				dots: false,
				items: 1,
				loop: true,
				autoplay: true,
				autoplayTimeout: 3000,
				slideBy: 1,
				smartSpeed: 1500,
				responsive: {
					0: {
						items: 1,
					},
					480: {
						items: 1,
					},
					768: {
						items: 1,
					},
					1024: {
						items: 1,
					}
				},
			});
				
			owl_p.on('changed.owl.carousel', function(event) {				
				var active = $('#owl4').find('.owl-item.active').next();
				var parnter_type = active.find('.d4-item').data("partner");
				$('.d4-ttl').html( parnter_type + '<img src="http://temp.mmco-expo.ru/wp-content/themes/mmco/img/d4-line.png" alt="" class="d4-line">');
				//console.log(event);
			});
			
			owl_p.on('dragged.owl.carousel', function(event) {				
			var active = $('#owl4').find('.owl-item.active').next();
			var parnter_type = active.find('.d4-item').data("partner");
			$('.d4-ttl').html( parnter_type + '<img src="http://temp.mmco-expo.ru/wp-content/themes/mmco/img/d4-line.png" alt="" class="d4-line">');
			//console.log(event);
			
			var active = $('#owl4').find('.owl-item.active');
			var parnter_type = active.find('.d4-item').data("partner");
			$('.d4-ttl').html( parnter_type + '<img src="http://temp.mmco-expo.ru/wp-content/themes/mmco/img/d4-line.png" alt="" class="d4-line">');
		});		
		

			$('#owl2 .slide-cont').after('<div class="ext-btn">Подробнее</div>'); 
		
			$('.ext-btn').on("click", function() {
				//$(this).prev('.slide-cont').toggleClass("slide-cont-ext");
				//$(this).prev('.slide-cont').css('height','auto');
				var cont_h = $(this).prev('.slide-cont').height();
				var h = $(this).prev('.slide-cont').find('.slide-content').height(); //css('height');
				var nh = 362; 
				if (cont_h < 175) nh = h + 330;				
				//console.log("cont_h" + cont_h);
				//console.log("h " + h);
				$(this).prev('.slide-cont').animate({
					height: nh,
				}, 500, function() {
					// Animation complete.
				});				
			});
			
			$('.slide-cont').each(function(){
				var h = $(this).find('.slide-content').height(); 
				if (h < 61) $(this).next('.ext-btn').hide();
			}); 
			
			function setEqualHeight(columns){
				var tallestcolumn = 0;
				columns.each(function(){
					currentHeight = $(this).height()+10;
					if(currentHeight > tallestcolumn){
						tallestcolumn = currentHeight;
					}
				});
				columns.height(tallestcolumn);
			}

			setEqualHeight($("#owl1 .d2-item"));		
			
			var sl_pos = $('#owl2').offset().top - $('#owl2').height() - 150;	
			
			$(window).scroll(function () {
				var scrol_pos = $(this).scrollTop();
				if (scrol_pos > sl_pos) owl_dir.trigger('play.owl.autoplay',[1000]);

				if ($(this).scrollTop() > 50) {
					//$('.arr-btn').hide();
					$('.arr-btn').addClass("arr-btn-unvis");
				} else {
					//$('.arr-btn').show();
					$('.arr-btn').removeClass("arr-btn-unvis");	
				}
				
				var h = $(window).height();
				if ($(this).scrollTop() > h-200) {
					//$('.header-fix-button').show();
					$('.header-fix-button').addClass("header-fix-button-vis");
				} else {
					//$('.header-fix-button').hide();
					$('.header-fix-button').removeClass("header-fix-button-vis");					
				}
			});

		});
    </script>
