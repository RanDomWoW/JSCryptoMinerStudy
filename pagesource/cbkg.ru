// Выбор региона
function change_region(region_id){
	
	var cregion_id = 2310;	
		
	$('#region_sug').hide();
	$('#region_select').hide();
	
	$.post( "/ajax.php?mod=geo&action=change_region", { region_id: region_id, cregion_id: cregion_id, url:window.location.href }, function ( data ) {
		var result = jQuery.parseJSON(data);
		if (result.newurl) window.location.replace(result.newurl);	
	});
	
	return false;
}

// Авторегион
$.post( "/ajax.php?mod=geo&action=get_region",{ url:window.location.href,cregion_id:2310 }, function( data ) {
	var result = jQuery.parseJSON(data);
	if (result.newurl){
		$('#region_sug').show();
		$('#slug_reg').html('<a onclick="change_region(' + result.region_id + ');return false;" href="' + result.newurl + '">'+result.city+'</a>');
		console.log('other region');
	}
  //$( ".result" ).html( data );
  console.log(data);
});




function suggestion_error(){
	
	var message = $('#suggestion_block textarea').val();
	
	$.post( "/ajax.php?mod=suggestion&action=send_error", { message: message } ).done(function( data ) {
		if (data=='OK'){
			$("#suggestion_status").html("<font color='green'><b>Спасибо за помощь, информация отправлена администрации!</b></font><br>");
			setTimeout(function(){ 
			    $('#suggestion_block').dialog( "close" );
			    location.reload();
			}, 3000 ); 
		} else {
			
		}
		
  	});
	
}



