        $(document).ready(function () {
            var img = 'http://budport.com.ua/assets/upload/userfiles/Exhibitions/InterBUILD+.jpg';
            var url = "www.buildexpo.kiev.ua";
            var protocol = "http://";
            if (img) {
                $('body').css({
                    'background-image': 'url(' + img + ')',
                    'background-color': '#eceae1',
                    'background-repeat': 'no-repeat',
                    'cursor': 'pointer'
                });
                if (url) {
                    $('body').click(function (ev) {
                        if (ev.target == $('body')[0]) {
                            ev.stopPropagation();
                            if (url.indexOf(protocol) == -1)
                                window.open("http://" + url);
                            else
                                window.open(url);
                            return false;
                        }
                    });
                }
            }
        });

    </script>
