            <script src="https://static.tagboard.com/public/js/libs/intlTelInput.min.js"></script>
            <script>
              $(function(){
                var telInput = $("#Field6"),
                    countryInput = $("#Field129");
            
                // init the international country code plugin
                telInput.intlTelInput({
                  nationalMode: false,
                  preferredCountries: ["us", "gb", "tw", "jp"],
                  initialCountry: "auto",
                  geoIpLookup: function(callback) {
                    $.get('//ipinfo.io?token=ec1546d9a0c741', function() {}, "jsonp").always(function(resp) {
                      var countryCode = (resp && resp.country) ? resp.country : "";
                      callback(countryCode);
                    });
                  },
                  utilsScript: "/public/js/libs/utils.js" // just for formatting/placeholders etc
                });
            
                // get the country data from the plugin
                var countryData = $.fn.intlTelInput.getCountryData(),
                addressField = $("#Field129");
            
                // trigger a fake "change" event now, to trigger an initial sync
                telInput.trigger("countrychange");
            
                // listen to the telephone input for changes
                telInput.on("countrychange", function() {
                  var countryText = telInput.intlTelInput("getSelectedCountryData").name;
                  addressField.val(countryText);
                });
            
                var nowTemp = new Date();
                var now = new Date(nowTemp.getFullYear(), nowTemp.getMonth(), nowTemp.getDate(), 0, 0, 0, 0);
            
                var checkin = $('#Field14').datepicker({
                  onRender: function(date) {
                    return date.valueOf() < now.valueOf() ? 'disabled' : '';
                  }
                }).on('changeDate', function(ev) {
                  if (ev.date.valueOf() > checkout.date.valueOf()) {
                    var newDate = new Date(ev.date)
                    newDate.setDate(newDate.getDate() + 1);
                    checkout.setValue(newDate);
                  }
                  checkin.hide();
                  $('#Field15')[0].focus();
                }).data('datepicker');
                var checkout = $('#Field15').datepicker({
                  onRender: function(date) {
                    return date.valueOf() <= checkin.date.valueOf() ? 'disabled' : '';
                  }
                }).on('changeDate', function(ev) {
                  checkout.hide();
                }).data('datepicker');
            
                  var elem = document.getElementById('onboard-eventdates'),
                  checkBox = document.getElementById('onboard-eventdisplay');
                  checkBox.checked = false;
                  checkBox.onchange = function() {
                    elem.style.display = this.checked ? 'block' : 'none';
                  };
            
              });
            
            </script>
          </div>
        </div>
      </div>
