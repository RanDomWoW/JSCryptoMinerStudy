        <script>
            $(document).ready(function() {
                $( "#busqueda_ico" ).mouseenter(function() {
                    $( "#tooltipSearch" ).show();
                })

                /* $( "#cont_menu_static" ).mouseenter(function() {
                 $( "#tooltipSearch" ).hide();
                 });*/
                $( "#contenido" ).mouseenter(function() {
                    $( "#tooltipSearch" ).hide();
                });
                $( "#contenido_interiores" ).mouseenter(function() {
                    $( "#tooltipSearch" ).hide();
                });
            });

            $( "#search_button_scroll" ).click(function() {
                if (location.href.indexOf("buscador") == -1){
                    window.location="/buscador.html#"+escape(limpiarCadena(jQuery("#cb_scroll").val()))+"#"+$("#tipoBusqueda_scroll").val()+"#"+$("#momento_scroll").val();
                }else{
                    var values = "";
                    var un = ["EL", "LA", "LOS", "LAS", "DE", "SIN", "CON"];
                    var value = limpiarCadena(jQuery("#cb_scroll").val());
                    var arrSpaces = value.split(' ');
                    if($("#tipoBusqueda_scroll").val()=="i"){
                        for (var valuec in arrSpaces) {
                            if (un.indexOf(arrSpaces[valuec].toUpperCase()) == -1){
                                if ( arrSpaces[valuec] != "" && arrSpaces[valuec] != " "){
                                    values += "keywords:("+arrSpaces[valuec]+"*) OR title:("+arrSpaces[valuec]+"*) OR description:("+arrSpaces[valuec]+"*)";
                                    if (valuec != arrSpaces.length-1) {values += " OR ";}
                                }
                            }
                        }
                        if($("#momento_scroll").val() != "todo"){
                            values = "keywords:("+$("#momento_scroll").val()+") AND ("+values+")";
                        }
                    }else{
                        for (var valuec in arrSpaces) {
                            if (un.indexOf(arrSpaces[valuec].toUpperCase()) == -1){
                                if ( arrSpaces[valuec] != "" && arrSpaces[valuec] != " "){
                                    values += "title:("+arrSpaces[valuec]+"*) OR description:("+arrSpaces[valuec]+"*)";
                                    if (valuec != arrSpaces.length-1) {values += " OR ";}
                                }
                            }
                        }
                        if($("#momento_scroll").val() != "todo"){
                            values = "keywords:("+$("#momento_scroll").val()+") AND ("+values+")";
                        }
                    }
                    if (values != ""){
                        Manager.store.addByValue('q', values);
                    }
                    Manager.doRequest();
                    $("#palabra_busqueda").html('"'+value+'"');
                }
            });
        </script>
