/******************* 팝업 롤링 *******************/
var pop_pre_process = true;
var popOverMouse = true;
var popLoop = 1;
var popLoopCnt = 0;


function popScrolling(len){
	
	if(len != null && typeof len != "undefined") {
		popLoopCnt = len;
	}
	
	if(popOverMouse && pop_pre_process){

		var objName = "pop" + popLoop;
		var obj = document.getElementById(objName);
		if(obj != null ){
			obj.style.display = "";
			obj.className = "active";
			for(var i = 1; i <= popLoopCnt; i++){
				if(i == popLoop) continue;
				var tmpobjName = "pop" + i;
				var tmpobj = document.getElementById(tmpobjName);
				tmpobj.style.display = "none";
				tmpobj.className = "hidden";
			}
			popLoop++;
			if(popLoop > popLoopCnt) popLoop = 1;
		}
	}
}
function popPlay(){
	if(pop_pre_process == true){
		pop_pre_process = false;
		document.getElementById('popPlayBtn').src='/images/portal/btn_m_pop_play.gif';
		document.getElementById('popPlayBtn').alt='시작';
	}else if(pop_pre_process == false){
		pop_pre_process = true;
		document.getElementById('popPlayBtn').src='/images/portal/btn_m_pop_stop.gif';
		document.getElementById('popPlayBtn').alt='정지';
	}
}
function popChange(mode, len){
	if(len != null && typeof len != "undefined") {
		popLoopCnt = len;
	}
	
	for(var i = 1; i <= popLoopCnt; i++){
		var tmpobjName = "pop" + i;
		var tmpobj = document.getElementById(tmpobjName);
		tmpobj.style.display = "none";
		tmpobj.className = "hidden";
	}
	if(mode == 1) popLoop = popLoop - 1;
	if(mode == 2) popLoop = popLoop + 1;
	if(popLoop > popLoopCnt) popLoop = 1;
	else if(popLoop == 0) popLoop = popLoopCnt;
	var objName = "pop" + popLoop;
	var obj = document.getElementById(objName);
	obj.style.display = "";
	obj.className = "active";
}

function goSelUrl(num){
	var obj = document.getElementById('select'+num);
	if(obj.value == '' || obj.value == 'total'){
		alert("항목을 선택해주세요.");
