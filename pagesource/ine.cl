<script>
$(document).ready(function(){
	$(".login").click(function(){		
		if ($(this).hasClass("showing")){
			$(".form-login").hide();
			$(this).removeClass("showing");
		} else {
			$(this).addClass("showing");
			$(".form-login").show();

		}

			$(".accesibilidad").hide();
			$(".accesibilidadMenu").removeClass("showing");
	});
	$(".icon-close-login").click(function(){		
			$(".form-login").hide();
			$(".login").removeClass("showing");
			return false;
	});


	$(".accesibilidadMenu").click(function(){		
		if ($(this).hasClass("showing")){
			$(".accesibilidad").hide();
			$(this).removeClass("showing");
		} else {
			$(this).addClass("showing");
			$(".accesibilidad").show();
		}

		$(".form-login").hide();
		$(".login").removeClass("showing");

	});
	$(".accesibilidadCerrar").click(function(){		
			$(".accesibilidad").hide();
			$(".accesibilidadMenu").removeClass("showing");
			return false;
	});


	$(".grayscale").click(function(){
		if(!$("body").hasClass("scaled"))
		{
			$("body").css("filter", "grayscale(100%)");
			$("body").addClass("scaled");
		}else{
			$("body").css("filter","");
			$("body").removeClass("scaled");
		}
	});

	$(".subrayar").click(function(){
		if(!$("a").hasClass("subrayed"))
		{
			$("a").css("text-decoration", "underline");
			$("a").addClass("subrayed");
			$("button").css("text-decoration", "underline");
			$("button").addClass("subrayed");
		}else{
			$("a").css("text-decoration","");
			$("a").removeClass("subrayed");
			$("button").css("text-decoration","");
			$("button").removeClass("subrayed");
		}
	});
	$(".s-small").click(function(){
		$("*:not(.cont-acc)").css("font-size", "98%");
	});
	$(".s-normal").click(function(){
		$("*:not(.cont-acc)").css("font-size", "");
	});
	$(".s-big").click(function(){
		$("*:not(.cont-acc)").css("font-size", "100.5%");
	});

});
</script>

