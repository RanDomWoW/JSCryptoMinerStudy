      if($("#txtRutEncuestador").val() != ""){
            $(".loading-encuestadores").show();
            $.ajax({
          type: "GET",
              url: "http://ve-ine.ine.cl/WebServiceBusqueda.asmx/Buscar_Encuestador",
              data: {rut : $("#txtRutEncuestador").val()},
          dataType: "xml",
          success: function(xml){
                 if($(xml).find("resultado").find("NO_ENCONTRADO").text() == "1")
                 {
                      $(".exitoEncuestadores").hide();
                      $(".noEncontrado").show();
                      $(".errorEncuestadores").hide();
                      $( "#resultado-encuestadores" ).animate({ right: "0px" });
                 }else{
                      $(".noEncontrado").hide();
                      $(".exitoEncuestadores").show();
                      $(".errorEncuestadores").hide();
                          $(".nombreEnc").empty().append($(xml).find("ENCUESTADOR").find("NOMBRES").text() + " " + $(xml).find("ENCUESTADOR").find("PATERNO").text() + " " + $(xml).find("ENCUESTADOR").find("MATERNO").text() );
                          $(".rutEnc").empty().append($(xml).find("ENCUESTADOR").find("RUT").text());
                          $(".imgEnc").attr("src","http://ve-ine.ine.cl/imagenes/personal/" +$(xml).find("ENCUESTADOR").find("FOTO").text());
                          $(".regionEnc").empty().append($(xml).find("ENCUESTADOR").find("REGION").text());
                   	  //$(".cargoEnc").empty().append($(xml).find("ENCUESTADOR").find("CARGO").text());
                      $( "#resultado-encuestadores" ).animate({ right: "0px" });
                 }
          }, error: function(x){
                 $(".exitoEncuestadores").hide();
                 $(".noEncontrado").hide();
                 $(".errorEncuestadores").show();
                 $( "#resultado-encuestadores" ).animate({ right: "0px" });
          }, complete: function(){
                 $(".loading-encuestadores").hide();
          
          }
      });
          
    }
    });
  
  
  });
  
</script>
