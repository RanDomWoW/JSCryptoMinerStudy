<script>
  $(document).ready(function(){
    
    
    $(".export-pdf").click(function() {
        ga('send', 'event', 'Calculadora IPC', 'Descarga PDF');
        $(".logo_interior").show();
        $(".export-pdf, .calcularBtn").hide();
        kendo.drawing.drawDOM($(".CalculadoraHome"))
        .then(function(group) {
            
            return kendo.drawing.exportPDF(group, {
                paperSize: "letter",
                margin: { left: "4cm", top: "4cm", right: "4cm", bottom: "4cm" }
            });
        })
        .done(function(data) {
            
            kendo.saveAs({
                dataURI: data,
                fileName: "reporte_ipc.pdf"

            });
        	$(".export-pdf, .calcularBtn").show();
              $(".logo_interior").hide();
        });
    });
    
    
    $(".ipc").click(function(){
    	$(this).addClass("tabActivo");
        $(".agenda").removeClass("tabActivo");
        $(".CalendarioHome").hide();
        $(".CalculadoraHome").show();
    });
    $(".agenda").click(function(){ 
    	$(this).addClass("tabActivo");
        $(".ipc").removeClass("tabActivo");
        $(".CalendarioHome").show();
        $(".CalculadoraHome").hide();
        $(".fc-today-button").click();
      $(".fc-content").each(function(){
      	$(this).css('display','none');
      });
      
           
    });
    $('.datepickerCalc').datepicker( {
    	format: "mm-yyyy",
        maxDate: "0",
        viewMode: "months",
        minViewMode: "months",
        locale: "es"
       
    });
    
    $("#valorCalcular").on("keypress keydown keyup change", function(){
      if( ($(this).val().length) > 10 ){
        $(this).val(  $(this).val().substring(0, 10) );
      }
    });
    
    $(".calcularBtn").click(function(){
      ga('send', 'event', 'Calculadora IPC', 'Click Botón Calcular');
      var fechaInicio =$(".FechaInicio").val();
      var fechaFin = $(".FechaFin").val();
      var valorPeso = $(".valorCalc").val();
      if(fechaInicio != "" && fechaFin != "" )
      {
          var inicio = $(".FechaInicio").val().split("-");
          var fin = $(".FechaFin").val().split("-");
            $.ajax({
                type: "GET",
              url: "http://encina.ine.cl/CALCULADORA/reportes/xmlVariacion.asp",
                    data: {a1 : inicio[1],
                           a2: fin[1],
                           m1: inicio[0],
                           m2: fin[0],
                           valor:$(".valorCalc").val()},
                dataType: "xml",
                success: function(xml){
                  $(".ContenedorMsjInicial").hide();
                  $(".CalculadoraHomeResultado").show();
                  if($(xml).find("Datas").eq(0).find("Vari").text().indexOf("rango") >= 0){
                    $(".CalculadoraHomeResultado").removeClass("errorCalc");
                    $(".CalculadoraHomeResultado").addClass("errorCalc");
                  }else{
                    $(".CalculadoraHomeResultado").removeClass("errorCalc");
                  }
                  $(".CalculadoraHomePeriodo").show();
                    $(".CalculadoraHomeValor").show();
                    $(".MensajeError").hide();
                  $(".mensajeCalc").empty().append(""+$(xml).find("Datas").eq(2).find("Vari").text());
                  $(".tiempoCalc").empty().append(""+$(xml).find("Datas").eq(1).find("Vari").text());
                  $(".valorCalc").empty().append($(xml).find("Datas").eq(0).find("Vari").text());
                  $(".valorAjus").empty().append("Valor ajustado: $"+$(xml).find("Datas").eq(3).find("Vari").text());
                },
                error: function(x){
                  $(".ContenedorMsjInicial").hide();
                  $(".CalculadoraHomeResultado").show();
                    $(".CalculadoraHomePeriodo").hide();
                    $(".CalculadoraHomeValor").hide();
                    $(".MensajeError").show();
                }
            });
    	}
    });
    
    $(function() {
      $("#valorCalcular").on("change", function (e) {
        var $field = $(this),
            val=this.value;
        if (val.length >  Number($field.attr("maxlength"))) {
          val=val.slice(0,  Number($field.attr("maxlength")));
          $field.val(val);
        }
      });   
    });
    
    $('.datepickerCalc').on('changeDate', function(ev){
        $(this).datepicker('hide');
    });

  });
</script>

