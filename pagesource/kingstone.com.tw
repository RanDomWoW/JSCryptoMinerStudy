// Charles Chen, 20111214
function ArrowFrame(obj){
    var container = this;
    var frames = $(obj + " .frame");
    var arrowLeft = $(obj + " div.buttons a.arrow_left");
    var arrowRight = $(obj + " div.buttons a.arrow_right");
    var posView = $(obj + " div.buttons span");
    var pos = 0;
    // init //
    posView.html((pos+1) +"/"+ frames.size());
    $(frames.get(0)).addClass("on")
    // methods //
    this.prev = function(e) {
        if (pos - 1 < 0){ return false; }
        $(frames.get(pos)).removeClass("on")
        pos -= 1;
        $(frames.get(pos)).addClass("on");
        posView.html((pos+1) +"/"+ frames.size());
    };
    this.next = function(e) {
        if (pos + 1 >= frames.size()){ return false; }
        $(frames.get(pos)).removeClass("on")
        pos += 1;
        $(frames.get(pos)).addClass("on");
        posView.html((pos+1) +"/"+ frames.size());
    };
    // listener //
    arrowLeft.click( jQuery.proxy( container, "prev" ));
    arrowRight.click( jQuery.proxy( container, "next" ));
}

// Charles Chen, 20111215
function SubMenu(obj){
    var container = this;
    var sensors = $(obj + " > ul li > a");
    var subs = $(obj + " > ul li > div");
    var t;
    // methods //
    this.showSub = function(e) {
        clearTimeout(t);
        $(sensors).each(function(i, ele){
            if (e.target == ele){
                $(subs.get(i)).parent().addClass("on");
                var offset = $(sensors.get(i)).offset();
                $(subs.get(i)).offset({ top: offset.top - 10, left: offset.left + $(subs.get(i)).parent().width() - 14});
            }else{
                $(subs.get(i)).parent().removeClass("on");
            }
        });
    }
    this.hideSub = function(e) {
        t = setTimeout(function() { $(subs).parent().removeClass("on"); }, 200);
    }
    this.show = function(e) {
        clearTimeout(t);
    }
    // listener //
    $(sensors).bind('mouseover', this.showSub);
    $(sensors).bind('mouseout', this.hideSub);
    $(subs).bind('mouseover', this.show);
    $(subs).bind('mouseout', this.hideSub);
}


function bookMenu(obj){
    var container = this;
    var sensors = $(obj + " > ul li > a");
    var subs = $(obj + " > ul li > div");
    var t;
    // methods //
    this.showSub = function(e) {
        clearTimeout(t);
        $(sensors).each(function(i, ele){
            if (e.target == ele){
                $(subs.get(i)).parent().addClass("on");
            }else{
                $(subs.get(i)).parent().removeClass("on");
            }
        });
    }
    this.hideSub = function(e) {
        t = setTimeout(function() { $(subs).parent().removeClass("on"); }, 200);
    }
    this.show = function(e) {
        clearTimeout(t);
    }
    // listener //
    $(sensors).bind('mouseover', this.showSub);
    $(subs).bind('mouseover', this.show);
}

