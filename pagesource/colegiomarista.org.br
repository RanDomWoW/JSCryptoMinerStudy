    <script>

        $(function () {

            $('.ls-popUp').click(function (e) {
                $('#ls-Container').fadeIn();

                var label = $(this).data("label");
                var unidade = $(this).parent().find("input[type='hidden'][name='lstUnidades']").val();

                var srcUrl = encodeURI("http://gvdasa.maristas.org.br:8080/apsweb/marista/lembrarsenha/index.html?label=" + label + "&unidade=" + unidade + "");

                var fullHeight = true;
                var hasUnidade = false;

                if (typeof (unidade) != "undefined") {
                    if (unidade != "undefined") {
                        hasUnidade = true;
                    }
                }
                if (hasUnidade) {
                    if (unidade != null) {
                        if (unidade != "") {
                            fullHeight = false;
                        }
                    }
                }

                if (fullHeight) {
                    $("#iframeLembrarSenha").css("height", "200px");
                    $("#ls-form-box").css("height", "");
                    $("#ls-Box").css("height", "");
                    $("#ls-Container").css("height", "");
                }
                else {
                    $("#iframeLembrarSenha").css("height", "156px");
                    $("#ls-form-box").css("height", "138px");
                    $("#ls-Box").css("height", "246px");
                    $("#ls-Container").css("height", "256px");
                }

                $("#iframeLembrarSenha").attr("src", srcUrl);

                e.preventDefault();
                return false;
            });

            $('#ls-close, #ls-BtnCancelar').click(function () {
                $('#ls-Container').fadeOut();
            });

            $('#ls-Feedback-Close').click(function () {
                $('#ls-Feedback, #ls-Feedback-Background').fadeOut();
            });

            $('#rpUnidade').change(function () {
                $('#ls-lstUnidades').val($(this).val());
            });
        });
    </script>
    <div id="ls-Feedback-Background">
    </div>
    <div id="ls-Feedback">
        <div id="ls-Feedback-Close">
