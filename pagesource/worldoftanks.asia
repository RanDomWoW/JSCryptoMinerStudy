                <script>
                $(function() {
                    'use strict';

                    var accountInfo = wgsdk.account_info();

                    if (!is_auth_user()) {
                        return;
                    }

                    var fireTagsFromCookie = function (accountInfo) {
                        var clanStatus = 'not clan gamer',
                            premiumAccount = 'haven\'t premium account',
                            battlesCountRanges = [
                                [0, 499],
                                [500, 999],
                                [1000, 1999],
                                [2000, 3999],
                                [4000, 5999],
                                [6000, 7999],
                                [8000, 9999],
                                [10000, 11999],
                            ],
                            battlesCountRange = '12000-and more',
                            battlesCount,
                            clanLeaderId;

                        if (accountInfo.get_clan_id()) {
                            clanStatus = 'clan gamer';

                            clanLeaderId = accountInfo.get_clan_leader_id();
                            if (clanLeaderId && clanLeaderId === accountInfo.get_spa_id()) {
                                clanStatus = 'clans leader';
                            }
                        }

                        if (accountInfo.is_premium_active()) {
                            premiumAccount = 'have premium account';
                        }

                        battlesCount = accountInfo.get_battles_count();
                        for (var i in battlesCountRanges) {
                            if (!battlesCountRanges.hasOwnProperty(i)) {
                                continue;
                            }

                            if (battlesCount >= battlesCountRanges[i][0] && battlesCount <= battlesCountRanges[i][1]) {
                                battlesCountRange = battlesCountRanges[i][0] + '-' + battlesCountRanges[i][1];
                                break;
                            }
                        }

                        window.dataLayer = window.dataLayer || [];
                        window.dataLayer.push({
                            'clans': clanStatus,
                            'premium_account': premiumAccount,
                            'battles_count': battlesCountRange,
                            'event': 'user_info'
                        });
                    };

                    accountInfo.queue(fireTagsFromCookie);
                });
                </script>
            
        

    

    

    

