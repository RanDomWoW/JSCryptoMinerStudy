<script>
$(window).scroll(function(){
    if ($(this).scrollTop() > 100)
        $("#yukari").fadeIn(500);
    else 
        $("#yukari").fadeOut(500);
});
$(document).ready(function(){
    $("#yukari").click(function(){ 
        $("html, body").animate({ scrollTop: "0" }, 500);
        return false;
    });
});
</script>

