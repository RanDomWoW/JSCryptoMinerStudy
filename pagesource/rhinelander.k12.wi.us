				$('#dipImage_349571' + nums).fadeOut(speed);
			}
			
			if($('#videoShell_349571' + nums).length){
				$('#videoShell_349571' + nums).fadeOut(speed);
			}
			if($('#dipContent_349571' + nums).length){
				$('#dipContent_349571' + nums).fadeOut(speed);
			}
			
			if(nums >= totalDips){nums = 1;}else{nums = nums + 1;}
			
			if($('#nav_349571' + nums).length){
				$('#nav_349571' + nums).addClass('on');
			}
			
			if($('#nav_li_349571' + nums).length){
