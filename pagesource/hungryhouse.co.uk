  <script>
      $(document).ready(function() {
        $('html').on('click', '.je-banner-wrap a', function(e) {
          var parseAnalyticsCookie = function() {
            var values = [];
            var cookie = document.cookie.match(new RegExp('__utmz=([^;]+)'));
            var map = {
                utmcsr: 'utm_source',
                utmccn: 'utm_campaign',
                utmcmd: 'utm_medium'
            };
            if (cookie) {
                var z = cookie[1].substring(cookie[1].match(new RegExp('.[a-zA-Z]+')).index + 1);
                if (z.length >= 4) {
                    var y = z.split('|');
                    for (i = 0; i < y.length; i++) {
                        var pair = y[i].split("=");
                        if (pair[0] == 'utmcsr' || pair[0] == 'utmcmd')
                            pair[1] = 'hh_' + pair[1];
                        if (map.hasOwnProperty(pair[0]))
                            values.push(map[pair[0]] + '=' + pair[1]);
                    }
                }
            }
          return values;
        }

        if (window.dataLayerManager && window.dataLayerManager.orderOnJusteatClicked) {
          e.preventDefault();
          var link = $(this).attr('href');
          var custId = '';
          var pageType = window.dataLayerManager.getPoolValue('pageType');
          window.dataLayerManager.orderOnJusteatClicked(-1);
          var umtzValues = parseAnalyticsCookie().join('&');

          setTimeout(function() {
              window.location = link + '?referrer=' + pageType + custId + '&' + umtzValues;
          }, 300);
        }
      });
    });
  </script>

