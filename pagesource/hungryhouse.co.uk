    $(document).ready(function(){

        var showErrorMessage = function(message) {
            postCode
                .addClass("error-input")
                .after("<div id='post-code-error-message'>" + message + "</div>");
        };

        var removeErrorMessage = function () {
            $("#post-code-error-message").remove();
            postCode.removeClass("error-input");
        };

        var _isValidFullPostCode = function (postcode) {
            return postcode.match(/^[A-Z](?:\d|\d\d|[A-Z]\d|[A-Z]\d\d|\d[A-Z]|[A-Z]\d[A-Z])\s?\d[A-Z][A-Z]$/i);
        };

        var formatterPostcode = function (postcode) {
            if (regres = postcode.match(/^(\w{2,4})\s?(\w{3})$/)) {
                return regres[1] + ' ' + regres[2]
            }
            return postcode;
        };

        var trimPostCode = function (postcode) {
            return postcode.replace(/\s+/, "").toUpperCase();
        };

        var postCode = $("#post-code-input");

        $('#form-homepage').submit(function(){
            var trimPostcode = trimPostCode(postCode.val());

            postCode.val(formatterPostcode(postCode.val()));

            if (!_isValidFullPostCode(trimPostcode)) {
                showErrorMessage('Please enter a full postcode.');
                sendDataToGTM(trimPostcode, false);
                return false;
            }

            $.ajax({
                context: this,
                url: "ajax.php",
                type: "GET",
                data: {"target": "postcode_validation", "postcode": trimPostcode},
                success: function(response) {
                    if (!JSON.parse(response).success) {
                        showErrorMessage('Not found, please check your entry.');
                        sendDataToGTM(trimPostcode, false);
                    } else {
                        removeErrorMessage();
                        sendDataToGTM(trimPostcode, true);
                        $.rests.showSpinner('#cta-homepage');
                        this.submit();
                    }
                }
            });
            return false;
        });

        postCode.keyup(function(e) {
            if (e.keyCode === 8) {
                this.value = this.value.replace(/\s+/, "");
            }
            if (e.keyCode !== 13) {
                removeErrorMessage();
            }
        });

    });

    function sendDataToGTM(val, isSuccess) {
        if (!dataLayerManager) return;
        if (isSuccess === null || typeof isSuccess === 'undefined') isSuccess = true;
        if (isSuccess) {
            dataLayerManager.addressSubmitted(val);
        } else {
            dataLayerManager.addressSubmittedWrong(val);
        }
    }

</script>

