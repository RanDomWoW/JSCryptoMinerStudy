<script language="javascript">
var IsDisplayDivConverterCurrencies =false;
var CurrenciesConverterXmlHttp = new BaseXmlHttp("");
var CurrenciesConverterSelectObjsToFill = null;


function CurrenciesConverterXmlFillCatSelect(xmlFile,objtoFill)
{
	
	CurrenciesConverterSelectObjsToFill = objtoFill;	
	CurrenciesConverterXmlHttp.callXmlWithUrl(xmlFile,InitialCurrenciesConverterCat);	
	
	return false;
}
function InitialCurrenciesConverterCat()
{			
	if( CurrenciesConverterXmlHttp.oXmlHttp.readyState != 4 )
		return;
	
	
	var nodes = CurrenciesConverterXmlHttp.oXmlHttp.responseXML.getElementsByTagName("categories")[0].childNodes;
	
	var selectObj = document.getElementById(CurrenciesConverterSelectObjsToFill);
	
	if(selectObj){
		for(i=0;i<nodes.length;i++){	
			selectObj.options.add(new Option(nodes.item(i).attributes(0).text ,nodes.item(i).nodeName )); //new Option(text,value);
		}
	}
	return false;
	
}

function ShowHideCurrenciesConverter()
{
	
	var showThis = "Открыть конвертер валют";
	var dontShowThis = "Закрыть конвертер валют";
	var e = document.getElementById("divConverterCurrencies");
	
	
	var div = document.getElementById("divDDLSearchTypeFooter");
	
	if ( IsDisplayDivConverterCurrencies == false)
	{
		//xmlFillCatSelect('/Data/srv-currencies-from.xml','From');
		//CurrenciesConverterXmlFillCatSelect('/Data/srv-currencies-from.xml','From');
		CurrenciesConverterXmlFillCatSelect('/Data/srv-currencies-to.xml','To');
		IsDisplayDivConverterCurrencies =true;
	}
			
	if (div!= null && div.style.display != "none")
	{
		ClosePopUpForFooter();
	}
		
		

	if (IsIE())
	{	
		var lbl = document.getElementById("trCurrenciesConverterFooter").children[0].children[0].children[0];
		if (e.style.display == "none" || e.style.display == "")
		{
			e.style.display = "inline";
			lbl.innerHTML = dontShowThis;
		}
		else
		{
			e.style.display = "none";
			lbl.innerHTML = showThis;
		}
	}
	else
	{
		var txt = document.all.trCurrenciesConverterFooter.cells.item(0).firstChild.innerHTML;

		if (e.style.display == "none" || e.style.display == "")
		{
			txt = txt.replace(showThis, dontShowThis);
			e.style.display = "inline";
		}
		else
		{
			txt = txt.replace(dontShowThis, showThis);
			e.style.display = "none";
		}

		document.all.trCurrenciesConverterFooter.cells.item(0).firstChild.innerHTML = txt;
	}

}
function preconvert() {
    window.open("RedirectPage.aspx?moduleID=1799&url=DesktopModules/converter_redirect.aspx?Amount="+document.getElementById("Amount").value+"&From="+document.getElementById("From").options[document.getElementById("From").selectedIndex].value+"&To="+document.getElementById("To").options[document.getElementById("To").selectedIndex].value);
}

if (parseInt(navigator.appVersion)>= 4)
	{
		
		
		if (navigator.appName == "Netscape")
			{
				layerStyleRef="layer.";
				layerRef="document.layers";
				styleSwitch="";
			}
		else
			{
				layerStyleRef="layer.style.";
				layerRef="document.all";
				styleSwitch=".style";
			}
	}

function OpenDiv(layerName)
{
	
	var item=eval(layerRef+'["'+layerName+'"]'+styleSwitch+'.display');	
	
	if (item=="none")
		show(layerName);
		
	else
		cls(layerName)	;
		
		
}	
function show(layerName)
	{
		
		if (parseInt(navigator.appVersion)>= 4)
			{
				eval(layerRef+'["'+layerName+'"]'+styleSwitch+'.display=""');
			}
	}
function cls(layerName)
{
	if (parseInt(navigator.appVersion)>= 4)
		{
			eval(layerRef+'["'+layerName+'"]'+styleSwitch+'.display="none"');
		}
}


</script>
