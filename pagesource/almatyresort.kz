    jQuery('document').ready(function() {
        jQuery('#cmbf-form-745 input').focus( function() { 
                jQuery(this).parents().eq(1).removeClass("valid-error");
        });
        jQuery('#cmbf-form-745').on('submit', function (event) {
            var form = jQuery(this);
            var error = false;

            form.find('input[type="text"]').each(function() {
                if (!jQuery(this).val().length) {
                    jQuery(this).parents().eq(1).addClass("valid-error");
                    error = true;
                }
            });

            if (!error)
            {
                ajaxsendmessage();
            }

            function ajaxsendmessage() {
                jQuery.ajax({
                    type: 'POST',
                    url: form.attr('action'),
                    data: form.serialize(),
                    cache: false,
                    beforeSend: function() {
                        form.find('input[type="submit"]').attr('value', 'Отправка');
                        form.find('input[type="submit"]').attr('disabled', 'disabled');
                    },
                    success: function () {
                            form.find('.cmbf-field-container').slideUp('fast');
                            form.find('.cmbf-success').slideDown("fast");
                    },
                    error: function(jqXHR, textStatus, errorThrown) {
                        console.log(textStatus, errorThrown);
                    }
                });
            }
        event.preventDefault();
        });

        jQuery('#cmbf-button-container-745').click(function() {
            jQuery('#cmbf-form-container-745, #cmbf-obfuscator-745').fadeIn('slow');
        });

        jQuery('#cmbf-obfuscator-745, #cmbf-form-close-745').click(function() {
            jQuery('#cmbf-form-container-745, #cmbf-obfuscator-745').fadeOut('slow');
        });
    }); 
