				li2.appendChild(document.createTextNode(entry.content));
				li3.appendChild(document.createTextNode(entry.publishedDate));
				li4.appendChild(document.createTextNode(entry.link));
	
				ul.appendChild(li1);
				ul.appendChild(li2);
				ul.appendChild(li3);
				ul.appendChild(li4);
	
				div.appendChild(ul);
	
				container.appendChild(div);
			}
        }
      });
    }
    google.setOnLoadCallback(initialize);

