<script>
$(document).on('ready', function() {
	/* visual */
	var hWin = $(window).height();
	$('.fadeshow').css('height', + hWin + 'px');
	$(window).on('load',function(e){
		var hWin = $(window).height();
		$('.fadeshow').css('height', + hWin + 'px');
	});
	$(window).resize(function(e){
		var hWin = $(window).height();
		$('.fadeshow').css('height', + hWin + 'px');
	});


	$('#header').scrollToFixed();
	$('.headerSP').scrollToFixed();


	/* biggerlink and lazyload */
	$('.eventList li, .workBox, .newsList li').biggerlink();
	$('.biggerLink').biggerlink();
	//$("#container img").lazyload();

	$("#container img").show().lazyload({ effect : "fadeIn" });


	/* workBox hover */
	$(window).on('load resize',function(e){
		if($(window).width() > 767){
			$(".workBox").hover(function(){
				$(this).children('.workInfo').slideDown({duration:200, easing:"easeOutCirc"});
			},
			function(){
				$(this).children('.workInfo').slideUp({duration:200, easing:"easeOutCirc"});
			});
		}
	});


	$(window).on('load resize',function(e){
		if($(window).width() < 768){
			$(".newsList li div").addClass('fixHeightChild');
			$(window).scroll(function () {
				var winTop = $(this).scrollTop();
				if (winTop > 50) {
					 $('.iconWhite').fadeOut();
				}else{
					$('.iconWhite').fadeIn();
				}
			});
		}else{
			$(".newsList li div").removeClass('fixHeightChild');
		}


	});


	/* banner list carousel */
	function carousel_01_pc() {
		$('#bnrCarousel').carouFredSel({
			width: '100%', circular: true,    align: "center",
			prev: '#prev',
			next: '#next',
			auto:false,
			items: {
				//width: '25%',
				//height: '30%', //optionally resize item-height
				visible: 6,
				start: 0,
				minimum: 4
			},
			scroll: {
				items: 1,
				pauseOnHover: false,
				duration: 1600,
				timeoutDuration: 5000,
				easing: 'easeOutQuint'
			},
			direction: 'left',
			mousewheel: false,
			swipe: {
				onMouse: true,
				onTouch: true
			}
		});
	}
	carousel_01_pc();
	$(window).resize(function () {
		carousel_01_pc();
	});//load, resize



});


