            term = encodeURIComponent(term);
            if (term != '') {
                var link = '/cauta/' + term.replace(/%2F/g, '%252F');
                link += $('#search-form input[name=search-category]').val();
                $('#search-button').attr('href', link);
            }
        }


        function enableTypeAhead() {
            var called = '';
            var $context = $("#search-form");
            $context.find("#search-field").typeahead({
                minLength: 3,
                highlight: true
            }, {
                name: 'products',
                displayKey: 'name',
                source: productsSearch.ttAdapter(),
                templates: {
                    suggestion: function(a) {

                        if (called != $('#search-field').val()) {
                            called = $('#search-field').val();
                            $('#home_tags_holder').fadeIn(200);
                            if ($('#search-field').val() !== '') {
                                var the_string = $('#search-field').val();
                                $.ajax({
                                    url: '/products/homePhonesSearch',
                                    type: 'post',
                                    dataType: 'html',
                                    data: {
                                        string: the_string
                                    },
                                    success: function(r) {
                                        createSearchLink();
                                        $('#home_tags_holder').html(r);
                                    }
                                })
                            } else {
                                $('#home_tags_holder').fadeOut(0);
                            }
                        }
                        return '<div>'
                            + '<span> <i class="fa fa-search" aria-hidden="true"></i> '
                            + a.name
                            + '</span></div>';
                    }
                }
            }).on('typeahead:render', function(a, b, c, d) {
                var searchTerm = $(this).val();
                $context.find('.tt-dataset-products').mark(searchTerm);
            }).on('typeahead:selected', function (a, b, c) {
                document.location.href = '/cauta/' + encodeURIComponent(b.name.replace('/', ''));
            }).on('typeahead:open', function() {
                $('.typeahead-overlay').fadeIn(100);
            }).on('typeahead:close', function() {
                $('.typeahead-overlay').fadeOut(100);
            });
        }


        $(document).ready(function() {
            $('#search-form .tt-menu').prepend('<p class="label-search">Sugestii de cautare:</p>');
            $('#search-form .tt-menu').append('<div id="home_tags_holder" class="clearfix"></div>');

            $(document).on("click",'#tt-submit-form',function() {
                $(location).attr('href', $('#search-button').attr('href'));
            });
        });

        /*$('#search-field').keyup(function () {
        })*/

        function createSearchLinkSecond() {
            var term = $('#search-field-second').val();
            term = encodeURIComponent(term);
            if (term != '') {
                var link = '/cauta/' + term.replace(/%2F/g, '%252F');
                $('#search-submit-second').attr('href', link);
                $('#search-form-second').attr('action', link);
            }
        }

        $('#mobile-search-toggle-js').on('click', function(){
            var search = $('.search-container');
            if(search.is(":hidden")) {
                search.slideDown(400);
            } else{
                search.slideUp(400);
