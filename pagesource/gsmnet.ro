<script>
    function go_off() {
        var hoverTimeoutIntent = setTimeout( function() {
            if ($('.category-hoverintent:hover').length != 0) {
                clearTimeout(hoverTimeoutIntent)
            } else {
                $('.top-megamenu-subcategories-background').fadeOut(200);
            }
        }, 220);
    }

    $('.mobile-menu-toggle').on('click', function(){
        $('#top-megamenu-nav').slideToggle(400)
    });

    function resizeMenu () {
        var win = $(this);

        if (win.width() <= 991 ) {
            $(".category-hoverintent").unbind("mouseenter").unbind("mouseleave");
            $(".category-hoverintent").removeProp('hoverIntent_t');
            $(".category-hoverintent").removeProp('hoverIntent_s');
        } else {
            $(".category-hoverintent").hoverIntent({
                over: function () {
                    $(this).find('.dropdown-menu').stop(true,true).fadeIn(100);
                    $('.top-megamenu-subcategories-background').fadeIn(200);
                    $('.top-megamenu-subcategories-background').addClass($(this).data('bordercolor'));
                },
                timeout: 350,
                out: function() {
                    $(this).find('.dropdown-menu').stop(true,true).fadeOut(100);
                    $('.top-megamenu-subcategories-background').removeClass($(this).data('bordercolor'));
                    go_off();
                }
            });
        }
    }

    $(window).on('resize', function(){
        resizeMenu();
    });

    $(document).ready(function(){
        var $menu = $('#top-megamenu-nav');
        if($(this).width() <= 768 && $menu.hasClass('mobile-menu-home')){
            $menu.css('display', 'block');
        }
    });

    resizeMenu();

</script>
<script type="text/javascript">
    $('#newsletterSubscribe2').validator().submit(function(e) {
        if (!e.isDefaultPrevented()) {
            e.preventDefault();
            submitNewletterReg2();
        }
    });
    function submitNewletterReg2() {
        formEl = $('#newsletterSubscribe2');
        url = formEl.attr('action');
        data = formEl.serialize();
        $.post(
            url,
            data,
            function(responseObj) {
                var message = responseObj['ret']['message'];
                var status = responseObj['ret']['status'];
                switch(status) {
                    case 'success' :{
                        toastr.success(message);
                        break;
                    }
                    case 'info':{
                        toastr.info(message);
                        if( responseObj['ret']['dismiss'] == true ) {
                            $('.newsletter-dismiss').click();
                        }
                        break;
                    }
                    case 'error': {
                        toastr.error(message);
                        break;
                    }
                    default: {
                        toastr.error('A intervenit o eroare! Va rugam, mai incercati!');
                    }break;
                }
            },
            'json'
        );
    }
</script>
