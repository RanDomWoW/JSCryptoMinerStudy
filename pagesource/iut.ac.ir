<script type="text/javascript" language="javascript">
var VST;// vertical slideshow timer
var t1=false,t2=false,t3=false,t4=false,t5=false;
function showContent(index,clear){
  	  _this = $("#s_menu_tabbar li.tab:eq(" + index + ")");
  	  $("#s_menu_tabbar li.tab").removeClass("selected");
  	  
  	  _this.addClass("selected").mouseenter();
  	  if($("#s_menu_content").children(":visible").index() != _this.index())
  	  {
	  	$("#s_menu_content").children().hide();
	  	var  _hh = $("#s_menu_content").height();
	  	$("#s_menu_content").height("auto");
	  	$("#s_menu_content").children(":eq(" + _this.index() + ")").fadeIn(500);	
	  	var  _hh2 = $("#s_menu_content").height();
	  	$("#s_menu_content").height(_hh);
	  	$("#s_menu_content").animate({height: _hh2 + 15},500);
  	  }
  }
	  
$(document).ready(function(){

	init_marquee();
	fix_exceptions();
	
	VST = setInterval('$("#next").click()',3000);
	
	$("#v_slideshow").parent().mouseenter(function(){
		clearInterval(VST);
	}).mouseleave(function(){
		VST = setInterval('$("#next").click()',3000);
	});
	
	$("#slideshow").simpleSlideshow({
		onLoaded: function(){
			t1 = setTimeout('showContent(1);',8000);
	  		t2 = setTimeout('showContent(2);',16000);
	  		t3 = setTimeout('showContent(3);',24000);
	  		t4 = setTimeout('showContent(4);',32000);
	  		t5 = setTimeout('showContent(5);',40000);
		}
	});
	
	  $("li.tab").mouseenter(function(){
	 

	 	newCSS={
	  		left : this.offsetLeft -4 ,
	  		width:$(this).outerWidth()+8
		}
		//li.css("position","relative");// :-|
		
		$("#slider").stop();
	  	$("#slider").animate(
	  		newCSS,
	  		{
				step: function(){
					$(this).css("overflow","visible");
				},
					easing: "easeOutCirc",
					duration: 650
			});
		
	  });
	  $("#s_menu_tabbar").mouseleave(function(){
	  	if( $("#s_menu_tabbar li.tab.selected").length == 0 )
	  		$("#slider").animate({left:$("#s_menu_tabbar").width()},"slow","",function(){
				$("#s_menu_tabbar li.tab").removeClass("hovered");
			});
		else{
			//li.css("position","static");
			$("#slider").animate(
				{
					left:$("#s_menu_tabbar li.tab.selected").get(0).offsetLeft -4,
					width:$("#s_menu_tabbar li.tab.selected").outerWidth()+8
				}
				,{
					step: function(){
						$(this).css("overflow","visible");
					},
					easing: "easeOutBounce",
					duration: 800
				}
			);
			//li.css("position","relative");
		}
	  });
	  
	  $("#s_menu_tabbar li.tab").click(function(event){
	  	showContent($(this).index(),false);
	  	show_sMenu();
	  	
	  	if(t1)
	  		clearTimeout(t1);
	  	if(t2)
	  		clearTimeout(t2);
	  	if(t3)
	  		clearTimeout(t3);
	  	if(t4)
	  		clearTimeout(t4);
	  	if(t5)
	  		clearTimeout(t5);
	  });
	  
	  //$("#s_menu_tabbar li.tab:last").click();
	  $("#s_menu_tabbar li.tab:eq(0)").click();
	  
	  
	  $("#s_menu_toggle").click(function(){
	  	$("#s_menu_toggle").toggleClass("closed");
	  	$("#s_menu_content").slideToggle();
	  });
	  
	  $("body").one("mousemove",function(){
	  	//$(document).scrollTop(100);
  		show_sMenu();
	  });
	  
	$("#s_menu_content").hide();

	
});

function fix_exceptions(){
	$("#s_menu_content").children().hide();// IE6

	/*if($.browser.version == "6.0") 
	{
		$("#s_menu_toggle").css("top","4px");// X(
			
		$(".box1 .title").each(function(){// X( png ha ro ke kharab neshoon mide, ba oon pngfix ham chap chin mishe
			$(this).text($(this).attr("title")).css("backgroundImage","none");
		});
	}*/
}
function show_sMenu(){
	$("#s_menu_toggle").removeClass("closed");
	$("#s_menu_content").slideDown();
} 
function search_focus(elem)
{
	if(elem.value=='جستجوی سایت')
		elem.value='';
	elem.style.color='#000000'
}
function search_blur(elem)
{
	if(elem.value=='')
		elem.value='جستجوی سایت';
	elem.style.color='#999999'
}
</script>

