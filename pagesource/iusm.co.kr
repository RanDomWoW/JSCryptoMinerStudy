			else if(cmd == 'next') { // 다음
				page_point_new_100 += 1;
				if(page_point_new_100 >= page_layer_count_new_100) page_point_new_100 = 0;
			}
			else if(cmd == 'current') { // 지정한 페이지
				page_point_new_100 = page_num_new_100 - 1;
