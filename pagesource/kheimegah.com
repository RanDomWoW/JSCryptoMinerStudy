function jafar_show_locs(locs,count){
	var myCenter=new google.maps.LatLng(33.1736668,53.4284654);
	clearOverlays_jafar(); //alert(locs.length);
	var infowindow = new google.maps.InfoWindow();
	var marker, i, j, cont = []; 
	for (i = 0; i < locs.length; i++) {
		var locations = locs[i];
		cont[i] = '';  
		for (j = 0; j < locations.length; j++) {  
				ela1 = locations[j][5];
				if( locations[j][5]!==locations[j][6] ){
					ela1 += (locations[j][6]) ? " الی "+locations[j][6] : "";
				}
				ela2 = locations[j][7];
				if( locations[j][7]!==locations[j][8] ){
					ela2 += (locations[j][8]) ? " الی "+locations[j][8] : "";
				}   
			cont[i] += "<div class='info_content'><h4 style='margin:0px;'>"+locations[j][0]+"</h4>"+ela1+"، ساعت "+ela2+" (<a href='/"+locations[j][1]+"/events/"+locations[j][2]+"' target='_blank'>"+locations[j][9]+")</a></div>";
		}
		
			marker = new google.maps.Marker({
				position: new google.maps.LatLng(locations[0][3], locations[0][4]),
				map: map, icon:'theme/files/flag.png'
			});
			markersArray.push(marker);

			google.maps.event.addListener(marker, 'click', (function(marker, i) {
			return function() {
				infowindow.close();
		 
				infowindow.setContent(cont[i]);
				infowindow.open(map, marker);
			}
		  })(marker, i));
	}
	map.setZoom(5);
	map.setCenter(myCenter);
}//FNC   

//});
}//FNC show map


var markersArray = [];  
function clearOverlays_jafar() {
  for (var i = 0; i < markersArray.length; i++ ) {
	markersArray[i].setMap(null);
  }
  markersArray.length = 0;
}







