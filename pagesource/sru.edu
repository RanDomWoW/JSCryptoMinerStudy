            document.addEventListener('click', function (e) {
                var x = e.target || e.originalTarget || e.srcElement;
                if (x.nodeName === 'A') {
                    var href = x.getAttribute('href');
                    if (href) {
                        var patt = new RegExp(".pdf");
                        if (patt.test(href)) {
