  $('.view-id-news_and_announcements .view-content').vTicker({
  showItems: 1
  });
  
$(".next").click(function(event){ 
				event.preventDefault();
				var checked = $("#animation").is(':checked');
				
				$('#news').vTicker('next', {animate:checked});
			});
		
		$(".prev").click(function(event){ 
				event.preventDefault();
				var checked = $("#animation").is(':checked');
				$('#news').vTicker('prev', {animate:checked});
			});
			
$(".down").click(function(event){ 
				event.preventDefault();
				var checked = $("#animation").is(':checked');
				
				$('#newit').vTicker('next', {animate:checked});
			});
		
		$(".up").click(function(event){ 
				event.preventDefault();
