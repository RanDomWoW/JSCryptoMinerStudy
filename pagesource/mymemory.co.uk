<script>
    (function($){
        $(function(){
            var $el = $('#modal-instacart'),
                $container = $('#js-instacart-container'),
                priceFormat = {"pattern":"\u00a3%s","precision":2,"requiredPrecision":2,"decimalSymbol":".","groupSymbol":",","groupLength":3,"integerRequired":1};

            $(Iweb.instacartInstance).on('add_to_cart', function () {
                var tpl = $('#js-instacart-loading-template').html();
                if (tpl) {
                    var rendered = Mustache.render(tpl);
                    $container.html(rendered);
                    $el.data('chopchop-toggle').activate();
                }
            });

            $(Iweb.instacartInstance).on('finish_queue_item', function (ev, data) {
                var status = data.response.responseJSON.status,
                    item = data.item,
                    offer = data.response.responseJSON.data.offer;
                if (status == 'success') {
                    var tpl = $('#js-instacart-template').html();
                    if (tpl) {
                        if (offer) {
                            offer.price = formatCurrency(offer.price, priceFormat)
                            if (offer.old_price) {
                                offer.old_price = formatCurrency(offer.old_price, priceFormat);
                            }
                        }
                        var rendered = Mustache.render(tpl, {
                            name: item.data.name,
                            price: formatCurrency(item.data.price, priceFormat),
                            thumbnail: item.originalData.mcextradata.thumbnail ? item.originalData.mcextradata.thumbnail : null,
                            alt: item.data.name,
                            offer: offer
                        });
                        var $rendered = $(rendered);
                        $rendered.find('.js-continue').toggle();
                        $container.html($rendered);
                    }
                } else {
                    if (item.originalData) {
                        var url = item.originalData.mcextradata['product-url'];
                        if (url) {
                            $el.data('chopchop-toggle').deactivate();
                            // normal error is that it is now out of stock
                            // send to the detail page where it should
                            // show the correct information
                            window.location.href = url;
                        }
                    }
                }
            });
        });
    })(jQuery);
</script>    </div>
