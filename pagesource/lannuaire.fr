		$("#list-deps2").toggle();
		if( $("#canvas_france").css("display") == "none"){
			$("#btn-idf").html("Île de France");

			map_mode = MAP_MODE_IDF;
		}else{
			$("#btn-idf").html("France entière");
			map_mode = MAP_MODE_DEP;
		}

		return false;
	});

	$("#letters").change(function(){
		letter = $("#letters").find(":selected").val();
	});
	$("#list-deps").click(function(){
		var sel = $("#list-deps").find(":selected").val();
		var a = sel.split('-');
		mapSelect("departement" + a[0]);
		gotoDep(sel);
	});
	$("#list-deps2").click(function(){
		var sel = $("#list-deps2").find(":selected").val();
		var a = sel.split('-');
		mapSelect("departement" + a[0]);
		gotoDep(sel);
	});

	$(".raz-form").click(function(){
		$("#"+$(this).attr("data-form")+" input").val("");
