                    <script type="text/javascript">
                        var ntfCurrentState = null;

                        var NTF_STATE_NEW = 1;
                        var NTF_STATE_COUPONEXISTS = 2;
                        var NTF_STATE_INELIGIBLE = 3;
                        var NTF_STATE_INELIGIBLE_PURCHASED = 4;
                        var NTF_STATE_HIDEALL = 5;

                        var ntfWidgetDialogCurrentState = null;
                        var ntfWidgetRibbonCurrentState = null;

                        var NTF_WIDGET_STATE_HIDE = 1;
                        var NTF_WIDGET_STATE_SHOW = 2;

                        var WIDTH_TA_TOASTER = 583;
                        var isIneligibleEmail = false;

                        var sectionURLForNTF = 'Direct Hit';

                        var useLessIntrusiveNTF = false;

                        function ntfInitWidgets() {
                            $.modal.close();
                            $('#ntf_dialog_enter_email').hide();
                            $('#ntf_dialog_show_couponcode').hide();
                            $('#ntf_dialog_email_invalid').hide();
                            $('#ntf_dialog_email_invalid_purchased').hide();

                            var ntf2_success = false;

                            if(ntfCurrentState == NTF_STATE_NEW) {
                                $('#ntf_dialog_enter_email').show();
                                setCookie("userSeenNtfDialogDate", "2018-03-17", 5*365); // 5 years
                            } else if(ntfCurrentState == NTF_STATE_COUPONEXISTS) {
                                if (useLessIntrusiveNTF) {
                                    $("#subsribe-deal").hide();
                                    $("#opt-in-deal").show();
                                    ntf2_success = true;
                                } else {
                                    $('#ntf_dialog_show_couponcode').show();
                                }
                            } else if(ntfCurrentState == NTF_STATE_INELIGIBLE) {
                                ntf2_success = true;
                                $('#ntf_dialog_email_invalid').show();
                            } else if(ntfCurrentState == NTF_STATE_INELIGIBLE_PURCHASED) {
                                $('#ntf_dialog_email_invalid_purchased').show();
                            } else if(ntfCurrentState == NTF_STATE_HIDEALL) {
                                $('#ntf_dialog_all').remove();
                                $('#ntfContainer').remove();
                            }
                            $('#ntf_ribbon_container').hide();
                            if(ntfWidgetDialogCurrentState == NTF_WIDGET_STATE_HIDE) {
                            } else if(ntfWidgetDialogCurrentState == NTF_WIDGET_STATE_SHOW) {
                                if (!useLessIntrusiveNTF) {
                                    $.modal({
                                        open: 'ntf_dialog_all',
                                        triggerOpen: true,
                                        padding: false,
                                    });
                                    $('#ntfContainer').show();
                                } else if (!ntf2_success) {
                                    $("#deal-up").slideToggle(400,function(){});
                                }
                            }

                            if(ntfWidgetRibbonCurrentState == NTF_WIDGET_STATE_HIDE) {
                            } else if(ntfWidgetRibbonCurrentState == NTF_WIDGET_STATE_SHOW) {
                                if (useLessIntrusiveNTF) {
                                    $('#deal-block').show();
                                } else {
                                    $('#ntf_ribbon_container').show();
                                    $('#ntfContainer').show();
                                }
                            }
                        }

                        // logic to hide and show deal when user interacts
                        var dealTimeout;
                        function hideDeal() {
                            if ($("#deal-up").is(":visible")) {
                                dealTimeout = setTimeout(function(){
                                    $("#deal-up").slideToggle(400,function(){
                                        $("#deal-block").toggle();
                                    });
                                }, 5000);
                            }
                        }
                        hideDeal();

                        $(document).on("click mouseenter keyup focusin", "#deal-up", function(e){
                            e.stopPropagation();
                            e.preventDefault();
                            clearTimeout(dealTimeout);

                        });
                        $(document).on("mouseleave", "#deal-up", function(e){
                            e.stopPropagation();
                            e.preventDefault();
                            hideDeal();
                        });

                        $("#ntf_ribbon").click( function() {
                            ntfWidgetDialogCurrentState = NTF_WIDGET_STATE_SHOW;
                            ntfWidgetRibbonCurrentState = NTF_WIDGET_STATE_HIDE;

                            ntfInitWidgets();
                        });

                        $("#ntf_ribbon_close").click( function() {
                            ntfWidgetDialogCurrentState = NTF_WIDGET_STATE_HIDE;
                            ntfWidgetRibbonCurrentState = NTF_WIDGET_STATE_HIDE;
                            ntfInitWidgets();
                            if(ntfCurrentState == NTF_STATE_NEW)  {
                                setCookie("name.cookie.ntf.no.ribbon", true, 1);
                            }
                        })

                        $("#ntf_dialog_close").click( function() {
                            if(isIneligibleEmail) {
                                ntfWidgetRibbonCurrentState = NTF_WIDGET_STATE_HIDE;
                            } else {
                                ntfWidgetRibbonCurrentState = NTF_WIDGET_STATE_SHOW;
                            }
                            ntfWidgetDialogCurrentState = NTF_WIDGET_STATE_HIDE;
                            ntfInitWidgets();
                        })


                        $("#ntf_just_let_shop").click( function() {
                            ntfWidgetRibbonCurrentState = NTF_WIDGET_STATE_SHOW;
                            ntfWidgetDialogCurrentState = NTF_WIDGET_STATE_HIDE;
                            ntfInitWidgets();
                        })

                        $("#ntf_continue_button_fail, #ntf_continue_button_fail_x").click( function() {
                            ntfWidgetDialogCurrentState = NTF_WIDGET_STATE_HIDE;
                            ntfWidgetRibbonCurrentState = NTF_WIDGET_STATE_HIDE;
                            ntfInitWidgets();
                        });

                        $("#ntf_continue_button_success, #ntf_continue_button_success_x").click( function() {
                            ntfWidgetDialogCurrentState = NTF_WIDGET_STATE_HIDE;
                            ntfWidgetRibbonCurrentState = NTF_WIDGET_STATE_SHOW;
                            ntfInitWidgets();
                        });

                        $("#ntf_user_email_elt").focus( function() {
                            toggleInvalidEmailAddress(false);
                        })


                        $("#ntf-submit-button").click( function() {
                            var emailAddress = $("#ntf_user_email_elt").val();
                            var checkboxValue = $('#ntf_gender_group input[name=email-pref]:checked').val()

                            var params = {};
                            params['emailAddress'] = emailAddress;
                            params['subscriptionGender'] = checkboxValue;
                            params['sectionURL'] = sectionURLForNTF;
                            params['useLessIntrusiveNTF'] = useLessIntrusiveNTF;
                            // $("#ntf-submit-button").attr('disabled', true);
                            var referrer = 'Direct Hit';
                            var link = "\/r\/Homepage.jsp";
                            var query =  "";
                            var uri = link + query + (window.location.hash != '' ? ('&page=' + window.location.hash.replace('#', '')) : '');

                            if (uri.indexOf("EditorialList.jsp") != -1 || uri.indexOf("HotList.jsp") != -1 || uri.indexOf("HotListMobile.jsp") != -1
                                    || uri.indexOf("LookBook.jsp") != -1 || uri.indexOf("LookBookMobile.jsp") != -1) {
                                uri = window.location.href
                            }

                            addPageParamsInfo(params, uri, referrer);

                            if(emailAddress == '') {
                                toggleInvalidEmailAddress(true);
                            } else {
                                $.ajax({
                                    url: '/r/ajax/NtfDialogSubmit.jsp',
                                    data: params,
                                    contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                                    type: "POST",
                                    cache: false,
                                    success: function (msg) {
                                        var msgJson = JSON.parse(msg);
                                        var result = msgJson['key.dialogsubmit.result'];
                                        var promoCode = msgJson['key.dialogsubmit.promocode'];

                                        if (result == "value.dialogsubmit.ineligible") { // testcancel@revolvemail.com
                                            isIneligibleEmail = true;
                                            ntfCurrentState = NTF_STATE_INELIGIBLE;
                                            ntfWidgetDialogCurrentState = NTF_WIDGET_STATE_SHOW;
                                            ntfWidgetRibbonCurrentState = NTF_WIDGET_STATE_HIDE;
                                            if (useLessIntrusiveNTF) {
                                                $('#rejected-email').html(emailAddress);

                                                $("#subsribe-deal").hide();
                                                $("#deal-email-exists").show();
                                            } else {
                                                $("#ntf_disclaimer").show();
                                                $("#ntf_just_let_shop").hide();

                                            }
                                            ntfInitWidgets();
                                        } else if (result == "value.dialogsubmit.ineligible.purchased") { // testcancel@revolvemail.com
                                            isIneligibleEmail = true;
                                            ntfCurrentState = NTF_STATE_INELIGIBLE_PURCHASED;
                                            ntfWidgetDialogCurrentState = NTF_WIDGET_STATE_SHOW;
                                            ntfWidgetRibbonCurrentState = NTF_WIDGET_STATE_HIDE;
                                            $("#ntf_disclaimer").show();
                                            $("#ntf_just_let_shop").hide();

                                            ntfInitWidgets();

                                        } else if (result == "value.dialogsubmit.newcode" || result == "value.dialogsubmit.codeexists") {
                                            ntfCurrentState = NTF_STATE_COUPONEXISTS;
                                            ntfWidgetDialogCurrentState = NTF_WIDGET_STATE_SHOW;
                                            ntfWidgetRibbonCurrentState = NTF_WIDGET_STATE_HIDE;

                                            $("#ntf_just_let_shop").hide();
                                            showNtfDialogWithPromoCode(emailAddress, promoCode);
                                            ntfInitWidgets();
                                        } else if (result == "value.dialogsubmit.invalid") {
                                            ntfCurrentState = NTF_STATE_NEW;
                                            ntfWidgetDialogCurrentState = NTF_WIDGET_STATE_SHOW;
                                            ntfWidgetRibbonCurrentState = NTF_WIDGET_STATE_HIDE;

                                            toggleInvalidEmailAddress(true);
                                        } else {
                                            console.log("Possible programming error: result not recognized, result[" + result + "], promoCode[" + promoCode + "]");
                                        }
                                    }
                                });
                            }
                        });

                        function showNtfDialogWithPromoCode(emailAddress, promoCode) {
                            if (useLessIntrusiveNTF) {
                                $('#accepted-email').html(emailAddress);
                                $('#signup-code').html(promoCode);
                            } else {
                                $("#ntf_dialog_email_address").html(emailAddress);
                                $("#ntf_dialog_promo_code").html(promoCode);
                            }
                        }

                        function toggleInvalidEmailAddress(isInvalid) {
                            if (useLessIntrusiveNTF && isInvalid) {
                                // not successful validated email
                                if ($("#ntf_user_email_elt").val().trim() == "")
                                    $("#deal-error").html("An email is required!");
                                else
                                    $("#deal-error").html("please enter a valid email. Example: name@domain.com");
                                $("#deal-error").css({"display":"block", "color": "#da3636"});
                            } else {
                                if (isInvalid) {
                                    $("#ntf_user_email_elt_container").addClass("is-error");
                                } else {
                                    $("#ntf_user_email_elt_container").removeClass("is-error");

                                }
                            }
                        }

                        // hide and show the deal
                        function thisDeal() {
                            $("#deal-up").slideToggle(400,function(){
                                $("#deal-block").toggle();
                            });
                        }
                        
                                        ntfCurrentState = NTF_STATE_NEW;
                                        ntfWidgetDialogCurrentState = NTF_WIDGET_STATE_SHOW;
                                        ntfWidgetRibbonCurrentState = NTF_WIDGET_STATE_HIDE;

                                        setCookie("userClosedNtfDialogCount", 1, 5*365); // 5 years
                                        setCookie("userLastSeenNtfDialogDate",  "2018-03-17", 5*365); // 5 years
                                    
                        ntfInitWidgets();
                    </script>
                


