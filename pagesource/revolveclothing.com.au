    function insertPageTrack(addToCartFlag, productCode, isSearchRedirect, callback, errorCallback, cookiesToPassToPageTrack) {
        var referrer = 'null';
        var link = "http:\/\/www.revolveclothing.com.au\/r\/Homepage.jsp";
        var query =  "";
        var uri = link + query + (window.location.hash != '' ? ('&page=' + window.location.hash.replace('#', '')) : '');
