            smsParams['addToCart'] = true;
        }

    }

    function updatePageParamsInfoForSearchRedirect(smsParams) {
        smsParams['newFlag'] = 'search';
        smsParams['subsubcat'] = 'null';
    }


    function getSubmissionType(smsParams) {
        var type = 'GET';

        var counter = 0;
        for(var i in smsParams) {
            if(typeof(smsParams[i]) == 'string') {
                counter += smsParams[i].length;
            }
        }
        // number can be as high as 2400, but keep it lower to be safe
        if(counter > 4700) {
            type='POST';
        }
        return type;
    }

