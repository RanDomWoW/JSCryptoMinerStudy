 <script>
     jQuery(document).ready(function ($) {

         // Back to top smooth scroll
         var offset = 300,
                 offsetOpacity = 1200,
                 scrollDuration = 700,
         toTop = $('#js-to-top');

         $(window).scroll(function () {
             ( $(this).scrollTop() > offset ) ? toTop.addClass('is-visible') : toTop.removeClass('is-visible');
         });

         toTop.on('click', function(event){
             event.preventDefault();
             $('body,html').animate({
                     scrollTop: 0 ,
                 }, scrollDuration
             );
         });


         // Toggle/Remove Class via data attr
         var toggleHandle = $('.js-toggle-handle');
         var toggledClasses = [];

         // Toggle class function
         toggleHandle.click(function(e){
             e.preventDefault();

             var toggleTarget  = $(this).data('toggle-target'),
                 toggleClass   = $(this).data('toggle-class'),
                 objTarget    = $(toggleTarget),
                 bareClass     = toggleClass.split('.').join(''),
                 inArray       = $.inArray(bareClass, toggledClasses),
                 isClassInEl   = objTarget.hasClass(bareClass);

             if (inArray == -1) {
                 toggledClasses.push(bareClass);
             }

             $.each(toggledClasses, function(i, v){
                 objTarget.removeClass(toggledClasses[i]);
             });

             if(isClassInEl) {
                 objTarget.removeClass(bareClass);
             } else {
                 objTarget.addClass(bareClass);
             }
         });
     });
 </script>

