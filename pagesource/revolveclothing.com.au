}


function heartProduct(elem, pcode,size,view){

 	var bn = elem;
	var brand = false;
	var plpHeartClass = "plp_main_img_heart";
	var plpHeartActiveClass = "active";
	if(blueHeart){
		plpHeartClass = "plp_main_img_heartm";
// 		plpHeartActiveClass = "activem";

	}

	if(typeof arguments[2] !== "undefined" && arguments[2] === "brand" ) brand = true;

	if(typeof elem.id === "undefined" || (elem.id !== "heart_active_"+pcode && $('div.active', elem).length == 0)){
		//alert("to add");
		if(brand)	submitHeart("brand", pcode, "add");
		else submitHeart("product", pcode, "add");
		for(var j=0; j< bn.childNodes.length; j++){
 			var bn2 = bn.childNodes[j];
// 		 	alert("j="+j+"=="+bn2.nodeType+", "+bn2.nodeName+", "+bn2.tagName+", "+bn2.className );
	 		switch(bn2.nodeType){
			case 3:
			   //text Node
			   break;
			case 1:
			   //Element node (anchor, div)
			   if(bn2.nodeName.toLowerCase() === "div" || bn2.className.toLowerCase() === plpHeartClass ){
				   bn2.className += " "+plpHeartActiveClass;
			   }
			   break;
			default:
			    //break;
			}
 		}
 		elem.id = "heart_active_"+pcode;


 		 if(typeof arguments[3] === "undefined"){

 			if(document.getElementById("plp_quickview_wrap_"+pcode)!=null){
	 			var pd = document.getElementById("plp_quickview_wrap_"+pcode);
	 			heartingUnhearting(pd,pcode,"add",plpHeartActiveClass);
	 		}
 		}else{

 			if(arguments[3]  === "qv"){

 				if(document.getElementById("plp_prod_wrap_"+pcode)!=null){
 					var pd = document.getElementById("plp_prod_wrap_"+pcode);
 					heartingUnhearting(pd,pcode,"add",plpHeartActiveClass);
 				}

 			}

 		}

	}else{
		//alert("to remove");
		if(brand)	submitHeart("brand", pcode, "remove");
		else{

			//alert(typeof arguments[2]);
			if(typeof arguments[2] !== "undefined" && arguments[2] !== ""){
				//alert(size);
				submitHeart("product", pcode, "remove",size);
			}else{
				submitHeart("product", pcode, "remove");
			}

		}
		for(var j=0; j< bn.childNodes.length; j++){
 			var bn2 = bn.childNodes[j];
// 		 	alert("j="+j+"=="+bn2.nodeType+", "+bn2.nodeName+", "+bn2.tagName+", "+bn2.className );
	 		switch(bn2.nodeType){
			case 3:
			   //text Node
			   break;
			case 1:
			   //Element node (anchor, div)
			   if(bn2.nodeName.toLowerCase() === "div" || bn2.className.toLowerCase() === plpHeartClass ){
				   if(bn2.className.toLowerCase().indexOf(plpHeartActiveClass) !== -1 )	bn2.className = plpHeartClass;
			   }
			   break;
			default:
			    //break;
			}
 		}
 		elem.id = "";



 		 if(typeof arguments[3] === "undefined"){

 			if(document.getElementById("plp_quickview_wrap_"+pcode)!=null){
	 			var pd = document.getElementById("plp_quickview_wrap_"+pcode);
	 			heartingUnhearting(pd,pcode,"remove",plpHeartClass);
 			}
 		}else{

 			if(arguments[3]  === "qv"){

 				if(document.getElementById("plp_prod_wrap_"+pcode)!=null){
 					var pd = document.getElementById("plp_prod_wrap_"+pcode);
 					heartingUnhearting(pd,pcode,"remove",plpHeartClass);
 				}

 			}

 		}


	}

}
function submitHeart(paramtype, paramvalue){
    if(typeof paramtype ==="undefined" || typeof paramvalue ==="undefined" ) return;
	if(typeof arguments[2] ==="undefined" ) return;
	var psize = "";
	// optional argument
	if(typeof arguments[3] !=="undefined" ) {
		psize = arguments[3];
	}
    var sectionUrl = "";
    if(typeof arguments[4] !=="undefined" ) {
        sectionUrl = arguments[4];
    }
    var callback = null;
	if (typeof arguments[6] !== 'undefined'){
		callback = arguments[6];
	}
	var onSuccess = arguments[7];
    /*
	if (window.XMLHttpRequest){
		// for IE7+, FF, Chrome, Opera, Safari
		xhttp2 = new XMLHttpRequest();
	}
	else{
		// for IE6 and older
		xhttp2 = new ActiveXObject("Microsoft.XMLHTTP");
	}
	var URL = "/r/ajaxHeartProduct.jsp?";
	var dataToSend="";
	if(paramtype === "product")	{
		dataToSend += "&pcode="+encodeURIComponent(paramvalue);
	}else if(paramtype === "brand")	{
		dataToSend += "&bcode="+encodeURIComponent(paramvalue);
	}
	dataToSend += "&action_T="+encodeURIComponent(arguments[2]);
	if(psize !=="")	dataToSend += "&psize="+encodeURIComponent(psize);

// 	alert(dataToSend);

// 	xhttp2.open(method, URL, async);
	xhttp2.open("POST", URL, false); // async
	xhttp2.setRequestHeader("Content-type","application/x-www-form-urlencoded"); // this is needed
	xhttp2.send(dataToSend);
	var res = xhttp2.responseText;



		updateHeartCount();
		*/
    var dataToSend="";
	var department = $('#header-jsp').data('department');
    if(paramtype === "product")	{
        dataToSend += "&pcode="+encodeURIComponent(paramvalue);
    }else if(paramtype === "brand")	{
        dataToSend += "&bcode="+encodeURIComponent(paramvalue);
    }
    dataToSend += "&action_T="+encodeURIComponent(arguments[2]);
    if(mensHeart)dataToSend += "&mensBrand="+encodeURIComponent("true");
    else dataToSend += "&mensBrand="+encodeURIComponent("false");
    if(kidsHeart)dataToSend += "&kidsBrand="+encodeURIComponent("true");
    else dataToSend += "&kidsBrand="+encodeURIComponent("false");
    if(psize !=="")
        dataToSend += "&psize="+encodeURIComponent(psize);
    if(sectionUrl !=="")	dataToSend += "&sectionURL="+encodeURIComponent(sectionUrl);
	if (department)
		dataToSend += "&d="+encodeURIComponent(department);
 	var action_T = arguments[2];
    $.ajax({
        type: 'POST',
        url: '/r/ajaxHeartProduct.jsp',
        data: dataToSend,
        success: function(response){
            /*
            // alert("jsp response="+res);
            res = res.replace(/^\s+|\s+$/g,"");
            // alert("jsp response="+res);
            */
			if(paramtype === "product") {
                updateHeartCount(response);
                if (response.action === 'add') {
                    insertCordialUpdateHeartItem('add', paramvalue);
                } else if (response.action === 'remove') {
                    insertCordialUpdateHeartItem('remove', paramvalue);
                }
				if(typeof gaUpdateHeartItem === 'function'){
					gaUpdateHeartItem(response.action,paramvalue);
				}
            } else if(paramtype === "brand"){
				if(typeof gaUpdateHeartBrand === 'function'){
					gaUpdateHeartBrand(response.action,paramvalue);
				}
			}
			if (typeof callback === 'function'){
				callback();
			}
			if(response.success) {
				if(typeof onSuccess === 'function') {
					onSuccess();
				}
			}
        }
    });
}

function updateHeartCount(rtnData){
	var count = eval($("#heart_total_count").html());
	if (rtnData.success) {
		if (rtnData.action === 'add')
			$("#heart_total_count").html(count + 1);
		else if (rtnData.action === 'remove')
			$("#heart_total_count").html(count - 1);
	}
}

function reloadHearts(){
    $.ajax({
        type: 'POST',
        url: '/r/ajax/ReloadHeartItems.jsp',
   		dataType: 'json',
        success: function(response){
        	for (var i=0; i < response.length; i++) {
        		$(".plp_prod_a_" + response[i]).attr("id","heart_active_" + response[i]);
        		$("#plp_prod_wrap_"+response[i]+" .plp_main_img_heart").attr("class", "plp_main_img_heart active");
        	}
        }
    });
}

