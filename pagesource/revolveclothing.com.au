<script type="text/javascript">


	var blueHeart = false;



var mensHeart = false;
var kidsHeart = false;



function heartBrand(elem, bcode){
    heartProduct(elem, bcode, "brand");
}

function heartProductUpdated(element, productCode, callback){
	var container = $(element);
	var activeHeartClass = 'favorite-button--active';
	var productName = container.data('product-name');

	if (container.hasClass(activeHeartClass)){
		submitHeart('product', productCode, 'remove', undefined, undefined, undefined, undefined, function() {
            container.removeClass(activeHeartClass);
		} );

	} else {
		var isGuest = $('#myFavLimitPopup').data('isGuest');
		var productSize = eval($("#heart_total_count").html());
		if (isGuest) {
			$('#guestLimitPop, #fav-create-link').attr('href', "/r/SignIn.jsp?favcode=" + productCode +"&page=" + encodeURIComponent(window.location.pathname + window.location.search));
			$.modal({triggerOpen:true, open: "guestLimitPop", type: "small"});
			return;
		} else if (!isGuest && productSize > 1999) {
			$.modal({triggerOpen:true, open: "userLimitPop"});
			return;
		}

		submitHeart('product', productCode, 'add', undefined, undefined, undefined, undefined, function() {
            container.addClass(activeHeartClass);
		});

	}
	setHeartScreenReader(element);

	if (typeof callback === 'function') {
		callback(element, productCode);
	}
}

function setHeartScreenReader(elem) {
	if ($(elem).hasClass('favorite-button--active')) {
		$(elem).find('.u-screen-reader').html($(elem).data('unfavorite-message'));
		$('#favAssist').html($(elem).data('favorite-notification'));
	} else {
		$(elem).find('.u-screen-reader').html($(elem).data('favorite-message'));
		$('#favAssist').html($(elem).data('unfavorite-notification'));
	}
}

function heartBrandUpdated(elem, bcode){
    var container = $(elem);
    var activeHeartClass = 'favorite-button--active';
    if (container.hasClass(activeHeartClass)){
        container.removeClass(activeHeartClass);
        submitHeart('brand', bcode, 'remove');
    } else {
        container.addClass(activeHeartClass);
        submitHeart('brand', bcode, 'add');
    }
}

function clearOldFavItems() {
	$.ajax({
		type: 'GET',
		url: '/r/ajaxHeartProduct.jsp?action_T=clearolditems',
		success: function (data) {
			location.reload();
		}
	});
}

function closeLimitLightBox(elemId) {
