                Slider.initialize({
                    key :           'current--responsive-slider-feb-2017',
                    delay :         '24000',
                    padding :       '0',
                    borderRadius :  '0'
                });
            
                if( Slider.getStep() == "-1" ) {

                    // go to the thank you message
                    Slider.gotoStep(3);

                } else {
                
                    // Set the slider to the state it was last loaded from
                    Slider.gotoStep( Slider.getStep() );

                    // set the slider email address so it posts
                    jQuery("#email_address").val( Slider.getEmail() );

                    // add extra tracking fields
                    jQuery("#sliderForm")
                        .append("<input type='hidden' name='screen_width' value='" + screen.width + "' />")
                        .append("<input type='hidden' name='screen_height' value='" + screen.height + "' />")
                        .append("<input type='hidden' name='user_agent' value='" + navigator.userAgent + "' />");

                } // end of cookie else
           
                // add slider js
                jQuery( document ).ready( function(){

    // step one
    jQuery("#emailSubmit").click(function(event){

        //on mobile, take one extra click to show the fields
        /*
        if( jQuery( ".fc-slider-mobile-test" ).css( "display" ) == "inline"  &&  !jQuery("#emailSubmit").hasClass( "clicked" ) ){
            jQuery( ".fc-step-full--step1" ).slideDown();
            jQuery("#emailSubmit").addClass( "clicked" );
            event.preventDefault();
            return false;
        }
        */

        var dataForm = new VarienForm('sliderForm', true);

        // validate the form fields with prototype
        if( dataForm.validator.validate() ) {

            // set the submit button to be disabled
            jQuery("#emailSubmit").prop('disabled', true);
            jQuery("#slider_source").val( Slider.getSource() );

            Slider.setEmail( jQuery("#email_address").val() );
            Slider.saveForm(2);

        }

        event.preventDefault();
    });

    // step two
    jQuery("#catalogSubmit").click(function(event){
        var dataForm = new VarienForm('sliderForm', true );

        // set the catalog fields to be required
        jQuery("#sliderForm #firstname").addClass('required-entry');
        jQuery("#sliderForm #lastname").addClass('required-entry');
        jQuery("#sliderForm #address1").addClass('required-entry');
        jQuery("#sliderForm #city").addClass('required-entry');
        jQuery("#sliderForm #state").addClass('required-entry');
        jQuery("#sliderForm #zip").addClass('required-entry');

        // set the catalog to true
        jQuery("#catalog").val(1);
        jQuery("#eventId").val(3164);

        // validate the form fields with prototype
        if( dataForm.validator.validate() ) {

            // set the submit button to be disabled
            jQuery("#catalogSubmit").prop('disabled', true);
            jQuery("#slider_source").val( Slider.getSource() );

            Slider.submitForm();
        }

        event.preventDefault();
    });

    // close the modal window
    jQuery("#closeSlider").click(function(event){
        jQuery.lightbox.close();
    });

    jQuery("#skipCatalog").click(function(event){
        // remove the catalog field requirements
        jQuery("#sliderForm #firstname").removeClass('required-entry');
        jQuery("#sliderForm #lastname").removeClass('required-entry');
        jQuery("#sliderForm #address1").removeClass('required-entry');
        jQuery("#sliderForm #city").removeClass('required-entry');
        jQuery("#sliderForm #state").removeClass('required-entry');
        jQuery("#sliderForm #zip").removeClass('required-entry');

        // set the catalog to false
        jQuery("#catalog").val(0);

        Slider.submitForm();

        event.preventDefault();
    });

});
                
