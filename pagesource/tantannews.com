    <script>
        $.ajaxSetup({
            headers: { 'X-CSRF-TOKEN': 'FgxliKHMdLwxR4xjnrNr6cplFr4KJYf74dex2n5a' }
        });

        truncate('.xxm--truncate');
        lazyload(document.querySelectorAll('.lazyload'));

        if (window.location.hash && window.location.hash == '#_=_' ) {
            if ( window.history && history.pushState ) {
                window.history.pushState("", document.title, window.location.pathname);
            } else {
                // Prevent scrolling by storing the page's current scroll offset
                var scroll = {
                    top: document.body.scrollTop,
                    left: document.body.scrollLeft
                };
                window.location.hash = '';
                // Restore the scroll offset, should be flicker free
                document.body.scrollTop = scroll.top;
                document.body.scrollLeft = scroll.left;
            }
        }

        function closeDrawer() {
            $('.xm--nav-acc-drawer, .xm--drawer-overlay').toggleClass("xxm--hide");
            $('html').toggleClass("xm--fixed");
        }

        $('#acc-drawer-btn, .xm--drawer-close-btn, .xm--drawer-overlay').on('click', function(e) {
            closeDrawer();
            e.preventDefault();
        });

        $('#nav-ex-btn').on('click', function (e) {
            $('.xm--nav-ex').toggleClass("xxm--hide");
            $('#nav-ex-btn').toggleClass("xxm--active");
            e.preventDefault();
        });

        $('#show-searchbar, #close-searchbar').on('click', function(e) {
            $('#searchbar').toggleClass('xxm--hide');

            if ($('#searchbar-input').is(':focus')) {
                $('#searchbar-input').blur();
            } else {
                $('#searchbar-input').focus();
            }

            e.preventDefault();
        });

        $(function() {
            $(".xm--drawer-scroll").niceScroll();
        });

        $('.to-top').click(function (e) {
            $('html, body').animate({scrollTop: $($(this).attr('href')).scrollTop()}, 1000);
            e.preventDefault();
        });

        // Login & Register modal
        function showLoginModal() {
            $('.xm--modal-overlay').show();
            $('#login-modal').fadeIn();
            $('#modal_login_email').focus();
            $('.register-modal').hide();
        }

        function hideLoginModal() {
            $('.xm--modal-overlay').hide();
            $('#login-modal').fadeOut();
        }

        function showRegisterModal($toggle) {
            $('.xm--modal-overlay').show();
            $('#register-modal').fadeIn();
            $('.register-modal').focus();
            $('.login-modal').hide();
        }

        function hideRegisterModal($toggle) {
            $('.xm--modal-overlay').hide();
            $('#register-modal').fadeOut();
        }

        $('.btn-login-modal').click(function () {
            closeDrawer();
            showLoginModal();
        });

        /*$('.btn-register-modal').click(function () {
            showRegisterModal();
            return false;
        });*/

        $('.xm--modal-overlay, .login-modal-close-btn').click(function () {
            hideLoginModal();
        });

        $(document).keyup(function (e) {
            if (e.keyCode === 27) { // Esc key
                hideLoginModal();
                showRegisterModal();
            }
        });

        

        function reseAlert() {
            $('.alert-message').hide();
            $('.alert-message').text('');
        }

        function showNotify(msg) {
            $('.alert-message').show();
            $('.alert-message').append('<li>' + msg + '</li>');
        }
    </script>
    <script>


    var loading = false;
    $('#btn-load-more').click(function () {
        if (loading) return;
        loading = true;

        $('#txt-load-more').toggleClass('hidden');
        $('#home-loader').toggleClass('hidden');

        $.ajax({
            url: 'http://tantannews.com',
            type: 'GET',
            data: {
                page: $('#story-list>li').length
            },
            success: function (response) {
                $('#story-list').append(response);
                $('#txt-load-more').toggleClass('hidden');
                $('#home-loader').toggleClass('hidden');

                if (response == '') {
                    $('#txt-load-more').html('已刷新到最底页');
                } else {
                    lazyload(document.querySelectorAll('.lazyload'));
                    loading = false;
                }
            }
        });
    });
</script>
    
    
    
    
    
    
    

    
    
    
    
    
