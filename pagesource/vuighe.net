<modal :show.sync="show.login" class="login-form hide" v-bind:class="{'hide': !show.login, 'show': show.login}" v-if="show.login">
    <h3 slot="header">ĐĂNG NHẬP</h3>
    <div slot="body">
    	<div class="modal-group">
    		<input type="text" name="username" placeholder="tên đăng nhập" v-model="user.username" v-on:keyup="notice.user.type = ''" v-on:keyup.13="login">
    		<i class="icon-user"></i>
    	</div>
    	<div class="modal-group">
        	<input type="password" name="password" placeholder="mật khẩu" v-model="user.password" v-on:keyup="notice.user.type = ''" v-on:keyup.13="login">
        	<i class="icon-lock"></i>
        </div>
    	<div class="notice" v-show="notice.user.type == 'error'">{{ notice.user.message }}</div>
    	<div class="modal-group">
        	<input type="submit" value="Đăng nhập" v-on:click="login" v-bind:class="{'disable': lock.login}">
        	<div class="loading input-loading login-loading">
        		<span></span><span></span><span></span><span></span><span></span>
        	</div>
        </div>
    	<div class="modal-group">
    		<div class="left-column remember-password">
	        	<input id="password_remember" type="checkbox" v-model="user.is_remember" checked="true"> <label for="password_remember">Ghi nhớ</label>
	        </div>
	        <div class="right-column lost-password">
	        	<a href="//id.vuighe.net/quen-mat-khau" target="_blank">Quên mật khẩu</a>
	        </div>
        </div>
