			<script type="text/javascript">

				iFrameResize({checkOrigin: false, heightCalculationMethod: 'max'});

				$(function() {

					var startButton = $('#slide_start_button').hide();
					var pauseButton =  $('#slide_stop_button').show();

					var slider = $('.slider');

					$('.inner').fadeIn(1000);

					slider.slick({
						slidesToShow: 2,
						slidesToScroll: 1,
						autoplay: true,
						autoplaySpeed: 6000,
						speed: 250,
						dots: false,
						centerPadding: 10,
						draggable: false,
						pauseOnHover: true
					});

					$('#slide_toggle_button').click(function() {
						isPaused() ? start() : pause();
					});

					slider.on('click', '.slick-arrow', function() {
						if (isPaused()) start();
					});

					slider.on('beforeChange', function(event, slick, currentSlide, nextSlide) {
						toggleButton('start');
					});

/*					slider.on('mouseenter', '.slick-slide', function() {
						toggleButton('stop');
					}).on('mouseleave', '.slick-slide', function() {
						toggleButton('start');
					});*/

					function isPaused() {
						return pauseButton.is(':hidden');
					}

					function start() {
						toggleButton('start');
						slider.slick('slickPlay');
					}

					function pause() {
						toggleButton('pause');
						slider.slick('slickPause');
					}

					function toggleButton(state) {
						if (state == 'start') {
							startButton.hide();
							pauseButton.show();
						}
						else {
							startButton.show();
							pauseButton.hide();
						}
					}
				});
			</script>
