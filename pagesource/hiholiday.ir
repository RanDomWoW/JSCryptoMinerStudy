<script>
    $(function () {
        initializeHeaderSearchBox();
    });

    function initializeHeaderSearchBox() {
        $("body").click(function () {
            closePopups();
        });
        $('#capacityTicker,#cityList').click(function (e) {
            e.stopPropagation();
        });
        $('.radio').click(function () {
            $('input:radio').prop("checked", false);
            $(this).find('input:radio').prop("checked", true);
        });
        $('[data-change-city]').click(function () {
            if ($(this).data('is-rotated')) {
                $(this).data('is-rotated', false);
                $(this).css('transform', 'rotate(0deg)');
            } else {
                $(this).data('is-rotated', true);
                $(this).css('transform', 'rotate(180deg)');
            }

            var form = $(this).parents('form');
            var departureCityName = form.find('#departureName');
            var departureCityCode = form.find('#departureCode');
            var arrivalCityName = form.find('#arrivalName');
            var arrivalCityCode = form.find('#arrivalCode');
            var departureCityNameVal = departureCityName.val();
            var departureCityCodeVal = departureCityCode.val();
            var arrivalCityNameVal = arrivalCityName.val();
            var arrivalCityCodeVal = arrivalCityCode.val();
            departureCityName.val(arrivalCityNameVal);
            departureCityCode.val(arrivalCityCodeVal);
            arrivalCityName.val(departureCityNameVal);
            arrivalCityCode.val(departureCityCodeVal);
        });
        var list = $('#cityList');
        var capacityTicker = $('#capacityTicker');
        var selectCityIcon, firedForm, calendar, selectCapacityIcon;
        function closePopups() {
            if (!!list)
                list.hide();

            if (!!capacityTicker)
                capacityTicker.hide();

            if (!!calendar)
                calendar.hide();
        }

        function createDatePicker(element, startField) {
            new timeFrame(element, {
                startField: startField,
                endField: startField,
                earliest: new Date(),
                format: '%Y-%m-%d',
                months: 4,
                maxRange: 1
            });
        }
        function setCityText(element) {
            list.hide();

            var code = $(element).data('code');
            var name = $(element).data('name');

            var dl = $(selectCityIcon).data('select-city');
            var form = $(selectCityIcon).parents('form');

            var cityName = form.find('#' + dl + 'Name');
            var cityCode = form.find('#' + dl + 'Code');
            cityName.val(name);
            cityCode.val(code);
        }
        function renderCities() {

            var popular =
            [
                { RegionName: 'تهران', Code: 'THR' }, { RegionName: 'مشهد', Code: 'MHD' },
                { RegionName: 'شیراز', Code: 'SYZ' }, { RegionName: 'اهواز', Code: 'AWZ' },
                { RegionName: 'اصفهان', Code: 'IFN' }, { RegionName: 'بندرعباس', Code: 'BND' },
                { RegionName: 'تبریز', Code: 'TBZ' }, { RegionName: 'کیش', Code: 'KIH' },
                { RegionName: 'نجف', Code: 'NJF' }, { RegionName: 'استانبول', Code: 'IST' }
            ];

            var firstRow = $('<div class="row text-center"><span class="legend-future">مسیرهای پر تردد</span><br/><br/></div><br/>').appendTo(list);
            var first = $('<div class="col-md-6 col-sm-6 col-xs-6 text-right"></div>').appendTo(firstRow);
            var second = $('<div class="col-md-6 col-sm-6 col-xs-6 text-right"></div>').appendTo(firstRow);
            var element;
            for (var i = 0; i < popular.length; i++) {
                if (i < popular.length / 2) {
                    element = $('<button class="btn btn-default btn-sm no-wrap no-border-radius" data-code="' + popular[i].Code + '" data-name="' + popular[i].RegionName + '"><i class="glyphicon glyphicon-map-marker"></i> <strong style="cursor:pointer;padding-bottom:2px">' + popular[i].RegionName + '</strong> </button>').appendTo(first);
                } else {
                    element = $('<button class="btn btn-default btn-sm no-wrap no-border-radius" data-code="' + popular[i].Code + '" data-name="' + popular[i].RegionName + '"><i class="glyphicon glyphicon-map-marker"></i> <strong style="cursor:pointer;padding-bottom:2px">' + popular[i].RegionName + '</strong> </button>').appendTo(second);
                }
            }
            list.on("click", "button[data-code]", function () {
                setCityText(this);
            });

            // Show eye icon after load city to not select before loading cities ...
            $('[data-select-city]').click(function (e) {
                e.stopPropagation();
                selectCityIcon = this;
                list.data('shown-form', $(this).parents('form')[0].id);
                list.show();

                var parent = $(this).parent()
                var h = 230;
                var w = 300;


                var position = $(this).parent().offset();
                list.height(h);
                list.width(w);
                list.offset({ top: position.top + parent.height() + 12, left: parent.width() >= w ? position.left : position.left - ((w - parent.width()) / 2) });

                if (!list.data('is-scroll')) {
                    list.data('is-scroll', true);
                    list.mCustomScrollbar({
                        theme: "dark-thin"
                    });
                }
                else
                    list.mCustomScrollbar('update');
            });
        }
        renderCities();

        $('[data-number-spinner] a').on('click', function (e) {
            e.stopPropagation();

            var btn = $(this),
                oldValue = btn.closest('[data-number-spinner]').find('input').val().trim(),
                id = $(this).parents('.input-group').find('input').data('type'),
                form = $('#' + capacityTicker.data('shown-form')),
                newVal;

            var inputs = $(this).parents('[data-capacity]'),
                adult = inputs.find('input[data-type="adult"]'),
                child = inputs.find('input[data-type="child"]'),
                infant = inputs.find('input[data-type="infant"]');

            if (parseInt(adult.val()) + parseInt(child.val()) + parseInt(infant.val()) === 9 && btn.attr('data-dir') == 'up')
                return;

            if (id === 'adult') {
                if (btn.attr('data-dir') == 'up') {
                    newVal = oldValue < 6 ? parseInt(oldValue) + 1 : oldValue;
                } else {
                    newVal = oldValue > 1 ? parseInt(oldValue) - 1 : 1;
                }
            } else {
                if (btn.attr('data-dir') == 'up') {
                    newVal = oldValue < 6 ? parseInt(oldValue) + 1 : oldValue;
                } else {
                    newVal = oldValue > 0 ? parseInt(oldValue) - 1 : 0;
                }
            }
            btn.closest('[data-number-spinner]').find('input').val(newVal);

            $(form).find('#adult').val(adult.val());
            $(form).find('#child').val(child.val());
            $(form).find('#infant').val(infant.val());

            var totalTicket = form.find('#totalTicket');
            var capacity = adult.val() + ' بزرگسال';
            capacity += parseInt(child.val()) > 0 ? '، ' + child.val() + ' کودک' : '';
            capacity += parseInt(infant.val()) > 0 ? '، ' + infant.val() + ' نوزاد' : '';
            totalTicket.val(capacity);
        });
        $('a.select-capacity').click(function (e) {
            e.stopPropagation();
            selectCapacityIcon = this;
            var form = $(this).parents('form')[0];
            capacityTicker.data('shown-form', form.id);

            var adult = $(form).find('#adult').val();
            var child = $(form).find('#child').val();
            var infant = $(form).find('#infant').val();

            $(capacityTicker).find('input[data-type="adult"]').val(adult);
            $(capacityTicker).find('input[data-type="child"]').val(child);
            $(capacityTicker).find('input[data-type="infant"]').val(infant);

            capacityTicker.show();
            var parent = $(this).parent();
            var h = 150;
            var w = 300;


            var position = $(this).offset();
            capacityTicker.height(h);
            capacityTicker.width(w);
            capacityTicker.offset({
                top: position.top + parent.height() + 12,
                left: parent.width() >= w ? position.left : position.left - ((w - $(this).width()) / 2)
            });

            if (!capacityTicker.data('is-scroll')) {
                capacityTicker.data('is-scroll', true);
                capacityTicker.mCustomScrollbar({
                    theme: "dark-thin"
                });
            }
            else
                capacityTicker.mCustomScrollbar('update');
        });

        $('#departureName,#arrivalName').autocomplete({
            maxHeight: 2000,
            serviceUrl: '/Home/AliasMatch',
            paramName: 'matchString',
            zIndex: 3,
            minChars: 2,
            autoSelectFirst: true,
            showNoSuggestionNotice: true,
            noSuggestionNotice: '<strong style="padding-right:5px">متاسفانه!</strong> هیچ نتیجه ای یافت نشد.',
            dataType: 'json',
            formatResult: function (suggestion) {
                if (suggestion.type === 'Airport')
                    return "<div class='paddingtopbot10 font11 bold no-wrap' style='text-overflow: ellipsis;overflow: hidden;'><small class='label label-danger'>" + suggestion.code + "</small>&nbsp;&nbsp;" + suggestion.nameLong + "</div>";
                else
                    return "<div class='paddingtopbot10 font11 bold no-wrap' style='text-overflow: ellipsis;overflow: hidden;'><small class='label label-info'>" + suggestion.code + "</small>&nbsp;&nbsp;" + suggestion.name + "&nbsp;&nbsp;<span title='" + suggestion.nameLong + "' class='text-muted' style='font-size:9px'>همه فرودگاه ها</span></div>";
            },
            onSearchStart: function () {
                $(this).parents('.input-future-container').find('.input-future-loader').show();
            },
            onSearchComplete: function () {
                $(this).parents('.input-future-container').find('.input-future-loader').hide();
            },
            transformResult: function (response) {
                var airports = $.map(response.airports, function (dataItem) {
                    return { type: 'Airport', name: dataItem.RegionName, nameLong: dataItem.RegionNameLong, code: dataItem.Code };
                });
                var regions = $.map(response.cities, function (dataItem) {
                    return { type: 'City', name: dataItem.ParentRegion.RegionName, nameLong: dataItem.ParentRegion.RegionNameLong, code: dataItem.ParentRegion.Code };
                });
                var suggestions = $.merge(regions, airports);
                return {
                    suggestions: $.map(suggestions, function (dataItem) {
                        return { type: dataItem.type, value: dataItem.name, code: dataItem.code, name: dataItem.name, nameLong: dataItem.nameLong };
                    })
                };
            },
            onSelect: function (suggestion) {
                var id = this.id.replace('Name', 'Code');
                var parent = $(this).parents('form')[0];
                var departureCode = $(parent).find('#' + id);
                departureCode.val(suggestion.code);
            }
        });
        $('#tourTab #regionName').autocomplete({
            serviceUrl: '/Tour/TourSearch',
            paramName: 'search',
            maxHeight: 600,
            width: 350,
            showNoSuggestionNotice: true,
            noSuggestionNotice: '<strong style="padding-right:5px">متاسفانه!</strong> هیچ نتیجه ای یافت نشد.',
            minChars: 2,
            dataType: 'json',
            autoSelectFirst: true,
            triggerSelectOnValidInput: false,
            formatResult: function (suggestion) {
                return (suggestion.id == "More" ?
                    "<i class='glyphicon glyphicon-search' style='font-size:12px'></i>" +
                    "&nbsp;&nbsp;<span class='font10'>" + suggestion.title + "</span>" :
                    "<i class='glyphicon glyphicon-globe' style='font-size:12px'></i>" +
                    "&nbsp;&nbsp;<span class='font10'>" + suggestion.title + (suggestion.id == "More" ? "" : " <bold class='font11'>(" + Number(suggestion.Price.toFixed(1)).toLocaleString() + " تومان)</bold>") + "</span>");
            },
            transformResult: function (response) {
                var suggestions = $.map(response.regions, function (dataItem) {
                    return { value: dataItem.TourTitle, title: dataItem.TourTitle, id: dataItem.TourID, Price: dataItem.MinPrice };
                });
                if (suggestions.length > 9) {
                    var search = $('#regionName').val();
                    suggestions.push({ value: search, title: "بیشتر...", id: "More" });
                }

                return { suggestions: suggestions };
            },
            onSelect: function (suggestion) {
                if (suggestion.id == "More") {
                    window.location.href = '/Tour/Search/' + suggestion.value;
                }
                else
                    window.location.href = '/Tour/Detail-' + suggestion.id + '/' + suggestion.title;
            },
            onSearchStart: function () {
                $(this).parents('.input-future-container').find('.input-future-loader').show();
            },
            onSearchComplete: function () {
                $(this).parents('.input-future-container').find('.input-future-loader').hide();
            }
        });
        $('#entityName').autocomplete({
            serviceUrl: '/Hotel/Entity',
            paramName: 'searchValue',
            zIndex: 100,
            minChars: 2,
            autoSelectFirst: true,
            showNoSuggestionNotice: true,
            noSuggestionNotice: '<strong style="padding-right:5px">متاسفانه!</strong> هیچ نتیجه ای یافت نشد.',
            dataType: 'json',
            formatResult: function (suggestion) {
                switch (suggestion.type) {
                    case 1:
                        return "<i class='glyphicon glyphicon-flag' style='font-size:12px'></i>&nbsp;&nbsp;<span class='bold'>" + suggestion.value + "</span>";
                    case 2:
                        return "<span title='" + suggestion.valueLong + "' style='white-space: nowrap;overflow: hidden;display:block;text-overflow:ellipsis'><i class='glyphicon glyphicon-tree-conifer' style='font-size:12px'></i>&nbsp;&nbsp;<span class='bold'>" + suggestion.value + "</span>&nbsp;&nbsp;<span title='" + suggestion.valueLong + "' class='text-muted' style='font-size:9px'>" + suggestion.valueLong + "</span><span>";
                    case 3:
                        return "<span title='" + suggestion.valueLong + "' style='white-space: nowrap;overflow: hidden;display:block;text-overflow:ellipsis'><i class='glyphicon glyphicon-map-marker' style='font-size:12px'></i>&nbsp;&nbsp;<span class='bold'>" + suggestion.value + "</span>&nbsp;&nbsp;<span title='" + suggestion.valueLong + "' class='text-muted' style='font-size:9px'>" + suggestion.valueLong + "</span><span>";
                    case 4:
                        return "<span title='" + suggestion.valueLong + "' style='white-space: nowrap;overflow: hidden;display:block;text-overflow:ellipsis'><i class='glyphicon glyphicon-cutlery' style='font-size:12px'></i>&nbsp;&nbsp;<span class='bold'>" + suggestion.value + "</span>&nbsp;&nbsp;<span title='" + suggestion.valueLong + "' class='text-muted' style='font-size:9px'>" + suggestion.valueLong + "</span><span>";
                }
            },
            transformResult: function (response) {
                return {
                    suggestions: $.map(response, function (dataItem) {
                        return { value: dataItem.EntityName, valueLong: dataItem.SearchValue, data: dataItem.EntityId, type: dataItem.EntityTypeId };
                    })
                };
            },
            onSelect: function (suggestion) {
                $('#entityID').val(suggestion.data);
            },
            onSearchStart: function () {
                $(this).parents('.input-future-container').find('.input-future-loader').show();
            },
            onSearchComplete: function () {
                $(this).parents('.input-future-container').find('.input-future-loader').hide();
            }
        });

        $('[data-close-city-list]').click(function () {
            if (!!list)
                list.hide();
        });
        $('[data-close-capacity-ticker]').click(function () {
            if (!!capacityTicker)
                capacityTicker.hide();
        });
        $('[data-close-date-picker]').click(function () {
            if (!!calendar)
                calendar.hide();
        });
        $('#search-tabs').on("click", "li", function () {
            if (!!calendar)
                calendar.hide();

            if (!!list)
                list.hide();
        });
        $('#departureDateButton,#returnDateButton,#startDateButton,#endDateButton').click(function (e) {
            e.stopPropagation();
            var calendarId;
            firedForm = $(this).parents('form');
            var datePickerName = firedForm.attr('data-DatePicker');

            if (datePickerName == "oneWay") {
                calendarId = '#calendarOneWay';
            } else if (datePickerName == "roundTrip") {
                calendarId = this.id === 'departureDateButton' ? '#calendarRoundTripStart' : '#calendarRoundTripEnd';
            } else if (datePickerName == "range") {
                calendarId = this.id === 'departureDateButton' ? '#calendarRangeStart' : '#calendarRangeEnd';
            }
            else if (datePickerName == "frmSearchHotel") {
                calendarId = this.id === 'startDateButton' ? '#calendarStartDate' : '#calendarEndDate';
            }

            var parent = $(this).parent()
            var h = 250;

            var w = 300;
            var position = $(this).parent().offset();
            calendar = $(calendarId);
            calendar.height(h);
            calendar.width(w);
            calendar.show();

            calendar.offset({ top: position.top + parent.height() + 12, left: parent.width() >= w ? position.left : position.left - ((w - parent.width()) / 2) });

            if (!calendar.data('is-scroll')) {
                calendar.data('is-scroll', true);
                calendar.mCustomScrollbar({
                    theme: "dark-thin",
                });
            }
            else
                calendar.mCustomScrollbar('update');

            $(document).on('mousedown', function (e) {
                if ($(e.target).hasClass("startrange") || $(e.target).hasClass("endrange") || $(e.target).hasClass("today")) {
                    calendar.hide();
                    $(document).off('mousedown');
                }
            });
        });

        createDatePicker('calendarOneWay', 'oneWay #departureDate');
        createDatePicker('calendarRoundTripStart', 'roundTrip #departureDate');
        createDatePicker('calendarRoundTripEnd', 'roundTrip #returnDate');
        createDatePicker('calendarRangeStart', 'range #departureDate');
        createDatePicker('calendarRangeEnd', 'range #returnDate');
        createDatePicker('calendarStartDate', 'frmSearchHotel #startDate');
        createDatePicker('calendarEndDate', 'frmSearchHotel #endDate');
        $("#departureDate,#returnDate,#startDate,#endDate").mask("0000-00-00", { placeholder: "روز-ماه-سال" });
    }
</script>
