		function getOrderHistoryData () {
			$.ajax({
				url: 'hp/api/public/customer/orderdetails/3',
				dataType: 'json'
			}).done(function(result){

				if($("#latest-order").length) {
					orderResults = "";

					$.each(result, function(index, order) {
						orderResults += '<li>';
						orderResults +=	'<img src="'+ order.product_image +'" alt="'+ order.product_name +'" />';
						orderResults += '<p class="title">'+ order.product_name +'</p>';
						orderResults += '<p class="description">' + order.product_description + '</p>';
						orderResults += '<p class="price"><span class="discount"><strike>&euro; 80.95</strike></span> ' + order.product_price + '</p>';
						orderResults += '<p class="action"><a href="'+ order.combination_path +'" class="bttn-primary"></a></p>';
						orderResults += '</li>';
					});

					$("#latest-order ul").html(orderResults);

				}

			}).error(function (xhr, thrownError){
				if(xhr.status==404) {
					$("#latest-order").hide();
				}
			});
		}

		// tiny helper function to add breakpoints
		function getGridSize() {
			return (window.innerWidth < 600) ? 1 :
			(window.innerWidth < 900) ? 2 : 4;
		}

$(document).ready(function() {

	if($("#latest-order").length) {
		getOrderHistoryData();
	}

	var activeOnHover = 0;

	$("#slider.flexslider").flexslider({
		animation: "fade",
		animationSpeed: 150,
		directionNav: false,
		controlNav: true,
		controlsContainer: ".flexslider-container",
		manualControls: "",
		slideshow: true,
		slideshowSpeed: 4000,
		after: function(slider){
			if(activeOnHover == 0) {
			$("#slider .flex-nav-content > div").removeClass("active");
			$("#slider .flex-nav-content > div").eq($("#slider .flex-control-paging li").children("a.flex-active").text()-1).addClass("active");
			}
		},
	});

	if($('.carousel.flexslider.single').length) {
		$('.carousel.flexslider.single').flexslider({
			directionNav: true,
			controlNav: false,
			animation: "slide",
			animationSpeed: 400,
			itemMargin: 0,
			itemWidth: 880,
			move: 1,
		});
	}

	if($('.carousel.flexslider.full').length) {
		$('.carousel.flexslider.full').flexslider({
			directionNav: true,
			controlNav: true,
			animation: "slide",
			animationSpeed: 400,
			itemMargin: 0,
			itemWidth: 283,
			move: 1,
			pauseOnHover: true,
			minItems: getGridSize(), // use function to pull in initial value
			maxItems: getGridSize(), // use function to pull in initial value
		});
	}

	//Change banners on mouseover
	$(".flex-nav-content > div").mouseenter(function() {
		activeOnHover = 1;
		$(".flex-nav-content > div").removeClass("active");
		$(this).addClass("active");
		$(".flex-control-paging").children("li").eq($(this).index()).children("a").click();
	});

	//Change font-size on orange circle (prices)
	$(".price.circle > .tax+span").each(function() {

			if($(this).text().length - $(this).children().text().length > 3) {

				$(this).css("font-size", "24px").children().css({"font-size": "14px","margin-left":"-6px"});
			}

	});

