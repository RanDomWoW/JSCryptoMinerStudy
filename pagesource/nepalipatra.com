	$(document).ready(function(){

		$(".news-detail .f-img img").each(function(i, ele) {

			$(ele).after("<div class='img-cap'>"+$(ele).attr("alt")+"</div>");

		});

		

		$(".news-detail p img").each(function(i, ele) {



			var flt = $(ele).css('float');

			if(flt == 'left')

				fltcls = 'img-lft';

			else if(flt == 'right')

				fltcls = 'img-rgt';

			else

				fltcls = 'img-center';

				

			$(ele).wrap( "<div class='"+fltcls+"'></div>" );

			$(ele).css('float', '');

			if($(ele).attr("alt") != '')

				$(ele).after("<div class='img-cap'>"+$(ele).attr("alt")+"</div>");			

		});

	});

</script>

