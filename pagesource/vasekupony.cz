//            selected: "",
            disabled: true
        });
    });

    $(document).ready(function () {
        $('.hidden-xs select[name=category]').change(function () {
            setTimeout(function () {
                $.nette.ajax({
                    url: "\/?do=getEshops",
                    data: {
                        'value': $('.wSelect.wSelect-theme-full.wSelect-active').find("div.wSelect-selected").text()
                    }
                });
            }, 100);
        });

        $('.visible-xs select[name=category]').change(function () {
            setTimeout(function () {
                $.nette.ajax({
                    url: "\/?do=getEshops",
                    data: {
                        'value': $('.wSelect.wSelect-theme-full.wSelect-active').find("div.wSelect-selected").text()
                    }
                });
            }, 100);
        });

        $(document).ajaxComplete(function () {
            $('.eshop').multiselect({
                columns: 1,
                placeholder: "Vyberte obchody",
                //search: true,
                search: "Vyberte obchody",
                selectedOptions: "vybran\u00e9",
                showCheckbox: true,
                maxHeight: '200px'
            });

            $('.ms-search').find("input[type=text]").each(function (ev) {
                if (!$(this).val()) {
                    $(this).attr("placeholder", "Vyberte obchody");
                }
            });
        });
    });

    $("#frm-searchCouponsForm").submit(function (e) {
        e.preventDefault();

        var category = $('#frmsearchCouponsForm-catUrl').val();

        var redirect = "\/kupony\/category";

        var redirectUrl = redirect.replace("category", category);

        var selectedEshops = [];

        $('.ms-reflow.selected').find('input').each(function (i, selected) {
            selectedEshops[i] = selected.value;
        });

        $.extend(
            {
                redirectPost: function (location, args) {
                    var form = '';

                    $.each(args, function (key, value) {
                        form += '<input type="hidden" name="' + key + '" value="' + value + '">';
                    });

                    $('<form action="' + location + '" method="POST">' + form + '</form>').appendTo('body').submit();
                }
            });

        var canSend = true;

        if (category === "cat") {
            canSend = false;
            $(this).find(".compareError").css("display", "block");
            $(this).find(".compareError").html("Mus\u00ed b\u00fdt zad\u00e1na kategorie.");
        }

        if (selectedEshops.length < 2) {
            canSend = false;
            $(this).find(".compareError").css("display", "block");
            $(this).find(".compareError").html("Mus\u00ed b\u00fdt zad\u00e1ny minim\u00e1ln\u011b 2 obchody.");
        }

        if (canSend === true) {
            $.redirectPost(redirectUrl, {
                'hpParam': 'filter',
                'hpCatUrl': category,
                'hpEshops': selectedEshops
            });
        }
    });
</script>


