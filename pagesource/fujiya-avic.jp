<script>
  $2(function(){
    'use strict';
    $2('#search_category').customSelect();

    /**
     *  gNaviBackgroundFlg: グロナビコンテンツ背景表示フラグ
     *  ScrollFlg: ヘッダー固定フラグ
     *  ScrollPoint: ヘッダー固定位置取得
     *  gNaviDisplayTime: メガメニュー表示遅延時間
     *  openContents:
     */
    var
      gNaviLi = $2('#header_gnavi > li'),
      gNaviBackground = $2(document.createElement('div')).attr({
        'id': 'bg_header_gnavi_wrap'
      }),
      gNaviBackgroundFlg = false,
      ScrollFlg = false,
      HeaderFixedArea = $2('#header_fixed_area'),
      ScrollPoint = HeaderFixedArea.offset().top,
      gNaviDisplayTime = 300,
      openContents;

        // コンテンツと背景表示
    gNaviLi.on({
      'mouseenter' : function(){

        openContents = $2(this).find('.header_gnavi_inner_contents');

        if (openContents.length > 0){

          // 背景の高さ計算
          gNaviBackground.css({'height' : openContents.innerHeight() + 10});

          if(gNaviBackgroundFlg === false) {
            // 背景が出ていない場合
            $2('#header_gnavi').before(gNaviBackground);
            openContents.hide(0).delay(gNaviDisplayTime).show(0);

            gNaviBackground.hide(0).delay(gNaviDisplayTime).show(0, function() {
              gNaviBackgroundFlg = true;
            });
          } else {
            // 背景が出ている場合
            openContents.show();
          }

          $2(this).addClass('mega_menu_open');

        } else {
          // コンテンツがない場合
          gNaviBackground.stop().detach();
          gNaviBackgroundFlg = false;
        }

      },
      'mouseleave' : function(){
        $2(this).removeClass('mega_menu_open').find('.header_gnavi_inner_contents').stop(true, false).hide();
        gNaviBackground.stop(true, false);
      }
    });

    // 背景mouseover時はコンテンツを表示
    gNaviBackground.on({
      'mouseenter':function() {
        openContents.show();
        openContents.parent().addClass('mega_menu_open');
      }
    });

    // 非表示
    $2('#header_gnavi_wrap').on('mouseleave', function(){
      if(gNaviBackgroundFlg === true) {
        GnaviClose();
      }
    });
    $2('.header_gnavi_close').find('img').on('click', function() {
      GnaviClose();
    });

    // 非表示関数
    function GnaviClose() {
      gNaviLi.find('.header_gnavi_inner_contents').stop().hide();
      gNaviLi.removeClass('mega_menu_open');
      gNaviBackground.stop().detach();
      gNaviBackgroundFlg = false;
    }

        $2(window).scroll(function() {

      if($2(this).scrollTop() > ScrollPoint && ScrollFlg === false){
        ScrollFlg = true;
        HeaderFixedArea.css({
          'position': 'fixed',
          'top': 0
        });
        $2('#header_fixed_margin').css('padding-top', HeaderFixedArea.outerHeight());
      }

      if($2(this).scrollTop() < ScrollPoint && ScrollFlg === true){
        ScrollFlg = false;
        HeaderFixedArea.css({
          'position': 'relative',
          'top': 'auto'
        });
        $2('#header_fixed_margin').css('padding-top', 0);
      }
    });
  });
</script>
