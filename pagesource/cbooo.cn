        function getPieData(){
            $.ajax({
                url: '/Home/GetScreenData',
                type: 'get',
                dataType: 'json',
                success: function (data) {
                    var arrObj = getChartData(data.data1);                    
                    var myDate = new Date();
                    var nowTimeStr = myDate.getFullYear() + "-" + (myDate.getMonth() + 1) + "-" + myDate.getDate();                   
                    if (browserRedirect()) {
                        drawChar.drawPie_wap_new('divScreenCanvas', nowTimeStr + '全国排片统计', arrObj.nameArr, arrObj.countArr);
                    } else {
                        drawChar.drawPie_day_new('divScreenCanvas', nowTimeStr + '全国排片统计', arrObj.nameArr, arrObj.countArr);
                    }
                }
            });
        }

        function browserRedirect() {
            var sUserAgent = navigator.userAgent.toLowerCase();
            var bIsIpad = sUserAgent.match(/ipad/i) == "ipad";
            var bIsIphoneOs = sUserAgent.match(/iphone os/i) == "iphone os";
            var bIsMidp = sUserAgent.match(/midp/i) == "midp";
            var bIsUc7 = sUserAgent.match(/rv:1.2.3.4/i) == "rv:1.2.3.4";
            var bIsUc = sUserAgent.match(/ucweb/i) == "ucweb";
            var bIsAndroid = sUserAgent.match(/android/i) == "android";
            var bIsCE = sUserAgent.match(/windows ce/i) == "windows ce";
            var bIsWM = sUserAgent.match(/windows mobile/i) == "windows mobile";
            if (!(bIsIpad || bIsIphoneOs || bIsMidp || bIsUc7 || bIsUc || bIsAndroid || bIsCE || bIsWM)) {
                return false;
            } else {
                return true;
            }
        }
        var getChartData = function (arrData) {
            var leng = arrData.length;
            var result = {
                nameArr: [],
                countArr: []
            };
            for (var i = 0; i < leng; i++) {
                var mName = arrData[i].moviename;
                if (mName.length > 8) mName = mName.substring(0, 8) + '... ' + arrData[i].Per_num + '%';
                else mName = mName + ' ' + arrData[i].Per_num + '%';
                var cb = {
                    value: arrData[i].movienum,
                    name: mName,
                    itemStyle: {
                        normal: {
                            color: drawChar.colorArr[i]
                        }
                    }
                };
                result.nameArr.push(mName);
                result.countArr.push(cb);
            }
            return result;
        };

        var ckDate = function (num) {
            if (num < 10)
                num = '0' + num;
            return num;
            
        };

        function search() {
            var txtSearch = $('#txtSearch').val();
            var url = '/search?k=' + encodeURI(txtSearch);

            if (txtSearch == '请输入影片/影人名称' || txtSearch == '' || txtSearch == 'null')
                return;
            window.location.href = url;
        }
        function enterSearch() {
            var event = window.event || arguments.callee.caller.arguments[0];
            if (event.keyCode == 13) {
                search();
            }
        }
        function GotoUrl(url)
        {
            window.location.href = url;
        }
        
