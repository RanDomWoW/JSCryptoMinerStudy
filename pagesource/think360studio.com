$('.psubmit').click(function(){
		var pname= $('.pname').val();
		var pemail= $('.pemail').val();
		var pskype= $('.pskype').val();
		var pattern = /^\b[A-Z0-9._%-]+@[A-Z0-9.-]+\.[A-Z]{2,4}\b$/i
		var validate = true;
		$('.pname, .pemail ').removeClass('erorplaceholder');	
		if(pname==""){
			$('.pname').attr('placeholder','Please insert Name..');
			$('.pemail').removeClass('erorplaceholder');
			$('.pname').addClass('erorplaceholder');
			validate = false;
		}
		else if(pemail==""){
			$('.pemail').attr('placeholder','Please insert Email..');
			$('.pname').removeClass('erorplaceholder');
			$('.pemail').addClass('erorplaceholder');
			validate = false;
		}else if(!pattern.test(pemail)){
			$('.pemail').val("");
			$('.pemail').attr('placeholder','not a valid e-mail address');
			$('.pname').removeClass('erorplaceholder');
			$('.pemail').addClass('erorplaceholder');
			validate = false;
		}
		if( validate == true){
			$.ajax({
				type: 'POST',
				url: 'https://think360studio.com/ajax_page.php',
				data:'name='+pname+'&skype='+pskype+'&email='+pemail,
				success: function(html)
				{
					$('.p-form').html('');
					$('.p-form').html('<h3>Thanx for show your Interest, I will send you link and details of our profile.</h3>');
				}
			});
		}
		
	});
