<script type="text/javascript">



	$(function() {
		$.ajax({
			url : 'http://app.gd.gov.cn/xxts/pushinfo_json.php',
			dataType : "jsonp",
			jsonp : "pushInfoJsonpCallBack",
			jsonpCallback : "pushInfoJsonpCallBack",
			success : function(data) {
			$('.loading-table').hide();
				$.each(data, function(i, json) {
					//var str1= json.pubDate;
					//str1=str1.substr(5,10);
					if (i > 9) {
						return;
					}
				
					//var link =json.link;
					var title=delHtmlTag(json.title);
					var pubDate=delHtmlTag(json.pubDate);
					//alert(cutstr(title,10));
					if(i==0){
					
					var title=delHtmlTag(json.title);

					if(title.length>=19)
						$("#con_four_3>div:first").append(
							
							"<a href"+"=\""+json.link+"\" title=\""+title+"\" target=\"_blank\">" + title.substring(0,19)+"..</a>");
					else
							$("#con_four_3>div:first").append(
								"<a href"+"=\""+json.link+"\" title=\""+title+"\" target=\"_blank\">" + title+"</a>");
							
					}else{
						var title=delHtmlTag(json.title);

						if(title.length>=18)
							$("#con_four_3 ul:first").append(
								
								"<li><span>"+pubDate.split("-")[1] +"-" +  pubDate.split("-")[2]
+"</span>· <a href"+"=\""+json.link+"\" title=\""+title+"\" target=\"_blank\">" + title.substring(0,18)+"..</a></li>");
						else
						
								$("#con_four_3 ul:first").append(
									"<li><span>"+pubDate.split("-")[1] +"-" +  pubDate.split("-")[2]
+"</span>· <a href"+"=\""+json.link+"\" title=\""+title+"\" target=\"_blank\">" + title+"</a></li>");
		
					}
					
				});
			},
			error : function() {
				//alert('fail');
			}
		})
	});
	function delHtmlTag(str){  
		return str.replace(/<[^>]+>/g,"");//去掉所有的html标记  
	}

</script>
