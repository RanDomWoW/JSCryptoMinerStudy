	jQuery('#fullteaser').on('mouseleave', function() { fullteaserChangeTimer = setTimeout(changeIndexTeaser, fullteaserChangeDelay); });
	*/

	jQuery(document).on('mousemove scroll keydown', function() { clearTimeout(fullteaserChangeTimer); fullteaserChangeTimer = setTimeout(changeIndexTeaser, fullteaserChangeDelay); });
});
