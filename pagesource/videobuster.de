		jQuery('#teaser_nav').attr('class','teaser_nav nav_' + jumpto);

		jQuery('#fullteaser .item').stop().hide();
		$current.removeClass('active').show();
		$next.addClass('active').addClass('loaded').fadeIn(
			500,
			function()
			{
				console.log('fading complete');
				$current.hide();
				clearTimeout(fullteaserChangeTimer);
				fullteaserChangeTimer = setTimeout(changeIndexTeaser, fullteaserChangeDelay);
			}
		);
		console.log('fading start');
	}
}

jQuery(document).ready(function()
{
	jQuery('#fullteaser .item').hide();
	var $initial = jQuery('#fullteaser').find('.item:first');
	$initial.show().addClass('active').addClass('loaded');
	// preloader
	var $initialNext = $initial.next('.item');
	if ($initialNext.length > 0)
	{
		$initialNext.addClass('loaded');
	}
	
	jQuery('#teaser_nav').addClass('nav_1');
	jQuery('#fullteaser .teaser_nav .dot[data-index='+1+']').addClass('active');

	fullteaserChangeDelay = fullteaserFirstDelay;
	fullteaserChangeTimer = setTimeout(changeIndexTeaser, fullteaserChangeDelay);

	jQuery('#teaser_nav a').click(function() {
		console.log('click jump');
		console.log(jQuery(this).data('index'));
		changeIndexTeaser(jQuery(this).data('index'));
	});

