	if (typeof jQuery != 'undefined') {
		var filetypes = /\.(zip|exe|dmg|pdf|doc.*|xls.*|ppt.*|txt)$/i;
		var baseHref = '';
		if (jQuery('base').attr('href') != undefined) baseHref = jQuery('base').attr('href');
		var hrefRedirect = '';

		jQuery('body').on('click', "a[href$='zip'], a[href$='exe'], a[href$='dmg'], a[href$='pdf'], a[href$='PDF'], a[href$='doc'], a[href$='DOC'], a[href$='docx'], a[href$='xls'], a[href$='xlsx'], a[href$='ppt'], a[href$='pptx'], a[href$='txt'], a[href^='mailto'], a[href^='tel']", function(event) {
			var el = jQuery(this);
			var track = true;
			var href = (typeof(el.attr('href')) != 'undefined' ) ? el.attr('href') : '';
			var isThisDomain = href.match(document.domain.split('.').reverse()[1] + '.' + document.domain.split('.').reverse()[0]);

			//consider YouTube to be same domain so we don't break FancyBox popups on YouTube videos
			if(el[0].hostname == "www.youtube.com"){
