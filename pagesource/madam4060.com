<script type="text/javascript">
	(function ($) {
		$(function () {

			var _hEc = $('.header_evt_ban_wrap .header_evt_content');
			if ($.cookie('top_evt') == undefined) {
				_hEc.css({
					'height': '75px'
				});
				$('.evt_onoff_btn').css({
					'background-position-y': '-40px'
				});
			} else {
				$('.evt_onoff_btn').css({
					'background-position-y': '0px',
					'bottom': '-40px'
				});
			}

			$('.header_evt_ban_wrap .evt_onoff_btn').on('click', function () {
				if ($.cookie('top_evt') == undefined) {
					$.cookie('top_evt', 'top_evt', {
						expires: 1,
						path: '/'
					});
					_hEc.animate({
						'height': 0
					});
					$('.evt_onoff_btn').animate({
						'background-position-y': '0px',
						'bottom': '-40px'
					});
				} else {
					$.cookie('top_evt', 'top_evt', {
						expires: -1,
						path: '/'
					});
					_hEc.animate({
						'height': 75
					});
					$('.evt_onoff_btn').animate({
						'background-position-y': '-40px',
						'bottom': '-20px'
					});
				}
			});

			var _allCate = $('.all_cate_wrap_logo .all_cate_wrap_logo_inn .all_cate .s_cate_wrapper');
			var _tinpText = $('.all_cate_wrap_logo .all_cate_wrap_logo_inn .sch_wrap input');
			_tinpText.attr('placeholder','99사이즈');

			$('.all_cate').on({
				mouseenter : function(){
					_allCate.stop().show(300);
				},mouseleave : function(){
					_allCate.stop().hide(300);
				}
			});


			$('.top_loc').click(function(){
				$('html,body').stop().animate({
					scrollTop : 0
				});
			});

			$('.dw_loc').click(function(){
				var _ouHeight = $('#wrap').outerHeight();
				$('html,body').stop().animate({
					scrollTop : _ouHeight
				});
			});

			$('.prod_thumb, .prod_item').on({
				mouseenter : function(){
					var _hitLength = $(this).find('.hit_cntz').text().length;
					if(_hitLength == 0){
						$(this).find('.prod_prv').stop().animate({
							'bottom' : '-40px'
						});	
					}else{
						$(this).find('.prod_prv').stop().animate({
							'bottom' : 0
						});	
					}
					
				},mouseleave : function(){
					$(this).find('.prod_prv').stop().animate({
						'bottom' : '-138px'
					});
				}
			});

			$('.prod_prvinn').each(function(){
				$(this).find('a').eq(0).click(function(){
					$('.mask_prv').show();
				});
			});

			$('.close-layer').click(function(){
				$('.mask_prv').hide();
			});

			$(window).on('scroll',function(){
				var _windowSct = $(window).scrollTop();
				var _tfwOffset = $('.top_fixed_wrap').offset().top;
				if(_windowSct > _tfwOffset){
					$('.top_fixed_inn').css({
						'position' : 'fixed',
						'top' : '0px',
						'z-index': '99999',
						'padding-top' : '10px'
					});
				}else{
					$('.top_fixed_inn').css({
						'position' : 'static',
						'top' : '0px',
						'padding-top' : '0px'
					});
				}
			});
			

			$('.hamburger').click(function(){

				if($('.hamburger-box').hasClass('is-active')){
					$('.hamburger-box').removeClass('is-active');
				}else{
					$('.hamburger-box').addClass('is-active');
				}
				
			});
		});
	})(jQuery);
</script>

