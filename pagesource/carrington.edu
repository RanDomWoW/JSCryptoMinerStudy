      	$('.site-header').removeClass('navbar-fixed-top');
      	$('.site-header').addClass('navbar-fixed-bottom');
      	$('.mobile-navigation').css('display', 'block');
      }
});

$(window).on('resize', function(){
      var win = $(this); //this = window
      if (win.width() >= 1025) {
      	$('.site-header').removeClass('navbar-fixed-bottom');
      	$('.site-header').addClass('navbar-fixed-top');
      	$('.mobile-navigation').css('display', 'none');
      	$('#site-navigation').css('display', 'block');
      }
});

$(window).on('resize', function(event){
      var win = $(this); //this = window
      if (win.width() > 1025) {
      	event.preventDefault();
      	$('#page > .site-header').removeClass('navbar-fixed-bottom');
