$('.mobile-menu-options > li').on("click", function(event) {
    $('.mobile-menu-options li').not(this).find('ul:not(.mobile-third-sub-menu)').css('display', 'none');
    $(this).find('ul:not(.mobile-third-sub-menu)').css('display', 'block');
    $('.mobile-menu-options > li').removeClass('selected');
    $(this).addClass('selected');
    $('.mobile-menu-navbar').removeClass('mobile-menu-initial');
    $('.mobile-menu-navbar').removeClass('mobile-search-navbar');
    $('.mobile-menu-navbar').addClass('mobile-navbar');
    $('.mobile-menu-options').addClass('menu-open');
		if ( $('.mobile-menu-search').is(':visible')) {
			$('.mobile-menu-search').css('display', 'none');
		}

});


var x = false;
$('.mobile-sub-menu > li').not('.mobile-menu-options').on('click', function(event){
 	if (!x) {
	 	event.preventDefault();
	    $(this).siblings().css('display', 'none');
	    $(this).find('ul').css('display', 'block');
	    $(this).find('.mobile-third-sub-menu').css('display', 'block');
	    $('.mobile-forward').hide();
	    $(this).addClass('option-selected');
	    $('.mobile-menu-navbar').removeClass('mobile-search-navbar');
	    $('.mobile-menu-search').hide();
	    $('.mobile-menu-back').toggle().css('display', 'block');
			if ( $('.mobile-menu-search').is(':visible')) {
				$('.mobile-menu-search').css('display', 'none');
			}
			if ( $(this).hasClass('mobile-dental') || $(this).hasClass('mobile-vet') || $(this).hasClass('mobile-nevada')) {
				$('.mobile-sub-menu').addClass('mob_list_one');
				$('.mobile-menu-navbar').addClass('mob_nav_list_one');
			}
			if ( $(this).hasClass('mobile-idaho') || $(this).hasClass('mobile-nm') || $(this).hasClass('mobile-oregon') || $(this).hasClass('mobile-texas') || $(this).hasClass('mobile-wash')) {
				$('.mobile-sub-menu').addClass('mob_list_two');
				$('.mobile-menu-navbar').addClass('mob_nav_list_two');
			}
  	x = true;
}
 else {
    $(this).siblings().css('display', 'block');
    $(this).find('ul').css('display', 'none');
    $(this).find('.mobile-third-sub-menu').css('display', 'none');
    $('.mobile-forward').show();
    $('.mobile-menu-back').css('display', 'none');
	$('.mobile-sub-menu').addClass('navbar-height');
	$(this).removeClass('option-selected');
	$('.mobile-sub-menu').removeClass('mob_list_one');
	$('.mobile-sub-menu').removeClass('mob_list_two');
	$('.mobile-menu-navbar').removeClass('mob_nav_list_one');
	$('.mobile-menu-navbar').removeClass('mob_nav_list_two');
    $('.mobile-menu-navbar').addClass('mobile-navbar');
		if ( $('.mobile-menu-search').is(':visible')) {
			$('.mobile-menu-search').css('display', 'none');
		}

  	x = false;
}

});

$('.mobile-search-option').on("click", function(event){
	$('.mobile-menu-navbar').removeClass('mobile-navbar');
	$('.mobile-menu-navbar').addClass('mobile-search-navbar');
	$('body').removeClass('option-selected');
	$('.mobile-menu-search').css('display', 'block');
	// $('.mobile-menu-navbar').removeClass('mob-navbar');
})

$('.mobile-menu-options > li').not('.mobile-search-option').on('click', function() {
	if ( $('.mobile-third-sub-menu').is(':visible')) {
		$('.mobile-menu-back').css('display', 'block');
		$('.mobile-forward').css('display', 'none');
		$('.mobile-menu-navbar').removeClass('mobile-search-navbar');
			if ( $('.mobile-dental').hasClass('option-selected') || $('.mobile-vet').hasClass('option-selected') || $('.mobile-nevada').hasClass('option-selected') &&
				$('.two-third').is(':visible')) {
				$('.mobile-menu-navbar').removeClass('mobile-navbar');
				$('.mobile-sub-menu').addClass('mob_list_one');
				$('.mobile-menu-navbar').addClass('mob_nav_list_one');
			}
			if ( $('.mobile-idaho').hasClass('option-selected') || $('.mobile-nm').hasClass('option-selected') || $('.mobile-oregon').hasClass('option-selected') ||
			 $('.mobile-texas').hasClass('option-selected') || $('.mobile-wash').hasClass('option-selected') && $('.one-third').is(':visible')) {
			 	$('.mobile-menu-navbar').removeClass('mobile-navbar');
				$('.mobile-sub-menu').addClass('mob_list_two');
				$('.mobile-menu-navbar').addClass('mob_nav_list_two');
			}
		x = true;
	}
	else {
		$('.mobile-menu-back').css('display', 'none');
		$('.mobile-forward').css('display', 'block');
		$('.mobile-sub-menu').addClass('navbar-height');
		$('.mobile-menu-navbar').addClass('mobile-navbar');
		x = false;
	}
})

