var clicks = 0;
$('.search-icon').click(function(){
    if(clicks == 0){
	    $(".menu-search").toggle('show').addClass('menu-search-active');
	    $('.search-cancel').toggle('show');
	    $('#menu-dropdown').removeClass('open');
	    $('.btn-primary').text("MENU");
	    $('.dropdown-menu li > ul').css('display', 'none');
	    $('.menu-icon').attr('src',"/wp-content/themes/carrington-edu/images/icons_menu.svg" );
	    $('.vertical-line').toggle('hide');
	    $('.search-icon').addClass('search-open');
	    $('.menu-search').focus();
    }else{
        $('#search-form').submit();
    }
    ++clicks;
});


