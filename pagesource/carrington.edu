function chatPop() {
	var theurl = window.location.pathname;

	if(document.cookie.indexOf('chat-shown') != -1 || theurl == '/financial-aid/introduction-to-fafsa/') {
		// alert('chat shown is there');
	} else {
		// alert('chat shown is not there');
	    $(".chat-name").delay(25000).fadeIn(500);
	    	setTimeout(sendchat, 25000);

	    $('.exit-button').on('click', function() {
	    	document.cookie = "Chat_Pop_up = chat-shown; expires 50*60*60*24";
	    })
	    $('#get_chat').on('click', function() {
	    	document.cookie = "Chat_Pop_up = chat-shown; expires 50*60*60*24";
	    	dataLayer.push({'event': 'chat-submitted'});
	        $.ajax({
	             url: '//rad.carrington.edu/api/session-tracking/chat-popup?status=clicked&location='+ theurl,
	         });

	    })
	    $('#get_chat_mobile').on('click', function() {
			document.cookie = "Chat_Pop_up = chat-shown; expires 50*60*60*24";
			dataLayer.push({'event': 'chat-submitted'});
	        $.ajax({
	            url: '//rad.carrington.edu/api/session-tracking/chat-popup?status=clicked&location=' + theurl,
		         });
	    })
	    $(window).on('click', function() {
	    	document.cookie = "Chat_Pop_up = chat-shown; expires 50*60*60*24";
	    })

	    function sendchat() {
	        $.ajax({
	             url: '//rad.carrington.edu/api/session-tracking/chat-popup?status=shown&location=' + theurl,
	         });
	        dataLayer.push({'event': 'chat-viewed'});
	    }
	}
}


