      }
});




$('.mobile-menu-icon, .tab-menu-open').on("click", function(event) {
	$('.overlay').toggle();
	$('.mobile-dropdown').toggle();
		if ( $('.mobile-menu-navbar').hasClass('mobile-navbar')) {
			$('.mobile-menu-navbar').removeClass('mobile-navbar');
		}
		if ( $('.mobile-menu-navbar').hasClass('mobile-search-navbar')) {
			$('.mobile-menu-navbar').removeClass('mobile-search-navbar');
		    $('.mobile-menu-navbar').removeClass('mob_nav_list_one');
		    $('.mobile-menu-navbar').removeClass('mob_nav_list_two');
		}
		if ( $('.mobile-menu-navbar').hasClass('mob_nav_list_one')) {
			$('.option-selected').trigger('click');
			$('.mobile-menu-navbar').removeClass('mobile-navbar');
			$('.mobile-menu-navbar').removeClass('mobile-search-navbar');
		}
		if ( $('.mobile-menu-navbar').hasClass('mob_nav_list_two')) {
			$('.option-selected').trigger('click');
			$('.mobile-menu-navbar').removeClass('mobile-navbar');
			$('.mobile-menu-navbar').removeClass('mobile-search-navbar');
		}
		if ( $('.mobile-sub-menu').is(':visible')) {
			$('.mobile-menu-navbar').removeClass('.mobile-menu-initial');
			$('.mobile-menu-navbar').addClass('mobile-navbar');
			$('.mobile-menu-navbar').removeClass('mob_nav_list_one');
    		$('.mobile-menu-navbar').removeClass('mob_nav_list_two');
		}
		if ( $('.mobile-menu-search').is(':visible')) {
			$('.mobile-menu-navbar').addClass('mobile-search-navbar');
		}
});




