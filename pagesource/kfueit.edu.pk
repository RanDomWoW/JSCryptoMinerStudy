                    pager:false
                  });
        setTimeout(function() {
            changeBgImage();
        }, 5000);

    });
    function getRandomArbitrary(min, max) {
        return Math.floor(Math.random() * (max - min + 2)) + min;
    }
    $(img).load(function() {
        var imageUrl = "url('" + img.src + "')";
        var animation=getRandomArbitrary(1
            ,6);

        if(animation==1)
        {
            $('#bg')
                .animate({left: ($('#bg').width()),right: ($('#bg').width()),top:($('#bg').height()),bottom:($('#bg').height()),opacity:.3},3000,'swing', function() {
                    $(this)
                        .css({'background-image': imageUrl})
                        .animate({left: "0",right:($('#bg').width()),top:($('#bg').height()),bottom:($('#bg').height()),opacity:1});
                });
        }
        if(animation==2)
        {
            $('#bg')
                .animate({right: ($('#bg').width()),opacity:.1},3000,'swing', function() {
                    $(this)
                        .css({'background-image': imageUrl})
                        .animate({right:($('#bg').width()),opacity:1});
                });
        }
        if(animation==3)
        {
            $('#bg')
                .animate({left: ($('#bg').width()),opacity:.3},3000,'swing', function() {
                    $(this)
                        .css({'background-image': imageUrl})
                        .animate({left:($('#bg').width()),opacity:1});
                });
        }
        if(animation==4)
        {
            $('#bg')
                .animate({top: ($('#bg').height()),opacity:.3},3000,'swing', function() {
                    $(this)
                        .css({'background-image': imageUrl})
                        .animate({top:($('#bg').height()),opacity:1});
                });
        }
        if(animation==5)
        {
            $('#bg')
                .animate({bottom: ($('#bg').height()),opacity:.3},3000,'swing', function() {
                    $(this)
                        .css({'background-image': imageUrl})
                        .animate({bottom:($('#bg').height()),opacity:1});
                });
        }if(animation==6)
        {
            $('#bg')
                .animate({opacity:.0},3000,'swing', function() {
                    $(this)
                        .css({'background-image': imageUrl})
                        .animate({opacity:1});
                });
        }


        $('#head').css('background-image', imageUrl);
        setTimeout(function() {
            changeBgImage();
        }, 5000);

    });
    function changeBgImage(){
        img.src=imageArray[imageIndex];
        imageIndex++;
        if (imageIndex >= imageArray.length) {
            imageIndex = 0;
        }
    }


</script>
