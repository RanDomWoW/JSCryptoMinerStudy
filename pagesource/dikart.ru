        <script>
            function showWarn(text){
                if($('.bx-auth-reg .warn-err').length<1)
                $('.bx-auth-reg').prepend('<div class="warn-err"><div class="warn-left">&nbsp;</div><p class="append_warn">&nbsp;'+text+'</p></div>');
                else
                    $('.warn-err').find('p.append_warn').html(text);

            }

            var dir = '/bitrix/templates/.default/components/itees/simple.component/auth';
            $(function(){

                //При загрузке страницы ссылкам форм подменяем атрибут href и добавляем data-атрибут для вызова модальных окон
                $('#modal-login a[href*="register"]').attr('href','#modal-register').attr('data-uk-modal','');
                $('#modal-login a[href*="forgot_password"]').attr('href','#modal-forgot-password').attr('data-uk-modal','');
                //$('#modal-forgot-password a[href*="login"]').attr('href','#modal-login').attr('data-uk-modal','');
                $('#modal-register a[href*="login"]').attr('href','#modal-login').attr('data-uk-modal','');

                //Обработчик формы авторизации
                $('#modal-login').on('submit','form',function(){
                    var form = this;
                     console.log($(this).serialize());
                     var form_action = $(this).attr('action');
                     var form_backurl = $(this).find('input[name="backurl"]').val();

                    //--unmask--//
                    $('input[name="USER_LOGIN"]').each(function() {
                        if($(this).mask()!=''){
                            BX.setCookie("BITRIX_SM_LOGIN", $(this).mask(), {expires: 86400});
                        }
                        $(this).val($(this).mask());
                    });

                    $('input[name="USER_SMS_CODE"]').each(function() {
                        $(this).val($(this).mask());
                    });
                    //--/unmask--//

                     $.ajax({
                         type: "POST",
                         url: dir+'/ajax/register.php',
                         data: $(this).serialize(),
                         timeout: 10000,
                         error: function(request,error) {
                             if (error == "timeout") {
                             alert('timeout');
                             }
                             else {
                                alert('Error! Please try again!');
                             }
                         },
                         success: function(data) {
                             var response = $.parseHTML( data );
                             if(data=="LocalRedirect" || $('input[name="USER_LOGIN"]',response).length==0){
                                 window.location.href = "/personal/";
                             } //success
                             else{

                             $('#modal-login .modal-content').html(data);
                             $('#modal-login form').attr('action',form_action);
                             $('#modal-login input[name=backurl]').val(form_backurl);
                             $('#modal-login a[href*="register"]').attr('href','#modal-register').attr('data-uk-modal','');
                             //$('#modal-login a[href*="forgot_password"]').attr('href','#modal-forgot-password').attr('data-uk-modal','');


                             $('input[name="USER_LOGIN"]').mask("+7(999)999 99 99");
                             $('input[name="USER_SMS_CODE"]').mask("9 9 9 9").focus();

                             }

                         }
                     });
                    return false;
                });


                var checked = false;


                function check_agree ($this){
                    if($($this).prop('checked')) {
                        checked = true;
                    }else
                    {
                        checked = false;
                    }
                    $($this).click(function() {
                        if($(this).prop('checked')) {
                            checked = true;
                        }else
                        {
                            checked = false;
                        }
                    });
                }
                //Обработчик формы регистрации
                $('#modal-register').on('submit','form',function() {
                    var chk_soc = false;
                    $('.soc_acc').each(function (i) {
                        if($(this).val()!='')chk_soc = true;
                    });
                    if(!chk_soc) {
                        showWarn('Укажите ссылку на один из профессиональных аккаунтов');
                       // $('.warn-err').find('p.append_warn').html('Укажите ссылку на один из профессиональных аккаунтов');
                        return false;
                    }  //soc* required




                    inp = '#modal-register input[name="UF_AGREE_INFO"]';
                    check_agree (inp);
                    if(checked)
                        $(inp).prop('checked',true);
                    if(!checked) {
                        showWarn('Подтвердите согласие с условиями обработки персональных данных');
                       // $('.warn-err').find('p.append_warn').html('Подтвердите согласие с условиями обработки персональных данных');
                        return false;
                    }
                    else {

                        var form_action = $(this).attr('action');
                        var form_backurl = $(this).find('input[name="backurl"]').val();

                        //--unmask--//
                        $('input[name="REGISTER[LOGIN]"]').each(function() {
                            $(this).val($(this).mask());
                        });

                        //--/unmask--//
                        $.ajax({
                            type: "POST",
                            url: dir+'/ajax/register.php',
                            data: new FormData(this),
                            timeout: 10000,
                            contentType: false,
                            cache: false,
                            processData: false,
                            error: function(request,error) {
                                if (error == "timeout") {
                                    alert('timeout');
                                }
                                else {
                                    alert('Error! Please try again!');
                                }
                            },
                            success: function(data) {
                                $('#modal-register .modal-content').html(data);
                                $('#modal-register form').attr('action',form_action);
                                $('#modal-register input[name=backurl]').val(form_backurl);
                                $('#modal-register a[href*="login"]').attr('href','#modal-login').attr('data-uk-modal','');

                                $('input[name="REGISTER[LOGIN]"]').mask("+7(999)999 99 99");

                                inp = '#modal-register input[name="UF_AGREE_INFO"]';
                                check_agree (inp);
                                if(checked)
                                $(inp).prop('checked',true);
                            }
                        });
                    }
                    return false;
                });

                //Обработчик сброса авторизации
                $('input[name="logout_butt"]').click(function(event){

                    event.preventDefault();
                    event.stopPropagation();
                    //location.reload();

                    window.location.href = "/personal/?logout=yes";
                });
            });

        </script>

