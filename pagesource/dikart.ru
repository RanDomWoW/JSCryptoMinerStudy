<script>
$(document).ready(function(){
	$('.tracery_pict_group_gallery').mousemove(function(e){
		var coll =  $(this).children().length
		if(coll > 9)
		{
			var 
				pos = $(this).parent().offset(), 		//положение блока с картинками, его координаты
				elem_top = pos.top, 					//вершина блока	
				Yinner = e.pageY - elem_top, 			//положение курсора внутри элемента
				h1 = $(".overflow_hidden_gallery").height(),	//высота блока в котором картинки становятся видны
				h2 = $(this).height(),					//высота блока с картинками которы будут показываться			
				y1 = 0,						//точка от которой будем отталкиваться  прокручивая блок с картинками (вершина видимого блока)
				y2 = y1-((Yinner-y1)*(h2-h1))/h1;		//расстояние на которое необходимо подвинуть блок с картинками
			
			//двигаем блок картинками.
			$(this).css("top",y2);
	
		}
	});
});
