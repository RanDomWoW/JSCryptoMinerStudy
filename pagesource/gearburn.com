jQuery(function($) {
    $.fn.slidebox = function() {
    var slidebox = this;
    var originalPosition = slidebox.css('right');
    var open = false;

    var boxAnimations;

      boxAnimations = {
        open: function() {
          slidebox.animate({
            'right': '0px'
          }, 300);
        },
        close: function() {
          slidebox.stop(true).animate({
            'right': originalPosition
          }, 100);
        }
      }
    
    $(window).scroll(function() {
        if (document.getElementById('iaa_content')) {
            var distanceTop = $('#iaa_content').offset().top + $('#iaa_content').height() - $(window).height();

            if ($(window).scrollTop() > distanceTop) {
                if (!open) {
                    open = true;
                    boxAnimations.open();
                }
            } else {
                open = false;
                boxAnimations.close();
            }
        }
    });

    slidebox.find('.close').click(function() {
      $(this).parent().remove();
    });
  }

  $(function() {
    $('#slidebox').slidebox();
  });
});

</script>
