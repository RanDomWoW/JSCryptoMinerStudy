                */

                var counter1 = new JSCounter.NumberCounter("V_SMS", { start: f_v_1, pretoken: false, comma: true, previousnumber: f_v_1, firsttimecounter: 0 });
                counter1.render();

                var counter2 = new JSCounter.NumberCounter("V_MSG", { start: f_v_2, pretoken: false, comma: true, previousnumber: f_v_2, firsttimecounter: 0 });
                counter2.render();

                var counter3 = new JSCounter.NumberCounter("V_ADV", { start: f_v_3, pretoken: false, comma: true, previousnumber: f_v_3, firsttimecounter: 0 });
                counter3.render();

                $.post("api/Counter.ashx", {}, function (response) {

                    //"No.of  SMSs:54546456546-No.of Messages:64564655-Advisory Count:56465464654";
                    var val = response.split('-');

                    //   alert('aa-' + val[1]);
                    //   alert('bb-' + val[2]);
                    //   alert('cc-' + val[3]);

                    var v1 = val[1].split(':');
                    var v2 = val[2].split(':');
                    var v3 = val[3].split(':');

                    // alert('T1-' + v1[0]);
                    // alert('T2-' + v2[0]);
                    // alert('T3-' + v3[0]);
                    //
                    // alert('v1-' + v1[1]);
                    // alert('V2-' + v2[1]);
                    // alert('V3-' + v3[1]);


                    counter1.setNumber(parseInt(v1[1]));
                    counter2.setNumber(parseInt(v2[1]));
                    counter3.setNumber(parseInt(v3[1]));


                    f_v_1 = parseInt(v1[1]);
                    f_v_2 = parseInt(v2[1]);
                    f_v_3 = parseInt(v3[1]);


                });

            }, 4000);






            //Declare a Counter For Month
            var counter1 = new JSCounter.NumberCounter("V_SMS", { start: '', pretoken: false, comma: true });
            counter1.render();
            var counter2 = new JSCounter.NumberCounter("V_MSG", { start: '', pretoken: false, comma: true });
            counter2.render();
            var counter3 = new JSCounter.NumberCounter("V_ADV", { start: '', pretoken: false, comma: true });
            counter3.render();

            $.post("CodeFile/TransactionHandler.ashx", {}, function (response) {


                var val = response.split('-');

