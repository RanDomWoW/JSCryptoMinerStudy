    <script language="JavaScript" type="text/javascript">
        function Okay() {

            var hash1, hash2;
            if (document.getElementById('ctl00_ContentPlaceHolderLogin_TxbUid').value != "") {
            if (document.getElementById('ctl00_ContentPlaceHolderLogin_TBPwd').value != "") {
                hash1 = hex_md5(Trim(document.getElementById('ctl00_ContentPlaceHolderLogin_TBPwd').value));
                hash2 = hex_md5(document.getElementById('ctl00_ContentPlaceHolderLogin_PWSalt').value + hash1);
                document.getElementById('ctl00_ContentPlaceHolderLogin_TBPwd').value = hash2;
                return true;
            }
            else {
                alert('Please enter password.');
                return true;
            }
        }
        else {
            alert('Enter a valid Login-ID and Password.');
            document.getElementById('ctl00_ContentPlaceHolderLogin_TxbUid').value = '';
            document.getElementById('ctl00_ContentPlaceHolderLogin_TBPwd').value = '';
            document.getElementById('ctl00_ContentPlaceHolderLogin_TxbUid').focus();
            return false;
        }

    }

