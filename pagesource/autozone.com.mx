				var BVenableShelf = 'false',
					BVenablePDP = 'false',
					BVtagShelf = '',
					BVtagPDP = '';
				//set flag to turn BazaarVoice on or off by checking BCC configs service
				if ((typeof bvPDP != "undefined" && bvPDP === true) || (typeof bvShelf != "undefined" && bvShelf === true)){
					$.ajax({
						url: "/rest/bean/autozone/diy/multisite/service/AZSiteConfigurationService/globalConfigs?atg-rest-depth=1",
						data: {"arg1": "RR_AZRWEB_SHELF_ENABLED,RR_AZRWEB_PDP_ENABLED,RR_AZRWEB_PDP_TAG,RR_AZRWEB_SHELF_TAG"},
						type: "GET",
						dataType: "json",
						success: function(response){
							var configData = response.globalConfigs;
							var bvConfigs = configData.globalConfigs
							BVenableShelf = bvConfigs.RR_AZRWEB_SHELF_ENABLED;
							BVenablePDP = bvConfigs.RR_AZRWEB_PDP_ENABLED;
							BVtagShelf = bvConfigs.RR_AZRWEB_SHELF_TAG;
							BVtagPDP = bvConfigs.RR_AZRWEB_PDP_TAG;
							//if Shelf BazaarVoice is enabled and we are on the shelf page...
							if (BVenableShelf === 'true' && bvShelf === true){
								//production script for Bazaar Voice (inline ratings)
								$.getScript(BVtagShelf).done(function(script, status){
									$BV.ui('rr', 'inline_ratings', {
										productIds: bvProductsObject,
										containerPrefix: 'BVRRInlineRating'
									});
								});
							}
							//if Product Detail page is enabled and we are on a detail page...
							if (BVenablePDP === 'true' && bvPDP === true){
								//production script for Bazaar Voice
								var bvTag = document.createElement('script');
								bvTag.setAttribute('async', '');
								bvTag.src = BVtagPDP;
								document.head.appendChild(bvTag);
								$('#reviewsBVoice').show();
								$('#ratingsTop').fadeIn(300);
							}
						}
					});
				}
			}
		});
