<script type="text/javascript">
function printParkinglot(success,result) {
    var aHTML = [];
    var target = document.getElementById("parkinglot");
    var sOnClickMethod;
	var parklot = [];

    if (result == null || !success) {
    	aHTML.push('<div class="parkinginfo">Uw parkeerplaats is leeg,<br>klik op<span class="parkingbtn small">P</span><br>om iets toe te voegen</div>');
    }
    else {
        for (var i = 0;i < result.length;i++) {            
                if (result != null && result[i] != null) {
                    aHTML.push('<div class="image">');
                    aHTML.push('<span class="removebtn" onClick="removeFromParkinglot('+ result[i].iVehicleId + ')">X</span>');
                        if (!result[i].bSold) {
                            sOnClickMethod = ("getVehicleDetailURL('"+result[i].iVehicleId+"')");
                            aHTML.push('<div class="parkinglotitem" onClick="'+sOnClickMethod+'">');
                            if (result[i].sVehicleMainPic != '') {
                                aHTML.push('<div class="blurred" style="background: url(/TruckPics/size1/'+result[i].sVehicleMainPic+') no-repeat center center;-webkit-background-size: cover; -moz-background-size: cover; -o-background-size: cover; background-size: cover;"></div>');  
                                aHTML.push('<div class="single">');
                                    aHTML.push('<picture>');
                                        aHTML.push('<!--[if IE 9]><video style="display: none;"><![endif]-->');
                                            aHTML.push('<source srcset="/TruckPics/size1/'+ result[i].sVehicleMainPic +'" media="(max-width: 768px)" />');
                                            aHTML.push('<source srcset="/TruckPics/size1/'+ result[i].sVehicleMainPic +'" media="(min-width: 769px) and (max-width: 992px)" />');
                                            aHTML.push('<source srcset="/TruckPics/size1/'+ result[i].sVehicleMainPic +'" media="(min-width: 993px) and (max-width: 1200px)" />');
                                            aHTML.push('<source srcset="/TruckPics/size1/'+ result[i].sVehicleMainPic +'" media="(min-width: 1201px)" />');
                                        aHTML.push('<!--[if IE 9]></video><![endif]-->');
                                        aHTML.push('<img srcset="/TruckPics/size1/'+ result[i].sVehicleMainPic +'"  alt="" />');
                                    aHTML.push('</picture>');
                                aHTML.push('</div>');
                            }
                            else {
                                aHTML.push('<div class="nopictureplaceholder"></div>');
                            }
                        }
                        else {
                            sOnClickMethod = ("showSoldSuggestions('"+result[i].iVehicleId+"')");
                            aHTML.push('<div class="parkinglotitem" onClick="'+sOnClickMethod+'">');
                            aHTML.push('<div class="soldplaceholder">Niet meer beschikbaar</div>');
                        }
                        aHTML.push('<div class="parkinginfooverlay">' + result[i].sMake + '<br><p class="truncatestring">' + result[i].sType + '</p></div>');
                    aHTML.push('</div>');
                    aHTML.push('</div>');
                    aHTML.push('</div>');
                }
            parklot.push(String(result[i].iVehicleId));
        }
		//Save list with findable vehicles
        setSettingValue('parkinglot',parklot);
    }
    target.innerHTML = aHTML.join('');
}
</script>
