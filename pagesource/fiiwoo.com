    <script type="text/javascript">
        //区域级联
        function AreaCascade(selectObj, fillObj, level, callback) {

            var areaname = $("#" + selectObj).find("option:selected").text();
            var areaid = $("#" + selectObj).val();

            if (areaname == '选择县市')
            {
                //地区互动
                $("#areatext").text($("#city").find("option:selected").text());
                $("#areaid").val($("#city").val());
            }
            else if (areaname == '选择地市') {
                //地区互动
                $("#areatext").text($("#province").find("option:selected").text());
                $("#areaid").val($("#province").val());
            }
            else {
                //地区互动
                $("#areatext").text(areaname);
                if (areaid == 'province')
                {
                    $("#areaid").val(86);
                }
                else $("#areaid").val(areaid);
            }            

            $.post("/Home/AreaCascade", { level: level, areaCode: areaid }, function (result) {
                //清空其他子区域集合
                switch (level) {
                    case "2":
                        $("#city").empty();
                        $("#city").append("<option value='city'>选择地市</option>");
                        $("#county").empty();
                        $("#county").append("<option value='county'>选择县市</option>");
                        $("#towns").empty();
                        $("#towns").append("<option value='towns'>选择乡镇</option>");
                        break;
                    case "3":
                        $("#county").empty();
                        $("#county").append("<option value='county'>选择县市</option>");
                        $("#towns").empty();
                        $("#towns").append("<option value='towns'>选择乡镇</option>");
                        break;
                    case "4":
                        $("#towns").empty();
                        $("#towns").append("<option value='towns'>选择乡镇</option>");
                        break;
                }

                //填充区域数据
                $.each(result, function (i, item) {
                    $('#' + fillObj).append("<option value='" + item.AreaCode + "'>" + item.AreaName + "</option>");
                });
                if ($.browser.msie == true && $.browser.version == "6.0") {
                    setTimeout(function () {
                        if (callback) {
                            callback();
                        }
                    }, 100);
                }
                else {
                    if (callback) {
                        callback();
                    }
                }

            });
        }
         
        //根据区域代码选中下拉事件
        $(function () {
            var proCode = '86';
            var cityCode = '';
            var townCode = '';
            var countryCode = '';
            selectedlist(proCode, "province");
            selectedlist(cityCode, "city");
            selectedlist(townCode, "towns");
            selectedlist(countryCode, "county");
        });

        //自动选中下拉框
        function selectedlist(val, id) {
            var isselected = false;
            var sel = document.getElementById(id);
            for (var i = 0; i < sel.options.length; i++) {
                if (sel.options[i].value == val) {
                    sel.options[i].selected = true;
                    isselected = true;
                }
                else {
                    sel.options[i].selected = false;
                }
            }
            //解决IE9默认下不能选中的问题，IE9显示的空白
            if (!isselected) {
                sel.options[0].selected = true;
            }
        }
        //区域搜索
        function SearchArea() { 
            //导航显示文本
            var province = $("select[name=province]").find("option:selected");
            var city = $("select[name=city]").find("option:selected");
            var country = $("select[name=county]").find("option:selected");
            var towns = $("select[name=towns]").find("option:selected");
            var reg = /^.*(下辖)+.*$/;
            var areaCode = "";
            if (province.text() != "大中华区") {
                if (!reg.test(province.text())) {
                    areaCode = province.val();
                }
                if (city.text() != "选择地市") {
                    if (!reg.test(city.text())) {
                        areaCode = city.val();
                    }
                    if (country.text() != "选择县市") {
                        if (!reg.test(country.text())) {
                            areaCode = country.val();
                        }
                        if (towns.text() != "选择乡镇") {
                            if (!reg.test(towns.text())) {
                                areaCode = towns.val();
                            }
                        }
                    }
                }
            }
            $("#areaCode").val(areaCode); 
            $('#tj').click(); 
            return true;
        }
        //大观搜索
        function SearchGrand() {

            //导航显示文本
            var province = $("select[name=province]").find("option:selected");
            var city = $("select[name=city]").find("option:selected");
            var country = $("select[name=county]").find("option:selected");
            var towns = $("select[name=towns]").find("option:selected");
            var reg = /^.*(下辖)+.*$/;
            var areaCode = "";
            if (province.text() != "大中华区") {
                if (!reg.test(province.text())) {
                    areaCode = province.val();
                }
                if (city.text() != "选择地市") {
                    if (!reg.test(city.text())) {
                        areaCode = city.val();
                    }
                    if (country.text() != "选择县市") {
                        if (!reg.test(country.text())) {
                            areaCode = country.val();
                        }
                        if (towns.text() != "选择乡镇") {
                            if (!reg.test(towns.text())) {
                                areaCode = towns.val();
                            }
                        }
                    }
                }
            }
            else {
                areaCode = 86;
            }
            if (areaCode == "undifend") {
                areaCode = 86;
            }
             
            window.location.href = "/Extend/FWGrand/" + areaCode;
        }
 

        function TownChanged(selectObj) {

            var areacode = $("#" + selectObj).val(); 
            var areaname = $("#" + selectObj).find("option:selected").text();

            if (areaname == '选择乡镇') {
                //地区互动
                $("#areatext").text($("#county").find("option:selected").text());
                $("#areaid").val($("#county").val());
            }
            else {
                $("#areaid").val(areacode);
                $("#areatext").text(areaname);
            }
            
        }
    </script>


 



