<script type="text/javascript">


    var IsReverseClick = false;
    function change() {
        IsReverseClick = false;
    }
    function reverseLocation() {
        IsReverseClick = true;
        var valueTo = $('#txtToAirportID').val();
        var hidvalueFrom = $('#hidFromAirportID').val();
        var hidvalueTo = $('#hidToAirportID').val();
        var valueFrom = $('#txtFromAirportID').val();
        $('#txtToAirportID').val(valueFrom);
        $('#txtFromAirportID').val(valueTo);
        $('#hidFromAirportID').val(hidvalueTo);
        $('#hidToAirportID').val(hidvalueFrom);
        setTimeout(function () { change(); }, 1000);
    }
    $(document).ready(function () {

        m = moment();
        $('#txtDate').val(m.format('jYYYY/jM/jD'));
          if("solar"=='solar') {
            $('#hidValue_txtDate').val(ConvertDateToMiladi($('#txtDate').val(),'solar'));
            $('#hidValueShamsi_txtDate').val($('#txtDate').val());
        }
        else {
            $('#hidValue_txtDate').val($('#txtDate').val());
            $('#hidValueShamsi_txtDate').val(ConvertDateToShamsi($('#txtDate').val(),'Gregorian'));
        }
          $('#txtReturnDate').click(function () {
              $('#txtReturnDate').val('');
              $('#hidValue_txtReturnDate').val('');
              $('#hidValueShamsi_txtReturnDate').val('');

              var textDate = $('#txtDate').val();
              var LanguageName;
              if ($("#toggle_txtDate").hasClass('aTab')) {
                  LanguageName = "gregorian";
                    loadCalendar($('#hidFromAirportID').val(), $('#hidToAirportID').val(), true, LanguageName,textDate);

              }
              else {
                  LanguageName = "solar";
                   loadCalendar($('#hidFromAirportID').val(), $('#hidToAirportID').val(), true, LanguageName,textDate);
              }
          });


          $('#rdbWent').prop('checked', true);
        function loadCalendar(sourceID, destinationID, isReturnDate, LanguageName,Date) {
            $.post("https://Calendar.Safarme.com/DualCalendar/Index", {
                ControlName: "txtDate",
                CalendarType: LanguageName, //"Gregorian",
                DefaultDate: Date,
                IsCheaperPriceForFlight: false,
                SourceID: sourceID,
                DestinationID: destinationID,
                BeforeDate: "0",
                IsReturnDate: isReturnDate,
                ControlNameReturn: "txtReturnDate",
                Type: "2",
                FunctionAfterChangeControl: ""
            }).success(function (data) {

                $("#divForDate").html(data);
                //eval(data.find("script").text());
            }).error(function (dt) {
                });

            //var defaultReturn = $('#txtDate').val();
            //$('#txtReturnDate').val(defaultReturn);

            //var defaultValueReturn = $('#hidValue_txtDate').val();
            //$('#hidValue_txtReturnDate').val(defaultValueReturn);


            //var defaultValueReturn_shamsi = $('#hidValueShamsi_txtDate').val();
            //$('#hidValueShamsi_txtReturnDate').val(defaultValueReturn_shamsi);

        }


            
        if (typeof Storage !== "undefined") {
            var SearchDataString = localStorage.getItem("SearchDatab3642705-78ba-4f5a-be67-e18ad8837255");
            if (SearchDataString != null) {
                SearchData = jQuery.parseJSON(SearchDataString);
                $('#hidFromAirportID').val(SearchData.Flight_SourceId);
                $('#hidToAirportID').val(SearchData.Flight_DestinationId);
                $('#txtFromAirportID').val(SearchData.Flight_Source);
                $('#txtToAirportID').val(SearchData.Flight_Destination);
                //$('#txtDate').val(SearchData.DepartingDate);
                //$('#txtReturnDate').val(SearchData.ReturningDate);
            }
        }
        

        loadCalendar($('#hidFromAirportID').val(), $('#hidToAirportID').val(), false, "solar", "1397/1/2");
        $('#txtDate').click(function () {
            if ($('#rdbSweep').is(":checked")) {
                if ($('#txtReturnDate').val() == '') {
                    $('#txtReturnDate').val('');
                    $('#hidValue_txtReturnDate').val('');
                    $('#hidValueShamsi_txtReturnDate').val('');
                    $('#txtDate').val('');
                    $('#hidValue_txtDate').val('');
                    $('#hidValueShamsi_txtDate').val('');
                }
                if ($("#toggle_txtDate").hasClass('aTab')) {
                    LanguageName = "gregorian";
                    loadCalendar($('#hidFromAirportID').val(), $('#hidToAirportID').val(), true, LanguageName, "2018/3/22");

                }
                else {
                    LanguageName = "solar";
                    loadCalendar($('#hidFromAirportID').val(), $('#hidToAirportID').val(), true, LanguageName, "1397/1/2");
                }
            }
        });
        $('.quux').iCheck({
            checkboxClass: 'icheckbox_flat-blue',
            radioClass: 'iradio_flat-blue'
        });

        $('#divFlightSearchData').bind('keydown', function (event) {

            callAccepyButton(event.keyCode);
        });
        function callAccepyButton(keyCode) {
            if (keyCode == 13 && $.trim($('#txtFromAirportID').val()).length > 0 && $.trim($('#txtToAirportID').val()).length > 0) {
                $('#btnSearch').click();
            }
            else if (keyCode == 13 && $.trim($('#txtFromAirportID').val()).length == 0) {
                $('#txtFromAirportID').focus();
            }
            else if (keyCode == 13 && $.trim($('#txtToAirportID').val()).length == 0) {
                $('#txtToAirportID').focus();
            }

        };
        $('#txtFromAirportID').keypress(function (e) {
            callAccepyButton(e.keyCode);
        });
        $('#txtToAirportID').keypress(function (e) {
            callAccepyButton(e.keyCode);
        });
        $.widget("custom.airportcomplete", $.ui.autocomplete, {

            _create: function () {
                this._super();
                this.widget().menu("option", "items", "> :not(.ui-autocomplete-category)");
            },

            _renderItem: function (ul, item) {


                if (item.type == 2) {
                    return $("<li >")
                    .attr("data-value", item.value)
                    .append($("<div class='autoComplete-1 ac-city'>").text(item.value))

    .append($("<div class='clear'>"))
                    .appendTo(ul);
                }
                else {
                    return $("<li >")
                 .attr("data-value", item.value)
                 .append($("<div class='autoComplete-1 ac-airport'>").text(item.value))
 .append($("<div class='clear'>"))
                 .appendTo(ul);
                }
            }
        });
        $("#txtFromAirportID").change(function () {
            $('#hidFromAirportID').val('');
        });
        $("#txtToAirportID").change(function () {
            $('#hidToAirportID').val('');
        });
        $('#txtFromAirportID').airportcomplete({
            minLength: 2,
            position: {
                my: "right top",
                at: "right bottom"
            },
            source: function (request, response) {
                if (request.term == "-default-") {
                    $.getJSON("/Scripts/data/location.json", function (data) {
                        response(data);
                    });
                }
                else {
                    $.get("/api/FlightApi/GetLocation?systemID=2aad5e28-3dd2-4674-8078-a40a143eaf88&languageID=b3642705-78ba-4f5a-be67-e18ad8837255&query=" + request.term).success(function (data) {
                        response(data);
                    });
                }

            },

            autoFocus: true,
            focus: function (event, ui) {
                $(document).on('click', function (e) {
                    var calenTarget = e.target;
                    if (!$(calenTarget).is('#txtFromAirportID')) {
                        if (IsReverseClick == false) {
                            $('#txtFromAirportID').val(ui.item.value);
                            $('#hidFromAirportID').val(ui.item.location);
                        }
                    }
