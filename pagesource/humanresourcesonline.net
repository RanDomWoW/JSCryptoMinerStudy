<script type="text/javascript">
 

(function($) {
function checkForm() {
    var emailAddress = $('#emailTfSmall').val();
    var errorMsg = '';
    var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,10})?$/;
    if (emailAddress == '') {
        errorMsg = "Empty Email Address" ;
        $('#emailTfSmall').addClass("has-error");
        var oldEmailVal = '';
        oldEmailVal = $('#emailTfSmall').val();
        $('#emailTfSmall').val(errorMsg);  
        var popError = setTimeout(function() {
          $('#emailTfSmall').val(oldEmailVal);
        },2000);
        //clearTimeout(popError);
        console.log(errorMsg);
        return false;       
    }
    else if( !emailReg.test( emailAddress ) ) {
        errorMsg = "Invalid Email Address" ;
        $('#emailTfSmall').addClass("has-error");
        var oldEmailVal = '';
        oldEmailVal = $('#emailTfSmall').val();
        $('#emailTfSmall').val(errorMsg);  
        var popError = setTimeout(function() {
          $('#emailTfSmall').val(oldEmailVal);
        },2000);
        //clearTimeout(popError);
        console.log(errorMsg);
        return false;   
    }                                                                                
    else {
      if ($('#emailTfSmall').hasClass("has-error")) {
        $('#emailTfSmall').removeClass("has-error");
      }             
      var invalAdd = /\@hotmail\.com|\@outlook\.com|\@gmail\.com|\@googlemail\.com|\@yahoo\.com|\@yahoo\.co\.|\@google\.com|\@tm\.net\.my|\@starhub\.net\.sg|\@singnet\.com\.sg|\@163\.com|\@qq\.com|\@126\.com|\@sina\.com|\@streamyx\.com|info\@|contact\@|enquiry\@|enquiries\@|inquiry\@|support\@|talk\@|customerservice\@|feedback\@|contactus\@|corporate\@|CustomerService\@|customercare\@|hello\@|hr\@|helpdesk\@/g; 
      if(invalAdd.test(emailAddress)) {
        errorMsg = "Please use a corporate email address" ;
        $('#emailTfSmall').addClass("has-error");
        var oldEmailVal = '';
        oldEmailVal = $('#emailTfSmall').val();
        $('#emailTfSmall').val(errorMsg);  
        var popError = setTimeout(function() {
          $('#emailTfSmall').val(oldEmailVal);
        },2000);
        //clearTimeout(popError);
        console.log(errorMsg);
        return false;
      }
      else {
        if ($('#emailTfSmall').hasClass("has-error")) {
          $('#emailTfSmall').removeClass("has-error");
          return true;
        }
        else {
          return true;
        }      
      }  
    }   
}
  $('.imgSubBtn').click(function(e) { 
    if (checkForm())
      $('#smallSub').submit();
    else 
      return false;
    });
})(jQuery);

</script>                                
