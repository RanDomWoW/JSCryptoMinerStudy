	$(function(){
		var param = "";
		$.ajax({
			 async:false,
	         type: "POST",
	         url: 'http://14.29.68.66:6200/dmDock/webDock/findQFWMessage',
	         data: param,
	         dataType: "json",
	         success: function(data){
	        	 if(data!=null){
	        	 	
	        		 var messageType = data.messageType;
	        		 var allMessage = data.message;
	        		 var normessage=data.normessage;
	        		 var messageNorType=data.messageNorType;
	        		 var messageKepuType=data.messagekepuType;
	        		 var NormalwebDockList=data.NormalwebDockList;
	        		 var html = "";
	        		 $("#messageContent").html('');
	        		 var title="";
	        		 if(messageType=='1'){
	        			// $("#messageTitle").text("预警信息");
	        			 for(var i=0;i<allMessage.length;i++){
	        				 var message = allMessage[i];
	        				 var messageContent = message.modelContent;
	        				 var icfilepath = message.icfilepath;
	        				 html+="<img src='"+icfilepath+"'></img>";
	        				 html+=messageContent+"";
	        				 title+=messageContent+"  <br>";
	        			 }
	        		 }
	        		if(messageNorType=='3'){
	        			if(data.content !=null){
	        				var messageContent=data.content;
	        				html=html+"【天气预报】"+messageContent+"";
	        				messageContent=messageContent.replace("\n","");
	        				title=title+"【天气预报】"+messageContent+" <br>";
	        			}
	        		}
	        		if(messageKepuType =='2'){
	        			for(var i=0;i<NormalwebDockList.length;i++){
	        				 var message = NormalwebDockList[i];
	        				 var messageContent = message.modelContent;
	        				 var icfilepath = message.icfilepath;
	        				 html+="<img src='"+icfilepath+"'></img>";
	        				 html+=messageContent+"";
	        				 title+=messageContent+"  <br>";
	        			 }
	        		}
	        		$("#messageContent").html(html);
		        	//$("#messageContent").attr("title",title);
		        	$("#messageTitle").attr("title",title);

	        	 }
	         },
			 error:function(e){
			 }
		});
	});
	
	function setMessageTitle(message){
		 var disasterName  = message.disasterName;
		 $("#messageTitle").text(disasterName);
	}
	
	function setMessageContent(message){
		 var messageTitle  = "";
		 var sendMessageId = message.sendMessageId;
		 var messageContent = message.modelContent;
		 var icfilepath = message.icfilepath;
		 var html = "";
		 if(sendMessageId=='3'){
			 html = messageContent
		 }
		 $("#messageContent").text(html);
	}
	
	
	
	var title_value = ''; 
	function title_show(td) {
	var div=document.getElementById("title_show");	
		title_value = td.title;
		div.style.left = (td.offsetLeft)+"px";	//设置title_show在页面中的位置。
		var words_per_line = 45;	//每行字数
		//var title = split_str(td.title,words_per_line);	//按每行25个字显示标题内容。
		div.innerHTML = td.title;
		div.style.display = '';
		td.title = '';	//去掉原有title显示。
	}
	
	function title_back(td) {
		var div=document.getElementById("title_show");	
		td.title = title_value;
		div.style.display = "none";
	}
</script>


