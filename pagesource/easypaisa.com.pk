<script>
    var span = jQuery('a.data-over').find("span");
    span.before("<span class='box'> </span>");
    span.after("<span class='low-arrow'> </span>");

    jQuery('a.data-over').hover(function () {
        var element = jQuery(this).find(".box");
        var text = jQuery(this).attr("data-over");
        element.text(text);
        var pos = parseFloat((element.outerWidth() / 2));
        if (pos > 20) {
            pos = pos - 20;
        } else {
            pos = 0;
        }
        element.css({left: "-" + pos + "px"});
    }, function () {
    });

</script>
