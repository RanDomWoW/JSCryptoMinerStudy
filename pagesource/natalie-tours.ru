                                          				fillCountries(xmlDoc);
                                          				var sCountries = document.getElementById("countries");
                                          				//sCountries.value = "";
                                          
                                          				if (initialLoadCountry) {
                                          					initialLoadCountry = false;
                                          					var stdSearchCookie = getCookieNT(COOKIE_STD_SEARCH_NAME).split(',');
                                          					var countryInitial = (parseInt(stdSearchCookie[2], 10) || COUNTRY_DEFAULT);
                                          					if (lovContains(sCountries, countryInitial)) {
                                          						sCountries.value = countryInitial;
                                          					}
                                          					getZones();
                                          				}
                                          
                                          			} else if(lov=="LOV.ZONEDYN" || lov=="LOV.ZONEDYNCTS"){
                                          				fillZones(xmlDoc);
                                          				var sZones = document.getElementById("zones");
                                          				if (sZones && sZones.options && sZones.options.length && sZones.options.length > 1) {
                                          					sZones.selectedIndex = 1;
                                          				} else {
                                          					sZones.value = "";
                                          				}
                                          
                                          			} else if(lov=="LOV.DEPARTCITYONLY" || lov=="LOV.DEPARTURECITY") {
                                          				fillDepartureCities(xmlDoc);
                                          
                                          				if (initialLoadDepartureCity) {
                                          					initialLoadDepartureCity = false;
                                          					var stdSearchCookie = getCookieNT(COOKIE_STD_SEARCH_NAME).split(',');
                                          					var departureCityInitial = (parseInt(stdSearchCookie[1], 10) || DEPARTURE_CITY_DEFAULT);
                                          					var sDepartureCities = document.getElementById("departureCities");
                                          					if (lovContains(sDepartureCities, departureCityInitial)) {
                                          						sDepartureCities.value = departureCityInitial;
                                          					}
                                          					initialLoadCountry = true;
                                          					getCountries();
                                          				}
                                          
                                          			} else if(lov=="LOV.TIPOREGIMEN") fillBoards(xmlDoc);
                                          			else if(lov=="LOV.HOTELSDYN") fillHotels(xmlDoc);
                                          		}
                                          
                                          		// Check if LOV contains the given value 
                                          		function lovContains(lov, val) {
                                          			if (lov && lov.options && lov.options.length) {
                                          				for (i = 0; i < lov.options.length; i++) {
                                          					if (val == lov.options[i].value) {
                                          						return true;
                                          					}
                                          				}
                                          			}
                                          			return false;
                                          		}
                                          
                                          			// ************************************************************************
                                          			// WEB SCRIPTING
                                          			// ************************************************************************
