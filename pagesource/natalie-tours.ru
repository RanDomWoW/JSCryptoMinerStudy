                                          				req = new XDomainRequest();
                                          			} else if (window.XMLHttpRequest){
                                          				 req = new XMLHttpRequest();
                                          			} else if (window.ActiveXObject){
                                          				try {
                                          					req = new ActiveXObject("Msxml2.XMLHTTP");
                                          				} catch(e) {
                                          					req = new ActiveXObject("Microsoft.XMLHTTP");
                                          				}
                                          			}
                                          			return req;
                                          		}
                                          
                                          		function throwRequest(params){
                                          			var req = getXMLRequest();
                                          
                                          			req.open("POST", URLb2c+params.toString(), true);
                                          
                                          			if (window.XDomainRequest) { // IE9
                                          				// https://cypressnorth.com/programming/internet-explorer-aborting-ajax-requests-fixed/
                                          				req.onerror = function () { };
                                          				req.onprogress = function () { };
                                          				req.ontimeout = function () { };
                                          				req.onload = function(){
                                          					handleResponse(new DOMParser().parseFromString(req.responseText, "text/xml"), params.getParamValue("lov"));
                                          				};
                                          				setTimeout(function () {
                                          					req.send();
                                          				}, 0);
                                          			} else {
                                          				req.onreadystatechange = function(){
                                          					if (req.readyState == 4 && req.status == 200)
                                          						handleResponse(req.responseXML,params.getParamValue("lov"));
                                          				}
                                          				req.send();
                                          			}
                                          		}
                                          
                                          		function handleResponse(xmlDoc,lov){
