	<!-- Images Defering -->
	<script>	
		// To check whether the user can see the image yet.
		function checkVisible(elm) {
	        var rect = elm.getBoundingClientRect();
	        var viewHeight = Math.max(document.documentElement.clientHeight, window.innerHeight);
	        return !(rect.bottom < 0 || rect.top - viewHeight >= 0);
	    }

		// To check whether the DOM has changed and make sure images / carousels are correctly loaded.
		var callback = function(allmutations) {			
			allmutations.map( function(mr) {

				$(".carouselLoader").remove();
			    $(".scrollable-lame").css("display","inherit");
				
				$(mr.target).find("img[data-src]").each(function(){
		            $(this).attr("src", $(this).attr("data-src"));
	                $(this).load(function(){
		                $(this).removeAttr("data-src");
	                });
	            });
			});
		},
		
		mo = new MutationObserver(callback),
		options = {
			'childList': true,
			'subtree': true
		}
		
		function loadVisibleImages(){
            $("img[data-src]").each(function(){
            	if(checkVisible($(this)[0])){
	                $(this).attr("src", $(this).attr("data-src"));
	                $(this).load(function(){
		                $(this).removeAttr("data-src");
	                });
            	}
            });
        }
		
		function loadDeferredImages(rootElement){
            rootElement.find("img[data-src]").each(function(){
                $(this).attr("src", $(this).attr("data-src"));
                $(this).load(function(){
	                $(this).removeAttr("data-src");
                });
            });
        }

		document.addEventListener('DOMContentLoaded', function(){
			loadVisibleImages();
			loadDeferredImages($(".scrollableTable"));
		});
		
        window.addEventListener('load', function(){
        	loadDeferredImages($("body"));	
    		mo.observe(document.body, options);
        });
	</script>
	<!-- Images Defering -->
