<script type="text/javascript">
$(document).ready(function() {

		//establecer foco por defecto
		$('input[name=dominio]','#formBuscadorDominios').focus();

		//validar y enviar formulario
		function validarDominio(dom) {
				if(dom=='' || dom.length<3 || dom.length>64) {
					return false;
				}
				if(!/\./.test(dom)) {
					dom += '.es';
				}
				if(!validarNombreDominio(dom)) {
					return false;
				}
			 return true;
		}

		function validarNombreDominio(dom)
		{
			var val = dom.toLowerCase();
			return /^([a-z0-9\u00E0\u00E1\u00E8\u00E9\u00ED\u00EF\u00F2\u00F3\u00FA\u00FC\u00E7\u00B7\u00F1\u00E7][a-z0-9\u00E0\u00E1\u00E8\u00E9\u00ED\u00EF\u00F2\u00F3\u00FA\u00FC\u00E7\u00B7\u00F1\u00E7\-]*[a-z0-9\u00E0\u00E1\u00E8\u00E9\u00ED\u00EF\u00F2\u00F3\u00FA\u00FC\u00E7\u00B7\u00F1\u00E7]\.)+[a-z]{2,64}$/i.test(val);
		}

		$('.btn-enviar','#formBuscadorDominios').on('click.enviarForm', function(evento) {
			evento.preventDefault();

			var repl = new RegExp('[^\-0-9a-z\u00F1\u00E0\u00E1\u00E8\u00E9\u00ED\u00EF\u00F2\u00F3\u00FA\u00FC\u00E7\u00B7\.]','g');

			var form = $('#formBuscadorDominios');
			var campo = $('input[name=dominio]',form);
			var defExt = typeof campo.data('default-ext') === 'undefined' || campo.data('default-ext') === '' ? '' : '.'+campo.data('default-ext');
			var dominio = $(campo).val().toLowerCase();
			dominio = (dominio.split('.').length === 1 ? dominio+defExt : dominio);
			dominio = dominio.replace(repl,'');
			campo.val(dominio);
			if (dominio=='' || !validarDominio(dominio)) {
				 if( !$('.popover',form).length )
					 campo.popover('show');
				 return false;
			}
			campo.popover('hide');
			form.submit();
			return true;
		});
		
		//desactivar envio con enter y texto vacío
		$('#formBuscadorDominios').on('keyup keypress', function(evento) {
			 var keyCode = evento.keyCode || evento.which;
			 if(keyCode===13) {
					evento.preventDefault();
					$('.btn-enviar',$(this)).trigger('click.enviarForm');
			 }
		});

});
</script>





