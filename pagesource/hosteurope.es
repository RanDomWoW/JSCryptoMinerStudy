<script type="text/javascript">
$(document).ready(function() {
	
	$.ajax({
		url: "/async/notifications",
		method: "POST",
		data: "view=root"
	}).done(function(response){
		if(!response.success) {return;}
		response.data.forEach(function(item,index){
			var tipo = item.tipo;
			var tmpl = Handlebars.compile(item.html);
			var html = tmpl(item.params);
			var css  = item.css;
			$("head").append(css);
			if(item.hasOwnProperty("script")) {
				var script = item.script;
				$("head").append(script);
			}
			switch(tipo) {
				case "banda":
					$(".notificaciones").append(html);
				break;
				case "modal":
					$(".notificaciones").after(html);
					$(html).modal("show");
				break;
			}
		});
		ObjNotif.preparar_bandas();
	});
	
}); 
</script>

