<script type="text/javascript">
var sb = new ShareButtons("From Pro Tools Expert");

function ShareButtons(text){

this.text = encodeURIComponent(text.trim());
this.url = window.location.href;
this.shares = 0;
this.hidden = false;
this.mobile = false;

this.buttons = [
{
buttonId: "facebookBtn",
color:"#3b5998",
imgURL:"http://static1.squarespace.com/static/54dcf1f3e4b03174c3dc75f5/t/570d191ab09f9591f4419e50/1460476186375/facebooklogo.png",
shareURL:"https://www.facebook.com/sharer/sharer.php?u="+this.url
},
{
buttonId: "twitterBtn",
color:"#55acee",
imgURL:"http://static1.squarespace.com/static/54dcf1f3e4b03174c3dc75f5/t/570d194a2fe1310178be5233/1460476234381/twitlogo.png",
shareURL:"https://twitter.com/intent/tweet?text="+this.url+" "+this.text
},
{
buttonId: "gplusBtn",
color:"#dd4b39",
imgURL:"http://static1.squarespace.com/static/54dcf1f3e4b03174c3dc75f5/t/570d19682fe1310178be535f/1460476264525/gpluslogo.png",
shareURL:"https://plus.google.com/share?url="+this.url
},
{
buttonId: "linkedInBtn",
color:"#0077b5",
imgURL:"http://static1.squarespace.com/static/54dcf1f3e4b03174c3dc75f5/t/570d19862fe1310178be549e/1460476294209/linkedinlogo.png",
shareURL:"https://www.linkedin.com/shareArticle?mini=true&url="+this.url+"&title="+this.text+"&summary=&source="
},
{
buttonId: "mailBtn",
color:"#128f4b",
imgURL:"http://static1.squarespace.com/static/54dcf1f3e4b03174c3dc75f5/t/570d19a32fe1310178be55f1/1460476323073/maillogo.png",
shareURL:"mailto:?&subject="+this.text+"&body="+this.url
}
],

this.init = function(){
$("body").append("<div id='shareBlock' style='width:50px;height:360px;top:50%;transform:translate(0%,-50%);position:fixed;z-index:999;'>"+
"<div id='buttonBlock' style='width:100%;height:100%;position:absolute'></div>"+
"</div>");

$("body").append("<div id='shareBlockMobile' style='width:100%;height:50px;bottom:0%;position:fixed;z-index:999;'>"+
"<div id='buttonBlockMobile' style='width:100%;height:100%;position:absolute'></div>"+
"</div>");

this.addSideButtons();
this.addMobileButtons();
this.addHoverListeners();
checkScreenSize();
},

window.onresize = function(event) {
checkScreenSize();
};

function checkScreenSize(){
var screenWidth = window.innerWidth;
if(screenWidth > 600){
$('#shareBlock').css('display','block');
$('#shareBlockMobile').css('display','none');
this.mobile = false;
}
else{
$('#shareBlock').css('display','none');
$('#shareBlockMobile').css('display','block');
this.mobile = true;
}
}

this.addMobileButtons = function(){
for(var i = 0; i<this.buttons.length; i++){

var buttonId = this.buttons[i].buttonId;
var imgURL = this.buttons[i].imgURL;
var shareURL = this.buttons[i].shareURL;
var color = this.buttons[i].color;

$("#buttonBlockMobile").append('<a style="text-decoration:none;" href="'+shareURL+'" target="_blank">'+
'<div class="mobBtnContainer" style="position:relative; width:20%; display:inline-block">'+
'<img class="socialLogoMobile" src="'+imgURL+'" style="width:30px;height:30px;max-width:30px;'+
'max-height:30px;min-width:30px;min-height:30px;position:absolute;left:50%;top:50%;transform:translate(-50%,-50%);"/>'+
'<div class="shareButtonMobile" id="'+buttonId+'" style="background-color:'+color+'; width:100%;'+
'height:50px; display:inline-block"></div>'+
'</div>'+
'</a>');
}
}

this.addSideButtons = function(){
for(var i = 0; i<this.buttons.length; i++){

var buttonId = this.buttons[i].buttonId;
var imgURL = this.buttons[i].imgURL;
var shareURL = this.buttons[i].shareURL;
var color = this.buttons[i].color;

$("#buttonBlock").append('<a style="text-decoration:none;" href="'+shareURL+'" target="_blank">'+
'<div class="shareButton" id="'+buttonId+'" style="top:'+(i+1)*55+'px; background-color:'+color+';'+
'width:30px;height:30px;border-radius: 100%;display:inline-block;padding:10px;position:absolute;">'+
'<img class="socialLogo" src="'+imgURL+'" style="position:relative;width:100%;height:100%;'+
'max-width:100%;max-height:100%;min-width:100%;min-height:100%;"/>'+
'</div>'+
'</a>');
}

$("#shareBlock").append('<div class="shareButton" id="expandBtn" style="top:'+(i+1)*55+'px; background-color:#aaaaaa;'+
'width:30px;height:30px;border-radius: 100%;display:inline-block;padding:10px;position:absolute;">'+
'<img class="socialLogo" style="position:relative;width:100%;height:100%'+
'max-width:100%;max-height:100%;min-width:100%;min-height:100%;" '+
'src="http://static1.squarespace.com/static/54dcf1f3e4b03174c3dc75f5/t/570d19c807eaa085f72bdbbe/1460476360434/arrow.png"/>'+
'</div>');
}

this.addHoverListeners = function(){
$(".shareButton").mouseenter(function(){
$(this).animate({
width: "36px",
height: "36px",
top:"-=3px",
left:"-=3px"
 }, 200 );
});

$(".shareButton").mouseleave(function(){
$(this).animate({
width: "30px",
height: "30px",
top:"+=3px",
left:"+=3px"
 }, 200 );
});

var self = this;
$("#expandBtn").click(function(){
if(self.hidden){
$("#buttonBlock").animate({
left:"0px"
}, 300 );
$(this).animate({
left:"0px",
opacity:"1"
}, 300 );
self.AnimateRotate(360);
self.hidden = false;
}
else{
$("#buttonBlock").animate({
left:"-50px"
}, 300 );
$(this).animate({
left:"-20px",
opacity:"0.6"
}, 300 );
self.AnimateRotate(180);
self.hidden = true;
}
});
}

this.AnimateRotate = function(angle) {
var $elem = $('#expandBtn');
$({deg: 0}).animate({deg: angle}, {
duration: 300,
step: function(now) {
$elem.css({
transform: 'rotate(' + now + 'deg)'
});
}
});
}

this.init();
}
</script>
