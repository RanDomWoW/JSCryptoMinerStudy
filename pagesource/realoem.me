    <script>
        $(document).ready(function () {
            var server = "http://www.realoem.me/";
            $("#search_vin").on("submit", function () {
                window.location.assign(server + "search/vin");
                return false;
            });
            $("#search_tvn").on("submit", function () {
                var tvn = $("#tvn").val();
                if (tvn === '') {
                    tvn = $("#tvn").attr("placeholder");
                }
                window.location.assign(server + "search/tvn/" + tvn);
                return false;
            });
