<script>
  jQuery(document).ready(function(){

    jQuery(".social").clone().appendTo(".muncul");
    var $container = jQuery('.bungkus-banner');

    $container.isotope({
      itemSelector: '.banner',
      masonry : {
        columnWidth : 98
      }
    });

	jQuery('a[data-toggle="tab"]').on('shown.bs.tab', function (e){
		jQuery('.list-group-item').removeClass('active');
		jQuery(this).addClass('active');
		$container.isotope('reLayout');
		
		// e.target;
	});

	$container.delegate( '.banner', 'click', function(){
      if(jQuery(this).hasClass('large')){
        jQuery(this).removeClass('large');
      }else{
        jQuery('.kategori-menu .banner.large').removeClass('large');
        jQuery(this).toggleClass('large');
      }
      $container.isotope('reLayout');
    });

    // Menu Collapse
    var $parent = jQuery('#info-tab .judul-menu');
    var $child = jQuery('#info-tab .judul-menu ~ a.list-group-item');

    function collapseMenu(){
    	$parent.removeClass("open");
    	$child.removeClass("open-child");
    }
    function unCollapseMenu(){
    	$parent.addClass("open");
    	$child.addClass("open-child");
    }
    jQuery(".collapse-toggle").click(function(e){
    	e.preventDefault();
    	if(jQuery(this).hasClass("aktif")){
    		jQuery(this).removeClass("aktif");
    	}else{
    		jQuery(".collapse-toggle").removeClass("aktif");
    		jQuery(this).addClass("aktif");
    	}
    });
    
  });
</script>


