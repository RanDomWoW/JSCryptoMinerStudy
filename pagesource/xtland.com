    var latestitem = 2;
    var active = 0;
    var latest_element = $(".btnlatest li.number").length;
    $(".box-latest-product > .latest").css("display", "none");
    $(".box-latest-product > .latest").slice(0, latestitem).css("display", "block");
    $('.btnlatest li.number').eq(0).addClass("active");


    $('.btnlatest li.number a').click(function() {
        var element_index = $('.btnlatest li.number a').index(this);
        active = element_index;
        $('.btnlatest li.number').removeClass("active");
        $('.btnlatest li.number').eq(element_index).addClass("active");
        $(".box-latest-product > .latest").hide();
        $(".box-latest-product > .latest").slice(element_index * latestitem, element_index * latestitem + latestitem).show();
        return false;
    });

    $('.btnlatest li a.prev').click(function() {
        active = active - 1;
        if (active < 0) {
            active = 0;
        }
        $('.btnlatest li.number').removeClass("active");
        $('.btnlatest li.number').eq(active).addClass("active");
        $(".box-latest-product > .latest").hide();
        $(".box-latest-product > .latest").slice(active * latestitem, active * latestitem + latestitem).show();
        return false;
    });

    $('.btnlatest li a.next').click(function() {
        active = active + 1;
        if (active >= latest_element) {
            active = latest_element - 1;
        }
        $('.btnlatest li.number').removeClass("active");
        $('.btnlatest li.number').eq(active).addClass("active");
        $(".box-latest-product > .latest").hide();
        $(".box-latest-product > .latest").slice(active * latestitem, active * latestitem + latestitem).show();
        return false;
    });
