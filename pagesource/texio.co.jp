<script>
	var brandName    = '';
	var feedinStatus = 0;
	function exBrandModal(brand){
		brandName = brand;
		if(feedinStatus == 0)
		{
			$('.exModalBackground').fadeIn('slow');
			$('.exModalBackgroundClose').fadeIn('slow');
			$('.exModal.'+brandName).fadeIn('slow');
			feedinStatus++;
		}
		else
		{
			$('.exModal.'+brandName).fadeOut('slow');
			$('.exModalBackgroundClose').fadeOut('slow');
			$('.exModalBackground').fadeOut('slow');
			feedinStatus--;
		}
	}
	$(document).on('click', '.exModalBackground', function(){
		$('.exModal.'+brandName).fadeOut('slow');
		$('.exModalBackgroundClose').fadeOut('slow');
		$(this).fadeOut('slow');
		feedinStatus--;
	});
	$(document).on('click', '.exModalBackgroundClose', function(){
		$('.exModal.'+brandName).fadeOut('slow');
		$(this).fadeOut('slow');
		$('.exModalBackground').fadeOut('slow');
		feedinStatus--;
	});
</script>
