<script>
	$(function(){
		
		var setTimeoutDescriptor = null;
		var remove_pop_up_time = 50;

		$( ".tiles-section .tiles-experts .tiles-expert" ).hover(
				function() {
					if(setTimeoutDescriptor){
						clearTimeout(setTimeoutDescriptor);
						setTimeoutDescriptor = null
					}
					$('.tiles-expert-pop-up.show').remove();
					var offset = $(this).offset();
					var expert_id = $(this).attr('data-expert-id');
					var pop_up_origin = $('.tiles-expert-pop-up[data-expert-id="'+expert_id+'"]');
					if($(pop_up_origin).length){
						var height_pop_up =  $(pop_up_origin).outerHeight();
						var width_expert_block = $(this).width();
						var pop_up = $(pop_up_origin).clone();
						pop_up.css({
							'position':'absolute',
							'top':((offset.top - height_pop_up))+'px',
							/*'left':(offset.left - minus_left_offset)+'px',*/
							'display': 'block',
						});
						pop_up.addClass('show');
						var all_th = $(pop_up).find('table tr th');
						setHoverForExpertPopUp($(pop_up));
						$('body').append($(pop_up));
						var max_width_th = 0;
						$(all_th).each(function( i, val ) {
							max_width_th = max_width_th < $(this).width() ? $(this).width() : max_width_th;
						});
						$(all_th).width(max_width_th);
						var width_pop_up = $(pop_up).width();
						var minus_left_offset = (width_pop_up - width_expert_block) / 2;
						pop_up.css('left',(offset.left - minus_left_offset)+'px');
					}
				}, function() {
					setTimeoutDescriptor = setTimeout(function(){
						$('.tiles-expert-pop-up.show').remove();
						setTimeoutDescriptor = null;
					},remove_pop_up_time);
				}
		);

		function setHoverForExpertPopUp(el){

			$(el).hover(
					function() {
						if(setTimeoutDescriptor){
							clearTimeout(setTimeoutDescriptor);
						}
					}, function() {
						setTimeoutDescriptor = setTimeout(function(){
							$('.tiles-expert-pop-up.show').remove();
							setTimeoutDescriptor = null;
						},remove_pop_up_time);
					}
			);

		}

	})
</script>
					
