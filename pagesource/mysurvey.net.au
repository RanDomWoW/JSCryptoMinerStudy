        <SCRIPT type="text/javascript">
                function addQueryString(element){
                  var queryStr = window.location.search.toString();

                  if(queryStr.length){

                    queryStr = queryStr.substring(1);

                    //query string exists - check if action has a value
                    var actionVal = getQueryVariable("action");

                    if(actionVal){
                      queryStr = queryStr.replace("action="+actionVal,"");

                      if(queryStr.indexOf("&") == 0 ){
                          queryStr = queryStr.substring(1);

                    } else if(queryStr.indexOf("&&")!=-1){
                        queryStr = queryStr.replace("&&","&");

                    } else if(queryStr.substring(queryStr.length-1,queryStr.length) == "&"){
                            queryStr= queryStr.substring(0,queryStr.length-1);
                    }

                    }
                    element.href = element.href +"&"+ queryStr;
                  }
                } //end function

                function getQueryVariable(variable)
                {
                   var query = window.location.search.substring(1);
                   var vars = query.split("&");
                   for (var i=0;i<vars.length;i++) {
                           var pair = vars[i].split("=");
                           if(pair[0] == variable){return pair[1];}
                   }
                   return(false);
                }

                function getQueryStringWithoutAction(){
                  var queryStr = window.location.search.toString();

                    if(queryStr.length){

                        queryStr = queryStr.substring(1);
                        //query string exists - check if action has a value
                        var actionVal = getQueryVariable("action");

                        if(actionVal){
                            queryStr = queryStr.replace("action="+actionVal,"");
                            if(queryStr.indexOf("&") == 0 ){
                                queryStr = queryStr.substring(1);
                            }else if(queryStr.indexOf("&&")!=-1){
                                queryStr = queryStr.replace("&&","&");
                            }else if(queryStr.substring(queryStr.length-1,queryStr.length) == "&"){
                                    queryStr= queryStr.substring(0,queryStr.length-1);
                            }
                        }
                        return queryStr;
                    }
                }
                </SCRIPT>
                

                
