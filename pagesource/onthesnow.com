        _delayMultiplier: 1,
        _url: '/ajax/simpleResortSearch',
        q: [],
        _pause: false,
        _length: 0,
        _prevLength: 0,
        resorts : [],
        
        _customKeyCodes: { 
            40: 'downArrow',
            38: 'upArrow',
            13: 'enter'
        },
        
        _searchBox: '#subSFsearch',
        _resultsDiv: '#subSFresults',
        
        init: function(){
            $(subSF.Search._searchBox).keyup(function(e) {
                subSF.Search._prevLength = subSF.Search._length;
                subSF.Search._length = $(e.target).val().replace(/ +(?= )/g,'').length;
                
                if( e.keyCode == 8 || subSF.Search._prevLength == subSF.Search._length || subSF.Search._prevLength - subSF.Search._length >= 1 ) {
                    subSF.Search._pause = false;
                    subSF.Search._delayMultiplier = 1.25;
                }
                subSF.Search.addToQueue( function() { subSF.Search.query( e ); } );
            });

            $('#subSFresults li,#snowreport_search li,#search_page_results li').on('mouseover', function(){
                $(this).addClass('highlight');
            })
            .on('mouseout', function(){
                $(this).removeClass('highlight');
            });
        },
        
        addToQueue: function( func ) {
            subSF.Search.q = []; // First reset queue to save server calls
            subSF.Search.q.push( func ); // Add to the queue;
            
            if( !subSF.Search._pause ) {
                setTimeout( 'subSF.Search.processQueue()', subSF.Search._delay*1000*subSF.Search._delayMultiplier );
            }
            
            
        },
        
        processQueue: function() {
            if( subSF.Search.q.length == 0 ) {
                return false;
            } else {
                subSF.Search.q.shift()();
            }
        },
        
        query: function( e ) {
            
            var term = $(e.target).val().replace(/ +(?= )/g,'');
            
            //${data.id_name}.Search._searchBox = '#' + $( e.target ).attr('id');
            //${data.id_name}.Search._resultsDiv = '#${data.id_name}results';
            if(subSF.Search._customKeyCodes[e.keyCode]){
                return subSF.Search[subSF.Search._customKeyCodes[e.keyCode]](e);
            }
            
            if(term.length < 3){
                subSF.Search._pause = false;
                return false;
            }
            $.post(subSF.Search._url, {search: term, jsClass: 'subSF'}, function(data){ 
                    if( data=='' ) {
                        subSF.Search._pause = true;
                    }
                    
                    subSF.Search.callback(data);
                    subSF.Search._delayMultiplier = 1;
                }
            );
            
        },
        
        callback: function(data){
            $(subSF.Search._resultsDiv).html(data);
            $(subSF.Search._resultsDiv).show();
            $('body').bind('click', function(){
                $(subSF.Search._resultsDiv).hide();
                $('body').unbind('click');
                if($(subSF.Search._searchBox).val() === ''){
                    $(subSF.Search._searchBox).val(OTS.Strings.Search.nameFinder);
                }
            });
        },
        
        //todo: scroll div with keyboard as well?
        downArrow: function(e){
            var $active = $(subSF.Search._resultsDiv + ' li.highlight');
            if(!$active){ 
                $active = $(subSF.Search._resultsDiv + ' li:first'); 
            }
           
            var $next = ($active.next().length === 0 
                ? $(subSF.Search._resultsDiv + ' li:first') 
                : $active.next());
             
            $active.removeClass('highlight');
            $next.addClass('highlight');
            
            return false;
        },
        
        upArrow: function(e){
            var $active = $(subSF.Search._resultsDiv + ' li.highlight');
            if(!$active){ 
                $active = $(subSF.Search._resultsDiv +  'li:last'); 
            }
           
            var $prev = ($active.prev().length == 0 
                ? $(subSF.Search._resultsDiv + ' li:last') 
                : $active.prev());
               
            $active.removeClass('highlight');
            $prev.addClass('highlight');
            return false;
            
        },
        
        enter: function(e){
            //off to highlighted item
            if($(subSF.Search._resultsDiv + ' li.highlight').length){
                window.location = $(subSF.Search._resultsDiv + ' li.highlight a').attr('href');
                return false;
            }
            //submit form for full search results page
            else {
               if ($(subSF.Search._resultsDiv + ' li a').length == 1) {
                   window.location = $(subSF.Search._resultsDiv + ' li a').attr('href');
                   return false;
               } else {
                   //validateGlobalSearch();
                   return true;
               }
            }
        },
        
        addResort: function(id, resortname) {
            if ($.inArray(id, subSF.Search.resorts) < 0) {
                $('#subSFForm input[name=resort_id]').val(id);
                $('#subSFForm input#subSFsearch').val(resortname);
                resortId = id;
            }
        },
        
    };
