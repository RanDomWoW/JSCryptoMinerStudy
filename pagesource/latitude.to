<script type="text/javascript">
  var
    c,
    gmz;

  $(document).ready(function() {
    var
      auc = new GMapz.autocomplete($('.g-search'));
      gmz = new GMapz.map(
        $('#gmapz'),
        {
          'center': [48.85341, 2.3488],
          'zoom': 12
        },
        {
          city: {
            pin: 'coo',
            lat: 48.85341,
            lng: 2.3488,
            draggable: true,
            iw : LL.msg.drag
          }
        }
      );

    auc.onChange = function () {
      var
        place = this.instance.getPlace(),
        pos = {};

      if(typeof place.geometry === 'undefined') {
        return false;
      }

      pos['autocomplete'] = {
        pin: 'coo',
        lat: place.geometry.location.lat(),
        lng: place.geometry.location.lng(),
        draggable: true,
        iw: LL.msg.drag
      };
      gmz.deleteAllMarkers().addLocations(pos).fitToPlace(place,18).openInfoWindow('autocomplete');

      c = new GPS(place.geometry.location.lat()+' '+place.geometry.location.lng());
      LL.ui.updateGeoBox(c, encodeURIComponent(place.formatted_address));
    };

    gmz.onMarkerDragEnd = function(marker) {
     var
       pos = marker.getPosition();
       gmz.map.panTo(pos);
       c = new GPS(pos.lat()+' '+pos.lng());
       $('.g-search').val('').attr('placeholder','Type a location...');
       LL.ui.updateGeoBox(c);
    };

        
    if(MQBE.current_state() === 'mobile') {
      gmz.onReady = function() {
        this.addScrollControl();
        this.lockScroll();
      }
    }

  });
</script>

