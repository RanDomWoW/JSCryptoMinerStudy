	<script type="text/javascript">
		$(document).ready(function() {
			$('#play').hide();
			$('#diapo1').slick({
				dots: true,
				fade: true,
				infinite: true,
				autoplay:true,
				autoplaySpeed:5000,
				arrows:false,
				speed: 2000,
				slidesToShow: 1,
				slidesToScroll: 1,
				pauseOnHover:true
			});
			
			$('#play').click(function(){
				$("#diapo1").slickPlay();
				$('#play').hide();
				$('#pause').show();
			});

			$("#pause").click(function() {
				 $("#diapo1").slickPause();
				 $('#pause').hide();
				 $('#play').show();
			});
			
		});
	</script>
			
	
