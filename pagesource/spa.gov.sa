function GetWeather() {
    $("#WeatherCarousel .carousel-inner").empty();

    $.ajax({
        url: "http://www.spa.gov.sa/notifications.php",
        type: "GET",
        format: "json",
        dataType: "json",
        success: function (data) {
            response = jQuery.parseJSON(JSON.stringify(data));
            //console.log(response);
            var activclass = 'active';
            $.each(response, function (key, value) {

                var htmlitem = '<div class="item ' + activclass + ' weatheritem clearfix">'
                    + '<a href="http://www.spa.gov.sa/weathermap.php" class="cursorlink">'
                    + '<h4 class="contryName" >' + value.regionAR + '</h4 >'
                    + '<div class="WeatherIcon"><img class="alertIcon" src="https://www.pme.gov.sa' + value.alertTypeCode + '" /></div>'          
                    +'<div class="WeatherText" onclick="window.location.href=\'weathermap.php\';">' + value.descriptionAr + '</div>'                            
                    + '</a></div>';
                activclass = '';

                $("#WeatherCarousel .carousel-inner").append(htmlitem);

            });
            if (response.length == 1) {
                $("#WeatherCarousel .carousel-control").hide();         
            }
            else {
                $("#WeatherCarousel .carousel-control").show();   
            }
        }
    });
}

 
 
 
