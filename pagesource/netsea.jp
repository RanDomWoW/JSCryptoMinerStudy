function controllNavi() {
    var content = jQuery('#content');
    var contentTop = 0;
    if ( document.getElementById("content") ) {
        contentTop = content.offset().top;
    }
    var recommend = jQuery('#recommend');
    var recommendTop = 0;
    if ( document.getElementById("recommend") ) {
        recommendTop = recommend.offset().top;
    }
    var itemHist = jQuery('#itemHist');
    var userRecom = jQuery('#userRecom');
    var categRanking = jQuery('#categRanking');
    var googletag = jQuery('#div-gpt-ad-1442468057153-0');
    var w = jQuery(window);
    var adjust = function(){
        var recommendHeight = recommend.outerHeight(true);
        if ( recommendHeight == 0 ) {
            return;
        }
        var winTop = w.scrollTop();
        var recommendTopNow = 0;
        if ( document.getElementById("recommend") ) {
            recommendTopNow = recommend.offset().top;
        }
        var itemHistTopNow = 0;
        if ( document.getElementById("itemHist") ) {
            itemHistTopNow = itemHist.offset().top;
        }
        var categRankingTopNow = 0;
        if ( document.getElementById("categRanking") ) {
            categRankingTopNow = categRanking.offset().top;
        }
        var userRecomTopNow = 0;
        if ( document.getElementById("userRecom") ) {
            userRecomTopNow = userRecom.offset().top;
        }
        var googletagTopNow = 0;
        if ( document.getElementById("div-gpt-ad-1442468057153-0") ) {
            googletagTopNow = googletag.offset().top;
        }
        var contentHeight = content.outerHeight(true);
        var itemHistHeight = itemHist.outerHeight();
        var userRecomHeight = userRecom.outerHeight();
        var categRankingHeight = categRanking.outerHeight();
        if ( itemHistHeight == 0 || itemHist.css('display') == 'none'  ) {
            itemHistTopNow = 0;
        }
        if ( userRecomHeight == 0 || userRecom.css('display') == 'none' ) {
            userRecomTopNow = 0;
        }
        if ( categRankingHeight == 0 || categRanking.css('display') == 'none' ) {
            categRankingTopNow = 0;
        }
        if( googletag.css('display') == 'none' ) {
            googletagTopNow = 0;
        }
        var lastContentTopNow = Math.max(itemHistTopNow, categRankingTopNow, userRecomTopNow, googletagTopNow);
        var otherLastContentHeight = lastContentTopNow - recommendTopNow;
        if (winTop > recommendTop + otherLastContentHeight){
            var newHeight = winTop - otherLastContentHeight;
            var lastContentHeight = recommendTopNow + recommendHeight - lastContentTopNow;
            if (winTop + lastContentHeight < contentTop + contentHeight) {
                recommend.css({
                    position: 'absolute',
                    top: newHeight
                });
            }
        } else {
            recommend.css('position', 'static');
        }
    }
    w.scroll('scroll', adjust);
}
