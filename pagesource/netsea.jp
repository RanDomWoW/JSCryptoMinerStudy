function setNewExhibitData() {
  var targetDate = new Date();
  setNewExhibitCount('today_date', 'today_item', targetDate);
  targetDate.setDate(targetDate.getDate() - 1);
  setNewExhibitCount('yesterday_date', 'yesterday_item', targetDate);
  targetDate.setDate(targetDate.getDate() - 1);
  setNewExhibitCount('dbyesterday_date', 'dbyesterday_item', targetDate);
}
function setNewExhibitCount(dateId, itemId, targetDate) {
  try {
    var mm = ('0' + (targetDate.getMonth() + 1)).slice(-2);
    var dd = ('0' + targetDate.getDate()).slice(-2);
    $('.' + dateId).text(mm + '/' + dd);
    var request = new XMLHttpRequest();
    var yyyymmdd = targetDate.getFullYear() + mm + dd;
    request.open('GET', '/new_item_count/' + yyyymmdd + '.txt');
    request.onreadystatechange = function() {
      if (request.readyState == 4 && request.status == 200) {
        $('.' + itemId).text(request.responseText);
        $('#' + itemId + 'f_08').text('商品');
        if (dateId == 'today_date') {
          var modDate = new Date(request.getResponseHeader("last-modified"));
          var hours = ('0' + modDate.getHours()).slice(-2);
          var minutes = ('0' + modDate.getMinutes()).slice(-2);
          $('.' + dateId).text(hours + ":" + minutes + '　更新');
        }
      } else {
        $('.' + itemId).text('未集計');
      }
    }
    request.send(null);
  } catch (e) {
  }
}
