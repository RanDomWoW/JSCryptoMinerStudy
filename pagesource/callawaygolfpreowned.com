$(function () { $("input").not("[type=button]").jqBootstrapValidation(); } );
function HeaderAuthentication() {
var url = "/on/demandware.store/Sites-CGPO4-Site/default/Login-HeaderLogin";
var system = $("#system");
var menu = $("#sign-in-menu");
var formErr = menu.find(".formError");
var errMsg = function (msg) {
var text = $("<p>", {
"class": "errorMsg smallText"
}).html(msg);
if (msg) {
errTimer && clearTimeout(errTimer);
formErr.append(text);
errTimer = setTimeout(function () {
text.remove();
}, 30000);
}
};
var errTimer;
$.ajax({
type: 'POST',
url: url + "?username=" + $('#username').val() + "&password=" + $('#password').val() + "&rememberme=" + $('#rememberme').is(':checked'),
cache: false,
context: system,
dataType: 'json',
beforeSend: function () {
errTimer && clearTimeout(errTimer);
formErr.html("");
},
complete: function (xhr, status) {
switch (status.toLowerCase()) {
case "timeout":
errMsg("It's taking longer than expected to sign in. Please try again.");
break;
case "abort":
errMsg("Sign in request aborted. Please try again.");
break;
case "parseerror":
case "error":
errMsg("Unable to complete your request. Please try again.");
};
},
timeout: 45000,
success: function (data) {
var refreshUrl = window.location && location.href && location.href.replace(/\.*$/gi, "");
if(refreshUrl.indexOf("Gigya") > -1) {
refreshUrl = "/account";
}
var ts = new Date();
if (data.status === "OK") {
if (refreshUrl) {
location.href = $.appendKeyValueToURL(refreshUrl, "signIn", (ts && ts.getTime() || ""));
} else {
errMsg("Please verify your Email Address and Password are correct, and try again.");
}
} else {
errMsg("Please verify your Email Address and Password are correct, and try again.");
};
},
async: true
});
}
