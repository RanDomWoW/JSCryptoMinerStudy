<script type="text/javascript">

    var cart     = null;

    jQuery(document).ready( function() {

        cart     = jQuery('#link-basket');
        cartClick = false;

        if (cart.length) {
            // init du gestionnaire js du panier
            var basketHandler = BasketHandler.getInstance('//www.teleshopping.fr/customer/ajax/getData/');
            var basketContainer = jQuery("#minicart-wrapper");
            var formProduct = jQuery("#product_addtocart_form");

            basketHandler.setLinkBasket(cart);
            basketHandler.setBasketContainer(basketContainer);
            basketHandler.setMaxProductInBasket(MAX_PRODUCT_IN_BASKET);

            if (formProduct.length) {
                basketHandler.setFormProduct(formProduct);
            }

            cart.click(function(){
                if(cartClick == true || !basketContainer.hasClass('visible')) {
                    basketHandler.toggleBasket();
                }
                cartClick = true;
            });

            cart.mouseenter(function(){
                if( basketHandler.nbItems > 0 && !basketContainer.hasClass('visible')) {
                    cartClick = false;
                    jQuery('body').click();
                    basketHandler.toggleBasket();
                }
            });
            cart.mouseleave(function(e){
                var target = jQuery(e.target);

                if (cartClick==false && !target.hasClass('tsh-active-basket') && basketContainer.hasClass('visible')) {
                    basketHandler.toggleBasket();
                }
            });
            basketContainer.mouseleave(function(e){
                if (cartClick==false && basketContainer.hasClass('visible')) {
                    basketHandler.toggleBasket();
                }
            });

            // ajout d'un pdt dans le panier à partir du cartouche de lacement d'achat sur la fiche pdt
            var buttonAddToCart = jQuery("#add-product-to-cart");

            jQuery("a[data-add-to-basket-url]").click(function (event) {
                var imgToFly = jQuery('.product-essential .product-image .product-image-gallery');
                if (imgToFly !== undefined) {
                    basketHandler.setImgtofly();
                }
                basketHandler.addToBasketWithLinkAddBasket(jQuery(this));
                event.preventDefault();
            });

            buttonAddToCart.click(function (event) {
                if (jQuery(this).data('product-id') === undefined) {
                    return false;
                }

                if (typeof productAddToCartForms[jQuery(this).data('product-id')] != 'undefined') {
                    var formObject = productAddToCartForms[jQuery(this).data('product-id')];
                }

                if (typeof formObject != 'undefined') {

                    formObject.submit(this, function(mediaHandler){
                        basketHandler.setImgtofly(mediaHandler.getCurrentSnapshot());
                        basketHandler.addToBasketWithFormProduct(jQuery(formObject.form));
                    });
                }
                event.preventDefault();
            });
        }
    });
</script><script type="text/javascript">
    var dataProvider = CustomerDataHandler.getInstance();
    dataProvider.setUrl('//www.teleshopping.fr/customer/ajax/getData/');

    new InterstitialHandler(dataProvider);
</script>    </div>
