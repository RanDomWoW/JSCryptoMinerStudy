        <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.2.1/owl.carousel.min.js"></script>
        <script type="text/javascript">
			

					$(function () {
                var reviewsSlider = $('#customer-reviews-slider');
                if (reviewsSlider.length > 0) {
                    if (typeof loadReviewDynamically !== 'undefined') {
                        $.get('/en/home/reviews/',
                            function(a) {
                                reviewsSlider
                                    .append(a)
                                    .owlCarousel({
                                        //navigation: true,
                                        //singleItem: true,
                                        //autoHeight: true
                                        items: 1,
                                        loop: false,
                                        autoHeight: true,
                                        responsiveClass: true,
                                        navText: ['<i class="fa fa-chevron-left"></i>', '<i class="fa fa-chevron-right"></i>'],
                                        responsive: {
                                            0: {
                                                nav: false,
                                                dots: true
                                            },
                                            540: {
                                                nav: false,
                                                dots: true,
                                            },
                                            1140: {
                                                nav: true,
                                                dots: true
                                            }
                                        }
                                    });
                                attachReviewsBtns();
                                collapseLongReviews();
                            });
                    } else {
                        reviewsSlider.owlCarousel({
                            //navigation: true,
                            //singleItem: true,
                            //autoHeight: true
                            items: 1,
                            loop: false,
                            autoHeight: true,
                            responsiveClass: true,
                            navText: ['<i class="fa fa-chevron-left"></i>', '<i class="fa fa-chevron-right"></i>'],
                            responsive: {
                                0: {
                                    nav: false,
                                    dots: true
                                },
                                540: {
                                    nav: false,
                                    dots: true,
                                },
                                1140: {
                                    nav: true,
                                    dots: true
                                }
                            }
                        });
                        attachReviewsBtns();
                        collapseLongReviews();
                    }

                }
                $(document).on('click', '.js-reviews-read-more', function () {
                    var p = $(this).parent(),
                        btn = $(this);
                    p.toggleClass('collapsed');
                    btn.text(p.hasClass('collapsed') ? btn.data('read-more') : btn.data('read-less'));
                    setTimeout(function() {
                        $('#customer-reviews-slider').trigger('refresh.owl.carousel');
                    }, 200);

                });
            });
            function attachReviewsBtns() {
                var btn = $('<button />')
                    .addClass('js-reviews-read-more')
                    .text("Read more")
                    .data({
                        'read-more': "Read more",
                        'read-less': "Read less"
                    });
                $.each($('.review-content'), function (i, v) {
                    $(v).append(btn.clone(true));
                });
            }
            function collapseLongReviews() {
                $('.review-content').removeClass('collapsed collapsible');
                $.each($('.review-content'), function (i, v) {
                    var p = $(v).find('p'),
                        h = parseInt(p.innerHeight());
                    if (h > 130) {
                        $(v).addClass('collapsed collapsible');
                    }
                });
            }

        </script>

        <script type="text/javascript">
            $(function () {

                if (typeof initPageScripts !== 'undefined' && $.isFunction(initPageScripts)) {
                    initPageScripts();
                }

                $(document).on('submit', '.js-register, .js-login, .js-reset-password', function (e) {
                    e.preventDefault();
                    var form = $(this),
                        btn = form.find('button[type="submit"]'),
                        url = form.attr('action'),
                        postData = form.serialize();

                    btn.prop('disabled', true)
                        .data('default-text', btn.html())
                        .html(btn.data('loading-text'));
                    $.post(url, postData)
                        .done(function (response) {
                            if (response.Errors == null) {
                                if (response.RedirectUrl != null) {
                                    window.location.href = response.RedirectUrl;
                                }
                            } else {
                                shakeIt(form);
                                showNotifyMessage('error', response.Errors[0].Value[0]);
                            }
                        })
                        .fail(function () {
                            showNotifyMessage('error', 'Sorry, a problem occured, please contact support');

                        })
                        .always(function () {
                            setTimeout(
                                function () {
                                    btn.removeAttr('disabled')
                                        .html(btn.data('default-text'));
                                }, 500
                            )
                        });
                });

                $(document).on('submit', '.js-landing-affiliate-register', function (e) {
                    e.preventDefault();
                    var form = $(this),
                        btn = form.find('button[type="submit"]'),
                        url = form.attr('action'),
                        postData = form.serialize();

                    btn.prop('disabled', true)
                        .data('default-text', btn.html())
                        .html(btn.data('loading-text'));
                    $.post(url, postData)
                        .done(function (response) {
                            if (response.Errors == null) {
                                if (response.RedirectUrl != null) {
                                    window.location.href = response.RedirectUrl;
                                }
                            } else {
                                shakeIt(form);

                                //if (response.Errors.length > 1) {
                                //    showNotifyMessage('error', 'Please check your entered data');
                                //} else {
                                //    showNotifyMessage('error', response.Errors[0]);
                                //}

                                $.each(response.Errors, function (index, item) {
                                    
                                    showNotifyMessage('error', item.Value[0]);

                                    if (form && $.trim(item.Key).length > 0) {
                                        var errElem = form.find(":input[name='" + item.Key + "']");
                                        if (errElem.length) {
                                            errElem.closest('.form-group').addClass('has-error');
                                            //errElem.tooltip({
                                            //    title: item.Value[0],
                                            //    placement : 'top',
                                            //    html: true,
                                            //    trigger: 'focus',
                                            //    template: '<div class="tooltip tooltip--danger" role="tooltip"><div class="tooltip-arrow"></div><div class="tooltip-inner"></div></div>'
                                            //});

                                        }
                                    }
                                });
                            }
                        })
                        .fail(function () {
                            showNotifyMessage('error', 'Sorry, a problem occured, please contact support');

                        })
                        .always(function () {
                            setTimeout(
                                function () {
                                    btn.removeAttr('disabled')
                                        .html(btn.data('default-text'));
                                }, 500
                            )
                        });
                });



                $(document).on('click', '.js-join-now', function () {
                    sendTrackingLabel($(this).data('track-id'));
                });

                $(document).on('click', '#WhitePaper', function () {
                    window.open('https://www.bondora.com/marketing/Bondora_Investor_Presentation.pdf?16112017','_blank');
                });

                $(document).on('click', '.nav-trigger', function(){
                    $(this).toggleClass('open');
                });

                $(document).on('click', '#RegisterForm', function(){
                    sendTrackingLabel('NewLPJoinDesktopContent');
                });

                $(document).on('click', '.langbar-trigger', function(e){
                    e.preventDefault();
                    e.stopPropagation();
                    $('body').toggleClass('langbar-open');
                });

                $(window).click(function(e) {
                    if ( !$(e.target).closest('.langbar-list').length ) {
                        var el = $('body'),
                            cls = 'langbar-open';
                        if(el.hasClass(cls)) {
                            el.removeClass(cls);
                        }
                    }
                });

                $(window).resize(function() {
                    $('body')
                        .removeClass('langbar-open');
                });

                initSocailLogin();
                initModals();
                initAffiliateRegistration();
            });

            function openSocialLoginPopup(url, title, w, h) {
                var dualScreenLeft = window.screenLeft != undefined ? window.screenLeft : screen.left;
                var dualScreenTop = window.screenTop != undefined ? window.screenTop : screen.top;
                width = window.innerWidth ? window.innerWidth : document.documentElement.clientWidth ? document.documentElement.clientWidth : screen.width;
                height = window.innerHeight ? window.innerHeight : document.documentElement.clientHeight ? document.documentElement.clientHeight : screen.height;
                var left = ((width / 2) - (w / 2)) + dualScreenLeft;
                var top = ((height / 2) - (h / 2)) + dualScreenTop;
                window.open(url, title, 'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=no, resizable=no, copyhistory=no, width=' + w + ', height=' + h + ', top=' + top + ', left=' + left);
            }


            function sendTrackingLabel (name) {
                $.post('/en/Authorize/SetTrackingLabel',
                    { key: "SignupTrackingLabel", value: name }
                );
            }

            function setPhoneCountryCode() {
                var self = $('#CountryAreaCode');
                var value = self.val();
                if (value != undefined) {
                    var countryCode = value.toLowerCase();
                    var title = self.find('option:selected').text();
                    $('#country-area-code-flag').html('<span class="flag f--' + countryCode + '" title="' + title + '"></span>');
                } else {
                    $('#country-area-code-flag').html('<span class="flag f--" title=""></span>');
                }
            }

            function initAffiliateRegistration() {
                $('#CountryAreaCode').val('US');//set default
                $('#CountryAreaCode').on('change', function () {
                    setPhoneCountryCode();
                });

                setPhoneCountryCode();
            }

            function initSocailLogin() {
                $(document).on('click', '.js-facebook-login', function (e) {
                    e.preventDefault();
                    openSocialLoginPopup(window.SocialLoginLinks.facebookLinkForLoginPage, 'facebookLogin', 600, 500);
                    return false;
                });
                $(document).on('click', '.js-google-plus-login', function (e) {
                    e.preventDefault();
                    openSocialLoginPopup(window.SocialLoginLinks.googleLinkForLoginPage, 'facebookLogin', 600, 400);
                    return false;
                });

                $(document).on('click', '.js-facebook-login-reg', function (e) {
                    e.preventDefault();
                    openSocialLoginPopup(window.SocialLoginLinks.facebookLinkForRegisterPage, 'facebookLogin', 600, 500);
                    return false;
                });
                $(document).on('click', '.js-google-plus-login-reg', function (e) {
                    e.preventDefault();
                    openSocialLoginPopup(window.SocialLoginLinks.googleLinkForRegisterPage, 'facebookLogin', 600, 400);
                    return false;
                });
            }


            function initModals() {
                $(document).on('click', '[data-toggle="modal"]', function () {
                    if ($('.modal.open').length > 0) {
                        $('.modal.open').removeClass('open');
                        $(document).unbind('keyup.modalClose');
                    }
                    var target = $(this).data('target'),
                        modal = $('#' + target);
                    modal.addClass('open');
                    switch (target) {
                    case "SignUpModal":
                        modal.find('input[name="Email"]').focus();
                        break;
                    case "LogInModal":
                        modal.find('input[name="Email"]').focus();
                        break;
                    }

                    $(document).on('keyup.modalClose', function (e) {
                        if (e.which == 27) { // esc key
                            $('.modal').removeClass('open');
                            $(document).unbind('keyup.modalClose');
                        }
                    });
                });

                $(document).on('click','.modal-close', function(e) {
                    e.preventDefault();
                    var modal = $(this).parents('.modal');
                    modal.removeClass('open');
                    $(document).unbind('keyup.modalClose');
                });
            }

            function shakeIt(obj){
                obj.addClass("animated shake");
                setTimeout(function() {
                    obj.removeClass("animated shake")
                }, 500);
            }

            function showNotifyMessage(type, msg) {
                var cls = "notify-message",
                    shownMessage = $('.' + cls),
                    container = $('<div />')
                        .addClass(cls)
                        .addClass(cls + '--' + type)
                        .addClass(cls + '--animation-show');

                if (shownMessage.length > 0) {
                    closeNotifyMessage(shownMessage);
                }

                var iconCls;
                switch(type) {
                case "error":
                    iconCls = 'fa-exclamation-triangle';

                    break;
                case "warning":
                    iconCls = 'fa-exclamation-triangle';
                    break;
                case "success":
                    iconCls = 'fa-check';
                    break;
                case "send":
                    iconCls = 'fa-paper-plane';
                    break;
                case "info":
                    iconCls = 'fa-comments';
                    break;
                }


                container
                    .append($('<i />').addClass('fa fa-fw ' + iconCls))
                    .append($('<span />').text(msg))
                    .appendTo('body');


                $(container).one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function () {
                    $(this).removeClass(cls + '--animation-show');
                });

                setTimeout(function () {
                    closeNotifyMessage(container);
                }, 5 * 1000);

                $(document).on('click', '.' + cls, function () {
                    closeNotifyMessage($(this));
                });
            }



            function closeNotifyMessage(container) {
                container.addClass('notify-message--animation-hide');
                $(container).one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function () {
                    $(this).remove();
                });
            }

        </script>
    </body>
