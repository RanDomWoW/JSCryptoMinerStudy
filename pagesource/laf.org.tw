    function reAction(obj) {
        if(obj.width()<=375){
            jQuery(".Pro-Scroll-list").slide({mainCell:".picList ul",autoPage:true,effect:"left",autoPlay:false,vis:1,trigger:"click"});
            jQuery(".Pro-Scroll-list-link").slide({mainCell:".picList ul",autoPage:true,effect:"left",autoPlay:false,vis:1,trigger:"click"});
        }
        else if(obj.width()<=640){
            jQuery(".Pro-Scroll-list").slide({mainCell:".picList ul",autoPage:true,effect:"left",autoPlay:false,vis:2,trigger:"click"});
            jQuery(".Pro-Scroll-list-link").slide({mainCell:".picList ul",autoPage:true,effect:"left",autoPlay:false,vis:2,trigger:"click"});

        }
        else if(obj.width()<=768){
            jQuery(".Pro-Scroll-list").slide({mainCell:".picList ul",autoPage:true,effect:"left",autoPlay:false,vis:3,trigger:"click"});
            jQuery(".Pro-Scroll-list-link").slide({mainCell:".picList ul",autoPage:true,effect:"left",autoPlay:false,vis:3,trigger:"click"});
        }
        else {
            jQuery(".Pro-Scroll-list").slide({mainCell:".picList ul",autoPage:true,effect:"left",autoPlay:false,vis:4,trigger:"click"});
            jQuery(".Pro-Scroll-list-link").slide({mainCell:".picList ul",autoPage:true,effect:"left",autoPlay:false,vis:4,trigger:"click"});
