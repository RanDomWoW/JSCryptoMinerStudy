    <script type="text/javascript">

        $(document).ready(function () {


            var location = "muscat";
            // Specify the ZIP/location code and units (f or c)
            var loc = 'MUXX0003'; // or e.g. SPXX0050
            var u = 'c';

            var query = "SELECT * FROM weather.forecast WHERE location='" + loc + "' AND u='" + u + "'";
            var cacheBuster = Math.floor((new Date().getTime()) / 1200 / 1000);
            var url = 'https://query.yahooapis.com/v1/public/yql?q=' + encodeURIComponent(query) + '&format=json&_nocache=' + cacheBuster;

            window['wxCallback'] = function (data) {
                var info = data.query.results.channel.item.condition;
                var info1 = data.query.results.channel.atmosphere;

                $('#wxIcon').css({
                    backgroundPosition: '-' + (61 * info.code) + 'px 0'
                }).attr({
                    title: info.text
                });
                $('#wxIcon3').append('<img src="http://l.yimg.com/a/i/us/we/52/' + info.code + '.gif" width="34" height="34" title="' + info.text + '" />');

                $('#wxTemp').html(info.temp + '&deg;' + (u.toUpperCase()));
                $('#wxHumidity').html(info1.humidity);
            };

            $.ajax({
                url: url,
                dataType: 'jsonp',
                cache: true,
                jsonpCallback: 'wxCallback'
            });

            var ajax_call = function () {
                var d = new Date();
                $('#currentTime').html(d.getHours() + ":" + d.getMinutes());
            };
            setInterval(ajax_call, 1000);

            var refreshIntervalId = null;
            var refreshIntervalId1 = null;
            var refreshIntervalId2 = null;
            var refreshIntervalId3 = null;

            if (location == "muscat") {
                $.ajaxSetup({ cache: false });
                $.get("FlightArrivalsHome.aspx", "", function (data) {
                    $("#slider").html(data);
                });
            }


            $('#FlightArrivals').click(function () {
                if (location == "muscat") {
                    $("#linktimming").attr("href", "/FlightArrivals.aspx?MID=8&SMID=55&Slide=True")
                    clearInterval(refreshIntervalId1);
                    clearInterval(refreshIntervalId2);
                    clearInterval(refreshIntervalId3);
                    refreshIntervalId = setInterval(function () {
                        $.ajaxSetup({ cache: false });
                        $.get("FlightArrivalsHome.aspx", "", function (data) {
                            $("#slider").html(data);
                        });
                    }, 4000);
                }
                else {
                    $("#linktimming").attr("href", "/FlightArrivals.aspx?MID=8&SMID=55&Salalah=Salalah&Slide=True")
                    clearInterval(refreshIntervalId1);
                    clearInterval(refreshIntervalId);
                    clearInterval(refreshIntervalId3);
                    refreshIntervalId2 = setInterval(function () {
                        $.ajaxSetup({ cache: false });
                        $.get("SalalahArrivalsHome.aspx", "", function (data) {
                            $("#slider").html(data);
                        });
                    }, 4000);
                }
            });
            $('#FlightDepartures').click(function () {
                if (location == "muscat") {
                    $("#linktimming").attr("href", "/FlightDepartures.aspx?MID=8&SMID=56&Slide=True")
                    clearInterval(refreshIntervalId);
                    clearInterval(refreshIntervalId2);
                    clearInterval(refreshIntervalId3);
                    refreshIntervalId1 = setInterval(function () {
                        $.ajaxSetup({ cache: false });
                        $.get("FlightDeparturesHome.aspx", "", function (data) {
                            $("#slider").html(data);
                        });
                    }, 4000);
                }
                else {
                    $("#linktimming").attr("href", "/FlightDepartures.aspx?MID=8&SMID=56&Salalah=Salalah&Slide=True")
                    clearInterval(refreshIntervalId);
                    clearInterval(refreshIntervalId2);
                    clearInterval(refreshIntervalId1);
                    refreshIntervalId3 = setInterval(function () {
                        $.ajaxSetup({ cache: false });
                        $.get("SalalahDeparturesHome.aspx", "", function (data) {
                            $("#slider").html(data);
                        });
                    }, 4000);
                }
            });


          $('#muscatLink').click(function () {
                location = "muscat";
                $("#linktimming").attr("href", "/FlightArrivals.aspx?MID=8&SMID=55&Slide=True")
                $("#FlightArrivals").removeClass('active').addClass('inactive');
                $("#FlightDepartures").removeClass('inactive').addClass('active');
                $("#muscatLink").css('background-color', '#A7A0D4');
                $("#salalahLink").css('background-color', '#d8d8d8');
                clearInterval(refreshIntervalId);
                clearInterval(refreshIntervalId1);
                clearInterval(refreshIntervalId2);
                clearInterval(refreshIntervalId3);

                $.ajaxSetup({ cache: false });
                $.get("FlightArrivalsHome.aspx", "", function (data) {
                    $("#slider").html(data);
                });

            });

            $('#salalahLink').click(function () {
                location = "salalah";
                $("#linktimming").attr("href", "/FlightArrivals.aspx?MID=8&SMID=55&Salalah=Salalah&Slide=True")
                $("#salalahLink").css('background-color', '#A7A0D4');
                $("#muscatLink").css('background-color', '#d8d8d8');
                $("#FlightArrivals").removeClass('active').addClass('inactive');
                $("#FlightDepartures").removeClass('inactive').addClass('active');
                clearInterval(refreshIntervalId);
                clearInterval(refreshIntervalId1);
                clearInterval(refreshIntervalId2);
                clearInterval(refreshIntervalId3);

                $.ajaxSetup({ cache: false });
                $.get("SalalahArrivalsHome.aspx", "", function (data) {
                    $("#slider").html(data);
                });
            });
          
         
         
        });

    </script>


