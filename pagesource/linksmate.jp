<script type="text/javascript">
    function calcCost() {
        var $costs = $('.new-simulation-area .plan:checked, .new-simulation-area .option:checked');

        var sum = 0;
        $costs.each(function (i, elem) {
            sum += parseInt($(elem).val());
        });

        $('.sum-price').text(sum.toLocaleString());
    }

    function showDescription(type, key) {
        var idStr = '.p-description-item.'+type+'-'+key;
        $(idStr).toggleClass('active');
    }

    function planSelected(planSize) {
        var plan = $('.new-simulation-area input[name=plan]:checked');
        if(plan.length==0) return;
        var size = plan.attr('id').replace('plan', '');
        if(size==planSize){
            setTimeout(function() {
                $('.new-simulation-area input[name=plan]:checked').prop("checked",false);
                calcCost();
            });
        }
    }

    $(function () {
        $('#carousel').carouFredSel({
            width: '100%',
            items: {
                visible: 3,
                start: -1,
                minimum: 3            },
            scroll: {
                items: 1,
                duration: 1000,
                timeoutDuration: 5000
            },
            prev: '#prev',
            next: '#next',
            pagination: {
                container: '#pager',
                deviation: 1,
                anchorBuilder: function (nr) {
                    var img = $(this).find('img.banner');
                    if(img.length==0) return '';
                    var src = img.attr('src').replace('/img/banner/', '/img/banner/thum/');
                    return '<a href="#" class="thumb' + nr + '"><img src="'+src+'"/></a>';
                }
            }
        }).animate({opacity: 1}, 300);

        $('#onsei').on('click', function () {
            if(!$(this).prop('checked')){
                $('#rusuden, #warikomi, #matephone').prop('checked',false);
            }
            calcCost();
        });

        $('#rusuden, #warikomi, #matephone').on('click', function () {
            if($(this).prop('checked')){
                $('#onsei').prop('checked',true);
            }
            calcCost();
        });

        $('#order,#order1,#order2').on('click', function () {
            var form = $('<form/>', {action: '/order', method: 'post'});

            var plan = $('.new-simulation-area input[name=plan]:checked');
            var size = plan.attr('id').replace('plan', '');
            if(plan.length > 0) form.append($('<input/>', {type: 'hidden', name: 'gbyte', value: size}));

            var options = $('.new-simulation-area .option:checked');

            options.each(function (i, elem) {
                var name = $(elem).attr('name');
                if(name) form.append($('<input/>', {type: 'hidden', name: name, value: "1"}));
            });

            form.appendTo(document.body);
            form.submit();
            return false;
        });
    });
</script>


