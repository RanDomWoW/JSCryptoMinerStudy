.ironybbcode {
	min-height: 56px;
	border: solid 1px #CCCCCC;
	background-color: #FCFDFE;
	border-radius: 6px;
	padding: 5px;
}

.ironybbcodeIcon {
	float: left;
	margin: 5px 20px;
}

.question, .idea, .statement {
	border-width: 1px;
	border-style: solid;
	line-height: 1.5;
	margin-top: 14px;
	padding: 11px 14px 11px 48px;
	position: relative;
}

.question:after, .idea:after, .statement:after {
	font-family: FontAwesome;
	font-size: 28px;
	display: block;
	height: 32px;
	left: 13px;
	margin-top: -19px;
	position: absolute;
	top: 50%;
	width: 32px;
}

.question a, .idea a, .statement a {
	text-decoration: underline;
}

@media only screen and (max-width: 800px) {
	.question, .idea, .statement {
		margin-left: 7px;
		margin-right: 7px;
		padding: 4px 7px;
	}
	.question:after, .idea:after, .statement:after {
		display: none;
	}
}

.question {
	background-color: #00FFFF;
	border-color: #40E0D0;
	color: #000000;
	text-shadow: 0 1px 0 rgba(255,255,255,0.8);
}
.question:after {
	content: "\f059";
}

.question a, .question a:hover {
	color: #000000;
}
.question .icon {
	color: #000000;
	text-shadow: 0 1px 0 rgba(255,255,255,0.8);
}

.idea {
	background-color: #FFFF00;
	border-color: #F7FE2E;
	color: #000000;
	text-shadow: 0 1px 0 rgba(255,255,255,0.8);
}

.idea:after {
	content: "\f0eb";
}
.idea a, .idea a:hover {
	color: #000000;
}

.idea .icon {
	color: #000000;
	text-shadow: 0 1px 0 rgba(255,255,255,0.8);
}

.statement {
	background-color: #64FE2E;
	border-color: #82FA58;
	color: #000000;
	text-shadow: 0 1px 0 rgba(255,255,255,0.8);
}

.statement:after {
	content: "\f06a";
}

.statement a, .statement a:hover {
	color: #000000;
}

.statement .icon {
	color: #000000;
	text-shadow: 0 1px 0 rgba(255,255,255,0.8);
}

.question, .idea, .statement {
	border-radius: 7px;
	border-width: 0;
	font-weight: bold;
	box-shadow: 0 1px 2px rgba(0, 0, 0, .5), inset 0 0 1px rgba(0, 0, 0, .2);
}

.question {
	background-color: #00FFFF;
	background-image: -webkit-linear-gradient(#00FFFF,#40E0D0);
	background-image: -moz-linear-gradient(#00FFFF,#40E0D0);
	background-image: -ms-linear-gradient(#00FFFF,#40E0D0);
	background-image: linear-gradient(#00FFFF,#40E0D0);
}

.idea {
	background-color: #FFFF00;
	background-image: -webkit-linear-gradient(#FFFF00,#F7FE2E);
	background-image: -moz-linear-gradient(#FFFF00,#F7FE2E);
	background-image: -ms-linear-gradient(#FFFF00,#F7FE2E);
	background-image: linear-gradient(#FFFF00,#F7FE2E);
}

.statement {
	background-color: #64FE2E;
	background-image: -webkit-linear-gradient(#64FE2E,#82FA58);
	background-image: -moz-linear-gradient(#64FE2E,#82FA58);
	background-image: -ms-linear-gradient(#64FE2E,#82FA58);
	background-image: linear-gradient(#64FE2E,#82FA58);
}
