<script>
	var HIDE_ACTIVATION = 'needLike';
	var HIDE_DISABLE_UNLIKE = 1;
	if(HIDE_ACTIVATION=='needLike'){
		$(function(){
			if(typeof WBB != 'undefined' && typeof WBB.Post != 'undefined' && typeof WBB.Post.Like != 'undefined' && typeof WBB.Post.Like.prototype._success != 'undefined'){
				$(window).load(function(){
					$postList = $('.wbbThreadPostList > li > .wbbPost');
					$postList.each(function(index, element){
						$post = $(element);
						$post.find('.hideLikeButton').click($post, function(event) {
							event.data.find('.wcfLikeButton').click();
						});
					});
					if(HIDE_DISABLE_UNLIKE){
						$likeContainer = $postList.find('.wcfLikeButton' + ' > .active').parent().parent();
						$likeContainer.find('.wcfLikeButton').unbind('click');
						$likeContainer.find('.wcfDislikeButton').remove();
					}
				});
				_originalSuccess = WBB.Post.Like.prototype._success;
				WBB.Post.Like.prototype._success = function(data, textStatus, jqXHR){
					_originalSuccess.call(this, data, textStatus, jqXHR);
					$container = $('#'+data.returnValues.containerID);
					likedPostID = parseInt($container.attr('data-post-id'));
					
					if(HIDE_DISABLE_UNLIKE && data.returnValues.newValue==1){
						$container.find('.wcfLikeButton').unbind('click');
						$container.find('.wcfDislikeButton').remove();
					}
					
					if(likedPostID>0 && $container.find('.hideBox').length>0){
						new WCF.Action.Proxy({
							autoSend: true,
							data: {
								actionName: 'getPosts',
								className: 'wbb\\data\\post\\PostAction',
								objectIDs: [likedPostID],
									parameters: {
										showCollapsedPosts: true
									}
							},
							success: function(data, textStatus, jqXHR) {
								var $posts = $('' + data.returnValues.template);
								$posts.each($.proxy(function(index, element) {
									var $element = $(element);
									
									if ($element.is('li')) {
										var $post = $element.children('article');
										var $postID = $post.data('postID');
										var $oldPost = $('#post'+$postID);
										$oldPost.find('.messageText').replaceWith($element.find('.messageText'));
									}
								}, this));
							}
						});
					}
				}
			}
		});
	}
	else if(HIDE_ACTIVATION=='needPost'){
		$(function(){
			if(typeof WBB != 'undefined' && typeof WBB.Thread != 'undefined' && typeof WBB.Thread.QuickReply != 'undefined' && typeof WBB.Thread.QuickReply.prototype._success != 'undefined'){
				_originalSuccess = WBB.Thread.QuickReply.prototype._success;
				WBB.Thread.QuickReply.prototype._success = function(data, textStatus, jqXHR){
					_originalSuccess.call(this, data, textStatus, jqXHR);
					postIDs = [];
					$('.wbbThreadPostList > li > .wbbPost').each(function( index, value ) {
						if($(this).find('.hiddenContent').length>0){
							postIDs.push(parseInt($(this).attr('data-post-id')));
						}
					});
					if(postIDs.length>0){
						new WCF.Action.Proxy({
							autoSend: true,
							data: {
								actionName: 'getPosts',
								className: 'wbb\\data\\post\\PostAction',
								objectIDs: postIDs,
									parameters: {
										showCollapsedPosts: true
									}
							},
							success: function(data, textStatus, jqXHR) {
								var $posts = $('' + data.returnValues.template);
								$posts.each($.proxy(function(index, element) {
									var $element = $(element);
									
									if ($element.is('li')) {
										var $post = $element.children('article');
										var $postID = $post.data('postID');
										var $oldPost = $('#post'+$postID);
										$oldPost.find('.messageText').replaceWith($element.find('.messageText'));
									}
								}, this));
							}
						});
					}
				}
			}
		});
	}
</script>
