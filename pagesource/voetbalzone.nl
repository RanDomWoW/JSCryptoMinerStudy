								$('body').addClass('fixed');
								$('.main-menu').prepend('<li class="fixedLogo"><img height="40" src="http://static.voetbalzone.nl/reskin/images/small-logo.png?v=2" /></li>');
								$('.fixedLogo').show("fast");
								$('.showInlogTx .tx').fadeIn("");
								$('.fixedLogo').click(function(){
									parent.location.href = "/";
								});

								fixedStatus = true;
							}

							if ( startPos < $(window).scrollTop() ) {
								//console.log("down");

								if (fixedStatus == true){
									if (subShown == true) {
										//console.log("SLIDE UP!");
										subShown = false;
										$('body.fixed .add-nav').slideUp( "fast", function() {
											$('body').addClass('down');
											$('body').removeClass('up');
										});
									}
								}


							}
							if ( startPos > $(window).scrollTop() ) {
								//console.log("up");

								if (fixedStatus == true){
									if (subShown == false) {
										//console.log("SLIDE DOWN!");
										subShown = true;
										$('body.fixed .add-nav').slideDown( "fast", function() {
											$('body').addClass('up');
											$('body').removeClass('down');
										});
									}
								}
							}

						} else { //NORMALE HEADER
							if ( $('body').hasClass('fixed') ) {
								//console.log("NORMAAL");
								$('body').removeClass('fixed');
								$('.main-menu .fixedLogo').remove();
								$('.add-nav').show();
							}
						}

						startPos = $(window).scrollTop();
					}

				});
				</script>
			
			<!-- END FOOTER BLOCK -->

	</div>




