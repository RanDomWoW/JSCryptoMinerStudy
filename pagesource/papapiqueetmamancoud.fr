function AfficherDivAgrandirProduit(item)
{
	$('#id_div_agrandir_produit_lien_precedent').unbind('click');
	$('#id_div_agrandir_produit_lien_suivant').unbind('click');
	$('#id_div_agrandir_produit_details').load('https://www.papapiqueetmamancoud.fr/index.php/div_produit/afficher/'+item.data('id-produit'));
	voisin_precedent=item.parent();
	do{
		voisin_precedent=voisin_precedent.prev();
	}
	while(voisin_precedent.length>0&&EstCache(voisin_precedent));
	if(voisin_precedent.length>0)
	{
		$('#id_div_agrandir_produit_lien_precedent').show();
		$('#id_div_agrandir_produit_lien_precedent').click(function(event){
			AfficherDivAgrandirProduit(voisin_precedent.children());
						ga('send', 'event', 'ListeProduits', 'ApercuPrecedent', voisin_precedent.children().data('id-produit'));
		});
	}
	else
	{
		$('#id_div_agrandir_produit_lien_precedent').hide();
	};
	voisin_suivant=item.parent();
	do{
		voisin_suivant=voisin_suivant.next();
	}
	while(voisin_suivant.length>0&&EstCache(voisin_suivant));
	if(voisin_suivant.length>0)
	{
		$('#id_div_agrandir_produit_lien_suivant').show();
		$('#id_div_agrandir_produit_lien_suivant').click(function(event){
			AfficherDivAgrandirProduit(voisin_suivant.children());
						ga('send', 'event', 'ListeProduits', 'AperçuSuivant', voisin_suivant.children().data('id-produit'));
		});
	}
	else
	{
		$('#id_div_agrandir_produit_lien_suivant').hide();
	};
	$('#id_div_agrandir_produit').css({left: Math.floor(($(window).width()-$('#id_div_agrandir_produit').width())/2)+'px',top:(50+$(window).scrollTop())+'px'});
	$('#id_div_agrandir_produit').show();
}		

