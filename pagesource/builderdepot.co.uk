                allowWrap: 1,
                carouselVisible: 1,
                autoHeight: 1,
                pager: '.cycle-pager',
                pagerEvent: 'mouseover'
            };

            $(document).on('cycle-bootstrap', function (e, opts, API) {
                API.buildPagerLink = function (opts, slideOpts, slide) {
                    var pagers = opts.API.getComponent('pager');
                    pagers.each(function () {
                        var pager = $(this);

                        var textHelper = $('span', slide).text();
                        var hrefHelper = $('a', slide).attr('href');
                        var linkElement = $('<a>').attr('href', hrefHelper).text(textHelper);
                        var spanElement = $('<span></span>');
                        $(linkElement).appendTo(spanElement);

                        pagerLink = $(spanElement).appendTo(pager);
                        pagerLink.on(opts.pagerEvent, function (e) {
                            e.preventDefault();
                            opts.API.page(pager, e.currentTarget);
                        });

                        pager.hover
                        (
                            function ()
                            {
                                API.pause(true);
                            },
                            function () {
                                API.resume(true);
                            }
                        );
                    });
                }
            });
            silder.cycleEvents().cycle(cycleOptions);

        })(jQuery);
