			pattern: "konqueror/([0-9.]+)"
		}
	];

	var browserCount = browserList.length;
	var i = 0;

	while (!browser && (i < browserCount))
	{
		var match = navigator.userAgent.match(new RegExp(browserList[i].pattern, "i"));
		if (match)
		{
			browser = browserList[i].name + " " + match[1];
		}

		++i;
	}

	var operatingSystemList = [
		{
			name: "Windows 7",
			pattern: "Windows NT 6\.1"
		},
		{
			name: "Windows Vista",
			pattern: "Windows NT 6\.0"
		},
		{
			name: "Windows XP",
			pattern: "Windows NT 5\.1"
		},
		{
			name: "Windows Phone",
			pattern: "Windows Phone (?:OS )?([0-9.]+)",
			isMobile: true
		},
		{
			name: "Windows",
			pattern: "Windows(?: ([0-9]+))?"
		},
		{
			name: "Android",
			pattern: "Android (3\.[0-9.]*)",
			isTablet: true
		},
		{
			name: "Android",
			pattern: "Android ([0-9.]+).*Tablet",
			isTablet: true
		},
		{
			name: "Android",
			pattern: "Android ([0-9.]+).*Mobile",
			isMobile: true
		},
		{
			name: "Android",
			pattern: "Android ([0-9.]+)",
			isMobile: false
		},
		{
			name: "iOS iPhone",
			pattern: "iPhone.*OS ([0-9_]+)",
			isMobile: true
		},
		{
			name: "iOS iPad",
			pattern: "iPad.*OS ([0-9_]+)",
			isTablet: true
		},
		{
			name: "Mac OS X",
			pattern: "Mac OS X(?: (10[0-9_]+))?"
		},
		{
			name: "Blackberry",
			pattern: "BlackBerry ([0-9]+)[^0-9]",
			isMobile: true
		},
		{
			name: "Linux",
			pattern: "Linux"
		},
		{
			name: "FreeBSD",
			pattern: "FreeBSD"
		},
		{
			name: "Symbian",
			pattern: "SymbianOS([^ ])",
			isMobile: true
		}
	];

	var operatingSystemCount = operatingSystemList.length;
	var i = 0;

	while (!operatingSystem && (i < operatingSystemCount))
	{
		var match = navigator.userAgent.match(new RegExp(operatingSystemList[i].pattern, "i"));

		if (match)
		{
			operatingSystem = operatingSystemList[i].name;
			if (match[1])
			{
				operatingSystem += " " + match[1];
			}

			isTablet = !!operatingSystemList[i].isTablet;
			isMobile = !!operatingSystemList[i].isMobile;
		}

		++i;
	}

	return {
		browser: browser,
		operatingSystem: operatingSystem,
		isTablet: isTablet,
		isMobile: isMobile
	};
}

