jQuery(document).available(
	".js-linkContainer:has(a)",
	function()
	{
		var container = jQuery(this);
		var link = jQuery("a", this).eq(0);

		var wrapper = jQuery("<a>");
		var linkAttributes = link.get(0).attributes;
		for (var i = 0; i < linkAttributes.length; ++i)
		{
			wrapper.attr(linkAttributes[i].name, linkAttributes[i].nodeValue);
		}

		container.children().each(function()
		{
			jQuery(this).detach().appendTo(wrapper);
		});

		container.append(wrapper).removeClass("js-linkContainer");

		link.replaceWith(link.html());
	}
);

/******************************************************************************
 * Images chargées en asynchrone
 *****************************************************************************/
function loadDelayedImages(images)
{
	if (typeof(images) === "undefined")
	{
		images = ".js-delayedImageLoading";
	}

	jQuery(images).each(function()
	{
		var imageAttributes = this.attributes;

		var image = jQuery("<img>");

		for (var i = 0; i < imageAttributes.length; ++i)
		{
			image.attr(imageAttributes[i].name, imageAttributes[i].nodeValue);
		}

		image.removeClass("js-delayedImageLoading");

		jQuery(this).replaceWith(image);
	});
}

jQuery(document).ready(function() { loadDelayedImages() });

