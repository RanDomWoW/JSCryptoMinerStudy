(function()
{
	var mutationObserver;

	var checkEmptiness = function(element, mutationObserver)
	{
		if (element.outerHeight())
		{
			element.removeClass("is-empty");
			if (typeof(mutationObserver) === "object")
			{
				mutationObserver.disconnect();
			}
			else
			{
				clearInterval(mutationObserver);
			}
		}
	}

	var mutationObserverIsAvailable = (typeof(MutationObserver) === "function");

	jQuery(document).available(
		".External.is-empty",
		function()
		{
			var element = jQuery(this);
			if (mutationObserverIsAvailable)
			{
				var mutationObserver = new MutationObserver(function()
				{
					checkEmptiness(element, this);
				});

				mutationObserver.observe(this, {
					childList: true,
					attributes: true,
					characterData: true,
					subtree: true
				});

				jQuery(document).ready(function()
				{
					setTimeout(
						function()
						{
							mutationObserver.disconnect();
						},
						60000
					);
				});
			}
			else
			{
				var intervalReference = setInterval(
					function()
					{
						checkEmptiness(element, intervalReference);
					},
					1000
				);

				jQuery(document).ready(function()
				{
					setTimeout(
						function()
						{
							clearInterval(intervalReference);
						},
						60000
					);
				});
			}
		},
		{endsOnDOMReady: true}
	);
})();
//page.js
(function()
{
	var hasRetractableOnlyElements = false;
	var buildRetractableMenu = function()
	{
		/**
		 * Construction du menu
		 */

		var menu = jQuery('<nav id="RetractableMenu" class="is-hidden"></nav>');

		menu.append('<a class="Menu-logo" href="' + jQuery(".SiteHeader-logo").attr("href") + '">Marie Claire</a>');
		menu.append('<button class="Menu-close js-hideRetractableMenu"></button>');
		var authenticationLinks = jQuery(".SiteHeader-authenticationLink");

		if (authenticationLinks.length)
		{
			var authenticationContainer = jQuery('<div class="Menu-authentication"></div>');
			menu.append(authenticationContainer);

			authenticationLinks.each(function()
			{
				authenticationContainer.append(
					jQuery('<div class="Menu-authenticationLinkContainer"></div>').append(
						jQuery("<div></div>").append(jQuery(this).clone()).html().replace(/SiteHeader/g, "Menu")
					)
				);
			});
		}

		// Copie du menu
		var source = jQuery(".MenuHidden .Menu-wrapper");		
		var container = source.clone();		
		

		var retractableOnlyElements = jQuery('.is-hidden',container);
		if(retractableOnlyElements.length>0) {
			hasRetractableOnlyElements = true;
		}

		// Réordonnancement des éléments si demandé
		var reorderedElements = container.find(".Menu-item[data-reorder-priority]").get();
		if (reorderedElements.length)
		{
			reorderedElements.sort(function(elementOne, elementTwo)
			{
				var elementOnePriority = jQuery(elementOne).data("reorderPriority");
				var elementTwoPriority = jQuery(elementTwo).data("reorderPriority");

				var status = 0;

				if (elementOnePriority > elementTwoPriority)
				{
					status = 1;
				}
				else if (elementOnePriority < elementTwoPriority)
				{
					status = -1;
				}

				return status;
			});

			var list = container.find(".Menu-list");
			for (var i = reorderedElements.length; i--;)
			{
				jQuery(reorderedElements[i]).detach().prependTo(list);
			}
		}

		// Ajout de l'abonnement dans le cas où la popup est présente dans le header
		if(jQuery('.MagazineSubscription-popup').length) {
			var subscriptionPopupSource = jQuery(".MagazineSubscription-popup").clone();
			subscriptionPopupSource.attr('href',subscriptionPopupSource.attr('data-burger-url'));
			jQuery('.MagazineSubscription-popup--image img',subscriptionPopupSource).attr('src',jQuery('.MagazineSubscription-popup--image img',subscriptionPopupSource).attr('data-burger-src'));
			jQuery('.MagazineSubscription-popup--links',subscriptionPopupSource).remove();
			jQuery('.MagazineSubscription-popup--subtitle',subscriptionPopupSource).remove();
			container.append(subscriptionPopupSource);
		}

		container.appendTo(menu);

		// Suppression des éléments cachés dans le menu original (ils servaient à alimenter le menu rétractable)
		jQuery(".Menu-item.is-hidden", source).remove();

		menu.prependTo("body");

		// Ajustement du placement si nécessaire
		var menuWidth = menu.outerWidth();

		// Correction des arrondis pour certains navigateurs (Chrome)
		var menuWidthCorrection = menuWidth - Math.floor(menu.get(0).getBoundingClientRect().width);
		menuWidth -= menuWidthCorrection;

		if (menuWidth > parseInt(menu.css("min-width")))
		{
			menu.css("left", -1 * menuWidth);
		}

		// La taille du menu est fixée afin de ne pas être changée ensuite par l'évolution des éléments présents à l'intérieur
		menu.width(menu.width() - menuWidthCorrection);

		// Ajout d'un bouton destiné à afficher les sous-éléments et d'une classe indiquant la présence de sous-éléments
		jQuery(".Menu-item:has(.Menu-sectionList), .Menu-sectionItem:has(.Menu-subSectionList)", menu).each(function()
		{
			var item = jQuery(this);
			var modifierName = item.hasClass("Menu-item") ? "item" : "section";
			jQuery('<button class="Menu-showChildren Menu-showChildren--' + modifierName + ' js-toggleMenuItemChildren"></button>').appendTo(this);
			item.addClass("Menu-parentItem--" + modifierName);
		});

		// Retrait de la classe plaçant un display: none sur le menu le temps de sa construction
		menu.removeClass("is-hidden");

		// Initialisation du défilement au sein du menu (avec barre de défilement)
		container.jScrollPane({
			clickOnTrack: false,
			verticalGutter: 0
		});
		var scrollPane = container.data("jsp");

		var reinitializeScrollPane = function(position)
		{
			scrollPane.reinitialise();

			if (typeof(position) === "number")
			{
				scrollPane.scrollToY(position);
			}
		}

		// Les transitions CSS ne fonctionnant pas avec la valeur "auto",
		// la taille est indiquée en dur avec openItem() et closeItem()
		var openItem = function(item, child)
		{
			item.one("transitionend", function() { item.css("height", ""); });
			item.height(item.height() + child.outerHeight(true));
			item.addClass("is-opened");
		}

		var closeItem = function(item)
		{
			item.height(item.height());
			item.removeClass("is-opened");
			setTimeout(function() { item.css("height", ""); }, 1);
		}

		var toggleItemChildren = function(item, callback)
		{
			if (item.length)
			{
				// Détermination des éléments à cibler
				var scope, itemSelector, listSelector;

				if (item.hasClass("Menu-item"))
				{
					scope = item.parents(".Menu-list").eq(0);
					itemSelector = ".Menu-item.is-opened";
					listSelector = ".Menu-sectionList";
				}
				else
				{
					scope = item.parents(".Menu-sectionList").eq(0);
					itemSelector = ".Menu-sectionItem.is-opened";
					listSelector = ".Menu-subSectionList";
				}

				var isItemOpened = item.hasClass("is-opened");

				// Taille de la zone d'affichage
				var paneContainer = container.find(".jspContainer");
				var paneContainerTop = paneContainer.offset().top;
				var paneContainerHeight = paneContainer.height();
				var paneContainerBottom = paneContainerTop + paneContainerHeight;

				var pane = container.find(".jspPane");

				// Taille et position du panneau déroulant avant action
				var previousPaneHeight = pane.height();
				var previousPaneTop = parseInt(pane.css("top"));

				// Initialisation des taille et position du panneau déroulant après action
				var newPaneHeight = previousPaneHeight;
				var newPaneTop = previousPaneTop;

				// Détermination de l'item éventuellement fermé lors de l'action
				var closingItem;
				if (isItemOpened)
				{
					closingItem = item;
				}
				else
				{
					closingItem = scope.find(itemSelector);
				}

				var itemPositionDelta = 0;	// Variation de la position de l'item dans son conteneur

				// S'il y a effectivement un item fermé
				if (closingItem.length)
				{
					var closingItemListHeight = closingItem.find(listSelector).outerHeight(true);

					// Le panneau déroulant se réduit de la taille de l'item fermé
					newPaneHeight -= closingItemListHeight;

					// Si l'item fermé est placé plus haut que l'item éventuel à ouvrir,
					// la liste va devoir être déplacée en conséquence pour conserver sa position
					if (!isItemOpened && (closingItem.offset().top < item.offset().top))
					{
						itemPositionDelta = closingItemListHeight;
						newPaneTop += closingItemListHeight;
					}
				}


				// Le panneau déroulant ne peut pas descendre sous le haut de la zone d'affichage
				newPaneTop = Math.min(newPaneTop, 0);

				var paneTopDelta = previousPaneTop - newPaneTop;

				// S'il y a ouverture d'un item
				if (!isItemOpened)
				{
					var openingItemTop = item.offset().top - paneContainerTop - newPaneTop - paneTopDelta - itemPositionDelta;
					var openingItemList = item.find(listSelector);
					if (openingItemList.length)
					{
						var openingItemListHeight = openingItemList.outerHeight(true);
						var openingItemBottom = openingItemList.offset().top - paneContainerTop - newPaneTop - paneTopDelta - itemPositionDelta + openingItemListHeight;

						// Le panneau déroulant s'agrandit de la taille de l'item ouvert
						newPaneHeight += openingItemListHeight;

						// Si le nouvel élément ouvert est plus grand que la zone d'affichage,
						// il est calé en haut de la zone d'affichage
						if (openingItemBottom - openingItemTop > paneContainerHeight)
						{
							newPaneTop = -1 * openingItemTop;
						}
						// Sinon, s'il dépasse le bas de la zone d'affichage,
						// il est calé en bas de la zone d'affichage
						else if (openingItemBottom + newPaneTop > paneContainerBottom)
						{
							newPaneTop = -1 * openingItemBottom + paneContainerHeight;
						}
					}
				}

				// Si le panneau déroulant est plus grand que la zone d'affichage,
				// le bas du panneau ne peut pas se trouver plus haut que le bas
				// de la zone d'affichage
				if (newPaneHeight > paneContainerHeight)
				{
					newPaneTop = Math.max(newPaneTop, paneContainerHeight - newPaneHeight);
				}
				// Sinon, le panneau ne peut pas monter plus haut que le haut de la zone d'affichage
				else
				{
					newPaneTop = Math.max(newPaneTop, 0);
				}

				if (closingItem.length)
				{
					closeItem(closingItem);
				}

				if (!isItemOpened)
				{
					openItem(item, openingItemList);
				}

				// Activation des effets et déplacement du panneau déroulant
				pane.addClass("is-transitioning");
				var endTransition = function()
				{
					// Une fois le défilement effectué, désactivation des effets et
					// ajustement du panneau déroulant
					pane.removeClass("is-transitioning");
					reinitializeScrollPane(Math.abs(newPaneTop));
				}
				pane.one("transitionend", endTransition);

				var transitionDuration = parseFloat(pane.css("transition-duration"));
				if (transitionDuration < 1)
				{
					transitionDuration *= 1000;
				}

				// L'événement "transitionend" étant lent à arriver, on tente de l'accélérer les choses
				setTimeout(endTransition, transitionDuration);

				if (typeof(callback) === "function")
				{
					setTimeout(
						function()
						{
							callback(item);
						},
						transitionDuration
					);
				}
			}
		}

		// Comportement du bouton d'affichage des sous-éléments
		menu.on(
			"click",
			".js-toggleMenuItemChildren",
			function()
			{
				var item = jQuery(this).parents(".Menu-item, .Menu-sectionItem").eq(0);
				toggleItemChildren(item);
			}
		);

		// Ouverture de l'arborescence actuelle
		pageEnvironment = pageEnvironment || {};
		pageEnvironment.context = pageEnvironment.context || {};
		if (pageEnvironment.context.entryId)
		{
			var toogleSectionCallback = false;
			if (pageEnvironment.context.thematicId)
			{
				toogleSectionCallback = function()
				{
					toggleItemChildren(jQuery("#RetractableMenu .Menu-sectionItem--" + pageEnvironment.context.thematicId));
				}
			}
			toggleItemChildren(
				jQuery("#RetractableMenu .Menu-item--section-" + pageEnvironment.context.entryId),
				toogleSectionCallback
			);
		}

		// Ajout des mécanismes de déploiement/rétractation du menu
		menu.on("toggle show hide", function(event)
		{
			var action;
			switch (event.type)
			{
				case "show":
					action = "addClass";
					break;
				case "hide":
					action = "removeClass";
					break;
				default:
					action = "toggleClass";
			}

			menu[action]("is-deployed");

			// Lorsque le menu est déployé, sa taille changeant en fonction de la taille de la fenêtre,
			// il faut ajuster le mécanisme de défilement lorsque la taille change
			if (menu.hasClass("is-deployed"))
			{
				reinitializeScrollPane();
				jQuery(window).on("resize", reinitializeScrollPane);
			}
			// Lorsque le menu est rétracté, ça n'est plus nécessaire, on peut donc désactiver l'ajustement
			// afin d'économiser les ressources
			else
			{
				jQuery(window).off("resize", reinitializeScrollPane);
			}
		});

		/**
		 * Mise en place du comportement
		 */

		// Actionner la molette de la souris sur le menu ne fait pas défiler la page en arrière plan
		// De même pour le glissement de doigt sur interface tactile
		menu.on({
			mousewheel: function(event)
			{
				event.preventDefault();
			},
			swipeleft: function(event)
			{
				menu.trigger("hide");
				event.preventDefault();
				event.stopPropagation();
			},
			touchstart: function()
			{
				jQuery("body").css("overflow", "hidden");	//@todo Devrait peut-être être géré par une classe
			},
			touchend: function()
			{
				setTimeout(
					function()
					{
						jQuery("body").css("overflow", "");	//@todo Idem, du coup, hein
					},
					50
				);
			}
		});

		// Ajout des mécanismes de déploiement/rétractation du menu sur les boutons
		jQuery(document).on(
			"click",
			".js-toggleRetractableMenu, .js-showRetractableMenu, .js-hideRetractableMenu",
			function(event)
			{
				event.preventDefault();

				var element = jQuery(this);

				var action = "toggle";
				if (element.hasClass("js-showRetractableMenu"))
				{
					action = "show";
				}
				else if (element.hasClass("js-hideRetractableMenu"))
				{
					action = "hide";
				}

				menu.trigger(action);
			}
		);
	}

	var buildHorizontalMenu = function()
	{
		var navigation = jQuery(".Menu");
		var container = jQuery(".Menu-wrapper", navigation);
		container.removeClass("is-static");

		var moreButton = jQuery('<button class="Menu-moreLinks js-toggleRetractableMenu"></button>');
		container.append(moreButton);

		var elements = jQuery(".Menu-item", navigation).get();

		elements.sort(function(elementOne, elementTwo)
		{
			var elementOne = jQuery(elementOne);
			var elementTwo = jQuery(elementTwo);

			var elementOneWeight = parseInt(elementOne.data("weight"));
			var elementTwoWeight = parseInt(elementTwo.data("weight"));

			var elementOneLength = elementOne.find(".Menu-link").outerWidth();
			var elementTwoLength = elementTwo.find(".Menu-link").outerWidth();

			var status = 0;

			if (elementOneWeight > elementTwoWeight)
			{
				status = 1;
			}
			else if ((elementOneWeight === elementTwoWeight))
			{
				if (elementOneLength > elementTwoLength)
				{
					status = -1;
				}
				else if (elementOneLength < elementTwoLength)
				{
					status = 1;
				}
			}
			else
			{
				status = -1;
			}

			return status;
		});

		elements = jQuery(elements);

		elements.on("mouseenter", function()
		{
			var element = jQuery(this);
			var sectionList = element.find(".Menu-sectionList");

			if (sectionList.length)
			{
				var documentWidth = jQuery(document).width();
				var listWidth = sectionList.outerWidth();

				var maxSubSectionListWidth = 0;
				jQuery(".Menu-sectionItem", sectionList).each(function()
				{
					var sectionItem = jQuery(this);
					var subSectionList = jQuery(".Menu-subSectionList", this);

					if (subSectionList.length)
					{
						sectionItem.addClass("Menu-parentItem");

						var subSectionListWidth = subSectionList.outerWidth();

						if (subSectionListWidth > maxSubSectionListWidth)
						{
							maxSubSectionListWidth = subSectionListWidth;
						}

						sectionItem.on("mouseenter", function()
						{
							sectionList.css("min-height", "");
							subSectionList.css("min-height", sectionList.height());
							sectionList.css("min-height", subSectionList.height());
						})
					}
					else
					{
						sectionItem.on("mouseenter", function()
						{
							sectionList.css("min-height", "");
						});
					}
				});

				listWidth += maxSubSectionListWidth;

				if (element.offset().left + listWidth > documentWidth)
				{
					sectionList.addClass("is-reversed");
				}
				else
				{
					sectionList.removeClass("is-reversed");
				}
			}
		});

		var moreButtonSize = Math.ceil(moreButton.outerWidth(true));
		var header = jQuery(".SiteHeader");
		var headerContent = jQuery(".SiteHeader-content", header);

		var toggleElements = function(secondPass)
		{
			var containerSize = Math.floor(navigation.width());
			var elementSize = 0;

			elements.each(function()
			{
				elementSize += Math.ceil(jQuery(this).outerWidth());
			});

			if ((elementSize > containerSize) || hasRetractableOnlyElements)
			{
				containerSize -= moreButtonSize;
			}

			var i = 0;

			var hasHiddenElements = false;
			while (i < elements.length)
			{
				var element = elements.eq(i);
				var elementIsVisible = element.is(":visible");

				if (elementSize >= containerSize)	// Ce devrait être ">" plutôt que ">=" mais IE et Chrome créent une nouvelle ligne si la taille est identique
				{
					elementSize -= Math.ceil(element.outerWidth());

					hasHiddenElements = true;
					if (elementIsVisible)
					{
						element.addClass("is-hidden");
					}
				}
				else
				{
					if (elementIsVisible)
					{
						i = elements.length;
					}
					else
					{
						element.removeClass("is-hidden");
					}
				}

				++i;
			}

			// Recalcul de la taille des éléments, certains navigateurs (Chrome et ses nombres à virgule...) n'attribuant pas la même taille s'ils sont masqués ou non
			elementSize = 0;
			elements.filter(":not(.is-hidden)").each(function()
			{
				elementSize += Math.ceil(jQuery(this).outerWidth());
			});

			if(hasRetractableOnlyElements) {
				hasHiddenElements = true;
			}

			if (hasHiddenElements)
			{
				header.removeClass("SiteHeader--complete");
				moreButton.addClass("is-visible");
				elementSize += moreButtonSize;
			}
			else
			{
				header.addClass("SiteHeader--complete");
				moreButton.removeClass("is-visible");
			}
			elementSize += 1;	// Ajout d'un pixel car certains navigateurs (Chrome, IE) n'arrivent pas à afficher tous les éléments dans un conteneur qui a pourtant leur taille
			container.css("width", elementSize);
			if (elementSize > parseInt(header.css("min-width")))
			{
				header.removeClass("SiteHeader--minimal").css("width", elementSize);
				headerContent.css("width", elementSize);
			}
			else
			{
				header.addClass("SiteHeader--minimal").css("width", "");
				headerContent.css("width", "");
			}

			if ((typeof(secondPass) !== "boolean") || !secondPass)
			{
				setTimeout(
					function() { toggleElements(true); },
					1
				);
			}
		}

		var loadIntervalReference;

		runOnPageDeclination(
			"large",
			function()
			{
				toggleElements();
				jQuery(window).on("resize", toggleElements);

				// La taille du menu est réévaluée jusqu'à ce que toutes les
				// ressources soient chargées afin de s'adapter au changement
				// de police
				if (document.readyState !== "complete")
				{
					clearInterval(loadIntervalReference);

					loadIntervalReference = setInterval(
						function()
						{
							if (document.readyState === "complete")
							{
								clearInterval(loadIntervalReference);
							}
							toggleElements();
						},
						1000
					);
				}
			},
			function()
			{
				jQuery(window).off("resize", toggleElements);
				jQuery(".Header").css("width", "");
				clearInterval(loadIntervalReference);
			}
		);

		jQuery(document).ready(function()
		{
			runOnPageDeclination(
				"large",
				toggleElements,
				{trackEvent: false}
			);
		});


		// Réduction de la taille des sous-menus
		var itemLimit = 10;

		function showMore(element)
		{
			var element = jQuery("#RetractableMenu ." + element.attr("class").replace(/^.*\s(Menu-(?:item--section-|sectionItem--)[0-9]+)(?:\s.*)$/, "$1"));
			var toggleButton = element.filter(":not(.is-opened)").find("> .js-toggleMenuItemChildren");

			var parentToggleButton = element.parents(".Menu-item:not(.is-opened)").eq(0).find("> .js-toggleMenuItemChildren");
			if (parentToggleButton.length)
			{
				parentToggleButton.trigger("click");
			}

			toggleButton.trigger("click");
		}

		elements.each(function()
		{
			var entry = jQuery(this);
			var sectionList = jQuery(".Menu-sectionList", this);
			var sectionItems = jQuery(".Menu-sectionItem", sectionList);

			if (sectionItems.length > itemLimit)
			{
				sectionItems.slice(itemLimit - 1).remove();

				var moreButton = jQuery('<button class="Menu-sectionLink Menu-sectionLink--more js-showRetractableMenu">Voir plus&hellip;</button>');

				moreButton.on("click", function()
				{
					showMore(entry);
				});

				jQuery('<li class="Menu-sectionItem"></li>').append(moreButton).appendTo(sectionList);

				sectionItems = jQuery(".Menu-sectionItem", sectionList);
			}

			sectionItems.each(function()
			{
				var sectionItem = jQuery(this);
				var subSectionList = jQuery(".Menu-subSectionList", this);
				var subSectionItems = jQuery(".Menu-subSectionItem", subSectionList);

				if (subSectionItems.length > itemLimit)
				{
					subSectionItems.slice(itemLimit - 1).remove();

					var moreButton = jQuery('<button class="Menu-subSectionLink Menu-subSectionLink--more js-showRetractableMenu">Voir plus&hellip;</button>');

					moreButton.on("click", function()
					{
						showMore(sectionItem);
					});

					jQuery('<li class="Menu-subSectionItem"></li>').append(moreButton).appendTo(subSectionList);
				}
			});
		});
	}

	jQuery(document).on("menuAvailable", function buildMenus()
	{
		buildRetractableMenu();
		buildHorizontalMenu();
		jQuery(document).off("menuAvailable", buildMenus);
	});
})();
jQuery(document).available(
	".js-ArticleListCarousel",
	function()
	{
		var container = jQuery(this);
		var viewer = jQuery(".js-ArticleListCarouselViewer", container);
		var links = jQuery(".ArticleLink", container);
		var timerReference;
		var timerDuration = 5000;

		// Rend un élément actif (et désactive le précédent)
		var setActiveElement = function(element)
		{
			links.removeClass("is-active");
			element.addClass("is-active");

			elementClasses = element.attr("class");

			var viewerContainer = jQuery('<div>').addClass(elementClasses).removeClass("ArticleLink ArticleLink--standard ArticleLink--stacked ArticleLink--highlight is-active");

			var viewerLink = element.find(".ArticleLink-link").clone();
			viewerLink.find(".ArticleLink-content").remove();

			viewerContainer.append(viewerLink);

			viewer.html(viewerContainer);
		}

		// Change automatiquement l'élément actif
		var rotate = function()
		{
			var currentActiveIndex = links.index(links.filter(".is-active").get(0));
			var nextActiveIndex = (currentActiveIndex + 1) % links.length;

			setActiveElement(links.eq(nextActiveIndex));

			timerReference = setTimeout(rotate, timerDuration);
		}

		var setHoverActiveElement = function()
		{
			setActiveElement(jQuery(this));
		}

		runOnPageDeclination(
			"large",
			function()
			{
				container.addClass("is-active");

				links.on("mouseenter", setHoverActiveElement);

				setActiveElement(links.eq(0));

				container.on("mouseleave", function()
				{
					clearTimeout(timerReference);
					timerReference = setTimeout(rotate, timerDuration);
				});
				container.trigger("mouseleave");
				container.on("mouseenter", function()
				{
					clearTimeout(timerReference);
				});
			},
			function()
			{
				container.removeClass("is-active");
				links.removeClass("is-active");
				links.off("mouseenter", setHoverActiveElement);
				clearTimeout(timerReference);
			}
		);
	},
	{
		endsOnDOMReady: true
	}
);
