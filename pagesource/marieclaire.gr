	if (String(parseFloat(target)) === String(target))
	{
		targetTop = target;
	}
	else
	{
		target = jQuery(target);
		if (target.length)
		{
			targetTop = target.offset().top;
		}
	}

	var virtualPageTop = getVirtualPageTop();

