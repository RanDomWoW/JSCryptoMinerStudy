jQuery(document).available(
	".SiteHeader",
	function()
	{
		// Calcul de la taille des liens de réseaux sociaux
		/* @todo
		runOnPageDeclination(
			"large",
			function()
			{
				var socialLinksContainer = jQuery(".SiteHeader-link-socialContainer");
				socialLinksContainer.attr("style", "width: auto !important");
				socialLinksContainer.css("width", socialLinksContainer.outerWidth());
			},
			{trackEvents: false}
		);*/

		// Mise en place du fonctionnement de l'entête minimisé
		var body = jQuery("body");
		var headerContainer = jQuery(".SiteHeader-container", this);
		var headerContentContainer = jQuery(".SiteHeader-contentContainer", this);
		var additionalContentContainer = jQuery('<div class="SiteHeader-additionalContent">');
		var additionalContent;

		var magazineSubscriptionContainer = jQuery(".SiteHeader-magazineSubscription");

		// Calcul de la hauteur de l'entête en version réduite
		body.addClass("is-scrolled");
		var contentHeight = headerContentContainer.height();
		body.removeClass("is-scrolled");

		var checkScroll = function()
		{
			var scrollTop = jQuery(document).scrollTop();
			var headerContainerTop = headerContainer.offset().top;
			var headerContainerBottom = headerContainerTop + headerContainer.height();
			var timerReference;

			if (scrollTop <= headerContainerTop)
			{
				body.removeClass("is-scrolled");
				headerContentContainer.css("height", "");
			}
			else if (headerContainerBottom - contentHeight < scrollTop)
			{
				body.addClass("is-scrolled");
				headerContentContainer.css("height", "");
			}
			else if (body.hasClass("is-scrolled"))
			{
				var space = headerContainerBottom - scrollTop;

				if (space > contentHeight)
				{
					headerContentContainer.css("height", space);
				}
				else
				{
					headerContentContainer.css("height", "");
				}
			}
		}

		jQuery(window).on("scroll", checkScroll);

		var setAdditionalContentProperties = function()
		{
			if (additionalContent)
			{
				var additionalContentWidth, additionalContentHeight;

				// Calcul de la taille du conteneur
				var additionalContentContainerWidth = 0, additionalContentContainerHeight = 0;
				var bodyIsScrolled = body.hasClass("is-scrolled");

				additionalContentContainer.css("width", "");
				additionalContent.css("width", "");
				body.addClass("is-scrolled");
				var additionalContentContainerWidth = additionalContent.get(0).getBoundingClientRect().width;
				var additionalContentContainerHeight = additionalContent.height();
				if (!bodyIsScrolled)
				{
					body.removeClass("is-scrolled");
				}

				// Calcul de la taille des contenus
				var additionalContentChildrenWidth = 0, additionalContentChildrenHeight = 0;
				jQuery("> *", additionalContent).each(function()
				{
					var element = jQuery(this);
					additionalContentChildrenWidth += element.outerWidth(true);
					var elementHeight = element.outerHeight(true);

					if (elementHeight > additionalContentChildrenHeight)
					{
						additionalContentChildrenHeight = elementHeight;
					}
				});

				// Prise en compte de l'un ou de l'autre, en fonction de quelle version a été ratée par le navigateur
				if (additionalContentContainerWidth > additionalContentChildrenWidth)
				{
					additionalContentWidth = additionalContentContainerWidth;
					additionalContentHeight = additionalContentContainerHeight;
				}
				else
				{
					additionalContentWidth = additionalContentChildrenWidth;
					additionalContentHeight = additionalContentChildrenHeight;
				}

				var additionalContentMarginTop = -1 * additionalContentHeight / 2;

				if (getPageDeclinationName() === "small")
				{
					additionalContentWidth = getViewportWidth(additionalContentWidth) + "vw";
					additionalContentMarginTop = getViewportWidth(additionalContentMarginTop) + "vw";
				}

				additionalContent.css("width", additionalContentWidth);
				additionalContentContainer.css("margin-top", additionalContentMarginTop);
				additionalContentContainer.css("width", additionalContentWidth);
			}
		}

		// Contenu additionel :
		// - Si présents sur article : partages sur réseaux sociaux
		// - Sinon, lien d'abonnement si présent dans l'entête
		jQuery(document).available(
			".ArticleHeader",
			function()
			{
				var socialLinks = jQuery(".ArticleHeader .js-SocialNetworkLink").slice(0, 2).add(".ArticleHeader .js-SocialNetworkLink[data-type=email]");
				if (socialLinks.length)
				{
					additionalContent = socialLinks.parents(".SocialNetwork").eq(0).clone().html("");
					additionalContent.addClass("SocialNetwork--light SocialNetwork--header");
					additionalContent.append(socialLinks.clone());
				}

				if (additionalContent)
				{
					additionalContentContainer.append(additionalContent).appendTo(jQuery(".SiteHeader-content", headerContentContainer));
					magazineSubscriptionContainer.addClass("is-static");
					setAdditionalContentProperties();
				}
			},
			{limit: 1, endsOnDOMReady: true}
		);

		setAdditionalContentProperties();
		jQuery(document).on("pageDeclinationChange", function()
		{
			setAdditionalContentProperties();
			setTimeout(setAdditionalContentProperties, 100);
		});

		// Recherche
		var searchOverlay = jQuery(".SiteHeader-search", this);

		jQuery(document).on("click", ".js-toggleSearchOverlay, .js-showSearchOverlay, .js-hideSearchOverlay", function(event)
		{
			event.preventDefault();

			var element = jQuery(this);

			if (element.hasClass("js-showSearchOverlay") || (element.hasClass("js-toggleSearchOverlay") && !searchOverlay.hasClass("is-visible")))
			{
				showOverlay(searchOverlay, {
					show: function()
					{
						jQuery(".js-searchQueryField", searchOverlay).focus();
					}
				});
			}
			else if (element.hasClass("js-hideSearchOverlay"))
			{
				hideOverlay(searchOverlay);
			}
		});
	},
	{limit: 1}
);
