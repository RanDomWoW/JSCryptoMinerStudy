jQuery(document).ready(function()
{
	if (jQuery.cookie("fbpop"))
	{
		jQuery(".FacebookPopin").remove();
	}
	else
	{
		runOnPageDeclination(
			"large",
			function()
			{
				jQuery(window).on("scroll", function handleFacebookPopin()
				{
					var limit = jQuery(document).outerHeight() / 5;

					var articleContent = jQuery(".ArticleContent");
					if (articleContent.length)
					{
						limit = Math.max(limit, articleContent.offset().top + articleContent.height() - window.innerHeight);
					}

					if (jQuery(document).scrollTop() > limit)
					{
						var popin = jQuery(".FacebookPopin");
						var content = jQuery(".FacebookPopin-content", popin);
						content.css("height", content.outerHeight());

						popin.addClass("is-visible is-highlighted");
						setTimeout(
							function()
							{
								popin.removeClass("is-highlighted");
							},
							5000
						);
						jQuery(window).off("scroll", handleFacebookPopin);
					}
				});
			},
			{once: true}
		);
	}
});

jQuery(document).on("click", ".js-closeFacebookPopin", function()
{
	jQuery.cookie("fbpop", 1, {expires: 7});
	jQuery(this).parents(".FacebookPopin").remove();
});

