var countCheckButtonInModal=0;
var checkFormSubmitResult = function () {
    var getTargetInNewModal=function(){
        countCheckButtonInModal++;
        // watch for popup load by button's check
        if(document.getElementsByClassName("submit_but alert_button").length<1){
            if(countCheckButtonInModal>=30){return;}
            setTimeout(getTargetInNewModal(), 50);
        }
        else {
            var tempCountFor=0;
            var getA=setInterval(function(){
                tempCountFor++;
                var tempPopup=$("#alert-message");
                if(tempPopup.find("a").length){
                    tempPopup.find("a").first().attr("href", "/ru/rules");
                    tempPopup.find("a").first().attr("target", "_blank");
                    tempPopup.find("a").first().removeAttr("onclick");
                    clearInterval(getA);
                }
            });
            var style = document.createElement("style");
            style.innerHTML = "#alert-message p{display:block !important;}";
            document.body.appendChild(style);
        }
    };
    if(document.getElementsByClassName("ui-dialog ui-widget").length>countOfModal){
        document.body.removeEventListener('DOMSubtreeModified', checkFormSubmitResult, false);
        getTargetInNewModal();
    }
};

/**
 * popup with form of registration for privacy change
 */
var checkFormPopup = function () {
    var popup=document.getElementById("registeruser0");
    if (popup!=null) {
        document.body.removeEventListener('DOMSubtreeModified', checkFormPopup, false);
        // get user type
        var state = localStorage.getItem("state-kad");
        if ( state == 'webmaster') {document.getElementById("userIsWeb").setAttribute('checked', 'checked');}
        else {document.getElementById("userIsAdv").setAttribute('checked', 'checked');}
        var inputs=document.getElementsByTagName("input");
        for(var i=0; i<inputs.length; i++){
            if(inputs[i].getAttribute("name")=="forms[registeruser0][userAgreed]"){
                inputs[i].setAttribute('checked', 'checked');
            }
        }
        // resize popup -- simply add Class
        var resizePopup=function() {
            var parentBlock, classList;
            var innerElement = document.getElementById("registeruser0");
            do {
                innerElement = innerElement.parentNode;
                classList = innerElement.className;
                if (typeof classList == "undefined"){break;}
                if (classList.indexOf("ui-dialog") != -1 && classList.indexOf("ui-widget") != -1 && classList.indexOf("ui-widget-content") != -1){parentBlock=innerElement;}}
            while (innerElement.parentNode != null);
            if(typeof parentBlock=="undefined"){
                setTimeout(function(){resizePopup()}, 30);
            }
            else{
                parentBlock.className=parentBlock.className.replace("ui-draggable", "newPopupForm");
                document.body.scrollLeft=0;
                document.body.scrollTop=0;
            }
        };
        resizePopup();
        // privacy
        var tempBlock=document.getElementsByClassName("user-terms validation-container");
        tempBlock=tempBlock[tempBlock.length-1];
        tempBlock.setAttribute("checked","checked");
        /*
        var newPrivacy=document.createElement("div");
        newPrivacy.setAttribute("class", "col-sm-12");
