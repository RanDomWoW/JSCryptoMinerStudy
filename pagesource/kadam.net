        newPrivacy.innerHTML='<hr> <p class="policy_text">Регистрируясь в Сервисе интернет-рекламы Kadam, я подтверждаю, что ознакомлен(а) и обязуюсь выполнять требования Правил использования Сервиса, а также даю свое согласие на обработку моих персональных данных в соответствии с <a href="http://docs.kadam.net/privacy_policy_v2.pdf">Политикой конфиденциальности.</a></p> <hr>';
        tempBlock.parentNode.insertBefore(newPrivacy, tempBlock);
        tempBlock.style.display="none";
        */

        // check for result message with error
        document.querySelector("input[type='submit']").addEventListener("click", function(){
            countOfModal=document.getElementsByClassName("ui-dialog ui-widget").length;
            document.body.removeEventListener('DOMSubtreeModified', checkFormSubmitResult, false);
            document.body.addEventListener('DOMSubtreeModified', checkFormSubmitResult, false);
            setTimeout(function () {
                document.body.removeEventListener('DOMSubtreeModified', checkFormSubmitResult, false);
            }, 10000)
        });

    }
};
document.getElementById("btn-registration").addEventListener("click", function() {
    document.body.addEventListener('DOMSubtreeModified', checkFormPopup, false);
});
