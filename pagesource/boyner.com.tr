<script type="text/javascript">
    $(document).ready(function () {

        // Visilabs best sellers carousel
        $('.js-vl-best-sellers-slider').on('click', '.js-tab-title', function() {
            var $tab = $(this);
            if (!$tab.data('loading')) {
                var categoryId = $tab.data('category-id');
                var visilabsId = $tab.data('visilabs-id');
                var vl = new Visilabs();
                $tab.data('loading', true);
                vl.AddParameter("json", true);
                vl.AddParameter("cat", categoryId);
                if (window.isMobile == true) {
                    vl.AddParameter("OM.vchannel", "mobile");
                }
                vl.Suggest(visilabsId, null, null, function (data) {
                    
                    RenderRecommendedProducts({
                        data: data,
                        url: "/product/getrecommendedproducts",
                        widgetContainer: ".reveal.js-vl-bestSeller",
                        productContainer: ".reveal .js-vl-bestSeller-content"
                    }, function () {
                        
                        completeBestSellers($tab);
                    }, function () {
                        //alert("hata");
                        
                        var $sliderContent = $('.reveal .js-vl-bestSeller-content');
                        $sliderContent
                            .html('Ürünleri getirirken bir hata oluştu.')
                            .css('line-height', $sliderContent.height() + 'px');
                        completeBestSellers($tab);
                    });
                });
                vl.CollectClicks();
            }
        });

        // Select first tab to load products
        $('.js-vl-best-sellers-slider .js-tab-title:first').click();

        // Callback function to be called after visilabs request
        function completeBestSellers($tab) {
            initCarousel(".reveal .js-vl-bestSeller-content", "v5");
            $tab.data('loading', false);
            $tab.removeClass('js-tab-title');
        }

        //Visilabs Week Deals slider
        (function() {
            var $sliderContent = $('.js-vl-weekDeal-content');
            RenderRecommendedProductsForWeeklySlider({
                data: $sliderContent.attr('data-productids').split(/[\s,]+/),
                url: "/product/getrecommendedproducts",
                widgetContainer: ".js-vl-weekDeal",
                productContainer: ".js-vl-weekDeal-content"
            }, function () {
                initCarouselV1(".js-vl-weekDeal-content", "v1");
                $sliderContent.addClass('carousel-v1');
            }, function () {
                $sliderContent
                    .html('Ürünleri getirirken bir hata oluştu.');
                initCarouselV1(".js-vl-weekDeal-content", "v1");
            });
        })();
    });
</script>



<script type="text/javascript">
    $(document).ready(function () {
        setTimeout(function () { initCarousels() }, 1000);
        //initSliders();
        var array = [];
        array = $(".main-slider img");
        $.each(array,
            function (index, element) {
                var dataSplit = [];
                var alt = $(element).prop('alt');
                var id = $(element).prop('id');
                var src = $(element).prop('src');
                dataSplit.push(id);
                dataSplit.push(alt);
                dataSplit.push(src);
                if (dataSplit.length >= 3 && dataSplit[0] != "" && dataSplit[1] != "" && dataSplit[2] != "") {
                    data = {
                        'id': dataSplit[0], // Benzersiz olmalı.
                        'name': dataSplit[1],
                        'creative': dataSplit[2],
                        'position': index + 1
                    }
                    promotionList.push(data);
                }
            });

        $('a').click(function () {
            var img = $(this).children('img')[0];
            if (img != null && img.id != "" && img.alt != "" && img.src != "") {
                var data = $.Enumerable.From(dataLayer[0].ecommerce.promoView.promotions).Where('$.id=="' + img.id + '"').FirstOrDefault();
                dataLayer.push({
                    'event': 'promotionClick',
                    'ecommerce': {
                        'promoClick': {
                            'promotions':
                            [
                                {
                                    'id': data.id,
                                    'name': data.name,
                                    'creative': data.creative,
                                    'position': data.position
                                }
                            ]
                        }
                    }
                });
            }

        });

        var elementDatas = [], newElementDatas = [], bannerIds = [];

        //Enhanced Ecommerce
        var firstline1Show = false;
        var secondline1Show = false;
        var firstline2Show = false;
        var secondline2Show = false;
        $(window).scroll(function () {
            if ((($(window).height() + window.scrollY) > ($('.entry-showcase').offset().top + $('.entry-showcase-box').height())) && firstline1Show == false) {
                var array2 = [];
                array2 = $(".entry-showcase-box:not(.entry-carousel) img, .banner-list figure img");
                elementDatas = [], newElementDatas = [];
                createPushElementDatas(array2, "showcase");
                newElementDatas = elementDatas.slice(0, 2);
                if (newElementDatas != '') {
                    sendProductImpressionsScrollPromotions(newElementDatas);
                }
                firstline1Show = true;
            }
        });

        $(window).scroll(function () {
            if ((($(window).height() + window.scrollY) > ($('.entry-showcase').offset().top + ($('.entry-showcase-box').height() * 2))) && secondline1Show == false) {

                var array2 = [];
                array2 = $(".entry-showcase-box:not(.entry-carousel) img, .banner-list figure img");
                elementDatas = [], newElementDatas = [];
                createPushElementDatas(array2, "showcase");
                newElementDatas = [];
                newElementDatas = elementDatas.slice(2, 4);
                if (newElementDatas != '') {
                    sendProductImpressionsScrollPromotions(newElementDatas);
                }
                secondline1Show = true;
            }
        });

        $(window).scroll(function () {
            if ((($(window).height() + window.scrollY) > ($('.entry-banner').offset().top + $('.entry-banner-box').height())) && firstline2Show == false) {
                var array2 = [];
                array2 = $(".banner-list figure img");
                elementDatas = [], newElementDatas = [];
                createPushElementDatas(array2, "banner-box");
                newElementDatas = elementDatas.slice(0, 2);
                if (newElementDatas != '') {
                    sendProductImpressionsScrollPromotions(newElementDatas);
                }
                firstline2Show = true;
            }
        });

        $(window).scroll(function () {
            if ((($(window).height() + window.scrollY) > ($('.entry-banner').offset().top + ($('.entry-banner-box').height() * 2))) && secondline2Show == false) {

                var array2 = [];
                array2 = $(".banner-list figure img");
                elementDatas = [], newElementDatas = [];
                createPushElementDatas(array2, "banner-box");
                newElementDatas = [];
                newElementDatas = elementDatas.slice(2, 4);
                if (newElementDatas != '') {
                    sendProductImpressionsScrollPromotions(newElementDatas);
                }
                secondline2Show = true;
            }
        });

        function sendProductImpressionsScrollPromotions(elementDatas) {
            dataLayer.push({
                "event": "promotion_impressions_scroll",
                "noninteraction": true,
                'ecommerce': {
                    'promoView': {
                        'promotions': elementDatas
                    }
                }
            });
        }

        function createPushElementDatas(elements, position) {
            $.each(elements, function (index, element) {
                if (element.id != "" && element.src != "" && element.alt != "") {
                    var elementData = {
                        id: element.id,
                        name: element.alt,
                        creative: element.src,
                        position: position + " | " + index
                    };

                    elementDatas.push(elementData);
                }
            });
        }

    });
</script>


