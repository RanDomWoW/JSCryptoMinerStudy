var searchVal =
$(document).ready(function() {

	function searchSuggest (request, response) {
		$.ajax({
			url: "/search/",
			dataType: "json",
			type: "GET",
			data: {
				q: $('#global_search').val(),
				hint: 1,
				c: $('#search_category_id').val() || null
			}, success: function (jsonResponse) {
				setTimeout(function(){
					response(jsonResponse);
				},300);

			}
		});
	};

	$('#global_search').autocomplete({
		source: searchSuggest,
		select: function(event, ui) {
			event.preventDefault();
			$(this).val(ui.item.title);
			$(this).attr('value',ui.item.title);
			/*$(this).autocomplete( "destroy" );*/
			document.location.href = ui.item.link;
		},
		focus: function(event, ui) {
			event.preventDefault();
			$(this).val(ui.item.title);

		},
		open: function( event, ui ) {
			var currSearchVal = $(this).val()? $.trim($(this).val()):null;
			$('.search_suggest_item').each(function(){
				if($(this).text().indexOf(currSearchVal) != -1){
					var startValNum = $(this).text().indexOf(currSearchVal);
					var finishValNum = startValNum + (currSearchVal.length-1);
					var finishStringBegin = $(this).text().slice(0, startValNum);
					var finishStringPreTarget = $(this).text().slice(startValNum, finishValNum+1);
					var finishStringTarget = '<span class="bold">' + finishStringPreTarget + '</span>';
					var finishStringEnd = $(this).text().slice(finishValNum+1);

					$(this).html(finishStringBegin + finishStringTarget + finishStringEnd);

					if($(this).text() == "услуги"){
						$(this).html('<i class="icon-biglion-tags font_tags"></i>&nbsp;<span class="bold">'+$(this).text()+'</span>')
					}else if($(this).text() == "отели"){
						$(this).html('<i class="icon-biglion-hotel-1 font_hotel"></i>&nbsp;<span class="bold">'+$(this).text()+'</span>')
					}else if($(this).text() == "туры"){
						$(this).html('<i class="icon-biglion-flight font_flight"></i>&nbsp;<span class="bold">'+$(this).text()+'</span>')
					}else if($(this).text() == "магазины"){
						$(this).html('<i class="icon-biglion-bag font_bag"></i>&nbsp;<span class="bold">'+$(this).text()+'</span>')
					}else if($(this).text() == "товары по купонам"){
						$(this).html('<i class="icon-biglion-headphones font_headphones"></i>&nbsp;<span class="bold">'+$(this).text()+'</span>')
					}
				}
			});
		},
		delay: 300,
		position: { my : "left top", at: "left bottom" , of: ".search-header"},
		appendTo: '#menu-container'

	});



	$('#global_search').focus(function(){
		setTimeout(function() {
			if ( $('.searchbox').hasClass('active') ) {
				$('#global_search').data("autocomplete").search(' ');
			}
		},300);
	});

	$('#global_search').data("autocomplete")._renderItem = function (ul, item) {
		//console.log($('#global_search').val());
		return $("<li></li>")
				.data("item.autocomplete", item)
				.append(function(){
					if(item.link == '/services/') {
						return "<a class='search_suggest_item' href='" + item.link + "'>" + "<div class='search_suggest_menu'><i class='icon-biglion-tags font_tags'></i>" + item.title + "</div>" + "<em>" + (item.catid ? item.catid : '') + "</em>" + "<div class='search_suggest_subtext'>" + (item.subtext ? item.subtext : '') + "</div>" + "</a>";
					}else if(item.link == '/hotels/'){
						return "<a class='search_suggest_item' href='" + item.link + "'>" + "<div class='search_suggest_menu'><i class='icon-biglion-hotel-1 font_hotel'></i>" + item.title + "</div>" + "<em>" + (item.catid ? item.catid : '') + "</em>" + "<div class='search_suggest_subtext'>" + (item.subtext ? item.subtext : '') + "</div>" + "</a>";
					}else if(item.link == '/tours/'){
						return "<a class='search_suggest_item' href='" + item.link + "'>" + "<div class='search_suggest_menu'><i class='icon-biglion-flight font_flight'></i>" + item.title + "</div>" + "<em>" + (item.catid ? item.catid : '') + "</em>" + "<div class='search_suggest_subtext'>" + (item.subtext ? item.subtext : '') + "</div>" + "</a>";
					}else if(item.link == '/shops/'){
						return "<a class='search_suggest_item' href='" + item.link + "'>" + "<div class='search_suggest_menu'><i class='icon-biglion-bag font_bag'></i>" + item.title + "</div>" + "<em>" + (item.catid ? item.catid : '') + "</em>" + "<div class='search_suggest_subtext'>" + (item.subtext ? item.subtext : '') + "</div>" + "</a>";
					}else if(item.link == '/services/goods/'){
						return "<a class='search_suggest_item' href='" + item.link + "'>" + "<div class='search_suggest_menu'><i class='icon-biglion-headphones font_headphones'></i>" + item.title + "</div>" + "<em>" + (item.catid ? item.catid : '') + "</em>" + "<div class='search_suggest_subtext'>" + (item.subtext ? item.subtext : '') + "</div>" + "</a>";
					}else{

						return "<a class='search_suggest_item' href='" + item.link + "'>" + "<div class='search_suggest_text'>" + item.title + "</div>" + "<em>"+ (item.catid ? item.catid : '') + "</em>" +"<div class='search_suggest_subtext'>" + (item.subtext ? item.subtext : '') + "</div>" + "</a>"
					}
				})
				.appendTo(ul);
	};


	$('form#search_form').submit(function(object){
		var $t = $(object.target);
		if($t.find('#search_category_id').val() == 'review') {
			var $el = $t.find('#global_search');
			$t.attr('action', '/review/');
			$el.after($('<input type="hidden" name="search_str_4"/>').val($el.val()));
		}
	});

	$('.searchbox .search-text').on('focus', function() {
		$(this).parents('.searchbox').addClass('active');

		var $searchInput = $('#global_search');
		var searchWidth = ($('#search_form').innerWidth()) - ($('#sCat').innerWidth() + $('.search-submit').innerWidth());
		var catName = $('.cat').html();
		var catBoxWidth = $('#sCat').find('.cat').html( catName ).end().outerWidth();

		$searchInput.width(searchWidth);

		$('.sCat_container').animate({'width':catBoxWidth},300);

		//$('#choose_city_btn').fadeOut(500); //исчезновение кнопки выбора города и появление фильра по цвету и цене
		//$('.filter_block').fadeIn(500);   //исчезновение кнопки выбора города и появление фильра по цвету и цене
	});


	$('#sCat').hover(function() {
		$(this).find('#sCatList').show();
	}, function() {
		$(this).find('#sCatList').hide();
	});

	$(document).on('click', function(e) {
		var target = $(e.target);
		//console.log(target);
		if ( $('.searchbox').hasClass('active') ) {
			$('#menu-container').show();
			if ( target.parents('.search-header').length == 0 && target.parents('#banner_pagi').length == 0 ) {
				$('.sCat_container').animate({'width':0},300);
				$('.searchbox').removeClass('active');
				$('#global_search').width( 300 );


				/*var textboxWidth = ( 355 - $('#sCat').outerWidth() );
				$('#global_search').width( textboxWidth );*/
				/*$('#choose_city_btn').fadeIn('slow');
				$('.filter_block').fadeOut('slow');*/
			} else {
				if ( target.parents('.search_icon_block2').length != 0 && target.is('a') ) {
					var catName = target.html();
					var catId = target.parent().attr('data-id');
					var textboxWidth = ( 355 - target.parents('#sCat').find('.cat').html( catName ).end().outerWidth() );
					var catBoxWidth = target.parents('#sCat').outerWidth();
					$('#global_search').width( textboxWidth );
					$('#menu-container').width( textboxWidth );
					$('#search_category_id').val( catId );
					target.parents('#sCatList').hide();

					$('.sCat_item').removeClass('active');
					target.closest('.sCat_item').addClass('active');
					$('.sCat_container').width(catBoxWidth);

				}

			}
		}else{
			$('#menu-container').hide();
			$('#global_search').autocomplete( "close" );
			$('#global_search').blur();

		}
		e.stopPropagation();
	});

});
