<script>
    $( document ).ready(function() {
        if(document.querySelector(".ushi_container") && document.querySelector(".ushi")){
            var showOpt = false;
            if(document.querySelector(".uho1")) {
                $(".ushi").append($(".uho1"));
                showOpt = true;
            }
            if(document.querySelector(".uho2")) {
                $(".ushi").append($(".uho2"));
                showOpt = true;
            }
            if(showOpt)  document.querySelector(".ushi").style.display='block';
        }
        $(window).resize();
        var aside = document.querySelector('div.ushi'),
            t0 = aside.getBoundingClientRect().top + window.pageYOffset - 60,
            t1 = document.documentElement.scrollHeight - 945;
        $(window).scroll(function(e) {
            if (window.pageYOffset > t1) {
                aside.classList.remove('prilip');
                aside.classList.add ('stop');
                aside.style.top = t1 - t0 + 'px';
            } else {
                aside.classList.remove('stop');
                if (t0 < window.pageYOffset) {
                    aside.classList.add('prilip');
                } else {
                    aside.classList.remove('prilip');
                }
                aside.style.top = '0';
            }
        });
        $(window).scroll();

    });
    $(window).on("resize", function(){
        if(document.querySelector(".ushi_container .ushi")){
            document.querySelector(".ushi_container .ushi").style.display = $(this).outerWidth() <= 1330 ? "none" : "block";
        }
    });
