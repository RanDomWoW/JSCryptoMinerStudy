<script id="template-upload" type="text/x-tmpl">
{% for (var i=0, file; file=o.files[i]; i++) { %}
    <div class="template-upload">
        <div class="soundRow">
            <div class="fileName">{%=file.name%}</div>

            <div class="cancel removeFile">{% if (!i) { %}
                {% if (!file.error) { %}
                    <button class="noStyleButton">
                        <i class="icon-remove"></i>
                    </button>
            {% }} %}
            </div>
            {% if (file.error) { %}
                <div class="error removeFile"><span class="label label-important">{%=locale.fileupload.errors[file.error] || file.error%}</span></div>
            {% } else if (o.files.valid && !i) { %}
                <div class="progress progress-success progress-striped active fileProgress"><div class="bar" style="width:0%;"></div></div>
                <div class="start">{% if (!o.options.autoUpload) { %}
                    <button style="display:none"></button>
                {% } %}</div>
            {% } %}
        </div>
    </div>
{% } %}
</script>
<!-- The template to display files available for download -->
<script id="template-download" type="text/x-tmpl">
{% for (var i=0, file; file=o.files[i]; i++) { %}
    <div class="template-download fade">
        <div class="soundRow">
            {% if (file.error) { %}
                <div class="fileName"><span>{%=file.name%}</span></div>
                <div class="removeFile"><span class="label label-important">{%=locale.fileupload.error%}</span></div>
                <div class="fileErrorMessage">{%=locale.fileupload.errors[file.error] || (file.error!=true)%}</div>
            {% } else { %}
                <div class="fileName"><span>{%=file.name%}</span></div>
                <div class="removeFile"><span class="label label-success">Completed</span></div>
            {% } %}
        </div>
    </div>
{% } %}
</script>
