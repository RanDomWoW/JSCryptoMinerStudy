<script>
  var clickShowPin = function(pinId){
      var pinHolderID = '#'+pinId;
      var isLiked =  $(pinHolderID+' .pinLikeIconClass').hasClass('fa-heart');
      var pinType = $(pinHolderID+' .pin__hidden__info__type').html();
      var pinTitle = $(pinHolderID+' .thumb__title').html();
      var pinImage = $(pinHolderID+' .pin_image_dummy').attr('src');
      var pinAuthorName = $(pinHolderID+' .pin__hidden__info__authorName').html();
      var pinAuthorLink = $(pinHolderID+' .pin__hidden__info__authorLink').html();
      var pinReviewText = $(pinHolderID+' .pin__hidden__info__reviewText').html();
      var pinReviewRate = $(pinHolderID+' .pin__hidden__info__reviewRate').html();
      var pinPurchaseUrl = $(pinHolderID+' .pin__hidden__info__purchaseUrl').html();
      var pinPurchaseProductImageUrl = $(pinHolderID+' .pin__hidden__info__purchaseProductImageUrl').html();

      $('#pinDetailsModal .pinDialogImage').attr('src',pinImage);
      $('#pinDetailsModal .pinDialogTitle').html(pinTitle);
      if (pinAuthorName && pinAuthorLink && pinAuthorName.length>0 && pinAuthorLink.length>0 && pinType != 'review'){
          $('#pinDetailsModal .pinDialogAuthor').html('@ ' + pinAuthorName);
          $('#pinDetailsModal .pinDialogAuthor').attr('href',pinAuthorLink);
      }else{
          $('#pinDetailsModal .pinDialogAuthor').html('');
          $('#pinDetailsModal .pinDialogAuthor').attr('href','');
      }
      if (pinType == 'review'){
          $('#pinDetailsModal .pinDialogReviewAuthor').html(pinAuthorName);
          $('#pinDetailsModal .pinDialogReviewText').html(pinReviewText);
          $('#pinDetailsModal .pinDialogProductImage').attr('src',pinPurchaseProductImageUrl);
          $('#pinDetailsModal .pinDialogProductShopButton').attr('href',pinPurchaseUrl);

          //show rate stars
          for (var i = 1; i < 6; i++) {
            $(".star" + i)[0].href.baseVal = "/icons/icons.svg#" + (pinReviewRate >= i ? "icon-star-full" : "icon-star-empty");
          }

          //show right panel
          $('#pinDetailsModal .pinDialogBodyLeft').removeClass('col-md-12').addClass('col-md-6');
          $('#pinDetailsModal .pinDialogBodyRight').show();
      }else{
          //hide right panel
          $('#pinDetailsModal .pinDialogBodyLeft').removeClass('col-md-6').addClass('col-md-12');
          $('#pinDetailsModal .pinDialogBodyRight').hide();
      }
      $('#pinDetailsModal').modal({show:true});
  }

  $(document).ready(function () {
    var limit = 4;
    $('.ambassadorPostCarousel').owlCarousel({
        items: limit,
        loop:true,
        margin:20,
        autoplay:true,
        autoplayTimeout:2000,
        autoplayHoverPause:true,
        responsive: {
          0: {
            items:2
          },
          640: {
            items:3
          },
          840: {
            items:limit
          }
        }
    });
  });

</script>
				 
