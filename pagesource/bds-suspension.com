function js_tooltip_init() {
	var el=document.getElementById('calendarWrap').getElementsByTagName('div');
	var lst=el.length;
	var ln=8;
	for (var i=0; i<lst; i++) {
		if (el[i].id.indexOf('event_tt')==0) js_tooltip_init_call(el[i].id.substr(ln));
	}
}

function js_tooltip_init_call(objId) {
	// select all desired input fields and attach tooltips to them
	$("#event"+objId).tooltip({  
		position: "center left",
		effect: "fade",
		opacity: 0.9,			
    tip : "#event_tt"+objId
	});  
