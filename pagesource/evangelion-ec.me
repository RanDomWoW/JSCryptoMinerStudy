.drop-k {
  display: inline-block;
  font-family: 'Permanent Marker', cursive;
  font-weight: 100;
  font-size: 9em;
  line-height: 0.8;
  padding: 0;
  position: relative;
  z-index: 3;
  
}
.drop-k:after {
  content: '\f13e';
  position: absolute;
  top: 0;
  left: 0;
  animation: pulse-fade 1.9s linear infinite;
}

@-webkit-keyframes pulse {
  0% {
    transform: scale(1);
  }
  20% {
    transform: scale(1.1);
  }
  30% {
    transform: scale(1);
  }
  40% {
    transform: scale(1.1);
  }
  50% {
    transform: scale(1);
  }
}
@-webkit-keyframes pulse-fade {
  0% {
    transform: scale(1);
    opacity: 1;
  }
  20% {
    transform: scale(1.2);
    opacity: 0;
  }
  30% {
    transform: scale(1);
    opacity: 1;
  }
  40% {
    transform: scale(1.2);
    opacity: 0;
  }
  50% {
    transform: scale(1);
    opacity: 1;
  }
}


