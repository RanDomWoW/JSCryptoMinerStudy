  $(function(){
 
    var $container = $('.blog-posts');
 
    $container.imagesLoaded(function(){
      $container.masonry({
        itemSelector: '.post-outer',
        columnWidth: 0
      });
    });
 
    $container.infinitescroll({
      navSelector  : '#blog-pager-older-link',    
