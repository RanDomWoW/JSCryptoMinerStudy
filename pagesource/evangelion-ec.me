/*Reset Slider*/
div.cream-slider > input {display: none;}
div.cream-slider ul.conten, div.cream-slider ul.conten li > img, div.cream-slider ul.conten li > label, div.cream-slider ul.conten li {height: 100%;}
div.cream-slider, ul.conten, ul.conten li, ul.conten li > label {
   -webkit-user-select: none;
   -moz-user-select: none;
/*box-sizing*/
   box-sizing: border-box;
   -webkit-box-sizing: border-box;
   -moz-box-sizing: border-box;
}
/*transiciones*/
div.cream-slider ul.conten li {
   -webkit-transition-timing-function: cubic-bezier(0.7, 0, 0.2, 1);
   -moz-transition-timing-function: cubic-bezier(0.7, 0, 0.2, 1);   
   transition-timing-function: cubic-bezier(0.7, 0, 0.2, 1);
/****/
   -webkit-transition-property: width, opacity, border-bottom;
   -moz-transition-property: width, opacity, border-bottom;   
   transition-property: width, opacity, border-bottom;
/****/
   -webkit-transition-duration: 500ms;
   -moz-transition-duration: 500ms;   
   transition-duration: 500ms;
/*Fix chrome
--------------------------*/
   -webkit-transform: translateZ(0);
}

/*Responsive transition (en vivo)*/
div.cream-slider, div.text h3, div.text span, div.text {
   -webkit-transition: all 0.4s ease 0s;
   -moz-transition: all 0.4s ease 0s;   
   transition: all 0.4s ease 0s;
/*Fix chrome
--------------------------*/
   -webkit-transform: translateZ(0);
}
/*Empezemos*/
div.cream-slider {
   background: #444;
   box-shadow: 0 3px 3px -2px rgba(0, 0, 0, 0.5);
   font-size: 16px;
   height: 380px;
   margin: 0 auto;
   max-width: 940px;
   width: 100%;
}
div.cream-slider ul.conten {
   overflow: hidden;
   position: relative;
   width: 100%;
}

div.cream-slider ul.conten li {
   background: #444;
   border-bottom: 4px solid #444;
   box-shadow: 0 0 3px 1px rgba(0, 0, 0, 0.6);
   display: block;
   float: left;
   position: relative;
   opacity:0.7;
}

div.cream-slider.n2 ul.conten li {width: 20%;}
div.cream-slider.n3 ul.conten li {width: 10%;}
div.cream-slider.n4 ul.conten li {width: 6.666%;}
div.cream-slider.n5 ul.conten li {width: 5%;}
div.cream-slider.n6 ul.conten li {width: 4%;}

.cream:nth-child(1):checked ~ ul.conten li:nth-child(1),
.cream:nth-child(2):checked ~ ul.conten li:nth-child(2),
.cream:nth-child(3):checked ~ ul.conten li:nth-child(3),
.cream:nth-child(4):checked ~ ul.conten li:nth-child(4),
.cream:nth-child(5):checked ~ ul.conten li:nth-child(5),
.cream:nth-child(6):checked ~ ul.conten li:nth-child(6) {
   width:80%;
   opacity:1;
   border-bottom:4px solid #336e7b;
}
.cream:nth-child(1):checked ~ ul.conten li:nth-child(1) > label,
.cream:nth-child(2):checked ~ ul.conten li:nth-child(2) > label,
.cream:nth-child(3):checked ~ ul.conten li:nth-child(3) > label,
.cream:nth-child(4):checked ~ ul.conten li:nth-child(4) > label,
.cream:nth-child(5):checked ~ ul.conten li:nth-child(5) > label,
.cream:nth-child(6):checked ~ ul.conten li:nth-child(6) > label {
   cursor:auto;
   z-index:10;
}

.cream:nth-child(1):checked ~ ul.conten li:nth-child(1) > div.text,
.cream:nth-child(2):checked ~ ul.conten li:nth-child(2) > div.text,
.cream:nth-child(3):checked ~ ul.conten li:nth-child(3) > div.text,
.cream:nth-child(4):checked ~ ul.conten li:nth-child(4) > div.text,
.cream:nth-child(5):checked ~ ul.conten li:nth-child(5) > div.text,
.cream:nth-child(6):checked ~ ul.conten li:nth-child(6) > div.text {
   opacity:0.966;
   top: 0;
   -webkit-transition: all 0.6s ease-out 0.4s;
   -moz-transition: all 0.6s ease-out 0.4s;
   -o-transition: all 0.6s ease-out 0.4s;
   transition: all 0.6s ease-out 0.4s;
}

div.cream-slider ul.conten li > label {
   cursor: pointer;
   display: block;
   left: 0;
   position: absolute;
   top: 0;
   width: 100%;
   z-index: 100;
}
div.cream-slider ul.conten li div.text {
   color: #fff;
   left: 0;
   padding: 46px 0 0 50px;
   position: absolute;
   text-shadow: 0 1px 1px rgba(0, 0, 0, 0.2), 0 1px 5px rgba(0, 0, 0, 0.05);
   top: 30px;
   z-index: 50;
   min-width: 400px;
   opacity:0;
}


div.cream-slider ul.conten li div.text h3 {
   background: #fff;
   font-size: 1.4em;
   font-weight: normal;
   position: relative;
   color: #555;
}

div.cream-slider ul.conten li div.text span {
   background: #555;
   font-size: 0.9em;
   color: #fff;
}
div.text span, div.text h3 {
   box-shadow: 0 1px 2px 0 rgba(0, 0, 0, 0.3);
   display: table;
   padding: 10px 16px;
   -webkit-font-smoothing: antialiased;
}

div.cream-slider ul.conten li div.text a {color: #0ab4d8;}
div.cream-slider ul.conten li div.text a:hover {text-decoration: none;color: #29d3f7;}

