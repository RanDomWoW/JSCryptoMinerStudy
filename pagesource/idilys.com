    $('#forgotPassword').on('click', function (e) {
        if ($.trim($("#email").val()).length < 6) {
            return false;
        }
        e.preventDefault();
        $.ajax({
            url: "/ajax/retrievePassword",
            method: "POST",
            type: "json",
            data: "email=" + $("#email").val(),
            success: function (data) {
                var json = JSON.parse(data);

                if (1 == json.success){
                    $('#forgot-box').hide();
                    $('#forgot-box-response').show();
                    $('#forgot-box-response-body').html(json.message);
                }
                else {
                    /*$('#lblEmail').html(json.message);*/
                    $('#forgot-box').hide();
                    $('#forgot-box-response').show();
                    $('#forgot-box-response-body').html(json.message);
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {
                console.log(jqXHR, textStatus, errorThrown);
            }
        });
    });

    $("#close-btn-end").on('click', function () {
        $('#forgot-box-response').hide();
        $('#overlay').hide();
    });

