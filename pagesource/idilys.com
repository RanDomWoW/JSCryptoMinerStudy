        $(".sexSelector").on('click', '.sexChoice', function () {
            var sexChoiced = $(this).data('sex');
            // remove current selected
            var currentSex = $(".sexSelector").find('.active');
            currentSex.removeClass('active');
            currentSex.removeClass('bkdColor3');
            currentSex.removeClass('color2');
            // active new one
