        });


        $("#formRegister_regular").on("submit", function () {
            fieldsValidator = [];
            fieldsValidator.push(emailValidator, birthDayValidator, zipCodeValidator);


            var params = $(this).serialize().split("&");
            var i = params.length;
            while (i--) {
                var pair = decodeURIComponent(params[i]).split("=");
                jQuery("[name=\"" + pair[0].replace("?", "") + "\"]", $("#form_full")).each(function () {
                    if (jQuery(this).is("[type=\"radio\"], [type=\"checkbox\"]")) {
                        checked = (pair[1] === jQuery(this).val());
                        jQuery(this).attr("checked", checked).prop("checked", checked);
                    } else {
                        jQuery(this).val(pair[1]);
                    }
                });
            }


            if (Validator.checkAll()) {
                $("#form_full").trigger("submit");
            }
            return false;


        });




