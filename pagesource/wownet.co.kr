<script language="javascript" type="text/javascript">
    function login()
    {
        if(document.getElementById("userid").value == "")
        {
            alert("아이디를 확인하세요.")
            document.getElementById("userid").focus();
            return false;
        }
        if(document.getElementById("password").value == "")
        {
            alert("비밀번호를 확인하세요.")
            document.getElementById("password").focus();
            return false;
        }

        
	        document.getElementById("returl").value = "http://www.wownet.co.kr/main.asp";
        

        document.loginform.submit();
    }
    function chgClassName(id, clsName)
    {
        document.getElementById(id).className = clsName;
    }
    
    function jsEnterLogin(val)
    {
        if(val.keyCode == 13)  login();
    }
    
    //아이디 저장 스크립트


    function setCookies (name, value, expires, domain) {
        var domain = "wownet.co.kr"
        document.cookie = name + "=" + escape (value) +
			"; path=/; expires=" + expires.toGMTString() + ";" + "domain=" + domain ;
    }

    function getCookies(Name) {
        var search = Name + "="
        if (document.cookie.length > 0) { // 쿠키가 설정되어 있다면
            offset = document.cookie.indexOf(search)
            if (offset != -1) { // 쿠키가 존재하면
                document.getElementById("userid").focus(); //쿠키가 있으면 포커스를 USERID로 이동
                offset += search.length
                // set index of beginning of value
                end = document.cookie.indexOf(";", offset)
                // 쿠키 값의 마지막 위치 인덱스 번호 설정
                if (end == -1)
                    end = document.cookie.length
                return unescape(document.cookie.substring(offset, end))
            }
        }
        return "";
    }
    function saveid(form) {
        var expdate = new Date();
        // 기본적으로 14일동안 기억하게 함. 
        if (form.checksaveid.checked)
            expdate.setTime(expdate.getTime() + 1000 * 3600 * 24 * 14); // 14일
        else
            expdate.setTime(expdate.getTime() - 1); // 쿠키 삭제조건
        setCookies("saveid", form.userid.value, expdate, "wownet.co.kr");

    }

    function getid(form) {
        form.checksaveid.checked = ((form.userid.value = getCookies("saveid")) != "");
    }		
</script>

