    <script>
    var initialUrl = document.URL;

    //Paginate action
    $(document).on('click', 'ul.pagination a', function() {
        var url = $(this).attr('href');


        makeRequest(url);

        if(history.pushState) {
            var stateObj = { path: url };
            history.pushState(stateObj, null, url);
        }

        window.url = url;
        var page = App.getPage();
        var initialTitle = App.getRawTitle();
        if (page > 1) {
            document.title = initialTitle + '-' + page;
        }
        else {
            document.title = initialTitle;
        }

        return false;
    });

    //IMDB rating slider init
    $(document).ready(function() {
        $( "#sliderImdbRating" ).slider({
            range: true,
            min: 1,
            max: 10,
            step: 0.1,
            values: [$("input[name='filter[imdbRating][min]']").val(),$("input[name='filter[imdbRating][max]']").val()],
            slide: function(event, ui) {
                for (var i = 0; i < ui.values.length; ++i) {
                    $("input.sliderValuefilterImdbRating[data-index=" + i + "]").val(ui.values[i]);
                }
            },
            change: function( event, ui ) {
                for (var i = 0; i < ui.values.length; ++i) {
                    $("input.sliderValuefilterImdbRating[data-index=" + i + "]").val(ui.values[i]);
                }
                $("input[name='filter[imdbRating][min]']").val($("input.sliderValuefilterImdbRating[data-index=0]").val());
                $("input[name='filter[imdbRating][max]']").val($("input.sliderValuefilterImdbRating[data-index=1]").val());
                waitForFinalEvent(applyNewFilters, 500, "filter");
            }
        });
        $("input.sliderValuefilterImdbRating").on('change', function() {
            var $this = $(this);
            var value = Number($this.val());
            value = isNaN(value) ?  $("#sliderImdbRating").slider("values", $this.data("index")) : value;

            if (($this.data("index") == 0) && ($("#sliderImdbRating").slider("values", 1) < value)) {
                value = $("#sliderImdbRating").slider("values", 1);
            }

            if (($this.data("index") == 1) && ($("#sliderImdbRating").slider("values", 0) > value)) {
                value = $("#sliderImdbRating").slider("values", 0);
            }

            $("#sliderImdbRating").slider("values", $this.data("index"), value);
        });
    });

    //Sorting links init
    $(document).on('click', 'a.sort', function() {
        var oldSort = $('label[for=sort-action].active a.sort');
        $('label[for=sort-action].active b.order-flag').removeClass('sort-desc');
        $('label[for=sort-action].active b.order-flag').removeClass('sort-asc');
        $('label[for=sort-action].active').removeClass('active');
        $(this).parent('label').addClass('active');
        var sort = $(this).attr('data-sort'),
                direction = $(this).attr('data-direction'),
                default_direction = $(this).attr('data-default-direction');
        if (typeof default_direction == "undefined") {
            default_direction = 'desc';
        }
        if (typeof direction == "undefined") {
            direction = default_direction;
        } else if (direction == 'asc') {
            direction = 'desc';
        } else {
            direction = 'asc';
        }
        oldSort.removeAttr('data-direction');
        $('label[for=sort-action].active b.order-flag').addClass('sort-'+direction);
        $(this).attr('data-direction', direction);
        $('input[name="filter[sort]"]').val(sort);
        $('input[name="filter[sortDirection]"]').val(direction);
        waitForFinalEvent(applyNewFilters, 500, "filter");

        return false;
    });

    //CheckBox filters init
    $(document).on('change', 'input.filter-action', function() {
        var genres = getFiltersByClassName('genres-filter');
        var qualities = getFiltersByClassName('qualities-filter');
        $('input[name="filter[genre]"]').val(genres.join ( ',' ));
        $('input[name="filter[quality]"]').val(qualities.join ( ',' ));
        waitForFinalEvent(applyNewFilters, 500, "filter");
        return false;
    });

    // Clear filters link init
    $(document).on('click', '.browse-filters-reset.is-enabled', function () {
        $('input.filter-action').attr('checked', false);
        $('input.filter-action:first').trigger('change');
        $("#sliderImdbRating").slider("option", "values", [1,10]);
        waitForFinalEvent(applyNewFilters, 500, "filter");
    });

    //Browser navigation init
    if(window.addEventListener) {
        window.addEventListener('popstate', function(e){
            if(e.state) {
                makeRequest(e.state.path);
            } else {
                makeRequest(initialUrl);
            }
            activateFiltersByNames('filter[genre]', 'genres-filter');
            activateFiltersByNames('filter[quality]', 'qualities-filter');
            activateSort();
            activateSearch();
            var ratingMin = Number(getParameterByName("filter[imdbRating][min]")),
                    ratingMax = Number(getParameterByName("filter[imdbRating][max]"));
            ratingMin = isNaN(ratingMin) ?  0 : ratingMin;
            ratingMax = isNaN(ratingMax) ?  10 : ratingMax;
            $("#sliderImdbRating").slider("option", "values", [ratingMin,ratingMax]);
            if (
                    ($('input[name="filter[genre]"]').val() != "")
                    || ($('input[name="filter[quality]"]').val() != "")
                    || ($('input[name="filter[imdbRating][min]"]').val() != 1)
                    || ($('input[name="filter[imdbRating][max]"]').val() != 10)
            ) {
                $('.browse-filters-reset').addClass('is-enabled');
            } else {
                $('.browse-filters-reset').removeClass('is-enabled');
            }

            //тут обробатывается событие навигации по истории
        }, false);
    }

    //Apply new filters action
    function applyNewFilters() {
        var url = '';
        if (typeof location.origin === 'undefined')
            url = location.protocol + "//" + location.host + location.pathname;
        else
            url = location.origin + location.pathname;
        if (
                ($('input[name="filter[genre]"]').val() != "")
                || ($('input[name="filter[imdbRating][min]"]').val() != 1)
                || ($('input[name="filter[imdbRating][max]"]').val() != 10)
        ) {
            $('.browse-filters-reset').addClass('is-enabled');
        } else {
            $('.browse-filters-reset').removeClass('is-enabled');
        }

        var params = [];

        var filter_genre            = $("form[name=filter] input[name=filter\\[genre\\]]").val();
        var filter_quality          = $("form[name=filter] input[name=filter\\[quality\\]]").val();
        var filter_imdbRatingMin    = $("form[name=filter] input[name=filter\\[imdbRating\\]\\[min\\]]").val();
        var filter_imdbRatingMax    = $("form[name=filter] input[name=filter\\[imdbRating\\]\\[max\\]]").val();
        var filter_all              = $("form[name=filter] input[name=filter\\[all\\]]").val();
        var filter_actorName        = $("form[name=filter] input[name=filter\\[actorName\\]]").val();
        var filter_directorName     = $("form[name=filter] input[name=filter\\[directorName\\]]").val();
        var filter_name             = $("form[name=filter] input[name=filter\\[name\\]]").val();
        var filter_series           = $("form[name=filter] input[name=filter\\[series\\]]").val();
        var filter_releaseYear      = $("form[name=filter] input[name=filter\\[releaseYear\\]]").val();
        var filter_sort             = $("form[name=filter] input[name=filter\\[sort\\]]").val();
        var filter_sortDirection    = $("form[name=filter] input[name=filter\\[sortDirection\\]]").val();

        if (filter_genre            != '') params.push('filter[genre]='             + filter_genre);
        if (filter_quality          != '') params.push('filter[quality]='           + filter_quality);
        if (filter_imdbRatingMin    != '1') params.push('filter[imdbRating][min]='  + filter_imdbRatingMin);
        if (filter_imdbRatingMax    != '10') params.push('filter[imdbRating][max]=' + filter_imdbRatingMax);
        if (filter_all              != '') params.push('filter[all]='               + filter_all);
        if (filter_actorName        != '') params.push('filter[actorName]='         + filter_actorName);
        if (filter_directorName     != '') params.push('filter[directorName]='      + filter_directorName);
        if (filter_name             != '') params.push('filter[name]='              + filter_name);
        if (filter_series           != '') params.push('filter[series]='            + filter_series);
        if (filter_releaseYear      != '') params.push('filter[releaseYear]='       + filter_releaseYear);
        if (filter_sort             != '3') params.push('filter[sort]='             + filter_sort);
        if (filter_sortDirection    != 'desc') params.push('filter[sortDirection]=' + filter_sortDirection);

        if (params.length > 0) {
            params = '?' + implode('&', params);
            url += encodeURI(params);
        }

        makeRequest(url);

        if(history.pushState) {
            var stateObj = { path: url };
            history.pushState(stateObj, null, url);
        }

        window.url = url;
    }

    function implode( glue, pieces ) {
        return ( ( pieces instanceof Array ) ? pieces.join ( glue ) : pieces );
    }

    //Make ajax request tut server
    function makeRequest(url) {
        $.ajax({
            url: url,
            cache: false,
            success: function(result) {
                $('div#filmsListContainer').replaceWith(result.films);
                $('html, body').animate({scrollTop: 0});
                App.height_name_ar();
                if($('.filter_film').is('.filter_film_hide')){
                    $('#filmsListContainer').addClass('width100');
                }
                App.setNthFilmNoMargin();
                App.refreshBanners();
            },
            beforeSend: function () {
                $("#ajaxPreload").css("display", "block");
            },
            complete: function () {
                $("#ajaxPreload").css("display", "none");
            }
        });
    }

    //Get checkbox filters by class name
    function getFiltersByClassName(className) {
        var filters = [];
        $('input.' + className + ':checked').each(
                function () {
                    filters.push($(this).val());
                }
        );
        return filters;
    }

    //Activate checkbox filters by name
    function activateFiltersByNames(paramName, className) {
        var param = getParameterByName(paramName);
        var filters = param.split(',');
        $('input.' + className).each(
                function () {
                    if (filters.indexOf($(this).val()) > -1) {
                        $(this).prop('checked', 'checked');
                    } else {
                        $(this).prop('checked', false);
                    }
                }
        );
        $('input[name='+paramName+']').val(param);
    }

    //Activate sorting in filter form
    function activateSort() {
        var sort = getParameterByName('filter[sort]');
        var direction = getParameterByName('filter[sortDirection]');
        if (sort == "") {
            sort = 0;
            direction = 'desc';
        }
        var oldSort = $('label[for=sort-action].active a.sort');
        var correctSort = $('label[for=sort-action] a.sort[data-sort='+sort+']');
        $('label[for=sort-action].active b.order-flag').removeClass('sort-desc');
        $('label[for=sort-action].active b.order-flag').removeClass('sort-asc');
        $('label[for=sort-action].active').removeClass('active');
        correctSort.parent('label').addClass('active');
        var default_direction = $(this).attr('data-default-direction');
        if (typeof default_direction == "undefined") {
            default_direction = 'desc';
        }
        if (typeof direction == "undefined") {
            direction = default_direction;
        } else if (direction == 'asc') {
            direction = 'desc';
        } else {
            direction = 'asc';
        }
        oldSort.removeAttr('data-direction');
        $('label[for=sort-action].active b.order-flag').addClass('sort-'+direction);
        correctSort.attr('data-direction', direction);
        $('input[name="filter[sort]"]').val(sort);
        $('input[name="filter[sortDirection]"]').val(direction);
    }

    //Set search fields
    function activateSearch() {
        var all = getParameterByName('filter[all]');
        var name = getParameterByName('filter[name]');
        var series = getParameterByName('filter[series]');
        var actorName = getParameterByName('filter[actorName]');
        var directorName = getParameterByName('filter[directorName]');
        if (all != "") {
            $('span#filterCurrentType').text("All");
            $('input#filterSearchField')
                    .attr("name", "filter[all]")
                    .val(all);
        }
        if (name != "") {
            $('span#filterCurrentType').text("Movie name");
            $('input#filterSearchField')
                    .attr("name", "filter[name]")
                    .val(name);
        }
        if (series != "") {
            $('span#filterCurrentType').text("Series name");
            $('input#filterSearchField')
                    .attr("series", "filter[series]")
                    .val(name);
        }
        if (actorName != "") {
            $('span#filterCurrentType').text("Actor");
            $('input#filterSearchField')
                    .attr("name", "filter[actorName]")
                    .val(actorName);
        }
        if (directorName != "") {
            $('span#filterCurrentType').text("Director");
            $('input#filterSearchField')
                    .attr("name", "filter[directorName]")
                    .val(directorName);
        }
        $('input[name="filter[all]"]').val(all);
        $('input[name="filter[name]"]').val(name);
        $('input[name="filter[series]"]').val(series);
        $('input[name="filter[actorName]"]').val(actorName);
        $('input[name="filter[directorName]"]').val(directorName);
    }

    //Get parameter from url by name
    function getParameterByName(name) {
        name = encodeURI(name);
        name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
        var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                results = regex.exec(location.search);
        return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
    }

    //Don't send request until event is not end
    waitForFinalEvent = (function () {
        var timers = {};
        return function (callback, ms, uniqueId) {
            if (!uniqueId) {
                uniqueId = "321";
            }
            if (timers[uniqueId]) {
                clearTimeout(timers[uniqueId]);
            }
            timers[uniqueId] = setTimeout(callback, ms);
        };
    })();
</script>
