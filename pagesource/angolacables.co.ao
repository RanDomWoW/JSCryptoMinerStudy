<style id='style-inline-css' type='text/css'>
/*breadcrumbs background color*/
.page-title .breadcrumbs {
background-color: rgba(0, 0, 0, 0.05);
}

h2 {
color: #ffffff !important;
}

h3 {
color: #27c8e3 !important;
}
div[id^=google-maps-builder] .gmb-infobubble p.place-title {
    color: #28c9e5;
    font-family: Roboto,Arial,sans-serif;
    font-size: 24px;
    line-height: 21px;
    margin-bottom: 0;
    font-weight: 700;
    min-height: 21px;
    white-space: normal;
}

.mapdiv:before {
content: none;
display: none;
}

.mapdiv.vc_row:before {
    content: none;
    display: none;
}

.entry-meta, .portfolio-categories {
    display: none;
    overflow: hidden;
}

.masthead .soc-ico a .icon, .dt-mobile-header .soc-ico a .icon a.facebook {
    fill: #823838;
}

.soc-ico >a.facebook {
    box-shadow: inset 0px 0px 0px 1px  #3b5998 !important;
}

.soc-ico >a.facebook svg {
    fill: #3b5998 !important;
}

.soc-ico >a.instagram {
    box-shadow: inset 0px 0px 0px 1px #e95950 !important;
}

.soc-ico >a.instagram svg {
    fill:  #e95950 !important;
}

.soc-ico >a.linkedin {
    box-shadow: inset 0px 0px 0px 1px #007bb5 !important;
}

.soc-ico >a.linkedin svg {
    fill:  #007bb5 !important;
}

.soc-ico >a.facebook svg.icon:hover {
    fill: #294fa0 !important;
}

.soc-ico >a.instagram svg.icon:hover {
    fill: #e22e23 !important;
}

.soc-ico >a.linkedin svg.icon:hover {
    fill: #00486b !important;
}

.vc_tta-color-grey.vc_tta-style-classic .vc_tta-panel.vc_active .vc_tta-panel-title>a {
    color: #ffffff;
    background-color: #022161;
}

.vc_tta-color-grey.vc_tta-style-classic .vc_tta-panel .vc_tta-panel-title>a {
    color: #ffffff;
    background-color: #27c8e3;
}

.vc_tta-color-grey.vc_tta-style-classic .vc_tta-panel .vc_tta-panel-title>a:hover {
    color: #ffffff;
    background-color: #022161;
}

.vc_tta-color-grey.vc_tta-style-classic .vc_tta-panel.vc_active .vc_tta-panel-title>a {
    color: #fff;
}

.vc_tta-color-grey.vc_tta-style-classic .vc_tta-panel .vc_tta-panel-title>a {
    color: #ffffff !important;
    text-transform: uppercase;
    font-size: 12px !important;
}

.contact-form form {
background-color: #27c8e3;
padding: 30px;
border-radius: 4px;
font-size: 0.8em;
}

.stripe .wpcf7-form-control-wrap, .stripe .wpcf7-form-control-wrap * {
    color: #000000;
}

.stripe-style-1 * {
    color: #000000;
}

.product-card {
border: 1px solid #e6e6e6;
background-color: #f6f6f6;
margin: 5px;
padding: 30px;
max-width: 580px;
min-height: 900px;
}

.product-card:hover {
box-shadow: 1px 1px 1px 1px #d7d7d7;
}

.product-card h3 {
font-weight: 700;
font-size: 1.8em;
margin-bottom: -20px !important;
}


.product-card p {
font-size: 0.85em;
}

.product-card img {
margin-top: 15px;
}

.news-card .pl-car-item {
    border: 1px solid #e6e6e6;
    background-color: #f6f6f6;
    min-height: 400px;
}

.news-card .pl-car-item:hover {
     box-shadow: 1px 1px 1px 1px #d7d7d7;
}

.news-card .pl-detailcnt .pl-permalink, .pl-mask .pl-permalink {
    background: #27c8e3;
    color: #fff;
    margin-top: 10px;
    padding: 5px 15px;
    border: 1px solid #27c8e3;
    border-radius: 3px;
    text-decoration: none;
    margin-bottom: 10px!important;
}

.news-card .pl-detailcnt .pl-permalink:hover {
    background-color: #022161 !important;
    border: 1px solid #022161 !important;
    padding: 5px 15px !important;
}

.pl-permalink i.fa {
    display: none;
}

/* timeline */


.timelineangolacables  .timeline-header h3 {
    font-size: 26px !important;
    color: #ffffff !important;
    font-weight: bolder !important;
    line-height: 1.5em;
    margin: 0 0 7px;
    display: block;
}


.timelineangolacables .timeline-header-block .timeline-header {
    font-size: 15px !important;
    color: #fff !important;
    line-height: 1.2 !important;
        display: block;
}

.timelineangolacables.timeline-post-left .ult-timeline-arrow l {
    border-left-color: rgb(39, 200, 227) !important;
}


.timelineangolacables.timeline-post-right .ult-timeline-arrow l {
    border-right-color: rgb(39, 200, 227) !important;
}

.timeline-icon-block:nth-child(odd) {
    position: absolute !important;
    left: auto !important;
    right: -96px !important;
}

.timeline-icon-block:nth-child(even) {
    position: absolute!important;
    left: -88px !important;
    right: auto !important;
}

.timeline-block .timeline-header-block:nth-child(even) {
    padding-right: 90px !important;
    padding-left: 0 !important;
}

.timeline-block .timeline-header-block:nth-child(odd) {
    padding-right: 0px !important;
    padding-left: 90px !important;
}

.timeline-post-left .timeline-header{
    text-align: left !important;
}

.timeline-post-right .timeline-header{
    text-align: right !important;
}

.timeline-block {
    border-color: transparent !important;
}

.ult-timeline-arrow {
    display: none;
}

.timeline-line {
    border-width: 10px !important;
    top: -100px;
    height: calc(100% + 200px) !important;
}

.timeline-dot {
    display: none !important;
}

.timeline-line:after {
    content: '';
    position: absolute;
    border-radius: 50%;
    border-width: 10px;
    border-color: #fff;
    border-style: solid;
    height: 18px;
    bottom: -35px;
    display: block;
    width: 18px;
    right: -23px;
}

.timeline-line:before {
    content: '';
    position: absolute;
    border-radius: 50%;
    border-width: 10px;
    border-color: #fff;
    border-style: solid;
    height: 18px;
    top: -35px;
    display: block;
    width: 18px;
    right: -23px;
}

.timeline-icon-block:nth-child(odd):before {
    content: '';
    width: 30px;
    height: 3px;
    position: absolute;
    top: 71px;
    left: -30px;
    background: #fff;
}

.timeline-icon-block:nth-child(even):before {
    content: '';
    width: 30px;
    height: 3px;
    position: absolute;
    top: 77px;
    right: -29px;
    background: #fff;
}

.timeline-icon-block:nth-child(even):after {
    content: '';
    width: 10px;
    height: 10px;
    position: absolute;
    top: 73px;
    right: -38px;
    background: #fff;
    border-radius: 50%;
}

.timeline-icon-block:nth-child(odd):after {
    content: '';
    width: 10px;
    height: 10px;
    position: absolute;
    top: 68px;
    left: -38px;
    background: #fff;
    border-radius: 50%;
}

.timeline-header-block {
    min-height: 160px;
}
.timeline-wrapper {
    margin: 100px 0 150px 0 !important;
}

@media (max-width: 767px) {
    .ult-timeline-icon { display: none; }
    .timeline-line { display: none; }
    .timeline-icon-block:nth-child(odd):after { display: none }
    .timeline-icon-block:nth-child(even):after { display: none }
    .timeline-icon-block:nth-child(odd):before{ display: none }
    .timeline-icon-block:nth-child(even):before { display: none }
    .timeline-wrapper { margin: 0 !important }
}

.timeline-block .timeline-header-block .timeline-header p, .timeline-feature-item .timeline-header-block .timeline-header p {
     font-size: 1.1em !important;
}

/* blog details and pagination */

.details {
    display: none;
}

.paginator .nav-prev, .paginator .nav-next {
    display: none;
}

/* home news highlights */

.pl-thumbcnt img {
    width: 100%;
    height: 180px !important;
}


.footer .widget-title {
    color: #113264 !important;
}

.mfp-title {
    display: none !important;
}

.wpb-js-composer .vc_tta-color-turquoise.vc_tta-style-flat .vc_tta-panel.vc_active .vc_tta-panel-heading {
    background-color: #27c8e3 !important;
}

.wpb-js-composer .vc_tta-color-turquoise.vc_tta-style-flat .vc_tta-panel .vc_tta-panel-heading {
    background-color: #29a0c0 !important;
}

/* end */  
</style>
