			setTimeout(function () {
				jQuery("#menu2 #menu2_sliders > li:eq(" + index + "):visible").not('.open').slideUp('fast', function () {
					li.removeClass('active');
				});
			}, 200);

		}
	);
	jQuery("#menu2 #menu2_sliders > li").hover(
