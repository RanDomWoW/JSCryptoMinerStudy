<script type="text/javascript">
            function getCookie(cname) {
                var name = cname + "=";
                var ca = document.cookie.split(';');
                for(var i = 0; i <ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0)==' ') {
                        c = c.substring(1);
                    }
                    if (c.indexOf(name) == 0) {
                        return c.substring(name.length,c.length);
                    }
                }
                return "";
            }
            function isMobile() { 
                if( navigator.userAgent.match(/Android/i)
                    || navigator.userAgent.match(/webOS/i)
                    || navigator.userAgent.match(/iPhone/i)
                    // || navigator.userAgent.match(/iPad/i)
                    || navigator.userAgent.match(/iPod/i)
                    || navigator.userAgent.match(/BlackBerry/i)
                    || navigator.userAgent.match(/Windows Phone/i)
                ){
                    return true;
                } else {
                    return false;
                }
            }

            if ( ! getCookie( 'cineflix_mobile_web_version_view' ) && isMobile() ) {
                var url = 'https://www.cineflix.com.br/m/index.html';
                var body = jQuery( 'body' );

                if ( body.hasClass( 'single-cinema' ) || body.hasClass( 'home' ) ) {
                    if ( body.hasClass( 'single-cinema' ) ) {
                        var regex = /postid-(\d+)/g;
                        var id = regex.exec( body.attr( 'class' ) );
                        console.log( id );
                        url = url.concat( '?cinema=' + id[1] );
                    }
                    if ( url ) {
                        window.location = url;
                    }
                }
            }
        </script>
