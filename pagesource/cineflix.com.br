<script type="text/javascript">
	function update_movie(id){
		jQuery("#movies_future_movie_html").slideUp('fast', function(){
			jQuery.ajax(
				"http://www.cineflix.com.br/wp-admin/admin-ajax.php",
				{
					data: {action: 'cflix_movie_load', 
							'movieid': id, 
							'nonce': "ab92b2eb7b"},
					dataType: 'html',
					type: 'POST',
					success: function(htmlresponse){
						jQuery("#movies_future_movie_html").html(htmlresponse).delay('500').slideDown('fast');
					}
				}
			);
		});
	}

	jQuery(document).ready(function(){

		jQuery("#movies_future li div[movieid]").click(function(){
			var movie = jQuery(this);
			if(movie.hasClass('current'))
				return;
			
			jQuery("#movies_future .current").removeClass('current');
			
			update_movie(movie.attr('movieid'));
			movie.addClass('current');
		});

		
		
	

	});


