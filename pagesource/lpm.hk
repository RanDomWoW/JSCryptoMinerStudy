<script>
    jQuery(function ($) {
        $('button.btn-cart').attr('onclick', '');
        $('button.btn-cart').click(function (ev) {
            ev.preventDefault();
            var form = $(this).closest('form');
            var formAction = form.attr('action');
            var matches = formAction.match(/product\/([0-9]+)\/form_key\/(.*)\//);

            if (matches) {
                var productId = matches[1];
                var formKey = matches[2];
                var qty = form.find('#qty').val() || 1;

                if (form.length) {
                    $.ajax({
                        url: 'https://www.lpm.hk/addtocartpopup/index/addToCart/',
                        dataType: 'JSON',
                        method: 'POST',
                        data: {product: productId, form_key: formKey, qty: qty},
                        success: function (data) {
                            if (data.status === 'success') {
                                $('.js-cart-popup').html(data['block_html']);

                                $.fancybox({
                                    type: 'inline',
                                    href: '.js-cart-popup',
                                    wrapCSS: 'fancybox-cart-wrap',
                                    padding: 0,
                                    margin: 0
                                });
                            } else {
                                alert(data.error_message);
                            }
                            console.log(data);
                        },
                        error: function () {
                            console.log('Connection error');
                        }
                    });
                } else {
                    console.log('Form not found');
                }
            }
        });

        $(document).on('click', '.js-add-to-cart-popup-continue', function (ev) {
            ev.preventDefault();
            $.fancybox.close();
        });

        $(document).on('click', '.js-add-to-cart-popup-add', function (ev) {
            ev.preventDefault();
            var addValue = parseInt($(this).attr('data-add-value'));
            var curValue = parseInt($('.js-add-to-cart-popup-qty').val());
            var newValue = curValue + addValue;
            if (newValue <= 0) {
                newValue = 1;
            }
            $('.js-add-to-cart-popup-qty').val(newValue);
        });

        $(document).on('click', '.js-add-to-cart-popup-update-qty', function (ev) {
            var productId = $(this).attr('data-product-id');
            var quoteItemId = $(this).attr('data-quote-item-id');
            var formKey = $(this).attr('data-form-key');
            var qty = parseInt($('.js-add-to-cart-popup-qty').val());

            var data = {update_cart_action: 'update_qty', form_key: formKey, cart: {}};
            data['cart'][quoteItemId] = {qty: qty};

            $.ajax({
                url: 'https://www.lpm.hk/addtocartpopup/index/updateQty',
                dataType: 'JSON',
                method: 'POST',
                data: {product: productId, form_key: formKey, qty: qty},
                success: function (data) {
                    if (data.status === 'success') {
                        if (data.method) {
                            $.ajax({
                                url: 'https://www.lpm.hk/lmp/checkout/savemethod',
                                data: {pmethod: data.method},
                                method: 'POST',
                                success: function () {
                                    showProductCartInfo(productId, formKey);
                                }
                            });
                        } else {
                            showProductCartInfo(productId, formKey);
                        }
                    } else {
                        alert(data.error_message);
                    }
                    console.log(data);
                },
                error: function () {
                    console.log('Connection error');
                }
            });
        });

        function showProductCartInfo(productId, formKey) {
            $.ajax({
                url: 'https://www.lpm.hk/addtocartpopup/index/getProductCartInfo',
                dataType: 'JSON',
                method: 'POST',
                data: {product: productId, form_key: formKey},
                success: function (data) {
                    if (data.status === 'success') {
                        $('.js-cart-popup').html(data['block_html']);

                        $.fancybox({
                            type: 'inline',
                            href: '.js-cart-popup',
                            padding: 0,
                            margin: 0
                        });
                    } else {
                        alert(data.error_message);
                    }
                    console.log(data);
                },
                error: function () {
                    console.log('Connection error');
                }
            })
        }
    });
</script>
