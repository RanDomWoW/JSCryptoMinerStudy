<script>
    jQuery(function($) {
        var updateMetalPricesTimestamp = null;
        function updatePrices(isForcibly) {
            isForcibly = isForcibly || false;
            $.ajax({
                //url: '/live-market-price/json/prices_rest_api.json',
                url: '/live-market-price/get-prices-long.php?nocache=' + Math.random() + (isForcibly ? '&f=1' : ''),
                data: {currency_code: 'USD', timestamp: updateMetalPricesTimestamp},
                dataType: 'JSON',
                success: function(data) {
                    if (data.metals != 'undefined') {
                        for (var metal in data.metals) {
                            $('.js-metal-price[data-metal="' + metal + '"]').each(function() {
                                if (data.metals[metal][$(this).attr('data-price-type') + 'PriceFormatted'] != 'undefined') {
                                    $(this).html(data.metals[metal][$(this).attr('data-price-type') + 'PriceFormatted']);
                                }
                                if ($(this).attr('data-price-type') == 'askChange') {
                                    $(this).removeClass('up').removeClass('down');

                                    var changeValue = parseFloat(data.metals[metal][$(this).attr('data-price-type') + 'Price']);
                                    changeValue = Math.round(changeValue * 100) / 100;
//                                    console.log(changeValue);
                                    if (changeValue < 0) {
                                        $(this).addClass('down');
                                    } else if (changeValue > 0) {
                                        $(this).addClass('up');
                                    }
                                }
                            });
                        }

                        if (typeof data.time.Hong_Kong != 'undefined') {
                            $('.js-metal-price-last-updated').html(data.time.Hong_Kong);
                        }

                        if (typeof data.fileTimestamp != 'undefined') {
                            updateMetalPricesTimestamp = data.fileTimestamp;
                        }
                    }
                },
                error: function() {
                    console.log('Error');
                },
                complete: function() {
                    updatePrices();
                }
            });
        }

        updatePrices(true);
    });
</script><script>
    jQuery(function ($) {
        var productTimestamps = [];

        function isProductExistsInAr(ar, id) {
            for (var i = 0; i < ar.length; i++) {
                if (ar[i].id == id) {
                    return true;
                }
            }
            false;
        }

        function updatePrices(isForcibly) {
            isForcibly = isForcibly || false;

            var products = [];
            $('.price').each(function () {
                var id = parseInt($(this).attr('data-product-id'));
//                var id = $(this).attr('id');

                if (id && !isProductExistsInAr(products, id)) {
                    products.push({id: id, timestamp: productTimestamps[id]});
//                    var found = id.match(/^product-minimal-price-([0-9]+)$/)
//                    if (found) {
//                    productIds.push(found[1]);
//                    }
                }
            });
//            console.log(products);
//            return;
            if (products.length) {
                $.ajax({
                    url: '/live-market-price/get-products-prices-long.php?nocache=' + Math.random() + (isForcibly ? '&f=1' : ''),
                    dataType: 'JSON',
                    data: {products: products, currency_code: 'HKD', ws:0},
                    success: function (data) {
//                        console.log(data.products.length);
                        for (var i = 0; i < data.products.length; i++) {
                            var productId = data.products[i].id;
                            productTimestamps[productId] = data.products[i].timestamp;

                            //var productPrice = data.products[i].price.wireprice;
                            var productPrice = data.products[i].priceMinimal;
                            var lpmBuyPrice = data.products[i].lpmBuyPrice;
                            $('.price[data-product-id="' + productId + '"]').html(productPrice);
                            $('.price[data-lpm-buy-price-product-id="' + productId + '"]').html(lpmBuyPrice);

                            if (data.products[i].tierprices.length) {
                                for (var j = 0; j < data.products[i].tierprices.length; j++) {
                                    var tierQtyRow = $('[data-tier-product-id="' + productId + '"]').find('#' + data.products[i].tierprices[j].item);
                                    tierQtyRow.find('[data-tier-type="wireprice"]').html(data.products[i].tierprices[j].wireprice);
                                    tierQtyRow.find('[data-tier-type="cardprice"]').html(data.products[i].tierprices[j].cardprice);
                                }
                            }
                        }
                    },
                    error: function () {
                        console.log('Error');
                    },
                    complete: function () {
                        updatePrices();
                    }
                });

            }
        }

        updatePrices(true);
    });
