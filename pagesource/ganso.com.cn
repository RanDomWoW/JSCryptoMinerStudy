<!--滚屏_js-->
<script>
    function slide_int() {
        if ($(window).width() < 992) {
            $(".section").height($(".num").height() - $(".pz_top").height() );
            $(".w_0100_2").height($(".num").height() - $(".pz_top").height() );
            $(".section_banner").height($(".num").height() - $(".pz_top").height());
        }
        if ($(window).width() >= 992) {
            $(function () {
                $('#dowebok').fullpage({
                    afterLoad: function (anchorLink, index) {
                    },
                    'navigation': false,
                });
                $(window).resize(function () {
                    autoScrolling();
                });

                function autoScrolling() {
                    var $ww = $(window).width();
                    if ($ww < 1200) {
                        $.fn.fullpage.setAutoScrolling(false);
                    } else {
                        $.fn.fullpage.setAutoScrolling(true);
                    }
                }

                autoScrolling();
            });
        }
        $(".opa").addClass("opac");
    }
    window.onload = slide_int;
    $(document).ready(function () {
        "use strict";/*进入严格模式标语*/
        $(window).resize(function () {
            slide_int();
        });
    });
</script>
<!--滚屏_js_end-->
