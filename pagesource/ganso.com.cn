<!--元祖文化_js_-->
<script type="text/javascript">
    $(document).ready(function () {

        if ($('.wh_pc').length > 1) {
            $("div[id*='listbox']").hover(
            function () { $(this).animate({ "margin-top": "-16px" }, 200) },
            function () { $(this).animate({ "margin-top": "0px" }, 200); }
        );
            var tmp = $("div[id*='listbox']").eq(0).children("img").attr("src");
            //鼠标点击过程
            $("div[id*='listbox']").click(function () {
                if (parseInt($(this).css("z-index")) <= 2) {
                    var curZindex = parseInt($(this).css("z-index"));
                    //通过z-index差计算该层需要经过几次轮换效果置顶，www.5icool.org
                    var fntimes = 3 - curZindex;
                    //对于当前处于第一位的图片点击无效果.
                    $(document).everyTime(300, function () {
                        $("div[id*='listbox']").each(function () {
                            if (parseInt($(this).css("z-index")) == 3) { $(this).css("z-index", "1"); }
                            else { $(this).css("z-index", "" + (parseInt($(this).css("z-index")) + 1) + ""); }
                            $(this).css("margin-top", "0px");
                            $(this).animate({ "margin-left": ((3 - parseInt($(this).css("z-index"))) * 50) + "px" }, 300);
                        });
                    }, fntimes);
                }
            });
            setInterval(function () {
                $("div[id*='listbox']").each(function () {
                    if (parseInt($(this).css("z-index")) == 3) { $(this).css("z-index", "1"); }
                    else { $(this).css("z-index", "" + (parseInt($(this).css("z-index")) + 1) + ""); }
                    $(this).animate({ "margin-left": ((3 - parseInt($(this).css("z-index"))) * 50) + "px" }, 300);
                });
            }, 3000);
        } else {
            $('#listbox0').css('margin-left', '50px');
        }

    })
</script>
<!--元祖文化_js_end-->
    
