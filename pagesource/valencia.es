<script type="text/javascript">
var duracion=60*24*30;        /*1 mes*/
var ckUtil = new CJL_CookieUtil("cookie_ayto", duracion, "/");
var enlaces=new Array();
enlaces["idioma_1"]="/ayuntamiento/home.nsf/(Portadas1)/$first?opendocument&amp;lang=1";
enlaces["idioma_2"]="/ayuntamiento/home.nsf/(Portadas2)/%24first?opendocument&amp;lang=2";
 var cookieVal = ckUtil.getSubValue("lang");
 if( cookieVal ){
         var cookieUrl = ckUtil.getSubValue("urllang");
         if( cookieUrl )  {
           document.location.href =enlaces[cookieVal];
         } else {
	document.location.href =enlaces["idioma_2"];
         }
 } else {
	document.location.href =enlaces["idioma_2"];
 }
</script>
