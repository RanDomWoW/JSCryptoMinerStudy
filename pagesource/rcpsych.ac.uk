                if (link.indexOf("http") != -1) {
                    window.location = link;
                } else {
                    var protocol = window.location.protocol;
                    var host = window.location.host;
                    window.location = protocol + "//" + host + '/' + link;
                } return false;
            }
        });
    })
</script>
