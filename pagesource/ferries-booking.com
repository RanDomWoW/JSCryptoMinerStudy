function updateport(portcode,list,country){
	var string = portcode;
	var SplitString = string.split('-');
	
	var the_portCode = SplitString[0];
	var the_portName;
	var listnr = list;
	var countrycode = SplitString[1];
	
	if (listnr == 1){
		// clear hidden fields
		jQuery('#to1TextHidden').val('');
		jQuery('#from2TextHidden').val('');
		jQuery('#to2TextHidden').val('');

		// clear form fields
		jQuery('#txtTo').val('');
		jQuery('#txtFromRet').val('');
		jQuery('#txtToRet').val('');

		jQuery('input#from1TextHidden').val(the_portCode);
	
		the_portName = jQuery("#"+string).text();
		jQuery('input#txtFrom').val(the_portName);
		jQuery('input#ccountry').val(countrycode);
		jQuery('#dialog-portsFrom').dialog('close');
	}

	if (listnr == 2) {
		// clear hidden fields
			jQuery('#from2TextHidden').val('');
			jQuery('#to2TextHidden').val('');

			// clear form fields
			jQuery('#txtFromRet').val('');
			jQuery('#txtToRet').val('');

			if (jQuery( "input#ccountry" ).val() == "GR"){
				jQuery( "input#ccountry" ).val(countrycode);
				}

		jQuery('input#to1TextHidden').val(the_portCode);
		
		the_portName = jQuery("#"+string).text();

		jQuery('input#txtTo').val(the_portName);
		jQuery('#dialog-portsTo').dialog('close');

		// get values to and from
		
		var to = jQuery('#txtTo').val();
		var from = jQuery('#txtFrom').val();

		// get hidden values to and from 
		var to_hidden = jQuery('#to1TextHidden').val();
		var from_hidden = jQuery('#from1TextHidden').val();
		
		// update values to and from 
		jQuery('#from2TextHidden').val(to_hidden);

			// check if return is valid for selected route
			jQuery('#to2TextHidden').val(from_hidden);

			// update hidden values to and from 
			jQuery('#txtFromRet').val(to);
			jQuery('#txtToRet').val(from);
	}

	if (listnr == 3) {
		jQuery('#to2TextHidden').val('');
		 
		jQuery('#txtToRet').val('');

		jQuery('input#from2TextHidden').val(the_portCode);
		
		the_portName = jQuery("#"+string).text();
		jQuery('input#txtFromRet').val(the_portName);
		jQuery('#dialog-portsRetFrom').dialog('close');
	}

	if (listnr == 4) {
		jQuery('input#to2TextHidden').val(the_portCode);
		
		the_portName = jQuery("#"+string).text();
		jQuery('input#txtToRet').val(the_portName);
		jQuery('#dialog-portsRetTo').dialog('close');
	}	
}
		
		
function setTriggers() {
	//Make the clicked elements active
	jQuery('.trigger').each(function () {

		jQuery(this).click(function () {
			
			//Add "active" classes to clicked elements
			jQuery(this).next().slideDown(function () {
				jQuery(this).prev().addClass('clicked');
				jQuery(this).addClass('active');
			});
			//Check if it is already active
			if (jQuery(this).attr('class') == 'trigger clicked') {
				jQuery('.active').slideUp(function () {
					jQuery(this).prev().removeClass('clicked');
					jQuery(this).removeClass('active');
				});
			}
			else {
				//Remove "active" classes from current elements.
				jQuery('.active').slideUp(function () {
					jQuery(this).prev().removeClass('clicked');
					jQuery(this).removeClass('active');

				});
			}
		});
	});


	
}
		
