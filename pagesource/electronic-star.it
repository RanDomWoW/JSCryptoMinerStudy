        var arrwish = itemsWishlist.split(",");
        var i;

        $(".actions").children("a").each(function (t) {
            attr = $(this).attr("onclick");
            if (typeof attr !== typeof undefined && attr !== false) {
               $(this).children("span").removeClass();
               $(this).children("span").addClass("icon-estar_icons_wishlist");
            }
        });

        for (i = 0; i < arrwish.length; i++) { 
            if (arrwish[i] != "") {
                $(".actions").children("a").each(function (t) {
                    attr = $(this).attr("onclick");
                    if (typeof attr !== typeof undefined && attr !== false) {
                        if (attr.indexOf(arrwish[i]) >= 0) {
                            $(this).children("span").removeClass("icon-estar_icons_wishlist");
                            $(this).children("span").addClass("icon-estar_icons_check");
                        }
                    }
                });
            }
        }
    });
