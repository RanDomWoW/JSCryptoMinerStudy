<script type="text/javascript">
$(window).load(function(){

	if ($("#close-suggest").length){
		var body = document.body, html = document.documentElement;

		var height = Math.max( body.scrollHeight, body.offsetHeight, html.clientHeight, html.scrollHeight, html.offsetHeight );
		$(window).scroll(function(){
			var windowTop = $(window).scrollTop();
			
			if (windowTop * 2 >= height) {
				$("#article-suggestion").css("display","block");
			} else {
				$("#article-suggestion").css("display","none");
			}
		});
	
		$("#close-suggest").click(function(event){
			$(event.currentTarget).parent().parent().remove();
		});
	}

    

    $("#hidemobilemenu").hide();
    $("#showmobilemenu").click(function() {
        $("#container").css('position','fixed');
        $("#container").animate({right: "220"}, 'fast', function() {});
        $("#showmobilemenu").hide();
        $("#hidemobilemenu").show();
    });
    $("#hidemobilemenu").click(function() {
        $("#container").animate({right: "0"}, 'fast', function() {});
        $("#container").css('position','absolute');
        $("#showmobilemenu").show();
        $("#hidemobilemenu").hide();
    });  

		if (!!$('.allrightfeatured').offset()) { // make sure ".sticky" element exists
			var stickyTop = $('.allrightfeatured').offset().top;    // returns number 
		    $(window).scroll(function(){ // scroll event
		      var windowTop = $(window).scrollTop(); // returns number 
		      if (stickyTop + 15 < windowTop){
			    var top = 15;
				$('.allrightfeatured').css({ position: 'fixed', top: top + 'px', 'max-width': $('.allrightfeatured').css('width') });
				if($(".nextopinion").length) {
					var bottom = $('.allrightfeatured').offset().top + $('.allrightfeatured').height();
					if (bottom > $(".nextopinion").offset().top)
					{
						top = $(".nextopinion").offset().top - bottom;
						$('.allrightfeatured').css({ position: 'fixed', top: top + 'px', 'max-width': $('.allrightfeatured').css('width') });
					}
				}
			}
		      else {
		        $('.allrightfeatured').css('position','static');
		      }
		    });
		  }
});
var windowsize = $(window).width();
$(window).resize(function() {
    windowsize = $(window).width();
    if (windowsize > 830) {
        $("#container").css('right','0');
    }
});
</script>
