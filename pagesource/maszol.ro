function getUpdates() {
	$.ajax({
		url: "/ajaxHandler.php",
		type: "POST",
		dataType: "JSON",
		data: {
			action: 'lastUpdate',
			fromTime: 1521412970		},
		success: function(response) {
			if (response.updates && response.updates > 0) {
				$("#new_articles").html("<a style='color: #FFF;' href='/index.php/frissites/1521412970'>" + response.updates + " ÚJ CIKK > </a>");
				if ($("#original_title").val() == '') {
					$("#original_title").val($("title").html());
