function setCookie(cname, cvalue, exdays) {
       var d = new Date();
       d.setDate(d.getDate());
       var midnight = new Date(d.getFullYear(), d.getMonth(), d.getDate(), 23, 59, 59);
       var expires = "expires=" + midnight;
       document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
   }

   function getCookie(cname) {
       var name = cname + "=";
       var decodedCookie = decodeURIComponent(document.cookie);
       var ca = decodedCookie.split(';');
       for (var i = 0; i < ca.length; i++) {
           var c = ca[i];
           while (c.charAt(0) == ' ') {
               c = c.substring(1);
           }
           if (c.indexOf(name) == 0) {
               return c.substring(name.length, c.length);
           }
       }
       return "";
   }

   function parseECTVFeed(result) {
       var data =result;
       var videoURL = data[0].link;
       var title = data[0].title;
       var thumbnailURL = '';
       title = title.rendered;
       var media = data[0]._embedded;
       media = media["wp:featuredmedia"];
       var imageSizes = media[0].media_details.sizes;
       jQuery.each(imageSizes, function (key, val) {
           var width = val.width;
           if (width == '300') {
               thumbnailURL = val.source_url;
               if (val.height < 150) {
                   padding = (150 - val.height) / 2;
               }
           }

       });
       createPopup(videoURL, title, thumbnailURL);
       jQuery("#adPopup").css("right", "15px");
   }
   function createPopup(videoURL, title, thumbnailURL) {
       jQuery('body').append('<div id="adPopup" class="col-sm-3 advt-video"></div>');
       var html = ''
      html += '<div class="closePopupDiv">' +
                   '<img src="wp-content/uploads/tv_cancel.png" class="closePopupImg" onclick="closePopup();"/>' +
               '</div>' +
               '<div id="imageDiv">' +
                   '<img src="' + thumbnailURL + '" style="margin-top:' + padding + 'px;" id="videoThumbnail"/>' +
                   '<img src="wp-content/uploads/tv_vimeo.png" class="videoPlayer" />' +
               '</div>' +
               '<div id="titleDiv" class="titleDiv">' +
                   '<div style="float:left;">' +
                      ' <img src="wp-content/uploads/ectv-logo2.png" />' +
                   '</div>' +
                   '<div class="lblText">' +
                      ' <label id="lbltitle" title="' + title + '">' + title + '</label>' +
                   '</div>' +
               '</div>'

       jQuery("#adPopup").append(html);
       if (padding == "0") {
           jQuery("#videoThumbnail").addClass("videoThumbnail");

       }
       jQuery("#adPopup").show();

       //hide popup after 10 secs.
       setTimeout(function () { jQuery("#adPopup").css("right", "-425px"); }, 10000);

       //jQuery("#adPopup").css("right", "-425px");

       jQuery('#adPopup').click(function (e) {
           if (jQuery(e.target).closest('.closePopupDiv').length) {
               jQuery("#adPopup").css("right", "-425px");
               return;
           }
           window.location.href = videoURL;
       });
   }
   </script>

