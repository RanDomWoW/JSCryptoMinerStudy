		<script>
			$(document).ready(function () {	
				$(function () {
					var map = new GMaps({
						div: '#map',
						lat: 35.703322,
						lng: 51.35008,
						controls: null
					});
					
					map.addMarker({
						lat: 35.703322,
						lng: 51.35008,
						title: 'Maktab Sharif',
						infoWindow: {
							content: '<p style="color: #222; padding: 10px; font-family: IRANSans; margin-right: 30px;">مکتب شریف</p>'
						}
					});
				});
			
				lightbox.option({
					'albumLabel': "تصویر %1 از %2",
				});	
				
				$('.start-status b').each(function() {
					var $this = $(this), countTo = $this.attr('data-count');				  
					$({ countNum: $this.text()}).animate({
						countNum: countTo
					},
					{
						duration: 5000,
						easing: 'linear',
						step: function() { $this.text(Math.floor(this.countNum)); },
						complete: function() {
						$this.text(this.countNum);
					}
					});
				});
				$('#professor-slider').slick({
					rtl: true,
					autoplay: true,
					dots: true,
					infinite: false,
					speed: 500,
					slidesToScroll: 1,
					prevArrow: '<button type="button" class="slick-prev eicon" id="icon-next_dark"></button>',
					nextArrow: '<button type="button" class="slick-next eicon" id="icon-prev_dark"></button>',
					responsive: [
					{
						breakpoint: 991,
						settings: {
							arrows: false
					  }
					}]
				});
				
				/*$('#company-slider').slick({
					touchMove: false,
					draggable: false,
					rtl: true,
					autoplay: false,
					dots: true,
					infinite: true,
					speed: 300,
					prevArrow: '<button type="button" class="slick-prev slick-dark"></button>',
					nextArrow: '<button type="button" class="slick-next slick-dark"></button>',
					slidesPerRow: 4,
					rows: 2,
					responsive: [
					{
						breakpoint: 767,
						settings: {
							slidesPerRow: 2,
							arrows: false
					  }
					},
					{
						breakpoint: 485,
						settings: {
							arrows: false,
							dots: false,
							slidesPerRow: 1	
					  }
					}]
				});*/
				
				var owl = $("#company-slider").owlCarousel({
					items: 2,
					dots: true,
					loop: true,
					navText: [],
					rtl: true,
					nav: true,
					onRefreshed: callback,
					stageClass: "myStage",
					itemClass: "owl-item myItem",
					responsive: {
						1000: {
							items: 5
						},
						767: {
							items: 2
						}
					}
				});
				
				var random = 2;
				function callback (event) {
                    var items = event.page.size;
                    if (items == 5) random = 2;
                    else random = 1;
                };
                
                var inview = new Waypoint.Inview({
				  element: $('#company')[0],
				  enter: function(direction) {
				    randomMaktabiha();
				  },
				  exited: function(direction) {
				    clearInterval(timer);
					clearInterval(mainTimer);
					$(".myStage").find(".myItem").removeClass("up");
				  }
				})
				
				var mainTimer, timer;
				function randomMaktabiha() {
    				mainTimer = setInterval(function(){
    					chooseLi(random);
    					
    					function chooseLi(random) {
    						$(".myStage").find(".myItem.active").eq(random).addClass("up");
    					}
    					
    					timer = setInterval(function(){ 
    						$(".myStage").find(".myItem.active").eq(random).removeClass("up");
    						
    						clearInterval(timer);
    						
    						owl.trigger('next.owl.carousel', [1000]);
    					}, 4000);
    				}, 5000);
				}
				
				$("#company-slider").hover(function() {
    				    $(".myItem").removeClass("up");
    				    clearInterval(timer);
    					clearInterval(mainTimer);
				    }, function() {
				        randomMaktabiha();
				    }
				);
				
				$(".cs-user").owlCarousel({
					items: 1,
					dots: true,
					loop: true,
					rtl: true,
					autoplay: true,
					autoplayHoverPause: false
				});
				
				$(function () {
    				lottie.loadAnimation({
                        container: document.getElementById('ATM'),
                        renderer: 'svg',
                        loop: false,
                        autoplay: true,
                        path: 'http://maktabsharif.ir/wp-content/themes/eMaktab/js/pay.json'
                    }); 
				});
			});
