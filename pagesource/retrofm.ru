<script type="text/javascript">
$(function(){

	function setBranding() {
		$("#all_wrapper").prepend('<div class="main_bg"></div>');
		$('body').addClass("legendy-nvsb-2017");
	};

	var isTest = (location.search == "?test");
	var showBranding = Boolean(0);

	var siteId = 1;
	var siteIdRegion = 0;
	var regionList = [2, 139, 8, 11, 18, 34, 42, 49, 63, 68, 77, 96, 102, 119, 108, 153];

	if ((isTest || showBranding) && $.inArray(siteIdRegion, regionList) > 0) {
		setBranding();
		return;
	}
	
	ymaps.ready(function(){
		ymaps.geolocation.get({
		    provider: 'yandex'
		}).then(function(res){
			var obj = res.geoObjects.get(0);
			if (obj.getCountryCode() !== "RU") {
				return;
			}
			var deleteWords = ['край', 'республика', 'область', 'автономная область', 'автономный округ', 'ао'];
			var yandexCity = obj.getLocalities()[0].toLowerCase();
			var yandexArea = obj.getAdministrativeAreas()[0].toLowerCase();
			
			for (var i = 0; i < deleteWords.length; i++) {
				yandexArea = yandexArea.replace(deleteWords[i], "");
			}
			yandexArea = $.trim(yandexArea);

			var formData = new FormData();
			formData.append("area", yandexArea);
			formData.append("city", yandexCity);

			$.ajax({
				url: "/stat.php",
				type: "post",
				data: formData,
				contentType: false,
				cache: false,
				processData: false
			});

			var areaList = [
				"челябинская",
				"кемеровская",
				"алтайский",
				"алтай",
				"новосибирская",
				"иркутская",
				"красноярский",
				"хакасия",
				"тыва",
				"томская",
				"тюменская",
				"бурятия",
				"омская",
				"хабаровский",
				"амурская",
				"приморский",
				"забайкальский"
			];
			if ((isTest || showBranding) && siteId == 1) {
				for (var i = 0; i < areaList.length; i++) {
					var item = areaList[i];
					if (yandexArea == item) {
						setBranding();
					}
				}
			}
		});
	});
});
</script>

