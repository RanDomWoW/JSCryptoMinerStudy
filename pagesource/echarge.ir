    <script type="text/javascript">





        $(document).ready(function () {
            var visit = localStorage.getItem('modalShow');
            if (visit != 'true') {
               showModal();
            };

            $('.close-btn').click(function () {
                localStorage.setItem('modalShow', 'true');
                hideModal()

            });
            $('.overlay').click(function () {
                $('#modal').fadeOut(500);
            });

            
        });

        function showModal() {
           $('#modal').fadeIn(900); 
        }

        function hideModal() {
            $('#modal').fadeOut(500); 
        }

        $(function () {

            $('#formMessages').bind('contentChanged', function () {
                //$('#lightBox .messageBox p, #lightBox .messageBox span').remove();
                $('#lightBox .messageBox div:not(.closeBtn)').remove();
                //$('#lightBox .messageBox').append($('#formMessages div').clone());
                $('p.user-info-message').text($('#formMessages div.user-info-message p').text());
                $('p.banks-message').text($('#formMessages div.banks-message p').text());
                $('p.cards-message').text($('#formMessages div.cards-message p').text());
                if ($('p.user-info-message').text() == '') {
                    $('p.user-info-message').css('display', 'none');
                }
                else {
                    $('p.user-info-message').css('display', 'block');
                }
                if ($('p.cards-message').text() == '') {
                    $('p.cards-message').css('display', 'none');
                }
                else {
                    $('p.cards-message').css('display', 'block');
                }
                if ($('p.banks-message').text() == '') {
                    $('p.banks-message').css('display', 'none');
                }
                else {
                    $('p.banks-message').css('display', 'block');
                }
                $('#lightBox').css('display', 'block');
                return;
            });
        });

    </script>
