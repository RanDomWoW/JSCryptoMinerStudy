		<script>
    var createError = function(error)
    {
        // Check for Servo Abort Exception
        if(error.type == 'Servo\\Api\\Exceptions\\ServoAbortException') {
            return createServoAbortError(error);
        }

        // Use General Error
        return createGeneralError(error);
    };

    var createServoAbortError = function(error)
    {
        return $('<div style="clear:both;" class="callout callout-warning ajax-error">' +
                    '<div class="callout-body">' + 
                        '<p>' + error.message + '</p>' +
                    '</div>' +
                '</div>');
    }

    var createGeneralError = function(error)
    {
        return $('<div style="clear:both;" class="callout callout-danger ajax-error">' +
                    '<h4 class="callout-heading">' +
                        '<i class="fa fa-exclamation-triangle"></i> ' + error.status + ': ' + error.title +
                    '</h4>' +
                    '<div class="callout-body">' + 
                        '<p>' + error.message + '</p>' +
                                                '<p class="text-right"><small>Log ID: <span class="text-monospace">' + error.id + '</span></small></p>' +
                    '</div>' +
                '</div>');
    }
</script>	<script>
	var Session = {

		/**
		 * The length (in milliseconds) until the Session times out.
		 *
		 * @var  {int}
		 */
		length: 900000,

		/**
		 * The timeout handle.
		 *
		 * @var  {int}
		 */
		timeout: null,

		/**
		 * Whether or not the Session is alive.
		 *
		 * @var  {boolean}
		 */
		alive: false,

		/**
		 * Starts the Session.
		 *
		 * @return  {void}
		 */
		start: function() {

			// Bring the Session to life
			this.alive = true;

			// Reset the Session timeout
			this.reset();

		},

		/**
		 * Resets the Session timeout.
		 *
		 * @return  {void}
		 */
		reset: function() {

			// Create the Previous Timeout
			this.clear();

			// Reset the Timeout
			var self = this;

			this.timeout = setTimeout(function() {
				self.end();
			}, this.length);

		},

		/**
		 * Clears the Session timeout.
		 *
		 * @return  {void}
		 */
		clear: function() {
			return clearTimeout(this.timeout);
		},

		/**
		 * Ends the Session.
		 *
		 * @return  {void}
		 */
		end: function() {

			// Mark the Session as dead
			this.alive = false;

			// Alert the User
			alert('Your Session has timed out');

			// Go to the Logout Page
			window.location = 'https://uasconnect.com/auth/logout';

		},

		/**
		 * Returns the Session Errors.
		 *
		 * @return  {object}
		 */
		getErrors: function() {
			return {};
		}

	};
</script>

