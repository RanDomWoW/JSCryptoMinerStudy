		}*/
		if(ddlHolTypeValue == "TOURING")
		{
			var ddlTourTypeValue = document.getElementById('AvailAndBook1_ddlTourerType').value;
			var ddlTourLengthValue = document.getElementById('AvailAndBook1_ddlTourerLength').value;
			
			if(ddlTourTypeValue == "")
			{
				alert('Please select a Tourer Type');
				return false;
			} 
			else if(ddlTourLengthValue == "")
			{
				alert('Please select a Length');
				return false;
			}
			
			// Restrict Peak holidays for PS
			
			var arrDate, strDay, strMonth, strYear;
			var arrDate = new Array();
			arrDate = txtDateValue.split("/");
			
			if (arrDate.length == 3) {
				strDay = arrDate[0]; if (strDay.length== 1) strDay = '0' + strDay;
				strMonth = arrDate[1]; if (strMonth.length == 1) strMonth = '0' + strMonth;
				strYear = arrDate[2]; if (strYear.length == 1)  strYear = '0' + strYear;
			}
			
			var dHolidayDate = new Date(strYear, strMonth-1, strDay);
			var peakStart1 = new Date(2018, 6, 21, 0, 0, 0);
			var peakEnd1 = new Date(2018, 7, 17, 23, 59, 59);
			//var peakStart2 		= new Date(2016,6,16,0,0,0);
			//var peakEnd2 		= new Date(2016,7,27,23,59,59);
