		else if(((ddlParksValue =="BR") || (ddlParksValue =="FG") || (ddlParksValue =="RV") || (ddlParksValue =="LY")) && (ddlDogssValue != "0") )
		{
			alert('Dogs are not allowed at the selected Park');
			return false;   
		}
		else if(txtDateValue == "" || txtDateValue == "dd/mm/yyyy" || txtDateValue.length < 8 || txtDateValue.indexOf("/") == -1)
		{
			alert("Please enter a valid date");
			return false;
		}
		else {
			isAvailBookValidated = true;
		}
		
		return true;
	}
	
