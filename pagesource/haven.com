<script type="text/javascript">
    //asp.net pageLoad used for rebinding after callback
    function pageLoad(sender, args)
    {
        changeDateBinding();//in dp_custom.js
        bindElements();
        $('.nojs').removeClass('nojs');
        
        /*if($('#AvailAndBook1_ddlHolType').val()==''){
            $("#AvailAndBook1_parkLocation select").attr("disabled", "true");          
            $('#AvailAndBook1_personCount').hide();
            $('#AvailAndBook1_letsGo').hide();
            $('#AvailAndBook1_dpSelf').hide();       
        }*/
        
        if($('#AvailAndBook1_ddlHolType').val()=='TOURING'){
            $('#dpSelf').show();
            $('#touringNgts').val('');
            if ($('#touringNgts').val() != '') {
                $('#touringNgts').append('<option value="">Please select</option>');
                for (var i = 1; i < 32; i++) {
                    if (i == 1) {
                        $('#touringNgts').append("<option value=" + i + ">" + i + ' night </option>');
                    } else {
                        $('#touringNgts').append("<option value=" + i + ">" + i + ' nights </option>');
                    }
                }
            }
            
	        $('#datepicker').datepicker( "option", "beforeShowDay", checkThisDayTouring );
	        $('#datepicker').datepicker( "option", "onChangeMonthYear", hightlightTouringDates() );
        	
	        $('#duration').val('');
	        $('#departureFromText').val('');
	        //$('#selectedDate').html('');
	        userSelectedDatesY1.length = 0;
	        userSelectedDatesY2.length = 0;	        
	        $('#duration').hide();
	        $('#touringNgts').show();
        	
	        hightlightTouringDates();

        } else {
            $("#datepicker").datepicker( "option", "beforeShowDay", disableDays );
            $('#duration').show();
            $('#touringNgts').val('');
	        $('#touringNgts').hide();
       }

       $('.ui-state-active').removeClass('ui-state-active').removeClass('ui-state-hover');
       
        if ($('#duration').val() != '' && $('#departureFromText').val() != '') {
            $('#datepicker').datepicker('setDate', $('#departureFromText').val());
        }
        if ($('#duration').val() == '') {
            $('.hiddenCalendar').hide();
            $('#departureFromText').val('');
            userSelectedDatesY1.length = 0;
            userSelectedDatesY2.length = 0;
        }       
        $('#touringNgts').val('');
       
        showIE8Placeholder();        
    } 

    function showIE8Placeholder(){
        if ( $.browser.msie) {
            if($("input#AvailAndBook1_txtDate").is("*") && $("input#AvailAndBook1_txtDate").val().trim()==''){
                $("input#AvailAndBook1_txtDate").val('dd/mm/yyyy');
                $("input#AvailAndBook1_txtDate").focus(function(){
                    this.select();
                });
            }    
        }
    }
    /* US2085 start */
    $('#AvailAndBook1_txtDate').attr('readonly', 'readonly');
    /* US2085 end */

    function hightlightTouringDates(){
        $('a.ui-state-default').addClass('fri');
    }

    // set the first possible ARRIVAL date
    function arrivalStartDate() {
	    var todayDate = new Date();
	    if ( todayDate < datepicker_season1Start ) {
		    return datepicker_season1Start;
	    } else {
		    //If the time is past 7PM, suggest tommorrow's arrival
		    if (todayDate.getHours() > 19) {
			    todayDate.setDate(todayDate.getDate()+1);
		    }
		    return todayDate;
	    }
    }

    function checkThisDayTouring(date) {
	    if ( date.getTime() <= datepicker_season1End.getTime() || date.getTime() >= datepicker_season2Start.getTime() ) {
		    for (i = 0; i < bankHolidays.length; i++) {
			    if (date.getDate() == bankHolidays[i][0] && date.getMonth() == bankHolidays[i][1] - 1 && date.getFullYear() == bankHolidays[i][2]) {
				    return [true, bankHolidays[i][3]];
			    }
		    }
		    return [true, ''];
	    } else {
		    return [false, 'outOfSeason'];
	    }
    }

    function bindElements(){        
       
        $('#arrivalMonth').bind('change',function(){
         setTimeout(function(){
            hightlightTouringDates();}, 100);
        });

        $('#touringNgts').bind('change',function(){
            if($(this).val()!=''){
		        $('.hiddenCalendar').fadeIn(500);
	        }else{
	            $('.hiddenCalendar').fadeOut(500);
	        }
	        $('#AvailAndBook1_ddlNights').val($(this).val());
        });

        //Set the number of nights when duration is clicked
        $('#duration').bind('change',function(){
            if($(this).val()!=''){
		        var opt = $(this).val().split('-');			
		        $('#AvailAndBook1_ddlNights').val(opt[0]);
	        }
        });

        //When datepicker is closed set the date
        $('#closeDatepicker').bind('click', function(){
            $("input#AvailAndBook1_txtDate").val($('#departureFromText').val());
            showIE8Placeholder();
        });

        //open datepicker when txtDate field is clicked
        $('#AvailAndBook1_txtDate').click(function(){
            $('#chgDate').click();
        });

        $('#chgDate').click(function(){
            if(chkAccBinding == false){
                makeAccordian();
                chkAccBinding = true;
