                                $("#scrolList").append(data);
                            }
                            else {
                                page = -1;
                            }
                            _inCallback = false;
                            $("div#loading").hide();
                            Start();
                        }
                    });
                }
            }
            // обработка события скроллинга
            $('#oldNews').on('click', function () {
                loadItems();
            });
        })
    
    </script>
    
    
</body>
