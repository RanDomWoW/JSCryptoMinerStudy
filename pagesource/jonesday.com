    function floatSelect(selectList) {
        if ($.browser.msie) {
            var initialWidth = 0;
            var currentOffsetX = $(selectList).position().left;
            if ($(selectList).attr("initialwidth") == null || $(selectList).attr("initialwidth") == "") {
                initialWidth = new Number($(selectList).outerWidth());
                $(selectList).attr("initialwidth", initialWidth);
            } else {
                initialWidth = new Number($(selectList).attr("initialwidth"));
            }
            $(selectList).css("width", "auto");
            var fullWidth = $(selectList).outerWidth();
            var displayX = (initialWidth - fullWidth);
            if (displayX < 0) {
                var searchSideDiv = $(selectList).parents("div.searchSide");
                var searchBottom = $(searchSideDiv).next("div.searchSidebtm");
                $(searchSideDiv).css("position", "absolute");
                $(searchBottom).css("visibility", "hidden");
                //$(searchBottom).hide();
                $(selectList).css("margin-left", displayX + "px");
            } else {
                $(selectList).css("width", initialWidth);
            }
        }
    }
    function unfloatSelect(selectList) {
        if ($.browser.msie) {
            var oldwidth = $(selectList).attr("initialwidth");
            if (typeof (oldwidth) == "undefined") { oldwidth = $(selectList).width(); }
            var searchSideDiv = $(selectList).parents("div.searchSide");
            $(searchSideDiv).css("position", "static");
            var searchBottom = $(searchSideDiv).next("div.searchSidebtm");
            $(searchBottom).css("visibility", "visible");
            $(selectList).css("width", oldwidth).css("margin-left", "0px").css("margin-right", "0px");
        }
    }
