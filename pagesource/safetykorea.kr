    <div class="main_cont03">
        <script type="text/javascript">
			
    		var pop_status = null;
    		
            $(document).ready(function () {
                recallIdx = 0;
                $('.main_cont03 .part01 ul.tabmenu>li').eq(0).addClass('on');
                $('.main_cont03 .part01 .tab_contents>div').eq(0).show();
                $('.part01 ul.tabmenu>li').click(function () {
                    var idx = $(this).index();
                    recallIdx = idx;
                    $(this).addClass('on').siblings().removeClass('on');
                    $(this).parent('ul').siblings('.tab_contents').children('div').eq(idx).show().siblings().hide();

                    var url = '/template/ajax/mainRecall';
                    if (idx == 1)url = '/template/ajax/mainFRecall';
                    $.ajax({
                        url: url,
                        dataType: "html",
                        success: function (result) {
                            $("#recall-notice").html(result);
                        }

                    });
                });
                $("#recall_more").click(function () {
                    if (recallIdx == 0) $("#recall_more").prop('href', '/recall/recallBoard');
                    else {
                        $("#recall_more").prop('href', '/recall/fRecallBoard');
                    }
                });

                boardIdx = 0;
                $('.main_cont03 .part02 ul.tabmenu>li').eq(0).addClass('on');
                $('.main_cont03 .part02 .tab_contents>ul').eq(0).show();
                $('.part02 ul.tabmenu>li').click(function () {
                    var idx_n = $(this).index();
                    boardIdx = idx_n;
                    $(this).addClass('on').siblings().removeClass('on');
                    $(this).parent('ul').siblings('.tab_contents').children('ul').eq(idx_n).show().siblings().hide();
                });

                $("#board_more").click(function () {
                    if (boardIdx == 0) $("#board_more").prop('href', '/news/promote');
                    else if (boardIdx == 1) $("#board_more").prop('href', '/news/notice');
                    else if (boardIdx == 2) $("#board_more").prop('href', '/news/publications');
                });
                
                var todayStr = getToday();
				
                /* 2016-06-14까지 리콜 워크샵 팝업 호출 */
                if(todayStr < '20160615') {
                	
                	pop_open();
                }
                
             
				
				var blnCookie = getCookie("systemNoticePopup");
				
                /* 2016-10-05까지 시스템 일시 중단 안내 팝업 호출 */
                if(todayStr < '20161006' && !blnCookie) {
                	
                	sysPop_open();
                }
                
              
                
            });
            $(window).load(function(){
            	function getCookie(name){    
            		var wcname = name + '=';
            		var wcstart, wcend, end;
            		var i = 0;   
            		  while(i <= document.cookie.length) {            
            		   wcstart = i;  
            		 wcend   = (i + wcname.length);
            		 
            		 if(document.cookie.substring(wcstart, wcend) == wcname) {                    
            		  if((end = document.cookie.indexOf(';', wcend)) == -1)                           
            		   end = document.cookie.length;                    
            		  return document.cookie.substring(wcend, end);            
            		   }            

            		 i = document.cookie.indexOf('', i) + 1;            
            		  
            		   if(i == 0)                    
            		  break;    
            		  }    
            		  return '';
            		} 
              	setTimeout(function(){
              		if(getCookie('safety') != 'rangs') {     
             	   /* 제품안전정보센터 이용고객 설문 팝업 호출 (2016-06-24 : 김연준 추가) */
                    /* openKATSSurvey(); */
                    openKATSRecall();
            		}
              		
              		if(getCookie('safety1') != 'rangs') {     
                         openPopup();
                 		}
            		
            	}, 1000);
            });
            
            /* 현재날짜 구하기 - 2016-06-02 (김연준 추가) */
            function getToday() {
            	
                var today = new Date();
                var year = today.getFullYear();
                var month = today.getMonth() + 1;
                var day = today.getDate();
                
                if(month < 10) {
                	
                	month = "0" + month;
                }
                
                if(day < 10) {
                	
                	day = "0" + day;
                }
                
                var todayStr = year + "" + month + "" + day;
                
                return todayStr;
            }
            
            /* 「자발적 리콜」 저변확대를 위한 워크숍 개최 안내 팝업 호출 */
            function pop_open() {
            	
            	var agent = navigator.userAgent.toLowerCase();
            	// 브라우저가 IE 인 경우 모달팝업으로 호출
            	
            	if((navigator.appName == 'Netscape' && navigator.userAgent.search('Trident') != -1) || (agent.indexOf("msie") != -1)) {
                	
                	var popUrl = "workshopPopup";
                	var popOption = "dialogWidth: 520px; dialogHeight: 250px; status:no; help: no; location: no";
                	
                	var pop = window.showModalDialog(popUrl, "", popOption);
                	
            	} else {	// IE 가 아닌 경우 Div로 보여줌
					
            		var screenWidth = screen.availWidth;
            		var screenHeight = screen.availHeight;
            		
            		$("#workshopPopup").css("left", screenWidth / 2 - 250);
            		$("#workshopPopup").css("top", 300);
                	$("#workshopPopup").show();
            	}
            }
            
            /* 「자발적 리콜」 저변확대를 위한 워크숍 개최 안내 팝업 닫기 */
            function pop_close() {
            	
            	$("#workshopPopup").hide();
            }
            
            /* 제품안전정보센터 이용고객 설문 팝업 호출 */
            function openKATSSurvey() {
            	
            	/* var KATSSurvey = window.open('/s2016/step1.jsp', '_blank', 'width=550px, height=400px, scrollbars=no'); */
            	var url = "/main/survey";
            	var KATSSurvey = window.open(url, '_blank', 'width=550px, height=300px, scrollbars=no');
            	KATSSurvey.focus();	
             	
            }
            
			function openKATSRecall() {
            	
            	var url = "/main/recall";
            	var KATSSurvey = window.open(url, '_blank', 'width=550px, height=450px, scrollbars=no');
            	KATSSurvey.focus();	
            	
            }
			
			function openPopup() {
            	
            	//var url = "/main/openPopup";
            	var url = "/main/openPopup";
            	var KATSSurvey = window.open(url, '_blank', 'left=550, width=550px, height=330px, scrollbars=no');
            	KATSSurvey.focus();	
            	
            }
            
            
            /* 「시스템 공지사항」시스템 중단 안내 팝업 호출 */
            function sysPop_open() {
				
				var agent = navigator.userAgent.toLowerCase();
            	
            	// 브라우저가 IE 인 경우 모달팝업으로 호출
            	if((navigator.appName == 'Netscape' && navigator.userAgent.search('Trident') != -1) || (agent.indexOf("msie") != -1)) {
                	
                	var popUrl = "noticePopup";
                	var popOption = "dialogWidth: 600px; dialogHeight: 420px; status:no; help: no; location: no";
                	
