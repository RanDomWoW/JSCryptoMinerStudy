                    country: country,
                    campaignId: campaignId,
                    contentId: contentId,
                    contentShortLabel: contentShortLabel,
                    contentRubIds: contentRubIds,
                    offerId: offerId
                }
            },
            dataType: 'json',
            success: function(jsonData) {
                if (jsonData.success == true) {

                    switch (action) {
                        case 'addToCart':
                            FG_DownloadManager_notifySuccess(cartNotificationElementAdded);
                            break;
                        case 'removeFromCart':
                            FG_DownloadManager_notifySuccess(cartNotificationElementRemoved);
                            break;
                    }

                    FG_DownloadManager_updateCartBadge(jsonData.count);

                    if (typeof FG_DownloadManager_updateHeaderText != 'undefined') {
                        FG_DownloadManager_updateHeaderText(jsonData.count);
                    }

                    ajaxIsSuccess(action == 'addToCart', action, jsonData);
                } else if (jsonData.alreadyInCart == true) {
                    // this is needed to get redirect if the content is already add in cart
                    ajaxIsSuccess(action == 'addToCart', action, jsonData);
                } else if (typeof jsonData.errorMessage != 'undefined') {
                    FG_DownloadManager_callErrorEvents(9099265, jsonData.errorMessage);
                    FG_DownloadManager_notifyError(jsonData.errorMessage);
                }
            }
        });

        return true;
    }

    /**
    * Update cart badge count in page
    *
    * @param {int} count
    */
    function FG_DownloadManager_updateCartBadge(count) {
        if ($('.FG_DownloadManager_badge').length) {
            var countText = (count == 0) ? 0 : '+' + count;

            $('.FG_DownloadManager_badge > a > span').html(countText);

            if (count == 0) {
                $('.FG_DownloadManager_badge').hide();
            } else {
                $('.FG_DownloadManager_badge').show();
            }
        }
    }
}

