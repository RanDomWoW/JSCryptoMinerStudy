<script type="text/javascript" src="http://kiryu-kyotei.com/themes/kiryu/js/top/script.js"></script>
<script type="text/javascript">
//カレンダーの処理
cal_today = '2018-03-19';
cal_disp  = cal_today.split('-');
cal_base  = cal_today.split('-');

cal_kaisai = Array('20180203','20180204','20180205','20180206','20180207','20180208','20180221','20180222','20180223','20180224','20180225','20180301','20180302','20180303','20180304','20180305','20180306','20180307','20180310','20180311','20180312','20180313','20180314','20180318','20180319','20180320','20180321','20180322','20180404','20180405','20180406','20180407','20180408','20180419','20180420','20180421','20180422','20180423','20180427','20180428','20180429','20180430','20180501','20180502','20180505','20180506','20180507','20180508','20180509','20180510','20180517','20180518','20180519','20180520','20180521','20180522','20180529','20180530','20180531','20180601','20180602','20180603','20180618','20180619','20180620','20180621','20180622','20180623','20180627','20180628','20180629','20180630');
$(function(){
	setcalender();
	$(document).on("click","a.cal_prev",function(){
		$("a.cal_next",".calendar__list__li").show();
		if( (cal_base[0] - cal_disp[0]) * 12 + (cal_base[1] - cal_disp[1]) >= 0 ){
			$("a.cal_prev",".calendar__list__li").hide();
		} else {
			$("a.cal_prev",".calendar__list__li").show();
		}
		if( (cal_base[0] - cal_disp[0]) * 12 + (cal_base[1] - cal_disp[1]) <= 0 ){
			if(--cal_disp[1]<=0){--cal_disp[0];cal_disp[1]=12;}
			cal_disp[1] = (cal_disp[1]<10)?'0'+cal_disp[1]:cal_disp[1];
			setcalender();
		}
	});
	$(document).on("click","a.cal_next",function(){
		$("a.cal_prev",".calendar__list__li").show();
		if( (cal_base[0] - cal_disp[0]) * 12 + (cal_base[1] - cal_disp[1]) <= -2 ){
			$("a.cal_next",".calendar__list__li").hide();
		} else {
			$("a.cal_next",".calendar__list__li").show();
		}
		if( (cal_base[0] - cal_disp[0]) * 12 + (cal_base[1] - cal_disp[1]) > -3 ){
			if(++cal_disp[1]>=13){++cal_disp[0];cal_disp[1]=1;}
			cal_disp[1] = (cal_disp[1]<10)?'0'+cal_disp[1]:cal_disp[1];
			setcalender();
		}
	});
});
function setcalender(){
	$("p.calendar__month",".calendar").html(cal_disp[0]+"年"+cal_disp[1]+"月");
	var str = Array();
	str[str.length] = '<tr>';
	str[str.length] = '<th class="col_sun">日</th>';
	str[str.length] = '<th>月</th>';
	str[str.length] = '<th>火</th>';
	str[str.length] = '<th>水</th>';
	str[str.length] = '<th>木</th>';
	str[str.length] = '<th>金</th>';
	str[str.length] = '<th class="col_sat">土</th>';
	str[str.length] = '</tr>'

	//1日の日付を取得
	var date = new Date(~~cal_disp[0], ~~cal_disp[1]-1, 1);
	var startday = date.getDay();//0が日曜日
	var cal_next = Array(~~cal_disp[0], ~~cal_disp[1]+1);
	if(cal_next[1]==13){++cal_next[0];cal_next[1]=1;}
	date = new Date(~~cal_next[0], ~~cal_next[1]-1, 0);
	var endday = date.getDate();//0日目は先月の末日になる

	var i=0;
	for(var w=0;w<6;++w){
		str[str.length] = '<tr>';
		for(var d=0; d<7; ++d){
			if(((w==0) && (d<startday))||(i>=endday)){
				str[str.length] = '<td></td>';
			} else {
				++i;
				var s_y = cal_disp[0];
//				var s_m = (cal_disp[1]<10)?'0'+cal_disp[1]:cal_disp[1];
				var s_m = parseInt(cal_disp[1], 10);
				s_m = (s_m<10)?'0'+s_m:s_m;
				var s_d = (i<10)?'0'+i:i;
				var s_key = s_y+''+s_m+''+s_d;
				if(array_search(s_key, cal_kaisai)!==false){
					str[str.length] = '<td class="is--open" data="'+s_y+s_m+s_d+'">'+i+'</td>';
				} else {
					str[str.length] = '<td data="'+s_y+s_m+s_d+'">'+i+'</td>';
				}
			}
		}
		str[str.length] = '</tr>';
		if(i>=endday){break;}
	}
	$("table.calendar__tbl",".calendar").html(str.join(''));

	function array_search(needle, arr) {
		var len = arr.length;
		for (var i=0; i<len; ++i)
		if (arr[i] == needle)
		return i;
		return false;
	}

}
</script>
