}
function setDataFromHidden(){
var seqs = $('#'+getIdClass("form")).attr("seqs");
config.req_seqs = seqs.split(",");
for (var i=0; i<config.req_seqs.length; i++) {
var seq = config.req_seqs[i];
var pid = getIdClass("tmpdata_"+seq);
config.product_data[seq] = {};
$("."+pid).each(function(){
var key = $(this).attr("rel");
var value = $(this).attr("value");
if (key == "opt_price" && value) {
value = $.parseJSON(value);
}
if (key == "opt_data" && value) {
value = $.parseJSON(value);
}
config.product_data[seq][key] = value;
});
}
}
function product_submit(callback)
{
if (checkSelectedItem() == false) return;
if (checkOptionRequired() == false) return;
if (checkAddOption() == false) return;
if (checkQuantity() == false) return;
initSubmitData();
var frm = $('#'+getIdClass("form"));
var main = config.main_data;
frm.find(":hidden").remove();
frm.append(getInputHidden('product_no', main.product_no));
frm.append(getInputHidden('product_name', main.product_name));
frm.append(getInputHidden('main_cate_no', main.main_cate_no));
frm.append(getInputHidden('display_group', main.display_group));
frm.append(getInputHidden('option_type', main.option_type));
frm.append(getInputHidden('product_price', main.product_price));
frm.append(getInputHidden('product_min', main.product_min));
frm.append(getInputHidden('command', main.command));
frm.append(getInputHidden('has_option', main.has_option));
frm.append(getInputHidden('basket_type', main.basket_type));
frm.append(getInputHidden('multi_option_schema', main.multi_option_schema));
frm.append(getInputHidden('multi_option_data', main.multi_option_data));
frm.append(getInputHidden('quantity', main.quantity));
frm.append(getInputHidden('delvType', main.delvtype));
frm.append(getInputHidden('redirect', main.redirect));
frm.append(getInputHidden('prd_detail_ship_type', main.prd_detail_ship_type));
frm.append(getInputHidden('product_max_type', main.product_max_type));
frm.append(getInputHidden('product_max', main.product_max));
frm.append(getInputHidden('relation_product', main.relation_product));
// check option and get option value
$('select[class="' + getIdClass("select_"+main.product_no) + '"]:visible').each(function()
{
frm.append(getInputHidden('optionids[]', $(this).attr('name')));
if ($(this).attr('required') == true) {
frm.append(getInputHidden('needed[]', $(this).attr('name')));
}
var iSelectedIndex = $(this).get(0).selectedIndex;
if (iSelectedIndex > 0) {
var sValue = $(this).val();
if (sValue) {
frm.append(getInputHidden($(this).attr('name'), sValue));
}
}
});
//check user add option and get value
var addopt = $("."+getIdClass("addopt_"+main.product_no));
if (addopt.length > 0) {
var aName = [];
for (var j=0; j<addopt.length; j++) {
frm.append(getInputHidden('option_add[]', $(addopt[j]).val()));
aName.push($(addopt[j]).attr("title_option"));
}
frm.append(getInputHidden('add_option_name', aName.join(';')));
}
// get data when multi product submitted
var sel_main = config.main;
for (var i=0; i<sel_main.length; i++)
{
var main_info = sel_main[i];
frm.append(getInputHidden('selected_item[]', main_info));
}
var sub = config.sub;
for (var i=0; i<sub.length; i++)
{
var sub_info = sub[i];
frm.append(getInputHidden('relation_item[]', sub_info));
}
var items = getSubmitProduct();
for (var i=0; i<items.length; i++) {
var seq = items[i];
if (!seq || seq == main.product_no) {
continue;
}
if ( $("."+getIdClass("addopt_"+seq)).length > 0 ) {
var nameJoin = [];
var valueJoin = [];
$("."+getIdClass("addopt_"+seq)).each(function(){
nameJoin.push( $(this).attr("title_option"));
valueJoin.push( $(this).val());
})
frm.append(getInputHidden('user_option_name_' + seq, nameJoin.join(',@,')));
frm.append(getInputHidden('user_option_' + seq, valueJoin.join(',@,')));
}
}
// submit using ajax
frm.attr("action", "/"+config.action_url);
var sParam = frm.serialize();
$.post(config.action_url, sParam, function(data)
{
if (typeof callback == "function") {
callback();
} else {
if (confirm(__("장바구니에 추가되었습니다. \n장바구니를 확인하시겠습니까?"))) {
makePop();
}
}
}, 'json');
}
function makePop()
{
var settings = 'toolbar=0, status=0, menubar=0, scrollbars=yes, height=960, width=1280';
