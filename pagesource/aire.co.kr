}
function checkSelectedItem()
{
var reqseqs = getSubmitProduct();
if (reqseqs.length < 1) {
alert(__("선택된 상품이 없습니다."));
return false;
}
return true;
}
function checkOptionRequired()
{
var items = getSubmitProduct();
for (var i=0; i<items.length; i++) {
var seq = items[i];
var flag = false;
if (!seq) {
return false;
}
$("."+getIdClass("select_"+seq)).each(function(){
if (flag == true) {
return;
}
if ($(this).attr('required')) {
if ($('option:selected', this).val().indexOf('*') > -1) {
$(this).focus();
flag = true;
}
}
});
if (flag == true) {
alert(__("선택안된 필수 옵션이 있습니다."));
return false;
}
}
return true;
}
function checkAddOption()
{
var items = getSubmitProduct();
for (var i=0; i<items.length; i++) {
var seq = items[i];
if (!seq) {
return false;
}
var addopt = $("."+getIdClass("addopt_"+seq));
if (addopt.length < 1) {
continue;
} else {
for (var j=0; j<addopt.length; j++) {
var $add = $(addopt[j]);
if ($add.attr('required')) {
if ($.trim($add.val()) == "") {
$add.focus();
alert(__("추가 옵션을 입력해 주세요."));
return false;
} else {
if ($add.val().length > 100) {
$add.focus();
alert(__("추가 옵션은 100 자 이상 적을수 없습니다."));
return false;
}
}
}
}
}
}
return true;
}
function getSubmitProduct()
{
var chkbox = $('input[class="'+getIdClass("chk")+'"]');
if (chkbox.length < 1) {
return config.req_seqs;
} else {
var items = $('input[class="'+getIdClass("chk")+'"]:checked');
var selProduct = [];
for (var i=0; i<items.length; i++) {
var item = items[i];
var seq = $(item).val();
selProduct.push(seq);
}
return selProduct;
}
}
function checkQuantity(seq, min)
{
var items = getSubmitProduct();
for (var i=0; i<items.length; i++) {
var seq = items[i];
if (!seq) {
return false;
}
var data = config.product_data[seq];
var min = parseInt(data['product_min'],10);
var max = parseInt(data['product_max'],10);
var targetInput = $("."+getIdClass("quan_"+seq));
$("."+getIdClass("quan_"+seq)).each(function(){
targetInput = $(this);
var quan = $.trim(targetInput.val());
quan = parseInt(quan, 10);
min = ( min < 1 ) ? 1 : min;
if (isNaN(quan)) {
targetInput.focus();
alert(__("정확한 수량을 입력해 주세요."));
return false;
}
if (quan < min) {
targetInput.focus();
alert(__("최소 주문 갯수를 확인해 주세요."));
return false;
}
if (max !== 0 && quan > max) {
targetInput.focus();
alert(sprintf(__("최대 %s개 주문 할 수 있습니다."), max));
return false;
}
});
}
return true;
}
function initSubmitData()
{
var mainFlag = true;
var items = getSubmitProduct();
for (var i=0; i<items.length; i++) {
var seq = items[i];
var data = config.product_data[seq];
if (data['has_option'] == 'T') {
$("."+getIdClass("itembox_"+data['product_no'])).each(function(){
var code = $(this).find("."+getIdClass("itemcode_"+data['product_no']));
var quan = $(this).find("."+getIdClass("quan_"+data['product_no']));
if (code.length > 0 && quan.length > 0) {
code = $(code[0]).val();
quan = $(quan[0]).val();
quan = parseInt(quan, 10);
if (mainFlag == true) {
setMainPrd(data, code, quan);
} else {
setSubPrd(code, quan);
}
}
});
} else {
var quan = getNonOptQuan(seq, data['product_min']);
var code = data['product_code'] + '000A';
if (mainFlag == true) {
setMainPrd(data, code, quan);
} else {
setSubPrd(code, quan);
}
}
if (config.main_data['product_no']) {
mainFlag = false;
}
}
}
function setMainPrd(data, code, quan)
{
if (!config.main_data['product_no']) {
var main = {};
main['product_no'] = data['product_no'];
main['product_name'] = data['product_name'];
main['main_cate_no'] = data['prd_main_category_no'];
main['display_group'] = '1';
main['option_type'] = data['option_type'];
main['product_price'] = data['product_price'];
main['product_min'] = data['product_min'];
main['command'] = 'add';
main['has_option'] = data['has_option'];
main['basket_type'] = data['basket_type'];
main['multi_option_schema'] = '';
main['multi_option_data'] = '';
main['quantity'] = 0;
main['delvtype'] = data['delvtype'];
main['redirect'] = "2";
main['prd_detail_ship_type'] = (data['delvtype'] != 'A') ? 'C' : 'P';
main['product_max_type'] = data['product_max_type'];
main['product_max'] = data['product_max'];
main['relation_product'] = 'yes';
config.main_data = main;
}
config.main_data['quantity'] += quan;
config.main.push(quan+"||"+code);
}
function setSubPrd(code, quan)
{
config.sub.push(quan+"||"+code);
}
function getNonOptQuan(seq, min)
{
var quan = $("."+getIdClass("quan_"+seq));
if (quan.length < 1) {
quan = min;
} else {
quan = $(quan[0]).val();
}
quan = parseInt(quan, 10);
if (isNaN(quan) || quan < 1) {
quan = 1;
}
return quan;
}
function getIdClass(raw, type)
{
var frags = [config.prefix, raw, config.appid, config.unique_id];
return frags.join(config.deli);
}
function getIdClassPrefix(raw, type)
{
return config.prefix+config.deli+raw+config.deli;
}
function getInputHidden(sName, sValue)
{
return "<input type='hidden' name='" + sName + "' value='" + sValue + "' />";
}
function addCommas(val)
{
if (typeof val == "string" || typeof val == "number") {
return val.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
}
return "";
}
})();
</script></body></html>