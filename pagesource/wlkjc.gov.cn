    var slide = {
        'i': 0,
        't': true,
        'start': function (a, b, c) {
            $(a).hover(
		function () { slide.t = false; },
		function () { slide.t = true; });
            if (typeof c == 'undefined') { c = $(a).find(b).length; }
            if ($(a + ' .listimg').length == 0) {
                $(a).append('<ul class="listimg"></ul>');
                for (var i = 0; i < c; i++) { $(a + ' .listimg').append('<li onmouseover="slide.tab(\'' + a + '\',' + i + ',' + c + ')" class="slideList_' + i + '">' +parseInt( i+1) + '</li>'); }
            }
            for (var i = 0; i < c; i++) { $(a).find(b).eq(i).addClass('slide_' + i); }
            if (slide.t == true) {
                if (slide.i >= c) {
                    slide.i = 0;
                    setTimeout('slide.start("' + a + '","' + b + '","' + c + '")', 5000);
                    slide.tab(a, slide.i, c);
                    slide.i = 1;
                } else {
                    setTimeout('slide.start("' + a + '","' + b + '","' + c + '")', 5000);
                    slide.tab(a, slide.i, c);
                    slide.i = slide.i + 1;
                }
            } else {
                setTimeout('slide.start("' + a + '","' + b + '","' + c + '")', 1000);
            }
        },
        'tab': function (a, b, c) {
            try {
                for (var i = 0; i < c; i++) {
                    if (i != b) {
                        $(a + " .slideList_" + i).removeClass('on');
                        $(a + " .slide_" + i).hide();
                    }
                }
                $(a + " .slideList_" + b).addClass('on');
              
       
        
                $(a + " .slide_" + b).fadeIn(1000);
                slide.i = b;
            } catch (f) { }
        }
        
    }
    </script>
