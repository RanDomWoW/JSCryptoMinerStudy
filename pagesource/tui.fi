<script>
    function downloadJSAtOnload(){
        var element=document.createElement("script")
        element.src='/crisis/widgets/assets/crisis-message.js'
        document.body.appendChild(element)
    }
    if(window.addEventListener){window.addEventListener('load',downloadJSAtOnload,false)}
    else if(window.attachEvent){window.attachEvent('onload',downloadJSAtOnload)}
    else{window.onload=downloadJSAtOnload}
</script>
<script>
    function startPageTracking(){
        var tuiSections=document.querySelectorAll('.tui-section')
        tuiSections.forEach(function(tuiSection,sectionIndex) {
            var tuiLinks=tuiSection.querySelectorAll('.tui-section a')
            tuiLinks.forEach(function(element,index){
                element.addEventListener('click',function(){
                    var cardHeading=(this.classList.contains('content-navigation-card'))?this.querySelector('.content-navigation-text').innerText:this.querySelector('.cc-heading').innerText
                    var eventLabel=tuiSection.querySelector('.list-header h2')
                    dataLayer.push({event:'genericTrackEvent',eventCategory:'[Page: Startpage]',eventAction:'[Link: '+cardHeading+'/Container Index#'+(sectionIndex + 1)+'/'+(index + 1)+']',eventLabel:eventLabel ? eventLabel.innerText : ''})
                })
            })
        })
        var startPageCtaButton=document.querySelector('.cta-startpage-button')
        if(startPageCtaButton){
            startPageCtaButton.addEventListener('click',function(){
                dataLayer.push({event:'genericTrackEvent',eventCategory:'[Page: Startpage]',eventAction:'[Link: '+startPageCtaButton.innerText+'/Container Index#0]',eventLabel:'Hero Cta Button'})
            })
        }
    }
    if(window.addEventListener){window.addEventListener('load',startPageTracking,false)}
    else if(window.attachEvent){window.attachEvent('onload',startPageTracking)}
</script>

<script>
    dataLayer = [{
        'webApplicationName': 'ssg',
        'pagePathName': document.location.pathname.toLowerCase(),
        'pageTitle': document.title,
