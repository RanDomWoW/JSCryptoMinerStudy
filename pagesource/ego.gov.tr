    function GetHatListesi() {

        $.post("/AjaxData/HatListesi", {})

            .done(function (data) {
                $("#hat_liste_combo").html(data);
            });
    }

    function OtobusNerede() {
        var durak_no = $("#durak_no").val();

        if (durak_no.length != 5) {
            $("#errmsg").html("5 haneli durak numarası giriniz").delay(3000).show().fadeOut("fast");
        }
        else {
            otobusnerede.submit();
        }
    }
</script>
