<script>
    $(document).ready(function () {
        //called when key is pressed in textbox
        $("#durak_no").keypress(function (e) {

            if (e.which == 13) {
                OtobusNerede();
                return false;
            }

            //if the letter is not digit then display error and don't type anything
            if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                //display error message
                $("#errmsg").html("Sadece rakam girilebilirsiniz").delay(2000).show().fadeOut("fast");
                return false;
            }
        });
    });
    $(document).ready(function () {
        //called when key is pressed in textbox
        $("#hat_no").keypress(function (e) {

            if (e.which == 13) {
                OtobusNerede();
                return false;
            }
        });
    });
</script>
<script>
    jQuery(document).ready(function () {
        // initiate layout and plugins
        App.init();
        FormSamples.init();

