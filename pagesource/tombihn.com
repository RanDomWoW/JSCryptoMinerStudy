        } else {
          jQuery('#mc_newsletter').addClass('bottom-drawer-open');
        }
      });
      jQuery('.mc_newsletter_close').click(function(e) {
        e.preventDefault();
        jQuery('#mc_newsletter').addClass('hidden');
      });
      
      //calculate if it is an available time
      var tawkIsAvailable = false;
      var d = new Date();
      var n = d.getUTCDay();

      var h = d.getUTCHours();
      var m = d.getUTCMinutes();

      // Monday - Friday
      if(n>0 && n<6) {
          //14:30 - 22:45 UTC = 06:30-14:45 PST (PDT == -1) 
          if((h==14 && m>=30) || (h==22 && m<=45) || (h>14 && h<22)) {
            tawkIsAvailable = true;
          }
      }
      if(tawkIsAvailable) {
        jQuery('.tawk-trigger').removeClass('time-setting-disabled').addClass('time-setting-enabled');
      }
    });

