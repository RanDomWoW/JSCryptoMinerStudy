	 	];	
	
  var pairs = [ ]; // only eligible params will be stored in this

  var query = getQueryArr();

  for(var i=0; i < query.length; i++){
    for(var z=0; z < affiliateParams.length; z++){
      if(query[i].name == affiliateParams[z]){
      	pairs.push( query[i] ); // found eligible param
      }
    }
  }
	
  if(pairs.length == 0){
    return; 
	}
  
  
  var paramStr = jQuery.param(pairs);
  // passes the referral url, and all other eligible params
  var l = "https://app.inventorysource.com/api-public/referral/?" + encodeURIComponent(r) + '&' + paramStr;  
  document.write("<img style=\"display:none\" src=\""+l+"\" WIDTH=1 HEIGHT=1 border=0>");
}

KBaffiliate();
