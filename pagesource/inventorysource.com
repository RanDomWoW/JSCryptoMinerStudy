// parses query string params into name:value array
function getQueryArr(){
	var arr = [ ]; var str = window.location.search.substring(1);
	str.replace(/([^=&]+)=([^&]*)/g, function(m, key, value) {
    		arr.push({ name : decodeURIComponent(key), value : decodeURIComponent(value) });
	}); return arr;
}

