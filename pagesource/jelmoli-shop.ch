            </a>
        </div>

        <div class="info-block">
            {{^options.hideProductName}}
            <div class="name">
                <a class="no-link hover-underline" href="{{#productURL}}{{#masterSku}}{{masterSku}}{{/masterSku}}{{^masterSku}}{{sku}}{{/masterSku}}@{{name}}{{/productURL}}#sku={{sku}}&ref=reco{{promo}}">
                    {{#shortName}}{{shortName}}{{/shortName}}{{^shortName}}{{name}}{{/shortName}}
                </a>
            </div>
            {{/options.hideProductName}}

            {{^options.hideRatings}}
            <div class="rating">
                {{#showAvgRating}}
                    <span class="weikavoice-stars stars-{{#formatRating}}avgRating{{/formatRating}} active"></span>
                {{/showAvgRating}}
            </div>
            {{/options.hideRatings}}

            <div class="prices">
                {{#showStrikePrice}}
                <div class="price price-old">
                    {{#fromPrice}}ab {{/fromPrice}}{{#formatMoney}}oldPrice{{/formatMoney}}
                </div>
                {{/showStrikePrice}}
                <div class="price {{#showStrikePrice}}price-highlight{{/showStrikePrice}}">
                    {{#fromPrice}}ab {{/fromPrice}}{{#formatMoney}}price{{/formatMoney}}
                </div>
            </div>

            <div class="action-block">
                <a class="button" href="{{#productURL}}{{#masterSku}}{{masterSku}}{{/masterSku}}{{^masterSku}}{{sku}}{{/masterSku}}@{{name}}{{/productURL}}#sku={{sku}}&ref=reco{{promo}}">
                    Zum Artikel
                </a>
            </div>
        </div>
    </li>
{{/data}}</script><div class="placeholder exit-layer">
