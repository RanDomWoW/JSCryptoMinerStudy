                </select>
                <br class="clear">

                <label>Ages:</label>
                <input type="number" name="age-min" id="age-min" pattern="[0-9]{2}" placeholder="18" value="18">
                <span>to</span>
                <input type="number" name="age-max" id="age-max" pattern="[0-9]{2}" placeholder="99" value="99">

                <label>Near:</label>
                <input class="zip" id="zip" name="zip" type="text" placeholder="Zip Code" value="">
                <a href="#" id="not-in-usa" style="float:left; display: block; color: #ccc">Not in<br>the US?</a>
                <div class="hidden" id="country-container" style="float:left">
                    <label>Country:</label>
                    <select name="country" id="country">
                        <option value="1" selected>United States</option>
                        <option value="2">Canada</option>
                        <option value="3">United Kingdom</option>
                        <option value="4">Australia</option>
                    </select>
                    <br class="clear">
                </div>
                <br class="clear">
                <input class="submit" type="submit" value="Find Booty" id="signup-submit">
                <input class="hidden" type="hidden" name="page" id="page" value="1">
            </form>

            <script>
                $(function(){
                    $('#not-in-usa').click(function(){
                        $('#country-container').show();
                        $(this).hide();
                        return false;
                    });

                    $('#zip').click(function(){
                        if ($(this).val() != ''){
                            $('#signup-error').slideUp();
                        }
                    });

                    $('#gender,#seeking').change(function(){
                        if ($(this).val() != 0){
                            $('#signup-error').slideUp();
                        }
                    });

                    $("#age-max,#age-min").keydown(function (e) {
                        if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 190]) !== -1 ||
                            (e.keyCode == 65 && e.ctrlKey === true) ||
                            (e.keyCode >= 35 && e.keyCode <= 39)) {
                                 return;
                        }
                        if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                            e.preventDefault();
                        }
                    }).blur(function(e){
                        if ($(this).val() > 99){
                            $(this).val(99);
                        } else if ($(this).val() < 18) {
                            if(this.id == 'age-min') {
                                $(this).val(18);
                            } else {
                                $(this).val(99);
                            }

                        }
                        if (this.id == 'age-max') {
                            if ($(this).val() < $('#age-min').val()) {
                                $('#age-min').val($(this).val());
                            }
                        } else {
                            if ($(this).val() > $('#age-max').val()) {
                                $('#age-max').val($(this).val());
                            }
                        }
                    });

                    $('#signup-submit').click(function(){
                        var errors = [];

                        var gender = $('#gender').val();
                        if (gender == 0) {
                            errors.push('To continue registering please choose a gender.');
                        }
                        var seeking = $('#seeking').val();
                        if (seeking==0) {
                            errors.push('To continue registering please select what you are looking for on OBC.');
                        }
                        var zip = $('#zip').val();
                        if (zip == '') {
                            errors.push('To continue registering please enter a valid zip code.');
                        }
                        if (errors.length > 0) {
                            $('#signup-error p').html(errors.join('<br>'));
                            $(document).scrollTop($('#signup-error').offset().top - 20);
                            $('#signup-error').slideDown();

                            return false;
                        }
                        var ageMin = $('#age-min').val();
                        var ageMax = $('#age-max').val();
                        if (ageMin == '' || isNaN(ageMin) || ageMin < 18) {
                            $('#age-min').val('18');
                        }

                        if (ageMax == '' || isNaN(ageMax) || ageMax >99) {
                            $('#age-min').val('99');
                        }

                        if (ageMin > ageMax) {
                            $('#age-max').val(ageMin);
                        }
                    });
                });
            </script>

