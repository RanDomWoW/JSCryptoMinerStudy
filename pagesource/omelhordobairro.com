<script type=text/javascript>
	var $window=$(window);
	$window.load(function()
	{
		$(".tooltip-help").tooltip({placement:"bottom"});
		$("#form-busca").on("submit",function(a){
			a.preventDefault();
			var b = $("#input-busca").val();
			console.log(b);
			if(b){
				location.href="/busca?q="+b
			}
			else{
				$("#input-busca").focus();
			}
		})
	});
	function createSelectEstado(b,a){$("#selEstado").select2({placeholder:"Escolha o estado",id:"id_estado",data:{results:b,text:"estado"},formatResult:function(c){return c.estado},formatSelection:function(c){return c.estado}}).on("change",function(c){alterSelectCidade(c.val)}).select2(a?"open":"close")}function alterSelectEstado(a){$.ajax({type:"GET",url:"https://www.omelhordobairro.com/buscaestados/"+a,dataType:"json"}).done(function(b){createSelectEstado(b.items)})}createSelectEstado([],false);alterSelectEstado(1);function createSelectCidade(b,a){$("#selCidade").select2({placeholder:"Escolha a cidade",id:"id_cidade",data:{results:b,text:"cidade"},formatResult:function(c){return c.cidade},formatSelection:function(c){return c.cidade}}).on("change",function(c){alterSelectBairro(c.val)}).select2(a?"open":"close")}function alterSelectCidade(a){$.ajax({type:"GET",url:"https://www.omelhordobairro.com/buscacidades/"+a,dataType:"json"}).done(function(b){createSelectCidade(b.items,true);createSelectBairro([],false)})}createSelectCidade([],false);function createSelectBairro(b,a){$("#selBairro").select2({placeholder:"Escolha o bairro",id:"id_bairro",data:{results:b,text:"bairro"},formatResult:function(c){return c.bairro},formatSelection:function(c){return c.bairro}}).on("change",function(c){location.href="/"+c.added.portal_url}).select2(a?"open":"close")}function alterSelectBairro(a){$.ajax({type:"GET",url:"https://www.omelhordobairro.com/buscabairros/"+a,dataType:"json"}).done(function(b){createSelectBairro(b.items,true)})}createSelectBairro([],false);</script>
