<script>
	var typingTimer;
	var doneTypingInterval = 500;
	var oldWindowWidth = 0;
	var searching = false;
	var xhrSearching = false;
	$( document ).ready(function() {
		var oldWindowWidth = $(window).innerWidth();
		$('#searchBox').keyup(function(){
			clearTimeout(typingTimer);
			if ($(this).val().length > 0){
				typingTimer = setTimeout(searchGames, doneTypingInterval);
			}else{
				typingTimer = setTimeout(clearSearchGames, doneTypingInterval);
			}
		});

		$('#searchBox').keydown(function(){ clearTimeout(typingTimer); });

		// Nasconde la finestra di ricerca se si clicca altrove
		$( 'body' ).not( '#searchGameResultCont' ).click(function(e){
			parent = $(e.target);
			k = 0;
			try {
				while(!$.isWindow( parent ) &&  parent.length > 0 && parent.parent().length >0 && parent.prop('id') != 'searchGameResultCont' && k < 5 ) {
					//console.log(parent);
					parent = parent.parent();
					k++;
				}

				if ( ($('#searchGameResultCont') && parent.prop('id') != '#searchGameResultCont') || $.isWindow( parent )){
					$( "#searchGameResultCont" ).toggle("drop", function(){ $( "#searchGameResultCont" ).empty(); })
				}
			}catch(err) {
				$( "#searchGameResultCont" ).toggle("drop", function(){ $( "#searchGameResultCont" ).empty(); })
			}
		});
		/*
		$('.libd-submenu').mouseover(function() {
			$( "#searchGameResultCont" ).toggle("drop", function(){ $( "#searchGameResultCont" ).empty(); });
		});
		*/

		// Vai alla pagina ricerca premendo il tasto enter
		$('#searchBox').keypress(function(e){
			var keyString = $(this).val();
			if ( keyString && e.which == 13 ){
				keyString = keyString.trim();
				if ( !keyString ){ return false; }
				keyString = encodeURI( keyString );
				window.location.href = '/store/search?type=games&key='+keyString+'&page=1';
			}
		});

		$('#iconsearch').click(function(e){
			var keyString = $('#searchBox').val();
			keyString = keyString.trim();
			if ( !keyString ){ return false; }
			window.location.href = '/store/search?type=games&key='+keyString+'&page=1';
		});

		$(window).resize(function() {
			if ($(window).innerWidth() < 1023 && oldWindowWidth != $(window).innerWidth()) {
				$("#searchGameResultCont").toggle("drop", function(){ $(".search-game-result-cont").empty(); });
				$('body').removeClass('sidebarmenu-open');
				$('#libdLeft').removeClass('libd-left-active');
			}
			oldWindowWidth = $(window).innerWidth();
		});

	});

	// Vai alla pagina ricerca cliccando sulla lente di ingrandimento o su more results
	$( document ).on( 'click', '.btn-search-games, .see-more a', function(e){
		e.preventDefault();
		var keyString = $( '#searchBox' ).val().trim();
		if ( !keyString ){ return false; }
		keyString = encodeURI( keyString );
		window.location.href = '/store/search?type=games&key='+keyString+'&page=1';
	});

	// Ricerca ajax
	function searchGames(){
		if ( $(window).width() < 480 ){
			return false;
		}

		var searchInputString = "";
		if ($("#searchBox").val() != ""){ searchInputString = $("#searchBox").val().trim(); }

		if (searching && xhrSearching !== false) {
			xhrSearching.abort()
		}
		searching = true;
	    xhrSearching = $.ajax({
			type: "GET",
			url: "/store/searchGamesProductPage",
			data: { inputString: searchInputString },
			dataType: "json",
			beforeSend: function(xhr, opts){},
			success: function(data,textStatus){

				if (data != "no_results") {
					$("#searchGameResultCont").empty().html(data["html_games_list"]);
				}else {
					$("#searchGameResultCont").html("Sorry, can\'t find the game you are looking for...");
				}
				if ($("#searchGameResultCont").is(":hidden")){ $("#searchGameResultCont").toggle("drop"); }
				searching=false;
			},
			error: function (xhr, ajaxOptions, thrownError){searching=false;},
			complete: function(){searching=false;}
		});


	}

	// Pulisce la ricerca
	function clearSearchGames(){
		$("#searchBox").val("");
		if ( $("#searchGameResultCont").is(":visible") ){
			$("#searchGameResultCont").toggle("drop", function(){ $(".search-game-result-cont").empty(); })
		}
	}


jQuery(function($) {

	$(window).resize(function() {
		wh = $('#resp3').css('display');
		if (wh == 'none') {
			$('body').removeClass('sidebarmenu-open');
		}
		hg0 = 0;
		$('#libdContainer > ul').each(function() {

			hg0 += $(this).height();
		});
		if ($('#libdLeft').get(0).scrollHeight > $('#libdLeft').height() || $('#libdContainer').height() > $('#libdLeft').height()-70 ) {
			$('.libd-box-social').addClass('social-concat');
		} else {
			$('.libd-box-social').removeClass('social-concat');
		}
		$('#libdLeft').height($(window).innerHeight());
	});

	if ($('#libdLeft').get(0).scrollHeight > $('#libdLeft').height() || $('#libdContainer').height() > $('#libdLeft').height()-70 ) {
		$('.libd-box-social').addClass('social-concat');
	} else {
		$('.libd-box-social').removeClass('social-concat');
	}
	$('#libdLeft').height($(window).innerHeight());

	$(window).resize(function() {
		wh = $('#resp3').css('display');
		if (wh == 'none') {
			$('#libdLeft').height($(window).innerHeight());
		} else {

		}
		$('#libdLeft').css({'overflow-x':'hidden','overflow-y':'auto'});
		// rimuovo i submenu
		$('body').removeClass('bodyminheigth');
		$('.libd-right').css('position','relative');
		$('#libdLeft').removeClass('libd-left-active');
		$('#libdContainer ul>li').removeClass('libd-active');
		$('#backgroundModal').animate({'opacity':'0'}, 100, function() {
			$('#backgroundModal').remove();
		});
		$('.libd-submenu-ul').each(function() {
			$(this).slideUp();
		});
		$('#libdContainer ul  ul').css({'width':''});
		$('#libdContainer ul  ul > li').css({'height':'','width':'' });

	});
	$('#libdLeft').css({'top':"0px"});
	$('#libdContainer ul ul').first().css({'top':"0px"});
	// chiudo il menu quando si clicca qualsiasi cosa
	$('#libdContainer a').click(function() {
		if ($('#libdBGCloseResponsive').length > 0) {
			if (!$(this).parent().hasClass('libd-submenu')) {
				$('body').removeClass('sidebarmenu-open');
				$('#libdLeft').removeClass('libd-left-active');
			}
		}
	});
	// closeMenu

	$('#libdBGCloseResponsive').click(function() {
		$('body').removeClass('sidebarmenu-open');
		$('#libdLeft').removeClass('libd-left-active');
	});

	$('#libdContainer ul>.libd-submenu>a').click(function(e) {
		var $this = $(e.currentTarget);
		if ($this.hasClass('browse-submenu-item') && $(window).width() < 1007){ location.href = '/browse'; return }
		wh = $('#resp3').css('display');
		$('#libdContainer ul ul').css({'width':''});
		$('#libdContainer ul  ul > li').css({'height':'','width':'' });
		$('#libdLeft').css({'overflow-x':'hidden','overflow-y':'auto'});

		if (wh == 'block') {
			$(this).parent().children('ul').css({'width':'', 'float':'none'});
			$curr = $(this).parent().children('ul');
			$('.libd-submenu-ul').each(function() {
				if ($curr.first()[0] != this) {
					$(this).slideUp();
				}
			});
			$curr.slideToggle(400, function() {
				if ($('#libdLeft').get(0).scrollHeight > $('#libdLeft').height() || $('#libdContainer').height() > $('#libdLeft').height()-70 ) {
					$('.libd-box-social').addClass('social-concat');
				} else {
					$('.libd-box-social').removeClass('social-concat');
				}
				$('#libdLeft').height($(window).innerHeight());

			});

		} else {
			if ($('#backgroundModal').length == 0) {
				$('body').removeClass('bodyminheigth');
				$('.libd-right').css('position','relative');
				$('#libdLeft').css({'overflow-x':'visible','overflow-y':'visible'});
				$('#libdContainer ul > li').removeClass('libd-active');
				$bg = $('<div id="backgroundModal"></div>');
				bundles = $(this).parent().find('ul > li').length;
				mwh = ($(window).width()-270)/bundles;
				mwrow = 1;
				numberBundlesRow = 0;

				while((mwh > 320 || mwh < 240) && numberBundlesRow < bundles) {
					numberBundlesRow++;
					mwh = ($(window).width()-270)/numberBundlesRow;
				}

				width = numberBundlesRow*mwh;
				mwrow = Math.ceil(bundles/numberBundlesRow);

				if (width == 0) {
					width = $(window).width()-260;
				}
				if (mwh > 320) mwh = 320;
				if (mwh < 200) mwh = 200;

				mwrow2 = Math.max(mwrow,1);
				if (mwrow2 <1 || mwrow2 >50) {
					mwrow2 = 1;
				}
				mheight =(mwh*186)/270;
				if ($(this).parent().find('.libd-our-game-info').length > 2) {
					mheight += 20;
				}

				$(this).parent().children('ul').css({'display':'','top':-mheight*mwrow2,'width':width});
				$(this).parent().children('ul').stop();

				if (!$this.hasClass('browse-submenu-item')){ 
					$('#libdContainer ul  ul > li').css({'height':mheight, 'width':mwh});
				}
				
				$(this).parent().children('ul').animate({'top':0},500);

				$('body').prepend($bg);
				$bg.animate({'opacity':'1'},100);
				$bg.click(function() {
					$('body').removeClass('bodyminheigth');
					$('.libd-right').css('position','relative');
					$('#libdLeft').css({'overflow-x':'hidden','overflow-y':'auto'});
					$('#libdContainer ul>li').removeClass('libd-active');
					$('#libdContainer ul  ul').css({'width':''});
					$('#libdContainer ul  ul > li').css({'height':'','width':'' });
					$(this).animate({'opacity':'0'},100, function() {
						$(this).remove();
					});
				});

				if ($('.libd-right').height() < 1600) {
					$('.libd-right').css('position','fixed');
				}

				$('body').addClass('bodyminheigth');
				$(this).parent().addClass('libd-active');

