    jQuery.noConflict();
	var time=1;
	var count=jQuery('.switch-tab a').length;
	function testshowpic(n,num)
	{
	
		if(num==1){clearInterval(id);}
		if(n>count)
		{
			n=1;
		}
		time=n;
		for(var p=1; p<=count;p++)
		{
			if(p==n)
			{	
				jQuery("#pic"+p).hide().fadeIn('slow');
				jQuery("#pic"+p).css("display","block");
                jQuery("#num"+p).attr("class","on");
				//jQuery("#num"+p).attr("class","on event-item");
				jQuery("#dot"+p).attr("class","current");
			}
			else
			{
				jQuery("#pic"+p).css("display","none");
                jQuery("#num"+p).attr("class","off");
				//jQuery("#num"+p).attr("class","off  event-item");
				jQuery("#dot"+p).attr("class"," ");
			}
		}
	}
	if (count > 1) {
		var id=window.setInterval("testshowpic(time+1)",7000);
	}
	function startpic()
	{
		id=window.setInterval("testshowpic(time+1)",7000);
