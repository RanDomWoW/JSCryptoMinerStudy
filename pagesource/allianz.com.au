<script>
$(function(){

      // set the default listener for the quote button
    $('#get-quote-mobile button').on('click', function() {
	addGAEvent ($('#get-quote-mobile select ').val());
		
        location.href = getLinkUrl( $('#get-quote-mobile select ').val(), 'quote' );
    });

    $('#get-quote-mobile .retrieve-quote').on('click', function() {
        location.href = getLinkUrl( $('#get-quote-mobile select').val(), 'retrieve' );
    });

$("#get-quote-mobile select").change(function(e) {
    var newVal = $(this).val();
	if (newVal === "Life Insurance") {
		$('.container-header-bg').css("background-image", "url(/images/internet/allianz-au/banners/AL0058_Mobile_Banner_LIFE_HOME_728x253.jpg)");
/*		$("#get-quote-mobile button.btn.full-width").html('<span class="icon-medium white-arrow pull-right"></span>Request a quote');
        $("#get-quote-mobile button.retrieve-quote").hide();*/
    } 
	
	else if (newVal === "Car Insurance") 
		$('.container-header-bg').css("background-image", "url(/images/internet/allianz-au/banners/AL0058_Mobile_Banner_CAR_728x253_v2.jpg)");

	else if (newVal === "Home Insurance") 
		$('.container-header-bg').css("background-image", "url(/images/internet/allianz-au/banners/AL0058_Mobile_Banner_HOME_728x253_v3.jpg)");
	
	else if (newVal === "Landlord Insurance") 
		$('.container-header-bg').css("background-image", "url(/images/internet/allianz-au/banners/AL0058_Mobile_Banner_HOME_landlords_728x253.jpg)");
	
	else if (newVal === "Boat Insurance") 
		$('.container-header-bg').css("background-image", "url(/images/internet/allianz-au/banners/banner-mobile-boat.jpg)");
	
	else {
		$('.container-header-bg').css("background-image", "url(/images/internet/allianz-au/banners/AL0058_Mobile_Banner_GENERIC_728x253_v4.jpg)");  
		$("#get-quote-mobile button.btn.full-width").html('<span class="icon-medium white-arrow pull-right"></span>Get a quote');
        $("#get-quote-mobile button.retrieve-quote").show();
    }
});

 function getLinkUrl(selectedProduct, linkType) {
 	// need to pass any params on as well
 	var sQuery = window.location.search;

 	var quoteUrlString = '';
 	//var sRedirectPrefix = '/internet/redirect.nsf/r?openagent&URL=';
	var sRedirectPrefix = '/redirect/';
 	var retrieveUrlString = sRedirectPrefix + 'retrieve-quote';

 	switch (selectedProduct) {
 		case 'Home Insurance':
 			quoteUrlString = sRedirectPrefix + 'home-quote-mobile';
 			break;
 		case 'Travel Insurance':
 			quoteUrlString = sRedirectPrefix + 'travel-quote-mobile';
 			break;
 		case 'Life Insurance':
 			//quoteUrlString = '/life-insurance/contact?open';
                                quoteUrlString = '/redirect/life-insurance-quick-quote';
 			retrieveUrlString = sRedirectPrefix + 'life-ins-retrieve';
 			break;
 		case 'Boat Insurance':
 			quoteUrlString = sRedirectPrefix + 'boat-quote';
 			break;
 		case 'Small Business Insurance':
 			quoteUrlString = sRedirectPrefix + 'small-business-quote';
                                 break;
 		case 'Landlord Insurance':
 			quoteUrlString = sRedirectPrefix + 'landlord-quote';
 			break;
 		default: //Car Insurance
 			quoteUrlString = sRedirectPrefix + 'motor-quote-mobile';
 	}

 	if (sQuery != '') {
 		// if first char in query is a ?, change it to a &
 		if (sQuery.substr(0, 1) == '?') {
 			sQuery = '&' + sQuery.substr(1, sQuery.length - 1);
 		}
 		// append query on end of sRetrievalAgentUrl
 		retrieveUrlString += sQuery;
 		quoteUrlString += sQuery;
 	}

 	if (linkType == 'retrieve') {
 		return retrieveUrlString;
 	} else {
 		return quoteUrlString;
 	}
 }
	
	function addGAEvent (selectedProduct){
		
		var sLabel = '';
		
		switch (selectedProduct) {
            case 'Home Insurance':
                sLabel = selectedProduct;
                break;
            case 'Travel Insurance':
                sLabel = selectedProduct;
                break;
            case 'Life Insurance':
                sLabel = selectedProduct;
              //  quoteUrlString = '/life-insurance/contact';
             
                break;
            case 'Boat Insurance':
                sLabel = selectedProduct;
                break;
            case 'Small Business Insurance':
                sLabel = 'Small Business Insurance';
                break;
            default: //Car Insurance
                 sLabel = selectedProduct;
        }
		
		var trackers = ga.getAll();
	for (i = 0; i < trackers.length; i++) 
	{ga(trackers[i].get('name') + '.send', 'event', 'Main banner', 'Get a quote', sLabel);};

	}

});
</script>
