      $('.livechat-wrapper').hide();
      $('.chat-min').show();
    });

    $('.chat-min').click(function() {
      $('.chat-min').hide();
      $('.livechat-wrapper').show();
    });

    $('.livechat-button').click(function(e) {
      $('.livechat-wrapper').css('display', 'block');
      $('.livechat-links').css('display', 'none');
        if(app.isMobileUserAgent){
          e.preventDefault(); e.stopPropagation();
         return window.open($(".livechat-online").attr('href'));
      } else {
        $('.livechat-wrapper').draggable({
          scroll: false,
          containment: "window"
        });
      }
    });

    $('.header-menu-menu').click(function(e) {
      $('.livechat-wrapper').css('display', 'none');
      $('.livechat-links').css('display', 'block');
    });
  });


var cc = 0;
var intervalChat = setInterval( function(){
    cc++;
    if($(".livechat-online:visible").length){
        clearInterval(intervalChat);
        if(  typeof $.ui == "undefined" || typeof $.ui.draggable != "function" ){
          var script = document.createElement( 'script' );
          script.type = 'text/javascript';
          script.src = 'https://code.jquery.com/ui/1.12.1/jquery-ui.js';
          $("head").append( script );
        }
    }
    if(cc == 30){
      clearInterval(intervalChat);
    }
  }, 1000);



</script>
