            $(window).ready(function(){
                reCalcAd();
            });
            
            $(window).resize(function(){
            	reCalcAd();
            });

            $(window).scroll(function(){
            	reCalcAd();
            });

			function bannerShow(data, shouldDisplayLayer) {

			    function ad1() {

			        setTimeout(function() {
			            $('#shadowOverlay').fadeOut('slow');
			        }, 7000);

			        $('#homeAd').click(function(event) {
			            event.stopPropagation();
			        });

			        reCalcAd();
 							ga('send', 'event', 'Ads', 'displayed', 'Minor hover ad');

			    }

			    function ad2() {

			        $('body').addClass('minor').append('<div class="linker_wrapper" onclick="window.open(\'http://www.minor.ch\', \'_blank\');"></div>');

 							ga('send', 'event', 'Ads', 'displayed', 'Minor background ad');

			    }

			    var serverTime = data * 1000;
			    var nowTime = new Date(serverTime);

			    if (($.cookie('cookie_day_ad1') === null) || ($.cookie('cookie_day_ad1') === undefined) || $.cookie('cookie_day_ad1').length <= 0 ) {

			    	var cookie_day_ad_content = ["1", serverTime];
			    	$.cookie('cookie_day_ad1', cookie_day_ad_content, {expires: 365, path: '/'});


                    if(shouldDisplayLayer=='layer'){
                        ad1();
                    }else if(shouldDisplayLayer=='hompage'){
                        ad2();
                    }

			    } else {

			    	var cookie = $.cookie('cookie_day_ad1');
			    	var cookie_array = cookie.split(',');
			    	var cookie_displayed = parseInt(cookie_array[0]);
			    	var cookie_first = parseInt(cookie_array[1]);

			    	if (cookie_displayed < 3) {

				    	cookie_displayed++;
				    	var cookie_day_ad_content = [cookie_displayed, cookie_first];
				    	$.cookie('cookie_day_ad1', cookie_day_ad_content, {expires: 365, path: '/'});

                        if(shouldDisplayLayer=='layer'){
                            ad1();
                        }else if(shouldDisplayLayer=='hompage'){
                            ad2();
                        }

			    	} else {

				    	var cookieTime = new Date(cookie_first);
				    	var cookieDate = cookieTime.getDate();
				    	var serverDate = nowTime.getDate();

				    	if (cookieDate != serverDate) {

				    		$.cookie('cookie_day_ad1', null, {expires: -365, path: '/'});

				    	}

                        if(shouldDisplayLayer=='hompage'){
                            ad2();
                        }

			    	}

			    }

			}

			function reCalcAd() {

			    var adHeight = $('div#adtop-region').height();
			    var wWidth = $(document).width();
			    var yScroll = $(window).scrollTop();
			    var bannerW = $('#adRight').width();
			    var billboardH = $('#adBig').height();

			    if (adHeight > 1) {

			        adHeight += 15;

			        $('div#adtop-region').css({
			            'margin-bottom': '15px'
			        });

			    }


                // var changeAt = $('#container').offset().top;
			    var changeAt = 175;

			    if (changeAt < yScroll) {

			        $('div#adRight').css({
			            position: 'fixed',
			            right: ((wWidth - 930) / 2 - 29 - bannerW) + 'px',
			            left: 'auto',
			            top: '10px'
			        });

			    } else {

			        $('div#adRight').css({
			            position: 'absolute',
			            left: '959px',
			            top: (-billboardH) + 'px'
			        });

			    }

			}

