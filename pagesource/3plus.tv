            },{
                startDate : "22.10.2015,00:00:00",
                endDate : "28.10.2015,23:59:59",
                layerCampaign : 'layer'
            },{
                startDate : "29.10.2015,00:00:00",
                endDate : "04.11.2015,23:59:59",
                layerCampaign : 'hompage'
            },{
                startDate : "05.11.2015,00:00:00",
                endDate : "11.11.2015,23:59:59",
                layerCampaign : 'layer'
            },{
                startDate : "12.11.2015,00:00:00",
                endDate : "18.11.2015,23:59:59",
                layerCampaign : 'hompage'
            },{
                startDate : "19.11.2015,00:00:00",
                endDate : "25.11.2015,23:59:59",
                layerCampaign : 'layer'
            },{
                startDate : "22.12.2015,00:00:00",
                endDate : "26.12.2015,23:59:59",
                layerCampaign : 'layer'
            }];


            var getServerTimeUrl;
            if(document.URL.indexOf("dev")>=0){
                getServerTimeUrl = "http://dev.3plus.tv/current_date.php";
            }else if(document.URL.indexOf("www")>=0){
            	getServerTimeUrl = "http://www.3plus.tv/current_date.php";
            }else{
            	getServerTimeUrl = "http://3plus.tv/current_date.php"
            }
            var currentServerTime = null;
            $.ajax({
                url: getServerTimeUrl,
                type: "POST",
                cache: false,
                success: function (resp) {
                    currentServerTime = resp;
                    campagnIntervals.forEach(function(element, index, array){

                        var startTimeUnix = dateToTimestamp(element.startDate);
                        var endTimeUnix = dateToTimestamp(element.endDate);

                        if(currentServerTime > startTimeUnix && currentServerTime < endTimeUnix){
                            //element.campaign();
                            bannerShow(resp, element.layerCampaign);
                        }
                    });
               },
               error: function(resp){
               		console.log("There was an error while getting server time from "+getServerTimeUrl);
               		console.log(resp);
               }
            });



            function dateToTimestamp(nizString){
                var DateNiz = nizString.split(",");
                var DateParts = DateNiz[0].split(".");
                var DateTimeParts = DateNiz[1].split(":");

                var date = new Date(DateParts[2], (DateParts[1] - 1), DateParts[0], DateTimeParts[0], DateTimeParts[1], DateTimeParts[2]);

                return date.getTime()/1000;
            };
