	<script type="text/javascript">
		function getLoggedInUsername() {
			if ( typeof getLoggedInUsername.promise != 'undefined' ) {
				return getLoggedInUsername.promise;
			}

			if ( typeof getLoggedInUsername.username == 'undefined' ) {
				getLoggedInUsername.username = '';
			}
			var d = $.Deferred();
			if(getLoggedInUsername.username == '') {
				$.getJSON('/profile/username/')
					.done(function(data) {
						getLoggedInUsername.username = data.Username;
						d.resolve(getLoggedInUsername.username);
					}).fail(d.reject);
			} else {
				d.resolve(getLoggedInUsername.username);
			}
			if ( typeof getLoggedInUsername.promise == 'undefined' ) {
				getLoggedInUsername.promise = d.promise();
			}
			return getLoggedInUsername.promise;
		}

		$.when(getLoggedInUsername()).done(function(username) {
			if(username != '') {
				$('#login-register').html('<span class="hidden-phone">Eingeloggt als</span> <a href="/profile/subscription/">' + username + '</a> / <a href="/logout/">Logout</a>');
			}
		});
	</script>

	

