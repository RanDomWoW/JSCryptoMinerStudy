        //nextGateway
        $('.nextGateway').click(function () {
            showGatewayHandler(true, $(this));
        });
        
        //prevGateway
        $('.prevGateway').click(function() {
            showGatewayHandler(false, $(this));
        });

        //prev and next count handler
        function showGatewayHandler(isNext, $element) {
            var currentIndex = $gateways().filter('.selected').index();
            var maxIndex = $gateways().length - 1;
            var minIndex = 0;
            var newIndex = currentIndex;

            if (isNext) {
                if (maxIndex != currentIndex) {
                    newIndex = currentIndex + 1;
                    $gateways().hide().removeClass('selected');
                    $gateways().eq(newIndex).show().addClass('selected');
                }
            } else {
                if (minIndex != currentIndex) {
                    newIndex = currentIndex - 1;
                    $gateways().hide().removeClass('selected');
                    $gateways().eq(newIndex).show().addClass('selected');
                }
            }
            
            $gateway.val($gateways().eq(newIndex).data('gatewayid'));
        }
        
        var showBankGateways = function(selectedBankId) {
            $gateways().remove();
            var defaultGatewayId = 0;
            var itemCount = 0;
            for (var vgi in validGateways) {
                if (validGateways[vgi].BankId == selectedBankId) {
                    for (var gi in gateways) {
                        if (gateways[gi].Cod == validGateways[vgi].GatewayId) {
