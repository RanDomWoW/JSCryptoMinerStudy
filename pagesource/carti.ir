<script type="text/javascript">

    application.messageManagerModule = function() {

        var $lightBox = $('#lightBox'), $messageWrapper = $('.messageWrapper', $lightBox);

        $(function() {
            $('.lightBoxBg').css('opacity', 0.3);
            $('.closeBtn').click(function() { $lightBox.css('display', 'none'); });
        });

        var showMessage = function(message) {
            $lightBox.css('display', 'block');
            $('html, body').animate({ scrollTop: 0 }, 'fast');
            $messageWrapper.html('');
            $messageWrapper.append(message);
        };


        var showMessageList = function(messageList) {
            var message = "";
            for (var i = 0; i < messageList.length; i++) {
                message += messageList[i].message;
            }
            showMessage(message);
        };

        return {
            showMessageList: showMessageList,
            showMessage: showMessage
        };
    }();

</script>
