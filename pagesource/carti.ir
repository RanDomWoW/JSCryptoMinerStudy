                            $gatewaysWrapper.append($tempGateway);
                            break;
                        }
                    }
                    if (validGateways[vgi].IsDefault) {
                        defaultGatewayId = itemCount;
                    }
                    itemCount++;
                }
            }
            $gateways().eq(defaultGatewayId).show();
            $('.nextGateway').click();
        };

        return {
            start: start,
            showBankGateways: showBankGateways
        };
    })();

