            <script type="text/template">
                {% set price = prices[currency].unitPrice %}
{% set salePrice = prices[currency].salePrice | default(null) %}
{% set colors = attributes.colors %}
{% set labels = attributes.labels %}
{% set brand = attributes.brand %}

<div class="m-product-tile">
    <a href="{{ url }}" class="m-product-tile__link--nohover js-link-track js-tag-product-recommendation-click-trigger"
       data-component-product-sku="{{ sku }}">
        <div class="m-product-tile__img-wrapper">
            <span class="m-product-tile__img-initial">
                <img class="lazyload" data-src="{{ img }}" alt="{{name}}" data-popup-id="{{ productId }}">
            </span>

            <div class="labels">
                {% for label in labels %}
                    <span class="a-label a-label--{{ label.type }}">{{ label.text }}</span>
                {% endfor %}
            </div>
        </div>
        {% spaceless %}
            <svg class="a-icon m-product-tile__brand">
                <use xlink:href="#{{ brand }}"></use>
            </svg>
        {% endspaceless %}

        <h3 class="small">{{ title }}</h3>
        <div class="m-product-tile__product-prices">{% spaceless %}
                <span class="a-price">
                {% if salePrice and salePrice != price %}
                    <del>{{ price }}&nbsp;{{ currencyText }}</del>
                    <ins>{{ salePrice }}&nbsp;{{ currencyText }}</ins>
                {% else -%}
                    {{ price }}&nbsp;{{ currencyText }}
                    {#{{ (price / 100) | number_format(2,',','.') }}{{ currencyText }}#}
                {% endif -%}
            </span>
            {% endspaceless %}</div>
    </a>
    <div class="m-product-tile__product-colors">
        {% for color in colors %}
            <a href="{{ color.url }}" class="a-color" style="background-color: {{ color.code }};"
               title="{{ color.name }}" data-component="color" data-color="{{ color.code }}">
                <svg class="a-icon">
                    <use xlink:href="#check"></use>
                </svg>
            </a>
        {% endfor %}
    </div>
</div>

            </script>
