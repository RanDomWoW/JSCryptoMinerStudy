<script type='text/javascript'>
    var pxInicial = '';
    function RecorrerChildNodes(objeto, encoger) {

        if (objeto.childNodes) {
            if (objeto.childNodes.length > 0) {
                for (i = 0; i < objeto.childNodes.length; i++) {
                    if (typeof (objeto.childNodes[i].id) != 'undefined') {
                        var idString = objeto.childNodes[i].id;

                        //Se valida si es el iframe que pinta google.
                        if (idString.indexOf('google') != -1 && idString.indexOf('hidden') == -1) {
                            if (pxInicial == '')
                                pxInicial = ObtenerPxInicialFrame(idString);

                            //Se valida el navegador
                            validarPosition(idString);
                            if (encoger) {
                                document.getElementById(idString).style.height = '60px';
                            }
                            else {

                                document.getElementById(idString).style.height = pxInicial;
                            }


                        }

                    }
                }
            }
        }
    }

    function ObtenerPxInicialFrame(stringId) {
        if (document.getElementById(stringId) != null) {
            return document.getElementById(stringId).height.toString() + 'px'
        }
        else
            return ObtenerPxInicialFrame(stringId);


    }

    function validarPosition(idString) {
        if (navigator.appName == 'Microsoft Internet Explorer') {
            //Se asigna la propiedad correcta para que el banner se monte en las demas cosas
            document.getElementById(idString).style.position = 'absolute';

        } else {
            document.getElementById(idString).style.position = 'relative';
        }
    }
</script>

