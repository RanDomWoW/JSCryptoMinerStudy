	function EnviarDatos()
	{
		var FrmIngresarNewHome = document.getElementById('FrmIngresarNewHome');
		//El input de usuario tienen nombre=identificacion, id=usuario ???
		var ASPusr = document.forms['FrmIngresarNewHome'].usuario.value
		var ASPpsw = document.forms['FrmIngresarNewHome'].Password.value

		if (ASPusr!="" & ASPpsw!="")
		{
			var frmIdentificacionASP = document.getElementById('frmIdentificacionASP');
			var urlDotNet = '//www.losconstructores.com/proveedores/identificacionasp.aspx?Email=' + ASPusr + '&Clave=' + ASPpsw;
			frmIdentificacionASP.src = urlDotNet;
		}
		if(document.getElementById("BotonEnviar").disabled != true)
		{
			FrmIngresarNewHome.submit();
		}
			
		document.getElementById("BotonEnviar").disabled = true;
	}
	
	function createCookie(name,value,days) {
		if (days) {
			var date = new Date();
			date.setTime(date.getTime()+(days*24*60*60*1000));
			var expires = "; expires="+date.toGMTString();
		}
		else var expires = "";
		document.cookie = name+"="+value+expires+"; path=/";
	}
	
	function entsub(e,myform) {
        if(myform!=null)
        {
         var keyCode=(document.all) ? e.keyCode : e.which;
            
         if (keyCode == 13)
            if (myform.name == "SE_CONTACTENOS_001")
            {
                submitform();
            }
            else if (myform.name == "FrmBuscar")
            {
                submitformbuscar();
            }
			 else if (myform.name == "FrmIngresarNewHome")
            {
				if(document.getElementById("BotonEnviar").disabled != true)
				{
					myform.submit();
				}
				document.getElementById("BotonEnviar").disabled = true;
            }
            else
            {
                myform.submit();
            }
            
        else
            return true;   
        }
        
    }
