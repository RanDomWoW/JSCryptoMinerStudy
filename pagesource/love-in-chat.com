	<% photos.private.each(function(ph){ %>

		<a class="one" data-photo-id="<%= ph.id %>" href="#">
			<img src="<%= url %>/_files/photo/<%= ph.path %>">
		</a>


	<% }) %>
	<div class="clear"></div>

	</div>

	<% } %>




</div>

<a class="addMe disabled" href="#">Добавить фото в ленту</a>

