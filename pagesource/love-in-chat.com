<script id="tpl_bansList" type="text/tpl">
<h2>Список банов</h2>



<% _.each(list, function(b, key){ %>

<div class="one">

	<b><%= b.data.who %></b>
	забанил(а)
	<b><%= b.data.target %></b>
	до
	<%= b.data.expireTime %>
	<br/>
	по ключу
	<b>
		<%= b.keyType %>:
		<%= key %>
	</b>

	





	<% if(c().get('userData').isModer()){ %>
	<br/>
	<a href="#" class="removeBan" data-key="<%= key %>" data-nick="<%= b.data.target %>">Разбанить</a>
	<% } %>

</div>


<% }) %>


</script>

