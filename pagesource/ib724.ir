	<script type="text/javascript">
	    $(document).ready(function () {
	        $('#fullpage').fullpage({
	            sectionsColor: ['#C63D0F', '#1B8FBC', '#688961'],
	            css3: true,
	            slidesNavigation: true,
	            navigation: true,
	            navigationPosition: 'right',
	            scrollingSpeed: 500,
	            continuousVertical: true,
	            afterLoad: function (anchorLink, index) {

	                //section 1

	                if (index == 1) {
	                    $('#section0').find('h1').delay(500).animate({
	                        bottom: '0%'
	                    }, 1000, 'easeOutBounce');
	                    $('#section0').find('p').delay(2000).animate({
	                        left: '0%'
	                    }, 1500, 'easeOutExpo');
	                    //-----------
	                    $('#section1').find('p').delay(500).animate({
	                        right: '130%'
	                    }, 1500, 'easeOutExpo');
	                    $('#section2').find('.intro').first().delay(500).animate({
	                        left: '130%'
	                    }, 1500, 'easeOutElastic');
	                }

	                //section 2
	                if (index == 2) {
	                    //moving the image
	                    $('#section1').find('p').delay(500).animate({
	                        right: '0%'
	                    }, 1500, 'easeOutExpo');
	                    //-----------
	                    $('#section0').find('h1').delay(500).animate({
	                        bottom: '250%'
	                    }, 1500, 'easeOutExpo');
	                    $('#section0').find('p').delay(500).animate({
	                        left: '130%'
	                    }, 1500, 'easeOutExpo');
	                    $('#section2').find('.intro').first().delay(500).animate({
	                        left: '130%'
	                    }, 1500, 'easeOutElastic');
	                }

	                //section 3
	                if (index == 3) {
	                    $('#section2').find('.intro').first().delay(200).animate({
	                        left: '0%'
	                    }, 1500, 'easeOutElastic');
	                    //-----------
	                    $('#section0').find('h1').delay(0).animate({
	                        bottom: '250%'
	                    }, 1500, 'easeOutExpo');
	                    $('#section0').find('p').delay(500).animate({
	                        left: '130%'
	                    }, 1500, 'easeOutExpo');
	                    $('#section1').find('p').delay(500).animate({
	                        right: '130%'
	                    }, 1500, 'easeOutExpo');
	                }
	            }
	        });
	    });
	</script>

