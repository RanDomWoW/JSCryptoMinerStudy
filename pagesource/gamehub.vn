        //Zero the idle timer on mouse movement.
        $(this).mousemove(function (e) {
            idleTime = 0;
        });
        $(this).keypress(function (e) {
            idleTime = 0;
        });



        //$(document).snowfall({round : true,flakeCount : 500, maxSpeed : 10, minSize: 5, maxSize:8}); // add rounded
    });

    function timerIncrement() {
        idleTime = idleTime + 1;
