    <div class="section bg-grey">
    <div class="container">
        <div class="section-header">
            
                <h2 class="h-beta overscore">Real stories</h2>
            
            <p class="lead">Hear from students and discover what it would be like to live, work and study in New Zealand. Find information for parents and hear from graduates forging successful careers around the world.</p>
        </div>
    </div>
    <div ng-controller="HomeStoriesDataController" ng-init="getStories(222)">
        <div class="stories-carousel ng-cloak" ng-class="{ loading: state.filter.loading }" stories-carousel data="slides">
            <div class="stories-carousel-wrapper">
                <a class="btn stories-carousel-prev-btn" ng-click="prev()" ng-show="state.activeIndex > 0"></a>
                <a class="btn stories-carousel-next-btn" ng-click="next()" ng-show="state.activeIndex < state.total - 1"></a>
                <div class="stories-container">
                    <div class="swiper-container stories-swiper-container">
                        <div class="swiper-wrapper stories-swiper-wrapper">
                            <div class="swiper-slide stories-slide" ng-repeat="slide in slides">
                                <div class="boxes" stories>
                                    <div class="box-sizer"></div>
                                    <div class="box-container" ng-repeat="story in slide">
                                        <a ng-href="{{story.StudentStoryPage.LocaleLink}}" class="box" ng-class="{ 'box': (story.Size == 'Small'), 'box-large': (story.Size == 'Large'), 'box-portrait': (story.Size == 'Portrait')}" story-box>
                                            <div class="box-overlay">
                                                <div class="box-cover">
                                                    <img ng-if="story.Size == 'Small'" ng-src="{{ story.StudentStoryPage.StudentSearchSmallThumbnail }}" />
                                                    <img ng-if="story.Size == 'Large'" ng-src="{{ story.StudentStoryPage.StudentSearchLargeThumbnail }}" />
                                                    <img ng-if="story.Size == 'Portrait'" ng-src="{{ story.StudentStoryPage.StudentSearchPortraitThumbnail }}" />
                                                </div>
                                                <div class="box-overlay-content">
                                                    <h3 class="box-overlay-title-beta" ng-bind-html="story.StudentStoryPage.Title"></h3>
                                                    <p ng-bind-html="story.DetailText"></p>
                                                </div>
                                            </div>
                                            <div class="box-content-wrapper">
                                                <div class="box-content">
                                                    <img ng-src="{{ story.StudentStoryPage.StudentSearchSmallThumbnail }}" class="visible-xs" />
                                                    <img ng-if="story.Size == 'Small'" ng-src="{{ story.StudentStoryPage.StudentSearchSmallThumbnail }}" class="hidden-xs" />
                                                    <img ng-if="story.Size == 'Large'" ng-src="{{ story.StudentStoryPage.StudentSearchLargeThumbnail }}" class="hidden-xs" />
                                                    <img ng-if="story.Size == 'Portrait'" ng-src="{{ story.StudentStoryPage.StudentSearchPortraitThumbnail }}" class="hidden-xs" />
                                                    <div ng-class="{ 'play-icon': story.StudentStoryPage.Video }" ></div>
                                                </div>
                                                <div class="box-caption">
                                                    <h4 ng-if="story.StudentStoryPage.InstitutionType" class="feature-tag ng-class: story.StudentStoryPage.InstitutionType | lowercase;" ng-bind="story.StudentStoryPage.InstitutionType"></h4>
                                                    <h3 class="box-caption-title-beta" ng-bind-html="story.StudentStoryPage.Title"></h3>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <ol class="stories-carousel-indicators ng-cloak" ng-show="state.total > 1">
                <li ng-class="{ active: state.activeIndex == $index }" ng-click="goto($index)" ng-repeat="slide in slides track by $index"></li>
            </ol>
        </div>
    </div>
</div>

