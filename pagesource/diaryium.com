				slideshowSpeed: 7000,
				start: function(slider) {
					currSlide = (typeof slider.currentSlide === 'undefined') ? 1 : slider.currentSlide + 1;
					var currSlideWidth = (typeof slider.currentSlide === 'undefined') ? $('html').outerWidth() : slider.itemW;
					changeLogInBlockBack(currSlide, currSlideWidth);
				},
				after: function(slider) {
					currSlide = (typeof slider.currentSlide === 'undefined') ? 1 : slider.currentSlide + 1;
					var currSlideWidth = (typeof slider.currentSlide === 'undefined') ? $('html').outerWidth() : slider.itemW;
					changeLogInBlockBack(currSlide, currSlideWidth);
				}
			});

			$(window).resize(function() {
				changeLogInBlockBack(currSlide, $('html').outerWidth());
			});

			function changeLogInBlockBack(IMAGE_ID, IMAGE_width) {
				var imageSrc = $('#flexslider li:nth-child(' + IMAGE_ID + ') img').attr('src');
				var positionLeft = $('#login_register_block').offset().left;
				$('#login-ice-back').attr('style', 'background-image: url("' + imageSrc + '"); background-size: ' + IMAGE_width + 'px auto; background-position: -' + positionLeft + 'px -16px');
			}
		});
	</script>
</div>
</div>
</div>
</div>
</div>
</div>
</body>
</html>