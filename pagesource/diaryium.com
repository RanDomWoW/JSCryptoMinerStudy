<script>
	var blockTitleWrapper = $('.menu_sub');
	var menuBlocksWithSubsCount = blockTitleWrapper.length;
	(function() {
		/*CLICKS ON MENU BLOCKS TITLES*/
		blockTitleWrapper.on('click', function(e) {
			$(this).toggleClass('opened');
			if($(this).hasClass('opened')) {
				unpinPinMenuBlock($(this).attr('id'), true);
                                var el = e ? e.target : window.event.srcElement;

                                if ($(this)['context']['id'] == 34 || $(this)['context']['id'] == 37) {
                                    var url = el.closest('div').querySelector('li:first-child a').getAttribute('href');
                                    $(location).attr('href', url);
                                } else if($(this)['context']['id'] == 45) {
                                    var url = el.closest('div').querySelector('li:nth-child(2) a').getAttribute('href');
                                    $(location).attr('href', url);
                                }
			} else {
				unpinPinMenuBlock($(this).attr('id'), false);
			}
		});
		/*IF EXIST COOKIE AND MENU BLOCKS WITH SUBMENU*/
		if (document.cookie.indexOf("activesMenuBlocks") >= 0 && menuBlocksWithSubsCount > 0) {
			var cookie = getCookieUnlimited('activesMenuBlocks');
			if (cookie != "") {
				var arr = cookie.split(',');
				for (var i = 0; i < arr.length; i++) {
					blockTitleWrapper.each(function(index, element){
						if($(element).attr('id') == arr[i]) {
							$(element).addClass('opened');
						}
					});
				}
			}
		} else {
			setCookieUnlimited('activesMenuBlocks', '', 365);
		}
	})();
</script>
