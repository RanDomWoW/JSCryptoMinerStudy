    //----------------------------------------------------  < Ajax History > ----------------------------------------------------
    function saveLocation(href, loadTo){
        $("#AjxHistory input:checked").nextAll().remove();
        $("#HisFwdBtn").addClass('ui-state-disabled');
        if($("#AjxHistory input:checked").val() == href) return;
        $("#HisBckBtn").removeClass('ui-state-disabled');
        $("#AjxHistory").append("<input name='hisItem' type='radio' loadTo='"+loadTo+"' value='"+href+"'/>");
        $("#AjxHistory input:last-child").attr('checked',true).addClass('loadingState');
        window.location.hash = '#m_' + $("#AjxHistory input").length;
    }
    //----------------
    function loadLocation(n){
        var obj;
        if(n){
                obj = $("#AjxHistory input:nth-child("+n+")");
                if(obj.hasClass("loadingState")){
                        obj.removeClass("loadingState");
                        return false;
                }
        }else   
                obj = $("#AjxHistory input:checked");
        var url = obj.val();
        if(!url) return false;
        var loadTo = obj.attr('loadTo');
        if(loadTo=="#tabs-home"){
                //alert($("#tabs-home ul li:nth-child(3) a").attr('href'));
                //$("#tabs-home ul li:nth-child(3)").trigger('click');
                        //$(loadTo).tabs("select", url);
        }else
                $(loadTo).load(url + "&print=5");
        
    }
    //----------------
    function ajxBack(){
        if(!$("#AjxHistory input:checked").prev().attr("checked", "checked").length) return;
        loadLocation();
        if($("#AjxHistory input:checked").is(":first-child"))
                $("#HisBckBtn").addClass('ui-state-disabled');
        $("#HisFwdBtn").removeClass('ui-state-disabled');
    }
    //----------------
    function ajxForward(){
        if(!$("#AjxHistory input:checked").next().attr("checked", "checked").length) return;
        loadLocation();
        if($("#AjxHistory input:checked").is(":last-child"))
                $("#HisFwdBtn").addClass('ui-state-disabled');
        $("#HisBckBtn").removeClass('ui-state-disabled');
    }

    $(window).hashchange( function(){
        var n = location.hash.replace( /^#m_/, '' );
        if(!n) n=1;
        loadLocation(n);
    });
