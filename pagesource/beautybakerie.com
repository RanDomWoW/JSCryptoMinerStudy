    $(document).ready(function(){
      //  rotating text banner

      var arrayTextLinks =
          ["Free Shipping on US Orders $50+", "$10 Expedited Shipping on Intl Orders $75+", "Worldwide Shipping"];
           var numCount = 1;

           setInterval(function(){
           $(".toggleParent").animate({
           //height:"0px",
           opacity:0
           },1000, "linear",function(){
           //cycle to next text display in array
           $(".toggleText").html(arrayTextLinks[numCount]);
      $(".toggleParent").animate({
        //height:"38px",
        opacity:1
      },1000,function(){
        //done with animation. increment value to next array index
        if(numCount != arrayTextLinks.length - 1){
          numCount ++;
        } else {
          numCount = 0;
        }
      });
    });
    },3500);
    });

