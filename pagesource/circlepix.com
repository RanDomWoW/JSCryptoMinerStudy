                    {
                        var message = 'Email could not be sent to the address in your account. The password has not been changed.';
                    }
                    else if(data.status == -2)
                    {
                        var message = 'That address was not found in our system.';
                    }
                    else if(data.status == 0)
                    {

                    }
                    alert(message);
                },'json');
        });
        $('body').on('hidden.bs.modal', function ()
            {
                $('body').css('padding-right','0px');
        });
    });
</script>
