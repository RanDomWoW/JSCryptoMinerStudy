	}
	else {
		DivElmnt.scrollTop = PreviousScrollTop = 0;
		ReachedMaxScroll = false;
	}
}
function pauseDiv() {
	clearInterval(ScrollInterval3);
}
function resumeDiv() {
	var num = parseInt($(".ps-scrollbar-y-rail")[0].style.top, 10);
