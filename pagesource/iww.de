<script type="text/javascript">
	
	// Cookie bereits gesetzt
	if (typeof Cookies.get("cookiestatement") == "undefined")
	{
		$(document).ready(function()
		{
			// Click-Event auf OK
			$("#cookie_statement a.button_blue").click(function(event)
			{
				event.preventDefault();
				$("#cookie_statement").fadeOut(500);
				Cookies.set("cookiestatement", 1, { expires: 150 });
			});
		});
			
	}
	else
	{
		// Hinweis ausblenden
		$("#cookie_statement").hide();
	} // end if
	
</script>		
