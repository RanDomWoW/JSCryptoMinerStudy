        /**  MultiRoutes - load multiple routes from one point to multiple points with
         *                  min requests possible
         *
         *    routes = new MultiRoutes();
         *    routes.timeout: int // max alowed time before throwing onerror, default 5000
         *    routes.onerror: function(msg) // error handler, default alert(msg)
         *    routes.getRoutes(string from, string[] to, function(GRoute[] routes) onload)
         */
        function MultiRoutes() {
            this.onload = null ;
            this.onerror = function(e) {
                alert("Error while roaddist: " + e)
            }
            ;
            this.gdir = new google.maps.DistanceMatrixService;
            this.result = [];
            this.finished = -1;
            this.qlen = 20;
            this.timeout = 5e3;
            this.timeoutId = null ;
            this.from = null ;
            this.to = [];
            this.numRequests = null ;
            var e = this;
            this.GDirLoad = function(t) {
                var n = 0;
                for (var r in t)
                    e.result[e.finished * e.qlen + n++] = t[r];
                e.finished++;
                if (e.finished == e.numRequests) {
                    e.clear();
                    e.onload(e.result)
                } else {
                    e.GDirRun(e.finished)
                }
            }
            ;
            this.GDirRun = function(t) {
                var n = [];
                for (var r = 0; r < e.qlen && t * e.qlen + r < e.to.length; r++) {
                    n.push(e.to[t * e.qlen + r])
                }
                e.gdir.getDistanceMatrix({
                    origins: [e.from],
                    destinations: n,
                    travelMode: google.maps.DirectionsTravelMode.DRIVING,
                    unitSystem: google.maps.UnitSystem.METRIC
                }, function(t, n) {
                    if (n == google.maps.DistanceMatrixStatus.OK) {
                        e.GDirLoad(t.rows[0].elements)
                    } else {
                        e.clear();
                        e.onerror("Error: Request error")
                    }
                })
            }
            ;
            this.clear = function() {
                e.gdir = null ;
                e.finished = -1;
                clearTimeout(e.timeoutId)
            }
            ;
            this.getRoutes = function(t, n, r) {
                if (this.finished != -1)
                    return;
                this.from = t;
                this.to = n;
                this.onload = r;
                this.finished = 0;
                this.numRequests = Math.ceil(this.to.length / this.qlen);
                this.GDirRun(this.finished);
                this.timeoutId = setTimeout(function() {
                    e.clear();
                    e.onerror("Error: Request timeout")
                }, this.timeout)
            }
        }
