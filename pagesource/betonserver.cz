        $(function () {
            $.nette.init();
        });

        $(document).on("change", "select[name='category']", function () {
            if ($(this).val()) {
                $.nette.ajax("\/?do=ChangeBanner"+"&page_id="+$(this).val());
            }
        });

        $(document).on("mouseover", ".marker-hover", function () {
            $(".results .item").removeClass('hover');
           $("#"+$(this).attr('data-id')).addClass('hover');
        });
        $(document).on("mouseout", ".marker-hover", function () {
            $("#"+$(this).attr('data-id')).removeClass('hover');
        });

        $(document).on("mouseover", ".results .item", function () {
            $(".marker-hover").removeClass('hover');
            $("#map-marker-"+$(this).attr('id')).addClass('hover');

        });
        $(document).on("mouseout", ".results .item", function () {
            $(".map-marker").removeClass('hover');
        });

        $(document).on("change", "select[name='category']", hashTag);
        //$(document).on("change", "input[name='latitude']", hashTag);

        function hashTag() {
            location.href = '#category='+$("select[name='category']").val()+'&lat='+$("input[name='latitude']").val()+'&lon='+$("input[name='longitude']").val();
        };

        $(document).on("change", "select[name='commodity']", search);

        $(document).on("submit", ".main-search", submit);

        $(document).on("mouseover", ".road", function () {

            mapZoom = map.getZoom();

            var start = new google.maps.LatLng($("input[name='latitude']").val(),$("input[name='longitude']").val());
            var end = new google.maps.LatLng($(this).attr('data-lat'),$(this).attr('data-lot'));
            var request = {
                origin:start,
                destination:end,
                travelMode: google.maps.TravelMode.DRIVING
            };
            //var directionsDisplay = new google.maps.DirectionsRenderer();// also, constructor can get "DirectionsRendererOptions" object
            //directionsDisplay.setMap(map); // map should be already initialized.

            //var directionsService = new google.maps.DirectionsService();

            map = new google.maps.Map(document.getElementById('map'), null),
                    // Instantiate a directions service.
                    directionsService = new google.maps.DirectionsService,
                    directionsDisplay = new google.maps.DirectionsRenderer({
                        map: map
                    }),

            directionsService.route(request, function(response, status) {
                if (status == google.maps.DirectionsStatus.OK) {
                    directionsDisplay.setDirections(response);
                }
            });
        });

        $(document).on("mouseout", ".road", function(){
            createHomepageGoogleMap($("input[name='latitude']").val(), $("input[name='longitude']").val() , customer_data,mapZoom, 1);
