        //取得代銷列表
        function getSearchProd(itme , pageNum,typeName) {
            if ($('#searchProd').val()==''){
                popupboxClear();
                return false;
            }
            var a = {};
            a.title = "";
            a.shadow = false;
            a.position = 'right';
            a.x = 37;
            a.y = 34;
            a.content = "<div class='row'>" +
            "<div class='col-xs-12 search2prod'>" +
            "<div id='searchProdContent' style='font-size: 14px;'><img src='img/loading.gif' >讀取中...</div>" +
            "</div>" +
            "</div>";
            popupboxCreate(itme, a);

            //中斷ajax
            if (mso) {
                mso.abort();
            }
            mso = $.getJSON(".", {
                'url':'index.php',
                'action': 'index',
                'option': 'searchForAjax',
                "searchName": itme.prop('value').trim(),
                "typeName":typeName,
                "ajax": 1,
                "page": pageNum
            }, function (data) {
                var returnHtml = '';
                if (data.totalCount > 0) {

                    returnHtml += '<div style="display:none;text-align:right;padding:0 0 3px 0;vertical-align:bottom;">共有<span style="font-size:18px;color:blue;">'+data.totalCount+'</span>筆';
                    if(data.perpage > 1){
                        returnHtml +='，目前在第<span style="font-size:18px;color:blue;">'+data.page_num+'</span>頁';
                        returnHtml +='，總共<span style="font-size:18px;color:blue;">'+Math.ceil(data.totalCount/data.perpage)+'</span>頁';
                    };
                    returnHtml += '</div>';

                    for(var key in data.rows){
                        returnHtml += '<div   style="display:block;line-height:20px;padding:5px;margin:2px 0;background:rgba(217,217,217,0.6);overflow : hidden;text-overflow : ellipsis;white-space : nowrap;" onmouseover="this.style.background=\'rgba(237,191,224,0.6)\';" onmouseout="this.style.background=\'rgba(217,217,217,0.6)\';" onclick=javascript:window.open("'+data.rows[key].url+'");clearPopupbox(); >';
                        returnHtml += '<a class="enterLink'+key+'" data-url="'+data.rows[key].url+'" href="#"  style="font-size: 18px;" >'+data.rows[key].name.trim()+'</a>';
                        returnHtml += '</div>';
                    };

                    if(data.perpage > 1){
                        returnHtml +='<div style="padding:3px 0;text-align: center;">'+data.pageShow+'</div>';
                    };

                } else {
                    popupboxClear();
                    return false;
                }
                ;
                returnHtml += "<div style='margin:4px 2px 0 0;text-align:right;'><a class='popupboxClear cursor'>關閉</a></div></div>";

                document.getElementById('searchProdContent').innerHTML = returnHtml;
            });
        }

