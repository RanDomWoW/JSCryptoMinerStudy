        //建商，代銷分頁(.searchProd 此class設定位置在house.php)
        $(document).on('click', '.searchProd', function (e) {
            var typeName=$('#search_concept').text();
            if (typeName!='請選擇'){
                var item = $('#searchProd');
                var pageNum = $(this).data('pagenum');
                if (pageNum == 'undefined' || pageNum == '') {
                    pageNum = 1;
                }
                getSearchProd(item, pageNum, typeName);
            }
