	$(".widget-toolbar-container input[type='checkbox']").on("change", function(){ handleNewsToolbarCheckbox($(this)); });
	
	function handleNewsToolbarCheckbox($me){
		//var $me = $(this);
		// get my parent widget-toolbar-container
		var $parent = $me.closest(".widget-toolbar-container");
		//remove tiny alert boxes created just now
		$parent.find(".tiny-alert-box").slideUp(100, function(){ $(this).remove(); });
		// get listContainer
		var $listContainer = $($parent.data("listcontainer"));
		// get class name
		var className = $me.data("newstypeclass")
		// set cookie name
		var cookieName = $me.data("cookie");
		//get cookie value
		var cookieValue = $.cookie(cookieName);
		//toggle cookie value
		if(cookieValue == "checked")
			cookieValue = "unchecked";
		else
			cookieValue = "checked";
		
		var currentCookiesValue = {
			newslist1: $.cookie("news1"),
			newslist2: $.cookie("news2"),
			newslist3: $.cookie("news3"),
			newslist4: $.cookie("news4")
		};
		currentCookiesValue[className] = cookieValue;
		//alert(currentCookiesValue.newslist1 +' - '+ currentCookiesValue.newslist2 +' - '+ currentCookiesValue.newslist3 +' - '+ currentCookiesValue.newslist4);
		
		var atLeastOneIsChecked = $('.widget-toolbar-container :checkbox:checked').length < 1;
		
		//if(atLeastOneIsChecked) alert('ok');
		
		// prevent showing blank box
		if(atLeastOneIsChecked)
		{
			// all values would be blank, so don't apply
			var $alertBox = $("<div class='tiny-alert-box'>حداقل یکی از گزینه ها باید فعال باشد.</div>");
			
			$alertBox.appendTo($parent).hide().slideDown(200, function(){
				setTimeout(function(){
					$alertBox.slideUp(200, function(){ $(this).remove(); });
				}, 2000);
			});
			$me.attr("checked", "checked");
			return false;
		}
		
		//set new value to cookie
		$.cookie(cookieName, cookieValue, { expires : 365 }); //expires in 365 days
		
		// toggle display
		if(cookieValue == "checked")
		{
			if(className=="cat32" || className=="cat95")
				$listContainer.find(".int-ext-ernal").removeClass("no-display");
			
			$listContainer.find("." + className).removeClass("no-display");
			
			// set appropriate checkbox value
			$me.attr("checked", "checked");
		}
 		else if(cookieValue == "unchecked")
		{
			if($.cookie("news1")==$.cookie("news2") && $.cookie("news1")=="unchecked"){
				$listContainer.find(".cat32, .cat95, .int-ext-ernal").addClass("no-display");
				console.log("int-ext");
			}
			$listContainer.find("." + className).addClass("no-display");
			
			if(currentCookiesValue.newslist4 == "checked")
				$listContainer.find(".cat97 .no-display").removeClass("no-display");
			
			$me.removeAttr("checked");
		}
	}
});
