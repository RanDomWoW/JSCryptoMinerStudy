<script>
$(function() {
try{
	var featuredNewsSlider = "#fns-item-container";
	if($(featuredNewsSlider).length){
		$(featuredNewsSlider).owlCarousel({
			navigation : false, // do not Show next and prev buttons
			slideSpeed : 300,
			paginationSpeed : 400,
			singleItem:true,
			autoPlay: true,
			stopOnHover: true,
			lazyLoad: true,
			mouseDrag: false,
			touchDrag: false,
			slideSpeed: 400,
			afterInit: function(){
				var pre = $('<div class="fns-sprite fns-arrow left"> </div>'),
					next = $('<div class="fns-sprite fns-arrow right"> </div>');
			
				$('#ds-video-carousel .item.static').hide();
			
				next.bind('click',function(){
					var owl = $(featuredNewsSlider).data('owlCarousel');
					owl.next();
				});
				pre.bind('click',function(){
					var owl = $(featuredNewsSlider).data('owlCarousel');
					owl.prev();
				});
			
				$(featuredNewsSlider).append(pre,next);
				
				// show bullets and position them correctly
				var controls = $('#fnews-slider .owl-controls');
				var bulletsCount = controls.find('.owl-page').length;
				var bulletsWidth = parseInt(controls.find('.owl-page').css("width"));
				var textContainerWidth = parseInt($('.fns-text-container').css("width"));
				var controlsLeft = (textContainerWidth - (bulletsWidth * bulletsCount) ) / 2;
				controls.css("left",controlsLeft+"px");
			}
		});
	} // end if
}
catch(err){}
});
</script>
