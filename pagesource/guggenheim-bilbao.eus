<script type="text/javascript">
    $(function () {

        
        if (typeof sec != "undefined") {
            $('#lista-videos').listavideos({
                maxResults: 12,
                section: sec
            });
        }

        var pagination = false;
        $(window).scroll(function () {
            if (typeof sec != "undefined") {
                var tope = 50;
                var numberOf = $('#load-more').data('numberOf');
                if ($(window).scrollTop() + $(window).height() > $('footer').offset().top - tope && !pagination && numberOf != 0) {
                    pagination = true;
                    $('#load-more').show();
                    setTimeout(function () {
                        $('#lista-videos').listavideos({
                            maxResults: 12,
                            pagination: true,
                            section: sec
                        });
                        pagination = false;
                    }, 1000);
                }
            }
        });
    });
