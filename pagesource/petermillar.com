<div ui-modal id="passwordModal" class="modal password-modal fade" ng-controller="PasswordCtrl">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal"></button>
				<h3>Change your password</h3>
			</div>
			<div class="modal-body">
				<div class="done" ng-show="done">Your password change is saved</div>
				<div class="alert alert-error text-center" ng-bind="formError" ng-show="formError && !done"></div>
				<form name="new-password" ng-show="!done">
					<span>Current Password</span>
					<sp-password password="member.currentPassword"></sp-password>
					<span>New Password</span>
					<sp-password password="member.password"></sp-password>
					<button sp-wait-btn class="btn btn-default col-md-6 submit" cname="submit" submit-form="updatePassword">Save Changes</button>
				</form>
			</div>
		</div>
	</div>
</div>



