  $(document).ready(function(){
    var $window = $(window),
      $card = $('.articles-slick'),
      toggleSlick;

    toggleSlick = function () {
      if ($window.width() < 768) {
        if($card.hasClass('slick-initialized') == false){
          $card.slick({
            dots: true,
            arrows: false
          });
        }
      } else {
        if($card.hasClass('slick-initialized')){
          $card.slick('unslick');
        }
      }
    }
    $window.resize(toggleSlick);
    toggleSlick();
  });
</script>


