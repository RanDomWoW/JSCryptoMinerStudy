<script type="text/javascript">
	var searchUrl="https://www.uea.ac.uk/search";
	function headertabsearch(el) {
	
		window.location.href = searchUrl + "?q=" + el.closest('.search-form-wrapper').children('.search-text').val();
	}

	$(".search-text").keypress(function(event){
		if(event.keyCode == 13){
			event.preventDefault();
			$(this).closest('.search-form-wrapper').children('.search-submit').click();
			return false;
		}
	});
	$(".search-submit").on('click',function(e){
		e.preventDefault();
		headertabsearch($(this));
	});
</script>	
	
	
