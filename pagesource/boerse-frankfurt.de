	<script language="JavaScript" type="text/javascript">
		var hschart1;
		$(document).ready(function () {
			var config = new Chart.HomepageChartConfiguration({ "ClosePrice" : "letzter Preis Vortag" });
			var initPeriod = 'Intraday';
			var hschart;
			var seriesOption = [];
			var plotLinesOption = [];

			var preLoadedSeries = [];



preLoadedSeries[0] = {name: "DAX-Indikation",identifier: '00010702,DEDB2KE7=DBBL,0,0',language: 'de',useSourceIdPrefix: true,isOppositeYAxis: false,data: null,pushOptions: {"QuoteTimeField":"quotetime","QuoteTimeOffset":0,"Symbol":null,"QuoteField":null,"QuoteTimeFormat":null,"QuoteValueFormat":null,"Room":null}};
			var createChart = function () {
				var yAxisOption;
				if (config.GetConfiguration().yAxis.length != undefined && config.GetConfiguration().yAxis.length > 1) {
					yAxisOption = config.GetConfiguration().yAxis;
				} else {
					yAxisOption = $.extend(
						{/* Empty */ },
						config.GetConfiguration().yAxis,
						(plotLinesOption.length === 0)
						? {/* Empty */ }
						: { plotLines: plotLinesOption }
					);
				}

				return $("#DetailChart").highcharts("StockChart",
					$.extend(
						{/* Empty */ },
						config.GetConfiguration(),
						{ series: seriesOption },
						(plotLinesOption.length === 0) ? {/* Empty */ } : { yAxis: yAxisOption }
					),
					config.GetOnCompleteFunction()
				);
			};

			/* get only Close-Values from OHLC */
			var formatDataPoints = function (data) {
				for (var i = 0; i < data.length; i++) {
					var element = data[i];
					element.splice(1, 3).splice(2, 2);
					if(Array.isArray(element) && element.length > 1) {
						if (element[1] === "") {
							data.splice(i, 1);
							i -= 1;
						}
					}
				}
				return data;
			};

			var loadInstrument = function (i, serie, period) {
				var pushOptions = getPushOptions(serie);
				if ($.isArray(serie.activePeriods) && serie.activePeriods.length > 0 && $.inArray(period, serie.activePeriods) < 0) {
					serie.visible = false;
					serie.showInLegend = false;
					seriesOption[i] = serie;
					return null;
				}

				var url = generateRequestParameter(serie, period, serie.useSourceIdPrefix);
				if (serie.identifier != null && serie.identifier != "") {
					return $.ajax({
						dataType: "json",
						async: false,
						url: url,
						success: function (data) {
							var dataToUse = data.instruments[0] != null ? reduceIntradayData(data.instruments[0].data, Math.ceil(data.instruments[0].data.length /  338)) : null;
							seriesOption[i] = $.extend(serie, {
								data: (data.instruments[0] != null) ? formatDataPoints(dataToUse) : [],
								pushOptions : pushOptions
							});
							
							if (period == "Intraday"
								&& data.instruments[0] != null
								&& data.instruments[0].hasPreviousDayClose
								&& data.instruments[0].previousDayClose != null) {

								plotLinesOption.push({
									value: data.instruments[0].previousDayClose,
									color: '#000099',
									width: 2
								});

								seriesOption.push({
									name: config.get_translation('ClosePrice'), //'Vortag'
									color: '#000099',
									data: null
								});
							}
						},
						error: function() {
							seriesOption[i] = $.extend(serie, { data:  [] });
						}
					});
				} else {
					return null;
				}
			};

			var generateRequestParameter = function(serie, period, usePrefixValue) {
				var prefix;
				var identifier = serie.identifier;
				if ( !!usePrefixValue === true && usePrefixValue === true) {
					switch (period) {
						case 'Intraday':		prefix = '10'; break;
						case 'CurrentWeek': prefix = '11'; break;
						case 'OneWeek':			prefix = '12'; break;
						case 'OneMonth':		prefix = '13'; break;
						case 'ThreeMonths': prefix = '14'; break;
						case 'SixMonths':		prefix = '15'; break;
						case 'OneYear':			prefix = '16'; break;
						case 'ThreeYears':	prefix = '17'; break;
						case 'FiveYears':		prefix = '18'; break;
						case 'TenYears':		prefix = '19'; break;
						case 'Maximum':			prefix = '99'; break;
						default:				prefix = '10'; break;
					}
				} else {
					prefix = '';
				}

				if (period === "Intraday"
					&& serie.intradaySourceIdentifier !== undefined
					&& serie.intradaySourceIdentifier !== "") {
					identifier = serie.intradaySourceIdentifier;
				}
				var url = 'http://proxy.boerse-frankfurt.de/cst/BoerseFrankfurt/Share/chart.json?' + "instruments=" + prefix + identifier + "&period=" + period;
				if (period === "Intraday") {
					//maybe the exchange is closed at the moment, use HistoricIntraday instead of Intraday.
					var identifiers = identifier.split(',');
					$.ajax({
						url: "/ajax/GetPeriodForIntradayHighstockChart/" + identifiers[2],
						type: "post",
						async: false,
						success: function(result) {
							url = 'http://proxy.boerse-frankfurt.de/cst/BoerseFrankfurt/Share/chart.json?' + "instruments=" + prefix + identifier + "&" + result;
							return url;
						}
					});
				}

				return url;
			};



			var getPushOptions = function(serie) {
				var pushOptions = {};
				if (serie.pushOptions !== undefined) {
					pushOptions["symbol"] = serie.pushOptions.Symbol;
					pushOptions["quoteField"] = serie.pushOptions.QuoteField;
					pushOptions["quoteTimeField"] = serie.pushOptions.QuoteTimeField;
					pushOptions["quoteTimeToGermanTime"] = serie.pushOptions.QuoteTimeOffset;
				}

				return pushOptions;
			};

			var loadData = function (series, period, callback) {
				var jxhr = [];
				seriesOption = [];
				plotLinesOption = [];

				// Deep copy
				var cloneSeries = jQuery.extend(true, {}, series);

				$.each(cloneSeries, function (i, serie) {

					if (serie.data != null) {
						seriesOption[i] = serie;
					} else {
						jxhr.push(loadInstrument(i, serie, period));
					}

				});

				$.when.apply(null, jxhr)
					.done(callback)
					.fail(callback);
			};

			var changePeriod = function (period) {
				if (hschart != null && hschart.highcharts() != null) {
					hschart.highcharts().destroy();
				}
				$.each(preLoadedSeries, function (i, serie) {
					serie.data = null;
				});
				if (period !== 'Intraday') {
					if (typeof(window.unregister_pushhidden_table) =="function") {
						unregister_pushhidden_table();
					}

				} else {
					if (typeof(window.reload_push_table_settings) == "function") {
						reload_push_table_settings();
					}
				}

				loadData(preLoadedSeries, period, function () {
					setHSDefaults(preLoadedSeries[0].language);
					hschart = createChart();
					hschart1 = hschart.highcharts();
					if (hschart.highcharts().series[0] !== undefined
						&&  hschart1.series[0].points.length < 1) {
						hschart1.showLoading(hschart.highcharts().options.lang.noData);
					}
				});
			};

			var setInstrument = function(serie, period) {
				if (hschart != null && hschart.highcharts() != null) {
					hschart.highcharts().destroy();
				}

				loadData([serie], period, function () {
					hschart = createChart();
					hschart1 = hschart.highcharts();
					if (hschart.highcharts().series[0] !== undefined &&
						hschart.highcharts().series[0].points.length < 1) {
						hschart.highcharts().showLoading(hschart.highcharts().options.lang.noData);
					}
				});
			};

			var tabId = 'IndexTabs';
			if (tabId != null) {
				$('ul.sub-men#' + tabId.trim() + ' li').live("click", function () {

					showLoader();
					silentTrackPI();

					$('ul.sub-men#' + tabId.trim() + ' li a').removeClass('active');
					$(this).find('a').addClass('active');

					var period = $(this).find('a').attr('data-period');
					if (period != null) {
						changePeriod(period);
					}
					var name = $(this).find('a').attr('data-instrument-name');
					var identifier = $(this).find('a').attr('data-instrument-identifier');
					if (name != null && identifier != null) {
						var serie = {
							name: name,
							identifier: identifier
						};
						setInstrument(serie, initPeriod);
					}

					hideLoader();
				});
			};


			loadData(preLoadedSeries, initPeriod, function () {
				if (initPeriod !== 'Intraday') {
					if (typeof(window.unregister_pushhidden_table) == "function") {
						unregister_pushhidden_table();
					}
				} else {
					if(typeof(window.reload_push_table_settings) == "function")
					{
						reload_push_table_settings();
					}

				}
				setHSDefaults(preLoadedSeries[0].language);
				hschart = createChart();
				hschart1 = hschart.highcharts();
				if (hschart.highcharts().series[0] !== undefined &&
				 hschart.highcharts().series[0].points.length < 1) {
					hschart.highcharts().showLoading(hschart.highcharts().options.lang.noData);
				}
			});

			function reduceIntradayData(data, step) {
				var resultData = [];
				if (data.length > 1000) {
					step += 2;
				}
				if (Array.isArray(data) && step > 1) {
					$.each(data, function(index, element) {
						if (index % step === 0) {
							resultData.push(element);
						}
					});
				}
				return resultData.length === 0 ? data : resultData;
			}
		});
	</script>

