<script type="text/javascript">
	var ipo_tool = document.getElementById("ipo_tool");
	var DATE_DELIMITER = '.';
	var TIME_DELIMITER = ":";
	var now = new Date();

	/**
	* openFancyBox
	*/
	function openFancyBox() {
		$.fancybox({
			height: 3000,
			width: 1200,
