						showHourglass();
						showSuggestBox();
						var val = this.value;
						delay(function () {
							triggerSearch(val);
						}, 350);
				}
			});

			/* EVENTS END */

			/* FUNCTIONS START */

			var moveUp = function (elem) {
				var newElem = elem.prev();
				var attr = newElem.attr('name');
				if (typeof attr !== typeof undefined && attr !== false) {
					elem.removeClass("active-tablerow");
					newElem.addClass("active-tablerow");
				} else {
					newElem = newElem.prev();
					attr = newElem.attr('name');
					if (typeof attr !== typeof undefined && attr !== false) {
						elem.removeClass("active-tablerow");
						newElem.addClass("active-tablerow");
					}
				}
				scrollCtr--;
				scrollTo(newElem, scrollCtr);
			}

			var moveDown = function (elem) {
				var newElem = elem.next();
				var attr = newElem.attr('name');
				if (typeof attr !== typeof undefined && attr !== false) {
					elem.removeClass("active-tablerow");
					newElem.addClass("active-tablerow");
				} else {
					newElem = newElem.next();
					attr = newElem.attr('name');
					if (typeof attr !== typeof undefined && attr !== false) {
						elem.removeClass("active-tablerow");
						newElem.addClass("active-tablerow");
					}
				}
				scrollCtr++;
				scrollTo(newElem, scrollCtr);
			}

			var scrollTo = function (element, i) {
				var heightElement = parseInt($(element).css('height'));
				var minRowOffset = 1;
				var topDistance = heightElement * (i - minRowOffset);

				if (i >= minRowOffset) {
					$('.headermarketbox-suggest').animate({
						scrollTop: topDistance + (heightElement * 1.1)
					}, 180);
				}
			};

			var triggerSearch = function (query) {
				if (isEmpty(query)) {
					return;
				}

				$.ajax({
					type: 'GET',
					url: '/Ajax/SearchController_JsonSuggest?Query=' + query + '&SearchType=false'
				}).done(function (data) {
					if (isEmpty(data)) {
						$('.headermarketbox-suggest').hide();
						return;
					}
					var html = new String();
					html += "<table class='table suggest-table'>";
					var ctr = 0;
					for (var instrumentType in data) {
						if (data.hasOwnProperty(instrumentType)) {
							html += "<tr>" +
												"<th>" +
													"<span class='header-cell'>" + instrumentType + "</span>" +
												"</th>" +
											"</tr>";
							for (var i = 0; i < data[instrumentType].length; i++) {
								var val = data[instrumentType][i];
								var identifier = getIdentifier(val);
								if (instrumentType === "Devisen") {
									val.Name = val.Name.split("-")[1].trim();
								}
						
								var css = ctr === 0 ? "active-tablerow" : "";
								html += '<tr identifier="' + identifier + '" name="' + val.Name + '" class="' + css + '">' +
													'<td class="suggest-cell" onclick="select(\'' + val.Name.replace(/'/g, '') + '\', \'' + identifier + '\', \'' + val.Category + '\', \'' + val.UrlName + '\')">' +
														'<span>' + val.Name + '</span>' +
													'</td>' +
												'</tr>';
								ctr++;
							}
						}
					}
					html += "</table>";
					$(".headermarketbox-suggest").append(html);
					$(".headermarketbox-suggest").removeClass("hide");
					hideHourglass();
				});
			}

			var getIdentifier = function (val) {
				switch (val.Category) {
					case "Indizes":
					case "Devisen":
					case "Aktien":
					case "Rohstoffe":
						return val.Valor;
					case "Fonds":
					case "ETFs":
					case "Anleihen":
					case "Zertifikate":
						return val.ISIN;
					case "ETCs":
						return val.WKN;
				}
				return null;
			}

			var checkCountOfTableRows = function () {
				if ($('table#headermarketboxtable tr:last').index() + 1 >= 6) {
					$(".headermarketbox-suggest").html(new String());
					$(".headermarketbox-suggest-input").val(new String());
					$(".search-row").addClass("hide");
				} else {
					$(".search-row").removeClass("hide");
				}
			}

			var select = function (instrumentName, identifier, category, url) {
				var row = '<tr>' +
										'<td identifier="' + identifier + '" name="' + instrumentName + '" category="' + category + '" >' +
											'<a href="' + url + '">' + instrumentName + '</a>' +
										'</td>' +
										'<td class="text-center"><span class="glyphicon glyphicon-arrow-up up noselect"></span></td>' +
										'<td class="text-center"><span class="glyphicon glyphicon-arrow-down down noselect"></span></td>' +
										'<td class="text-center"><span class="glyphicon glyphicon-remove-sign remove noselect" aria-hidden="true"></span></td>' +
									'</tr>';
				$(".customized-tbody").append(row);
				$(".headermarketbox-suggest").hide();
				checkCountOfTableRows();
			}

			var createJson = function () {
				customizedItems = [];
				var ctr = 0;
				$(".customized-tbody").eq(1).children("tr").each(function () {
					var td = $(this).children("td:first-child");
					customizedItems.push({
						Position: ctr,
						Name: td.attr("name"),
						Identifier: td.attr("identifier"),
						SuggestSearchCategory: td.attr("category"),
						PushItem: td.attr("pushItem"),
						Url: td.children("a").attr("href"),
						GatrixxExchangeCode: "FSE"
					});
					ctr++;
				});
			}

			var delay = (function () {
				var timer = 0;
				return function (callback, ms) {
					clearTimeout(timer);
					timer = setTimeout(callback, ms);
				};
			})();

			var isEmpty = function (obj) {
				// null and undefined are "empty"
				if (obj == null) {
					return true;
				}

				// Assume if it has a length property with a non-zero value
				// that that property is correct.
				if (obj.length > 0) {
					return false;
				}
				if (obj.length === 0) {
					return true;
				}

				// Otherwise, does it have any properties of its own?
				// Note that this doesn't handle
				// toString and valueOf enumeration bugs in IE < 9
				for (var key in obj) {
					if (hasOwnProperty.call(obj, key)) {
						return false;
					}
				}

				return true;
			}

			var showHourglass = function () {
				$(".magnifier").eq(1).children("span").removeClass("glyphicon-search");
				$(".magnifier").eq(1).children("span").addClass("glyphicon-hourglass");
			}

			var hideHourglass = function () {
				$(".magnifier").eq(1).children("span").addClass("glyphicon-search");
				$(".magnifier").eq(1).children("span").removeClass("glyphicon-hourglass");
			}

			var showSuggestBox = function () {
				$(".headermarketbox-suggest").show();
				$(".headermarketbox-suggest").html(new String());
			}
			/* FUNCTIONS END */
		</script>
	</div>
</div>
