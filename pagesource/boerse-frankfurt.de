	/**
	* getDateFromDateString
	*/
	function getDateFromDateString(dateString) {
		var parsedDate = undefined;
		var dateStringParts = dateString.split(' ') || dateString.split('T');
		if (!!dateStringParts === false || dateStringParts.length < 1) {
			return parsedDate;
		}
		//try to parse the date
		var dateConstitutents = dateStringParts[0].trim().split(DATE_DELIMITER);
		if (dateConstitutents.length !== 3) {
			//wrong dateConstitutents
			return parsedDate;
		}
		var dd = dateConstitutents[0];
		var mm = dateConstitutents[1] - 1;
		var yyyy = dateConstitutents[2];

		//try to parse the hours
		var timeConstituents;
		var hh = mn = ss = '00';

		if (dateStringParts.length > 1) {
			timeConstituents = dateStringParts[1].trim().split(TIME_DELIMITER);
			hh = timeConstituents.length > 0 ? timeConstituents[0] : '00';
			mn = timeConstituents.length > 1 ? timeConstituents[1] : '00';
			ss = timeConstituents.length > 2 ? timeConstituents[2] : '00';
		}

		parsedDate = new Date(yyyy, mm, dd, hh, mn, ss);
		return parsedDate;
	}


