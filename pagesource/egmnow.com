	}
	
	function saveNewIds (code,accessToken,userId,noRefresh,pageId,isSignup){
		  
		jQuery.ajax({
		
			url:"http://www.egmnow.com/wp-admin/admin-ajax.php",
			type:'POST',
			data:'action=update_fb_cookies&code='+code+"&access_token="+accessToken+"&user_id="+userId+"&page_id="+pageId,
			success:function(results){
			
				jQuery("#n_head_my_egm_pad_float").show ();
			
				if (pageId){
				
					//Subscribe to the page.
					subscribeToPage (pageId,'http://www.egmnow.com');
				
				}
			
				var endStr = results.replace ("-1","");
				var endStr = endStr.replace ("1","");
				var endStr = endStr.replace ("0","");
				
				var hideMessage = document.getElementById ("hide_error");
				
				if (hideMessage){
				
					hideMessage.innerHTML = results;
				
				}
				
				gp_update_social_on ('http://www.egmnow.com');
			
				if ( endStr == "finish_signup" ){
				
					//Redirect to finalize setup.
					pt_load_registration ('http://www.egmnow.com','yes');
					//window.location.href = "http://www.egmnow.com/partner-program/?type=profile";
				
				} else if ( endStr == "new_user" ){
				
					//Redirect to profile page.
					pt_load_registration ('http://www.egmnow.com','yes');
					//window.location.href = "http://www.egmnow.com/partner-program/?type=profile";
				
				} else {
				
					//Success, logged in.
