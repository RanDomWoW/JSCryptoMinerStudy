        <style type="text/css">.optin-box-9 {margin-bottom:0px;}
div.feature-box-creator .feature-box-content {padding:10px}
.banner .op-logo img { max-width: 100px !important; }
.vid-container {
    margin-top: 20px;
}

.col-md-4 {
    width: 33.33333333%;
    float: left;
    position: relative;
    min-height: 1px;
    padding-right: 15px;
    padding-left: 15px;
    box-sizing: border-box;
}
@media (max-width:768px) {
    .col-md-4 { width:100%; float:none; }
}
.vid {
    width: 260px;
    margin: 0 auto 0px;
    overflow: hidden;
}
.content-vid {
    background: #000000;
    color: #444444;
    font-size: 24px;
    text-align: center;
}

.content-vid a {
    color: #444444;
    font-size: 24px;
    text-align: center;
    text-decoration: none;
    display: block;
    padding: 50px 20px;
    font-family: 'Open Sans', sans-serif;
    line-height: 14px;
    text-shadow: none;
    font-weight: bold !important;
}

.num_1 .content-vid a {
    background: url(http://formationlfa.com/wp-content/uploads/2016/11/LFA-V1.png);
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center center;
    padding: 71px 20px !important;
    opacity: .4;
}
.num_2 .content-vid a {
    background: url(http://formationlfa.com/wp-content/uploads/2016/11/LFA-V2-BN.png);
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center center;
    padding: 71px 20px !important;
    opacity: .7;
}
.num_3 .content-vid a {
    background: url(http://formationlfa.com/wp-content/uploads/2016/11/LFA-V3-BN.png);
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center center;
    padding: 71px 20px !important;
    opacity: .7;
}
.num_2.now_playing .content-vid a {
    background: url(http://formationlfa.com/wp-content/uploads/2016/11/LFA-V2.png);
background-size: cover;
    background-repeat: no-repeat;
    background-position: center center;
    padding: 71px 20px !important;
    opacity: 1;
}
.num_3.now_playing .content-vid a {
    background: url(http://formationlfa.com/wp-content/uploads/2016/11/LFA-V3.png);
background-size: cover;
    background-repeat: no-repeat;
    background-position: center center;
    padding: 71px 20px !important;
    opacity: 1;
}
.now_playing a {
    color: #808080 !important;
    font-weight: bold;
    font-size: 30px !important;
}

.now_playing .content-vid a {
    opacity: 1;
}




.trapezoid-num {
    position: relative;
    text-align: center;
    background: #AB8640;
}
.trapezoid-num:before {
    width: 0;
    height: 0;
    content: "";
    border-top: 18px solid #AB8640;
    border-left: 20px solid #282828;
    position: absolute;
    left: 0px;
    bottom: 0;
}
.trapezoid-num:after {
    width: 0;
    height: 0;
    content: "";
    border-top: 18px solid #AB8640;
    border-right: 20px solid #282828;
    position: absolute;
    right: 0px;
    bottom: 0;
}
.trapezoid-num a {
    text-decoration: none;
    line-height: 14px !important;
    text-shadow: none !important;
}
span.numcont {
    height: 18px;
    width: 100%;
    display: block;
    margin: 0 auto;
    background: #AB8640;
    padding: 0px;
}
i.num {
    border-radius: 50%;
    padding: 9px 17px;
    background: #AB8640;
    color: #fff;
    line-height: 16px;
    font-size: 21px;
    font-style: normal;
}

.vid.now_playing h4 {
    opacity: 1;
}
.vid h4 {
    text-align: center;
    margin-top: 20px !important;
    display: block;
    font-family: 'Open Sans', sans-serif;
    font-weight: 700;
    padding-left: 0;
    color: white;
    opacity: .4;
}


.right p {
    color: black;
    margin-top: 0px;
    font-size: 15px;
    line-height: 22px;
    padding: 10px 0;
    margin-bottom: 0px;
}
#le_body_row_4_col_1 .image-caption img { border-radius: 50%; }



.agenda_block {
    text-align: center;
    padding: 90px 0 185px;
}
.agenda_block h2 {
    color: #fff;
}
#agnd_tab {
    text-align: center;
    position: relative;
}
.agnd_menu {
    list-style: none;
}
.agnd_menu li {
    display: inline-block;
    position: relative;
}
.agnd_menu li.r-tabs-state-active:after {
    content: '';
    display: block;
    position: absolute;
    left: 50%;
    bottom: -13px;
    margin-left: -9px;
    border-left: 9px solid transparent;
    border-right: 9px solid transparent;
    border-bottom: 9px solid #fff;
}
.agnd_menu li a {
    display: block;
    margin: 0 12px;
    width: 177px;
    height: 177px;
    border-radius: 50%;
    background: rgba(255, 255, 255, 0.17);
    padding: 25px;
    -webkit-transition: all ease-in-out 400ms 0s;
    -moz-transition: all ease-in-out 400ms 0s;
    -ms-transition: all ease-in-out 400ms 0s;
    -o-transition: all ease-in-out 400ms 0s;
    transition: all ease-in-out 400ms 0s;
}
.agnd_menu li:hover a, .agnd_menu li.r-tabs-state-active a {
    background: rgba(139, 195, 74, 0.3);
}
.agnd_menu li a .agenda-circle {
    width: 127px;
    height: 127px;
    border: 2px solid #c6e536;
    border-radius: 50%;
    -webkit-transition: all ease-in-out 400ms 0s;
    -moz-transition: all ease-in-out 400ms 0s;
    -ms-transition: all ease-in-out 400ms 0s;
    -o-transition: all ease-in-out 400ms 0s;
    transition: all ease-in-out 400ms 0s;
}
.agnd_menu li:hover a .agenda-circle, .agnd_menu li.r-tabs-state-active a .agenda-circle {
    border-color: #c6e536;
    background: #c6e536;
    background: #aacf2c;
    background: -moz-linear-gradient(-45deg, #aacf2c 0%, #80ad1c 50%, #447d06 100%);
    background: -webkit-linear-gradient(-45deg, #aacf2c 0%, #80ad1c 50%, #447d06 100%);
    background: linear-gradient(135deg, #aacf2c 0%, #80ad1c 50%, #447d06 100%);
}
.agnd_menu li a .agenda-cell {
    color: #fff;
    display: table;
    height: 127px;
    width: 100%;
}
.agnd_menu li a .agenda-valing {
    display: table-cell;
    vertical-align: middle;
