		if (field.type == 'text' ||
		field.type == 'textarea' ||
		field.type == 'select-one' ||
					field.type == 'radio') {

					var value;
					// get field's value
					if (field.type == "select-one") {
						var si = field.selectedIndex;
						value = field.options[si].value;
					} else {
						value = field.value;
					}

		if (value.length > 0) {

