<script type="text/javascript">

		$("#quote-tabs li a").click( function() {

			var tab = $(this).attr('id').toString();

			if(tab == 't_residential')
			{
				$("a#t_residential").addClass('active');
				$("a#t_commercial").removeClass('active');
				$("div#quote-commercial").fadeOut();
				$("div#quote-residential").fadeIn();
				$("div#sc-quote").removeClass('tender-bg');
			}
			else
			{
				$("a#t_commercial").addClass('active');
				$("a#t_residential").removeClass('active');
				$("div#quote-residential").fadeOut();
				$("div#quote-commercial").fadeIn();
				$("div#sc-quote").addClass('tender-bg');
			}

		});

		$(".quotation-type-ul li").click(function() {
			$("div#quotation-residential-inner").hide().fadeIn();
		});

		$('.quotation-type-button').click(function () {
			toggleSolarBattery($(this).attr('data-type'));
		});

		function toggleSolarBattery(newValue) {
			var oldValue = ((newValue == 'battery') ? 'solar' : 'battery');

			$('.quotation-type-button').each(function () {
				$(this).removeClass('quotation-type-button-checked');
			});
			$('.quotation-type-button[data-type="' + newValue + '"]').addClass('quotation-type-button-checked');

			$('input[type="radio"][name="quoteType"][value="' + newValue + '"]')
				.attr('checked', 'checked');

			$('[data-field-type="' + oldValue + '"]').each(function () {
				$(this).hide();
			});
			$('[data-field-type="' + newValue + '"]').each(function () {
				$(this).show();
			});

			if (newValue == 'solar') {
				$('label[for="lstSpecialPreferences[]_386"]').show();
				$('label[for="lstSpecialPreferences[]_387"]').show();
				$('label[for="lstSpecialPreferences[]_388"]').show();
			} else {
				$('label[for="lstSpecialPreferences[]_386"]').hide();
				$('label[for="lstSpecialPreferences[]_387"]').hide();
				$('label[for="lstSpecialPreferences[]_388"]').hide();
			}
		}

	$(document).ready(function() {

		// Preload the loading gif
		loading = new Image();
		loading.src = "/templates/ngageit/images/loading.gif";

		// Form show loading gif
		$("form#frmQuotationResidential").submit(function() {
			var gif = '<img src="/templates/ngageit/images/loading.gif" />';
			var button = '<input class="sc-submit" type="submit" name="btnSubmit" value="Submit" />';

			$("p#submit-cont").html(gif);

			// Fallback to button if there is some sort of js error and the page doesn't reload
			setTimeout(function() {
				$("p#submit-cont").html(button);
			}, 30000);


			$.modal("<div><p>Please wait..</p><p>" + gif + "</p></div>", { opacity: 80, zIndex: 99999, close: false, overlayCss: {'background-color': '#000'}, dataId : 'processingQuotation'});

			// Release modal if longer than 30 seconds
			setTimeout(function() {
				$.modal.close();
			}, 30000);


			return true;

		});

		
		toggleSolarBattery('solar');
	});


</script>
