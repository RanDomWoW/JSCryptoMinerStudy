    function isValidDate(day, month, year) {
	if (month < 1 || month > 12) {
	    return false;
	}
	if (day < 1 || day > 31) {
	    return false;
	}
	if ((month == 4 || month == 6 || month == 9 || month == 11) &&
	    (day == 31)) {
	    return false;
	}
	if (month == 2) {
	    var leap = (year % 4 == 0 &&
		       (year % 100 != 0 || year % 400 == 0));
	    if (day>29 || (day == 29 && !leap)) {
		return false;
	    }
	}
	return true;
    }


