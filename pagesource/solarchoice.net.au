		     pp="("+p13+")"+p14+p15;
		     m.value=pp;
		}
		l16=p.length;
		p16=p.substring(d2+1,l16);
		l17=p16.length;
		if(l17>3&&p16.indexOf('-')==-1){
			p17=p.substring(d2+1,d2+4);
			p18=p.substring(d2+4,l16);
			p19=p.substring(0,d2+1);
		pp=p19+p17+"-"+p18;
		m.value="";
		m.value=pp;
		}
	}

	}

