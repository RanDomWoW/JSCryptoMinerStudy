    <script type="text/javascript">
     
        
        var iPadMsgShown = false;
        var isRuCulture = true;
        var Page = {
            PasswordCheckInterval: null,
            OnLoad: function() {

                if (!iPadMsgShown && isRuCulture) {
                    iPadMsgShown = true;
                    RedirectToAppStore();
                }

                var localTimeZoneField = $get("hfLocalTimeZoneOlson");
                localTimeZoneField.value = jstz.determine().name();

                var loginButton = $get("LoginButton");
                loginButton.value = loginButton.title; //для Ipad

                //костыль для ie. В старых версиях не поддерживается аттрибут placeholder
                if ($.browser.msie) {
                    Page.SetLoginLabelVisible();
                    Page.SetPasswordLabelVisible();
                    var loginTextBox = $get("LoginTextBox");
                    var loginLabel = $get("LoginLabel");
                    loginTextBox.onfocus = loginLabel.onclick = Page.HideLoginLabel;
                    loginTextBox.onchange = loginTextBox.onblur = Page.SetLoginLabelVisible;

                    var passwordTexBox = $get("PasswordTextBox");
                    var passwordLabel = $get("PasswordLabel");
                    passwordTexBox.onfocus = passwordLabel.onclick = Page.HidePasswordLabel;
                    passwordTexBox.onchange = passwordTexBox.onblur = Page.SetPasswordLabelVisible;
                } else {
                    $("#LoginTextBox").attr("placeholder", 'Логин');
                    $("#PasswordTextBox").attr("placeholder", 'Пароль');
                }
            },
            HideLoginLabel: function() {
                $get("LoginLabel").style.display = "none";
                $get("LoginTextBox").focus();
            },
            SetLoginLabelVisible: function() {
                var visible = $get("LoginTextBox").value.trim().length == 0;
                $get("LoginLabel").style.display = visible ? "" : "none";
            },
            HidePasswordLabel: function() {
                $get("PasswordLabel").style.display = "none";
                $get("PasswordTextBox").focus();
                Page.SetPasswordCheckInterval(false);
            },
            SetPasswordLabelVisible: function() {
                visible = $get("PasswordTextBox").value.trim().length == 0;
                if($get("PasswordLabel")!=null)
                    $get("PasswordLabel").style.display = visible ? "" : "none";
                Page.SetPasswordCheckInterval(visible);
            },
            SetPassword: function(pwd) {
                $get("PasswordTextBox").value = pwd;
                Page.SetPasswordLabelVisible();
            },
            SetPasswordCheckInterval: function(value) {
                if (value && !Page.PasswordCheckInterval)
                    Page.PasswordCheckInterval = setInterval(Page.SetPasswordLabelVisible, 1);

                if (!value && Page.PasswordCheckInterval) {
                    clearInterval(Page.PasswordCheckInterval);
                    Page.PasswordCheckInterval = null;
                }
            },
            ChangeLanguage: function(lang) {
                Cookies.Set("UILang",
                    lang, {"Ticks":25920000000000,"Days":30,"Hours":0,"Milliseconds":0,"Minutes":0,"Seconds":0,"TotalDays":30,"TotalHours":720,"TotalMilliseconds":2592000000,"TotalMinutes":43200,"TotalSeconds":2592000}, true);
                document.location.replace(document.location.href.replace(document.location.search, ''));
            }
        };

        var iPadMessageWindow;
        function RedirectToAppStore() {
            if(IsIpad) {
                var showIPadMsgCookie = Cookies.Get("IPadAnswer");
                if(showIPadMsgCookie == null || showIPadMsgCookie == true) {
                    iPadMessageWindow = new RadDockWindow("IPadDetectedMessage_RadDock", "IPadDetectedMessage_RadDock_T_TitleLabel",
                        "IPadDetectedMessage_RadDock_C_ContentContainer");
                    iPadMessageWindow.Open(null, null, 330, 160);
                    $(".rdCommands").remove();
                }
            }
        }
        
        function onIPadMsgOk() {
            setIPadMsgCookie();
            window.location = "http://itunes.apple.com/ru/app/interfaks-hd/id459068174?mt=8";
        }
        
        function onIPadMsgCancel() {
            setIPadMsgCookie();
            if (iPadMessageWindow != undefined) {
                iPadMessageWindow.Close();
            }
        }
        
        function setIPadMsgCookie() {
            if ($get("IPadDetectedMessage_RadDock_C_DontShowIPadMsgCheckBox").checked) {
                Cookies.Set("IPadAnswer", false, {"Ticks":315360000000000,"Days":365,"Hours":0,"Milliseconds":0,"Minutes":0,"Seconds":0,"TotalDays":365,"TotalHours":8760,"TotalMilliseconds":31536000000,"TotalMinutes":525600,"TotalSeconds":31536000}, true);
            }
        }

        Sys.WebForms.PageRequestManager.getInstance().add_endRequest(Page.OnLoad);
    </script>
    
