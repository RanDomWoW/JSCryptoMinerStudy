<script>
$(document).ready(function() {
  /* Update timer */
  function timer() {
    var timecount = 60;
    var x = setInterval(function() {
      if (document.getElementById("resendtimer")) {
        document.getElementById("resendtimer").innerHTML = "OTP Resend in " + timecount + " seconds";
      }
      --timecount;
      if (timecount == 0) {
        clearInterval(x);
        $('#resendotp').show();
        $('#resendtimer').hide();
      }
    }, 1000);
  }
  /* Register Form */
  $('#registerform').on('submit', function(e) {
    $.ajax({
      method: "POST",
      url: "/sendotp",
      data: $('form[name="registerform"]').serialize(),
    }).done(function(data) {
      $('#register').html(data);
      timer();
    });
    e.preventDefault();
    $('#registersubmit').css('pointer-events', 'none').html('Please Wait');
  });
  /* OTP Form */
  $('#resendotp').on('submit', function(e) {
    $('#resendotp').hide();
    $.ajax({
      method: "POST",
      url: "/resendotp",
      data: $('form[name="resendotp"]').serialize(),
    }).done(function(data) {
      if (data == true) {
        $('#resendtimer').show();
        timer();
        alert('OTP has been resent! Sometimes it may take up to 10 minutes to reach your mail.');
      } else {
        $('#resendtimer').show();
        timer();
        $('#register').html(data);
      }
    });
    e.preventDefault();
    $('.otpresend').css('pointer-events', 'none').html('Please Wait');
  });
  /* OTP Form */
  $('#otpform').on('submit', function(e) {
    $.ajax({
      method: "POST",
      url: "/register",
      data: $('form[name="otpform"]').serialize(),
    }).done(function(data) {
      if (data == true) {
        location.reload();
      } else {
        $('#register').html(data);
        timer();
      }
    });
    e.preventDefault();
    $('#otpsubmit').css('pointer-events', 'none').html('Please Wait');
  });
});
</script>
