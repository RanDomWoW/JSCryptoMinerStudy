			}
		}
		if (!popupSubscribed) {
			document.getElementById('user-id').value = cookieParts[0];
			document.getElementById('user-email').value = cookieParts[2];
			document.getElementById('newsletter-popup--nologeado').style.display = 'none';
			document.getElementById('newsletter-popup--logeado').style.display = 'block';
			document.getElementById('newsletter-popup').style.display = 'block';
		}
	}
	else {
		document.getElementById('newsletter-popup--nologeado').style.display = 'block';
		document.getElementById('newsletter-popup--logeado').style.display = 'none';
		document.getElementById('newsletter-popup').style.display = 'block';
	}
}
</script>
