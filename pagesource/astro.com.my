<script type="text/javascript">
    $(window).load(function () {

        $('#latestcontent .loadmorecontainer .loadmore').click(function () {

            if ($('#latestcontent .contentholder').attr('data-latestcontent-loading') == "no") {
                var filter = $('#latestcontent .dropdownbox>select').val();
