            $('#latestcontent .loading').show();
            $('#latestcontent .loadmorecontainer .loadmore').attr('data-latestcontent-pagination', pagination);

            populateContents(filter, sort, pagination, data);

        })

        if ($('#latestcontent .contentholder>a').length == 0) {
            var cookiename = getCookie("acmLatestContentPreference");
