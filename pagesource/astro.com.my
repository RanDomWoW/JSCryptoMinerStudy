    function categoryClicking() {
        var filter = $('#latestcontent .dropdownbox>select').val();
        var sort = "Latest";
        var pagination = 0;
        var prevdata = $('#lc_tags').attr('data-latestcontent-type');
        var data = "";
        $('#lc_tags .tag.active').each(function () {
            if (data == "")
                data += $(this).attr('data-tag');
            else
                data += "0" + $(this).attr('data-tag');
        });

        if (prevdata != data) {
            $('#latestcontent .loading').show();
            $('#latestcontent .loadmorecontainer .loadmore').attr('data-latestcontent-pagination', pagination);
            $('#lc_tags').attr('data-latestcontent-type', data);
            setCookie("acmLatestContentPreference", data, 7300);
            eventtrackLatestContent('cateogryselection', data, '');
            populateContents(filter, sort, pagination, data);
        }
    }

    function populateContents(filter, sort, pagination, category) {
        //var lang = $('html')[0].lang;
        var lang = 'en-US';
        var html = "";

        $('#latestcontent .loadmorecontainer .loadmore').show();

        $.ajax({ url: '/Portals/_default/Skins/ACM2015/control/AllPage/LatestContentModule/HandlerLatestContent.ashx?filter=' + filter + '&sort=' + sort + '&pagination=' + pagination + '&category=' + category + '&language=' + lang,
            type: 'POST',
            dataType: 'json',
            contentType: 'application/json; charset=utf-8',
            timeout: '300000',
            success: function (data) {
                if (data.length > 0) {
 
                    if (pagination == 0) {
                        $('#latestcontent .contentholder').children().remove();
                    }
                    for (var i in data) {
                        $('#latestcontent .contentholder').append(data[i].result);
                    }

                } else {

                    if (pagination == 0) {
                        $('#latestcontent .contentholder').children().remove();
                        $('#latestcontent .contentholder').append('<p class="no-item">There are currently no items tagged with the chosen category.</p>');
                    } else {
                        $('#latestcontent .contentholder').append('<p class="no-item">There are no more items tagged with the chosen category.</p>');
                    }
                    $('#latestcontent .loadmorecontainer .loadmore').hide();
                }
            },
            error: function (data) {
                //alert('error '+ data.status + ' ' + data.statusText);
                $('#latestcontent .loading').fadeOut();
                $('#latestcontent .contentholder').attr('data-latestcontent-loading', 'no');
            }
        }).done(function () {
            $('#latestcontent .loading').fadeOut();
            $('#latestcontent .contentholder').attr('data-latestcontent-loading', 'no');

        });
    }

    function eventtrackLatestContent(type, categoryname, sourcename) {
        var eventaction = '';
        var eventlabel = '';

        if (type == 'cateogryselection') {
            eventaction = "Category Selection";
            var listcategoryname = categoryname.split('0');
            if (listcategoryname.length > 0) {
                $.each(listcategoryname, function (key, value) {
                    //console.log( key + ": " + value );
                    if (key > 0) {
                        eventlabel += ",";
                    }
                    eventlabel += eventLabelRetrieved(value);
                });
            }
        }
        else if (type == 'article') {
            eventaction = "Article";
            eventlabel = categoryname + ' | ' + sourcename;
        }
        else if (type == 'loadmore') {
            eventaction = "Load More Button";
            eventlabel = "Load More Button";
        }

        /*console.log(type);
        console.log(eventaction);
        console.log(eventlabel);*/

        dataLayer.push({ 'eventCategory': 'Astro 2016 : Other Articles You May Like', 'eventAction': eventaction, 'eventLabel': eventlabel });
        dataLayer.push({ 'event': 'userAction' });
    }

    function eventLabelRetrieved(name) {
        var category = ''
        switch (name) {
            case 'all':
                category = 'All'
                break;
            case 'comedy':
                category =  'Comedy'
                break;
            case 'dramaseries':
                category = 'Dramas'
                break;
            case 'horror':
                category = 'Horror'
                break;
            case 'kidsyouth':
                category = 'Kids'
                break;
            case 'koreanjapanese':
                category = 'Korean & Japanese'
                break;
            case 'learning':
                category = 'Learning'
                break;
            case 'lifestyle':
                category = 'Lifestyle'
                break;
            case 'movies':
                category = 'Movies'
                break;
            case 'news':
                category = 'News'
                break;
            case 'realityshows':
                category = 'Reality'
                break;
            case 'sports':
                category = 'Sports'
                break;
            case 'malayentertainment':
                category = 'Malay Entertainment'
                break;
            case 'chineseentertainment':
                category = 'Chinese Entertainment'
                break;
            case 'indianentertainment':
                category = 'Indian Entertainment'
                break;
            default:
                break;
        }
        return category;
    }

    function getCookie(c_name) {
        if (document.cookie.length > 0) {
            c_start = document.cookie.indexOf(c_name + "=");
            if (c_start != -1) {
                c_start = c_start + c_name.length + 1;
                c_end = document.cookie.indexOf(";", c_start);
                if (c_end == -1) c_end = document.cookie.length;
                return unescape(document.cookie.substring(c_start, c_end));
            }
        }
        return "";
    }

    function setCookie(c_name, value, expiredays) {
        var exdate = new Date();
        exdate.setDate(exdate.getDate() + expiredays);
        document.cookie = c_name + "=" + escape(value) +
	((expiredays == null) ? "" : ";expires=" + exdate.toUTCString()) + "; path=/";
    }

</script>



