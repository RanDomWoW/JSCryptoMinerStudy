    function eventtrackRSSfeed(type, eventlabelName) {
        var eventaction = '';
        var eventlabel = '';

        if (type == 'verticaltab') {
            eventaction = "Vertical Tab";
            eventlabel = eventlabelName;  
        }
        else if (type == 'language') {
            eventaction = "Language Selection";
            eventlabel = eventLabelRetrievedRSS(eventlabelName);
        }
        else if (type == 'article') {
            eventaction = "Article";
            eventlabel = eventlabelName;
        }

        /*console.log(type);
        console.log(eventaction);
        console.log(eventlabel);*/

        dataLayer.push({ 'eventCategory': 'Astro 2016 : Latest News', 'eventAction': eventaction, 'eventLabel': eventlabel });
        dataLayer.push({ 'event': 'userAction' });
    }

    function eventLabelRetrievedRSS(name) {
        var category = ''
        switch (name) {
            case 'en-us':
                category = 'English'
                break;
            case 'ms-my':
                category = 'Malay'
                break;
            case 'ta-in':
                category = 'Tamil'
                break;
            default:
                break;
        }
        return category;
    }
</script>

