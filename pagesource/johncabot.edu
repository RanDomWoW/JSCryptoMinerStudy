<script type="text/javascript">

	
jQuery(document).ready(function( $ ) {
     var url_string = window.location.href;
	 var lang = 'en';
	 var languageString = 'English';
		
	 if (url_string.indexOf("/russian/") >= 0){
		 
		  lang = 'ru';
		  languageString = 'Русский';
			  
	 }else if (url_string.indexOf("/spanish/") >= 0){
		 
		  lang = 'es';
	      languageString = 'Español';
			 
	 }else if(url_string.indexOf("/chinese/") >= 0){
		 
		  lang = 'zh';
		  languageString = '中文';
		 
	 }else if(url_string.indexOf("/universita-americana/") >= 0){
		  
		  lang = 'it';
		  languageString = 'Italiano';
	
	 }
	
	window.language = lang;
	window.langString = languageString;
	
	$('#polyglotLanguageSwitcher a.current').text(languageString);
	$('#polyglotLanguageSwitcher a.current').attr("href" , "javascript:void(0)");
	
	$('#polyglotLanguageSwitcher .dropdown a#'+lang).parent('li').remove();
	
	if(lang != 'en' && $('ul.dropdown li').length < 4 ){
			var el = $('ul.dropdown li').first().clone(true);
		    el.find('a').text('English').attr('href' , 'http://www.johncabot.edu/');
		    $('ul.dropdown').prepend(el);
	}
	
	
	
	

});
	
$( window ).load(function() {

	console.log('debug: window load triggered');
	$(".slider").fadeIn(200);
	$(".kebo-tweets").fadeIn(200);
	
	
	// Mobile Hack
	
	// Make the Current Language Active and Remove Link
	$('li.select-language a').each(function(){
			if( !$(this).hasClass('mm-next') ){
				var CurrentLangLink = $(this);
				CurrentLangLink.text(window.langString);
				CurrentLangLink.attr("href" , "javascript:void(0)");
				return false; 
			}
		
	});
	
	// Remove Current Language From the List	
	$('#mm-2 ul.mm-listview li').each(function(){
		var langText = $(this).find('a').text();	
		if( window.langString == langText){
			$(this).remove();
		}
		
	});

});	
		

	
</script>

