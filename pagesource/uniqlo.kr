<html>
<script type="text/javascript" src="/js/orgRefererCookie.js"></script>
<script type="text/javascript">
function redirectPage()
{
	// 채널쿠키 생성을 위해 이전페이지에 대한 정보를 쿠키로 생성
	makeRefererCookie();
	
	var query = window.location.search.substring(1);

	if( query == '' )
	{
		document.location.href = '/index.jsp';
	}
	else
	{
		document.location.href = '/index.jsp?' + query;
	}
	
}
</script>
<body onload="javascript:redirectPage();"></body>
</html>