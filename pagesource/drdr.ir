                        $('#timer_text').val(60);
                        $('.resend_interval').fadeIn();
                        timer();
                    } else {
                        swal({
                            title: "خطا",
                            text: data.message,
                            type: "error",
                            showCancelButton: false,
                            confirmButtonColor: "#DD6B55",
                            confirmButtonText: "بستن",
                            closeOnConfirm: false
                        }, function (isConfirm) {
                            window.location.reload();
                        });
                    }
                })
                .fail(function () {
                    swal("خطا", "اطلاعات ذخیره نشد، مشکل در برقراری ارتباط.", 'error');
                })
                .always(function () {
                    nanobar.go(100);
                });
    });

    // کلیک روی لینک فراموشی رمز عبور
    $(document).on('click', '#forgetPassword', function (e) {
        e.preventDefault();
        $('.nanobar').remove();
        var nanobar = new Nanobar({target: document.getElementById('nanobar_body')});
        var jqxhr = $.ajax({
            url: "/userApplications/login",
            type: "POST",
            data: {forgetPassword: 1},
            dataType: 'json',
            beforeSend: function (xhr) {
                $('#forgetPassword').find(".icon-spin-6").remove();
                $('#forgetPassword').append("<i class='glyph-icon icon-spin-6 spin'></i>");
                nanobar.go(99);
            }
        })
                .done(function (data) {
                    if (data.success) {
                        if (data.registerCode) {
//                            $("#re-captcha-form1").remove();
//                            grecaptcha.reset();
//                            $("#re-captcha-form2").slideDown();
                        }
                        if (data.resend_count) {
                            $(".login_level_2").slideUp();
                            $(".login_level_3").slideDown();
                            $('.login_forget_mobile input').val(data.mobile);
                            $('#loginRegisterModal #ForgetPasswordForm_password').focus();
                            $('.resend_interval').remove();
                        } else {
                            $(".login_level_2").slideUp();
                            $(".login_level_3").slideDown();
                            $('.login_forget_mobile input').val(data.mobile);
                            $('#loginRegisterModal #ForgetPasswordForm_password').focus();

                            $('#timer_text').val(60);
                            timer();
                        }
                    }
                })
                .fail(function () {
                    swal("خطا", "اطلاعات ذخیره نشد، مشکل در برقراری ارتباط.", 'error');
                })
                .always(function () {
                    $('#forgetPassword').find(".icon-spin-6").remove();
                    nanobar.go(100);
                });
    });

