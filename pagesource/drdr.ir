    // تابع تایمر ارسال مجدد فراموشی رمز عبور
    function timer() {
        var x = setInterval(function () {
            var now = $('#timer_text').val();
            var remained = now - 1;

            $('#timer_text').val(remained);
            $('#resend_message').remove();
            if (remained == 0) {
                $('.resend_interval').fadeOut();
                $('.timer_countainer').append('<div id="resend_message" style="float: left">ارسال مجدد</div>');
                clearInterval(x);
            }
        }, 1000);
    }

    // تابع تایمر ارسال مجدد ثبت نام
    function regTimer() {
        var x = setInterval(function () {
            var now = $('#timer_register_text').val();
            var remained = now - 1;

            $('#timer_register_text').val(remained);
            $('#resend_message_reg').remove();
            if (remained == 0) {
                $('.resend_interval').fadeOut();
                $('.timer_countainer').append('<div id="resend_message_reg" style="float: left">ارسال مجدد</div>');
                clearInterval(x);
            }
        }, 1000);
    }

    // ارسال مجدد پیام وریفای موبایل
    $(document).on('click', '#resend_message_reg', function () {
        var nanobar = new Nanobar({target: document.getElementById('nanobar_body')});
        var jqxhr = $.ajax({
            url: "/userApplications/resendVerification",
            type: "POST",
            dataType: 'json',
            beforeSend: function (xhr) {
                nanobar.go(99);
            }
        })
                .done(function (data) {
                    if (data.title == "success") {
                        $('#resend_message_reg').remove();
                        $('#timer_register_text').val(60);
                        $('.resend_interval').fadeIn();
                        regTimer();
                    } else {
                        swal({
                            title: "خطا",
                            text: data.message,
                            type: "error",
                            showCancelButton: false,
                            confirmButtonColor: "#DD6B55",
                            confirmButtonText: "بستن",
                            closeOnConfirm: false
                        }, function (isConfirm) {
                            window.location.reload();
                        });
                    }
                })
                .fail(function () {
                    swal("خطا", "اطلاعات ذخیره نشد، مشکل در برقراری ارتباط.", 'error');
                })
                .always(function () {
                    nanobar.go(100);
                });
    });

    // ارسال مجدد پیام فراموشی رمز عبور
    $(document).on('click', '#resend_message', function () {
        var nanobar = new Nanobar({target: document.getElementById('nanobar_body')});
        var jqxhr = $.ajax({
            url: "/userApplications/resendPassword",
            type: "POST",
            dataType: 'json',
