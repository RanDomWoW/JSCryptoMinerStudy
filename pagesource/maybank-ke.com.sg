			keyboard:false,
			after: function(slider){
				startCountDown(slider.currentSlide);
				clearInterval(slider.animatedSlides);
				slider.animatedSlides = setInterval(slider.animateSlides,'3000');	
			},
			start: function(slider){
				//console.log('called')
				$('body').removeClass('loading');
				startCountDown(slider.currentSlide);
