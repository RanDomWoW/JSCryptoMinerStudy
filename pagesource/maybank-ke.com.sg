		}, false);
		
			$("#nav_right").bind(Evt.PUSH, function(e){TweenLite.to("#container", 0.5, {x:getRightPosition(), onComplete:onCheckLocation});})
			//.bind("mouseenter", function(e){TweenLite.to ("#nav_right", 0.2, {css: { right: "-13px"  }, ease:Sine.easeInOut});})
			//.bind("mouseleave", function(e){if($(this).hasClass("nav_hide")){}else{TweenLite.to ("#nav_right", 0.2, {css: { right: "-23px"  }, ease:Sine.easeInOut});}});
							
			$("#nav_left").bind(Evt.PUSH, function(e){TweenLite.to("#container", 0.5, {x:getLeftPosition(), onComplete:onCheckLocation});})
			//.bind("mouseenter", function(e){TweenLite.to ("#nav_left", 0.2, {css: { left: "-13px"  }, ease:Sine.easeInOut});})
			//.bind("mouseleave", function(e){if($(this).hasClass("nav_hide")){}else{TweenLite.to ("#nav_left", 0.2, {css: { left: "-23px"  }, ease:Sine.easeInOut});}});						
		
			$("body").keydown(function(e) {
			  if(e.keyCode == 37) { 
				TweenLite.to("#container", 0.5, {x:getLeftPosition(), onComplete:onCheckLocation});
