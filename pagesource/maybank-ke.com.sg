					onCheckLocation();
					/*if(prevX > this.x) {
						//console.log("left :"+ e.pageX);
						onCheckLocation();
						prevX = this.x;
					}
					else if(prevX < this.x) { // dragged right
						//console.log("right :"+ e.pageX);
						onCheckLocation();		
						prevX = this.x;
						
					}*/
						e.stopImmediatePropagation();
						e.stopPropagation();
						e.preventDefault();
				}	
			});
			
		if(parseInt($('#container').width(),10) < somewith){
			Draggable.get('#container').disable();
			$('#main').unbind('touchmove');
		}else{
			Draggable.get('#container').enable();
			initDragable();
			$('#main').bind(
			  'touchmove',
