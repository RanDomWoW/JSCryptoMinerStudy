				Draggable.create("#container", {
				type:"x",
				edgeResistance:0.2,
				bounds:"#main", 
				zIndexBoost :false,
				cursor : "default",
				throwProps:true,
				dragClickables:true,
				onClick: onElementClick,
				onDragEnd:function(e) {
					if(prevX > this.x) {
						//console.log("left :"+ e.pageX);
						onRightComplete();
						prevX = this.x;
					}
					else if(prevX < this.x) { // dragged right
						//console.log("right :"+ e.pageX);
						onLeftComplete();			
						prevX = this.x;
						
					}
					e.stopImmediatePropagation();
					e.stopPropagation();
					e.preventDefault();
				}	
					
			});
			if(parseInt($('#container').width(),10) < somewith){
			Draggable.get('#container').disable();
			$('#main').unbind('touchmove');
		}else{
			Draggable.get('#container').enable();
			initDragable();
			$('#main').bind(
			  'touchmove',
			  function(e) {
			  e.preventDefault();
			  }
			);
		}
			
		window.addEventListener("orientationchange", function() {
			//location.reload();
		}, false);
		
					$("#nav_right").bind(Evt.PUSH, function(e){
						TweenLite.to("#container", 0.5, {x:getRightPosition(), onComplete:onCheckLocation});})
						//.bind("mouseenter", function(e){TweenLite.to ("#nav_right", 0.2, {css: { right: "-13px"  }, ease:Sine.easeInOut});})
						//.bind("mouseleave", function(e){if($(this).hasClass("nav_hide")){}else{TweenLite.to ("#nav_right", 0.2, {css: { right: "-23px"  }, ease:Sine.easeInOut});}});
									
					$("#nav_left").bind(Evt.PUSH, function(e){
						TweenLite.to("#container", 0.5, {x:getLeftPosition(), onComplete:onCheckLocation});})
						//.bind("mouseenter", function(e){TweenLite.to ("#nav_left", 0.2, {css: { left: "-13px"  }, ease:Sine.easeInOut});})
						//.bind("mouseleave", function(e){if($(this).hasClass("nav_hide")){}else{TweenLite.to ("#nav_left", 0.2, {css: { left: "-23px"  }, ease:Sine.easeInOut});}});						
						
					$("body").keydown(function(e) {
					  if(e.keyCode == 37) { 
						 
						TweenLite.to("#container", 0.5, {x:getLeftPosition(), onComplete:onCheckLocation});
					  }
					  else if(e.keyCode == 39) { 
						   
						TweenLite.to("#container", 0.5, {x:getRightPosition(), onComplete:onCheckLocation});
					  }
					});
				}
			
			}
		else
		{
			Draggable.create("#container", {
				type:"x",
				edgeResistance:0.2,
				bounds:"#main", 
				zIndexBoost :false,
				cursor : "default",
				throwProps:true,
				dragClickables:true,
