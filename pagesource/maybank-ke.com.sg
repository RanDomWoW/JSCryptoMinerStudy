					Evt = {
						PUSH : 'click',
						MOVE : 'mousemove',
						RELEASE : 'mouseup'
					};
				}

			$(document).ready(function(){
				
				if(getParameterByName("mode") == "preview")
				{
					$("body a").each(function() {
						var url = $(this).attr("href");
						if(url!=null && url!="")
						{
							if(url.match("^/"))
							{
								url = getParameterByName("url") + url;
								$(this).attr("href",url);
							}
							
						}
		
					});
					
					$("#hd_logo").attr("href",getParameterByName("url"));
				}
			
			
			
				var rest=location.host;
				var url = location.href;
				var res = url.substring(url.indexOf("prod.maybank-ke.com.sg/event-module"));
				var res1= url.substring(url.indexOf("prod.maybank-ke.com.sg/event-module"));
				
				if(res=="prod.maybank-ke.com.sg/event-module")
				{
				   location.href="http://prod.maybank-ke.com.sg/event-module"; 
				}
				
				if(res1=="prod.maybank-ke.com.sg")
				{
					location.href="http://www.maybank-ke.com.sg"; 
				}
			});

		function switchLanguageUrl(lang)
		{
			
			var url = location.href;
			if(lang=="en")
			{
				
				if(url.indexOf("/en") > 0 || url.indexOf("/th") > 0)
				{
				  url = url.replace("/th","/en");
				}
				else
				{
				  url = url + "en";	
				}
			}
			else 
			{
				url = url.replace("/en","/th");	
			}
			location.href = url;
		}

	
	</script>
