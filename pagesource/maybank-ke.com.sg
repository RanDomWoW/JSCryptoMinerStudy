				//console.log($('.flex-control-nav li').length);
				$( ".flexslider " ).append( "<div class='timerBar'></div><div class='timerAnim'></div>" );
			}
		
		//Prevent main slider move
		
		$('.flexslider').on('touchmove',function(){
			Draggable.get('#container').disable();	
			$('#main').unbind('touchmove');
		});
		
		$('#widget,#breadcrum').on('touchstart',function(){
			if(Draggable.get('#container')){
				if($(window).width()<972){
					Draggable.get('#container').disable();
