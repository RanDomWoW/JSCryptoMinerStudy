					Evt = {
						PUSH : 'mousedown',
						MOVE : 'mousemove',
						RELEASE : 'mouseup'
					};
				}

	$(document).ready(function(){
		var prevX = 0;
		var somewith = 992;
		//Browser Detector
		if(navigator.appName.indexOf("Internet Explorer")!=-1){     //yeah, he's using IE
			var badBrowser=(
				navigator.appVersion.indexOf("MSIE 9")==-1 &&   //v9 is ok
				navigator.appVersion.indexOf("MSIE 1")==-1  //v10, 11, 12, etc. is fine too
			);
