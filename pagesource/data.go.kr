<script>
	$(function() {
		$( "#main-popup-modal" ).draggable();
		
		$.ajax({
			url: "/selectNoticePopup.do",
       	  	cache: false,
       	  	dataType: "json",
       	  	success: function(data) {
       	  		if(data.noticePopup != null){
					var noticePopup = data.noticePopup[0];
	       		  
		       		var mainPopAt="true";	// 팝업여부
		    		var popdeAt = "true";	//팝업 날짜 설정 여부
		    		var popupBgnde = noticePopup.popupBeginDe;	//팝업 날짜 설정
		    		var popupEndde = noticePopup.popupEndDe;	//팝업 날짜 설정
		    		
		    		$("#popupSj").text(noticePopup.popupSj);
		    		
		    		var imgUrl = "/comm/file/download.do?atchFileId="+ noticePopup.atchFileId+"&fileDetailSn="+noticePopup.fileDetailSn;
		    		$("#popupImg").css('background', 'url(' + imgUrl + ') no-repeat');
		    		
		    		$("#popupImg").css('width', noticePopup.popupWidth);
		    		$("#popupImg").css('height', noticePopup.popupHeight);
		    		
		    		if(noticePopup.url != null && noticePopup.url.length > 0){
		    			$("#popupImg").css("cursor","pointer");
		    		
						$(document).on("click","#popupImg", function(){
							var w = window.open( noticePopup.url, '_blank');
							w.document.title = "새창";
							
						});
		    		}
		    		
		    		
		    		
		    		var today = '20180318';
		    		
		    		if(mainPopAt == "true" && getCookie('mainPop') != today){
		    			if(popdeAt == "true"){
		    				if(popupBgnde<=today && popupEndde>=today) $("#main-popup-modal").css("display","block");
		    			}else{
		    				$("#main-popup-modal").css("display","block");
		    			}
		    		}
       	  		} else{
       	  			$("#main-popup-modal").css("display","none");
       	  		}
    		
       	  },
       	  error: function (request, status, error) { console.log(status + ", " + error); }
       	});
		
		
		
		
		
	});

	// 쿠키 생성
	function setCookieMainPop(cName, cValue, cDay){
		var expire = new Date();
		expire.setDate(expire.getDate() + cDay);
		cookies = cName + '=' + escape(cValue) + '; path=/ '; // 한글 깨짐을 막기위해 escape(cValue)를 합니다.
		if(typeof cDay != 'undefined') cookies += ';expires=' + expire.toGMTString() + ';';
		document.cookie = cookies;
	}
	
	// 쿠키 가져오기
	function getCookie(cName) {
		cName = cName + '=';
		var cookieData = document.cookie;
		var start = cookieData.indexOf(cName);
		var cValue = '';
		if(start != -1){
			start += cName.length;
			var end = cookieData.indexOf(';', start);
			if(end == -1)end = cookieData.length;
			cValue = cookieData.substring(start, end);
		}
		return unescape(cValue);
	}

	//팝업닫기
	function mainPopClose() {
		//'오늘은 이창을 열지 않음' 체크박스를 클릭했을경우
		if($('input:checkbox[id="chk"]').is(":checked") == true){
			setCookieMainPop('mainPop','20180318', 1);
			//setCookie('mainPop',today, 1);
			//setCookie('mainPop', '', -1);//쿠키제거시사용
		}
		$("#main-popup-modal").css("display","none");
	}
</script>

