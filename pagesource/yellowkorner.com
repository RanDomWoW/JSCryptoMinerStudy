	<!-- type_page :  SplashPage -->
	<script type="text/javascript">
	var dataLayer = [];
	var type_contenu = "SplashPage";
	var localisation = "default";
	var status_log = "true";
	var type_client = "Prospect";
	var freq_achat = parseInt("0");
	var breadcrumb = "/";
	var user_id = "default";
	var type_visiteur = "default";
	var just_login = "null";
	var ccj0_selected = "false";
	var ccj0Store_selected = "";
	var shippingMethod_selected ="null";
	var CouponCode = "null";
	var CouponStatus = "";
	var qNbResults = "null";
	var qSearch = "null";
	var siteLanguage = "fr_FR";
	var shipmentSegment = "segment_A";

	function trackPagesGtmEvent(type,localisation,connexion,client,achat,breadcrumb,userId,visiteur,siteLanguage){
		dataLayer.push({
			'data.type-contenu': type,
			'data.localisation': localisation,
			'data.statut-connexion': connexion,
			'data.type-client': client,
			'data.frequence-achat': achat,
			'data.breadcrumb': breadcrumb,
			'data.user-id': userId,
			'data.type-visiteur': visiteur,
			'Site-LG': siteLanguage
		});
	}

	function trackPagesGtmEventFicheProd(productID,productName){
		dataLayer.push({
			'data.product-id': productID,
			'data.product-name': productName
		});
	}

	function trackPagesGtmEventCheckout(productsIDs,quantityProducts){
		dataLayer.push({
			'product-id-checkout': productsIDs,
			'product-qty-checkout':quantityProducts
		});
	}

	function trackPagesGtmEventCheckoutConfirm(productsIDs,quantityProducts,mail){
		dataLayer.push({
			'product-id-checkout': productsIDs,
			'product-qty-checkout':quantityProducts,
			'email':mail
		});
	}

	function trackTagEvent(eventName, eventCategory ,eventAction, eventLabel) {
		dataLayer.push({
			'event': eventName,
			'eventCategory': eventCategory,
			'eventAction': eventAction,
			'eventLabel': eventLabel
		});
	}

	function trackTagEventValue(eventName, eventCategory ,eventAction, eventValue) {
		dataLayer.push({
			'event': eventName,
			'eventCategory': eventCategory,
			'eventAction': eventAction,
			'eventValue': eventValue
		});
	}
	

	function trackTagEventLabelAndValue(eventName, eventCategory ,eventAction, eventLabel, eventValue) {
		dataLayer.push({
			'event': eventName,
			'eventCategory': eventCategory,
			'eventAction': eventAction,
			'eventLabel': eventLabel,
			'eventValue': eventValue
		});
	}

	function trackTagEventNoLabel(eventName, eventCategory ,eventAction) {
		dataLayer.push({
			'event': eventName,
			'eventCategory': eventCategory,
			'eventAction': eventAction
		});
	}


	function trackPurchaseGtm(id,paymentType,shippingMethod,currency,total,shipping,promoCode,tax,discount,products, wotaxes){
		dataLayer.push({
			'event': 'purchase',
			'transactionId': id,
			'transactionPaymentType': paymentType,
			'transactionShippingMethod': shippingMethod,
			'currencyCode': currency,
			'transactionTotal': total ,
			'transactionShipping': shipping,
			'transactionPromoCode': promoCode,
			'transactionTax': tax ,
			'transactionDiscount':discount,
			'transactionWoTaxes':wotaxes,
			'transactionProducts':products
		});
	}

	function createCookie(name,value,days) {
		if (days) {
			var date = new Date();
			date.setTime(date.getTime()+(days*24*60*60*1000));
			var expires = "; expires="+date.toGMTString();
		}
		else var expires = "";
		document.cookie = name+"="+value+expires+"; path=/";
	}

	function readCookie(name) {
		var nameEQ = name + "=";
		var ca = document.cookie.split(';');
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		}
		return null;
	}

	//Jquery Get Url Param
	function GetURLParameter(sParam){
		var sPageURL = window.location.search.substring(1);
		var sURLVariables = sPageURL.split('&');
		for (var i = 0; i < sURLVariables.length; i++)
		{
			var sParameterName = sURLVariables[i].split('=');
			if (sParameterName[0] == sParam)
			{
			    return sParameterName[1];
			}
		}
	}

	//Login customer status
	var status_log = "true"
	if(status_log == "false"){
		status_log = "Logged";
		user_id = "ab8CufSmtHPY73BdCIcS8xay4A";
	}
	else{
		status_log = "Unlogged";
		freq_achat = "N/A";
		user_id = "N/A";
	}

	//Customer type
	if(freq_achat == 1){
		type_client = "New customer";
	}
	else if(freq_achat > 1){
		type_client = "Repeat customer";
	}

	//Visitor type
	if(readCookie("YK_gtm") == null){
		var date = new Date();
			date.setTime(date.getTime()+(7*24*60*60*1000));
			var expires = "; expires="+date.toGMTString();
		type_visiteur = "New visitor";
		createCookie("YK_gtm","OldVisitor",expires);
	}
	else{
		type_visiteur = "Existing visitor";
	}
	</script>


	
		
	

	

	
	<script type="text/javascript">

		//call this on all pages
		trackPagesGtmEvent(type_contenu,localisation,status_log,type_client,freq_achat,breadcrumb,user_id,type_visiteur,siteLanguage);

		//If product page
		if(type_contenu == "Fiche photographie"){
			var productname = '';
			var productId = '';
			trackPagesGtmEventFicheProd(productId,productname);
		}
		// Checkout Step one : Customer info form
		else if(type_contenu.indexOf("Checkout-Identification") >= 0){
			if(typeof trackPageview != "undefined")
				trackPageview('/livraison');
			var countrySelectedInForm = "fr";
			trackTagEventNoLabel('Funnel', 'VuePageIdentification_checkout', countrySelectedInForm); //Event for GTM
		}
		else if(type_contenu.indexOf("Identification") >= 0){
			if(typeof trackPageview != "undefined")
				trackPageview('/login');
			trackTagEventNoLabel('GAevent', 'Account_Identification', 'Identification'); //Event for GTM

		}
		//If checkout part
		else if(type_contenu.indexOf("Checkout") >= 0){

			// Checkout step two : shipping method choice
			if(type_contenu.indexOf("Checkout-Livraison") >= 0){
				if(typeof trackPageview != "undefined")
					trackPageview('/livraison');
				trackTagEventNoLabel('Funnel', 'VuePageLivraison', 'Livraison_start'); //Event for GTM
			}
			else if(type_contenu.indexOf("Checkout-Paiement") >= 0){
				if(typeof trackPageview != "undefined")
					trackPageview('/paiement');
				trackTagEventNoLabel('Funnel', 'VuePagePaiement', 'Paiement_start'); //Event for GTM
				
				if(shippingMethod_selected=='SFS'){
					trackTagEventNoLabel('Funnel_SFS', 'VuePagePaiement_SFS', 'VuePagePaiement_SFS');
				}
				else if(ccj0_selected=="true"){
					//tag event: TAG Paiement : event : Funnel_CCJ0,eventcategory : VuePagePaiement_CCJ0,eventaction : VuePagePaiement_CCJ0,eventlabel : Galerie sélectionnée
					trackTagEvent('Funnel_CCJ0', 'VuePagePaiement_CCJ0', 'VuePagePaiement_CCJ0', ccj0Store_selected); //Event for GTM
				}
			}else if(type_contenu.indexOf("Checkout-Confirmation") >= 0){
				var ccj0_confirmed = "null";
				trackTagEventNoLabel('Funnel', 'VuePageConfirmation', 'Transaction_success'); //Event for GTM
				trackTagEventLabelAndValue('GAevent', 'A-B_Test_FdP', 'ConfirmationCommande', shipmentSegment, "");
				if(shippingMethod_selected=='SFS'){
					trackTagEventNoLabel('Funnel_SFS', 'VuePageConfirmation_SFS', 'VuePageConfirmation_SFS');
				}else if(ccj0_confirmed=="true"){
					//tag event event : Funnel_CCJ0, eventcategory : VuePageConfirmation_CCJ0,eventaction : VuePageConfirmation_CCJ0, eventlabel : Galerie sélectionnée
					trackTagEvent('Funnel_CCJ0', 'VuePageConfirmation_CCJ0', 'VuePageConfirmation_CCJ0', "null"); //Event for GTM ""					
				}
			}

			var productIds = 'null';
			var email = '';
			var quantityProduct = 'null';
			if(type_contenu.indexOf("Checkout-Panier") >= 0){
				trackTagEventValue('Funnel', 'VuePagePanier', 'VuePagePanier', quantityProduct);
				trackTagEvent('GAevent', 'A-B_Test_FdP', 'VuePagePanier', shipmentSegment);
			}
			else{
				trackPagesGtmEventCheckoutConfirm(productIds,quantityProduct,email);
			}
		}
		else if(type_contenu.indexOf("Error404") >= 0){
			var thisURL = window.location.href;
			var thisReferrer = document.referrer;
			trackTagEvent('GAevent', 'erreur404', thisURL, thisReferrer); //Event for GTM
		}
		else if(type_contenu.indexOf("Redirect301") >= 0){
			var thisURL = window.location.href;
			var thisReferrer = document.referrer;
			trackTagEvent('GAevent', 'redirect301', thisURL, thisReferrer); //Event for GTM
		}

		//track search
		if(qSearch && qSearch != 'null' && qNbResults){
			trackTagEvent('GAevent', 'search_header',qSearch,qNbResults);
		}

		//track confirm cmde
		if("false" == "true"){
			var transactionProduct = null;
			trackPurchaseGtm("","","","","","","","","",transactionProduct, "");
			if(typeof trackPageview != "undefined")
				trackPageview('/confirmation');
		}
	</script>

	
	
	
		
	
	

