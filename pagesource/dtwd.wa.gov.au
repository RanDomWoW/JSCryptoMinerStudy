  $(document).ready(function(){
    var counter = 0;
    function setAcronym(term) {
      var termIndex = aArr.indexOf(term);
      var termTitle = tArr[termIndex].replace("'", "\'");
      var re = new RegExp("\\b"+term+"\\b", "g");
      $("body:not(.page-glossary) .c-main-content *").contents().filter(function() { return this.nodeType === 3; }).each(function(i, el){
        var data = el.data || el.textContent || el.innerText;
        var modData = data.replace(re, "<span class='acronym' data-content='"+ termTitle +"'>" + term + "</span>");
        if (data !== modData) {
          counter++;
          var wrapper = $("<span>").html(modData);
          $(el).before(wrapper.contents()).remove();
        }
      });
    }

    $("body").on("click", "span.acronym", function(){
      if (window.innerWidth <= 767){
        $(this).addClass("open");
      }
    });

    $("body").on("click mouseover", "span.acronym", function(){
      $(this).removeClass("anchored-left");
      $(this).removeClass("anchored-right");
      if (($(this).offset().left + (0.5 * $(this).width())) < 140){
        $(this).addClass("anchored-left");
      }
      if (((window.innerWidth - $(this).offset().left) - (0.5 * $(this).width())) < 140){
        $(this).addClass("anchored-right");
      }
    });

    $(window).scroll(function(){
      $("span.acronym").removeClass("open");
    });

    //var a = performance.now();
    for (var x=0; x<aArr.length; x++){
      setAcronym(aArr[x]);
    }
    //var b = performance.now();
    //console.log(aArr.length + " acronyms in total. "+ counter +" acronym replacements made, taking " + Math.round(b - a) + "ms to process.");

  });

})(jQuery);
