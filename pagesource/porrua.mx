<script type="text/javascript">

$(document).ready(function(){
	$("#slider,#banner1,#banner2,#banner3,#banner4").click(function () {
	var Idbanner = this.id;
	var section;
	var banner_posicion;
	switch(Idbanner){
	case "slider":
		var dataSlide = parseInt($('#carousel-example-generic').find('.active').attr('data-slide-to')) + parseInt(1);
		section= "superiores_grandes";
		banner_posicion = dataSlide;
		break;
	case "banner1":
		section ="superiores_chicos";
		banner_posicion = 1;
		break;
	case "banner2":
		section ="superiores_chicos";
		banner_posicion = 2;
		break;
	case "banner3":
		section ="inferiores_chicos";
		banner_posicion = 1;
		break;
	case "banner4":
		section = "inferiores_chicos";
		banner_posicion = 2;
		break;
	default:
		//alert("no hizo clic en ningun banner");
		break;
	}
	$.ajax({
		url:'inc_banners_home.php',
        type: 'get',
        data:{
			bannerSeccion:section, 
			bannerPosicion:banner_posicion
		},
		cache: false,
        success:function(data){
        }

        });
	});
});

</script>