		$(this).hide();
		$(".index_search_input").removeClass("index_search_input_s");
	});
	$("#q").focus(function(){
		if ($(this).val() == q_defaultValue) {
			$(this).val("");
			$(this).addClass("index_search_input_s");
		} 
	}).blur(function(){
		if (!$(this).val() || $(this).val() == q_defaultValue) {
			$(this).removeClass("index_search_input_s");
			$(this).val(q_defaultValue)
		}
	});
	$("#searchStockInput").focus(function(){
