	function getCookie(cname) {
	    var name = cname + "=";
	    var ca = document.cookie.split(';');
	    for(var i = 0; i <ca.length; i++) {
	        var c = ca[i];
	        while (c.charAt(0)==' ') {
	            c = c.substring(1);
	        }
	        if (c.indexOf(name) == 0) {
	            return c.substring(name.length,c.length);
	        }
	    }
	    return "";
	} 

	var d = new Date();
   	d.setTime(d.getTime());
    //var enddate= Date.parse(getCookie('enddate'));
    //if(d < enddate){
    	//jQuery( "div.mass-popup" ).remove();
    //}
    var clickclose= Date.parse(getCookie('clickclose'));
    if(d < clickclose){
    		jQuery( "div.mass-popup" ).remove();
    }

    jQuery( "img.popup-close" ).click(function() {
			jQuery( "div.mass-popup" ).remove();
			var d = new Date();
			d.setTime(d.getTime()+43200000);
			document.cookie =  "clickclose="+ d.toUTCString();
		});

	jQuery( document ).ready(function() {
		setTimeout(function () { 
		    var mt=(jQuery(window).height() - jQuery('div.main-popup > div').height())/2;
		    jQuery('div.main-popup > div').css('top',mt);
		    jQuery('div.main-popup > div').css('left',(jQuery(window).width() - jQuery('div.main-popup > div').width())/2);
		    jQuery('.mass-popup').css('opacity',1);
		}, 1000);
	    //	    alert(jQuery('div.main-popup > div .img-popup').height());
	});
	jQuery( window ).resize(function() {
	    var mt=(jQuery(window).height() - jQuery('div.main-popup > div').height())/2;
	    jQuery('div.main-popup > div').css('top',mt);
	    jQuery('div.main-popup > div').css('left',(jQuery(window).width() - jQuery('div.main-popup > div').width())/2);
	});
	jQuery( "div.mass-popup" ).click(function() {
		jQuery( "div.mass-popup" ).remove();
	});
	
	
	setTimeout(function () { 
		jQuery( "div.mass-popup" ).remove(); 
	}, 99999);

</script>
