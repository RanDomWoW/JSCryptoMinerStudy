<script type="text/javascript">
var cookie_str = getCookie("fx_user");
//为空  需要登录
if(strEsempty(cookie_str)){	
	$(".zhankai").show();
}else{
	//cookie里面有值可以直接登录
	var temp_str = cookie_str.replace(/"/g, "")
	var nickname = getNickname(temp_str);
	
	$("#nickname").css("position","absolute");
	$("#nickname").css("top","10px");
	$("#nickname").css("color","#FFF");
	$("#nickname").css("left","1135px");
	$("#nickname").css("border","none");
	$("#nickname").css("width","250px");
	var html = "<a style='color: #FFFFFF;text-align: center;' href='javascript:void(0)'>"+nickname+" , 您好！   </a><a style='color: #FFFFFF;text-align: center;text-decoration: underline;' href='javascript:void(0)' onclick='outLogin()'> 退出 </a>";
	
	//显示昵称
	$("#nickname").html(html);
	$("#nickname").show();
}

//读取cookies
function getCookie(name){
    var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");
    if(arr=document.cookie.match(reg))
        return unescape(arr[2]);
    else
        return null;
}
//读取nickname
function getNickname(cookie_str){
 var arrs=cookie_str.split(",");
 return arrs[2];
}
//判断是否为空
function strEsempty(temp){
	if(temp == null || temp==undefined|| temp=="undefined" || typeof(temp)=="undefined" ||   temp == "null" ||  temp == "" || temp.length==0 ){
		return true;
	}else{
		return false;
	}
}

function outLogin(){
	delCookie("fx_user");
}

function delCookie(name){
	var exp = new Date();
	exp.setTime(exp.getTime() - 1);
	var cval=getCookie(name);
	if(cval!=null)
	document.cookie=name+"=; expires="+exp.toGMTString()+"; path=/;domain=fxgold.com";
	$("#nickname").hide();
	$(".zhankai").show();
}
	
</script>

