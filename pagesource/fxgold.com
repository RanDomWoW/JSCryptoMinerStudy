			  $("#next").hover(function(){ 
			    clearInterval(autoChange);
			  },function(){ 
			    autoChangeAgain();
			  });
			  $("#next").click(function(){ 
			    curIndex = (curIndex < imgLen - 1) ? (++curIndex) : 0;
			    changeTo(curIndex);
			  });
			  $(".indexList").find("li").each(function(item){ 
			    $(this).hover(function(){ 
			      clearInterval(autoChange);
			      changeTo(item);
			      curIndex = item;
			    },function(){ 
			      autoChangeAgain();
			    });
			  });
			  function autoChangeAgain(){ 
			      autoChange = setInterval(function(){ 
			      if(curIndex < imgLen-1){ 
