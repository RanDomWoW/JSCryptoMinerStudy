        <script>
            function setCookie(cname, cvalue, exdays) {
                var d = new Date();
                d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
                var expires = "expires=" + d.toUTCString();
                document.cookie = cname + "=" + cvalue + "; " + expires;
            }

            function are_cookies_enabled()
            {
                var cookieEnabled = (navigator.cookieEnabled) ? true : false;

                if (typeof navigator.cookieEnabled == "undefined" && !cookieEnabled)
                {
                    document.cookie="testcookie";
                    cookieEnabled = (document.cookie.indexOf("testcookie") != -1) ? true : false;
                }
                return (cookieEnabled);
            }

            function getCookie(cname) {
                var name = cname + "=";
                var ca = document.cookie.split(';');
                for (var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == ' ') c = c.substring(1);
                    if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
                }
                return "";
            }
            function validateEmail(email) {
                var re = /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
                return re.test(email);
            }

            $(document).ready(function () {
                $( ".country_select" ).change(function() {
                        var id = $(this).attr("class").match(/count__\w+/)[0];
                        if(id) {
                            var zipCode = $("input." + id);
                            if ($(this).val() == '' || $(this).val() == 250) zipCode.attr("required", "required");
                            else zipCode.removeAttr("required");
                        }
                    }
                );
                $(".lower-case-force").keyup(function(event){
                   $(this).val($(this).val().toLowerCase());
                });
                if(are_cookies_enabled()) {
                    if (!getCookie("cookieDirective")) {
                        $('#cookiesdirective').toggle("fade", 2000);
                    }
                }
                else {
                    $('#cookiesdisabled').toggle("fade", 2000);
                }
                $('#accept-cookie').click(function (event) {
                    event.preventDefault();
                    setCookie("cookieDirective", "all", 365);
                    $('#cookiesdirective').toggle("fade", 2000);
                });
                $.datepicker.setDefaults($.datepicker.regional['fr']);
            });
        </script>
    
