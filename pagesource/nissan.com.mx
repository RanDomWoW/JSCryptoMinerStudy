        <script>
            function setCookie(cname, cvalue, exdays) {var d = new Date();d.setTime(d.getTime() + (exdays*24*60*60*1000));var expires = "expires="+d.toUTCString();document.cookie = cname + "=" + cvalue + "; path=/; " + expires;}
            function getCookie(cname) {var name = cname + "=";var ca = document.cookie.split(';');for(var i=0; i<ca.length; i++) {var c = ca[i];while (c.charAt(0)==' ') c = c.substring(1);if (c.indexOf(name) == 0) return c.substring(name.length, c.length);}return "";}
            function getParam(p){var match = RegExp('[?&]' + p + '=([^&]*)').exec(window.location.search);return match && decodeURIComponent(match[1].replace(/\+/g, ' '));}
        </script>
        
        <!--     DATALAYER GTM       -->
        <script>
            if(getCookie('nissan-user-id') == ""){
                var customUserId = "";
                var possible = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789";
                for( var i = 0; i < 9; i++ ){
                    if(i%3 == 0 && i !== 0){
                        customUserId += '-';
                    }
                    customUserId += possible.charAt(Math.floor(Math.random() * possible.length));
                    
                }
                setCookie('nissan-user-id',customUserId,2500);
            }else{
                var customUserId = getCookie('nissan-user-id');
            }
            var dataLayer = [{
                'event': 'analyticsEvent', 
                'eventCategory': 'customCategory', 
                'eventAction': 'customEventAction', 
                'eventLabel': 'customEventLabel',
                'userID': customUserId
            }];
            if(getParam('gclid')){
                var gclsrc = getParam('gclsrc');
                if(!gclsrc || gclsrc.indexOf('aw') !== -1){
                    setCookie('gclid', getParam('gclid'), 2500);
                }
            }
        </script>
