        <script type="text/javascript">
            $(document).ready(function(){
                var requestAnimationFrame = window.requestAnimationFrame || window.mozRequestAnimationFrame || window.webkitRequestAnimationFrame || window.msRequestAnimationFrame;
                
                var winInitHeight = $(window).height();
                
                var icons = $('.iconAppear'),
                    circle = $('.circle'),
                    offset = 1000,
                    requestID;
                    
                var offsetMe = function() {
                    if(offset <= 650){
                        return;
                    }
                    icons.css('stroke-dashoffset', offset);
                    circle.css('stroke-dashoffset', offset);
                    offset--;
                    requestID = requestAnimationFrame(offsetMe);
                    
                }

                $('.appearFromBottom, .appearFromLeft, .fadeIn').each(function(i){
                    var initPosTop = $(this).offset().top;
                    if(initPosTop >= winInitHeight){ 
                        if($(this).hasClass('appearFromBottom')){
                            $(this).addClass('non-create');
                        }else if($(this).hasClass('appearFromLeft')){
                            $(this).addClass('non-create');
                        }else if($(this).hasClass('fadeIn')){
                            $(this).addClass('non-create');
                        }
                        
                    }
                });

                $(window).scroll(function(){        
                    var winHeight = $(window).height();
                    var winPosTop = $(this).scrollTop();
                    var winPosBot = winPosTop + winHeight;
                    $('.appearFromLeft, .appearFromBottom, .fadeIn').each(function(i){
                        var animPosTop = $(this).offset().top;
                        var animHeight = $(this).outerHeight();
                        var animPosBot = animPosTop + 200;
                        if ((animPosBot <= winPosBot) && (animPosTop >= winPosTop) && ($(this).hasClass('non-create'))) {
                            if($(this).hasClass('appearFromLeft')){
                                $(this).removeClass('non-create');
                                $(this).addClass('createFromLeft');
                            }else if($(this).hasClass('appearFromBottom')){
                                $(this).removeClass('non-create');
                                $(this).addClass('createFromBottom');
                                offsetMe();

                                setTimeout(function(){
                                    // $('.createFromBottom .iconAppear').show();
                                    offsetMe();
                                }, 3000);
                            }else if($(this).hasClass('fadeIn')){
                                $(this).removeClass('non-create');
                                $(this).addClass('createFadeIn');
                            }
                        }
                    });
                });
            });
        </script>
        
        

        
