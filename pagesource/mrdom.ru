    <script>
    
        function callbackToogle(sender_name) {
            console.log(sender_name)
            if (sender_name != 'close') {
                $obj = $('.callback-form');
                $('.callback-form').detach();
                $(sender_name+' .callback-button').after($obj);
                $('.callback-form').removeClass('hidden');
                if (sender_name == 'header') {
                    $('.callback-form').addClass('header-form');
                    $('.callback-form').removeClass('footer-form'); 
                }
                if (sender_name == 'footer') {
                    $('.callback-form').removeClass('header-form');
                    $('.callback-form').addClass('footer-form');
                }
            }
            else {
                $('.callback-form').toggleClass('hidden');
            }
        }
        <!-- показывает и скрывает форму регистрации -->
        function showReg() {
            var state = $('#reg-wrapper').css('display');
            if (state=='none') {
                $('#reg-wrapper').css('display','block');
            }
            else {
                $('#reg-wrapper').css('display','none');
            }
            
        }
        
        
        <!-- показывает и скрывает форму восстановления пароля -->
        function showRecovery() {
            var state = $('#password-recovery-wrapper').css('display');
            if (state=='none') {
                $('#password-recovery-wrapper').css('display','block');
            }
            else {
                $('#password-recovery-wrapper').css('display','none');
            }
            
        }
        <!-- показывает и скрывает формы статуса заказа -->
        function showOrderStatus(sender) {
            var obj = $(sender);
            if (obj.hasClass('form-close')){
                $('#order-stat-form1').css('display','none');
                $('#order-stat-form2').css('display','none');
                $('#order-stat-form3').css('display','none');
            }
            else{
                var state = $('#order-stat-form1').css('display');
                if (state=='block') {
                    $('#order-stat-form1').css('display','none');
                }
                else {
                    $('#order-stat-form1').css('display','block');
                } 
            }
            if (obj.is('form')) {
                $('#order-stat-form1').css('display','none');
                $('#order-stat-form2').css('display','none');
                $('#order-stat-form3').css('display','none');
                if (obj.attr('id')=='form-order-stat-1') {
                    $('#order-stat-form2').css('display','block');
                }
                if (obj.attr('id')=='form-order-stat-2') {
                    $('#order-stat-form3').css('display','block');
                }
            }     
        }
        <!-- показывает и скрывает форму обратной связи -->
        function showAskQuestion(sender) {
            var state = $('#ask-question-form').css('display');
            if (state=='none') {
                $('#ask-question-form').css('display','block');
            }
            else {
                $('#ask-question-form').css('display','none');
            }   
        }
        <!-- угадайка города -->
        function hideForm(obj) {
            var form = $(obj);
            if (form.hasClass('show')) {
            form.removeClass('show');
            form.addClass('hide');
            } else {
            form.removeClass('hide');
            form.addClass('show');            
            }
        }
        function citySelect(obj) {
            hideForm(obj);
            $('#geo .selectric').click();
            $('#geo .selectric-input').val('');
        }
        <!-- проверка формы -->
        function formSubmit() {
            var res = true;
            var old_placeholder = '';
            var reg = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            $('#registration-form>input[type=text]').each(function(index,value){
                if (this.value.trim().length==0) {
                    res=false;
                    old_placeholder = $(this).attr("placeholder");
                    $(this).attr("placeholder", "Поле "+old_placeholder+" обязательно для заполнения");
                    $(this).addClass('error');
                    
                }
            })
            
            if ( $('#registration-form>input[type=email]').val().trim().length==0) {
                    res=false;
                    old_placeholder = $('#registration-form>input[type=email]').attr("placeholder");
                    $('#registration-form>input[type=email]').attr("placeholder", "Поле "+old_placeholder+" обязательно для заполнения");
                    $('#registration-form>input[type=email]').addClass('error');
            }
            else {
                if (!reg.test($('#registration-form>input[type=email]').val())) {
                    res=false;
                    $('#registration-form>input[type=email]').addClass('error');
                }
            }
              
            if ( $('#registration-form>input[type=password]').val().trim().length==0) {
                    res=false;
                    old_placeholder = $('#registration-form>input[type=password]').attr("placeholder");
                    $('#registration-form>input[type=password]').attr("placeholder", "Поле "+old_placeholder+" обязательно для заполнения");
                    $('#registration-form>input[type=password]').addClass('error');
            }
                
                
                
            if ($('#reg-confirm').is(':not(:checked)')) {
                $('#registration-form>.err-msg').removeClass('hide');
                $('#registration-form>.err-msg').addClass('show');
                res=false;
            }
            
            
            return res;
        }
        
        function testAskQuestion() {
            var res = true;
            
            var mail = testMail($('#form-ask-question input[type="email"]'),false);
            
            var notEmpty = testNotEmpty($('#form-ask-question textarea'));
            
            console.log(notEmpty);
            
            res = mail && notEmpty;
            
            return res;
        }
        
        function passRecovery() {
            var res = true;
            var old_placeholder = '';
            var reg = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            if ( $('#form-password-recovery>input[type=email]').val().trim().length==0) {
                    res=false;
                    old_placeholder = $('#form-password-recovery>input[type=email]').attr("placeholder");
                    $('#form-password-recovery>input[type=email]').attr("placeholder", "Укажите e-mail");
                    $('#form-password-recovery>input[type=email]').addClass('error');
            }
            else {
                if (!reg.test($('#form-password-recovery>input[type=email]').val())) {
                    res=false;
                    $('#form-password-recovery>input[type=email]').addClass('error');
                }
            }
            return res;
        }
        
        
        
        
        function orderStatFormCheck(sender) {
            
            var form = $(sender);
            var form_id = $(sender).attr('id');
            res = true;
            switch (form_id) {
                case 'form-order-stat-1': {
                    if ( $('#order-stat-phone').val().trim().length==0) {
                    res=false;
                    old_placeholder = $('#order-stat-phone').attr("placeholder");
                    $('#order-stat-phone').attr("placeholder", "Данное поле обязательно для заполнения");
                    $('#order-stat-phone').addClass('error');
                    }
                    break;
                }
                case 'form-order-stat-2': {
                    res=true;
                    break;
                }
            }
            return res;
        }
        
        
        function sendViaAjax(sender) {
            $res = orderStatFormCheck(sender);
            alert($res);
            if ($res==true)
            {
            var form = $(sender);
            var form_id = $(sender).attr('id');
            switch (form_id) 
							{
                case 'form-order-stat-1': 
                    /*$.post("/status", form.serialize(),  function(response) {
			
			             //$('#post_form_success').html(response);
		            });*/
                    $('#form-order-stat-2').data('phone',$('#order-stat-phone').val());
                    showOrderStatus(sender);
                    break;
                
                case 'form-order-stat-2': 
                    $.get('/status',	[],  function(data) {$('#order-stat-form3').html(data);});

                    //$('#form-order-stat-3').data('result',$('#order-stat-phone').val());

                    showOrderStatus(sender);
                    break;
							}
            }    
		    return false;
            }
            
        function tabsSwitch(sender) {
            var btn = $(sender);
            if (btn.hasClass('passive')) {
                if (btn.attr('id')=='completed-orders') {
                    $('.orders-list-wrapper.current').css('display','none');
                    $('.orders-list-wrapper.completed').css('display','block');
                    btn.removeClass('passive');
                    btn.addClass('active');
                    $('#current-orders').removeClass('active');
                    $('#current-orders').addClass('passive');
                }
                else
                {
                    $('.orders-list-wrapper.completed').css('display','none');
                    $('.orders-list-wrapper.current').css('display','block');
                    btn.removeClass('passive');
                    btn.addClass('active');
                    $('#completed-orders').removeClass('active');
                    $('#completed-orders').addClass('passive');  
                }
            }
