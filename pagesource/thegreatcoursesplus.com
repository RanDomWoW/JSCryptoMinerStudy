				>
				
				<a class="object-link {{getTraySlideImageSize imageOverridden ../../trayType ../../config.defaultTrayType}}" 
					href="{{permalink}}" data-type="{{type}}">
					<div class="object-link-image-container" data-object-id="{{id}}">
						{{#lazyLoadTrayImages ../lazyLoad (getFirstDefinedValue ../../trayType ../../config.defaultTrayType) @index ~}}
							<img class="hidden-xs" src="{{#if (getFirstDefinedValue ../episodic @root.data.episodic)}}{{images.widget}}{{else}}{{image}}{{/if}}" alt="Image of {{title}}" />
							<img class="visible-xs" src="{{#unless imageXS}}{{#if (getFirstDefinedValue ../episodic @root.data.episodic)}}{{images.widget}}{{else}}{{image}}{{/if}}{{/unless}}{{#if imageXS}}{{imageXS}}{{/if}}" alt="Image of {{title}}" />
						{{~/lazyLoadTrayImages}}
						
						{{#if featuredTag}}
						<div class="featured-tag-container">
							<img src="{{featuredTag}}" class="featured-tag-image" alt="Featured banner" />
						</div>
						{{/if}}
						
						<div class="play-icon">{{{svgIcon "icon-play"}}}</div>
					</div>
			
					<div class="title slider-title contain">
						{{#if preTitle}}
							<div class="slider-pre-title">{{{preTitle}}}</div>
						{{/if}}
						<span class="title-content">{{#compare (getFirstDefinedValue ../maxTitleDisplayLength @root.data.maxTitleDisplayLength) false operator="!=" ~}}
								{{{ellipsify title (getFirstDefinedValue ../maxTitleDisplayLength @root.data.maxTitleDisplayLength)}}}
						{{~ else ~}}
								{{title}}
						{{~/compare~}}
						</span>{{#if postTitle ~}}
							<div class="slider-post-title">{{postTitle}}</div>
						{{~/if~}}
					</div>
			
					<span class="item-icon"><span class="item-inside ion-ios7-arrow-right"></span></span>
				</a>
				
				{{#unless ../episodic}}
					{{#if queueable}}
						<button disabled="disabled" class="queue-delegate async color-site with-icons" 
							data-queued="false" 
							data-content-id="{{id}}" 
							data-queued-text="<span class='queue-icon'>{{{svgIcon (getFirstDefinedValue ../../config.dequeueIconClassName @root.config.dequeueIconClassName)}}}</span> <span class='queue-text'>{{getFirstDefinedValue ../../config.dequeueText @root.config.dequeueText}}</span>"
							data-dequeued-text="<span class='queue-icon'>{{{svgIcon (getFirstDefinedValue ../../config.queueIconClassName @root.config.queueIconClassName)}}}</span> <span class='queue-text'>{{getFirstDefinedValue ../../config.queueText @root.config.queueText}}</span>"
							>
							<span class="queue-icon">
								{{{svgIcon (getFirstDefinedValue ../../config.queueIconClassName @root.config.queueIconClassName)}}}
							</span> 
							<span class="queue-text">{{getFirstDefinedValue ../../config.queueText @root.config.queueText}}</span>
						</button>
					{{/if}}
				{{/unless}}
				
				{{#if ../progress}}
					<div class="film-progress"><div class="bar" style="width: {{watchInfo.watchedPercentage}}%;"></div></div>
				{{/if}}
			</div>
		{{/each}}
		</div>
		<span class="next">{{{svgIcon "icon-arrow-right"}}}</span>
	</div>
</div>
{{/with}}

{{/unless}}

{{#if data.addModuleContainer}}
</div>
{{/if}}
</script>
<script id="login-template" type="text/x-handlebars">
<div class="sign-in-container">
	<h1 class="site-color hidden-xs">Sign In to Your Account</h1>
	<h1 class="site-color visible-xs">Sign In</h1>
	<form action="#" method="post" name="login">
		<p class="label-focus-container">
			<label class="label-focus" for="Email Address">Email Address</label>
			{{#if email}}
			<input type="email" class="sign-in-input" required name="email" value={{email}} />
			{{else}}
			<input type="email" class="sign-in-input" required name="email" />
			{{/if}}
			<span class="active-line"></span>
			<label for="email" style="display:none;"></label>
		</p>
		<p class="label-focus-container">
			<label class="label-focus" for="password">Password</label>
			<input type="password" class="sign-in-input" required name="password" />
			<span class="active-line"></span>
			<label for="password" style="display:none;"></label>
		</p>
		<p class="forgot-password-link-container">
			Forgot your <a href="/forgot-password">Email or Password?</a>
		</p>
		<p>
			<input type="checkbox" checked name="rememberMe" />
			<span class="remember-me">Remember Me</span>
		</p>
		<p class="text-center">
			<button class="color-site sign-in-button" type="submit">Sign In</button>
		</p>
	</form>
	<hr class="visible-xs">
	<p class="tgc-auth-or visible-xs">OR</p>
	<p class="text-center">
		<button class="with-icons facebook facebook-signin">
			<span class="social-icon">{{{svgIcon "icon-facebook"}}}</span>
			<span>Sign In using Facebook</span>
		</button>
	</p>
</div>
<div class="dashed-line"></div>
<div class="login-flow-right-section">
	<p>Don't have an Account?&nbsp;&nbsp;<font class="visible-xs sign-up-button mobile-signup-text">Sign Up Today!</font></p>
	<p class="visible-xs">FREE 1 Month Trial Available!</p>
	<button class="color-site sign-up-button hidden-xs" type="submit">Choose a Plan/Sign Up</button>
</div></script>
<script id="register-template" type="text/x-handlebars">
<div class="login-container">
	<h1 class="site-color">Create a Membership Account</h1>
	<form action="#" method="post" name="register">
		<p class="label-focus-container">
			<input class="large" type="email" required name="email" placeholder="Enter Your Email Address." />
			<label class="label-focus" for="email">Enter Your Email Address.</label>
			<label for="email" style="display: none;"></label>
		</p>
		<p class="label-focus-container">
			<input class="large" type="password" required name="password" placeholder="Create Password" />
			<label class="label-focus" for="password">Create Password</label>
			<label for="password" style="display: none;"></label>
		</p>
		{{#if data.googleRecaptchaSiteKey}}
			<div class="g-recaptcha" data-sitekey="{{data.googleRecaptchaSiteKey}}"></div>
			<label for="g-recaptcha-response" style="display: none;"></label>
		{{/if}}
		<p>
			<input type="checkbox" checked name="rememberMe" class="remember-me" /> Remember Me
		</p>
		<p class="text-center">
			<input type="hidden" name="registrationType" value="snagfilms" />
			<button class="large color-site" type="submit">Sign Up</button>
		</p>
		<p id="registration-terms-agreement-message" class="text-center">
			By clicking <em><strong>Sign Up</strong></em>, you agree to our <a href="/tos" target="_blank">terms of use</a> and <a href="/privacy" target="_blank">privacy policy</a>.

			<style type="text/css">

			#registration-terms-agreement-message {
				font-size: 14px;
				margin-top: 20px;
    			margin-bottom: 20px;
			}

			</style>
		</p>
	</form>
	
	<hr />
	
	<p class="tgc-auth-or">OR</p>
	<p class="text-center">
	{{#if config.facebookLogin}}
		<button class="with-icons facebook facebook-login">
			<span class="social-icon">{{{svgIcon "icon-facebook"}}}</span>
			<span>Sign up with Facebook</span>
		</button>
	{{/if}}
	</p>
	
	<hr />
	
	<p class="register-link-container">Have an account? <a class="login-link" href="/sign-in">Sign In</a></p>
</div>
</script>
<script id="typeahead-search-results-template" type="text/x-handlebars">
<div class="typeahead-search-results-container">
{{#if errorMessage}}
	<p class="typeahead-search-results-error">{{errorMessage}}</p>
{{else}}
	{{#each results}}
		<a data-type="{{contentType}}" href="{{permaLink}}" class="typeahead-search-result {{#if @first}}large-search-result{{/if}}">
			{{#if @first}}<div class="image-container"><img alt="{{title}}" src="{{imageUrl}}" /></div>{{/if}}
			<div class="title-container">
				{{#compare contentType "film"}}
				<span class="item-icon">{{{svgIcon "icon-film"}}}</span>
				{{/compare}}
				<span>{{title}}</span>
			</div>
		</a>
	{{/each}}
{{/if}}
</div></script>
<script id="player-template" type="text/x-handlebars">
<div class="film-player">
	<div class="film-container">
		<iframe src="/embed/player?filmId={{id}}{{#if isClip}}&clip=true{{/if}}" frameborder="0" allowfullscreen="true" width="100%" height="100%"></iframe>
	</div>
</div></script>
