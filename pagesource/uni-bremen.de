            //Mute/Unmute control clicked
            $('.dce'+id+' .video-controls .unmuted').on('click mouseup', function(e) {
                if(e.which == 1) {
                    video = $(this).closest('.video-wrap').children('video');
                    e.stopPropagation();
                    e.preventDefault();
                }
            });
            $('.dce'+id+' .video-controls .unmuted').on('mousedown', function(e) {
                if(e.which == 1) {
                    e.stopPropagation();
                    e.preventDefault();
                    video = $(this).closest('.video-wrap').children('video');
                    $(this).toggleClass('muted');
                    video[0].muted = !video[0].muted;

                    if(video[0].muted) {
                        video.closest('.lslide.active .video-wrap').find('.volume').css('width', '0%');
                    } else {
                        video.closest('.lslide.active .video-wrap').find('.volume').css('width', video[0].volume*100+'%');
                    }
                }
            });

            var volumeDrag = false;
            var clickedProgress2;
            //Volume control clicked
            $('.dce'+id+' .video-controls .video-volume .volumeBar').mousedown(function(e) {
                if(e.which == 1) {
                    e.stopPropagation();
                    clickedProgress2 = $(this);
                    volumeDrag = true;
                    updateVolume(e.pageX, $(this));
                }
            });

            $('.dce'+id+' .video-controls .video-volume .volumeBar').on('click mouseup', function(e) {
                if (volumeDrag) {
                    video = $(this).closest('.video-wrap').children('video');
                    e.stopPropagation();
                    volumeDrag = false;
                    updateVolume(e.pageX, clickedProgress2);
                }
            });

            var updateVolume = function(x,object) {
                var position = x - object.offset().left;
                var percentage = 100 * position / object.width();
                if(percentage > 100) percentage = 100;
                else if(percentage < 0) percentage = 0;

                if(percentage == 0) {
                    object.closest('.video-volume').find('.unmuted').addClass('muted').removeClass('half');
                } else if(percentage > 0 && percentage < 50) {
                    object.closest('.video-volume').find('.unmuted').removeClass('muted').addClass('half');
                    video[0].muted = false;
                } else {
                    object.closest('.video-volume').find('.unmuted').removeClass('muted half');
                    video[0].muted = false;
                }
                object.find('.volume').css('width', percentage+'%');
                video[0].volume = percentage / 100;
            };

