    var index = 0;
    var widthOfSlide = 0;
    var totalSlides = 0;
    var slides = [];
    var screenWidth = window.innerWidth
        || document.documentElement.clientWidth
        || document.body.clientWidth;

    var intervalTimeout = false;

    var direction = 'next';

    document.addEventListener("DOMContentLoaded", function() {
        slides = document.querySelectorAll('.banner-slide');

        totalSlides = slides.length - 1;

        var slide = slides[0];
        var width = parseInt(getComputedStyle(slide.parentElement.parentElement).width);
        var height = parseInt(getComputedStyle(slide.parentElement.parentElement).height);

        slide.parentElement.parentElement.dataset.slideLength = width;
        widthOfSlide = width;

        for (var i = 0; i < slides.length; i++) {
            var single = slides[i];

            single.style.width = width + 'px';
            if (screenWidth > 959) {
                single.style.height = height + 'px';
            }

            if (i == 0) {
                single.classList.add('active');
            }
        }

        slide.parentElement.style.width = slides.length * width + 'px';
    });

    document.getElementById('next').addEventListener('click', function(e) {
        if (intervalTimeout) {
            clearInterval(intervalTimeout);
        }

        index++;
        if (index >= totalSlides) {
            index = totalSlides;
        }

        move();
    });

    document.getElementById('previous').addEventListener('click', function() {
        if (intervalTimeout) {
            clearInterval(intervalTimeout);
        }

        index--;
        if (index < 0) {
            index = 0;
        }

        move();
    });

    var move = function() {
        var inner = document.querySelector('.banner-slider-inner');
        inner.style.left = '-' + index * widthOfSlide + 'px';

        for (var i = 0; i < slides.length; i++) {
            var single = slides[i];

            if (i == index) {
                single.classList.remove('was-active');
                single.classList.add('active');
            } else {
                if (single.classList.contains('active')) {
                    single.classList.add('was-active');
                    single.classList.remove('active');

                    setTimeout(function(item) {
                        item.classList.add('done-being-active');
                        item.classList.remove('was-active');
                        setTimeout(function(innerItem) {
                            innerItem.classList.remove('done-being-active');
                        }, 400, item)
                    }, 1000, single);
                }
                single.classList.remove('active');
            }
        }
    }

    intervalTimeout = setInterval(function() {

        if (index == totalSlides) {
            direction = 'previous';
        } else if (index == 0) {
            direction = 'next';
        }

        if (direction == 'next') {
            index++;
        } else {
            index--;
        }

        move();
    }, 5000);

    move();
