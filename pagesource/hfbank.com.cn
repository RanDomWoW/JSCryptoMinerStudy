<script language="JavaScript"> 
	var xPos = 0; // 起始横向位置(从左算起，单位象素) 
	var yPos = 0; // 页面本身高度 
	var step = 1; 
	var delay = 40; // 速度，值越大速度越慢 
	var height = 0; 
	var Hoffset = 0; 
	var Woffset = 0; 
	var yon = 0; 
	var xon = 0; 
	var pause = true; 
	var interval; 
	moveWindow.style.top = yPos; 
	function hbcms_changePos() { 
		width =  document.documentElement.clientWidth||document.body.clientWidth; // 判断浏览器窗口的宽度 
		height = document.documentElement.clientHeight||document.body.clientHeight;
		Hoffset = moveWindow.offsetHeight; 
		Woffset = moveWindow.offsetWidth; 
		var scroll_top;  
		if (document.documentElement && document.documentElement.scrollTop){
			scroll_top = document.documentElement.scrollTop;   
		}else if (document.body){
			scroll_top = document.body.scrollTop;
		}
			  
		moveWindow.style.left = (xPos + document.body.scrollLeft) + "px"; 
		moveWindow.style.top = (yPos + scroll_top) + "px"; 
		if (yon) { 
			yPos = yPos + step; 
		}else { 
			yPos = yPos - step; 
		} 
		if (yPos < 0) { 
			yon = 1; 
			yPos = 0; 
		} 
		if (yPos >= (height - Hoffset)) { 
			yon = 0; 
			yPos = (height - Hoffset); 
		} 
		if (xon) { 
			xPos = xPos + step; 
		}else { 
			xPos = xPos - step; 
		} 
		if (xPos < 0) { 
			xon = 1; 
			xPos = 0; 
		} 
		if (xPos >= (width - Woffset)) { 
			xon = 0; 
			xPos = (width - Woffset); 
		} 
	} 
	function hbcms_start_pp() { 
		moveWindow.visibility = "visible"; 
		interval = setInterval('hbcms_changePos()', delay); 
	} 
	function pause_resume() { 
		if(pause) {
			clearInterval(interval);
			pause = false;
		}else {
			interval = setInterval('hbcms_changePos()',delay);
			pause = true;
		}
	} 
	hbcms_start_pp();
	$('#moveWindow').on('mouseOver',function(){
		console.log(1)
		pause_resume()
	})
	$('#moveWindow').on('mouseOut',function(){
		hbcms_start_pp()
	})
	$('#moveWindow').on('click','a',function(){
		hbcms_start_pp()
	})
</script>
