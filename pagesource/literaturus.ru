color: #d2083b;
}
.tabs-inner .widget li a {
display: inline-block;
margin: 0;
margin-right: 1px;
padding: .65em 1.5em;
font: normal normal 12px Arial, Tahoma, Helvetica, FreeSans, sans-serif;
color: #ffffff;
background-color: #d2083b;
border-style: solid;
border-color: #eeeeee;
