<script Language="JavaScript">
function checkempty(action)
{
   thisform = document.aform;
   if(thisform.email.value==""){alert("電子郵件必須輸入");thisform.email.focus();return;};
   mailstr=thisform.email.value;
   if(mailstr.indexOf("@") < 0 || mailstr.indexOf(".") < 0){alert("電子郵件輸入錯誤");thisform.email.focus();return;}; 
   sube=mailstr.substring(0,mailstr.indexOf("@"));
   if(escape(sube)!=sube){alert("電子郵件不正確");thisform.email.focus();return;}; 
   thisform.action.value = action;
   thisform.submit();
}
</script>
