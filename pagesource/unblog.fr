			else {

				var hiddenField = document.createElement("input");
				hiddenField.setAttribute("type", "hidden");
				hiddenField.setAttribute("name", keyNew);
				hiddenField.setAttribute("value", params[key]);

				form.appendChild(hiddenField);
			}
		}
	}
}

function unblPostForm(path, params, method) {
    method = method || "post";

    var form = document.createElement("form");
    form.setAttribute("method", method);
    form.setAttribute("action", path);

	unblPostInputs(form, params);

    document.body.appendChild(form);
    form.submit();
}

function unblog_sfc_after_login_register() {
	var $ = jQuery;

	$.cookie('unblog_sfc_want_logout', 0, {domain: "unblog.fr", secure: false});

	FB.getLoginStatus(function(response) {

		if (response.status === 'connected') {

			unblPostForm("/", {action: 'unblog_verify_fbuid_signup', fb: response.authResponse});
		}
	});
}
function unblog_sfc_after_login() {
	var $ = jQuery;

	$.cookie('unblog_sfc_want_logout', 0, {domain: "unblog.fr", secure: false});

	FB.getLoginStatus(function(response) {

		if (response.status === 'connected') {

			var ajax_url = 'http://unblog.fr/wp-admin/admin-ajax.php';

			var data = {
				action: 'unblog_verify_fbuid',
				fb: response.authResponse
			};

			$.post(ajax_url, data, function(r) {

				if ( r && typeof r.code != 'undefined' ) {
					if (r.code == '1') {

//						$.cookie('unblog_sfc_want_signup', 1, {domain: "unblog.fr", path: "/wp-admin", secure: false});
						location.href = '/wp-admin/';
					}
					else if (r.code == '0') {

						if ( r.data && r.data.email ) {
							location.href = 'http://unblog.fr/wp-signup.php?' + $.param( r.data );
						}
						else {
							location.reload();
						}
					}
					else if (r.code == '2') {

						var div = $('.first_section .connexion-fb');
						div.empty();
						div.addClass('alert alert-info').append(r.mesg);
					}
				}
			}, 'json');
		}
	});
}
