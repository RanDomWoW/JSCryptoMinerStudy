				// 下一批
				var moveNext = function(){
					if (index < ttl - 1) {
						index++;
						_this.animate({
							left: -index*iw
						},{
							duration: d,
							complete: function(){
								triggerPrev.removeClass("bg-l-disable");
								if (index == ttl - 1) {
									triggerNext.addClass("bg-r-disable");
								}
							}
						});
						
						//window.console.log('move right, now index is ' + index);
					}else{
						clearInterval(itvl);
					}
				};
				
				// 绑定触发器
				triggerPrev.bind('mouseover', function(){
					movePrev();
					itvl = setInterval(movePrev, wait);
				});
				triggerPrev.bind('mouseout', function(){
					clearInterval(itvl);
				});
				triggerNext.bind('mouseover', function(){
					moveNext();
					itvl = setInterval(moveNext, wait);
				});
				triggerNext.bind('mouseout', function(){
					clearInterval(itvl);
				});
			});
			
	  </script>
