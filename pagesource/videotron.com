<script>
$(function() {
    function showTheRightMessageInFiltersNoResult(base,plus,premium){
    	// console.info("showTheRightMessageInFiltersNoResult(base,plus,premium)");
		if(!base && plus && premium){
			$("#entete-no-famillies-one-base").show();
			$("#entete-no-famillies-reset-one-base").show();
		} else if(base && !plus && premium){
			$("#entete-no-famillies-one-plus").show();
			$("#entete-no-famillies-reset-one-plus").show();
		} else if(base && plus && !premium){
			$("#entete-no-famillies-one-premium").show();
			$("#entete-no-famillies-reset-one-premium").show();
		} else if(!base && !plus && premium) {
			$("#entete-no-famillies-base-plus").show();
			$("#entete-no-famillies-reset-base-plus").show();
		} else if(!base && plus && !premium){
			$("#entete-no-famillies-base-premium").show();
			$("#entete-no-famillies-reset-base-premium").show();
	    } else if(base && !plus && !premium){
			$("#entete-no-famillies-plus-premium").show();
			$("#entete-no-famillies-reset-plus-premium").show();
	    } else if(!base && !plus && !premium){
			$("#entete-no-famillies-base-plus-premium").show();
			$("#entete-no-famillies-reset-base-plus-premium").show();
	    }
	}

	function showFamillyHeadbandIfHasDevices() {
		// console.info("showFamillyHeadbandIfHasDevices()");
		var showbase = false;
		var showplus = false;
		var showpremium = false;
		setTimeout( function() {}, 1);
		$(".famillyPackageFiltrePointer").each(function() {
			if (($(this).attr("id") == 'filterable-devices-base')
					&& $(this).is(':visible')) {
				showbase = true;
			}
		});
		if (showbase)
			$(".entete-base").show();
		else {
			$(".entete-base").hide();
		}

		$(".famillyPackageFiltrePointer").each(function() {
			if (($(this).attr("id") == 'filterable-devices-plus')
					&& $(this).is(':visible')) {
				showplus = true;
			}
		});
		if (showplus)
			$(".entete-plus").show();
		else {
			$(".entete-plus").hide();
		}

		$(".famillyPackageFiltrePointer").each(function() {
			if (($(this).attr("id") == 'filterable-devices-premium')
					&& $(this).is(':visible')) {
				showpremium = true;
			}
		});

		if (showpremium)
			$(".entete-premium").show();
		else {
			$(".entete-premium").hide();
		}

		if (!showbase || !showplus || !showpremium) {
			$(".entete-no-famillies").each(function() {
				$(this).show();
				$(".entete-no-famillies > .titre-entete").each(
						function() {
							$(this).hide();
						});
				$(".entete-no-famillies > .sous-titre-entete-filter-msg").each(
						function() {
							$(this).hide();
						});
				showTheRightMessageInFiltersNoResult(showbase,showplus, showpremium);
			});
		}
	}

	function showFamillyHeadbandIfHasDevicesWhenClickOnCheckboxfilter() {
		// console.info("showFamillyHeadbandIfHasDevicesWhenClickOnCheckboxfilter()");
		var showbase = false;
		var showplus = false;
		var showpremium = false;
		$(".famillyPackageFiltrePointer > div").each(function() {
			if (($(this).attr("id") == 'filterable-fn-adjust-4-devices-base')
					&& $(this).is(':visible')) {
				showbase = true;
			}
		});
		if (showbase)
			$(".entete-base").show();
		else {
			$(".entete-base").hide();
		}

		$(".famillyPackageFiltrePointer > div").each(function() {
			//alert("filterable-fn-adjust-4-devices-plus");
			if (($(this).attr("id") == 'filterable-fn-adjust-4-devices-plus')
					&& $(this).is(':visible')) {
				showplus = true;
			}
		});
		if (showplus)
			$(".entete-plus").show();
		else {
			$(".entete-plus").hide();
		}

		$(".famillyPackageFiltrePointer > div").each(function() {
			//alert($(this).is(':visible'));
			if (($(this).attr("id") == 'filterable-fn-adjust-4-devices-premium')
					&& $(this).is(':visible')) {
				showpremium = true;
			}
		});
		if (showpremium)
			$(".entete-premium").show();
		else {
			$(".entete-premium").hide();
		}

		if (!showbase || !showplus || !showpremium) {
			$(".entete-no-famillies").each(function() {
				$(this).show();
				$(".entete-no-famillies > .titre-entete").each(function() {
					$(this).hide();
				});
				$(".entete-no-famillies > .sous-titre-entete-filter-msg").each(function() {
					$(this).hide();
				});
				showTheRightMessageInFiltersNoResult(showbase,showplus, showpremium);
			});
		}
		if (showbase && showplus && showpremium) {
			$(".entete-no-famillies").hide();
			setTimeout( function() {$(".no-result").hide();}, 1);
		}

	}

	//Initialize filters and Show all  devices
	function initfiltersAndShowall() {
		// console.info("initfiltersAndShowall()");
		$(":checkbox:checked").each( function() {
			this.click();
		});
		$(".famillyPackageFiltrePointer,.entete-base,.entete-plus,.entete-premium").each( function() {
			$(this).show();
		});
		$(".entete-no-famillies, .no-result").each( function() {
			$(this).hide();
		});
	}

	$("#new-button").bind("click", function(event) {
		event.preventDefault();
		initfiltersAndShowall();
		$(".filterable-devices-is-promotion , .filterable-devices-").each( function() {
			$(this).hide();
		});
		showFamillyHeadbandIfHasDevices();
	});

	$("#promotion-button").bind("click", function(event) {
		event.preventDefault();
		initfiltersAndShowall();
		$(".filterable-devices-is-new , .filterable-devices-").each( function() {
			$(this).hide();
		});
		showFamillyHeadbandIfHasDevices();
	});

	$("#all-button, .fn-reset").bind("click", function(event) {
		event.preventDefault();
		initfiltersAndShowall();
		showFamillyHeadbandIfHasDevices();
		//reset all button
		$("#all-button").removeClass("toolbar-button-pressed");
		$("#new-button").removeClass("toolbar-button-pressed");
		$("#promotion-button").removeClass("toolbar-button-pressed");
		$("#all-button").addClass("toolbar-button-pressed");
	});

	$(".characteristics-filter-checkbx-row > input[type=checkbox]").bind("click", function(event) {
		$(".entete-base,.entete-plus,.entete-premium").each( function() {
			if (!($(this).is(':visible'))) {
				$(this).show();
			}
		});
		setTimeout( function() {showFamillyHeadbandIfHasDevicesWhenClickOnCheckboxfilter();}, 1);
	});

	$("#button-add").bind("click", function(event) {
		$('body,html').animate({ scrollTop: $("#returnTab").offset().top }, 'slow');
	});

	$("#button-update").bind("click", function(event) {
		$('body,html').animate({ scrollTop: $("#returnTab").offset().top },  'slow');
	});

	$("#button-add-package").bind("click", function(event) {
		$('body,html').animate({ scrollTop: $("#returnTab").offset().top }, 'slow');
	});

	$("#button-update-package").bind("click", function(event) {
		$('body,html').animate({ scrollTop: $("#returnTab").offset().top }, 'slow');
	});

	$("#left-nav ul li a").bind("click", function(event) {
		$("#left-nav ul li a").each(function() {
			$(this).removeClass("left-nav-activate");
		});
		$(this).addClass("left-nav-activate");
	});

	$(document).ready(function() {
		// console.info("$(document).ready");
		var formCheckboxFilters = $("form[data-enhance='channelsFilter'] input[type='checkbox']");
		$("#characteristics-btn").addClass("last");
		$("#all-button").addClass("toolbar-button-pressed");
		$("#characteristics-btn").bind("click", function(event) {
			event.preventDefault();
			$(this).toggleClass("open");
			$("#characteristics-div").toggle();
		});
		if ($(formCheckboxFilters).is(":checked")) {
			$("#characteristics-btn").toggleClass("open");
			$("#characteristics-div").toggle();
		}
	});

	//Set stickynav loading position and z-index(s) to make sure sticky nav is up front of everything
	$(window).load( function() {
		// console.info("$(window).load");
		if ($('#sticky').length) {
			var percent = percentValue($('.black').position().top);
			$("#stickynav-close").css( { "top" : percent });
			$("#sticky").css( { "top" : percent });
			$("#header").css( { "z-index" : '1' });
			$(".toolbar-section").css( { "z-index" : '2' });
			$(".section-more-service,.footer-section,.foot-note,"+".optional-features .service-total .total, #footer").css({ "z-index" : '0' });
		}

		$("#promotion-button ").bind("click", function(event) {
			event.preventDefault();
			$(this).addClass("toolbar-button-pressed");
			$("#new-button").removeClass("toolbar-button-pressed");
			$("#all-button").removeClass("toolbar-button-pressed");
		});

		$("#new-button").bind("click", function(event) {
			event.preventDefault();
			$(this).addClass("toolbar-button-pressed");
			$("#promotion-button").removeClass("toolbar-button-pressed");
			$("#all-button").removeClass("toolbar-button-pressed");
		});

		$("#all-button").bind("click", function(event) {
			event.preventDefault();
			$(this).addClass("toolbar-button-pressed");
			$("#new-button").removeClass("toolbar-button-pressed");
			$("#promotion-button").removeClass("toolbar-button-pressed");
		});
		setTimeout( function() {showFamillyHeadbandIfHasDevicesWhenClickOnCheckboxfilter();}, 1);
	});

	// For when page loads from a different Tab, make sure on switch to device list the family banners are called
	$(document.documentElement).bind("onAjaxTabVisible",showFamillyHeadbandIfHasDevicesWhenClickOnCheckboxfilter);

	//Return the correct value of the StickyNav position
	//considering the boundaries of the screen and other resitrictions
	function percentValue(HauteurDiv) {
		// console.info("percentValue(HauteurDiv)");
		var bodyHtmlElement = 'body,html';
		//Check browser since scrollTop() isn't working the same in different browser
		if($.browser.safari || $.browser.chrome){
			bodyHtmlElement = 'body';
		}
		var bodyHtmlValue = $(bodyHtmlElement).scrollTop();
		var percent = bodyHtmlValue * 100/ ($(document).height());
		//Making sure the sticky doesn't go further than
		//the black background on the right of the page
		if (bodyHtmlValue < (HauteurDiv-20)) {
			return (HauteurDiv-bodyHtmlValue)+20;
		}
		else if (percent > 78) {
			return '78%';
		}
		else {
			return percent + '%';
		}
	}

	$(window).scroll( function() {
		// console.info("$(window).scroll");
		if ($('#sticky').length) {
			var percent = percentValue($('.black').position().top);
			if ($('#stickynav-open').css('display') == 'none') {
				$("#stickynav-close").css( { "top" : percent });
				$("#sticky").css( { "top" : percent });
			}
			if ($('#stickynav-open').css('display') == 'block') {
				$("#stickynav-close").css( {
					"top" : percent
				});
				$("#sticky").css( {
					"top" : percent
				});
			}

			var plusPosition = $("#ddci13550-fmlpkg-plus").position().top+400;
			var premiumPosition = $("#ddci13550-fmlpkg-premium").position().top+300;
			var econoPosition = $("#ddci13550-fmlpkg-econo").position().top+200;
			var stikyposition = $("#sticky").position().top;
			var isEconoVisible = $('.entete-base').is(':visible');
			var isPlusVisible = $('.entete-plus').is(':visible');
			var isPremiumVisible = $('.entete-premium').is(':visible');
			var bodyHtmlElement=$('body,html');
			if($.browser.safari || $.browser.chrome) {
				bodyHtmlElement = $("body");
			}

			$("#left-nav ul li a").each( function() {
				$(this).removeClass("left-nav-activate");
			});

			$("#left-nav ul li a").each( function() {
				if($(this).attr("href")=='#ddci13550-fmlpkg-premium' &&!isPremiumVisible) {
					$(this).removeClass("left-nav-activate");
				}
				if($(this).attr("href")=='#ddci13550-fmlpkg-plus' &&!isPlusVisible) {
					$(this).removeClass("left-nav-activate");
				}
				if($(this).attr("href")=='#ddci13550-fmlpkg-econo' &&!isEconoVisible) {
					$(this).removeClass("left-nav-activate");
				}
			});

			if($.browser.safari || $.browser.chrome) {
				bodyHtmlElement = $("body");
			}

			if(bodyHtmlElement.scrollTop()>=premiumPosition &&isPremiumVisible){
				$("#left-nav ul li a").each( function() {
					$(this).removeClass("left-nav-activate");
				});
				$("#left-nav ul li a").each( function() {
					if($(this).attr("href")=='#ddci13550-fmlpkg-premium') {
						$(this).addClass("left-nav-activate");
					}
				});
			}

			if(bodyHtmlElement.scrollTop()>plusPosition && isPlusVisible){
				$("#left-nav ul li a").each( function() {
					$(this).removeClass("left-nav-activate");
				});
				$("#left-nav ul li a").each( function() {
					if($(this).attr("href")=='#ddci13550-fmlpkg-plus')
					$(this).addClass("left-nav-activate");
				});
			}

			if(bodyHtmlElement.scrollTop()>econoPosition && isEconoVisible){
				$("#left-nav ul li a").each( function() {
					$(this).removeClass("left-nav-activate");
				});
				$("#left-nav ul li a").each( function() {
					if($(this).attr("href")=='#ddci13550-fmlpkg-econo')
					$(this).addClass("left-nav-activate");
				});
			}
		}
	});

	$("#left-header-hide").bind("click", function(event) {
		event.preventDefault();
		$("#stickynav-open").hide();
		$("#stickynav-close").show();
		$("#stickynav-close").css( { "top" : percentValue() });
	});

	$("#stickynav-close").bind("click", function(event) {
		event.preventDefault();
		$("#stickynav-open").show();
		$("#stickynav-close").hide();
		$("#stickynav-open").css( { "top" : percentValue() });
	});

	$('a[href*=#]:not([href=#])').click( function() {
		if ($('#sticky').length) {
			if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '') && location.hostname == this.hostname) {
				var target = $(this.hash);
				target = target.length ? target : $('[name=' + this.hash.slice(1) + ']');
				if (target.length) {
					$('html,body').animate( { scrollTop : target.offset().top }, 1000);
					return false;
				}
			}
		}
	});
});
</script>
</body>
</html>