        if( typeof window.gastrocalendar_id_chain == 'undefined' )
          window.gastrocalendar_id_chain = [];

        var currEventId = $("#gastrocalendar").data('event-id');

        if( window.gastrocalendar_id_chain.length < 1 ) //first
          window.gastrocalendar_id_chain.push(currEventId);

        $('.gastroCalendarPrevNext').click(function(e){
          e.preventDefault();
          var $currLink = $(this);
          var $currGastroCalendar = $("#gastrocalendar");
          
          var isPrev = $currLink.hasClass('gastroCalendarPrev');
          
          var arrayCopyToBeRotated = window.gastrocalendar_id_chain; //reverse() is by reference => COPY!
          if( ! isPrev ) arrayCopyToBeRotated.reverse();

          var indexOfCurrentItem = arrayCopyToBeRotated.indexOf( currEventId );
          var not_ids = arrayCopyToBeRotated.slice( indexOfCurrentItem );
          
          console.log( '----------------------------------');
          console.log( window.gastrocalendar_id_chain );
          console.log( not_ids.join(',') );
                    
          $currGastroCalendar.append(loaderHTML);
          $.get( $currLink.data('href') + '&not_ids=' + not_ids.join(','), function( data, textStatus, jqXHR ){
            if( (typeof data != "undefined") && (typeof data.html == 'string') )
                $('#gastrocalendar').replaceWith(data.html);
            //loader removed with replace...
            
            var recievedEventId = $("#gastrocalendar").data('event-id');
            console.log('recieved: ' + recievedEventId);
            if( window.gastrocalendar_id_chain.indexOf(recievedEventId) == -1 ){ //if it's not there...
                if( isPrev )
                    window.gastrocalendar_id_chain.unshift(recievedEventId);
                else
                    window.gastrocalendar_id_chain.push(recievedEventId);
            }
          }, 'json' ).fail(function(){
            $currGastroCalendar.find('.loader').remove();
          });
        });  
