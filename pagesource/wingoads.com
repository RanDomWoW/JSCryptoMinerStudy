                        $('#signUp').on('hide.bs.modal', function () {
                            cb();
                        });
                    },
                    isShown: function () {
                        return ($("#signUp").data('bs.modal') || {}).isShown;
                    }
                }
            });

            $resetPasswordForm.wingoadsValidator({
                rules: {
                    'username': usernameValidatorConfig
                }
            });

                        
                        $signInForm.wingoadsValidator('getForm', function(form) {
                form.registerSubmitCallback(function(event) {
                    event.preventDefault();
                    form.getInputByName('_username').trimValue();
                }, 99999999);

                form.registerSubmitCallback(function(event) {
                    event.preventDefault();

                    $.ajax({
                        type: $signInForm.prop('method'),
                        url: $signInForm.prop('action'),
                        data: $signInForm.serializeArray(),
                        success: function (redirectPath) {
                            form.lockForm('Waiting..');

                            $(location).prop('href', redirectPath);
                        },
                        error: function (data) {
                            form.getInputByName('_username').addError(data.responseJSON['error']);
                            form.getInputByName('_password').addError(data.responseJSON['error']);
                        }
                    });
                }, -1);
            });

            $signUpForm.wingoadsValidator('getForm', function(form) {
                form.registerSubmitCallback(function(event) {
                    event.preventDefault();
                    form.getInputByName('sonata_user_registration_form[email]').trimValue();
                }, 99999999);

                form.registerSubmitCallback(function(event) {
                    event.preventDefault();

                    $.ajax({
                        type: $signUpForm.prop('method'),
                        url: $signUpForm.prop('action'),
                        data: $signUpForm.serializeArray(),
                        success: function (data) {
                            form.lockForm('Waiting..');

                            if (data.redirect_path !== undefined) {
                                $(location).prop('href', data.redirect_path);
                            } else {
                                $('#signUp').find('.modal-body').html(
                                    '<div class="text-center">To complete registration click on the link we have sent you by e-mail</div>'
                                );
                            }
                        },
                        error: function (data) {
                            $.each(data.responseJSON, function(field, message) {
                                form.getInputByName('sonata_user_registration_form[' + field + ']').addError(message);
                            });
                        }
                    });
                }, -1);
            });

            $resetPasswordForm.wingoadsValidator('getForm', function (form) {
                form.registerSubmitCallback(function(event) {
                    event.preventDefault();
                    form.getInputByName('username').trimValue();
                }, 99999999);

                form.registerSubmitCallback(function (event) {
                    event.preventDefault();

                    $.ajax({
                        type: $resetPasswordForm.prop('method'),
                        url: $resetPasswordForm.prop('action'),
                        data: $resetPasswordForm.serializeArray(),
                        success: function (data) {
                            form.lockForm('Waiting..');

                            $('#resetPassword').find('.modal-body').addClass('text-center').html(data);
                        },
                        error: function (data) {
                            form.getInputByName('username').addError(data.responseJSON);
                        }
                    });
                }, -1);
            });

                    }());
    </script><script type="text/javascript">
        $('#subscribe').click(function () {
            var email = $('#email').val();
            var url = "/subscribe/email".replace('email', email);
            var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            var message = $(".b-subscribe__message");

            if (!re.test(email)) {
                message.show();
                return;
            }

            if (message.is(":visible")) {
                message.hide();
            }

            $.ajax({
                type: 'post',
                url: url,
                data: '',
                success: function (data) {
                    var $subscribeModal = $('#subscribeModal');

                    $subscribeModal.find(".wingoads-modal__body").html('<p class="text-center">Thank you for subscribing!</p>');
                    $subscribeModal.modal();
                },
                error: function () {
                    var $subscribeModal = $('#subscribeModal');

                    $subscribeModal.find(".wingoads-modal__body").html('<p class="text-center">Subscribing: Something went wrong. Please try again</p>');
                    $subscribeModal.modal();
                }
            });
        });
