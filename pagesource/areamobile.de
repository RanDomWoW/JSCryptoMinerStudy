$(document).ready(function(){
    
    if('mobile' === $.cookie('amb_display') || null === $.cookie('amb_display')){

        // create a simple instance
        // by default, it only adds horizontal recognizers
        var mc = new Hammer($('.stageToplist')[0]);

        var left;
        mc.on("panstart panend", function(ev) {
            left =  parseInt($('.items').css('left'));
            if(isNaN(left)){
                $('.items').css('left', '0px');
                left = 0;
            }
        });

        // listen to events...
        mc.on("panleft panright", function(ev) {
            var newLeft     = left + ev.deltaX;
            var deviceList  = $('.items').width();
            var widthScreen = $(window).width();
            var maxLeft = 0;
            var minLeft = -1 * deviceList + widthScreen - 56;

            if(maxLeft < newLeft){
                newLeft = maxLeft;
            } else if(newLeft < minLeft){
                newLeft = minLeft;
            }
           $('.items').css('left', newLeft+'px');
        });
    
    }

});

