        $(document).ready(function () {

            $('body').css('padding-top', $('#fixedNavbar').height() + 'px')

            $('.navbar-fixed-top .dropdown').on('show.bs.dropdown', function (e) {
                $(this).children('.dropdown-menu').addClass('animated flipInX');
            });

            $('.navbar-fixed-top .dropdown').on('hide.bs.dropdown', function (e) {
                $(this).children('.dropdown-menu').removeClass('flipInX');
            });

            $('.navbar-static-top .dropdown').on('show.bs.dropdown', function (e) {
                $(this).children('.dropdown-menu').addClass('animated fadeIn');
            });

            $('.navbar-static-top .dropdown').on('hide.bs.dropdown', function (e) {
                $(this).children('.dropdown-menu').removeClass('fadeIn');
            });

            $('[data-toggle="tooltip"]').tooltip();

            $('.menu a.dropdown-toggle').click(function () {

                var $menu = $(this).closest('.dropdown').find('.dropdown-menu');
                var left = $(this).position().left;
                if (left - $menu.outerWidth() < 0) {
                    $menu.css('left', 0);
                }
            });

            $(window).scroll(function () {
                if ($(this).scrollTop() > 220) {
                    $('.back-to-top').fadeIn(500);
                } else {
                    $('.back-to-top').fadeOut(500);
                }
            });

            $('.back-to-top').click(function (event) {
                event.preventDefault();
                $('html, body').animate({ scrollTop: 0 }, 500);
                return false;
            });

        });
    </script>



    <script type="text/javascript">

        var searchTimer = 0;

        function searchLookup() {
            var query = $.trim($("#SeachInput").val());

            if (query.length >= 3) {

                $('#ApplySearchBtn').button('loading');

                $.ajax({
                    method: 'POST',
                    dataType : 'json',
                    data: { query: query },
                    url: 'http://search.gozine2.ir/api/search',
                    xhrFields: { 
                        withCredentials: true 
                    },
                    success: function(result) {
                        
                        $('#ApplySearchBtn').button('reset');
                        $('#SearhcResultLookup').empty();

                        var resultLength = (result.links.length > 0 ? 1 : 0) + (result.movies.length > 0 ? 1 : 0);

                        if (resultLength === 0) {
                            $('#SearhcResultLookup').append('<h5 >هیچ موردی یافت نشد</h5>');
                        } else {

                            if (result.links.length > 0) {
                                var linksHtml = '<div id="linksSection">';
                                for (var i = 0; i < result.links.length; i++) {
                                    linksHtml += '<h3 class="serach-result-category">' + result.links[i].category + '</h3>';
                                    linksHtml += '<ul>';
                                    for (var j = 0; j < result.links[i].items.length; j++) {
                                        var item = result.links[i].items[j];
                                        linksHtml += '<li><a href="' + item.url + '" target="_blank">' + item.title + '</a></li>';
                                    }
                                    linksHtml += '</ul>';
                                }

                                if (result.tags.length > 0) {
                                    linksHtml += '<p id="tagSection">';
                                    for (var i = 0; i < result.tags.length; i++) {
                                        var item = result.tags[i];
                                        linksHtml += ' <span class="label label-primary"><a href="' + item.url + '" target="_blank">' + item.title + '</a></span> ';
                                    }
                                    linksHtml += '</p>';
                                }

                                linksHtml += '</div>';

                                $('#SearhcResultLookup').append(linksHtml);
                            }

                            if (result.movies.length > 0) {
                                var moviesHtml = '<div id="moviesSection"><h3 class="serach-result-category">فیلم ها</h3><ul>';
                                for (var i = 0; i < result.movies.length; i++) {
                                    var item = result.movies[i];
                                    moviesHtml += '<li><a href="' + item.url + '" target="_blank"><img src="' + item.thumbnailUrl + '"/><p>' + item.title + '</p></a></li>';
                                }
                                moviesHtml += '</ul></div>';

                                $('#SearhcResultLookup').append(moviesHtml);
                            }

                            $('#SearhcResultLookup')[(resultLength === 1 ? 'addClass' : 'removeClass')]('global-search-result-full-width');

                            $('#SearhcResultLookup').append('<button type="button" id="MoreSearchResultBtn" class="btn btn-block btn-g2 btn-sm">مشاهده نتایج بیشتر</button>');
                        }


                        $('#SearhcResultLookup').fadeIn();

                    }
                });

            }
            else {
                $("#SearhcResultLookup").fadeOut();
            }
        };

        $(document).ready(function () {
            $('#SeachInput').on('keyup', function () {
                clearTimeout(searchTimer);
                searchTimer = setTimeout(searchLookup, 1500);
            });


            $(document).on('mouseup', function (t) {
                $(t.target).closest('#SearhcResultLookup').is('#SearhcResultLookup') || $('#SearhcResultLookup').fadeOut();
            });

            $('#ApplySearchBtn').click(function () {
                searchLookup();
            });

            $('#SearhcResultLookup').on('click.MoreSearchResultBtn', '#MoreSearchResultBtn', function () {
                window.open("http://article.gozine2.ir/search?query=" + encodeURIComponent($.trim($("#SeachInput").val()), "_blank"));
            });

        });
    </script>

