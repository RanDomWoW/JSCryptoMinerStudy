	<section class="common gate">
	<section class="sign-in">
		<form id="login" class="autofill" method="post">
			<h2 class="bn">Nāc iekšā!</h2>
			<div class="field">
				<input id="login_login" class="field stretch" name="login" type="text" autocomplete="off">
				<label for="login_login">Lietotājvārds (e-pasts)</label>
			</div>
			<div class="field">
				<input id="login_password" class="field stretch" name="password" type="password" autocomplete="off">
				<label for="login_password">Parole</label>
			</div>
			<div class="form-actions">
				<button class="simple" type="submit">Ienākt</button>
				<span class="links">
					<a class="js_form_toggle" href="#recover">Es neatceros savu paroli</a>
				</span>
			</div>
		</form>

		<form id="recover" method="post" class="hidden">
			<h2 class="bn">Aizmirsi paroli?</h2>
			<div class="field field-label">
				<input id="login_email" class="field stretch" name="email" type="text" autocomplete="off">
				<label for="login_email">Lietotājvārds (e-pasts)</label>
			</div>
			<div class="form-actions">
				<button class="simple" type="submit">Atjaunot paroli</button>
				<span class="links">
					<a id="login_link" class="js_form_toggle" href="#login">Tomēr atceros!</a>
				</span>
			</div>
		</form>
	</section>
	<script>
		rnp.xhr.form('#login', {
			url: '/api/login',
			onSuccess: function(form, response){
        document.location.href = response.redirect ? response.redirect : '/payments';
			}
		});
		rnp.xhr.form('#recover', {
			url: '/api/recover',
			onSuccess: function(form, response){
				$('#login_link').trigger('click');
				$('#login_login').val($('#login_email').val()).trigger('blur');

				$.modal({
					tools: false,
					content: 	'<div class="amnesia-success">' +
											'<div class="modal-header">' +
												'<h2 class="bn">Paroles atjaunošana</h2>' +
											'</div>' +
											'<p>Uz norādīto e-pasta adresi tika nosūtīta saite paroles maiņai. Lūdzu pārbaudiet e-pastu!</p>' +
											'<div class="actions">' +
												'<a class="js_modal_close button" href="#close">Paldies, sapratu</a>' +
											'</div>' +
										'</div>'
				});
			}
		});
	</script>

	<section class="registration">
		<h2 class="bn">Ērti un pārskatāmi</h2>
		<p>Iesniegt skaitītāju rādījumus</p>
		<p>Saņemt rēķinus elektroniskā formā</p>
		<a class="button button-big" href="/sign-up">Reģistrēties E-Pārvaldniekā</a>
	</section>
</section>

