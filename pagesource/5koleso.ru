		endpos = 3;
	}
	if (!document.getElementById('thumb_' + (photo + 1))) {
		startpos = 8;
		endpos = 2;
	}
	if (!document.getElementById('thumb_' + (photo - 4))) {
		startpos = 4;
		endpos = 6;
	}
	if (!document.getElementById('thumb_' + (photo - 3))) {
