		document.getElementById('buttonScrollRight').style.display = 'none';
	}
	
	// Покажем (промотаем) нужные (текущие) тумбнейлы под фоткой
	showNeededThumbs(photo);		
}

function showNeededThumbs(photo) {
	var step = 9;
	for (i = 1; i < maxPhotos; i++) {
		if (document.getElementById('thumb_' + i)) {
			document.getElementById('thumb_' + i).style.visibility = 'hidden';
