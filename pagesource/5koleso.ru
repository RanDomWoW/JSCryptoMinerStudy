	}
	if (!document.getElementById('thumb_' + (photo - 2))) {
		startpos = 2;
		endpos = 8;
	}
	if (!document.getElementById('thumb_' + (photo - 1))) {
		startpos = 1;
		endpos = 9;
	}

	for (i = photo - startpos; i < photo + endpos; i++) {
		if (document.getElementById('thumb_' + i)) {
			document.getElementById('thumb_' + i).style.visibility = 'visible';
			document.getElementById('thumb_' + i).style.display = 'block';
