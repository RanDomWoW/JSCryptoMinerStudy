function getCurHashByAction() {
	var curHash = window.location.hash.substring(1);
	var curHashPrefix = curHash.substring(0, 5);
	var curHashPhoto = parseInt(curHash.replace(/photo/g, ''));
	if (curHashPrefix == 'photo' && curHashPhoto > 0)
		showGalleryOverlay(curHashPhoto);
}

function hideSideBar() {
	// Скроем правую колонку с ненужными баннерами
	if (document.getElementsByClassName('region-sidebar-first')[0]) {
		document.getElementsByClassName('region-sidebar-first')[0].style.visibility = 'hidden';
		document.getElementsByClassName('region-sidebar-first')[0].style.display = 'none';
	}
}

function showGalleryOverlay(photo) {
	
	hideSideBar();
	
	// Скроем и покажем нужную (текущую) фотку
	for (i = 1; i < maxPhotos; i++) {
		if (document.getElementById('photo_' + i)) {
			document.getElementById('photo_' + i).style.visibility = 'hidden';
			document.getElementById('photo_' + i).style.display = 'none';
		}
		if (document.getElementById('photoText_' + i)) {
			document.getElementById('photoText_' + i).style.visibility = 'hidden';
			document.getElementById('photoText_' + i).style.display = 'none';
		}
	}
	if (document.getElementById('photo_' + photo)) {
		document.getElementById('photo_' + photo).style.visibility = 'visible';
		document.getElementById('photo_' + photo).style.display = 'block';
	}
	if (document.getElementById('photoText_' + photo)) {
		document.getElementById('photoText_' + photo).style.visibility = 'visible';
		document.getElementById('photoText_' + photo).style.display = 'block';
	}

	// Покажем кнопку "назад" на фотке (если надо)
	if (photo > 0 && document.getElementById('imgButtonScrollLeft_' + photo)) {
		document.getElementById('imgButtonScrollLeft_' + photo).style.visibility = 'visible';
		document.getElementById('imgButtonScrollLeft_' + photo).style.display = 'block';
	}
	if (photo == 1
		&& document.getElementById('imgButtonScrollLeft_' + photo)
		&& document.getElementById('buttonScrollLeft')) {
		document.getElementById('imgButtonScrollLeft_' + photo).style.visibility = 'hidden';
		document.getElementById('imgButtonScrollLeft_' + photo).style.display = 'none';
		document.getElementById('buttonScrollLeft').style.visibility = 'hidden';
		document.getElementById('buttonScrollLeft').style.display = 'none';
	}
	// Уберем кнопку "вперед" на фотке (если ситуация такова)
	if (!document.getElementById('photo_' + (photo + 1))
		&& document.getElementById('imgButtonScrollRight_' + photo)
		&& document.getElementById('buttonScrollRight')) {
		document.getElementById('imgButtonScrollRight_' + photo).style.visibility = 'hidden';
		document.getElementById('imgButtonScrollRight_' + photo).style.display = 'none';
