            $('.totop_btn').click(function () {
                $('html, body').animate({ scrollTop: 0 }, 1000);
                return false;
            });

        </script>
        <script type="text/javascript">
        $(window).load(function() {
            $('#slider').nivoSlider();
        });
        </script> 

        <!-- End To Top Btn Code -->
    </div>
</body>
