					<div class="b-popup-scroller js-cart_scroller">
						<table class="b-table b-table--fast-cart b-table--fast-cart-products cm-cart-content cm-cart-content-thumb cm-cart-content-delete js-fast-cart-content"
								data-cart-total="{{=it.cart.subtotal }}"
								id="cart_items">
							<colgroup>
								<col width="60px"/>
								<col width="190px"/>
								<col width="90px"/>
							</colgroup>

							<tbody>
								{{~it.cart.products :p}}
									<tr class="b-table-row js-cart-item" data-product-id="{{=p.product_id}}" data-product-name="{{=p.product_name}}" data-product-price="{{=p.display_price}}" data-product-amount="{{=p.amount}}">
										<td class="b-table-cell">
											{{? p.status !== 'H'}}
												<a href="{{=fn_url('products.view?product_id=' + p.product_id)}}">
											{{?}}
												<div class="b-card-image_wrapper product-image">
													{{? p.iconPath}}
														<img src="{{=p.iconPath}}" width="40" height="40" alt="" title="" border="0" />
													{{?}}
												</div>
											{{? p.status !== 'H'}}
												</a>
											{{?}}
										</td>
										<td class="b-table-cell">
											{{? p.status !== 'H'}}
												<a href="{{=fn_url('products.view?product_id=' + p.product_id)}}">
											{{?}}
													<div class="b-card-details">
														<div class="b-card-info">
															<h2 class="b-card-brand">{{=p.product_brand}}</h2>
															<h1 class="b-card-title">{{=p.product_name}}</h1>
															<div class="b-card-description">{{=p.product_category}}</div>
														</div>
													</div>
											{{? p.status !== 'H'}}
												</a>
											{{?}}
										</td>
										<td class="b-table-cell b-table-cell--price">
											{{? p.display_price != 0}}
												<div class="minicart-tools cm-cart-item-delete b-table-content b-table-content--remove">
													<a name="delete_cart_item" href="{{=fn_url('checkout.delete.from_status?cart_id=' + p.cart_id)}}" class="cm-ajax cm-ajax-full-render js-remove_from_fast_cart_button" rev="" data-cart-id="{{=p.cart_id}}" data-product_id="{{=p.product_id}}" data-category_id="{{=p.main_category}}" data-user_id="{{=it.cart.auth.user_id}}">
														<i class="b-icons b-icons--remove"></i>
														<span class="b-cart-item-action-text">Убрать</span>
													</a>
												</div>
											{{?}}
												{{? p.display_price == 0}}
													<div class="minicart-tools b-table-hidden--price cm-cart-item-delete b-table-content b-table-content--remove">
														<div class="b-icons--gift_product_popup">
															<i class="b-icons--gift-big"></i>
														</div>
														<div class="b-table-content-gift--text">
															Подарок
														</div>
													</div>
												{{?}}
											<div class="b-table-content b-table-content--price">
												{{? p.display_price == 0}}
													<div class="b-icons--gift_product_popup">
														<i class="b-icons--gift-big"></i>
													</div>
													<div class="b-table-content-gift--text">
														Подарок
													</div>
												{{?? p.old_price}}
													<span>{{=p.amount}}</span><span>&nbsp;x&nbsp;</span>
													<i class="b-fastcart-old_price">{{=App.formatPrice(p.old_price, 0)}}</i>
													<br />
													<i class="b-fastcart-new_price">{{=App.formatPrice(p.display_price, 0)}}</i>
												{{??}}
													<span>{{=p.amount}}</span><span>&nbsp;x&nbsp;</span>
													{{=App.formatPrice(p.display_price, 0)}}
												{{?}}
											</div>
										</td>
									</tr>
								{{~}}

								{{~it.cart.gift_certificates :p}}
									<tr class="b-table-row">
										<td class="b-table-cell">
											<div class="b-card-image_wrapper product-image">
												<img src="/static/img/icons/gift_cert.png" width="40" height="40" alt="" title="" border="0" />
											</div>
										</td>
										<td class="b-table-cell">
											<div class="b-card-details">
												<div class="b-card-info">
													<h2 class="b-card-brand">PUDRA</h2>
													<h1 class="b-card-title">Подарочный сертификат</h1>
													<div class="b-card-description">На покупку товаров</div>
												</div>
											</div>
										</td>
										<td class="b-table-cell b-table-cell--price">
											{{? p.display_price != 0}}
											<div class="minicart-tools cm-cart-item-delete b-table-content b-table-content--remove">
												<a name="delete_cart_item" href="{{='/index.php?dispatch=gift_certificates.delete&gift_cert_id=' + p.gift_cert_id + '&redirect_mode=cart'}}" class="cm-ajax cm-ajax-full-render js-remove_from_fast_cart_button" data-cart-id="{{=p.cart_id}}" data-user_id="{{=it.cart.auth.user_id}}">
													<i class="b-icons b-icons--remove"></i>
													<span class="b-cart-item-action-text">Убрать</span>
												</a>
											</div>
											{{?}}
											{{? p.display_price == 0}}
												<div class="minicart-tools b-table-hidden--price cm-cart-item-delete b-table-content b-table-content--remove">
													Бесплатно
												</div>
											{{?}}
											<div class="b-table-content b-table-content--price">
												<span>1</span><span>&nbsp;x&nbsp;</span>
												{{=App.formatPrice(p.amount, 0)}}
											</div>
										</td>
									</tr>
								{{~}}
						</tbody>
						</table>
					</div>

					{{ var subtotalHasDiscount = typeof it.cart.original_subtotal !== 'undefined' && it.cart.original_subtotal > it.cart.subtotal; }}
					{{ var enoughToMakeOrder = !it.cart.orderAmount.fail; }}
					{{ var enoughForFreeShipping = it.cart.until_free_shipping == 0; }}
					{{ var enoughToShowCostUntilFreeShipping = it.cart.subtotal >= it.cart.min_cart_cost_for_free_shipping / 100 * 60; }}

					<table class="b-table b-table--fast-cart">
						<colgroup>
							<col width="60px"/>
							<col width="190px"/>
							<col width="90px"/>
						</colgroup>

						<tbody>
							<tr class="b-table-row b-table-row--last">
								<td class="b-table-cell"></td>
								<td class="b-table-cell b-table-cell--sum" style="vertical-align: top;">
									Сумма покупки
								</td>
								<td class="b-table-cell b-table-cell--sum_price" style="line-height: 1.3;">
									{{? !subtotalHasDiscount }}
										{{= App.formatPrice(it.cart.subtotal, 0) }}
									{{??}}
										<div class="b-fastcart-old_price b-fastcart-old_price--subtotal">
											{{= App.formatPrice(it.cart.original_subtotal, 0) }}
										</div>
										<br />
										<div class="b-fastcart-new_price b-fastcart-new_price--subtotal">
											{{= App.formatPrice(it.cart.subtotal, 0) }}
										</div>
									{{?}}
								</td>
							</tr>

							{{? !enoughToMakeOrder }}
								<tr>
									<td colspan="3" class="b-table--fast-cart-notification">Для оформления заказа не хватает {{= App.formatPrice(it.cart.orderAmount.notEnough, 0) }}</td>
								</tr>
							{{?}}
						</tbody>
					</table>

					<div class="b-content-col b-content-col--last">
						{{? !enoughToMakeOrder }}
							<a class="b-button b-button--medium_content b-button--full_width" href="/">
								<span class="b-button-content">Вернуться к покупкам</span>
							</a>
						{{??}}
							<a class="b-button b-button--medium_content b-button--full_width" href="{{= fn_url('checkout.cart') }}">
								<span class="b-button-content">Оформить заказ</span>
							</a>
						{{?}}
					</div>

					{{? enoughToMakeOrder && !enoughForFreeShipping && enoughToShowCostUntilFreeShipping }}
						<div class="b-fastcart-free-delivery-notification">
							До бесплатной доставки осталось <strong>{{= App.formatPrice(it.cart.until_free_shipping, 0) }}</strong>
						</div>
					{{?}}
				{{??}}
					<img src="/static/img/cart/cart_box.png" />

					<div class="b-popup--fast-cart_empty">
						Ваша корзина пуста
					</div>
				{{?}}
			</div>
		</div>
