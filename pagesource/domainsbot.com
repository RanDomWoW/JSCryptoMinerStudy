            $('input#searchBox').focus();

            $(document).keypress(function (e) {
                if (e.which == 13) {
                    StartSearch(searchType, $('input#searchBox').val());
                }
            });

            $('a[bind="searchCommand"]').click(function () {


                StartSearch(searchType, $('input#searchBox').val());

            });

            $('a[bind="searchSelection"]').click(function () {

                // Set search type

                var selType = $(this).attr("data");
                searchType = selType;
                $('a[bind="searchSelection"]').parent().removeClass("s-domain");
                $('a[bind="searchSelection"]').parent().removeClass("s-toll");
                $('a[bind="searchSelection"]').parent().removeClass("s-whois");
                $('a[bind="searchSelection"]').parent().removeClass("selected");

                if (selType == "domain") {
                    $(this).parent().addClass("s-domain");
                    var title = "Type in a domain or keyword";
                    $("#searchBox").attr("title", title);
                    $("#searchBox").attr("placeholder", title);
                } else if (selType == "tollfree") {
                    $(this).parent().addClass("s-toll");
                } else if (selType == "whois") {
                    $(this).parent().addClass("s-whois");
                    var title = "Type in a domain for whois lookup";
                    $("#searchBox").attr("title", title);
                    $("#searchBox").attr("placeholder", title);
                }
                $(this).parent().addClass("selected");

            });



