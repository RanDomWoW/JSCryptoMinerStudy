  <script>
    function initMap(currentMarkers) {
      var latLng = new google.maps.LatLng(35.710600, 139.720131);
      var mapId = document.getElementById('googleMap');
      function map(_latlng, _mapId, _zoom) {
        var mapOptions = {
          center: _latlng,
          zoom: _zoom,
          panControl: false,
          scrollwheel:false,
          zoomControl: true,
          mapTypeId:'roadmap',
          disableDefaultUI: true,
        }
        var map = new google.maps.Map(_mapId,mapOptions);
        var marker = new google.maps.Marker({
          position: _latlng,
          icon: './images/ic_map.png',
          animation: google.maps.Animation.DROP
        });
        marker.setMap(map);
        google.maps.event.addDomListener(window, 'resize', function() {
          map.setCenter(mapOptions.center);
        });
      }
      map(latLng, mapId, 17);
    }
  </script>

  <script>
    $(function(){

      function clearNewsTabs() {
        $('.tab_item').each(function(tid){
          $(this).find('.list_item').empty();
        });
        return;
      }

      function setOnceNewsTab(n) {
        var tabs = $('.tab_item');
        if(n in tabs == false) {
          return;
        }
        var target = $(tabs[n]).find('.list_item');
        if(target.html()) {
          return;
        }
        var urls='/get_post_list.php?tid='+n;
        $.ajax({
          url: urls,
          type: 'GET',
          dataType: 'html',
        })
        .done(function(data) {
          target.html(data);
        })
        .fail(function( data ) {
        });
      }

      $('#tab_top li a').each(function(i){
        $(this).on('click',function() {
          setOnceNewsTab(i);
        });
      });
      clearNewsTabs();
      // setNewsTab(0);
      for(i=0;i<=3;i++) {
        setOnceNewsTab(i);
      }
    });
  </script>
