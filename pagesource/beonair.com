<script>
var bubble = '.rtvb-pop.';
$(document).ready(function() {
$('li.rtvb-list').mouseover(function(event) {
var litem = $(this).data('litem');
openBubble = bubble + litem;
$(openBubble).show();
var leftList = ['terms', 'legal', 'org', 'assign'];
if ($.inArray(litem, leftList) !== -1) {
var tPosX = '465px';
var tPosY = '75px';
} else {
var tPosX = '-20px';
var tPosY = '75px';
}
//var tPosX = event.pageX - 10;
//var tPosY = event.pageY - 1700;
//var mPosX = 0 - (tPosX/2.25);
$(openBubble).css({
'position': 'absolute', 
'top': tPosY, 
'left': tPosX,
//'margin-left': mPosX
});
}).mouseout(function() {
var litem = $(this).data('litem');
closeBubble = bubble + litem;
$(closeBubble).hide();
});   
});
</script>
