<script>
login_tooltip = 0;

if($('bar_password')){
    $('bar_password').observe('keypress',caps_lock);
}

function caps_lock(e){
 if(!login_tooltip){
    login_tooltip = new AG.Tooltip();
    login_tooltip.top   = -37;
    login_tooltip.left  = 86;
    login_tooltip.tooltip_time = 7000;
 }

 if(!$('bar_password')) return true;

 kc = e.keyCode?e.keyCode:e.which;
 sk = e.shiftKey?e.shiftKey:((kc == 16)?true:false);

 if(
    ((kc >= 65 && kc <= 90) && !sk) ||
    ((kc >= 97 && kc <= 122) && sk)



 )
  login_tooltip.show($('bar_password'),'У вас нажата клавиша CapsLock - все буквы вводятся БОЛЬШИМИ. Вы уверены в пароле?');
 else if((((kc >= 1040 && kc <= 1071) || kc == 1025) && !sk) ||
    (((kc >= 1072 && kc <= 1103) || kc == 1025) && sk))
  login_tooltip.show($('bar_password'),'У вас включена русская раскладка и нажата клавиша CapsLock - все буквы вводятся БОЛЬШИМИ. Вы уверены в пароле?');
 else if(
    (((kc >= 1040 && kc <= 1071) || kc == 1025)) ||
    (((kc >= 1072 && kc <= 1103) || kc == 1025))

 )
  login_tooltip.show($('bar_password'),'У вас включена русская раскладка. Вы уверены в пароле?');
 else
  login_tooltip.hide();
 return true;
}
</script>


