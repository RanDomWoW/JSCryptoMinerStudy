<script type="text/javascript">
	$(document).ready(function(){
		
		

		
	//	$('#index_gallery #preview_images .arrows').hide();
		$('#preview_images ul img').css({height:78});
		
		
		//$('#preview_images').width(0);
		//$('#preview_images #small_div').width(0);
		//$('#preview_images ul').width(0);
		//$('#preview_images ul li').width(0);
		//$('#preview_images ul img').width(0);
		
		var last_in=0;
		var last_out=0;
		var last_move=0;
		var first_images_load=false;
		
		var code="";
		 
		link='/articles/effektivnost-raboty-geotermalnoy-teplonasosnoy-sistemy-teplosnabzheniya-v-zhilom-dome';
		if(code){
		
		


			$('#big_img_div').on('click touchstart', function(){
					$('#small_div').stop().animate({'margin-left':'-148px'},function(){
					
						try {
						   code=$(code);
						   
						   var tmph= $(code).height() ? $(code).height() : 330;
						   if(tmph !=330)
							$(code).css({
								'margin-top': -( (tmph-330)/2 )+'px'
							})
						   $(code).height( (tmph/746)*$('#big_img_code').width() );
						   $(code).width(  $('#big_img_code').width() );
						}
						catch (exception_var) {
						 
						}
						
						$('#big_img_code').html(code);	
						$('#big_img_desc span').hide();						
					});
					//alert('m')
				});
		}
		else
		if(link){
				$('#big_img_div').on('click touchstart', function(){
					location.href=link;
				});
				$('#big_img_div').css('cursor','pointer');
			}else{
				$('#big_img_div').css('cursor','default');
			}

		function load_first4images(){
				/* подгружаем большие картинки для первых 4 превью при первом наведении*/
				if(!first_images_load){
		
					var img='';
					img=$('#small_div #gim_1').eq(0).closest('li').find('input.img_big').val();
					$('#big_img_div img#new_gallery_image_1').attr('src',img);
					img=$('#small_div #gim_2').eq(0).closest('li').find('input.img_big').val();
					$('#big_img_div img#new_gallery_image_2').attr('src',img);
					img=$('#small_div #gim_3').eq(0).closest('li').find('input.img_big').val();
					$('#big_img_div img#new_gallery_image_3').attr('src',img);
					img=$('#small_div #gim_4').eq(0).closest('li').find('input.img_big').val();
					$('#big_img_div img#new_gallery_image_4').attr('src',img);
					first_images_load=true;
			
					var li=$('#small_div #gim_1').eq(0).closest('li').prev();
					img=$(li).find('input.img_small').val();
					$(li).find('img').attr('src',img);
			
					li=$('#small_div #gim_5').eq(0).closest('li')
					img=$(li).find('input.img_small').val();
					$(li).find('img').attr('src',img);
					first_images_load=true;
				}
		}
		
		var obj_big_img_div = document.getElementById('big_img_div');
		obj_big_img_div.addEventListener('touchstart', function(event) {
		  	  if (event.targetTouches.length == 1) {
				  load_first4images();
				  if(parseInt($('#small_div').css('marginLeft')))
				  {
					  $('#big_img_div').unbind('click').unbind('touchstart');
					  $('#index_gallery #preview_images').css('display','block');
					  $('#small_div').animate({'margin-left':0})
				  }
				  return false;
		  	  }
		}, false);
				
					
		
		$('#small_div').css({'margin-left':'-148px'});
		
		$('#index_gallery ul li').css('margin-bottom','6px');
		$('#small_div').css('margin-right','6px');
		
		
		
		$('#index_gallery').hover(function(){
			
			last_in=new Date();	
			
			$('#index_gallery').mousemove(function(){
				last_move=new Date();
				setTimeout(function(){
				time=new Date();
				if(last_in>last_out)
				if(time-last_move >= 200){
					load_first4images();
					$('#index_gallery #preview_images').css('display','block');
					$('#small_div').animate({'margin-left':0},{
					step:function(n){
					if(!$('#big_img_code').data('code_fixed_size'))
						$('#big_img_code').children().css({
							width: (598-n)+'px'
						})
						
					}
					})
					}
				},200);
				
			});
			
			
			
		},function(){
			$('#index_gallery').unbind('mousemove');
			
			last_out=new Date(); 
			setTimeout(function(){
			if(last_out>last_in){
				
				$('#small_div').animate({'margin-left':'-148px'},{
					step:function(n){
						if(!$('#big_img_code').data('code_fixed_size'))
						$('#big_img_code').children().css({
							width: (598-n)+'px'
						})
						
					},
					complete: function(){
							$('#index_gallery #preview_images').css('display','none');
							}
					})
				}
			},400);
		});
		
		
		
		$('#preview_images').hover(function(){
			$('#index_gallery #preview_images .arrows a').fadeIn(500);
		},
		function(){
			$('#index_gallery #preview_images .arrows a').fadeOut(500);
		});
		
		if($("#small_div li").length)
			$("#small_div").jCarouselLite({
			vertical: true,
			visible:4,
			btnNext: "#preview_images .next",
			btnPrev: "#preview_images .prev",
			mouseWheel: true,
			afterEnd:function(lis){
				$(lis).each(function(j,li){
					var big_img=$(li).find('input.img_big').val();
					var id=$(li).find('img').attr('id').replace('gim_','');
					$('#big_img_div img#new_gallery_image_'+id).attr('src',big_img);
				});
				
				var li=$(lis[lis.length-1]).next();
				$(li).find('img').attr('src',$(li).find('input.img_small').val());
				var li=$(lis[0]).prev();
				$(li).find('img').attr('src',$(li).find('input.img_small').val());
				
			}
			});
		
		
		$('#index_gallery #preview_images').css('display','none');
		
		
		$('#small_div img').click(function(){
			
			img=$(this).closest('li').find('input.img_big').val();
			desc=$(this).closest('li').find('input.desc').val();
			link=$(this).closest('li').find('input.link').val();
			var code=$(this).closest('li').find('input.code').val();
			var size=$(this).closest('li').data('fontsize');
			var code_fixed_size=$(this).closest('li').find('input.code_fixed_size').val();
			
			$('#big_img_code').data('code_fixed_size',code_fixed_size);
			
			var w=$('#big_img_div').width();
			$('#big_img_desc').width(w);
			$('#big_img_div').width(w);
			$('#big_img_divs_container').width(w);
			
			$('#big_img_div,#big_img_desc').stop().animate({marginLeft:-w+'px'},100, function(){
				//if(!code){
					$('#big_img_div').css('background-image','url('+img+')');
					$('#big_img_code').html('');
					$('#big_img_desc span').show();
				//}
				//if(code){
					
				//}
					
				$('#big_img_desc span').html(desc);
				
				$('#big_img_desc span').css({'font-size':size+'px'});
				$('#big_img_div').unbind('click').unbind('touchstart');
				if(code){
							$('#big_img_div').on('click touchstart', function(){
					//alert(code);
						$('#small_div').animate({'margin-left':'-148px'},function(){
							try {
							   code=$(code);
							   $(code).height( (330/746)*$('#big_img_code').width() );
							   $(code).width(  $('#big_img_code').width() );
							}
							catch (exception_var) {
							 
							}
							$('#big_img_code').html(code);				
						});
						
						$('#big_img_div').css('background-image','none');
						$('#big_img_desc span').hide();
					})
				}
				else{
						if(link){
							$('#big_img_div').on('click touchstart', function(){
								location.href=link;
							});
							$('#big_img_div').css('cursor','pointer');
						}else{
							$('#big_img_div').css('cursor','default');
						}
				}
						$('#big_img_div,#big_img_desc').css({marginLeft:w+'px'});	
						$('#big_img_div,#big_img_desc').animate({marginLeft:0},300,function(){
							$('#big_img_desc').width('auto');
							$('#big_img_div').width('auto');
							$('#big_img_divs_container').width('auto');
						});
				
			});
			
			
			return false;
		});
		
	});
</script>

		
