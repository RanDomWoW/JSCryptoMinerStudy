        link.attr('href', link.attr('href') + hash);
        
        // toggle pseudo focus and hover states for child button
        link.on('mouseenter mouseleave', function(ev) {
            button.toggleClass('active', ev.type === 'mouseenter');
        }).on('focusin focusout', function(ev) {
            button.toggleClass('focus', ev.type === 'focusin');
        });
    });
});
