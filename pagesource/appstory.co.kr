			document.getElementById(name).style.display = '';

			if(name == 'sign_layer_popup_ifrmae'){ // 회원가입 레이어 팝업이면
				
				frame_url = "https://ssl.appstory.co.kr/sign_layer/?hoya_domain=http%3A%2F%2Fwww.appstory.co.kr&top_url=" + top.location.href;
				frame_name = "iframe_sign_layer";

			}
			else {
				frame_url = src_url;
				frame_name = "layer_iframe";
				
				var margin_left_px = parseInt(width_px.replace("px","")) / 2; // 해당 div 가운데 정렬을 위한 가로사이즈 2로 나눔
				margin_left_px = "-" + String(margin_left_px) + "px"; 
				document.getElementById(name).style.margin = "0 0 0 " + margin_left_px; // 나눈 가로사이즈 margin에 적용
				document.getElementById(frame_name).width = width_px;
				document.getElementById(frame_name).height = height_px;
			}

			document.getElementById(frame_name).src = frame_url;
	

		}else{

			document.getElementById('sign_layer_popup_bg').style.display = 'none';
			document.getElementById(name).style.display = 'none';

		}

	}
