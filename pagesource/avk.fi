    function VehicleBrandModelSetup() {
        var type = $("#SidebarVehicleType");
        var brand = $("#SidebarVehicleBrand");
        var model = $("#SidebarVehicleModel");


        BrandModelSetup(brand, model, function (b, m) {
            return b + '-' + m +  '-0';
        });

        type.on("click change", function () {
            brand.html('');

            var selectedType = $(this).val();
            var brandArr = new Array();
            var brandIdx = 0;
            if (selectedType != null && selectedType != "") {
                var availableBrands = window.brands[selectedType];
                if (!availableBrands) return;

                $.each(availableBrands, function (i, item) {
                    brandArr[brandIdx++] = '<option value="';
                    brandArr[brandIdx++] = item.id;
                    brandArr[brandIdx++] = '">';
                    brandArr[brandIdx++] = item.name;
                    brandArr[brandIdx++] = '</option>';
                    // brand.append($("<option />").val(item.id).text(item.name));
                });
            } else {
                var allBrands = window.brands[0];
                $.each(allBrands, function (i, item) {
                    brandArr[brandIdx++] = '<option value="';
                    brandArr[brandIdx++] = item.id;
                    brandArr[brandIdx++] = '">';
                    brandArr[brandIdx++] = item.name;
                    brandArr[brandIdx++] = '</option>';
                    //brand.append($("<option />").val(item.id).text(item.name));
                });
            }
            brand.append(brandArr.join(''));

            brand.val('0');
            brand.change();

        }).change();

                brand.change();

    }

        function PartBrandModelSetup() {
