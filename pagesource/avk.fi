    function BrandModelSetup(brand, model, valFunc) {
        var brandArr = new Array();
        var brandIdx = 0;
        $.each(window.brands[0], function (i, item) {
            brandArr[brandIdx++] = '<option value="';
            brandArr[brandIdx++] = item.id;
            brandArr[brandIdx++] = '">';
            brandArr[brandIdx++] = item.name;
            brandArr[brandIdx++] = '</option>';
            //brand.append($("<option />").val(item.id).text(item.name));
        });
        brand.append(brandArr.join(''));
        brand[0].options[0].text = 'Merkki';
        brand[0].options[0].value = '';

        brand.change(function () {
            model.html('');

            var selectedBrand = $(this).val();

            if (selectedBrand != null && window.models !== undefined) {
                var availableModels = window.models[selectedBrand];

                if (!availableModels) {
                    model.append($("<option />").text("Malli")).prop('disabled', true);
                    return;
                }

                model.prop('disabled', false);

                var modelArr = new Array();
                var modelIdx = 0;
                $.each(availableModels, function (i, item) {
                    var name = (i == 0) ? 'Malli - ' + item.name : item.name;
                    modelArr[modelIdx++] = '<option value="';
                    modelArr[modelIdx++] = valFunc(selectedBrand, item.id);
                    modelArr[modelIdx++] = '">';
                    modelArr[modelIdx++] = name;
                    modelArr[modelIdx++] = '</option>';
                   // model.append($("<option />").val(valFunc(selectedBrand, item.id)).text(name));
                });
                model.append(modelArr.join(''));
            }
        });
    }

    function PartGroupSetup() {
        var partType = $("#SidebarPartType");

        $("#SidebarPartGroup").change(function () {
            partType.html('');

            var selectedGroup = $(this).val();

            if (selectedGroup != null && window.parts !== undefined) {
                var availablePartTypes = window.parts[selectedGroup];

                if (!availablePartTypes) {
                    partType.append($("<option />").text("Osa")).prop('disabled', true);
                    return;
                }

                partType.prop('disabled', false);
                var arr = new Array();
                var arrIdx = 0;
                $.each(availablePartTypes, function (i, item) {
                    var name = (i == 0) ? 'Osa - ' + item.name : item.name;
                    arr[arrIdx++] = '<option value="';
                    arr[arrIdx++] = item.id;
                    arr[arrIdx++] = '">';
                    arr[arrIdx++] = name;
                    arr[arrIdx++] = '</option>';
                    //partType.append($("<option />").val(item.id).text(name));
                });
                partType.append(arr.join(''));
            }

        }).change();


    }

    /* SideBar search tabs */
    function SetSidebarSearchTabs(activeTabIndex) {
        /*
         * Tab indexes:
         *   0 = vehicle
         *   1 = parts
         *   2 = tires
         */
        var availableIndexes = [0, 1, 2];
        try {
            if (activeTabIndex === null || availableIndexes.indexOf(activeTabIndex) == -1) {
                activeTabIndex = 0; // Set default index value
            }
        } catch (e) {
            activeTabIndex = 0;
        }

        /* SideBar search tabs */
        var tabContent = $('.tab-content');
        var tabMenu = $('ul#sidebar-tab-search li');

        tabMenu.eq(activeTabIndex).addClass('active'); // Set active menu item
        tabContent.hide(); // Hide all contents
        tabContent.eq(activeTabIndex).show(); // Show active tab content

        tabMenu.on('click', '', function (e) {

            tabMenu.removeClass('active');
            tabContent.hide();

            $(this).addClass('active');
            var activeTab = $(this).attr("rel");
            $("#" + activeTab).show();

            e.preventDefault();
        });
    }


    var sidebarUpdateInProgress = false;
    var sidebarUpdateHandle = null;

    function updateSidebarResultCount(form, resultTarget) {
        if (sidebarUpdateInProgress || sidebarUpdateHandle) {
            if (!sidebarUpdateHandle) {
                sidebarUpdateHandle = setTimeout(function(){callAjaxUpdateSidebarResultCount(form, resultTarget);}, 250);
            }
        } else {
            callAjaxUpdateSidebarResultCount(form, resultTarget);
        }
    }

    function callAjaxUpdateSidebarResultCount(form, resultTarget) {
        sidebarUpdateInProgress = true;
        sidebarUpdateHandle = null;

        $.get(resultTarget.data('url'), form.serialize())
            .done(function (response) {
                resultTarget.html(response ? '(' + response.Count + ')' : '');
            })
            .always(function () {
                sidebarUpdateInProgress = false;
            });
    }
