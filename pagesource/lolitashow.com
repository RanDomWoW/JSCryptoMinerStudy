//click img
function setCountries(obj){

	var src=$(obj).attr("data-source");

	$("#national_flag").attr("src","http://img.mlo.me/lolitashow/images/national_flag/big/"+src+".png");//显示大国旗

	var country_name=$(obj).attr("title");
	$("#country_name").html(country_name);//显示国家名

	var language=$(obj).attr("lang");


	//选择国家后对应的语言显示
	if(language!=''){
		language=language.split(",");
		$("#languageList a").hide();

		$("#languageList a").each(function(){

			for(var i=0;i<language.length;i++){
				if( $(this).attr("language") == language[i] ){
					$(this).show();
				}else{
					//$(this).show();
				}
			}


			$("#languageList .more").show();

		});
	}else{
		$("#languageList a").show();
		$("#languageList .more").hide();
	}


	$("#countryid").val($(obj).attr("id"));


}
;(function($){

	$(function(){

		/*
			by yang yong hong
		*/
		//
		$("#languageList a").click(function(){

			if($(this).hasClass("more")){

				$(this).siblings().show();
				$(this).hide();

			}else{
				$(this).addClass("current").siblings().removeClass("current");

				var language=$(this).attr("lang");

				$("#language").val(language);

				if($("#language").val()!=""){

					//$("#languageForm").submit();

					$.ajax({
						url:'http://www.lolitashow.com/countries/index.html',
						type:'POST',
						data:"lang="+$("#language").val()+"&country="+$("#countryid").val(),
						dataType:'html',
						success:function(result){

							window.location.href=result;
						},
						error:function(){

						}
					});

				}
			}

		});

	});
})(jQuery);

