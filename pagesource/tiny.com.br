<script>
function exibirBoxLoginPrincipal() {
	$("#item-menu-login").hide();
	setStorageItem("box_login_open", "S");

	setTimeout(function() {
		$("#box-login-principal").slideDown("fast", function() {
			if (!possuiSuporteTouch()) {
				$("#username").focus();
			}
		});
	}, 30);
}

function ocultarBoxLoginPrincipal() {
	setStorageItem("box_login_open", "N");

	$("#box-login-principal").slideUp("fast", function() {
		$("#item-menu-login").show();
	});
}

function toggleMenuLateralSite() {
	if ($("body").hasClass("sidebar-open")) {
		fecharMenuLateralSite();
	} else {
		abrirMenuLateralSite();
	}
}

function abrirMenuLateralSite() {
	if (!$("body").hasClass("sidebar-open")) {
		$("body").addClass("sidebar-open");
	}
}

function fecharMenuLateralSite() {
	$("body").removeClass("sidebar-open");
}

function fecharMenuLateralSiteOnClick(event) {
	if (event != null
		&& event.originalEvent != null
		&& event.originalEvent.target.tagName == "BODY"
		&& $(event.originalEvent.target).hasClass("sidebar-open")
		&& $(window).width() < 992) {
		fecharMenuLateralSite();
	}
}

$(document).ready(function() {
	$("body").on("click", fecharMenuLateralSiteOnClick);

	$(window).on("resize", function() {
		if ($(window).width() < 992) {
			ocultarBoxLoginPrincipal();
		} else {
			fecharMenuLateralSite();
		}
	});

		$(window).on("scroll", function(evt) {
		if ($(window).scrollTop() > 180) {
			$("#navbar-menu-principal").addClass("menu-fixo");
		} else {
			$("#navbar-menu-principal").removeClass("menu-fixo");
		}
	});
	});
