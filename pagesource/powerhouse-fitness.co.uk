<script type="text/javascript">
    require([
        "jquery",
    ], function($){
        'use strict';

        $("#order-tracker-link").on('click', function () {
            event.preventDefault();
            $( "#order-tracker" ).slideToggle( 300, function() {
                // Animation complete.
            });
        });

        $("#close-tracker-link").on('click', function () {
            event.preventDefault();
            $( "#order-tracker" ).slideToggle( 300, function() {
                // Animation complete.
            });
        });

        $("#order-tracker-form").submit(function( event ) {
            event.preventDefault();
            $.ajax({
                url: "/OrderTracker/AJAX/getOrder",
                dataType: 'json',
                data: {
                    orderNo: $('#tracker-order-no').val(),
                    email: $('#tracker-email').val()
                },
            }).success(function (data) {
                if(data['error']) {
                    $('#order-status-message').html(data['error']);
                } else {
                    $('#order-status-message').html(data['msg']);
                }

                if(data['shipments'] !== undefined && Array.isArray(data['shipments']) && data['shipments'].length !== 0)  {
                    $("#shipment-list").html("");  // remove old list before we build up new list of shipments
                    $("#tracking-info").show();
                    $("#no-shipments").html(data['shipments'].length);
                    if(data['shipments'].length > 1 ) {
                        $("#no-shipments-msg").show();
                    } else {
                        $("#no-shipments-msg").hide();
                    }
                    for (var curShipment = 0; curShipment < data['shipments'].length; curShipment++) {
                        var shipmentHtml = '<li>';
                        if(data['shipments'][curShipment]['tracking'] !== undefined) {
                            if (data['shipments'][curShipment]['tracking']['carrier'] !== undefined) {
                                shipmentHtml += 'Delivered by ' + data['shipments'][curShipment]['tracking']['carrier'] + '. ';
                            }
                            if (data['shipments'][curShipment]['tracking']['number'] !== undefined) {
                                shipmentHtml += 'Tracking Number: ' + data['shipments'][curShipment]['tracking']['number'];
                            }
                            if (data['shipments'][curShipment]['tracking']['url'] !== undefined) {
                                shipmentHtml += ' Click <a href="' + data['shipments'][curShipment]['tracking']['url'] + '" target="_blank">here</a> to track your order.';
                            }
                        }
                        if(data['shipments'][curShipment]['items'] !== undefined && Array.isArray(data['shipments'][curShipment]['items']) && data['shipments'][curShipment]['items'].length !== 0 ) {
                            shipmentHtml += '<br/>Shipment Contains:<br/>';
                            for (var curItem = 0; curItem < data['shipments'][curShipment]['items'].length; curItem++) {
                                shipmentHtml += data['shipments'][curShipment]['items'][curItem]['qty'] + " x " + data['shipments'][curShipment]['items'][curItem]['name'];
                            }
                        }
                        $("#shipment-list").prepend(shipmentHtml + "</li>");

                    }

                    $('#tracking-info').show();
                } else {
                    $('#tracking-info').hide();
                }
                if(data['address'] !== undefined)  {
                    if (data['address']['line_1']) {
                        $('#ph-ot-address-1').html(data['address']['line_1']);
                        $('#order-delivery-address').show();
                    } else {
                        $('#order-delivery-address').hide();
                    }
                    if (data['address']['line_2']) {
                        $('#ph-ot-address-2').html(data['address']['line_2']);
                        $('#ph-ot-address-2').show();
                    } else {
                        $('#ph-ot-address-2').hide();
                    }
                    if (data['address']['city']) {
                        $('#ph-ot-city').html(data['address']['city']);
                        $('#ph-ot-city').show();
                    } else {
                        $('#ph-ot-city').hide();
                    }
                    if (data['address']['postcode']) {
                        $('#ph-ot-postcode').html(data['address']['postcode']);
                        $('#ph-ot-postcode').show();
                    } else {
                        $('#ph-ot-postcode').hide();
                    }
                    if (data['address']['region']) {
                        $('#ph-ot-region').html(data['address']['region']);
                        $('#ph-ot-region').show();
                    } else {
                        $('#ph-ot-region').hide();
                    }
                } else {
                    $('#order-delivery-address').hide();
                }
                $('#order-details').show();
            });
        });
    });
</script>


