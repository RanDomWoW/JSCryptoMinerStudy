    float:left;

    padding:2px 5px;

}

span.jPag-current{

    cursor:default;

    font-weight:normal;

    line-height:15px;

    height:18px;

    padding:2px 5px;

    margin:2px;

    float:left;

}

ul.jPag-pages li span.jPag-previous,

ul.jPag-pages li span.jPag-next,

span.jPag-sprevious,

span.jPag-snext,

