/*   引用
--------------------------------------------- */
blockquote {
background-color:#EEEEEE;
position: relative;
margin: 2em 0;
padding: 3em;
border: 1px solid #CCC;
border-radius: 6px;
}
blockquote:before,
blockquote:after{
font-family:"ＭＳ Ｐゴシック",sans-serif;
color:#A9A9A9;
position: absolute;
font-size: 4em;
}
blockquote:before{
font-size:600%;
line-height:1.2em;
position:absolute;
left:0;
top:0;
content: '\201C';
}
blockquote:after{
font-size:600%;
line-height:0em;
position:absolute;
right:0;
bottom:0;
bottom: 0px;
right: 0px;
content: '\201D';
}
@media only screen and (max-width: 780px) {
blockquote {
padding: 3em 0.5em;
}
}
