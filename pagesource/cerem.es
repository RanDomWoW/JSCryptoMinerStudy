$(document).ready(function() {
    /* testimonio toggle */
    $('.testimonio-toggle').click(function(e) {
        e.preventDefault();
        var full_container = $(this).siblings('.testimonio-full-hidden');
        var short_container = $(this).siblings('.testimonio-text');
        if (!$(this).hasClass('topen')) {
            var full_text = full_container.html();
            var short_text = short_container.html();
            short_container.html(full_text);
            full_container.html(short_text);
            short_container.css('height', '100%');
            var h = short_container.height();
            short_container.css('height', '120px');
            short_container.animate({height:h+'px'}, 600);
            $(this).html('Cerrar').addClass('topen');
        }
        else {
            var full_text = short_container.html();
            var short_text = full_container.html();
            short_container.animate({height:'120px'}, 600, function() {
                short_container.html(short_text);
                full_container.html(full_text);
                short_container.css('display', 'inline');
            });
            $(this).html('Leer más').removeClass('topen');
        }
    });
    
    $('#home-ventajas-side li a').click(function(e) {
        e.preventDefault();
        $(this).siblings('span').slideToggle();
    });
    
    $(".home-video").on('click', 'a', function(e) {
        $.ajaxSetup ({cache: false});
        var id = 0;
        e.preventDefault();
        $(this).colorbox({
            close:'cerrar',
            width: '80%',
            height: '80%',
            onComplete: function() {
                if ($('video').length > 0) {
                    id = $('video').attr('id');
                    videojs(id).ready(function(){
                        var myPlayer = this;
                        myPlayer.load();
                        myPlayer.play();
                    });
                }
            },
            onCleanup: function() {
                if (id != 0) {
                    videojs(id).dispose();
                }
            }
        });
    }); 
    
    /* CARRUSEL EMPRESAS */
    
    $('#carousel-empresas').jcarousel({
        scroll:7,
        itemLoadCallback: itemLoadCallbackFunction3
    });
    
    function itemLoadCallbackFunction3(carousel3, state) {
        if (carousel3.has(carousel3.first, carousel3.last)) {
            return
        } 
        $.ajax({
            type: 'post',
            url: '/empresas/cargar/7/page:' + Math.ceil(carousel3.last/7),
            dataType: 'json',
            success: function(data) {
                itemAddCallback3(carousel3, carousel3.first, carousel3.last, data);
            }
        });
    }
    
    function itemAddCallback3(carousel3, first, last, data) {
        var pos = 0;
        $.each(data, function(i, item){
            if (item.carouselSize - first < 7) {
                pos = (Math.ceil(carousel3.last/7))*7 - last;
            }
            carousel3.add(first+i+pos, '<img src="/img/' + item.imgurl + '" width="140" height="70" alt="' + item.name + '" title="' + item.name + '" />');
            carousel3.size(item.carouselSize);
        });
    }
})

