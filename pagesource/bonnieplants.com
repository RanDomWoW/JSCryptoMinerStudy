  var cntapi = 0;

  function refreshtrending() {

  cntapi = 0;
  $("#trenditemswrapper_buffer").html("");

  $.getJSON("https://api.chartbeat.com/live/toppages/v3/?apikey=748e8b2bf1dbea1dd94aa367f1b68979&host=bonnieplants.com&limit=6",
  {format: "jsonp"})
  .done(function( data ) {

  cntapi = 0;
  $("#trenditemswrapper_buffer").html("");
  var tboxmain = $(".trenditem").clone();

  var all_page_titles = $(".brick-body h4 a");
  var trend_page_titles = $(".trenditem h4 a");

  $.each(data.pages, function(k, page) {
  //var tbox = $(".trenditem").clone();

  /*for(tx = 0; tx < page_titles.length; tx++) {
      if(page.title.indexOf($(page_titles[tx]).text().trim()) > -1) {
        for(ttx = 0; ttx < trend_page_titles.length; ttx++) {
          if(page.title.indexOf($(trend_page_titles[ttx]).text().trim()) > -1) {
              console.log("got one");
          }
          console.log($(page_titles[tx]).text().trim());
        }
      }
  }*/
  
  var okaytouse = true;        
  // Custom Page Rules
  if (page.path.trim() == "/") {
      okaytouse = false;
  }
  if (page.title.indexOf("Newsletter") > -1) {
      okaytouse = false;
      $("body").append("<br/>" + page.title + " - " + page.path);
  }

  // check to compare against all titles and 
  // current trending titles to see if okaytouse
  // has to pass above rules
  if (okaytouse) {
        for (tx = 0; tx < all_page_titles.length; tx++) {
            if (page.title.indexOf($(all_page_titles[tx]).text().trim()) > -1) {
                //don't use unless it was in trending already
                okaytouse = false;
                //console.log("oops, cant use:" + page.title);
                for (ttx = 0; ttx < trend_page_titles.length; ttx++) {
                    if (page.title.indexOf($(trend_page_titles[ttx]).text().trim()) > -1) {
                        okaytouse = true;
                        //console.log("nevermind, can use:" + page.title);
                        //break;
                    }
                    //console.log($(all_page_titles[tx]).text().trim());
                }
                //if(okaytouse) { break; }
            }
        }
  }

  if(okaytouse) {
    //console.log("okay to use"); 
           
    var tbox = $('<div class="trenditem">'); //tboxmain.clone();
    parts = page.path.split("\/");

    $(tbox).load("/trending/?sslugg=" + parts[2] + "&visitors=" + page.stats.people, function() {
        cntapi++;
        $("#trenditemswrapper_buffer").append(tbox);
        //$(tbox).show();
        if(cntapi == 3) {
          $("#trenditemswrapper").html("");
          $("#trenditemswrapper").html($("#trenditemswrapper_buffer").html());
          $("#trenditemswrapper_buffer").html("");
          //cntapi = 0;
        }
        
        if(cntapi > 6) {
          cntapi = 0;
        }
      });
      
  }
    
  });
  });

  }

  refreshtrending();

  var atrendclear = setInterval(function(){ refreshtrending(); console.log("updated trending") }, 15000);

  })(jQuery);

</script>