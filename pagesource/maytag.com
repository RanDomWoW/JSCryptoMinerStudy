            digitalData.page = {
                pageInfo: {
                    "pageID": "/content/maytag/en_us.html",
                    "destinationURL": "http://www.maytag.com/content/maytag/en_us.html",
                    "referringURL": document.referrer,
                    "pageType": "whirlpool/digitalplatform/components/templates/homepage",
