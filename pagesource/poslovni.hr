    <script>
$(document).ready(function() {
    var $form = $('#daily_yes_no_x_pool');

    $('#daily_yes_no_x_pool_submit').click(function(e) {
        var $selectedRadio = $form.find('input[name=radio-choice]:checked').val(),
            url = $form.attr('action') + $selectedRadio;

        e.preventDefault();
        $.post(url, {'csrfmiddlewaretoken': 'cxiut7LJId7GXuAwCkg87lI92YGQRslv'},
                function(response) {
            })
            .success(function(data, status, jqxhr) {

                if(jqxhr.status == '201'){
                    var agree = data.result.agree;
                    var disagree = data.result.disagree;
                    var dontknow = data.result.dontknow;

                    $form.find('span.red').text(agree + '%');
                    $form.find('span.blue').text(disagree + '%');
                    $form.find('span.gray').text(dontknow + '%');

                    setNotiff ( 'Hvala!', data.message, '', 'T', true );
                } else {
                    setNotiff ( 'Greška', data, '', 'T', true, 'error' );
                }
            })
            .error(function(r, status, jqxhr){
                setNotiff ('Greška', r.responseText, '', 'T', true, 'error');
            });

        return false;
    });
})

