	{
		$('input.input-sm').focus();
		$('input.input-min-sm').focus();
	});
	var companyDataGrid = $('#companyDataGrid').DataTable({
		"fixedHeader": true,
		"processing": true,
        "serverSide": true,
        "ajax": {
			"url": "/wp-admin/admin-ajax.php?action=companydatatabletest_action",
			"type": "POST"
		},
		"paging": true,
		"pageLength": 150,
		"pagingType": "full_numbers",
		"info": true,
		"dom": 'Bfrtip',
		"stateSave": false,
		"responsive": true,
		"columns": [
			{
				"width": "19%",
				"orderable": false,
				"searchable": true,
				"className": "col_company_name",
				"render": function(data, type, row)
				{
					return '<a href="//'+row[1]+'" target="_blank" style="font-size:15px;"><img src="https://www.google.com/s2/favicons?domain='+row[1]+'" alt="'+data+'" />&nbsp;&nbsp;'+data+'</a>&nbsp;&nbsp;<a href="'+row[2]+'" target="_blank"><img src="http://saas1000.com/wp-content/themes/consulting/css/images/linkedin.gif" alt="linkedin" width="16px" /></a>';
				},
			},
			{
				"visible": false,
				"orderable": false,
				"searchable": true,
				"className": "col_url",
			},
			{
				"visible": false,
				"orderable": false,
				"searchable": true,
				"className": "col_company_linkedin_url",
			},
			{
				"width": "9%",
				"orderable": false,
				"searchable": true,
				"className": "col_city",
			},
			{
				"width": "9%",
				"orderable": false,
				"searchable": true,
				"className": "col_state",
			},
			{
				"visible": false,
				"orderable": false,
				"searchable": true,
				"className": "col_country",
			},
			{
				"type":"num",
				"visible": false,
				"orderable": false,
				"searchable": true,
				"className": "col_november_2016_employees mynumcol",
			},
			{
				"width": "12%",
				"type":"num",
				"orderable": false,
				"searchable": true,
				"className": "col_may_2017_employees mynumcol",
			},
			{
				"width": "15%",
				"type":"num",
				"orderable": false,
				"searchable": true,
				"className": "col_6_month_growth mynumcol",
			},
			{
				"width": "17%",
				"orderable": false,
				"searchable": true,
				"className": "col_leadinvestor mynumcol",
			},
			{
				"width": "5%",
				"orderable": false,
				"searchable": false,
				"className": "col_links",
				"render": function(data, type, row)
				{
					var linkStr = '';
					if(data != "")
					{
						linkStr += '<a href="'+data+'" target="_blank">Interview</a>';
					}
					return linkStr;
				},
			},
			{
				"width": "14%",
				"type":"num",
				"visible": true,
				"orderable": false,
				"searchable": true,
				"className": "col_CurrentRanking mynumcol",
			},
		],
		"createdRow": function(row, data, index)
		{
            if(data[8] < 0)
			{
				$('td', row).eq(4).addClass('red');
			}
			else
			{
				$('td', row).eq(4).addClass('green');
			}

			if(data[12] != "")
			{
				$('td', row).eq(6).append(' <a href="'+data[12]+'" target="_blank">Growth</a>');
			}
			
			$('td', row).eq(7).addClass('bold');

			/*
			if(data[13] < data[11])
			{
				//$('td', row).eq(7).addClass('red').html(Math.abs(data[11]-data[13])+'<img src="http://saas1000.com/wp-content/themes/consulting/images/down-arrow16.png" style="vertical-align:sub; margin-left:5px;" />');
				$('td', row).eq(7).html(Math.abs(data[11]-data[13]));
			}
			else if(data[13] > data[11])
			{
				//$('td', row).eq(7).addClass('green').html(Math.abs(data[13]-data[11])+'<img src="http://saas1000.com/wp-content/themes/consulting/images/up-arrow16.png" style="vertical-align:sub; margin-left:5px;" />');
				$('td', row).eq(7).html(Math.abs(data[13]-data[11]));
			}
			*/

        },
		"order": [[ 11, "asc" ]],
		"buttons": [], // 'copy', 'csv', 'excel', 'pdf', 'print'
	});
	$('body').on('click', 'p.ascending a', function()
	{
		var col_no = $(this).parents('p').attr('col');
		companyDataGrid.order([col_no,'asc']);
		companyDataGrid.ajax.reload();
	});
	$('body').on('click', 'p.descending a', function()
	{
		var col_no = $(this).parents('p').attr('col');
		companyDataGrid.order([col_no,'desc']);
		companyDataGrid.ajax.reload();
	});
	$('body').on('keyup change', 'input.input-sm', function()
	{
		var col_no = $(this).attr('col');
		companyDataGrid.search('like!^!'+col_no+'!^!'+this.value);
		companyDataGrid.ajax.reload();
	});
	$('body').on('click', 'span.input-group-addon', function()
