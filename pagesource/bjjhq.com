<script language="javascript">
    surveyValidate = new Validation('form1',{immediate:true, focusOnError : false});    //initialize the validation
      
    function SubmitSurvey()
    {   
        if(surveyValidate.validate())
        {
            document.body.style.cursor = "wait";
            var body = Form.getInputs('form1','radio','body').find(function(radio) { return radio.checked; }).value;
            var gender = Form.getInputs('form1','radio','gender').find(function(radio) { return radio.checked; }).value;
            var height, weight;
            if(Form.getInputs('form1','radio','stats_country').find(function(radio) { return radio.checked; }).value == 'us')
            {
                height = parseFloat($F('ft') * 12) + parseFloat($F('in'));
                weight = $F('lb');
            }
            else
            {
                height = Math.round(($F('cm') / 2.54) * 100) / 100;
                weight = Math.round(($F('kg') * 2.20462) * 100) / 100;
            }
          
            productCallServer('sizing_suggestions?profile=&gender=' + gender + "&body_type=" + body + "&height=" + height + "&weight=" + weight,'');
            _gaq.push(['_trackEvent', "sizing_suggestion", "submit"]);
        }
    }
    
 
    function toggleGender(gender)
    {
        if(gender == "male")
        {
            $$("img[id$='_male']").each(function(i){i.show()});    
            $$("div[id$='_female']").each(function(i){i.hide()});    
        }
        else
        {
            $$("img[id$='_male']").each(function(i){i.hide()});    
            $$("div[id$='_female']").each(function(i){i.show()});           
        }
    }  
</script>


   
