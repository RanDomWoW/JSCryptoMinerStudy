<script type="text/javascript">
    var z_HoursLeft = "10";
    var z_MinutesLeft = "41";
    var z_SecondsLeft = "23";
    var z_IsMultipleDeal = false;

    function setTimeLeft()
    {
        z_SecondsLeft--;
        if(z_SecondsLeft < 0)
        {
            z_SecondsLeft = 59;
            z_MinutesLeft--;
        }
        if(z_MinutesLeft < 0)
        {
            z_MinutesLeft = 59;
            z_HoursLeft--;
        }
        //if the hours left is -1 then the deal is over so refresh the page
        if(z_HoursLeft < 0)
        {
            //window.location.reload(); - removed to test affect on load YH 2/25/11
            z_HoursLeft = 0;
            z_MinutesLeft = 0;
            z_SecondsLeft = 0;
            
            clearInterval(timerinterval);
            var randms = randomFromTo(0, 120000);  //random milliseconds until reload
            setTimeout("window.location.reload()",randms);
        }
        
        $$('span[id $=timer_Hours]').each(function(element) { element.innerHTML = PadLeft(z_HoursLeft.toString(),'0',2);});
        $$('span[id $=timer_Minutes]').each(function(element) { element.innerHTML = PadLeft(z_MinutesLeft.toString(),'0',2);});
        $$('span[id $=timer_Seconds]').each(function(element) { element.innerHTML = PadLeft(z_SecondsLeft.toString(),'0',2);});
    }

    var timerinterval = setInterval("setTimeLeft()",1000);
       
</script>

