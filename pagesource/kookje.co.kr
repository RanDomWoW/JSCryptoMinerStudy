		<script type='text/javascript'>
			function setCookie( name, value, expiredays )
			{
				var todayDate = new Date();
				todayDate.setDate( todayDate.getDate() + expiredays );
				document.cookie = name + '=' + escape( value ) + '; path=/; expires=' + todayDate.toGMTString() + ';'
			}
			function getCookie( name )
			{
				var nameOfCookie = name + '=';
				var x = 0;
				while ( x <= document.cookie.length )
				{
						var y = (x+nameOfCookie.length);
						if ( document.cookie.substring( x, y ) == nameOfCookie ) {
							if ( (endOfCookie=document.cookie.indexOf( ';', y )) == -1 )
								endOfCookie = document.cookie.length;
								return unescape( document.cookie.substring( y, endOfCookie ) );
						}
						x = document.cookie.indexOf( ' ', x ) + 1;
						if ( x == 0 )
							break;
				}
				return '';
			}
			function popupHide()
			{
				document.getElementById('popup4').style.display='none';
				if (document.getElementById('popupChk').checked) { 
					setCookie('popup_stop','done',1);
				}
			}
			function popupHide_slide(){
				document.getElementById('slide_popup').style.display='none';
				if (document.getElementById('popupChk').checked) { 
					setCookie('popup_stop','done',1);
				}
			}
			function popupHide_slide2(){
				document.getElementById('system_popup').style.display='none';
				if (document.getElementById('popupChk2').checked) { 
					setCookie('popup_stop2','done',1);
				}
			}
			//팝업 있을 때
			if ( getCookie('popup_stop') != 'done' ){
				document.getElementById('popup4').style.display='none'; //popup 추가시 none->block로 변경
			}

			function popupHide2(){
				document.getElementById('popup9').style.display='none';
			}			
		
		</script>

