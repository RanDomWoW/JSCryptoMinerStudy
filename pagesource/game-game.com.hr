    <script>
      var page=0;
      function loadGames(link){
        if(link.loading) return;
        link.loading = true;
        var url = window.location.toString();
        var expr = new RegExp('/page([0-9]+).*', 'gi');
        var postfix = '';
        var res = expr.exec(url)
        if(!page){
          aURL = '/ajax'+window.location.pathname;
                    if(res){
            page = parseInt(res[1])+1;
            aURL = aURL.replace('page'+res[1],'page'+page);
          }else{
            page = 2;
            aURL+='page2/';

          }
        }else{
          page++;
          aURL = aURL.replace(/page[0-9]+/ig,'page'+page);
        }
        $('#myButton').html('Učitavanje ...');

        $.ajax({
          url: aURL,
          success: function( data ) {
            link.loading = false;
            $("#games .pagination").remove();
            $("#games #clear_pg").remove();
            $("#games .more").remove();
            $('html, body').animate({
                scrollTop: $("#games .item").last().offset().top+50
            }, 1000);
            var total = $($.parseHTML(data)).filter('.item').length;
            $( $.parseHTML(data) ).appendTo('#gamelist').show('slow');
            $('#games').append(data);
            if(total==0){
              $(link).remove();
            }
            $('#myButton').html("Više igre ...");
            showVisible();
          },
          dataType: 'html'
        });
      }

      
    </script>
   
