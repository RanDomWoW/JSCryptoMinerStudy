	<script type="text/template" id="search-result-template-staff_directory">
		<div class="typeahead__result">
			<article>
				<h1 class="h4">
					<a href="/?p=<%=ID%>"><%=post_title%></a>
				</h1>

				<ul class="typeahead__result__detail-list">
					<%
					var jobTitle = _.findWhere(meta, {meta_key : '_job_title'});

					if (jobTitle) { %>
						<li>
							<span class="icon-text">
								<span class="icon-text__icon icon-circle icon-circle--grey"><i class="icon icon-user" aria-hidden="true"></i></span>
								<span class="icon-text__text"><b>Job title:</b> <%=jobTitle.meta_value%></span>
							</span>
						</li>
					<% } %>

					<%
					var department = _.findWhere(meta, {meta_key : '_department'});

					if (department) { %>
					<li>
						<span class="icon-text">
							<span class="icon-text__icon icon-circle icon-circle--grey"><i class="icon icon-book" aria-hidden="true"></i></span>
							<span class="icon-text__text"><b>Department:</b> <%=department.meta_value%></span>
						</span>
					</li>
					<% } %>
				</ul>
			</article>
		</div>
	</script>

