                    var words = ['zero','one','two','three','four', 'five','six','seven','eight','nine'];
                    var customerCount = response.customers.toString();
                    var html = '<h5>BrightLocal is used by <span class="counter">';
                    for (var i = 0, len = customerCount.length; i < len; i += 1) {
                        html += '<span class="' + words[customerCount.charAt(i)] + '">' + customerCount.charAt(i) + '</span> ';
