<script type="text/javascript">
function brands_menu_load() {
    $(".brands-menu .menu").each(function (i) {
        $(this).click(function () {
            $(this).addClass("on").siblings().removeClass("on");
            $("ul.brands").eq(i).removeClass("off").siblings().addClass("off");
        })
    })
}

$(function () {
    $.ajax({
        type: "GET",
        url: SHOP_SITE_URL + "/index.php?act=index&op=json_brand_street",
        dataType: "json",
        success: function (data) {
            if (data.config.brand_street_status == 1) {
                var brands_menu_li = "", brands_list_ul = "", class_on = "", class_off = "", i, j, url;
                var image_url = "https://image.kilimall.com/kenya/shop/common/";
                for (i = 0; i < data.config.brand_street_cat_array.length; i++) {
                    class_on = i == 0 ? "on" : "";
                    class_off = i == 0 ? "" : "off";
                    brands_menu_li += "<li class='menu " + class_on + "'>" + data.config.brand_street_cat_array[i] + " <span></span></li>";
                    brands_list_ul += '<ul class="brands brands-content ' + class_off + '">';
                    if (typeof (data.list[i]) != "undefined") {
                        for (j = 0; j < data.list[i].length && j < data.config.brand_street_number; j++) {
                            url = (data.list[i][j].url == "" || data.list[i][j].url == "http://") ? "" : ' href="' + data.list[i][j].url + '"';
                            brands_list_ul += '<li><a ' + url + ' target="_blank"><img src="' + image_url + data.list[i][j].image + '" alt="' + data.list[i][j].brand_name + '"/></a></li>'
                        }
                    }
                    brands_list_ul += '</ul>';
                }
                $(".brands-menu").html(brands_menu_li);
                $(".brand-list").append(brands_list_ul);
                $(".brand-sider-bar").show();
                $(".brand-list").show();
                brands_menu_load();
            }
        }
    })
});
</script>
