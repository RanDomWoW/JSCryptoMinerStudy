            mask.addClass("screen-mask");
            $('body').append(mask);
            $('.screen-mask').load(SITEURL + '/index.php?act=login&op=quick_register&inajax=1',function(){
                $('.screen-mask .quick-register').css({"display":"block",top:'200px',left:'35%'});
                $('.close').click(function(){$('.screen-mask').remove();})
            });
            return false;
        }
    }
</script>
