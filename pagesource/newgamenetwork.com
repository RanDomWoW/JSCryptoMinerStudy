		$(".news-header-text a").click(function() {
					$(".news-header-text").removeClass("selected");
					$(this).parent().addClass("selected");
		});
		$(function(){
				var menu = $('#dropmenucontainer');
				pos = menu.offset();
				var sa = $('#scroll-ad');
				sapos = sa.offset();
				rc = $('#right-container').offset();
				$(window).scroll(function(){
					if($(this).scrollTop() > pos.top && (!menu.hasClass('fixedmenu'))){
						menu.addClass('fixedmenu').fadeIn('fast');
						$("#menu-packer").show();
						$('#socialbar').show();
					} else if($(this).scrollTop() <= pos.top && menu.hasClass('fixedmenu')){
						menu.removeClass('fixedmenu').fadeIn('fast');
						$("#menu-packer").hide();
						$('#socialbar').hide();
					}
					if(sapos){
						if(($(this).scrollTop() > (sapos.top - menu.height() - 10)) && (!$('#menu-button').is(':visible'))){
							sa.css("position", "fixed");
							sa.css("top", menu.height() + 10);
							sa.css("width", $('#right-container').width());
							sa.css("right", rc.right);
						} else if($(this).scrollTop() <= (sapos.top + menu.height() + 10)){
							sa.css("position", "static");
							sa.css("top", "auto");
							sa.css("right", "auto");
							sa.css("width", "auto");
						}
					}
				});
			});
