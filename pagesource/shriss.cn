                function clockon() {
                    thistime = new Date()
                    var Years = thistime.getFullYear()
                    var Months = thistime.getMonth() + 1
                    var Dates = thistime.getDate()
                    var hours = thistime.getHours()
                    var minutes = thistime.getMinutes()
                    var seconds = thistime.getSeconds()
                    if (eval(hours) < 10) { hours = "0" + hours }
                    if (eval(minutes) < 10) { minutes = "0" + minutes }
                    if (seconds < 10) { seconds = "0" + seconds }
                    thistime = Years + "年" + Months + "月" + Dates + "日" + " " + d[thistime.getDay() + 1] + " " + hours + ":" + minutes + ":" + seconds + " " + d[thistime.getDay() + 1]
                    document.getElementById('timess').innerHTML = thistime;
                    var timer = setTimeout("clockon()", 200)
                }
          </script>
