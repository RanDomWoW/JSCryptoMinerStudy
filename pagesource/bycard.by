    var cur_width = 1000;

    $(window).resize(function () {
        var x = $(document).width();
        var y = 1000;
        if (x >= 1162)
            y = 1162;
        if (x >= 1324)
            y = 1324;
        if (y != cur_width) {
            cur_width = y;
            $('body.wide .header4 .full-width').css('width', y + 'px');
            $('body.wide .footer .main3footer').css('max-width', y + 'px');
        }
    });

    $(document).ready(function () {
        $('.selected-region').on('click tap', function () {
            if ($('ul.dropdown-menu.sr').css('display') == 'block') {
                $('ul.dropdown-menu.sr').hide();
                return false;
            }
            $('ul.dropdown-menu.uc').hide();
            $('ul.dropdown-menu.sr').show();
            return false;
        });

                $('.dropdown.usercab .username').on('click tap', function () {
            if ($('ul.dropdown-menu.uc').css('display') == 'block') {
                $('ul.dropdown-menu.uc').hide();
                return false;
            }
            $('ul.dropdown-menu.sr').hide();
            $('ul.dropdown-menu.uc').show();
            return false;
        });

        $('.list-part a').on('click tap', function () {
            var new_id = $(this).data('id');
            var old_id = $.cookie('region');
            if (old_id != new_id || true) {
                $.cookie('region', new_id, {
                    expires: 365,
                    path: '/',
                    domain: "bycard.by"
                });
                window.location.reload();
            }
            return false;
        });

        $('body').on("click", function (event) {
            $('.search-block input').val('');
            $('#search-results').hide();
        });

        initSearch();
    });

    $(document).click(function (event) {
        if (!$(event.target).closest('ul.dropdown-menu.sr').length)
            $('ul.dropdown-menu.sr').hide();
        if (!$(event.target).closest('ul.dropdown-menu.uc').length)
            $('ul.dropdown-menu.uc').hide();
        event.stopPropagation();
    });

    $(".close-select-region").on('click tap', function () {
        $(".region-menu").removeClass('active');
        return false;
    });
    $(".region").on('click tap', function () {
        showReg();
    });

    function showReg() {
        $(".region-menu").addClass('active');
        return false;
    }

    function initSearch() {
        $('.search-block input').focus(function () {
            $('.search-block').search();
        });
    }

    </script>
