        $(document).ready(function () {
            $('.rating .action.active').on('click', function () {
                var obj = $(this);
                $.ajax({
                    type: "post",
                    dataType: 'jsonp',
                    url: "http://order.bycard.by/updateRating/" + obj.attr('comment-id') + "/" + obj.attr('action-id'),
