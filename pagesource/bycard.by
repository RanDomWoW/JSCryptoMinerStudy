            var c = $('#list-bonuses .face').length;
            var flip = function () {
                var isProbablyVisible = function ($face) {
                    return $faceFirst.offset().top + 250 > $face.offset().top;
                };
                var $faces = $('#list-bonuses .face');
                var $faceFirst = $faces.eq(0);
                do {
                    var r = Math.floor(Math.random() * c);
                    var $div = $faces.eq(r);
                } while (!isProbablyVisible($div));
                do {
                    var to = Math.floor(Math.random() * c);
                    var $div2 = $faces.eq(to);
                } while (isProbablyVisible($div2));
                var $divClone = $div.clone();
                var $divClone2 = $div.clone();
                var changePartner = function () {
                    var html = $div2.html();
                    $div.html(html);
                    $divClone.html(html);
                };
                var changePartnerComplete = function () {
                    $divClone.insertAfter($div);
                    $div.remove();
                    $div2.html($divClone2.html());
                };
                $div.rotate3Di(180, 500, {
                    sideChange: changePartner,
                    complete: changePartnerComplete
                });
                return false;
            };

            $(window).load(function () {
                setInterval(flip, 1000);
            });

