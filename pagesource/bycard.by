        function switchLists(show, hide) {
            $('#' + hide).removeClass('active');
            $('#' + show).addClass('active');
            $('.events_list.' + hide).hide();
            $('.events_list.' + show).show();
            return false;
        }
        function switchTop(x) {
            var cur = $('.top-event li.active');
            cur.removeClass('active');
            var curId = cur.find('a').attr('id');
            $('#' + curId + '-container').hide();

            x = $('#' + x.getAttribute('id'));
            x.parent().addClass('active');
            curId = x.attr('id');
            $('#' + curId + '-container').show();
            return false;
        }
    </script>

