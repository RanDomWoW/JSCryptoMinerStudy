         <script type="text/javascript">
		    $(document).on('submit', '#login form', function(){
			    var $form = $(this);
                var service = '';
                if(typeof Order !== 'undefined') {
                    service = Order.service;
                }
			    if($(this).data('ajax-enabled')){
				    $.ajax({
					    url: $(this).attr('action') + '?ajax=1&service='+service,
					    dataType: "jsonp",
					    data: $(this).serialize(),
					    success: function(data){
						    $form.find('input').removeClass('error');
						    if(data.success) {
							    var $newEl = $('<div>' + data.header + '</div>').find('.header6');
							    var $oldEl = $('.header6');

							    $('#login').modal('hide');
							    $oldEl.replaceWith($newEl);

							    if(typeof Order !== 'undefined') {
								    Order.ChangeLogoutReturnUrl();
								    if (window.afterLogin) {
									    Order.Pay.Submit($('.pay-btn'), window.afterLogin == 'reserve');
								    }
							    }
							    delete window.afterLogin;
						    }else{
							    if(typeof Notify !== 'undefined'){
								    for(i in data.info.errorFields){
									    if(data.info.errorFields.hasOwnProperty(i)) {
										    el = data.info.errorFields[i];
										    $form.find('input[name$="[' + el + ']"]').addClass('error');
									    }
								    }
								    for(i in data.info.messages){
									    if(data.info.messages.hasOwnProperty(i)) {
										    msg = data.info.messages[i].message;
										    Notify.Error(msg);
									    }
								    }
							    }
						    }
					    }
				    });

				    return false;
			    }
		    });
