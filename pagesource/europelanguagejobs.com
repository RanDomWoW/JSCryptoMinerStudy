<script type="text/javascript">
    if(typeof GA_jobs !== "undefined")
    {
        ga('require', 'ec');

        GA_jobs.forEach(function(element) {
                ga('ec:addImpression', element);
            });
    }

    if(typeof  GA_job !== "undefined")
    {
        ga('require', 'ec');

        ga('ec:addProduct', GA_job);

        ga('ec:setAction', 'detail');
    }

    if(typeof GA_applied !== "undefined" && GA_applied)
    {
        //the product is already added
        ga('ec:setAction', 'add');
        ga('send', 'event', 'UX', 'click', 'apply');
    }

    ga('send', 'pageview');

    function onJobClick(element)
    {

        GA_job = GA_jobs.find(job => job.id == $(element).data('id'));

        ga('ec:addProduct', GA_job);
        ga('ec:setAction', 'click', {list: GA_job.list});

        ga('send', 'event', 'UX', 'click', 'Results', {
            hitCallback: function() {
                document.location = $(element).prop('href');
            }
        });
    }

    function onApply(id)
    {
       if(typeof  GA_job === "undefined")
       GA_job = GA_jobs.find(job => job.id == id);

       GA_job.price = 1;
       GA_job.quantity = 1;

        ga('ec:addProduct', GA_job);
        ga('ec:setAction', 'add');
        ga('send', 'event', 'UX', 'click', 'apply');
    }

</script>
