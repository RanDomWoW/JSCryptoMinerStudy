	    var getTicketUrl = "https://my.it-enterprise.com/api/Web/GetTicket?lang=uk-UA";
	    var userLogin = "";

	    showWidget(isEnableWSUrl, getTicketUrl, userLogin,
	        function() {
	            setNotificationCountFrameVisible();
	            document.getElementById("notification-count-frame").src = "https://my.it-enterprise.com/wssm/webparts/?ID=NOTIFICATIONSCOUNTANON&Services=SMARTTENDER_ORG_KOM%2cSMARTTENDER_ORG_GOS%2cSMARTTENDER_ORG_ACT%2cSMARTTENDER_PART&Color=255%2c255%2c255&itLanguage=uk&TIMEOUT=900";
	        },
	        function() {
	            getAuthUrl(function(authParam) {
	                setNotificationCountFrameVisible();
	                document.getElementById("notification-count-frame").src = "https://my.it-enterprise.com/wssm/webparts/?ID=NOTIFICATIONSCOUNT&Services=SMARTTENDER_ORG_KOM%2cSMARTTENDER_ORG_GOS%2cSMARTTENDER_ORG_ACT%2cSMARTTENDER_PART&Color=255%2c255%2c255&itLanguage=uk&TIMEOUT=900&" + authParam;
	            });
	        }
	    );

	    function setNotificationCountFrameVisible() {
	        document.getElementById("notification-count-li").style.visibility = "visible";
	    }

	    function checkNeedShowNotificationsList() {
	        if (getTicketUrl) {
	            getAuthUrl(function(url) {
	                getTicket(getTicketUrl, url, function (ret) {
	                    if (ret) {
	                        $.ajax({
	                            type: "POST",
	                            url: webServiceUrl,
	                            data: JSON.stringify({ calcId: "GETNOTIFICATIONSCOUNT", args: JSON.stringify({ Services: ["SMARTTENDER_ORG_KOM", "SMARTTENDER_ORG_GOS", "SMARTTENDER_ORG_ACT", "SMARTTENDER_PART"] }), ticket: ret }),
	                            contentType: "application/json; charset=utf-8",
	                            dataType: "text",
	                            success: function(result) {
	                                if (result > 0) {
	                                    showNotificationsList();
	                                }
	                            }
	                        });
	                    }
	                });
	            });
	        }
	    }

	    function showNotificationsList() {
	        showWidget(isEnableWSUrl, getTicketUrl, userLogin,
	            function() { showNotificationsListWidget('https://my.it-enterprise.com/wssm/webparts/?ID=NOTIFICATIONSANON&Services=SMARTTENDER_ORG_KOM%2cSMARTTENDER_ORG_GOS%2cSMARTTENDER_ORG_ACT%2cSMARTTENDER_PART&itLanguage=uk') },
	            function() { getAuthUrl(function(authParam) { showNotificationsListWidget('https://my.it-enterprise.com/wssm/webparts/?ID=NOTIFICATIONS&Services=SMARTTENDER_ORG_KOM%2cSMARTTENDER_ORG_GOS%2cSMARTTENDER_ORG_ACT%2cSMARTTENDER_PART&itLanguage=uk&' + authParam) }) });
	    }

	    function showNotificationsListWidget(src) {
	        var $iframe = $('<iframe width="100%" height="100%" frameborder="0" marginwidth="0" marginheight="0" allowfullscreen></iframe>');
	        $iframe.attr({
	            src: src,
	            width: 850,
	            height: 500
	        });
	        $("<div></div>").append($iframe).dialog({
	            autoOpen: false,
	            modal: true,
	            resizable: false,
	            width: "auto",
	            height: "auto",
	            autoResize: true,
	            dialogClass: "clickoncloseoutside",
	            close: function() {
	                var frame = document.getElementById("notification-count-frame");
	                frame.parentNode.replaceChild(frame.cloneNode(false), frame);
	            }
	        }).dialog("option", "title", "Сповіщення").dialog("open");

	        return false;
	    }

	    $("body")
	        .on("click", "div.ui-widget-overlay:visible", function() {
	            $(".ui-dialog.clickoncloseoutside:visible").find(".ui-dialog-content").dialog("close");
	        });

	    $("#notification-count-button").click(showNotificationsList);
	</script>
    
    <script type="text/javascript">

        $("#callback-panel").smart_modal({ hide_on_overlay_click: true });
        $("#callback-finish").smart_modal({ hide_on_overlay_click: true });
        $("#callback-error").smart_modal({ hide_on_overlay_click: true });

        $("body").on("click", "#callback-btn", function () {
            $('#callback-panel').smart_modal_show();
            return false;
        });

        function validatePhoneNumber() {
            var value = $(".cb-tl-in")[1].value;
            value = value.replace("+38", "").replace(/\s*/g, "").replace("-", "").replace("(", "").replace(")", "");
            if (value) {
                if (!(/^0\d{9}$/.test(value))) {
                    $($(".cb-tl-in")[1]).css("border", "1px solid red");
                } else {
	                dataLayer.push({
		                'eventCategory': 'Form',
		                'eventAction': 'CallBack',
		                'event': 'UAeventCB'
	                });
                    $.ajax({
                        type: "POST",
                        url: "https://m.it.ua/ws/webservice.asmx/ExecuteEx?pureJson=",
                        data: JSON.stringify({
                            calcId: "ADDCALLBACK",
                            args: JSON.stringify({ PhoneNumber: value, RealPhone: "" }),
                            ticket: ""
                        }),
                        contentType: "application/json; charset=utf-8",
                        dataType: "text",
                        success: function() {
                            $('#callback-panel').smart_modal_hide();
                            $("#callback-finish").smart_modal_show();
                        },
                        error: function() {
                            $('#callback-panel').smart_modal_hide();
                            $("#callback-error").smart_modal_show();
                        }
                    });
                }
            }
            return false;
        }
    </script>
    

