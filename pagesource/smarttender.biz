                        $.ajax({
                            url: "/Services/Common.asmx/ChangeLanguage",
                            type: "POST",
                            dataType: "json",
                            contentType: "application/json",
                            data: JSON.stringify({ lang: selectedValue, path: window.location.pathname + window.location.search }),
                            success: function (data) {
                            	if (data.d.Success) {
                            		var link = $("link[rel=alternate][hreflang=" + selectedValue + "]");
