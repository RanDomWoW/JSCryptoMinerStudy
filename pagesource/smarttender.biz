                });
                if ($("input.redirect-to-url").val() != "") {
                    $('.open-login-popup').smart_modal_show();
                }
                $("body").on("click", "a.need-login", function() {
                    if (!loggedIn) {
                        var ref = $(this).attr('href');
                        $("input.redirect-to-url").val(ref);
                        $('.open-login-popup').smart_modal_show();
                        return false;
                    }
                });

