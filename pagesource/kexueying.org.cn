    <script type="text/javascript">
        $("document").ready(function () {
            //导航样式
            $("ul.sc-list li").removeClass("sc-cur").eq(0).addClass("sc-cur");

            //插入顶部flash
            //$("#topNavFlash").flashembed({ src: "swf/topNav.swf", wmode: "transparent", bgcolor: "#ffffff", menu: "false", width: 720, height: 120 });
            //$("#indexFocusFlash").flashembed({ src: "swf/imagefocus.swf", wmode: "transparent", bgcolor: "#ffffff", menu: "false", width: "970", height: 347 });
            //$("#animateBG").flashembed({ src: "swf/animateBG.swf", wmode: "transparent", bgcolor: "#ffffff", menu: "false", width: "100%", height: 120 });
            //indexTab();
            //从这里开始是新加的js
            var scrollTimer = null;
            $(".rollList").hover(
    		function () {
    		    clearInterval(scrollTimer);
    		}, function () {
    		    scrollTimer = setInterval(function () { scrollNews(); }, 3000);
    		}).triggerHandler('mouseout');

            function scrollNews() {

                $(".rollList").animate({ 'marginTop': -26 + 'px' }, 600, function () { $(".rollList").css({ margin: 0 }).find('li:first').appendTo($(".rollList")); });
            }
            function imgTab() {
                var oTab = $(".bannerLeft");
                var oDiv = $(".big");
                var oA = oDiv.find("a");
                var oUl = $(".little");
                var aA = oUl.find("a");
                var timer = null;
                var iNow = 0;

                for (var i = 0; i < aA.length; i++) {
                    aA[i].index = i;
                    aA[i].onmouseover = function () {
                        iNow = this.index;
                        show();
                    }
                }
                function show() {

                    for (var i = 0; i < aA.length; i++) {
                        aA[i].className = '';
                        startMove(oA[i], { 'opacity': 0 }, function () {
                            this.style.display = "none";
                        });
                    }
                    aA[iNow].className = 'active';
                    oA[iNow].style.display = "block";
                    startMove(oA[iNow], { 'opacity': 100 });
                }
                timer = setInterval(autoPlay, 3000);
                function autoPlay() {

                    if (iNow == aA.length - 1) {
                        iNow = 0;
                    } else {
                        iNow++;
                    }
                    show();
                }
                oTab.mouseover(function () {
                    clearInterval(timer);
                })
                oTab.mouseout(function () {
                    clearInterval(timer);
                    timer = setInterval(autoPlay, 3000);
                })
                function startMove(obj, json, fnEnd) {
                    clearInterval(obj.timer);
                    obj.timer = setInterval(function () {
                        doMove(obj, json, fnEnd);
                    }, 30);
                }
                function doMove(obj, json, fnEnd) {
                    var iCur = 0;
                    var attr = null;
                    var bStop = true;
                    for (attr in json) {
                        if (attr == 'opacity') {
                            iCur = parseInt(100 * getStyle(obj, attr)) || 0;
                        }
                        else {
                            iCur = parseInt(getStyle(obj, attr)) || 0;
                        }
                        var iSpeed = (json[attr] - iCur) / 8;
                        iSpeed = (iSpeed > 0) ? Math.ceil(iSpeed) : Math.floor(iSpeed);

                        if (iCur != json[attr]) {
                            bStop = false;
                        }

                        if (attr == 'opacity') {
                            obj.style.filter = 'alpha(opacity=' + (iCur + iSpeed) + ')';
                            obj.style.opacity = (iCur + iSpeed) / 100;
                        }
                        else {
                            obj.style[attr] = iCur + iSpeed + 'px';
                        }

                    }
                    if (bStop) {
                        clearInterval(obj.timer);
                        if (fnEnd) {
                            fnEnd.call(obj);
                        }
                    }
                }
                function getStyle(obj, attr) {
                    if (obj.currentStyle) {
                        return obj.currentStyle[attr];
                    }
                    else {
                        return getComputedStyle(obj)[attr];
                    }
                }
            }

            imgTab();
        });
    </script>
