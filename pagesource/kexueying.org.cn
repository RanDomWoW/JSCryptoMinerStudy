        function indexTab() {
            var iNow = 0;
            var timer = null;

            function showHid() {
                if (iNow >= 3) {
                    iNow = 0;
                } else {
                    iNow++;
                }
                $(".campIndexTab li").removeClass("show");
                $(".campIndexTab li").eq(iNow).addClass("show")
            }

            timer = setInterval(showHid, 2000);

            $(".campIndexTab").mouseover(function () {
                clearInterval(timer);
            })
            $(".campIndexTab").mouseout(function () {
                clearInterval(timer);
                timer = setInterval(showHid, 2000);
            })
        }
