		jQuery.getJSON("http://www.newsbomb.al/app/cache/modules/yahoo-weather/cities/" + name + ".json", {}, function(jsonData) {
			var data = jsonData['query']['results']['channel'];
			
			jQuery("#cur-weather-image").attr("src", "app/assets/img/yahoo-weather/"+ data['item']["condition"]["code"] +"d.png");
			jQuery(".cur-temp-nr").text(data['item']["condition"]["temp"]);
			jQuery("#max-temp-nr").text(data['item']["forecast"][0]["high"]);
			jQuery("#min-temp-nr").text(data['item']["forecast"][0]["low"]);
			jQuery(".sunrise").text(data['astronomy']['sunrise']);
			jQuery(".sunset").text(data['astronomy']['sunset']);
		});
	}
	jQuery( "#weatherListCity" ).change(function() {
		showWeatherForCityWithName(jQuery( this ).val());
		console.log(jQuery( this ).val());
	});
	showWeatherForCityWithName("tirane");
	
