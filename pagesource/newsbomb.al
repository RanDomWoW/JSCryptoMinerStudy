<script type="text/javascript">
	function checkPollVoted(pollID){
		if(getCookie("pollWithID"+pollID) != ""){//poll voted
			var alternativeVoted = getCookie("pollWithID"+pollID);
			showPollResults(pollID);
			jQuery("#poll-"+ pollID +" .vote-poll").hide();
			jQuery("#poll-"+ pollID +" .poll-alternatives li input").prop('disabled', true);
			jQuery("#poll-"+ pollID +" .poll-alternatives li:eq("+ alternativeVoted +")").addClass("voted");
			jQuery("#poll-"+ pollID +" .poll-alternatives li input:eq("+ alternativeVoted +")").prop('checked', true);
		}else{
			jQuery("#poll-"+ pollID +" .vote-poll").show();
			jQuery("#poll-"+ pollID +" .show-poll-results").show();
		}
	}
	function showPollResults(pollID){
		jQuery("#poll-"+ pollID +" .poll-alternatives span").show();
		jQuery("#poll-"+ pollID +" .show-poll-results").hide();
	}
	function updatePollResults(pollID){
		jQuery.getJSON("http://www.newsbomb.al/app/cache/modules/poll/"+pollID+".json", {}, function(jsonData) {
			jQuery("#poll-"+pollID+" .poll-alternatives span").each(function( index ) {
				$(this).text(jsonData[index].votes + " Vota");
			});
		});
	}
	jQuery("body").on("click", ".vote-poll", function(event) {
		var selectedPollID 		= $(this).attr("data-id");
		var radioButtons 		= jQuery("#poll-" + selectedPollID + " .poll-alternatives input:radio[name='poll-alternatives-" + selectedPollID + "']");
		var selectedIndex 		= radioButtons.index(radioButtons.filter(':checked'));
		jQuery.ajax({
			type		: "POST",
			url			: "http://www.newsbomb.al/app/api.php",
			dataType	: "json",
			data		: "action=storePollVote&pollID=" + selectedPollID + "&pollAlternativeIndex=" + selectedIndex,
			success		: function(respose) {
				console.log(respose);
				document.cookie = "pollWithID" + selectedPollID + "=" + selectedIndex;
				updatePollResults(selectedPollID);
				showPollResults(selectedPollID);
				jQuery("#poll-" + selectedPollID + " .vote-poll").hide();
				jQuery("#poll-" + selectedPollID + " .poll-alternatives li input").prop("disabled", true);
				checkPollVoted(selectedPollID);
			}
		});
	});
</script>
