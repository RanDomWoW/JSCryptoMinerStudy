<script type="text/javascript">
	function getCookie(cname) {
		var name			= cname + "=";
		var decodedCookie	= decodeURIComponent(document.cookie);
		var ca				= decodedCookie.split(';');
		for(var i = 0; i < ca.length; i++) {
			var c			= ca[i];
			while (c.charAt(0) == ' ') {
				c			= c.substring(1);
			}
			if (c.indexOf(name) == 0) {
				return c.substring(name.length, c.length);
			}
		}
		return "";
	}
	function checkVotingStatus() {
		jQuery.ajaxSetup({cache:false});
		jQuery.getJSON("http://www.newsbomb.al/app/cache/modules/barometer.json", {}, function(jsonData) {
			jQuery("#barometer > .barometer-item > .voting-container img.voting-img").each(function() {
				jQuery(this).data("id", "");
			});
			jQuery(".barometer-item .votes").text(function(index) {
				return jsonData[index].votes;
			});
			jQuery(".barometer-item .barometer-item-title").html(function( index ) {
			  return '<a href="http://www.newsbomb.al/' + jsonData[index].alias + '-' + jsonData[index].articleID + '">' + jsonData[index].title + '</a>';
			});
			jQuery(".barometer-item .main-img").each(function( index ) {
				jQuery(this).attr("src", jsonData[index].img_src)
			});
			jQuery(".barometer-item .voting-img").each(function( index ) {
				jQuery(this).attr("data-id", jsonData[index].articleID)
			});
			if (/(^|;)\s*barometerVotedID=/.test(document.cookie)) {
				var barometerVoted		= getCookie("barometerVotedID");
				jQuery("#barometer > .barometer-item > .voting-container img.voting-img").each(function() {
					var selectedItem	= $(this).attr("data-id");
					if (barometerVoted == selectedItem) {
						jQuery(this).attr("title", "Ju keni votuar këtë titull!");
						jQuery(this).attr("src", "app/assets/img/barometer/voted_article.png");
						jQuery(this).removeClass("trigger-voting");
					} else {
						jQuery(this).attr("src", "app/assets/img/barometer/vote_disabled.png");
						jQuery(this).attr("title", "Ju keni votuar më parë!");
						jQuery(this).removeClass("trigger-voting");
					}
				});
			}
		});
	}
	checkVotingStatus();
</script>
