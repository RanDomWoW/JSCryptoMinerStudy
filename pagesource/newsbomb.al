		var selectedCityName			= jQuery("#weatherListCity option:selected").text();
		jQuery("#cityName").html("Parashikimi javor për qytetin " + selectedCityName);
		var selectedCity 				= jQuery("#weatherListCity option:selected").val();
		jQuery.getJSON("http://www.newsbomb.al/app/cache/modules/yahoo-weather/cities/" + selectedCity + ".json", {}, function(jsonData) {
			var data = jsonData['query']['results']['channel']['item']["forecast"];
