	if (browser_url == '') {
		browser_url = document.URL;
	}
	var lastChar = browser_url.substring(browser_url.length-1);
	if (lastChar == "#") {
	   browser_url	= browser_url.slice(0, browser_url.length-1);
	}
	var navigation	= document.getElementById("main-menu");
	var menus		= navigation.getElementsByTagName("a");
	for (i = 0; i < menus.length; i++) {
		if (menus[i].href == browser_url) {
			menus[i].className = 'active';
		}
	}
