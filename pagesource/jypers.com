<script type="text/javascript">
(function ($) {
$(document).ready(function(){
	var reMove = $('.bxslider').bxSlider({
	auto: true,
	mode: 'fade',
	speed: 300,
	pause: 5000,
	pager: true,
	controls: false,
	// 初期スライドの画像ページャーにactiveクラス付加
	onSliderLoad:function(current){
		$('#bxThumb a').eq(current).addClass('active');
	},
	// スライド移動時にactiveクラスの付加
	onSlideBefore: function($slideElement, oldIndex, newIndex){
		$('#bxThumb a').eq(oldIndex).removeClass('active');
		$('#bxThumb a').eq(newIndex).addClass('active');
	},
	// スライド移動時に自動スライドを再開
	onSlideAfter:function(){
		reMove.startAuto();
	}
	});
	
	// 画像ページャーをクリックしたときの処理
	$('#bxThumb a').click(function(){
	var target = $('#bxThumb a').index(this);
	$('.bx-pager-item a').eq(target).click();
	return false;
	});
	
	
	// 絞り込みカテゴリカスタマイズ
	var categories = $('#shiborikomi_category ul li');
	for(var i=0;i<categories.length;i++){
		var href = categories.eq(i).find('a').attr('href');
		var name = categories.eq(i).find('a').text();
		var option = $('<option></option>').val(href).text(name);
		if(location.origin + location.pathname == href){
			option.prop('selected',true);
		}
		$('#shiborikomi_select').append(option);
	}
	
});
})(jQuery);

