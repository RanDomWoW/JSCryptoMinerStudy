<script>
    function send_fr()
    {
        var formData = new FormData($('#formdata')[0]);
        $('#bt_fr').val('Отправка...');
        $('#bt_fr').prop("disabled", true);

        $.ajax({
            url: "/Content/Test/Send_fr",
            cache: false,
            type: "POST",
            dataType: 'json',
            processData: false,
            contentType: false,
            data: formData,
            success: function(data,status) {
                if(data.status!="error")
                {
                    $("#pd_news").removeAttr('href');
                    $('#form-news-share').hide();
                    $('#form-news-share-ok').show();
                    setTimeout(function(){
                        $('#form-news-share-ok, .overlay').hide();
                    },4000);
                }
                else
                {
                    $('#bt_fr').val('Отправить');
                    $('#bt_fr').prop("disabled", false);
                    alert(data.text_error);
                }

            }
        });
    }

    $('.kam24').click(function() {
        var checked = document.getElementById('kam24_my_ques').checked;
        if(checked){
            document.getElementById('ques_text').style.display = 'block';
        }
        else{
            document.getElementById('ques_text').style.display = 'none';
        }
    });
    function SendVote(id,num)
    {
        var value=0;
        var res='';
        my_ques =  document.getElementById('kam24_my_ques');
        for(var i=0; i<num;i++)
        {
            elem = document.getElementById('kam24_'+i);
            value = elem.value;
            if(elem.checked)
            {
                res = res+(i+1)+',';
            }
        }
        if(my_ques && my_ques.checked)
        {
            var text = document.getElementById('kam24_my_ques_text').value;
            if(!text)
            {
                alert('Поле не заполнено!');
            }
            else
            {
                if($.post("https://kam24.ru/Content/Vote/vote_ques", { text: text, id: id } ))
                {
                    var SummDok = document.getElementById('ret');
                    $.get('https://kam24.ru/Content/Vote/resvote', {id:id}, function(data) {
                        SummDok.innerHTML = data;
                    });
                    alert('Спасибо, ваш голос учтен!')
                }
            }
        }
        else if(res)
        {
            var SummDok = document.getElementById('ret');
            $.get('https://kam24.ru/Content/Vote/vote', {id:id,val:res}, function(data) {
                SummDok.innerHTML = data;
            });
            alert('Спасибо, ваш голос учтен!')
        }
        else
        {
            alert('Ничего не выбрано!');
        }
    }
    function ResVote(id)
    {
        var SummDok = document.getElementById('ret');
        $.get('https://kam24.ru/Content/Vote/resvote', {id:id}, function(data) {
            SummDok.innerHTML = data;
        });

    }
</script>
