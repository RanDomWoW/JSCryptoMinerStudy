<script type="text/javascript">
$(document).ready(function(){
  $("#login_ajax_form input[name='user[mobile]']").intlTelInput({
    initialCountry: "tw",
    autoFormat: true,
    autoPlaceholder: false,
    nationalMode: true,
    preferredCountries: ['tw'],
    utilsScript: "/js/libphonenumber/utils.js",
    geoIpLookup: function(callback) {
      $.get('http://ipinfo.io', function() {}, "jsonp").always(function(resp) {
        var countryCode = (resp && resp.country) ? resp.country : "";
        callback(countryCode);
      });
    },
  });
});

$('#login_ajax_form').submit(function(e){
  e.preventDefault();
  $("#login_ajax_form input[name='user[mobile]']").val($("#login_ajax_form input[name='user[mobile]']").intlTelInput('getNumber'));
  var appierRtUserEmail = $("#login_ajax_form input[name='user[mobile]']").val();
  // $("#new_user").submit();
  $.ajax({
    url: '/users/sign_in',
    method: 'POST',
    dataType: 'json',
    data: $("#login_ajax_form").serialize(),
  })
  .done(function(data, textStatus, jqXHR) {
    if (typeof(window.APPIER_RETARGET)  == 'undefined') {
      (function(w, d, s, m) {
        var f = d.getElementsByTagName('script')[0],
        j = d.createElement('script'),
        ns = 'APPIER_RETARGET';
        w._appierSendQueue = w._appierSendQueue || [];
        w['appierRetargetJson'] = { id: s, site: m};
        j.async = true;
        j.src = '//jscdn.appier.net/aa.js?id='+m;
        f.parentNode.insertBefore(j, f);
        !w[ns] && (w[ns] = {});
        (!w[ns].send) && (w[ns].send = function(j){
          w._appierSendQueue.push(j);
        });
      })(window, document, "S0wM", "neogence.com.tw");
    }
    window.APPIER_RETARGET.send({"userEmail": appierRtUserEmail});
    window.location = data.url;
  })
  .fail(function(data, textStatus, jqXHR) {
    pNotify('錯誤', data.responseJSON.error);
  });
});

