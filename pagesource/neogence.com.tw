<script type="text/javascript">
Vue.component('side-container-component', {
  template: '#side-container-template',
  props: ['order'],
  data: function() {
    return {
      gap: 0,
      shipment_gap: 0,
    }
  },
  created: function() {
    this.fetchShipmentGap();
  },
  computed: {
    order_item_count: function() {
      return ((ref = this.order) != null ? ref.item_count : 0);
    },
    order_total: function() {
      return ((ref = this.order) != null ? ref.total : 0);
    }
  },
  methods: {
    fetchShipmentGap: function() {
      var self = this;
      if (this.order == null) {
        this.shipment_gap = 0;
        return ;
      }
      $.ajax({
        url: '/carts/calculators/shipment',
        method: 'GET',
        dataType: 'json',
      })
      .done(function(data, textStatus, jqXHR) {
        self.gap = data;
        if (self.gap - ((ref = self.order) != null ? ref.subtotal : 0) > 0 && self.order_item_count > 0) {
          self.shipment_gap = self.gap - ((ref = self.order) != null ? ref.subtotal : 0);
        }
        else {
          self.shipment_gap = 0;
        }
      });
    }
  },
  watch: {
    'order.subtotal': function(val, oldVal) {
      this.fetchShipmentGap();
    },
  }
});
</script>

      
