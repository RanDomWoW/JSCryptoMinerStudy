Vue.component('cart-component', {
  template: '#cart-template',
  props: ['order'],
  ready: function () {
  },
  computed: {
    isGiftItems: function() {
      if(_.find(this.order.line_items, {line_item_type: 'Variant::Gift'})) {
        return true;
      }
      else {
        return false;
      }
    },
    isRedGreenItems: function() {
      if(_.find(this.order.line_items, {line_item_type: 'Variant::RedLabel'}) || _.find(this.order.line_items, {line_item_type: 'Variant::GreenLabel'})) {
        return true;
      }
      else {
        return false;
      }
    },
    isAddBuyItems: function() {
      if(_.find(this.order.line_items, {line_item_type: 'Variant::AddBuy'})) {
        return true;
      }
      else {
        return false;
      }
    },
    isOptionalItems: function() {
      if(_.find(this.order.line_items, {line_item_type: 'Variant::Optional'})) {
        return true;
      }
      else {
        return false;
      }
    },
    isGeneralItems: function() {
      if(_.find(this.order.line_items, {line_item_type: 'Variant::General'})) {
        return true;
      }
      else {
        return false;
      }
    },
    allGiftItems: function() {
      return _.filter(this.order.line_items, {line_item_type: 'Variant::Gift'});
    },
    allRedGreenItems: function() {
      return _.concat(this.allRedItems, this.allGreenItems);
    },
    allRedItems: function() {
      return _.filter(this.order.line_items, {line_item_type: 'Variant::RedLabel'});
    },
    allGreenItems: function() {
      return _.filter(this.order.line_items, {line_item_type: 'Variant::GreenLabel'});
    },
    allAddBuyItems: function() {
      return _.filter(this.order.line_items, {line_item_type: 'Variant::AddBuy'});
    },
    allOptionalItems: function() {
      return _.filter(this.order.line_items, {line_item_type: 'Variant::Optional'});
    },
    allGeneralItems: function() {
      return _.filter(this.order.line_items, {line_item_type: 'Variant::General'});
    },
  },
  methods: {
  },
});
</script>


