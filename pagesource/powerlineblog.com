<script type='text/javascript'>
var PLVIP = {};
PLVIP.trigger = document.getElementById('tpm-trigger');

PLVIP.ADList = [
	'div-gpt-ad-1367363740891-0',
	'div-gpt-ad-1367363740891-1',
	'div-gpt-ad-1367363740891-2',
	'div-gpt-ad-1367363740891-3',
	'div-gpt-ad-1367363740891-4',
	'div-gpt-ad-1367363740891-5',
	'div-gpt-ad-1367363740891-6',
	'div-gpt-ad-1367363740891-7',
	'vip-banner',
	'parsely-root'
];

PLVIP.cookies = {
  getItem: function (sKey) {
    return decodeURIComponent(document.cookie.replace(new RegExp("(?:(?:^|.*;)\\s*" + encodeURIComponent(sKey).replace(/[\-\.\+\*]/g, "\\$&") + "\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1")) || null;
  },
  setItem: function (sKey, sValue, vEnd, sPath, sDomain, bSecure) {
    if (!sKey || /^(?:expires|max\-age|path|domain|secure)$/i.test(sKey)) { return false; }
    var sExpires = "";
    if (vEnd) {
      switch (vEnd.constructor) {
        case Number:
          sExpires = vEnd === Infinity ? "; expires=Fri, 31 Dec 9999 23:59:59 GMT" : "; max-age=" + vEnd;
          break;
        case String:
          sExpires = "; expires=" + vEnd;
          break;
        case Date:
          sExpires = "; expires=" + vEnd.toUTCString();
          break;
      }
    }
    document.cookie = encodeURIComponent(sKey) + "=" + encodeURIComponent(sValue) + sExpires + (sDomain ? "; domain=" + sDomain : "") + (sPath ? "; path=" + sPath : "") + (bSecure ? "; secure" : "");
    return true;
  },
  removeItem: function (sKey, sPath, sDomain) {
    if (!sKey || !this.hasItem(sKey)) { return false; }
    document.cookie = encodeURIComponent(sKey) + "=; expires=Thu, 01 Jan 1970 00:00:00 GMT" + ( sDomain ? "; domain=" + sDomain : "") + ( sPath ? "; path=" + sPath : "");
    return true;
  },
  hasItem: function (sKey) {
    return (new RegExp("(?:^|;\\s*)" + encodeURIComponent(sKey).replace(/[\-\.\+\*]/g, "\\$&") + "\\s*\\=")).test(document.cookie);
  },
  keys: /* optional method: you can safely remove it! */ function () {
    var aKeys = document.cookie.replace(/((?:^|\s*;)[^\=]+)(?=;|$)|^\s*|\s*(?:\=[^;]*)?(?:\1|$)/g, "").split(/\s*(?:\=[^;]*)?;\s*/);
    for (var nIdx = 0; nIdx < aKeys.length; nIdx++) { aKeys[nIdx] = decodeURIComponent(aKeys[nIdx]); }
    return aKeys;
  }
};

PLVIP.advertize = function () {

	if ( document.readyState == 'interactive' || document.readyState == 'complete' ) {
	
		for ( var i = 0; i < PLVIP.ADList.length; i++ ) {
			if ( /div-gpt-ad/.test(PLVIP.ADList[i]) ) {
				if ( document.getElementById(PLVIP.ADList[i]) ) {
					googletag.cmd.push(function() { googletag.display( PLVIP.ADList[i] ); });
				}
			}
		}

		// start the HTML5 VIP banner
		if ( document.getElementById('vip-banner') ) {
			startVIPBanner();
		}

	} else {
		document.addEventListener('DOMContentLoaded', PLVIP.advertize, false);
	}

};

PLVIP.removeAD = function () {

	if ( document.readyState == 'interactive' || document.readyState == 'complete' ) {
	
		for ( var i = 0; i < PLVIP.ADList.length; i++ ) {
			var el = document.getElementById(PLVIP.ADList[i]);

			if ( el ) {
				el.parentNode.removeChild(el);
			}
		}
		
	} else {
		document.addEventListener('DOMContentLoaded', PLVIP.removeAD, false);
	}

};

PLVIP.showAD = function () {

	if ( document.readyState == 'interactive' || document.readyState == 'complete' ) {
	
		for ( var i = 0; i < PLVIP.ADList.length; i++ ) {
			var el = document.getElementById(PLVIP.ADList[i]);

			if ( el ) {
				el.style.left = '';
				el.style.position = '';
			}
		}
		
		// footer banner
		var elem = document.createElement('script');
		elem.src = 'http://ads.saymedia.com/ai017ee3d689cb970d017c3232ec4a970b.js?ord='+ Math.random();
		elem.async = true;
		elem.type = "text/javascript";
		document.body.appendChild(elem);

	} else {
		document.addEventListener('DOMContentLoaded', PLVIP.showAD, false);
	}

};

PLVIP.waitForIt = function () {
	if ( /(^|\s)tpmHasAccess(\s|$)/.test(PLVIP.trigger.className) ) {
		console.log('PowerLine: Welcome VIP');

		PLVIP.cookies.setItem('PLVIP', 'y', 60*60*3, '/');
		PLVIP.isVIP = true;
		PLVIP.removeAD();
		return;
	}
	
	if ( /(^|\s)(tpmMetered|tpmEpired)(\s|$)/.test(PLVIP.trigger.className) ) {
		console.log('PowerLine: Add banners');

		PLVIP.cookies.removeItem('PLVIP', 'n', 60*3, '/');
		PLVIP.isVIP = false;
		PLVIP.advertize();
		PLVIP.showAD();
		return;
	}
	
	setTimeout(PLVIP.waitForIt, 20);	
};

PLVIP.isVIP = (function () {
	var c = PLVIP.cookies.getItem('PLVIP');
	
	if ( c == 'y' ) {
		console.log('PowerLine: VIP from Session');

		PLVIP.removeAD();
		return true;
	}

	if ( c == 'n' ) {
		console.log('PowerLine: NO VIP from Session');

		PLVIP.advertize();
		PLVIP.showAD();
		return false;
	}

	console.log('PowerLine: Wait for TinyPass');

	setTimeout(PLVIP.waitForIt, 20);
	
	return undefined;
})();

</script>

