<script>
jQuery( document ).ready(function() {
    /* Arruma Side Menu */
    jQuery(".side_menu_button i").removeClass().addClass('fa-cube').show();
    jQuery(".side_menu_button a").addClass('some');
    jQuery('.side_menu .menu ul').hide();
    jQuery('.side_menu .menu ul:first').addClass('sidemenu_open').slideToggle();

    jQuery('.side_menu .menu > li').each(function() {
        jQuery(this).find('a:first').replaceWith('<p style="font-size: 13px; font-weight:bold; padding:0px; cursor: pointer;">' + jQuery(this).find('a:first').html() +'</p>');
    });
    
    jQuery('.side_menu .menu > li').click(function() {
        if(!jQuery(this).find('ul').hasClass('sidemenu_open')) { 
            jQuery('.side_menu .menu ul').slideUp();
            jQuery(this).find('ul').slideToggle();
            jQuery('.side_menu .menu ul').removeClass('sidemenu_open');
            jQuery(this).find('ul').toggleClass("sidemenu_open");
        };
    });
    

    
    /* Esse class esta publicando o link do menu 
    jQuery('.main_menu a').click(function() {
        window.location.href = jQuery(this).attr('href');
    });
    */
    
    /* Arruma Menu superior  */
    jQuery('.header_top .left').remove();
    jQuery(".main_menu li[class^='posicao']").each(function(){
        var distancia = -24;
        var posicao = jQuery(this).attr('class').split(' ')[0].split("-").pop();
        var posicao_final = ((posicao * distancia) + (-7) );
        jQuery(this).closest("ul").css('top', posicao_final);
    });
});



</script>

