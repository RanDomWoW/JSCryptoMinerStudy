function isEmail(emailAddress) {
    emailAddressValue = emailAddress.value;
    var emailAddressRegex = /^[^@\s]+@[^\.@\s]+(\.[^@\s^\.]+)+$/;
    if (emailAddress.value == '' || emailAddress == null) {
        return true;
    }
    if (!emailAddressRegex.test(emailAddressValue)) {
        alert('The email address doesn\'t seem to be correct,\nplease ensure it is entered correctly.');
        emailAddress.focus();
        return false;
    }
    return true;
}
String.prototype.trim = function() {
    return this.replace(/^\s*(\b.*\b|)\s*$/, "$1");
}

function mandatoryText(input, fieldName) {
    if (input.value.trim() == '' || input == null) {
        alert('Please enter your ' + fieldName + '.');
        input.focus();
        return false;
    } else {
        return true;
    }
}

function validForm() {
    if (!mandatoryText(document.getElementById('EMAIL_FIELD'), 'your email')) return;
    if (!isEmail(document.getElementById('EMAIL_FIELD'))) return;
    document.getElementById('emvForm').submit();
}</script>
