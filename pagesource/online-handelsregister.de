<script>
    $.ajaxSetup({
        headers: {
            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
        }
    });

    /*Live search begin*/
    var timer;
    function up() {
        timer = setTimeout(function () {
            var keywords = $("#search-input").val();

            if (keywords.length > 0) {
                $.post("/livesearch", {keywords: keywords}, function (markup) {
                    $(".searchPanelBody").show();
                    $('#search-input').removeClass('loading-icn');
                    $("#search-result").fadeIn("fast");
                    $("#search-result").html(markup);
                });
            }
            if (keywords.length == 0) {
                $(".searchPanelBody").hide();
                $('#search-input').removeClass('loading-icn');
                $("#search-result").fadeOut("fast");
            }
        }, 500);
    }

    function down() {
        $('#search-input').addClass('loading-icn');
        clearTimeout(timer);
    }
    /*Live search end*/

    //Toggle search results when clicking inside input holds previous keyword
    $('#search-input').click(function () {
        if ($("#search-result").html() != '' && $("#res-container").css('display') == 'none') {
            $('#res-container').show();
        }
    });
    //Toggle search container display on page click
    $(document).mouseup(function (e) {
        var container = $("#res-container");
        // if the target of the click isn't the container...nor a descendant of the container
        if (!container.is(e.target) && container.has(e.target).length === 0) {
            container.hide();
        }
    });

</script>

<script>
    $(document).ready(function () {
        $("body").tooltip({selector: '[data-toggle=tooltip]'});

        $("[name='my-checkbox']").bootstrapSwitch();


    });

    //shopping basket functionality
    $(document).ready(function () {

        //Display shopping basket popover
        $('li.basketMenu a').on('click', function (event) {
            $(this).parent().toggleClass("open");
        });

        $('body').on('click', function (e) {
            if (!$('li.basketMenu').is(e.target) && $('li.basketMenu').has(e.target).length === 0 && $('.open').has(e.target).length === 0) {
                $('li.basketMenu').removeClass('open');
            }
        });

        //Clear shopping basket
        $(document).on('click', '#clearCartBtn', function () {
            var container = $('#cartActions');
            var errordiv = $('#cartMsg');
            $.post("/cart/destroy", {}, function (data) {
                if (errordiv.length) {
                    errordiv.remove();
                }
                if (data.resp == "done") {
                    location.href = "https://www.online-handelsregister.de";
                }
                else {
                    $('<div id="cartMsg" class="text-danger">' + data.resp + '</div>').insertAfter(container);
                    $('div#cartMsg').delay(3000).slideUp(300);
                }
            }, "json");
        });

        //Delete an item in shopping basket and
        // instantly refresh contents of basket and checkout  order summary
        $(document).on('click', '.delCartItem', function () {
            var itemclick = $(this);
            var inputp = itemclick.data('param');
            var container = $('#cartActions');
            var errordiv = $('#cartMsg');
            var decision = itemclick.data('decision');
            $.post("/cart/item/delete", {item: inputp, utype: decision}, function (data) {
                if (errordiv.length) {
                    errordiv.remove();
                }
                if (data.resp == "done") {
                    var suffix = 'Dokument';
                    if (data.itemcount == 0 || data.itemcount > 1) {
                        suffix = 'Dokumente';
                    }
                    $('#basketContents').html(data.cart);
                    $('#basketCounter').html(data.itemcount + ' ' + suffix);
                    if ($('#orderSummary').length)
                        $('#orderSummary').html(data.orderSummary);
                    if ($('#hiddenLst').length)
                        $('#hiddenLst').html(data.hiddenList);

                    if (data.itemcount == '0') {
                        if ($('#submit').length)
                            $('#submit').attr('disabled', true).addClass('disabled');
                        setTimeout(function () {
                            location.reload();
                        }, 2000);
                    }
                }
                else {
                    $('<div id="cartMsg" class="text-danger text-center">' + data.resp + '</div>').insertAfter(container);
                    $('div#cartMsg').delay(3000).slideUp(300);
                }
            }, "json");

        });

        //Deselect subscription plan from basket and reload page
        $(document).on('click', '.cart-deselect-subscription', function () {
            var container = $('#cartActions');
            var errordiv = $('#cartMsg');
            $.post("/cart/subscription/deselect", function (data) {
                if (errordiv.length) {
                    errordiv.remove();
                }
                if (data.resp == "done") {
                    location.reload();
                }
                else {
                    $('<div id="cartMsg" class="text-danger text-center">' + data.resp + '</div>').insertAfter(container);
                    $('div#cartMsg').delay(3000).slideUp(300);
                }
            }, "json");

        });

        //csrf token for AJAX Request
        var token = "mCjcmkmPTC2BGrvWheCOxBfHzE4g59J5H9mvPkUo";
        jQuery.ajaxSetup({
            headers: {'X-CSRF-Token': token}
        });

    });

    $(document).ready(function () {
        $(document).on('click', "a.showform", function () {
            var filenamo = 'YXNzZXRzL2pzL2NvbXAudHh0';
            $("#compdata").html("");
            $("#compdata").load(atob(filenamo));
            return false;
        });
    });

    /*Live search begin*/
    var timer;
    function searchup() {
        timer = setTimeout(function () {
            var keywords = $("#search-inputs").val();

            if (keywords.length > 0) {
                $.post("/navsearch", {keywords: keywords}, function (markup) {
                    $(".searchPanelBodys").show();
                    $('#search-inputs').removeClass('loading-icn');
                    $("#search-results").fadeIn("fast");
                    $("#search-results").html(markup);
                });
            }
            if (keywords.length == 0) {
                $(".searchPanelBodys").hide();
                $('#search-inputs').removeClass('loading-icn');
                $("#search-results").html('').fadeOut("fast");
            }
        }, 500);
    }

    function searchdown() {
        $('#search-inputs').addClass('loading-icn');
        clearTimeout(timer);
    }
    /*Live search end*/

    //Toggle search results when clicking inside input holds previous keyword
    $('#search-inputs').click(function () {
        if ($("#search-results").html() != '' && $(".searchPanelBodys").css('display') == 'none') {
            $('.searchPanelBodys').show();
        }
    });

    //Toggle search container display on page click
    $(document).mouseup(function (e) {
        var container = $(".searchPanelBodys");
        // if the target of the click isn't the container...nor a descendant of the container
        if (!container.is(e.target) && container.has(e.target).length === 0) {
            container.hide();
        }
    });
</script>
