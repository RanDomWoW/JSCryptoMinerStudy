        function getproduct() {
            var newcat = "";
            var oldcat = "";
            var a = "";

            var url = "";
            var item = "";
            var html = "";
            var div = "";
            var Param = "__RequestVerificationToken=" + $('input[name="__RequestVerificationToken"]').val();
            $.ajax({
                type: "POST",
                dataType: 'json',
                //contentType: 'application/json',
                beforeSend: function (xhr) {
                    xhr.setRequestHeader("Content-Encoding", "gzip");
                },
                url: "/Ahmedabad/Ajax-YP/GetHomeCategories",
                data: Param,
                success: function (data) {
                    var htmlstr = "";
                    jsonObj = jQuery.parseJSON(data);
                    for (var i = 0; i < jsonObj.cats.length; i++) {

                        url = "";
                        item = "";

                        url = "/Ahmedabad/" + jsonObj.cats[i].caturl + "/" + jsonObj.cats[i].catid + "?searchtext=" + jsonObj.cats[i].product.replace("&", "^") + "&FApplied=2:" + jsonObj.cats[i].prodid;
                        item = '<li><a href="' + url + '">' + titleCase(jsonObj.cats[i].disprod) + '</a></li>';
                        newcat = jsonObj.cats[i].dispcat;

                        if (newcat != oldcat) {
                            if (html != "") {
                                div += html.replace("[data]", a);
                                html = "";
                                a = "";
                            }
                            oldcat = newcat;
                            html = '<div class="col-md-3 col-sm-6"><div class="product_bx"><div class="product_icon"><span><img src="' + jsonObj.cats[i].image + '" alt=""></span></div><div class="product_info"><h5>' + titleCase(newcat) + '</h5><ul>[data]</ul></div></div></div>';
                        }
                        a += item;
                    }
                    if (html != "")
                        div += html.replace("[data]", a);

                    $("#homecategory").html(div);
                    setTimeout(function () {
                        var maxHeight = -1;
                        $('#homecategory div.col-md-3:lt(4)').each(function () {
                            maxHeight = maxHeight > $(this).height() ? maxHeight : $(this).height();
                        });

                        $('#homecategory div.col-md-3:lt(4)').each(function () {
                            $(this).height(maxHeight);
                        });
                        maxHeight = -1;
                        $('#homecategory div.col-md-3:gt(3)').each(function () {
                            maxHeight = maxHeight > $(this).height() ? maxHeight : $(this).height();
                        });

                        $('#homecategory div.col-md-3:gt(3)').each(function () {
                            $(this).height(maxHeight);
                        });
                    }, 1000);
                },
                error: function (data, status, jqXHR) {
                }

            });
        }
        function titleCase(str) {
            str = str.toLowerCase().split(' ');
            for (var i = 0; i < str.length; i++) {
                str[i] = str[i].charAt(0).toUpperCase() + str[i].slice(1);
            }
            return str.join(' ');
        }
