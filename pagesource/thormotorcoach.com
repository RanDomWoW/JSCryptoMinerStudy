<script type="text/javascript">
    
    $('#brochure-nav-form').detach().appendTo('#site-wrapper');

    $('.brochure-nav-modal').click(function (e) {
        e.preventDefault();
        var $form = $('#brochure-nav-form .aimbase-form');
        var brochureLink = $(this).attr('data-brochure-url');
        var brandCode = $(this).attr('data-brand-code');
        var productCode = $(this).attr('data-product-code');
        var productYear = $(this).attr('data-product-modelyear');
        $('#brochure-nav-form').addClass('active');
        $("#navbar-main").addClass('open-modal');
        $('#brochure-nav-download').attr('href', brochureLink);
        $('input[name=BrandCode]', $form).val(brandCode);
        $('input[name=ProductModelCode]', $form).val(productCode);
        $('input[name=ProductModelYear]', $form).val(productYear);
    });

    $('#brochure-nav-form #pc-button-modal-content .modal-close').click(function () {
        $('#brochure-nav-form').removeClass('active');
        if ($("#navbar-main").hasClass('open-modal')) {
            setTimeout(function () {
                $("#navbar-main").removeClass('open-modal');
                $('#brochure-nav-form-wrapper').show();
                $('#brochure-nav-thanks').hide();
            }, 500);
        }
    });

    $('#brochure-nav-form .submit-download-brochure').click(function () {

        // Form submission and redirect
        var $form = $(this).closest('.form');

        //Add ignore class to Postal Code and Phone inputs if not required
        if (!$('#phone-input').attr('required') && $('#phone-input').val() == "") {
            $('#phone-input').addClass('validate-ignore');
        }
        else {
            $('#phone-input').removeClass('validate-ignore');
        }

        if (($('#postal-code-input').attr('required') && $('#postal-code-input').val() == "") && ($('#country-input').val() == "US" || $('#country-input').val() == "CA")) {
            $('#postal-code-input').removeClass('validate-ignore');
        }
        else {
            $('#postal-code-input').addClass('validate-ignore');
        }


        if ($form.valid()) {

            var domainCheck = false;

            if ($('input[name=BrandOfInterest]').length > 0 || $('select[name=BrandOfInterest]').length > 0) {
                if ($('input[name=BrandOfInterest]').val() == "") {
                    $('input[name=BrandCode]').val($('input[name=BrandOfInterest]').val());
                }
                else {
                    $('input[name=BrandCode]').val($('select[name=BrandOfInterest]').val());
                }

                if ($('input[name=ProductModelCode]').val() == "") {
                    $('input[name=ProductModelCode]').val('UN' + $('input[name=BrandCode]').val());
                }

            }

            if ($('#productBuilderObject').length > 0) {
                buildObj = JSON.parse(localStorage.getItem('ProductBuilderObject'));
                buildObj.PostalCode = postalCodeInput.val();
                buildObj.CountryCode = countrySelect.val();
                $('#productBuilderObject').val(JSON.stringify(buildObj));

                var productCode = buildObj.ProductCode;
                var modelYear = buildObj.ModelYear;
                var brandCode = buildObj.BrandCode;

                if ($('input[name=ProductModelCode]').length > 0) {
                    $('input[name=ProductModelCode]').val(productCode);
                }
                if ($('input[name=ProductModelYear]').length > 0) {
                    $('input[name=ProductModelYear]').val(modelYear);
                }
                if ($('input[name=BrandCode]').length > 0) {
                    $('input[name=BrandCode]').val(brandCode);
                }
            }

            if ($('input[name=brochureVia]').length > 0) {
                var selectedValue = $('input[name=brochureVia]:checked').val();

                if (selectedValue.toLowerCase() != undefined && selectedValue.toLowerCase().indexOf("download") >= 0 && productNodeIds.length != 0) {
                    successUrlParams = "?p=" + productNodeIds.toString();
                }

            }

            if ($('#email-ignore').length > 0) {
                var userDomain = "@" + emailInput.val().split("@")[1];
                var ignoreDomains = $('#email-ignore').val().split(";");
                domainCheck = checkDomains(userDomain, ignoreDomains);
            }

            if (domainCheck) {
                setBrochure();
                $('#email-ignore-modal').modal();
            }
            else {

                showLoader();

                //Send google analytics tracking
                try {
                    submit_eventTrack();
                } catch (e) {

                }

                //Render custom data array in hidden input on form
                if ($('*[data-custom]').length > 0) {
                    generateCustomDataArray($form);
                }

                if ($('#product-of-interest-input').val() != undefined) {
                    if ($('#product-of-interest-input').val().length > 0) {
                        $('#productName').val($('#product-of-interest-input option:selected').text());
                    }
                }

                $.ajax({
                    type: "POST",
                    url: $form.attr("action"),
                    data: $form.serialize(),
                    dataType: "json",
                    success: function (response) {
                        $('#brochure-nav-form-wrapper').fadeOut(1000, function () {
                            $('#brochure-nav-thanks').fadeIn(1000);
                        });
                        $('#brochure-nav-download').get(0).click();
                        hideLoader();
                    },
                    error: function (response) {
                        window.location.href = "/error/";
                    }
                });
            }
        }
        return false;
    });
</script>
    


                                    


        
