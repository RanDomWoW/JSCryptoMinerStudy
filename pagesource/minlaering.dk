    <script>

        var counted = false;

        $('#go-to-books-section').click(function () {
            $('html, body').animate({
                scrollTop: $("#section-books").offset().top
            }, 500);
        });

        $(document).ready(function () {
            countAnimation();
        });

        $(window).scroll(function(){
            countAnimation();
        });

        function countAnimation() {
            if (counted == false) {
                var $elem = $('.school-year-numbers-info .number');
                if (isElementInViewport($elem)) {
                    $elem.each(function () {
                        $(this).prop('Counter', 0).animate({
                            Counter: $(this).text()
                        }, {
                            duration: 3000,
                            easing: 'swing',
                            step: function (now) {
                                $(this).text(Math.ceil(now).toLocaleString('da-DK'));
                                counted = true;
                            }
                        });
                    });
                }
            }
        }

        function isElementInViewport(elem) {
            var $elem = $(elem);

            // Get the scroll position of the page.
//            var scrollElem = ((navigator.userAgent.toLowerCase().indexOf('webkit') != -1) ? 'body' : 'html');
            var viewportTop = $('html').scrollTop() + $('body').scrollTop();
            var viewportBottom = viewportTop + $(window).height();

            // Get the position of the element on the page.
            var elemTop = Math.round( $elem.offset().top );
            var elemBottom = elemTop + $elem.height();

            var isInView = (elemTop < viewportBottom) && (elemBottom > viewportTop);
//            console.log('(' + elemTop + ' < ' + viewportBottom + ') && (' + elemBottom + ' > ' + viewportTop + ')');
//            console.log('isInView: ' + isInView);
            return isInView;
        }
    </script>
