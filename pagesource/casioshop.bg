var newhash = function(first){
	if(window.location.hash != null &&  window.location.hash.length >7){
		var hash = unescape(window.location.hash);
		hash = hash.slice(1);
		hash_array = hash.split("&");
		for(var i=0;i<hash_array.length;i++){

			var filter = hash_array[i].split("=");
			jQuery("#"+filter[0]).val(filter[1]);

			if(filter[0] == "min"){
				//jQuery("#getmin").val(filter[1]);
				jQuery("#min").val(filter[1]);
			}
			if(filter[0] == "max"){
				//jQuery("#getmax").val(filter[1]);
				jQuery("#max").val(filter[1]);
			}

			if(filter[0] == "page"){
				jQuery("#page").val(filter[1]);
			}

			if(filter[0] == "sortselect"){
				jQuery("#sortselect option").removeAttr( "selected" );
				jQuery("#sortselect option[value="+filter[1]+"]").attr('selected','selected');
			}
			if(filter[0] == "perpageselect"){
				//jQuery("#getpage").attr('rel',filter[1]);
				if(filter[1] == 24){
					jQuery("#getpage").html('12 на страница');
				}else{
					jQuery("#getpage").html('24 на страница');
				}
			}


		}
		callJax2(first);
	}
}


var Gethash = function(hash){
	if(typeof(hash)!="undifined" && hash!= null){
		window.location.hash = hash;
	}
	if(window.location.hash != null &&  window.location.hash.length >7){
		var hash = unescape(window.location.hash);
