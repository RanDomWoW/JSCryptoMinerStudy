    <script>
     (function() {
         if (!Modernizr.flexbox && !Modernizr.csscolumns) {
             
             // If Flexbox and CSS Columns are not supported, re-order menu-items elements 
             // instead of 1,2,3,4,5,6,7,8... reorder to 1,14,27,40,2,15,28,41,3...
             var items = $('.main-index .main-index-item');
             var columnCount = $('.main-index').css('column-count');
             if ( typeof columnCount != 'number' ) {
                 columnCount = 3;
             }
             var itemCount = items.length;
             function reflow() {
                 var rowCount = Math.ceil(itemCount / columnCount);
                 items.detach();
                 $('.main-index').empty(); // Prevent build-up of dummy items
                 for (var row = 0; row < rowCount; row++) {
                     for (var column = 0; column < columnCount; column++) {
                         var index = row + (column * rowCount);
                         if ( index < itemCount ) {
                             $('.main-index').append(items[index]);
                         }
                     }
                 }
             }
             reflow();
             $(window).on('resize', function() {
                 var newColumnCount = $('.main-index').css('column-count');
                 if ( typeof newColumnCount != 'number' ) {
                     newColumnCount = 3;
                 }
                 if ( newColumnCount != columnCount ) {
                     columnCount = newColumnCount;
                     reflow();
                 }
             });
         }
     })();

     (function() {
         // ======================================================================
         // News Notification Bar
         // ======================================================================
         
         // Find message id
         var messageID = $(".notification__message").data("message-id");
         var dismissedMessageID = Cookies.get('notification_dismissed');

         $(".notification__dismiss").on("click", function() {
             // On click, hide the notification bar.
             $(".notification")
                 .css("height", $(".notification").height())
                 .removeClass("is-active");
             $(".notification").get(0).offsetWidth; // force browser to repaint
             $(".notification").css("height", "0");
             
             // Set cookie to 'true'
             Cookies.set('notification_dismissed', messageID, {expires:180, path: '/', secure: true});
         });
         
         if ( dismissedMessageID != messageID ) {
             // Notification not dismissed - show notification bar.
             $('.notification').addClass('is-active');
         }
     })();
    </script>
