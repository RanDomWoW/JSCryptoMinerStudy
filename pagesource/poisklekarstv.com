                            position: 'top',
                            alignment: 'end'
                        },
                        tooltip: {
                            textStyle:{fontSize:'12'}
                        },
                        chartArea: {
                            width: '100%'
                        },
                        hAxis: {
                            ticks: data.ticks,
                            titleTextStyle: {color: '#333'},
                            gridlines: {color: '#fff', count: 6},
                            showTextEvery: 5
                        },
                        vAxis: {
                            minValue: 0,
                            textPosition: 'in',
                            gridlines: {color: '#f3f3f3', count: 6}
                        },
                        pointSize: 7,
                        lineWidth: 4
                    };

                    drawChart();

