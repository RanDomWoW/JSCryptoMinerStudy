function renderCelebrity(selector, json, num) {
	var html = '';
	for (var i=0; i<num && i<json.length; i++) {
		var item = json[i];
		if (typeof(item._CELEBRITY) == 'undefined')
			continue;

		html += '<div class="index_celebrity" id="celebrity_' + item._CELEBRITY.CID + '">';
		html +=   '<div class="text">';
		html +=     '<a href="/list/專欄/' + item._CELEBRITY.CID + '">';
		html +=       '<div class="image">';
		if (typeof(item._CELEBRITY.THUMBLINK)!='undefined' && item._CELEBRITY.THUMBLINK != '')
			html +=       '<img src="' + item._CELEBRITY.THUMBLINK + '" onload="fitImageEvent(event);" onerror="this.src=\'/images/default.png\';"/>';
		else
			html +=       '<img src="/images/writer_pic_default_s.png"  onload="fitImageEvent(event);"/>';
		html +=       '</div>';
		html +=     '</a>';
		html +=     '<a href="/news/' + item.EID + '">'
		html +=       '<div class="first_title">' + item.TITLE + '</div>'; 
		html +=     '</a>'
		html +=     '<a href="/list/專欄/' + item._CELEBRITY.CID + '">';
		html +=       '<div class="name">' + item._CELEBRITY.NAME + '<span class="first_pubtime"/></div>'; 
		html +=     '</a>';
		html +=     '<div class="first_brief">' + item.BRIEF.replace(/\\n/g, '') + '</div>';
		html +=     '<a href="/news/' + item.EID + '">'
		html +=       '<div class="first_more">…閱讀全文 &#187;</div>';
		html +=     '</a>'
		html +=   '</div>';

		// html +=   '<div class="article" id="celebrity_' + item.CID + '_article"/>';
		html +=   '<hr/>';
		html += '</div>';

		//ajaxCelebrityArticle(item.CID);
	}
	$(selector).html(html);
}

function ajaxCelebrityArticle(cid)
{
	var uri = '/resource/lists/CELEBRITY/' + cid;
	$.ajax(uri, {
		type: 'GET',
		dataType: 'json',
		cache: false,
		timeout: 10000,
		success: function(json) {
			json = json.data_list;
			if (json.length > 0) {
				$('#celebrity_' + cid + ' .first_title')
				.text(json[0].TITLE)
				.wrap('<a href="/news/' + json[0].EID + '"></div>');
				$('#celebrity_' + cid + ' .first_pubtime').text(json[0].PUBTIME.substring(0, 16));
				$('#celebrity_' + cid + ' .first_brief').text(json[0].BRIEF);
				$('#celebrity_' + cid + ' .first_more').wrap('<a href="/news/' + json[0].EID + '"></div>');

				// Neil_20131218: Remove the celebrity article list.
				// renderCelebrityArticle('#celebrity_' + cid + '_article', json.slice(1, json.length), 2);						
			}
		},
	});
}

function renderCelebrityArticle(selector, json, num) {
	var html = '';
	for (var i=0; i<num && i<json.length; i++) {
		var item = json[i];
		html += '<a href="/news/' + item.EID + '">';
		html +=   '<div class="index_celebrity_article">&#9642;&nbsp;' + item.TITLE + '</div>';
		html += '</a>';
	}

	$(selector).append(html);
}</script></body></html>