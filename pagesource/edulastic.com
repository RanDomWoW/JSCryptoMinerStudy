<script>
function onLoadAffix() {
        var bannerHeadingOffsetTop = 100;
        if ($('.banner-heading').length > 0) {
            bannerHeadingOffsetTop = $('.banner-heading').offset().top;
        }
        var headerHeight = $('#main-header').height();
        $('#main-header').affix({
            offset: {
                top: bannerHeadingOffsetTop - headerHeight
            }
        });

        var dataButton = $(".data-btn");

        dataButton.on("click", function() {
            $(".data-btn").removeClass("active");
            $(this).addClass("active");
        });

        if ($(".carousel-indicators li.first").hasClass('active')) {
            $('.data-btn.first').addClass('active');
        }
    }
  
  var headerContainer = $("#main-header");
   $(window).on('scroll', function() {
        if ($(this).scrollTop() < 120) {
            headerContainer.removeClass('affix affix-top').addClass('affix-top');
        } else{
            headerContainer.removeClass('affix-top affix').addClass('affix');
        }
  });
</script>
<script>
   function loadCSS(href, before, media) {
        "use strict";
        var ss = window.document.createElement("link");
        var ref = before || window.document.getElementsByTagName("script")[0];
        var sheets = window.document.styleSheets;
        ss.rel = "stylesheet";
        ss.href = href;
        ss.media = "only x";
        ref.parentNode.insertBefore(ss, ref);

        function toggleMedia() {
            var defined;
            for (var i = 0; i < sheets.length; i++) {
                if (sheets[i].href && sheets[i].href.indexOf(href) > -1) {
                    defined = true;
                }
            }
            if (defined) {
                ss.media = media || "all";
            } else {
                setTimeout(toggleMedia);
            }
        }
        toggleMedia();
        return ss;
    }
  loadCSS("//cdn.edulastic.com/JS/website/edulastic-web/css/fonts.min.css");
