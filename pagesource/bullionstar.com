    <script type="text/javascript">
        var ajaxQueue = null;
        var changeData = function(pmi, obj){
            if(!pmi && !obj){alert('all null'); return;}
            var _this = obj ? obj : $("#frontGraphTabGroup").find("td[pm='" + pmi + "']");
            var pm = pmi ? pmi : _this.attr("pm");
            $("#frontGraphTabGroup td").removeClass("active");
            _this.addClass("active");
            currFrontGraphPm = pm;
            var wu = $("#frontPageRadio input[name='weightUnit']:checked").val();
            var ti = $("#frontPageRadio input[name='interval']:checked").val();

            $.cookie('rightColMetal' + cookieSuffix, pm, { path: '/' });
            $.cookie('rightColWeight' + cookieSuffix, wu, { path: '/' });
            $.cookie('rightColInterval' + cookieSuffix, ti, { path: '/' });

            if(!document.createElementNS){
                $("#frontGraph").attr('src', "https://static.bullionstar.com/static/partners/bullionstar/images/load_image.gif");
            }
            $(".frontGraphData").hide();
            $(".frontGraphData[pm='" + pm + "']").show();
            if(ajaxQueue){
                ajaxQueue.abort();
            }
            ajaxQueue = $.ajax({
                cache : false,
                type : "POST",
                url : chartUrl + "/graph/get",
                crossDomain : true,
                data : {
                    pm : pm,
                    weightUnit : wu,
                    timeInterval : ti,
                    width : 240,
                    height : 150,
                    currency : "USD"},
                dataType : "json",
                success : function(d){
                    if(d){
                        if(d.spotPrices){
                            $.each(d.spotPrices, function(k,v){
                                var _h = $("div[pm=" + k + "]");
                                _h.find(".fpgPriceVal").html(v.value);
                                var _pricePer = _h.find(".fpgPricePer");
                                _pricePer.html(v.percent).removeClass("positive").removeClass("negative");
                                if(v.signum > 0){_pricePer.addClass("positive");}
                                if(v.signum < 0){_pricePer.addClass("negative");}
                            });
                        }
                        $("#fpgLastUpdateDate").html(d.lastUpdateDate);

                        $("#rcFrontGraph").bstarchart({
                            type : "line",
            				withHorizontalLinesEnds : false,
            				withHorizontalLines : false,
            				withVerticalLines : false,
            				withVerticalLinesEnds : false,
				            withBorder : true,
                            width: 240,
                            height : 150,
                            dataSeries: [{name: 'series1', area:true, color:"#ebd894", lineColor:"#edb989", lineWidth:"1", startDate:d.startDate, data:d.dataSeries, numDecimals:d.numDecimals}]
                        });

                        $('#frontGraphArea').show();
                        
                    }
                },
                error : function() {
                    setTimeout("changeData(currFrontGraphPm);", 60000);
                }
            });
        };

        $(document).ready(function() {
            var defaultFrontGraphPm = $.cookie('rightColMetal' + cookieSuffix);
            defaultFrontGraphPm = defaultFrontGraphPm ? defaultFrontGraphPm : "XAU";
            $(".frontGraphData[pm='" + defaultFrontGraphPm + "']").show();
            $("#frontGraphTabGroup").find("td[pm='" + defaultFrontGraphPm + "']").addClass("active");

            var rightColWeight = $.cookie('rightColWeight' + cookieSuffix);
            var rightColInterval = $.cookie('rightColInterval' + cookieSuffix);

            if (rightColWeight == null) {
                rightColWeight = 'tr_oz';
            }

            if (rightColInterval == null) {
                rightColInterval = 'TYPE_1D';
            }

            $('#frontPageRadio').find('input[name="weightUnit"][value="' + rightColWeight + '"]').attr('checked', 'checked');
            $('#frontPageRadio').find('input[name="interval"][value="' + rightColInterval + '"]').attr('checked', 'checked');

            changeData(defaultFrontGraphPm);

            $(".frontGraphTab").click(function(){
                changeData(false, $(this));
            });

            $("#frontPageRadio input[type='radio']").change(function(){
                changeData(currFrontGraphPm);
            });
        });


        //setTimeout("changeData(currFrontGraphPm);", 60000);
