function controlCheckboxs(obj){
   checkBoxArr = document.getElementsByTagName("INPUT"); 
   var objId = obj.id;
                             
   for(var i = 0; i<checkBoxArr.length; i++) {  
    if(checkBoxArr[i].type=="checkbox") {
     if ((objId=="controlJobType" && checkBoxArr[i].id.indexOf("jobtype")!=-1) || (objId=="controlPositionLevel" && checkBoxArr[i].id.indexOf("positionlevel")!=-1)) {
      if (obj.checked) {
       checkBoxArr[i].checked=false;
       checkBoxArr[i].disabled=true;
      } else {
       checkBoxArr[i].disabled=false;
      }
     } 
    }
   }   
}

function initCheckboxs(hasDefaultValue){
   checkBoxArr = document.getElementsByTagName("INPUT"); 
   var hasJobType=false;
   var hasPositionLevel=false;
   var isJobTypeDisabled=false;
   var isPositionLevelDisabled=false;
   
   for(var i = 0; i<checkBoxArr.length; i++) {  
    if(checkBoxArr[i].type=="checkbox") {
     if (checkBoxArr[i].id.indexOf("positionlevel")!=-1) {
      if (checkBoxArr[i].checked) {
       hasPositionLevel=true;
      }
      if (checkBoxArr[i].disabled) {
       isPositionLevelDisabled=true;
      }  
     }else if (checkBoxArr[i].id.indexOf("jobtype")!=-1) {
      if (checkBoxArr[i].checked) {
       hasJobType=true;
      }
      if (checkBoxArr[i].disabled) {
       isJobTypeDisabled=true;
      } 
     }
    } 
   } 
   
   if (!hasJobType && document.getElementById('controlJobType') != null && hasDefaultValue) {
    document.getElementById('controlJobType').click();
   }
   
   if (!hasPositionLevel && document.getElementById('controlPositionLevel') != null && hasDefaultValue) {
    document.getElementById('controlPositionLevel').click();
   }
   
   for(var i = 0; i<checkBoxArr.length; i++) { 
    if ((hasJobType && isJobTypeDisabled && checkBoxArr[i].id.indexOf("jobtype")!=-1) || (hasPositionLevel && isPositionLevelDisabled && checkBoxArr[i].id.indexOf("positionlevel")!=-1)) {
     checkBoxArr[i].disabled=false;
    } 
   }
}
    
function hasJobSelected(tableId){

   var jobTable = document.getElementById(tableId);
   if (jobTable == null) {
      return false;
   }
   
   var inputArr = jobTable.getElementsByTagName("INPUT");
   if (inputArr == null || inputArr.length == 0) {
      return false;
   }
   
   var checkBoxArr = new Array();
   var checkBoxIndex = 0;
   
   for(var i = 0; i<inputArr.length; i++) {  
    if(inputArr[i].type=="checkbox") {
       checkBoxArr[checkBoxIndex] = inputArr[i];
       checkBoxIndex++;
    } 
   }
   
   for(var i = 0; i<checkBoxArr.length-1; i++) {  
    if(checkBoxArr[i].checked) {
       return true;
    } 
   } 
   return false; 
}

function deleteJobs(tableId){
