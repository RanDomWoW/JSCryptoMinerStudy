	$(".div-epg-channel").css( {
		"border-bottom": "1px solid #333",
    	"border-right": "1px solid #757575",
		"border-top": "0",
		"border-left": "0"
	});
	$("#lbl-channel-found").html("");
	if (chnl.length == 0) {
		return false;
	}
	
	$.fn.extend({
		filterAlt: function (text) {
			return $(this).filter(function() {
				return $(this).attr('alt').toLowerCase().indexOf(text.toLowerCase()) > -1;
			});
		}
	});
	$(".img-channel-logo").filterAlt(chnl).parent().parent().parent().css("border","1px solid #FC00FF");
