<script type="text/javascript">
    function bf_flowmanagerlink(self, link) {
        location.href = link;
    }

    function bf_flowmanager_first(ruletype, isdeal) {
        var canonical = '';
        try {
            canonical = $("link[rel=canonical]").attr('href');
        } catch (e) { }
            
        try {
            if (!canonical) {
                canonical = location.pathname || location.href;
            }
        } catch (e) { }
        var perSession = 'data-bf_flowmanager.' + ruletype;
        var perPage = perSession + canonical;
        
        if (isdeal) {
            if (sessionStorage[perSession] || sessionStorage[perPage]) {
                return false;
            } else {
                sessionStorage[perPage] = true;
                sessionStorage[perSession] = true;
                return true;
            }
        } else {
            if (sessionStorage[perPage]) {
                return false;
            } else {
                sessionStorage[perPage] = true;
                return true;
            }
        }
    }

    function bf_flowmanager_ismildflow(self, flowType, hascashback, hascode) {
        

        switch (flowType) {
            case 'ShopNow':
            case 'Online':
            case 'FreeProductPromo':
                return !hascashback && !hascode;
            case 'Printable':
            case 'Restaurant':
                return true;
            case 'Deal':
                return !hascashback;
        }
        return false;
    }

    function bf_flowmanager_forceMildDialog(self, objectID, objectType) {
        document.forms[0].target = '_blank';
        bf_create_show(this, objectID, objectType, '', PageData.PlacementReferralID);
    }

    function bf_flowmanager(self, flowType, objectID, objectDetails, offersuffix) {
        
        var True = true;
        var False = false;
        
        var flowData = {
            HasCashback: objectDetails['HasCashBack'],
            HasCode:     objectDetails['HasCode'],
            IsRetailer:  objectDetails['IsRetailer']
        };

        //Probably need more page data than this
        var PageData = { PlacementReferralID: 0 };
        var UserData = { LoggedIn: False};
        var suffix = '';
        if (PageData.PlacementReferralID) {
            suffix = suffix + '&pref=' + PageData.PlacementReferralID;
        }
        if (offersuffix) {
            suffix = suffix + offersuffix;
        }

        var mildflow = 
            !UserData.LoggedIn && 
            bf_flowmanager_ismildflow(self, flowType, flowData.HasCashback, flowData.HasCode);

        var objectType = 1;
        var trackOut = true;
        var param;
        if (flowType == 'WeeklyAd') {
            param = 'wky';
        } else if (flowData.IsRetailer) {
            param = 'rtr';
            objectType = 2;
            trackOut = false;
        } else if (flowType == 'FreeProductPromo') {
            param = 'fpp';
        } else {
            param = 'c';
        }
        var destinationSite = '/coupons/cashbacksignuppopup/nu/?' + param + '=' + objectID + suffix;
        if (trackOut) {
            googleTrackOut(objectID, false);
        }

        switch (flowType) {
            case 'HardDialogNoThanks':
                window.open(destinationSite, '_blank');
                var gatedpanel = $find('ClickoutModalGatedPanel');
                if(gatedpanel) gatedpanel.hide();
                break;
            case 'ShopNow':
            case 'Online':
            case 'Deal':
            case 'Printable':
            case 'Restaurant':
            case 'FreeProductPromo':
            case 'WeeklyAd':
                var isDeal = flowType == 'Deal';

                ga('send', 'pageview',destinationSite);
                if (UserData.LoggedIn) {
                    window.open(destinationSite, '_blank');
                } else if (mildflow) {
                    window.open(destinationSite, '_blank');
                    if (bf_flowmanager_first('mildflow', isDeal)) {
                        bf_create_show(this, objectID, objectType, '', PageData.PlacementReferralID);
                    }
                } else if (flowData.HasCashback && bf_flowmanager_first('hardgate',isDeal)) {
                    bf_create_show(this, objectID, objectType, '', PageData.PlacementReferralID, bf_create_show_customArgs.Gate);
                } else if (flowData.HasCashback || flowData.HasCode) {
                    window.open(destinationSite, '_blank');
                    if (bf_flowmanager_first('mildflow', isDeal)) {
                        bf_create_show(this, objectID, objectType, '', PageData.PlacementReferralID);
                    }
                } else {
                    document.forms[0].target = '_blank';
                    bf_create_show(this, objectID, objectType, '', PageData.PlacementReferralID);
                    
                    setTimeout(function() { location.href = destinationSite; }, 50);
                }
                break;
        }
