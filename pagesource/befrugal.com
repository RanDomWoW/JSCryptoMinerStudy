<script type="text/javascript">
    function bf_login_show_offer(objId, objType, customArgs) {
        $get('ctl00_bfLoginStatus_hidCustomArg').value = customArgs;
        $get('ctl00_bfLoginStatus_hidObjId').value = objId;
        $get('ctl00_bfLoginStatus_hidObjType').value = objType;
        __doPostBack('ctl00$bfLoginStatus$btnLogin','');
        focusLoginTextField()
        return false;
    }

    function bf_login_show(username) {
        try {
			 ga('send', 'event', 'signups', 'signupDialog', document.location);
			 ga('send', 'pageview', '/signups/signupDialog.html');
        } catch (e2) { }
        if (username) {
            $get('ctl00_bfLoginStatus_hidLoginUsername').value = username;
            $get('ctl00_bfLoginStatus_LoginControl_txtUser').value = username;
        }

        var signuppopup = $find('noUserOfferClickPop');
        if (signuppopup) {
            signuppopup.hide();
        }

        $(document.body).addClass("login");
        
        $find('bfLoginBehavior').show();
        if (username && bf_is_password_visible_ctl00_bfLoginStatus_LoginControl()!="show") {
            $('#ctl00_bfLoginStatus_LoginControl_btnLogin').click();
        } else {
            focusLoginTextField();
        }
        return false;
    }

    function bf_login_hide() {

        $(document.body).removeClass("login");
        $find('bfLoginBehavior').hide()
    }
    
    
    function bf_login_then_toggle_favorite_show(objId, objType) {
        $get('ctl00_bfLoginStatus_hidFavoriteObjId').value = objId;
        $get('ctl00_bfLoginStatus_hidFavoriteObjType').value = objType;

        bf_login_show();
    }

    bf_create_show_customArgs = {
        Gate: "gate"
    }

    function bf_create_show(lnk, objId, objType, username, placementReferralID, customArgs) {
        if (username) {
            $get('ctl00_bfLoginStatus_hidLoginUsername').value = username;
        }
        $get('ctl00_bfLoginStatus_hidObjId').value = objId;
        $get('ctl00_bfLoginStatus_hidObjType').value = objType;
        $get('ctl00_bfLoginStatus_hidCustomArg').value = customArgs;

        try {
			 ga('send', 'event', 'signups', 'signupDialog', document.location);
			 ga('send', 'pageview', '/signups/signupDialog.html');
        } catch (e2) { }

        __doPostBack('ctl00$bfLoginStatus$btnCreate','');
        focusLoginTextField();

        return false;
    }

    function focusLoginTextField() {
        var focusField = $('#ctl00_bfLoginStatus_LoginControl_txtUser');
        if(focusField.length>0 && screen.width >= 750)
            focusField.focus();
    }
    
    function bf_create_show_under(lnk, objId, objType) {
        try {
            pu = window.open(lnk.href, '_blank', 'toolbar=1,scrollbars=1,location=0,statusbar=0,menubar=0,resizable=1,width=600,height=520,left=400,top=200');
            pu.blur();
            window.focus();
        } catch (ex) { }
        bf_create_show(lnk, objId, objType);
        return false;
    }

    function unl_LoginComplete(hasObj, acntCreated, multiRole) {
        try {
            if (acntCreated) {
				ga('send', 'event', 'signups', 'signupSuccessful', document.location);
				ga('send', 'pageview', '/signups/signupSuccessful.html');
            }
        } catch (e2) { }
        if (!hasObj && acntCreated && !multiRole) {
            document.location = '/cashback/';
        }
    }

    function bf_show_create_final() {
        try {
			ga('send', 'event', 'signups', 'signupSuccessful', document.location);
			ga('send', 'pageview', '/signups/signupSuccessful.html');
        } catch (e2) { }
    }
</script>


