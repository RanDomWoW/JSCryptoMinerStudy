<!-- SPEC BAN-NERONE --><script>var sl = "/";
var jsn = '';
var urli = sl+sl+"assets.hmai.it"+sl+"speciali"+sl+pageInfo.blogname+sl+"config.js?1";

jQuery.ajaxSetup({
   cache: true
});

jQuery.ajax({
  url: urli,
  dataType: "jsonp"
});

function showSpec(datajson) {

jsn =datajson.spec;

      var random;
      var num = 0;
      lottery = {};
      var winner = '';
      var curr = pageInfo.cat.join(',');
      var show;
      var dentro = false;
      jQuery.each(jsn,function(key,val){


         show = true;
         var special = val.slug.split('|');
         var specialHome = val.slug_categoria_primaria;
         var current = curr.split(',');
         var found = jQuery.inArray(specialHome , current) > -1;
         if(found){
            show = false;
             dentro = true;
         }
         jQuery.each(current, function(el,v) {
            if (jQuery.inArray(v, special) > -1){
               show = false;
               dentro = true;
            }
         });
         var arr = special.concat(current);
         var sorted_arr = arr.sort();
         var results = [];
         for (var i = 0; i < arr.length - 1; i++) {
            if (sorted_arr[i + 1] == sorted_arr[i]) {
               results.push(sorted_arr[i]);
            }
         }
         var now = (new Date()).toISOString().substring(0, 10)
         if(val.datafine <= now){
            show = false;
         }

			var compete = true;
			if(val.promoInterna!=1){
				compete = false;
			}
			if(val.onlyforce==1 && compete){
				compete = false;
			}
			if(val.megaSpeciale!=1 && compete){
            compete = false;
         }
			if(val.sponsor!=1 && compete){
            compete = false;
         }

         if(compete && show){
            if(val.priority == 0){
               num = num+5;
            }else if (val.priority == 1){
               num = num+10;
            }
            lottery[num] = val.keyunique;
         }
         if(dentro) return false;
      })
		random=Math.floor(Math.random()*num)
      n = 0;
      jQuery.each(lottery,function(k,v){
         if(random >= n && random <= k){
            winner = v;
            return false;
         }else {
            n = parseInt(n)+parseInt(k);
         }
      });

      var sp = getObjects(jsn, 'keyunique', winner);
      if(jQuery.isEmptyObject(sp[0])){
         show = false;
      }
		if(getCookie('megaSpClose')== 'closed'){
			 show = false;
	   }

      if(show){
         if(sp[0].megaSpeciale == 1 && jQuery(".bigTail").length >0){
				var slash = "/";
            jQuery(".bigTail").load(slash+slash+location.host+slash+"speciali"+slash+winner+slash+"megaspeciale.html",function(){
               jQuery('.header-wrap').css('position','static');
               jQuery('.header-placeholder-no-class').css('display','none');
               jQuery('span.tiRepTitl').replaceWith(sp[0].title);
					jQuery('body #megasp a.linkami').attr('href',sp[0].linkhome);
					jQuery('body #megasp').addClass(sp[0].keyunique);
					jQuery("#slidetoggle2").css('top',220);
					if(pageInfo.groupName == 'g-stl17'){
						jQuery('.header-wrap.new-header').addClass('is-fixed');
				   	useDifferentHeader = true;
					}
            });
         }

      }



	function getCookie(cname) {
   var name = cname + '=';
   var decodedCookie = decodeURIComponent(document.cookie);
   var ca = decodedCookie.split(';');
   for(var i = 0; i <ca.length; i++) {
      var c = ca[i];
      while (c.charAt(0) == ' ') {
         c = c.substring(1);
      }
      if (c.indexOf(name) == 0) {
         return c.substring(name.length, c.length);
      }
   }
   return '';
};

   function getObjects(obj, key, val) {
      var objects = [];
      for (var i in obj) {
         if (!obj.hasOwnProperty(i)) continue;
         if (typeof obj[i] == 'object') {
            objects = objects.concat(getObjects(obj[i], key, val));
         } else if (i == key && obj[key] == val) {
            objects.push(obj);
         }
      }
      return objects;
   }
	 jQuery('body').on('click',' a.close',function(event){
      document.cookie="megaSpClose=closed; domain=."+location.hostname.split('.').reverse()[1]+"."+location.hostname.split('.').reverse()[0]+"; path=/";
      $(this).parent('.megaSp').hide();
   });

}
</script>

