    $('.samw-menu li > a').each(function() {
        if ($(this).parent().children('ul').length) {
            $('<a href="#" class="chevron"><i class="fa fa-angle-down"></i></a>').appendTo($(this)); 
        }
    });
 
	$('.samw-menu li .chevron').click(function(e) {
	  	e.preventDefault();
	  
	    var $this = $(this), $next = $this.closest('li').find('ul').first();
	  
	    if ($next.parent().hasClass('active')) {
	        $next.parent().removeClass('active');
	        $next.slideUp('100');
	    } else {
	        $this.closest('li').parent().find('li.menu-item-has-children').removeClass('active');
	        $this.closest('li').parent().find('li .sub-menu').slideUp('100');
	        $next.parent().toggleClass('active');
	        $next.slideToggle('100');
	    }
	});

});
</script>
