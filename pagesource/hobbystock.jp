function loadSection(section, largeCategoryId) {
	// 未選択の場合は大カテゴリIDを設定しない
	if (typeof largeCategoryId === "undefined") largeCategoryId = 0;

	switch (section) {
		case 'order':
			$.ajax({
				url: '/tops/findOrderItemForFront/' + largeCategoryId,
				type: 'GET',
				success: function(data) {
					$("#orderSection").html(data);
				}
			});
			break;
		case 'new':
			$.ajax({
				url: '/tops/findNewItemForFront/' + largeCategoryId,
				type: 'GET',
				success: function(data) {
					$("#newSection").html(data);
				}
			});
			break;
		case 'group':
			$.ajax({
				url: '/tops/findGroupItemForFront/' + largeCategoryId,
				type: 'GET',
				success: function(data) {
					$("#groupSection").html(data);
					if (data != 0){

					}else{
						$("#groupSection").remove();
					}
				}
			});
			break;
		case 'few':
			$.ajax({
				url: '/tops/findFewItemForFront/' + largeCategoryId,
				type: 'GET',
				success: function(data) {
					$("#fewSection").html(data);
				}
			});
			break;
	}
}

function addCarts(id) {
	var submitType = document.createElement("input");
	submitType.setAttribute("name", "Item[id]");
	submitType.setAttribute("type", "hidden");
	submitType.setAttribute("value", id);
	document.addToCart.appendChild(submitType);
	document.addToCart.submit();
}

