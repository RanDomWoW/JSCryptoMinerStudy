			if($(this).data("type")=="close"){
				$(self)
					.data("type","open")
					.find("img")
					.attr("src","/img/user/btn_close_01.png");
				target.slideDown();
			}else{
				$(self)
					.data("type","close")
					.find("img")
					.attr("src","/img/user/btn_more_01.png");
				target.slideUp();
			}
			return false;
		});
	}
}
window.onload=loadSubItem;
</script>
