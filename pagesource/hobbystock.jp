<script>
/**
 * 次月、前月カレンダー取得
 */
function changeMonth(type) {
	var elem = document.getElementById("thisMonthIndex");
	if (!elem) {
		return false;
	}
	var index = elem.value;
	index = Number(index);
	var max = document.getElementById("countMonth").value;
	max = Number(max);
	max -= 1;
	if (type == 1) {
		var newIndex = index - 1
		if (newIndex < 0) {
			return;
		}
		if (newIndex == 0) {
			document.getElementById("prev").style.display = "none";
		}
		if (newIndex <= max) {
			document.getElementById("next").style.display = "block";
		}
		document.getElementById("thisMonthIndex").value = newIndex;
	} else if (type == 2) {
		var newIndex = index + 1
		if (newIndex > max) {
			return;
		}
		if (newIndex == max) {
			document.getElementById("next").style.display = "none";
		}
		if (newIndex >= 0) {
			document.getElementById("prev").style.display = "block";
		}
		document.getElementById("thisMonthIndex").value = newIndex;
	}
	writeMonthData(newIndex);
}

/**
 * カレンダー更新
 */
function writeMonthData(index) {

	var userAgent = window.navigator.userAgent.toLowerCase();

	var ff = false;
	if (userAgent.indexOf("firefox") > -1) {
		ff = true;
	}

	var elem = document.getElementById(index + "-yearMonth");
	if (!elem) {
		return false;
	}

	var viewYearMonth = elem.value;
	var viewYearMonthAry = viewYearMonth.split("-");
	if (ff) {
		document.getElementById("calendarYear").textContent = viewYearMonthAry[0];
		document.getElementById("calendarMonth").textContent = viewYearMonthAry[1];
	} else {
		document.getElementById("calendarYear").innerText = viewYearMonthAry[0];
		document.getElementById("calendarMonth").innerText = viewYearMonthAry[1];
	}
	for (var i = 1; i <= 42; i++) {
		if (ff) {
			document.getElementById("cell" + i).textContent = "";
		} else {
			document.getElementById("cell" + i).innerText = "";
		}
		document.getElementById("cell" + i).className = "";
	}
	var days = document.getElementById(index + "-days").value;
	var day = document.getElementById(index + "-day").value;
	var status = document.getElementById(index + "-status").value;
	var statusAry = status.split(",");
	var firstDay = day;
	firstDay = Number(firstDay);
	day = Number(day);
	var classStr = "";
	for (var i = 0; i < days; i++) {
		firstDay += 1;
		if (ff) {
			document.getElementById("cell" + firstDay).textContent = i+1;
		} else {
			document.getElementById("cell" + firstDay).innerText = i+1;
		}
		classStr = "";
		if (day == "0") {
			classStr = "sun ";
		} else if (day == "1") {
			classStr = "man ";
		} else if (day == "2") {
			classStr = "tue ";
		} else if (day == "3") {
			classStr = "wed ";
		} else if (day == "4") {
			classStr = "thu ";
		} else if (day == "5") {
			classStr = "fri ";
		} else {
			classStr = "sat ";
		}
		day++;
		if (day > 6) {
			day = 0;
		}
		if (statusAry[i] == "1") {
			classStr = classStr + "holiday";
		} else if (statusAry[i] == "2") {
			classStr = classStr + "stopped";
		}
		document.getElementById("cell" + firstDay).className = classStr;
	}
}
</script>
