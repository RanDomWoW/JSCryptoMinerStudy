      var query = {};
      var a = qstr.substring(qstr.indexOf('?')+1).split('&');
      for (var i = 0; i < a.length; i++) {
          var b = a[i].split('=');
          query[decodeURIComponent(b[0])] = decodeURIComponent(b[1] || '');
      }
      return query;
  }

