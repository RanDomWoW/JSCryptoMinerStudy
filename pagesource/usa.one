document.getElementById("LosAngeles").innerHTML = worldClock(-8, "NAmerica")
document.getElementById("NewYork").innerHTML = worldClock(-5, "NAmerica")


setTimeout("worldClockZone()", 1000)
}
window.onload=worldClockZone;
    
