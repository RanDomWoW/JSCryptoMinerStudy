    <script>
        var INTENT_MEDIA = {

            required_parameters: {},
            user_detail_parameters: {},
            generic_search_parameters: {},
            hotel_search_parameters: {},
            conversion_parameters: {},

            init_intent_media: function(loadIndentMediaTag, data)
            {
                if(typeof  window.IntentMediaProperties != 'undefined'){
                    $.extend(window.IntentMediaProperties , data);
                }else{

                    window.IntentMediaProperties = data;
                }
                console.log(data);

                // Populate IntentMedia parameters
               // window.IntentMediaProperties = data;

                // Assign IntentMedia bindings object
                window.IntentMediaBindings = {

                    segmentation_group_updated: {

                        callback: function (data) {

                            // To store OZ
                            if(typeof Generic !== 'undefined')
                            {
                                var generic = new Generic;

                                generic.storeImScoreToOz(data);
                            }

                            // For google dataLayer
                            if(typeof dataLayer !== 'undefined' && typeof data.group !== 'undefined')
                            {
                                dataLayer.push({ 'intent': data.group });
                            }
                        },
                        groups: 20
                    }

                    ,ads_displayed: function (ad_type) {

                        var eventLabel = 'IntentMedia:' + window.location.href;

                        VRS.googleEventTrackCommonMethod('Ads_Display', ad_type, eventLabel);

                        var dataLayerAddType = '',
                            country_code = typeof common_info.country_code !== 'undefined' && common_info.country_code ? common_info.country_code : '';

                        if(ad_type !== 'undefined' && ad_type !== "")
                        {
                            dataLayerAddType = ad_type;
                        }

                        VRS.pushDataToDataLayer({"event":"IntentAd","AdType":dataLayerAddType, "AdAction":"Shown", "CountryCode":country_code,"Origin":data.page_id});

                        VRS.TRACE('IM event "' + ad_type + '" has been triggered!', true);
                    }
                };

                // Load IntentMedia tag
                loadIndentMediaTag(data.page_id);
            },

            load_indent_media_tag: function(pageId)
            {
                //todo: IM tag  is loaded via GTM for SR, we have to do that for all sites after gtm completed for all sites

                if("1" != "1")
                {

                    (function() {

                        var site_domain = "bedroomvillas.com",
                            url = '//compare.' + site_domain + '/javascripts/v1/p/alt_core.js';  // https://projects.leftofthedot.com:8443/browse/VRS-2446

                        if("BEDROOMVILLAS" === "RENTALHOMES")
                        {
                            if ( typeof pageId !== "undefined" && pageId === "hotel.confirmation" )
                            {
                                url = '//a.cdn.intentmedia.net/javascripts/intent_media_meta_conversions.js';  // https://projects.leftofthedot.com:8443/browse/VRS-2159
                            }
                        }

                        if("BEDROOMVILLAS" === "VACATIONHOME" || "BEDROOMVILLAS" === "ALOHAVILLAS")
                        {
                            url = '//a.cdn.intentmedia.net/javascripts/v1/intent_media_core.js';
                        }

                        var script = document.createElement('script');
                        script.src = url;
                        script.async = true;
                        document.getElementsByTagName('head')[0].appendChild(script);
                    }());
                }

                // Redirect to partner page after loading IM tag

                if('0' === '1' && window.IntentMediaProperties.page_id === "hotel.confirmation" && typeof referralTransaction === 'function')
                {
                    referralTransaction();
                }
            },

            merge_objects: function(merge_to, data_object)
            {
                var key;

                for (key in data_object)
                {
                    merge_to[key] = data_object[key];
                }
            },

            redraw_intent_media: function() {
                if(window.IntentMediaProperties.page_id && window.IntentMediaProperties.page_id !== "hotel.home" && window.IntentMedia && IntentMedia.trigger)
                {
                    IntentMedia.trigger("onpage_ads_redraw");
                }
            },

            update_im_from_keyword: function(callback, keyword, search_url, date_start, date_end)
            {
                if(!keyword)
                {
                    return false;
                }

                var self = this, geocoder = null;

                try
                {
                    geocoder = new google.maps.Geocoder();
                    geocoder.geocode({ 'address': keyword}, function(results, status)
                    {
                        if(typeof (results[0]) !== "undefined")
                        {
                            var address_components = results[0].address_components,
                                country = "", region = "", place = "", type = null, long_name = "", short_name = "",
                                extend_place = typeof(address_components[0].long_name) !== "undefined" ? address_components[0].long_name : "";

                            for (var i = 0; i < address_components.length; i++)
                            {
                                type = address_components[i].types[0];
                                long_name = address_components[i].long_name;
                                short_name = address_components[i].short_name;

                                if (!place && long_name)
                                {
                                    switch (type)
                                    {
                                        case "establishment": // typically indicates a place that has not yet been categorized.
                                        case "locality": //matches against both locality and sublocality types.
                                            place = long_name;
                                            break;
                                    }
                                }

                                if(!region && short_name.length === 2 && type === "administrative_area_level_1")
                                {
                                    region = short_name.toUpperCase();
                                }

                                if(!country && short_name && type === "country")
                                {
                                    country = short_name;
                                }
                            }

                            // If no place name and region code are found from google geo data
                            if(!place)
                            {
                                place = extend_place;
                            }

                            if(!region)
                            {
                                region = country;
                            }

                            if (window.IntentMediaProperties.page_id === "car.home")
                            {
                                callback({city: place, state: region, country: country});
                            }
                            else
                            {
                                if(date_start && date_end)
                                {
                                    date_start = self.machineReadableDateFormat(date_start).replace(/\-/g, "");
                                    date_end = self.machineReadableDateFormat(date_end).replace(/\-/g, "");

                                    self.generic_search_parameters.travel_date_start = date_start;
                                    self.generic_search_parameters.travel_date_end = date_end;

                                    self.merge_objects(window.IntentMediaProperties, self.generic_search_parameters);
                                }

                                if(country && region && place)
                                {
                                    self.hotel_search_parameters.hotel_country = country;
                                    self.hotel_search_parameters.hotel_state = region;
                                    self.hotel_search_parameters.hotel_city = place;

                                    self.merge_objects(window.IntentMediaProperties, self.user_detail_parameters);
                                    self.merge_objects(window.IntentMediaProperties, self.generic_search_parameters);
                                    self.merge_objects(window.IntentMediaProperties, self.hotel_search_parameters);
                                }

                                callback();
                            }
                        }
                        else
                        {
                            callback();
                        }
                    });

                }catch(e)
                {
                    callback();
                }
            },

            intent_media_update: function(date_start, date_end)
            {
                var self = this;

                self.generic_search_parameters.travel_date_start = date_start;
                self.generic_search_parameters.travel_date_end = date_end;

                self.merge_objects(window.IntentMediaProperties, self.generic_search_parameters);

                setTimeout(function () {
                    self.redraw_intent_media();
                }, 1000);
            },

            trigger_exit_unit: function(trigger_type)
            {
                if (window.IntentMedia && IntentMedia.trigger)
                {
                    VRS.TRACE(JSON.stringify(IntentMediaProperties, null, '\t'));

                    if (typeof (trigger_type) !== "undefined" && trigger_type === "open")
                    {
                        IntentMedia.trigger("open_exit_unit");
                    }
                    else
                    {
                        IntentMedia.trigger("fill_exit_unit");
                    }
                }
                else
                {
                    VRS.TRACE("window.IntentMedia is undefined");
                }
            },

            trigger_overlay: function()
            {
                if (window.IntentMedia && IntentMedia.trigger)
                {
                    IntentMedia.trigger('open_overlay');
                }
            },

            update_page_id: function(im_page_id)
            {
                window.IntentMediaProperties.page_id = im_page_id;

                if (window.IntentMedia && IntentMedia.trigger)
                {
                    VRS.TRACE(JSON.stringify(IntentMediaProperties, null, '\t'));

                    IntentMedia.trigger("page_id_updated");
                }
            },

            machineReadableDateFormat: function(date)
            {
                if(date)
                {
                    if (/\d{4}\-\d{2}\-\d{2}/.test(date))
                    {
                        return date;
                    }
                    else if (/\d{2}\-\d{2}\-\d{4}/.test(date))
                    {
                        var date_elements = date.split('-');

                        if(typeof (date_elements[2]) !== "undefined")
                        {
                            if(date_elements[0].length === 4)
                            {
                                return date;
                            }
                            else
                            {
                                return date_elements[2] + '-' + date_elements[0] + '-' + date_elements[1];
                            }
                        }
                    }
                }

                return '';
            }

        };

        $(document).ready(function() {
