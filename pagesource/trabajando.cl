<script type="text/javascript">

var fixSpace = function() {
	setTimeout(function() {
	try{
		var finalYOfertas, firstYBase, delta, addedHeight, offerHeight;

		finalYOfertas = $("#ofertas_slider").offset.top + $("#ofertas_slider").outerHeight();
		firstYBase = $("#base").offset().top;
		delta = firstYBase - finalYOfertas;
		offerHeight = $('.md2_oferta').outerHeight();

		if(delta > (offerHeight / 2)) {
			addedHeight = Math.ceil(delta / offerHeight) * offerHeight;
			$("#ofertas_slider").height($("#ofertas_slider").height() + addedHeight);
		}
	}
		catch(err){
			console.log(err.message);
		}
	}, 2500);
}

//cache ofertas
var cacheStore = new Array();
var from = 0;

function toLoad(url) {
	cacheStore.push(url);
}

function cacheOffer() {
	var to = from + 12;

	for(i = from; i < to; i++) {
		var Ajax = new XMLHttpRequest();
		Ajax.open("GET", cacheStore[i], true);
		Ajax.send();
	}

	from += 12;
}
//fin cache ofertas

