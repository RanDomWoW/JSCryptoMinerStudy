    <script>

      //know language of web
      function langLangify(){
        var lang;
        var aux = langify.helper.getSelectedLanguage();
        switch(aux){
          case 'ly15938': lang='es'; break;
          case 'ly15939': lang='en'; break;
          case 'ly15940': lang='it'; break;
          case 'ly15941': lang='de'; break;
          case 'ly15942': lang='pt'; break;
          case 'ly15943': lang='el'; break;
          case 'ly15944': lang='fr'; break;
          default: lang='en'; break;
        }
        return lang;
      }
      //know langify code
      function codeLangify(aux){
        switch(aux){
          case 'es': lang='ly15938'; break;
          case 'en': lang='ly15939'; break;
          case 'it': lang='ly15940'; break;
          case 'de': lang='ly15941'; break;
          case 'pt': lang='ly15942'; break;
          case 'el': lang='ly15943'; break;
          case 'fr': lang='ly15944'; break;
          default: lang='ly15939'; break; //ingles
        }
        return lang;
      }
      //URL
      function insertParam(key, value) {
        key = encodeURI(key);
        value = encodeURI(value);
        var kvp = document.location.search.substr(1).split('&');
        var i = kvp.length; var x; while (i--) {
          x = kvp[i].split('=');

          if (x[0] == key) {
            x[1] = value;
            kvp[i] = x.join('=');
            break;
          }
        }

        if (i < 0) { kvp[kvp.length] = [key, value].join('='); }

        //this will reload the page, it's likely better to store this until finished
        document.location.search = kvp.join('&');
      }

      //Coges el parámetro de la url
      function getParameterByName(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, ""));
      }

      function hayUrl() {
        var url_lang = getParameterByName('lang');
        var idioma_langify = codeLangify(url_lang);
        if( url_lang=='es' || url_lang=='en' || url_lang=='de' || url_lang=='it' || url_lang=='pt' || url_lang=='el' || url_lang=='fr' ){
          if(url_lang != langLangify()){
            langify.helper.saveLanguage(idioma_langify);
          }
        }
        /*if (url_lang == null) {
          insertParam('lang', langLangify());

        }else{
          if(url_lang != langLangify()){
            langify.helper.saveLanguage(idioma_langify);
          }
        }*/
      }

      function redirectA(){
        var url_lang = getParameterByName('lang');
        if(url_lang != null){
          if( url_lang=='es' || url_lang=='en' || url_lang=='de' || url_lang=='it' || url_lang=='pt' || url_lang=='el' || url_lang=='fr' ){
            $('.js-redirect-lang').each(function(){
              var url = $(this).attr('href');
              if(url[url.length -1] == '/'){
                $(this).attr("href", url+'?lang=' + langLangify());
              }else{
                $(this).attr("href", url+'/?lang=' + langLangify());
              }
            });
          }
        }
      }

      $(document).ready( function() {
        setTimeout(function(){
          hayUrl();
        }, 10);

        redirectA();
      });
    </script>
    

    
    
    

