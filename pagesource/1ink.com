if ($(window).width() < 769){
	jQuery(function(){
		var numLettersShown = 80;

		var elements_to_wrap = $('.CategoryDescription h1 ~ p');
		$(elements_to_wrap).wrapAll('<div class="minimized_wrapper">')

		var minimized_elements = $('.CategoryDescription .minimized_wrapper');


		minimized_elements.each(function(){
			var text = $(this).text();
			if(text.length <  numLettersShown) return;

			$(this).html(
				text.slice(0, numLettersShown)+'<span>... </span><a href="#" class="more">Read More</a>'+
				'<span style="display:none;">'+ text.slice( numLettersShown,text.length)+' <a href="#" class="less">Read Less</a></span>'
			);
		});

		$('a.more', minimized_elements).click(function(event){
			event.preventDefault();
			$(this).hide().prev().hide();
			$(this).next().show();
		});

		$('a.less', minimized_elements).click(function(event){
			event.preventDefault();
			$(this).parent().hide().prev().show().prev().show();
		});
	});
}


