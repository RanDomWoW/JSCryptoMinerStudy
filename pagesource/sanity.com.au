                    <script type="text/javascript">
                        var date_future = new Date("2018/03/21 09:00:00"); //format should be yyyy/MM/dd HH:mm:ss

                        function countdownsanit() {
                            var date_now = new Date();
                            var date_seconds = Math.floor((date_future - (date_now)) / 1000);
                            var date_minutes = Math.floor(date_seconds / 60);
                            var date_hours = Math.floor(date_minutes / 60);
                            var date_days = Math.floor(date_hours / 24);

                            date_hours = date_hours - (date_days * 24);
                            date_minutes = date_minutes - (date_days * 24 * 60) - (date_hours * 60);
                            date_seconds = date_seconds - (date_days * 24 * 60 * 60) - (date_hours * 60 * 60) - (date_minutes * 60);

                            if (date_days < 0) {
                                 document.getElementById('dday').innerHTML = '0';
                                document.getElementById('dhour').innerHTML = '0';
                                document.getElementById('dmin').innerHTML = '0';
                                document.getElementById('dsec').innerHTML = '0';
                                //document.getElementById('CountDownBannerWrapperDiv').style.display = "none";
                                return;
                            }
                            else {
                                document.getElementById('dday').innerHTML = date_days;
                                document.getElementById('dhour').innerHTML = date_hours;
                                document.getElementById('dmin').innerHTML = date_minutes;
                                document.getElementById('dsec').innerHTML = date_seconds;
                                setTimeout("countdownsanit()", 1000);
                            }
                        }

                        countdownsanit();
                    </script>



