.wrap_fixed_updown.fixed {position:static;}
.right_aside .wrap_fixed_updown {right:-240px;}
.fixed_updown_btn {display:none;position:fixed;bottom:90px;z-index:1000;}
.fixed .fixed_updown_btn {right:50px;}
.fixed_updown_btn.type2 {bottom:374px;}
.fixed_updown_btn .btn_updown {display:block;overflow:hidden;width:75px;height:38px;background:url(http://omsh.kr/shop/img/updown.png) no-repeat;text-indent:-9999px; border:0px;}
.fixed_updown_btn .btn_up {background-position:0 0;}
.fixed_updown_btn .btn_up:hover {background-position:0 -80px;}
.fixed_updown_btn .btn_down {height:37px;background-position:0 -40px;}
.fixed_updown_btn .btn_down:hover {background-position:0 -120px;}

</style>
<script>
$(function() {
	var win = $(window);
	var fixedUpdownBtn = $('.fixed_updown_btn');
	if (win.scrollTop() > 500) {
		fixedUpdownBtn.fadeIn();
	}
	win.scroll(function() {
		if (win.scrollTop() > 500) {
			fixedUpdownBtn.fadeIn();
		} else {
			fixedUpdownBtn.fadeOut();
		}
	});
	$('.fixed_updown_btn .btn_up').click(function() {
		$('html, body').animate({
			scrollTop: 0
		}, 500,"easeOutQuint");
		//$('html, body').stop().animate({scrollTop:0},1000,"easeOutQuint");
		//win.scrollTop(0);
	});
	$('.fixed_updown_btn .btn_down').click(function() {
		var scrollDown = $('body, #wrap').height();
		var footerScrollTop = $('#ft').offset().top;
		var winHeight = win.height();
		if (winHeight < scrollDown) {
			$('html, body').animate({
				scrollTop: footerScrollTop
			}, 500,"easeOutQuint");
			//win.scrollTop(footerScrollTop);
		} else {
			/*$('html, body').animate({
				scrollTop: scrollDown
			}, 500);*/
			win.scrollTop(scrollDown);
		}
	});

	function fixedUpdown() {
		var wrapUpdown = $('.wrap_fixed_updown');
		if (win.width() < 2670) {
			wrapUpdown.addClass('fixed');
		} else {
			wrapUpdown.removeClass('fixed');
		}
	}
	fixedUpdown();
	win.resize(function() {
		fixedUpdown();
	});
});
</script>
<!-- top, down 버튼 END-->


<script>
	$(document).ready(function(){

		$('#to_top').click(function(){
			$('html, body').stop().animate({scrollTop:0},1000,"easeOutQuint");
		});
				
	});
</script>
<script src="http://omsh.kr/js/jquery.easing.min.js"></script>


<script src="http://omsh.kr/js/sns.js"></script>
