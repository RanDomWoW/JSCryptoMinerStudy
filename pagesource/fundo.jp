<!--Sticky Header-->
<script>
var $window = $(window), //ウィンドウを指定
$content = $("#main"), //#main部分
$change = $(".header-scroll"), //スクロール後に変わる部分
topContent = $content.offset().top; //#mainの位置を取得
 
var sticky = false;
 
 $window.on("scroll", function () {
      if ($window.scrollTop() > topContent) {　//scroll位置が#mainの上にある場合
           if ( sticky === false ){
                $change.slideDown();　//.header-scroll部分が上がる。
                sticky = true;
           }
      } else {
           if ( sticky === true ){　//scroll位置が下にある場合
                $change.slideUp();//.header-scroll部分が降りてくる。
                sticky = false;
           }
      }
 });
 $window.trigger("scroll");
</script>
