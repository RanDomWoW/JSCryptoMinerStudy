<script type="text/javascript">


  function email_offer_popup() {
 	
     
    // get all the inputs into an array.
    var $inputs = $('#homeemailpop :input');

   
    var values = {};
    $inputs.each(function() {
        values[this.name] = $(this).val();
    });

	
  if(values['email_emailp'].length > 0){
      
      var aEmail = values['email_emailp'];
      var fEmail = values['from_email'];
      var tEmail = values['to_email'];
    
      var promoCode = "0534656";/*1921_16070*/
      
      var emailValid = false;
      if( /(.+)@(.+){2,}\.(.+){2,}/.test(aEmail) ){
        emailValid = true;
      }
    
      if((aEmail.length <= 0) || (aEmail=="Enter Your Email Address Here") || (aEmail=="Enter Your Email for Instant Savings")) {
        $("#promomsgnewsltrerr").show();
        alert("Email address is required");
        return false;
      }
      else if (emailValid == false) {
        $("#promomsgnewsltrerr").show();
        alert("A valid email address is required");
        return false;
      }
      else {
        $("#promomsgnewsltrerr").hide();
        $.post("/promo_offer", { p: promoCode }, function(response){ });
        $.post("index.php", { action: "NewsletterRequest", to_email: tEmail, from_email: fEmail, add_email: aEmail, p: promoCode } );
        $.post("/newsletter", { method: "emailsignup", to_email: tEmail, from_email: fEmail, add_email: aEmail, location: "email_offer:solicitation:popup", p: promoCode } );
        UCX.LogContact("33D72149", "", aEmail, "", "email_offer::banner", promoCode);
        windowshade_setCookie('emailCollected','true',15);
        windowshade_setCookie('email',aEmail,15);
        $('#promomsgnewsltrcont').hide();
        $('#promomsgnewsltrthanks').show();   
        $('#emailpop_thanks').show();
        $('#emailpop_form').hide();
      
      // ensures the optimizely object is defined globally using window.optimizely. 
      window.optimizely = window.optimizely || []; 
      // sends a tracking call to Optimizely for the given event name. 
      // event name does not need to be 'pre-registered' 
      window.optimizely.push(['trackEvent', 'OfferPopupSignup']);
      
        return false;
      }
      return false;
      
    }
    return false;
  
  }
  //windowshade_setCookie('emailPopDisplayed','true',1);

</script>
