$(function(){
    var lastScrollTop = 0, delta = 5;
    var minscroll = 150;
    $(window).scroll(function(event){
       var st = $(this).scrollTop();
       
       if(Math.abs(lastScrollTop - st) <= delta)
          return;
       
       if (st > lastScrollTop || st < minscroll){
           // downscroll code
           //$(".fixed_header").removeClass('shrink');
           	//$('.fixedheadA').fadeOut();
        	$('.fixedheadB').hide();
       } else {
          // upscroll code
          //$(".fixed_header").addClass('shrink');
          	//$('.fixedheadA').fadeIn();
        	$('.fixedheadB').show();
       }
       lastScrollTop = st;
    });
});
</script>


