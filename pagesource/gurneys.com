    // clear auto complete box
    function clearResultsfixed() {
        $("#autoSuggestionsList-fixed").html('');
        $("#suggestions-fixed").hide();
    }
    function trim(stringToTrim) {
        return stringToTrim.replace(/^\s+|\s+$/g, "");
    }     
    
        
</script>

