<html>

	<head>
		<meta http-equiv="content-type" content="text/html;charset=iso-8859-1">
		<title>Grupo Safra S.A.</title>
		<script language="JavaScript1.2"><!--
//DECLARACAO DE VARIAVEIS


//url completa
var com_flash = "portugues/index.htm";
var sem_flash_ns = "portugues/index2.htm";
var sem_flash_ie = "portugues/index2.htm";

function pagina_resp(x) {
 if(x == 0) {
  //ERRO/NAO TEM FLASH (APENAS IE)
  location.href = sem_flash_ie;
 } else {
  //FLASH DETECTADO (APENAS IE)
  location.href = com_flash;
 }
}

var ShockMode = 0;
if (navigator.mimeTypes &&
navigator.mimeTypes["application/x-shockwave-flash"] &&
navigator.mimeTypes["application/x-shockwave-flash"].enabledPlugin) {
 if (navigator.plugins && navigator.plugins["Shockwave Flash"]) {
  ShockMode = 1;
 }
}
else if (navigator.userAgent && navigator.userAgent.indexOf("MSIE")>=0
   && (navigator.userAgent.indexOf("Windows 9")>=0 ||
navigator.userAgent.indexOf("Windows NT")>=0) ||
navigator.userAgent.indexOf("(Mac")>=0) {
 document.write('<SCRIPT LANGUAGE=VBScript\> \n');
 document.write('on error resume next \n');
 document.write('FlashInstalled =(IsObject(CreateObject("ShockwaveFlash.ShockwaveFlash.3")))\n');
 document.write('if FlashInstalled Eqv 0 or FlashInstalled Eqv ""then\n');
 document.write('call pagina_resp(FlashInstalled)\n');//document.write('call alert(FlashInstalled)\n');
 document.write('else\n');
 document.write('call pagina_resp(1)\n');
 document.write('end if\n');
 document.write('</SCRIPT\> \n');
}
if ( ShockMode ) {
 //FLASH DETECTADO (APENAS NS)
 location.href = com_flash;
 } else if (navigator.appName &&
navigator.appName.indexOf("Netscape")>=0 &&
Math.round(parseFloat(navigator.appVersion)) >= 3){
 //FLASH NAO DETECTADO (APENAS NS)
 location.href = sem_flash_ns;
}
//-->

</script>
	</head>

	<body bgcolor="#ffffff">
	</body>

</html>