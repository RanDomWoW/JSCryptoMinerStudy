<script>
    require(['jquery'],function ($) {
        var categoryRight = $('[data-category-all-right]'),
            categoryLeft = $('[data-category-all-left]'),
            categoryLeftSub = categoryRight.find('[data-category-cell-warp]'),
            categoryLeftLi = categoryLeft.find('li');

        categoryLeft.on('mouseenter','li',function () {
            categoryLeftSub.hide();
            categoryLeftLi.removeClass('on');
            $(this).addClass('on');
            categoryRight.find('#category-list-cell-'+$(this).attr('data-catid')).show();
        });

        var categoryBox = $('[data-category-warp-zindex]');
        $('[data-show-category-click]').on('click',function () {
            var time =0;
            if(categoryBox.css('display')=='block'){
                categoryBox.hide(time);
                $(this).html('全部分类');
            }else{
                categoryBox.show(time);
                categoryLeftLi.first().trigger('mouseenter');
                $(this).html('关闭');
            }
        });
        $(function () {
            categoryBox.css({'display':'none','top':'80px'});
        });
    });
</script>
