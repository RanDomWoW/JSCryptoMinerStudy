  <div id="dtrn-mid" class="container-fluid">
    <div class="container">
      <div class="row">
        <!-- Sua Habilitação -->
        <div id="dtrn-form-h" class="col-lg-4 col-md-4 col-sm-4 col-xs-12 form-group">

          <!-- Formulario de pesquisa de habilitação -->
          <div style="width: 100%; float: left;">
            <form action="https://internet.detrannet.mt.gov.br/habilitacao/consultaProcesso.asp" method="get" target="_blank">
              <div class="dtrn-vin text-size-acessibilidade">
                Consulte sua Habilitação
              </div>
              <div class="input-group">
                <input name="Renach" type="text" class="form-control dtrn-frm-main input_renach" id="input_renach" placeholder="Renach" required="required">
                <span class="input-group-btn">
                  <a class="btn btn-warning" href="javascript:void(0)" data-toggle="modal" data-target="#modalHabilitacao">
                    <i class="fa fa-question-circle" aria-hidden="true"></i>
                  </a>
                </span>
              </div>
              <div>
                <input name="CPF" type="text" class="form-control dtrn-frm-sec input_cpf" id="input_cpf" placeholder="CPF"  maxlength="11"  required="required">
              </div>
              <div>
                <input type="submit" class="dtrn-frm-sub dtrn-vin text-size-acessibilidade" value="Consultar">
              </div>
            </form>
          </div>

          <!-- Link de Processos CNH Online -->
          <div class="box-btn-processo-cnh text-center">
            <a class="text-center text-uppercase" href="http://www.mt.gov.br/web/mt/-/193445-consulta-condutor-online-" target="_blank">
              Processos de CNH On-line
            </a>
          </div>

        </div>

        <!-- Serviços Detran Online -->
        <div id="dtrn-header-forms" class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
          <div class="text-center">
            <a href="http://www.mt.gov.br/web/mt/-/193455-consulta-veiculo-online-" class="text-center text-uppercase" target="_blank">
              <span class="span-service-detran">
                <i class="fa fa-address-card-o" aria-hidden="true"></i>
              </span>
              Serviços Detran On-line
            </a>
          </div>
        </div>

        <!-- Consulta de Veiculo -->
        <div id="dtrn-form-v" class="col-lg-4 col-md-4 col-sm-4 col-xs-12 form-group">
          <form action="https://internet.detrannet.mt.gov.br/ConsultaVeiculo.asp" method="get" target="_blank" style="width: 100%; float: left;">
            <div class="dtrn-vin text-size-acessibilidade">
              Consulte Seu Veículo
            </div>
            <div class="input-group">
              <input name="Placa" type="text" class="form-control dtrn-frm-main input_placa" placeholder="Placa" id="input_placa" maxlength="7" required="required" />
              <span class="input-group-btn">
                <a class="btn btn-warning" href="javascript:void(0)" data-toggle="modal" data-target="#modalVeiculo">
                  <i class="fa fa-question-circle" aria-hidden="true"></i>
                </a>
              </span>
            </div>
            <div>
              <input name="Renavam" type="text" class="form-control dtrn-frm-sec input_renavam" placeholder="Renavam" id="input_renavam" pattern="[0-9]+$" required="required" />
            </div>
            <div>
              <input type="submit" class="dtrn-frm-sub dtrn-vin text-size-acessibilidade" value="Consultar">
            </div>
          </form>

          <div class="box-btn-processo-cnh text-center">
            <a class="text-center text-uppercase" href="http://www.mt.gov.br/web/mt/-/192975-emitir-licenciamento-anual-online-" target="_blank">
              Receba seu documento em casa
            </a>
          </div>
        </div>
      </div>

