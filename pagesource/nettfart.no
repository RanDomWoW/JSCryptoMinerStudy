<script type="text/javascript">

$(document).ready(function() {
	
	var text = "<h4>Målinger med mobilt bredbånd</h4><br/>Målinger med mobilt bredbånd vil gi mer variable resultater enn målinger med fast bredbånd. Dette kan skyldes:<br/><ul><li>Typen mobilt nettverk du er tilkoblet (forskjellige nett har varierende kapasitet, avhengig av geografisk dekning)</li><li>Antall samtidige brukere i samme område (du kan forvente redusert kapasitet når det er høy belastning i nettet)</li><li>Ulendt terreng, på grunn av fjell, daler eller høye bygninger</li><li>Hvor god dekning det er der du er</li></ul>";
	$('#mobile-broadband-help-bt').NPTtooltip({
		text: text,
		position: {corner: {target: 'rightMiddle', tooltip: 'leftMiddle'}},
		corner: 'leftMiddle'
	});
	
	var text = "Tiden det tar å utveksle informasjon mellom din nettleser og måleserver";
	$('#latency-help-bt').NPTtooltip({text: text});
	
	var text = "Se informasjon fra din tilbyder (f.eks siste faktura/din side), eller kontakt tilbyders brukerstøtte.";
	$('#product-help-bt').NPTtooltip({text: text});

	$('select').sb({fixedWidth: true, optionFormat: function() {
		var comboId = $(this).parent().attr('id');
		return '<span class="' + comboId + '-option-id-' +this.value+ '">' +this.text+ '</span>'; 
	}});

	});



 // Cross-browser event handler
 function listenToEvent (element, ev, fn){
   // Non IE
   if (element.addEventListener) {
     element.addEventListener(ev, fn, false);
   // IE
   } else if (element.attachEvent) {
     element.attachEvent('on' + ev, fn);
   }
 }

 function handleResultData (event){
   // Only allow messages from the correct speedtest custom url to prevent XSS attacks.
   // https://developer.mozilla.org/en-US/docs/Web/API/Window/postMessage
   // REPLACE THE URL BELOW WITH YOUR SPECIFIC SPEEDTEST CUSTOM TEST URL
   if (event.origin !== 'http://nettfart.speedtestcustom.com') {
     Return;
   }

   // event.data is the resultData object
   // REMOVE THIS LINE AND ADD YOUR DATA CONSUMER HERE

   //console.log(event.data);
   test_completed(event.data);

  
   $("#save_product").click(function(){
		saveProduct(event.data);
	});
   
 }

 // Attach the event handler for 'message' to window
 listenToEvent(window, 'message', handleResultData);
