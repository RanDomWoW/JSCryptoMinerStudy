<script type="text/javascript">
var str = new Array();
var str_1 = new Array();
var $con = $("#newslist_1");
var _time=[],_year=[],_month=[],_day=[],time=[];
function sortNum(a,b){
	return b[0]-a[0];
}
function sortData(dataSource){
	var init = function(){
		for(var i=0;i<dataSource.length;i++){
			str[i] = new Array();
			_time[i] = dataSource[i][0].replace("[","").replace("]","").split("-");
			_year[i] = _time[i][0];
			_month[i] = _time[i][1];
			_day[i] = _time[i][2];
			time[i] = _year[i].concat(_month[i]).concat(_day[i]);
			str[i][0] = time[i];
			str[i][1] = dataSource[i][1];
			str[i][2] = dataSource[i][2];
			str[i][3] = dataSource[i][3];
		}
		str.sort(sortNum);

		for(var j = 0; j<dataSource.length;j++){
			str_1[j] = new Array();
			var _f = "[";
			var _l = "]";
			time[j] = str[j];
			var year = _f.concat(time[j][0].substring(0,4).concat("-"));
			var month = year.concat(time[j][0].substring(4,6).concat("-"));
			str_1[j][0] = month.concat(time[j][0].substring(6,8).concat(_l));
			str_1[j][1] = time[j][1];
			str_1[j][2] = time[j][2];
			str_1[j][3] = time[j][3];
			$con.append('<tr><td><a target="_blank" href="'+str_1[j][2]+'"><img src="http://images.mmosite.com/www/new/tag1.gif">'+str_1[j][1]+'</a> <em>'+str_1[j][0]+'</em><p>'+str_1[j][3]+'</p></td></tr>');
		}
	}
	init();
};

var td_data = new Array();

var _tr = document.getElementById("newslist").getElementsByTagName("tr");
var _td = [],_tdurl=[],_tdtitle=[],_tddec=[];

for(var m=0;m<_tr.length;m++){
	td_data[m] = new Array();
	td_data[m][0] = $($(_tr[m]).find("td")[0]).find("em").html();
	td_data[m][1] = $($(_tr[m]).find("td")[0]).find("a").html();
	td_data[m][2] = $($(_tr[m]).find("td")[0]).find("a").attr("href");
	td_data[m][3] = $($(_tr[m]).find("td")[0]).find("p").html();
}
sortData(td_data);
</script>
