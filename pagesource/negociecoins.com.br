        var brokerUrl = "https://broker.negociecoins.com.br";

        function precoBTC() {
            $.ajax({
                type: "GET",
                url: brokerUrl + "/api/v3/btcbrl/ticker",
            }).done(function (data) {
                $("#precoBTC").html("<b>BTC R$ " + data.last.toFixed(0) + "</b>");
                $("#unidadeBTC").html("<b> R$ " + data.last.toFixed(0) + "</b>");
                $("#compra").html("<b> R$ " + data.buy.toFixed(0) + "</b>");
                $("#venda").html("<b> R$ " + data.sell.toFixed(0) + "</b>");
                $("#alta").html("<b> R$ " + data.high.toFixed(0) + "</b>");
                $("#baixa").html("<b> R$ " + data.low.toFixed(0) + "</b>");
                $("#volume").html("<b> BTC " + data.vol.toFixed(0) + "</b>");

                var date = new Date(data.date * 1000);
                // hours part from the timestamp
                var hours = (date.getHours() <= 9 ? "0" : "") + date.getHours();
                // minutes part from the timestamp
                var minutes = (date.getMinutes() <= 9 ? "0" : "") + date.getMinutes();
                // seconds part from the timestamp
                var seconds = (date.getSeconds() <= 9 ? "0" : "") + date.getSeconds();

                var dia = (date.getDate() <= 9 ? "0" : "") + date.getDate();
                var mes = (date.getMonth() + 1 <= 9 ? "0" : "") + (date.getMonth() + 1);
                var ano = date.getFullYear();

                var formattedTime = dia + '/' + mes + '/' + ano + ' ' + hours + ':' + minutes + ':' + seconds;

                $("#atualizacao").html("<b> " + formattedTime + "</b>");


            });

        }
    </script>
