<script type="text/javascript">
    $(document).ready(function () {
        GetUsersLocation()        
    });
    function GetUsersLocation() {
        if (navigator.geolocation) {
            navigator.geolocation.getCurrentPosition(GetNearestStoreByCurrentLocation, function (error) {
                switch (error.code) {
                    case error.TIMEOUT:
                        $(".neareststorewrapper").hide();
                        break;
                    case error.POSITION_UNAVAILABLE:
                        $(".neareststorewrapper").hide();
                        break;
                    case error.PERMISSION_DENIED:
                        $(".neareststorewrapper").hide();
                        
                        LogUserInput(true)
                         
                       
                        break;
                    case error.UNKNOWN_ERROR:
                        $(".neareststorewrapper").hide();
                        break;
                }
            }, { 'enableHighAccuracy': true, 'timeout': 15000 });
        } else {
            $(".divStoreDetails").hide();

        }
    }

    function GetNearestStoreByCurrentLocation(position) {
        var latitude = position.coords.latitude;
        var longitude = position.coords.longitude;

        $.ajax({
            type: "POST",
            url: "/Default.aspx/GetNearestStoresByCurrentLocation",
            data: "{'Latitude' : '" + latitude + "' , 'Longitude': '" + longitude + "'}",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                if (response.d != "null") {
                    var JsonObj = $.parseJSON(response.d);
                    if (JsonObj._Status == "OK") {
                        var Store = JsonObj._Data;
                        populateStoreInfo(JsonObj._Data);
                    }
                    else {$(".neareststorewrapper").hide();}
                }
                else {$(".neareststorewrapper").hide();}
            },
            failure: function (msg) {
            }
        });

    }

    function populateStoreInfo(Store) {
        if (Store.StoreAddress1.length > 0 && Store.StoreAddress2.length > 0 && Store.ZipCode.length > 0) {
            $(".address1").html((Store.StoreAddress1.length > 49 ? Store.StoreAddress1.substring(0, 50) + '...' : Store.StoreAddress1) + ".");
            $(".address2").html(Store.StoreAddress2.length > 29 ? Store.StoreAddress2.substring(0, 30) + '...' : Store.StoreAddress2);
            $(".storelink").attr("href", 'https://www.ediblearrangements.com/' + "Stores/StoreLocator.aspx?q=" + Store.ZipCode + "&currloc=false");
            $(".neareststorewrapper").show();
        }
        else { $(".neareststorewrapper").hide(); }
    }

    function LogUserInput(IsBlocked)
    {

        $.ajax({
            type: "POST",
            url: "/Default.aspx/LogUserInteractionWithGeolocation",
            data: "{'IsBlocked' : '" + IsBlocked + "'}",
            contentType: "application/json; charset=utf-8",
            dataType: "json"            
        });

    }

</script>
