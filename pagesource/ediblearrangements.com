        <script type="text/javascript">
            var keywordManagement = false;
            var tempKeywordTable = null;
            var PreviousID = "";
            var PreviousLocation = "";
            var NewLocation = "";
            var objArrangements;
            var GSXHR;
            var _timer;
            function EscapeChars(txt) {
                var escapedName = txt.replace(/\\['\\]|'/g, function (s) {
                    if (s == "'") return "&apos;";
                    else return s;
                });
                return escapedName;
            }
            function searchArrangement(searchText) {
                searchText = searchText.replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;');
                if (searchText == '' || searchText == $("#txtArrangementSearch").attr("placeholder")) {
                    $("#txtArrangementSearch").css("border", "solid 1px #E21836");
                }
                else {
                    if( $("#divSuggestion ul li").length > 0){
                        if( $("#divSuggestion ul li.hoverClass").length > 0){
                            searchText = $("#divSuggestion ul li.hoverClass").text();
                        }                      
                    }
                }
                if (keywordManagement == true) {
                    var id = $("#divSuggestion ul li.hoverClass").attr("id");
                    if (id != undefined && id > -1) {
                        ShowUrlAndFaq(id);
                    }
                    else if (!IsFaqOrUrl()) {
                        RedirectToUnSuggestedKeywords(searchText);
                    }
                }
                else {
                    RedirectToUnSuggestedKeywords(searchText);
                }
                }
            function RedirectToUnSuggestedKeywords(searchText){
                if (!(searchText.match(/^\d+$/) && searchText.length == 4)){
                    if ($("#divSuggestion ul li").length > 0){
                        location.href = encodeURI('https://www.ediblearrangements.com/fruit-arrangements?SearchText=' + encodeURIComponent(searchText));
                    }
                    else  if(!IsFaqOrUrl()){
                        location.href = encodeURI('https://www.ediblearrangements.com/fruit-arrangements?SearchText=' + encodeURIComponent(searchText));
                    }
                }
                else {
                    location.href = encodeURI('https://www.ediblearrangements.com/fruit-arrangements?SearchText=' + encodeURIComponent(searchText));
                }
            }         
            function SearchByButton() {
                if ($("#txtArrangementSearch").val() == "" || $("#txtArrangementSearch").val() == $("#txtArrangementSearch").attr("placeholder")) {
                    $("#txtArrangementSearch").css("background-color", "#FFDDDD");
                    $("#txtArrangementSearch").focus();
                }
                else {
                    var DefaultCountryID = '1';
                    var regExpAreaUS = /^\d{5}(-\d{4})?$/;
                    var regExpAreaCan = /(^[ABCEGHJKLMNPRSTVXYabceghjklmnprstvxy]{1}\d{1}[A-Za-z]{1} *\d{1}[A-Za-z]{1}\d{1}$)/;                    
                    if (DefaultCountryID == 1) {
                        if (regExpAreaUS.test($("#txtArrangementSearch").val()) == true) {
                            window.location = encodeURI("https://www.ediblearrangements.com/Stores/StoreLocator.aspx?q=" + encodeURIComponent($("#txtArrangementSearch").val()) + "&currloc=false");
                        }
                        else if ($("#txtArrangementSearch").val().match(/^\d+$/) && $("#txtArrangementSearch").val().length == 4 && $("#divSuggestion ul li.hoverClass").length == 0)
                        {
                            location.href = encodeURI('https://www.ediblearrangements.com/fruit-arrangements?SearchText=' + encodeURIComponent($("#txtArrangementSearch").val()));
                        }
                        else {
                            searchArrangement($('#txtArrangementSearch').val());
                        }
                    }
                    else if (DefaultCountryID == 2) {
                        if (regExpAreaCan.test($("#txtArrangementSearch").val()) == true) {
                            window.location = encodeURI("https://www.ediblearrangements.com/Stores/StoreLocator.aspx?q=" + encodeURIComponent($("#txtArrangementSearch").val()) + "&currloc=false");
                        }
                        else if ($("#txtArrangementSearch").val().match(/^\d+$/) && $("#txtArrangementSearch").val().length == 4 && $("#divSuggestion ul li.hoverClass").length == 0)
                        {
                            location.href = encodeURI('https://www.ediblearrangements.com/fruit-arrangements?SearchText=' + encodeURIComponent($("#txtArrangementSearch").val()));
                        }
                        else {
                            searchArrangement($('#txtArrangementSearch').val());
                        }
                    }                    
                }
            }
            
            $("#txtArrangementSearch").keyup(function (e) {
                clearTimeout(_timer);
                var code = (e.keyCode ? e.keyCode : e.which);
                var txtSearch = $("#txtArrangementSearch");
                txtSearch.removeClass("validatorTxt");
                txtSearch.css("background-color", "");
                if (code == 13) {
                    if (txtSearch.val() == "") {
                        txtSearch.addClass("validatorTxt");
                        txtSearch.css("background-color", "#FFDDDD");
                        return false;
                    } else {
                    HideSuggestion();
                    $("#imgbtnArrangementSearch").click();
                        return false;                     
                    }
                }
                else if (code == 40 || code == 38) {
                    txtSearch.focus();
                    var Elems = $("#divSuggestion ul li");
                    var total = Elems.length;
                    var i = 0;
                    if (total > 0) {
                        var selected = $("#divSuggestion ul li.hoverClass");
                        if (selected.length == 0) {
                            if (code == 40) {
                                $($("#divSuggestion ul li")[0]).css("padding", "2px 7px 2px 15px");
                                $($("#divSuggestion ul li")[0]).addClass("hoverClass");
                                txtSearch.val($($("#divSuggestion ul li")[0]).text());
                            } else if (code == 38) {
                                var lastElm = $("#divSuggestion ul li");
                                $((lastElm)[lastElm.length - 1]).css("padding", "2px 7px 2px 15px");
                                $((lastElm)[lastElm.length - 1]).addClass("hoverClass");
                                txtSearch.val($((lastElm)[lastElm.length - 1]).text());
                            }
                        } else {
                            $(Elems).each(function () {
                                selected.removeClass("hoverClass");
                                selected.css("padding", "4px 7px 4px 15px");
                                if (code == 40) {
                                    var next = $(selected.next());
                                    if (next.length == 0) {
                                        txtSearch.val(txtSearch.attr("SearchText"));
                                    }
                                    else {
                                        next.css("padding", "2px 7px 2px 15px");
                                        next.addClass("hoverClass");
                                        txtSearch.val(next.text());
                                    }
                                } else if (code == 38) {
                                    var prev = $(selected.prev());
                                    if (prev.length == 0) {
                                        txtSearch.val(txtSearch.attr("SearchText"));
                                    }
                                    else {
                                        prev.css("padding", "2px 7px 2px 15px");
                                        prev.addClass("hoverClass");
                                        txtSearch.val(prev.text());
                                    }
                                }

                            });
                        }
                    }
                    txtSearch.focus();
                } else {
                    txtSearch.attr("SearchText", txtSearch.val());
                    if (txtSearch.val() != '') {
                        setTimeout("ShowKeywords()", 10);
                    }
                    else{
                        HideSuggestion();
                    }
                }
            });
            function ShowKeywords() {
                if (tempKeywordTable == null) {
                    $.ajax({
                        url: encodeURI('https://www.ediblearrangements.com/ajax/searchcatalog.aspx'),
                        success: function (data) {
                            var JsonObj = $.parseJSON(data);
                            if (JsonObj._Status == "OK") {
                                tempKeywordTable = JsonObj._Data;
                            }
                        },
                        async: false
                    });
                }
                var txtSearch = $("#txtArrangementSearch").val().trim().toLowerCase();
                if (tempKeywordTable != null) {
                    keywordManagement = false;
                    var container = $("#divSuggestion ul");
                    container.empty();
                    for (i = 0; tempKeywordTable.length > i; i += 1) {
                        if (tempKeywordTable[i].KeywordName.toLowerCase().indexOf(txtSearch) > -1) {
                            var txt = $("#txtArrangementSearch").val().trim().replace(/\s{2,}/g, ' ').replace('\\', ' ');
                            var splitTxt = txt.split(' ');
                            var suggestion = null;
                            suggestion = tempKeywordTable[i].Name;
                            var replacement = "***$&###";
                            for (var j = 0; j < splitTxt.length; j++) {
                                var regex = new RegExp('(' + splitTxt[j].split('\\').join('\\\\') + ')', 'i');
                                suggestion = suggestion.replace(regex, replacement);
                            }
                            var reg2 = new RegExp('(###)', 'gi');
                            suggestion = suggestion.replace(/\*{3}/g, "<span style='color:#000;'>").replace(/#{3}/g, "</span>");
                            var elm = $("<li style='cursor:pointer;' id='" + i + "'>" + suggestion + "</li>");
                            elm.hover(function () {
                                var e = $("#divSuggestion ul li");
                                $(e).each(function () {
                                    $(this).css("padding", "4px 7px 4px 15px");
                                    $(this).removeClass("hoverClass");
                                });
                                $(this).css("padding", "2px 7px 2px 15px");
                                $(this).addClass("hoverClass");
                            });
                            elm.click(function () {
                                $("#txtArrangementSearch").val($(this).text());
                                $("#txtArrangementSearch").attr("SearchText", $("#txtArrangementSearch").val());
                                $("#divSuggestion ul").empty();
                                HideSuggestion();
                                ShowUrlAndFaq($(this).attr("id"));
                            });
                            keywordManagement = true;
