    $(document).ready(function() {
        $("#txtMailingListEmail").keydown(function(e) {
            var code = (e.keyCode ? e.keyCode : e.which);
            if (code == 13) {
                SubscribeToEmail();
            }
            else {
            $(this).removeClass("validatorTxt");
            $(this).css("background-color","");
            }
    });
    $("#txtPhoneList").keydown(function (e) {
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 13) {
            SubscribeToPhone();
        }
        else {
            $(this).removeClass("validatorTxt");
            $(this).css("background-color", "");
        }
    });
    }); 
    function SubscribeToEmail() {
        var emailAddress = trim($("#txtMailingListEmail").val());
        var phoneNumber = '';
        var msg = "";
        if (emailAddress == "" || emailAddress == ('Your email address'))
            msg = 'Enter email address.';
        else if (emailAddress != "" && emailAddress != ('Your email address') && (isEmailFooter(emailAddress) == false))
            msg = 'Enter a valid email address.';        
        if (msg != "") {
            $("#txtMailingListEmail").css("background-color", "#FDD");
            $("#txtMailingListEmail").addClass("validatorTxt");
        }
        else {
            if (emailAddress == ('Your email address'))
                emailAddress = "";
            var img = new Image();
            img.src = "https://www.ediblearrangements.com/Subscribe.aspx?EmailAddress=" + emailAddress + "&PhoneNumber=" + phoneNumber + "&Section=1";
            $("#txtMailingListEmail").val('');
            ShowSubcribeModalPopup();
        }
        return false;
    }
    function SubscribeToPhone() {
        var emailAddress = '';
        var phoneNumber = trim($("#txtPhoneList").val());
        if ((phoneNumber == "" || phoneNumber == ('Your cell phone #')) || (phoneNumber.length <= 9 && phoneNumber.length <= 17) || (hasNumbers(phoneNumber) < 10) || (!ValidateMgagePhoneNumber($("#txtPhoneList")))) {
            $("#txtPhoneList").css("background-color", "#FDD");
            $("#txtPhoneList").addClass("validatorTxt");
            return false;
        }
        if (!($("#chkPhoneTerms").is(':checked'))) {
            $("#agreephonevalidator").css("display", "block");
            $("#chkPhoneTerms").addClass("error");
            $("#terms").css("margin-top", "");
            return false;
        }
        var img = new Image();
        img.src = "https://www.ediblearrangements.com/Subscribe.aspx?EmailAddress=" + emailAddress + "&PhoneNumber=" + phoneNumber + "&Section=1";
        $("#txtPhoneList").val('');
        $("#agreephonevalidator").css("display", "none");
        $("#chkPhoneTerms").attr('checked', false).removeClass("error");
        ShowPhoneModalPopup();
        return false;
    }
    function hasNumbers(t) {        
       var myString = t;
       var count = 0;
       for (var i = 0; i < myString.length; i++) {
           var c = myString.charAt(i);          
           if (c >= '0' && c <= '9') {
               count++;
           }
       }
       return count;
    }
    function isEmailFooter(str) {
        return /^[a-zA-Z0-9\.\-\_]+\@[a-zA-Z0-9](?:[\.\-]{0,1}[a-zA-Z0-9]+)*\.[a-zA-Z]{2,4}$/.test(str);
    }
    function trim(sInput) {
        return sInput.replace(/^ +/, "").replace(/ +$/, "");
    }
    function ShowSubcribeModalPopup() {
        $.ajax({
            type: "GET",
            url: "https://www.ediblearrangements.com/Ajax/PageUtil.aspx?ThankYouEmailPopup=1",
            success: function (response) {
        DisableBackground();
                $("body").append($(response));
        $("#divThanksEmailFooter").center();
                $("#divThanksEmailFooter").css("display","block");
        $("#divThanksEmailFooter").css("top", getScrollTop() + 250);
        $("#txtMailingListEmail").blur();
    }
        });
    }
    function ShowPhoneModalPopup() {
        $.ajax({
            type: "POST",
            url: "https://www.ediblearrangements.com/Ajax/PageUtil.aspx?ThankYouMobilePopup=1",
            success: function (response) {
        DisableBackground();
                $("body").append($(response));
        $("#divThanksPhoneFooter").center();
        $("#divThanksPhoneFooter").css("display", "block");
        $("#divThanksPhoneFooter").css("top", getScrollTop() + 250);
                $("#txtMailingListEmail").blur();
            }
         });
    }
    function HideSubcribeModalPopup() {
        EnableBackground();
        $("#divThanksEmailFooter").remove();
    }
    function HidePhoneModalPopup() {
        EnableBackground();
        $("#divThanksPhoneFooter").remove();
    }
