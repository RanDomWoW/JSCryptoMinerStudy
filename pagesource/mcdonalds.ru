<script>
  var increment;
  var animationRotationBall = new TimelineMax();
  function processDot(e) {
    var x = e.clientX;
    var y = e.clientY;
    TweenLite.to(animationRotationBall, .5, {timeScale:1});
    [].forEach.call(document.querySelectorAll(".fifa_cursor-ball"), function(val, index) {
      increment = 15;
      TweenMax.to(val, .5, {x: x+increment, y: y+increment, delay: 0.1*index});
    });
  }
  animationRotationBall.staggerTo(".fifa_cursor-ball .i-svg", 1, {rotation: 360, repeat: -1, ease: Power0.easeNone}, .2);
  document.addEventListener("mousemove", processDot);
  document.onmousemove = (function() {
    var onmousestop = function() {
      TweenLite.to(animationRotationBall, 1, {timeScale:0});
    }, thread;
    return function() {
      clearTimeout(thread);
      thread = setTimeout(onmousestop, 800);
    };
  })();
</script>


