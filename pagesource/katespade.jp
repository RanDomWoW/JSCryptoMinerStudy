        success: function (result) {
          if (result != '') {
            var data = eval("(" + result + ")");
          }
          if (data.result == "error") {
            if (data.msg == 2) {
              $.facebox({ div: '#kiyaku_wrap4' });
            } else if (data.msg == 3) {
              $.facebox({ div: '#kiyaku_wrap5' });
            } else if (data.msg == 4) {
              $.facebox({ div: '#kiyaku_wrap6' });
            } else {
              if (data.msg.match("入力形式")) {
                $.facebox({ div: '#kiyaku_wrap1' });
              } else if (data.msg.match("使用できない")) {
                $.facebox({ div: '#kiyaku_wrap2' });
              } else if (data.msg.match("入力されて")) {
                $.facebox({ div: '#kiyaku_wrap3' });
              } else {
                document.getElementById("errorMessage").innerHTML = "<h1>" + data.msg + "</h1>";
                $.facebox({ div: '#kiyaku_wrap3' });
              }
            }
          } else {
            $.facebox({ div: '#kiyaku_wrap7' });
          }
          g_joinas_request = null;
        }
      });
    }
//]]>
</script>

