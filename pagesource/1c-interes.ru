      $('#auth_from').submit(function(){

        var $this = $(this);
        $this.find('#button_enter').attr('disabled','disabled');
        $this.find('#login_loader').show(); 
        $this.find('input.error').removeClass('error');
        $this.find('span.input-placeholder-text').css('color','#636363');
       	
       	$this.find('#err_login').html(" ");
    	$this.find('#err_reg').html(" ");
         var $form = {
            action: $this.attr('action'),
            post: {'ajax_key':'b1e469c726d7e423034efa56d61dc484'}
         };
      
       $.each($this.find('input'), function(){
             
            if ($(this).attr('name')) {
            	 if($(this).attr('type')=="checkbox" || $(this).attr('type')=="radio"){ 
                	
                   if($(this).attr('checked')=='checked')
                    $form.post[$(this).attr('name')] = $(this).val();
                   }
            	 else{
            		 $form.post[$(this).attr('name')] = encodeURIComponent($(this).val());
                  }
            }
         });
    
         $.post($form.action, $form.post, function(xml){ 
              $('input', $this).removeAttr('disabled');
            
                var type = jQuery(xml).find('type').text();
                var form_action = jQuery(xml).find('action').text();
                if (type != 'merge_accounts') {
                	$this.find('#login_loader').hide();
                }
               
            if (type == 'error') {
            	if(form_action == "AUTH"){
            	$this.find('input[name="USER_LOGIN"]').addClass('error');
            	$this.find('input[name="USER_PASSWORD"]').addClass('error');
               
            	$this.find('input[name="USER_PASSWORD"]').prev('span.input-placeholder-text').css('color', 'red');
            	$this.find('#err_login').html('<strong class="error-text">'+jQuery(xml).find('message').text()+'</strong>');
            	}
                if(form_action == "NEW_USER_REGISTR"){
                	$this.find('input[name="REG_LOGIN"]').addClass('error');
                	$this.find('input[name="REG_PASSWORD"]').addClass('error');
                	$this.find('input[name="REG_PASSWORD"]').prev('span.input-placeholder-text').css('color', 'red');
                	$this.find('#err_reg').html('<strong class="error-text">'+jQuery(xml).find('message').text()+'</strong>');
                }
            } else if (type == 'merge_accounts') {
            	$.ajax({
        			url: '/personal/merge_accounts.php',
        			success: function(body) {
        				$.fancybox({
        					content: body
        				});
        				
        				$this.find('#login_loader').hide();
        			}
        		});
            } else {
                var login = $this.find('input[name="USER_LOGIN"]').val();
                if(login){
                    (window["rrApiOnReady"] = window["rrApiOnReady"] || []).push(function()
                        {
                            rrApi.setEmail(login);
                        }
                    );
                }
                setTimeout(function () {
                    window.location.reload();
                }, 100);
            };
         },
         
         'xml').fail(function(d) {
        	    console.log(d)
         }); 
         return false;
      });
      
</script>
