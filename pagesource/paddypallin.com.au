function injectFormKey(form_key)
{
    $$('a','button','form','input[name="form_key"]').each(function(element) {
        var newAttributeValue = form_key;
        var oldAttributeValue = false;
        var targetType = false;
        var targetAttribute = false;
        var targetAttrCondition = false;
        var attributeValue = "";

        if(element.tagName == 'A' ||
            element.tagName == 'BUTTON' ||
            element.tagName == 'FORM') {

            if (element.tagName == 'A') {
                targetAttribute = 'href';
                attributeValue = element.href;
            }
            else if (element.tagName == 'BUTTON') {
                // TODO: onclick seems to be not accessible without
                // executing the function within the attribute.
                // Skipping for now.
                return;

                targetAttribute = 'onclick';
                attributeValue = element.readAttribute('onclick');
            }
            else if (element.tagName == 'FORM') {
                targetAttribute = 'action';
                attributeValue = element.action;
            }

            // attribute values resembling URLs with form_key in it
            targetAttrCondition = (attributeValue.indexOf('form_key') !== -1);
            targetType = 'url';

        } else if (element.tagName == 'INPUT') {
            targetAttrCondition = true; // always true for name="form_key"
            targetAttribute = 'value';
            attributeValue = element.value;
            targetType = 'value';
        }

        if(!targetAttribute || !targetAttrCondition)
            return;

        if(targetType == "url" && attributeValue.match("form_key\/.*?\/")) {
            var target = attributeValue.match("form_key\/.*?\/");
            oldAttributeValue = target[0].replace(/\/$/, "");
            newAttributeValue = attributeValue.replace(oldAttributeValue, newAttributeValue);
        } else if(targetType == "value") {
            oldAttributeValue = attributeValue;
            newAttributeValue = newAttributeValue.replace('form_key\/', "");
        } else {
            return;
        }

        if (attributeValue != newAttributeValue) {
            element.setAttribute(targetAttribute, newAttributeValue);
        }
    });
}

