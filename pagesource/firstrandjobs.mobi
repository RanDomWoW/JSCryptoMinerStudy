                <script src="/Content/kendo/dataviz/map/js/chroma.min.js"></script>

                <script>
                    var scale = chroma.scale(["#C8C6BA", "#FECB00"]).domain([1, 1000]);

                    function onShapeCreated(e) {
                        var shape = e.shape;
                        var users = shape.dataItem.properties.users;
                        if (users) {
                            var color = scale(users).hex();
                            shape.options.fill.set("color", color);
                        }
                    }

                    function MarkerActivate(e) {
                        $('#divMap .k-map .k-layer').first().css('z-index', '4');
                    }

                    function onShapeClick(e) {
                        var shape = e.shape;
                        var name = shape.dataItem.properties.name;
                        var id = name.replace(' ', '-').replace('&', '-').replace(',', '-');

                        $('#countries option').each(function () {
                            //alert('id=' + id + ', country = ' + $(this).text());
                            if ($(this).text() == id.replace('-', ' ')) {
                                showLoader();
                                $('.cd-main-content').css('padding-top', '0px');
                                clearCache('/Jobs/cacheSearchCriteria');
                                window.location = '/Jobs/Search?Country=' + id + '&startIndex=1&containerOnly=false';
                            }
                        });


                    }

                    function onMarkerClick(e) {
                        var marker = e.marker;
                        var name = marker.options.title;
                        var id = name.replace(' ', '-').replace('&', '-').replace(',', '-');
                        showLoader();
                        $('.cd-main-content').css('padding-top', '0px');
                        clearCache('/Jobs/cacheSearchCriteria');
                        window.location = '/Jobs/Search?Country=' + id + '&startIndex=1&containerOnly=false';
                    }

                    function onShapeMouseEnter(e) {
                        var shape = e.shape;
                        var users = shape.dataItem.properties.users;
                        if (users > 1) {
                            e.shape.options.set("fill.opacity", 1);
                        }
                    }

                    function onShapeMouseLeave(e) {
                        var shape = e.shape;
                        var users = shape.dataItem.properties.users;
                        if (users > 1) {
                            e.shape.options.set("fill.opacity", 0.7);
                        }
                    }

                   

                </script>
