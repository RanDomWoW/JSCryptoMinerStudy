    <script>
        function Resize() {
            var cw = parseInt(0);
            cw = parseInt(document.body.clientWidth);
            var count = 0;
            var cl = '';

            var obj = $('#professionalareas');
            var len = obj.children().length;

            if (obj != null && len > 0) {
                //alert('cw=' + cw);
                if (cw > parseInt('659')) {

                    $('#professionalareas > div').each(function (index, element) {

                        if ((count % 2) == 0) {
                            if (cl == '') {
                                cl = 'right';
                            } else {
                                if (cl == 'right') {
                                    cl = 'left';
                                } else {
                                    cl = 'right';
                                }
                            }
                        }

                        if (cl != '') { $(element).children().each(function () { $(this).removeClass('left').removeClass('right').addClass(cl); }); }
                        count++;
                    });

                } else if (cw <= parseInt('659')) {

                    $('#professionalareas > div').each(function (index, element) {

                        if ((count % 2) == 0) {
                            cl = 'right';
                        } else {
                            cl = 'left';
                        }

                        if (cl != '') { $(element).children().each(function () { $(this).removeClass('left').removeClass('right').addClass(cl); }); }
                        count++;
                    });
                }
            }

            $("#map").data("kendoMap").resize();
            //if (cw <= 659) {
            //    var map = $("#map").data("kendoMap");
            //    map.center([-36.140904, 34.774989]).zoom(10);
            //    map.resize();
            //}
            
        };

        $('.tile_section_centered .pictureTextTile .textTile').on('mouseenter', function () {

            var id = $(this).attr('id');
            $('#' + id).addClass('hovering');//.css('opacity', '1')

            var pid = $('#' + id + ' .arrow').first().attr('id');
            if ($(this).hasClass('left')) {
                $('#' + pid).css('border-right-color', $('#' + id).css('background-color'));//arrowhovering
            } else if ($(this).hasClass('right')) {
                $('#' + pid).css('border-left-color', $('#' + id).css('background-color'));//arrowhovering
            }

        });

        $('.tile_section_centered .pictureTextTile .textTile').on('mouseleave', function () {

            var id = $(this).attr('id');
            $('#' + id).removeClass('hovering');

            var pid = $('#' + id + ' .arrow').first().attr('id');

            if ($(this).hasClass('left')) {
                $('#' + pid).css('border-right-color', $('#' + id).css('background-color'));//arrowhovering
            } else if ($(this).hasClass('right')) {
                $('#' + pid).css('border-left-color', $('#' + id).css('background-color'));//arrowhovering
            }

        });

        $('.tile_section_centered .pictureTextTile .pictureTile').on('mouseenter', function () {

            var id = $(this).attr('id');

            var sib = $('#' + id).siblings('.textTile');
            if (sib != null) {
                $('#' + sib.attr('id')).addClass('hovering');//.css('opacity', '1'); }

                var pid = $('#' + sib.attr('id') + ' .arrow').first().attr('id');
                if ($('#' + pid).parent().hasClass('left')) {
                    $('#' + pid).css('border-right-color', $('#' + pid).parent().css('background-color'));//arrowhovering
                } else if ($('#' + pid).parent().hasClass('right')) {
                    $('#' + pid).css('border-left-color', $('#' + pid).parent().css('background-color'));//arrowhovering
                }
            }

        });
        $('.tile_section_centered .pictureTextTile .pictureTile').on('mouseleave', function () {

            var id = $(this).attr('id');

            var sib = $('#' + id).siblings('.textTile');
            if (sib != null) {
                $('#' + sib.attr('id')).removeClass('hovering');//.css('opacity', '1'); }

                var pid = $('#' + sib.attr('id') + ' .arrow').first().attr('id');
                if ($('#' + pid).parent().hasClass('left')) {
                    $('#' + pid).css('border-right-color', $('#' + pid).parent().css('background-color'));//arrowhovering
                } else if ($('#' + pid).parent().hasClass('right')) {
                    $('#' + pid).css('border-left-color', $('#' + pid).parent().css('background-color'));//arrowhovering
                }
            }

        });


        $('#professionalareas .pictureTextTile').on('click', function () {
            var pa = $(this).attr('data-attr-parea');
            //alert('data-attr-parea=' + pa);
            showLoader();
            $('.cd-main-content').css('padding-top', '0px');
            clearCache('/Jobs/cacheSearchCriteria');
            window.location = '/Jobs/Search?professionalArea=' + encodeURIComponent(pa) + '&startIndex=1&containerOnly=false';
        });

        $(window).resize(function () {
            Resize();
        });

        $(document).ready(function () {
            Resize();

            $('.cd-main-content').css('padding-top', '68px');
            var mouseX;
            var mouseY;
            $(document).mousemove(function (e) {

                mouseX = e.pageX;
                mouseY = e.pageY;
            });

            $('.logo-button').click(function () {
                var brand = $(this).attr('data-attr-brand');
                if (brand != "") {
                    showLoader();
                    $('.cd-main-content').css('padding-top', '0px');
                    clearCache('/Jobs/cacheSearchCriteria');
                    window.location = '/Jobs/Search?brand=' + brand + '&startIndex=1&containerOnly=false';
                }

            });
        });
