<script type="text/javascript">
var page = 2;
//var sort = ;
//var category = ;

$(".project-more a.load-more").on('click',function(){
  page++;
  $.ajax({
    beforeSend: function(){
     $('.project-more a.load-more').hide();
     $('.project-more .loading').show();
    },
    url: "/top/more?page=" + page,
    page: page
  }).done(function(res) {
    var className = 'img-' + Date.now(),
        $res = $(res).each(function() {
          $(this).find('.lazy').addClass(className);
        });

    $('.project-more .loading').hide();
    $('#pj-incremental').append($res);
    $('.' + className).lazyload({
      threshold:    100,
      effect:       'fadeIn',
      effect_speed: 3000
    });

    if ( $res.last().val() == 9 ) {
      $(".project-more a.load-more").show();
    }
    $res.last().remove();

    $(".pj-bar").each(function(){
      //global variables
      var wH = $(window).height();
      var wW = $(window).width();
      var mobile = false;
      if (wW < 769) {
        mobile = true;
      }
      var scrollTop = $(window).scrollTop();
      $(window).scroll(function () {
        scrollTop = $(window).scrollTop();
      });
      var $bar =  $(this);
      var per = $bar.data("percent");
      var pos = $bar.offset().top;
      if (per > 100) {
        per = 100;
      }

      //リサイズ時に位置を再計算
      $(window).on("orientationchange resize",function(){
        if (timer !== false) {
          clearTimeout(timer);
        }
        timer = setTimeout(function() {
          pos = $bar.offset().top;
        }, 200);
      });

      //描画
      function calc() {
        if (scrollTop + wH > pos) {
          $bar.animate({
            width: per + "%"
          }, 900, "easeInOutQuart");
        }
      }
      calc();

      $(window).scroll(function () {
        calc();
      });
    });
  });
  return false;
});

</script>


