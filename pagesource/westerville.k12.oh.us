    <script type="text/javascript">
        $(window).resize(function () {
            if ($('body').width() < 766) {
                articlesToShow = 1;
            }
            else {
                articlesToShow = 1;
            }
        });

        $(document).ready(function () {
            var $newsSectionNumber = 1;
            var slideshowInterval = null;

            if ($('body').width() < 767) {
                articlesToShow = 1;
            }
            else {
                articlesToShow = 1;
            }

            for (var $i = 1; $i <= 5; $i++) {                
                if (($i <= ($newsSectionNumber * articlesToShow)) & ($i > (($newsSectionNumber - 1) * articlesToShow))) {
                    var $active = ($('.article.Newsarticle.rows:nth-child(' + $i + ')'));
                    $active.removeClass('inactive');
                    $active.addClass('active');
                } else {
                    var $inactive = ($('.article.Newsarticle.rows:nth-child(' + $i + ')'));
                    $inactive.removeClass('active');
                    $inactive.addClass('inactive');
                }                
            }
            startTheSlideshowTimer();
                       
            function startTheSlideshowTimer() {
                slideshowInterval = setInterval(function () {
                    if ($newsSectionNumber < Math.ceil((5 / articlesToShow))) {
                        $newsSectionNumber ++;								
                        swapNews($newsSectionNumber);
                    }
                    else {
                        $newsSectionNumber = 1;								
                        swapNews($newsSectionNumber);
                    }
                }, 4000);
                console.log(slideshowInterval);
            }                                

            function swapNews(sectionNumber) {
                for (var $i = 1; $i <= 5; $i++) {
                    if (($i <= (sectionNumber * articlesToShow)) & ($i > ((sectionNumber - 1) * articlesToShow))) {
                        var $active = ($('.article.Newsarticle.rows:nth-child(' + $i + ')'));
                        $active.removeClass('inactive');
                        $active.addClass('active');
                    } else {
                        var $inactive = ($('.article.Newsarticle.rows:nth-child(' + $i + ')'));
                        $inactive.removeClass('active');
                        $inactive.addClass('inactive');
                    }
                }
            }

            $('section#news-list').hover(function(){clearInterval(slideshowInterval);}, function(){
                slideshowInterval = setInterval(function () {
                    if ($newsSectionNumber < Math.ceil((5 / articlesToShow))) {
                        $newsSectionNumber ++;								
                        swapNews($newsSectionNumber);
                    }
                    else {
                        $newsSectionNumber = 1;								
                        swapNews($newsSectionNumber);
                    }
                }, 4000);
            });        
        });
    </script>

