<script>
	$("#txtNewsContent").outerHeight(30);
	$("#txtNewsContent").outerWidth(510);
	$("#btnFile").outerHeight(30);
	$("#txtNewsContent").focus(function(){
		$("#txtNewsContent").outerHeight(60);
		$("#lblCounter").show();
		$("#lnkNews").show();
		$("#lblCounter").text(255-$("#txtNewsContent").val().length);
		});
	$("#txtNewsContent").keyup(function(){
		$("#lblCounter").text(255-$("#txtNewsContent").val().length);
		});
	$("#btnFile").click(function(){
			$("#inpFile").click();
		});
	$("body").delegate("#inpCloseInfoError","click",function(){
			$("#divInfoError").hide();
		});
	$("body").delegate("#inpCloseInfoSuccess","click",function(){
			$("#divInfoSuccess").hide();
		});
	function readURL(input) {
    	if (input.files && input.files[0]) {
			var reader = new FileReader();
			reader.onload = function (e) {
				$('#blah').attr('src', e.target.result);
				$('#blahlnk').attr('href', e.target.result)
				$("#prewImage").show();
				}
        	reader.readAsDataURL(input.files[0]);
    		}
		}
		$("#inpFile").change(function(){
			readURL(this);
		});
	$("#inpClosePreview").click(function(){
		$("#prewImage").hide();
		$("#inpFile").val("");
		});
	$("#btnNewsSave").click(function(){
		var file  =  $('#inpFile')[0].files[0];
		if(file && file.size > 20971520){
			setErrorMessage("Dosya boyutu 20mb'tan büyük olamaz.");
			return;
			}
		var imgname  =  $('#inpFile').val();
		//var fileSize = file.size;
		var newsContent = $("#txtNewsContent").val();
		
		if(!newsContent || newsContent.trim()==""){
			setErrorMessage("İçerik alanını doldurunuz");
			return;
			}
		
		var ext =  imgname.substr( (imgname.lastIndexOf('.') +1) );
		
		var data = new FormData();
		if(file){
			data.append('foto1', file);
		}
		data.append('newsContent',newsContent);
		
		
		$.ajax({
			url:'haberekle_ajax.asp',
			type:'POST',
			data:data,
			ENCTYPE: 'multipart/form-data',
       		processData: false,  // tell jQuery not to process the data
      		contentType: false,  // tell jQuery not to set contentType
			}).done(function(data){
				console.log(data);
				if(data=="ok"){
					console.log("işlem başarılı");
					var cache = $('#lblInfoSuccess').children();
					$("#lblInfoSuccess").text("Başarılı bir şekilde eklenmiştir").append(cache);
					$("#divInfoError").hide();
					$("#prewImage").hide();
					$("#inpFile").val("");
					$("#txtNewsContent").val("");
					$("#divInfoSuccess").show();
					window.setTimeout(function(){
						$("#divInfoSuccess").hide();
						},5000);
					
				}else {
					console.log("hata oluştu");
					if(data=="1001"){
						setErrorMessage("oops bir şeyler yanlış gitti: Ama bu bir resim değil ki?");
					}else if(data=="1002"){
						setErrorMessage("oops bir şeyler yanlış gitti: Kullanıcı girişi yapılması gerek. Sağ üst bölümden kullanıcı girişi yapabilirsiniz.");
					}else if(data=="1003"){
						setErrorMessage("oops bir şeyler yanlış gitti: İçerik alanını boş bırakmayınız.");
					}else{
						setErrorMessage("oops bir şeyler yanlış gitti: "+data);
					}
				}
				});
		
		});

var setErrorMessage= function(message){
	var cache = $('#lblInfoError').children();
	$("#lblInfoError").text(message);
	$("#divInfoSuccess").hide();
	$("#lblInfoError").append(cache);
	$("#divInfoError").show();
	}
</script>

