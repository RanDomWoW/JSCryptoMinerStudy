 <script>
	$("body").delegate("#username,#password","keydown",function(e){
	if(e.keyCode==13){
		$("#btnLogin").click();
		}
	});
	
	$("body").delegate("#btnLogin","click",function(){
		var username = $("#username").val();
		var pass = $("#password").val();
		if(!username){
			alert("Kullanıcı adı boş bırakılmamalı"); return;
			}
		if(!pass){
			alert("Şifre alanı boş bırakılmamalı"); return;
		}
		$.ajax({
			url:"giris_ajax.asp",
			type:"POST",
			data: {"username":username,"pass":pass,"page":"giris_ajax"},
			error: function (xhr, status) {
				$("#lblLoginMessage").html("");
				$("#lblLoginMessage").append(xhr.responseText);
				$("#lblLoginMessage").show();
        	},
			success: function(data){
				var isError = false;
				var errorText="";
				
				if(data.indexOf("&2004&")!=-1){isError=true;}
				if(data.indexOf("&2001&")!=-1){
				 errorText += "- Geçersiz kullanıcı adı ya da şifre<br>";
				}
				if(data.indexOf("&2002&")!=-1){
				 errorText += "- Kullanını adı girelim<br>";
				}
				if(data.indexOf("&2003&")!=-1){
				 errorText += "- Fakat en az 6 karakterlik şifre?<br>";
				}
				if(isError){
				$("#lblLoginMessage").html("");
				$("#lblLoginMessage").append(errorText);
				$("#lblLoginMessage").show();
				}else {
				var result = $('<div />').append(data).find('#divAjaxLogin').html();
				$("#divAjaxLogin").html(result);
				}
				}
			})
		
		});
		
		$("body").delegate("#lnkLogout","click",function(){
		$.ajax({
			url:"giris_ajax.asp",
			type:"POST",
			data: {"logout":"OK"},
			error: function (xhr, status) {
				$("#lblLoginMessage").html("");
				$("#lblLoginMessage").append(xhr.responseText);
				$("#lblLoginMessage").show();
        	},
			success: function(data){
				var result = $('<div />').append(data).find('#divAjaxLogin').html();
				$("#divAjaxLogin").html(result);
				}
			});
	 });

</script>
