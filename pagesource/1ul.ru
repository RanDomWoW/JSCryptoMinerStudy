            navUl.append('<li id="fast_nav_'+$(this).attr('name')+'"><a href="#'+$(this).attr('name')+'">'+$(this).text()+'</a></li>');

            navLinkPosition.push($(this).offset().top);
            navLinkId.push('fast_nav_'+$(this).attr('name'));
        });

        navLinkCount = navLinkPosition.length;

        $('#fast_nav').show();

        $(document).scroll(function(e){
            var i = navLinkCount,
                top = 0,
                changed = false;

            currentScrollTop = $(document).scrollTop() + 150;

            if (Math.abs(lastScrollTop - currentScrollTop) < 100)
                return;

            for (i = navLinkCount; i >= 0; i--)
            {
                top = parseInt(navLinkPosition[i], 10);

                if (top <= currentScrollTop)
                {
                    navUl.find('li.active').removeClass('active');
                    $('#'+navLinkId[i]).addClass('active');
                    changed = true;
                    break;
                }
            }

            if (!changed)
                navUl.find('li.active').removeClass('active');

            lastScrollTop = currentScrollTop;
        });
    });
</script>                                                    <script>
