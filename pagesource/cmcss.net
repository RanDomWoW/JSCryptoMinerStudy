        $('.photo').after('<a href="http://www.google.com" class="banner_link" id="phLink">Full Story</a><a href="#" id="pause"><img id="pimg" src="images/pause.png" alt="Pause Slideshow"></a><div id="switch"></div>').cycle({
            fx: 'fade',
            fit: 1,
            speed: 1000,
            timeout: 7000,
            pause: 1,
            pager: '#switch'
        });
        
        /* Sets functionality for the 'pause' link. Lets the user stop the slideshow. */
        $('#pause').click(function () {
            if ($('#pimg').attr('src') == 'images/pause.png') {
                $('#pimg').attr('src', 'images/play.png');
                $('.photo').cycle('pause');
            }
            else {
                $('#pimg').attr('src', 'images/pause.png');
                $('.photo').cycle('resume');
            }

        });
        setTimeout(function () {
            $('#flashContent').hide();
        }, 7000);


        //Set up the modalDialog to respond to 'esc' key, and hide it onLoad
        $(".modal").modal({
            'keyboard': true,
            'show': false
        });

    });

</script>


