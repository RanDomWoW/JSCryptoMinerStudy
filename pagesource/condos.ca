<script>
function login (){
	var xmlhttp;
	if (window.XMLHttpRequest)
	{// code for IE7+, Firefox, Chrome, Opera, Safari
		xmlhttp=new XMLHttpRequest();
	}
	else
	{// code for IE6, IE5
		xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
	}
	
	xmlhttp.onreadystatechange=function()
	{
		if (xmlhttp.readyState==4 && xmlhttp.status==200)
		{
			var responseText = xmlhttp.responseText;
			//alert(responseText);
			if(responseText == "Success"){
                location.reload();
			}
			// else if(responseText == "Success - redirect to rental profile"){
			// 	document.location.href="/investor-profile";
			// }
			else document.getElementById("login_error").innerHTML=responseText;
		}
	}
	if($("#remember").is(':checked'))
		var rem = 1;
	else
		var rem = 0;
	
	xmlhttp.open("POST","ajaxlogin",true);
	xmlhttp.setRequestHeader("Content-type","application/x-www-form-urlencoded");
	xmlhttp.send("email="+encodeURIComponent(document.getElementById("email").value)+"&password="+document.getElementById("password").value+"&remember="+rem);
	return false;
}

function setCityHeader()
{
	if (typeof default_city !== 'undefined') {
    	// the variable is defined
		//alert(default_city);
		document.getElementById("city_text_span").innerHTML= default_city;
	}
}
setCityHeader();
</script>
