function resizeSelect()
{
    $('#index_area').width( $('#hidnSpan').html($('#index_area').find(":selected").text()).width() + step_size)

    $('#index_area').change(function()
    {
        $('#index_area').width( $('#hidnSpan').html($('#index_area').find(":selected").text()).width() + step_size)
    });
}

function set_index_area()
{
	var a = document.getElementById("index_area");
	index_area_id = a.options[a.selectedIndex].value;
	index_area_name = index_areas[index_area_id];
}

function set_index_type()
{
	var t = document.getElementById("index_sale_types");
	index_type = t.options[t.selectedIndex].value;
	if (index_type  == 'Sale' ) sales_type = 1;
	else sales_type = 2;
}

function get_past_tense_index_type()
{
	if (index_type == 'Sale') return 'Sold';
	if (index_type == 'Lease') return 'Leased';
}

function toggle_full_report_btn()
{
	var full_report_btn = document.getElementById("full-index");
	full_report_btn.style.display = full_report_btn.style.display === 'none' ? '' : 'none';
}

function set_top_index_values()
{
	var current_set = market_index[index_area_id];
	// Top Blocks
	document.getElementById("index-area-1").innerHTML = index_area_name;
	document.getElementById("index-area-2").innerHTML = index_area_name;
	document.getElementById("index-type-1").innerHTML = get_past_tense_index_type();
	document.getElementById("big_price").innerHTML = current_set['historic'][sales_type]['latest']['avg_ppsqft'];
	document.getElementById("index-60days").innerHTML = '<img src="/application/themes/boot/images/'+current_set['historic'][sales_type]['60days']['direction']+'-arrow.png" alt="" class="'+current_set['historic'][sales_type]['60days']['direction']+'-arrow">'+current_set['historic'][sales_type]['60days']['change']+'%<small>since 60 days ago</small>';
	document.getElementById("index-1year").innerHTML = '<img src="/application/themes/boot/images/'+current_set['historic'][sales_type]['1year']['direction']+'-arrow.png" alt="" class="'+current_set['historic'][sales_type]['1year']['direction']+'-arrow">'+current_set['historic'][sales_type]['1year']['change']+'%<small>since 1 year ago</small>';
		document.getElementById("latest_sold_stats").innerHTML = '$'+current_set['historic'][1]['latest']['avg_price']+' <span style="color:#909ba0">/</span> '+current_set['historic'][1]['latest']['avg_sqft']+' FT<sup>2</sup>';
	document.getElementById("latest_rental_stats").innerHTML = '$'+current_set['historic'][2]['latest']['avg_price']+' <span style="color:#909ba0">/</span> '+current_set['historic'][2]['latest']['avg_sqft']+' FT<sup>2</sup>';
	document.getElementById("latest_counts").innerHTML = current_set['historic'][1]['latest']['count']+' Sold <span style="color:#909ba0">/</span> '+current_set['historic'][2]['latest']['count']+' Rented';
		
}

function update_index()
{
	set_index_area();
	set_index_type();
	// update table first
	set_top_index_values();
	}
function detectIE() {
	var ua = window.navigator.userAgent;

	var msie = ua.indexOf('MSIE ');
	if (msie > 0) {
		// IE 10 or older => return version number
		return parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10);
	}

	var trident = ua.indexOf('Trident/');
	if (trident > 0) {
		// IE 11 => return version number
		var rv = ua.indexOf('rv:');
		return parseInt(ua.substring(rv + 3, ua.indexOf('.', rv)), 10);
	}

	var edge = ua.indexOf('Edge/');
	if (edge > 0) {
		// IE 12 (aka Edge) => return version number
		return parseInt(ua.substring(edge + 5, ua.indexOf('.', edge)), 10);
	}
	// other browser
	return false;
}
