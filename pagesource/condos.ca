<!--searchbox-->
<script type="text/javascript">
	function iniSearchSliderNav(){
		
		$("#buy-slider-nav").noUiSlider({
			range: [150000, 850000]
			,start: [calc_value_for_buy_slider(350000), calc_value_for_buy_slider(550000)]
			,connect: true
			,step: 5000
			,serialization: {
				 to: [ $('#buy_min_temp'), $('#buy_max_temp') ]
				,resolution: 1
			}
			,slide: function(){
				updateBuyValues();
			}
		});
		
		$("#rent-slider-nav").noUiSlider({
			range: [1000, 5000]
			,start: [calc_value_for_rent_slider(1000), calc_value_for_rent_slider(2500)]
			,connect: true
			,step: 25
			,serialization: {
				 to: [ $('#rent_min_temp'), $('#rent_max_temp') ]
				,resolution: 1
			}
			,slide: function(){
				updateRentValues();
			}
		});
		
		$("#size-slider-nav").noUiSlider({
			range: [200, 2050]
			,start: [calc_value_for_size_slider(0), calc_value_for_size_slider(99999999)]
			,connect: true
			,step: 50
			,serialization: {
				 to: [ $('#unit_area_min_temp'), $('#unit_area_max_temp') ]
				,resolution: 1
			}
			,slide: function(){
				updateSizeValues();
			}
		});
	}

 <!--   BUY SLIDER FUNCTIONS   -->
	

	function calc_value_from_buy_slider(value)
	{
		if (value <= 600000) return value;
		if (value <= 700000)
		{
			over = value - 600000;
			return 600000 + (4*over);	// upto 1,000,000
		}
		else
		{
			over = value - 700000;
			return 1000000 + (10*over);	// upto 2,500,000
		}
	}
	
	function calc_value_for_buy_slider(value)
	{
		if (value <= 600000) return value;
		if (value <= 1000000)
		{
			over = value - 600000;
			return 600000 + (over/4);	
		}
		else if (value <= 2500000) // upto 2,500,000
		{
			over = value - 1000000;
			return 700000 + (over/10);	
		}
		else return 850000; // max slider value
	}
	
	function updateBuyValues() 
	{  
		buy_min_temp = document.getElementById("buy_min_temp").value;
		adjusted_buy_min = calc_value_from_buy_slider(buy_min_temp);
		document.getElementById("slider_buy_min").innerHTML = '$'+format_money(adjusted_buy_min, 0, "", ",");
		document.getElementById("buy_min").value = adjusted_buy_min;
		if (buy_min_temp <= 150000)
		{
			document.getElementById("slider_buy_min").innerHTML = '$0';
			document.getElementById("buy_min").value = 0;
		}
		
		buy_max_temp = document.getElementById("buy_max_temp").value;
		adjusted_buy_max = calc_value_from_buy_slider(buy_max_temp);
		if (buy_max_temp < 200000)
		{
			buy_max_temp = 200000;
			$("#buy-slider").val([null, buy_max_temp]);
		}
		else if (buy_max_temp < 850000)
		{
			document.getElementById("slider_buy_max").innerHTML = '$'+format_money(adjusted_buy_max, 0, "", ",");
			document.getElementById("buy_max").value = adjusted_buy_max;
		} 
		else // buy_max_temp >= 850000
		{
			document.getElementById("slider_buy_max").innerHTML = 'Unlimited';
			document.getElementById("buy_max").value = 99999999;
		}
	}

<!--   RENT SLIDER FUNCTIONS   -->

	
	
	// RENT SLIDER FUNCTIONS
	function calc_value_from_rent_slider(value)
	{
		if (value <= 1250) 
		{
			under = 1250 - value;
			return 1250 - (2*under);	// downto 750
		}
		if (value <= 4000) return value;
		if (value <= 4500)
		{
			over = value - 4000;
			return 4000 + (2*over);	// upto 5000
		}
		else
		{
			over = value - 4500;
			return 5000 + (10*over);	// upto 10000
		}
	}
	
	function calc_value_for_rent_slider(value)
	{
		if (value <= 1250)
		{
			under = 1250 - value;
			return 1250 - (under / 2);	// downto 750
		}
		if (value <= 4000) return value;
		if (value <= 5000)
		{
			over = value - 4000;
			return 4000 + (over/2);	
		}
		else if (value <= 10000)// upto 10000
		{
			over = value - 5000;
			return 4500 + (over/10);	
		}
		else return 5000; // max slider value
	}
		
	function updateRentValues() 
	{  
		rent_min_temp = document.getElementById("rent_min_temp").value;
		adjusted_rent_min = calc_value_from_rent_slider(rent_min_temp);
		document.getElementById("slider_rent_min").innerHTML = '$'+format_money(adjusted_rent_min, 0, "", ",");
		document.getElementById("rent_min").value = adjusted_rent_min;
		if (rent_min_temp <= 750)
		{
			document.getElementById("slider_rent_min").innerHTML = '$0';
			document.getElementById("rent_min").value = 0;
		}
		
		rent_max_temp = document.getElementById("rent_max_temp").value;
		adjusted_rent_max = calc_value_from_rent_slider(rent_max_temp);
		if (rent_max_temp < 1125)
		{
			rent_max_temp = 1125;
			$("#rent-slider").val([null, rent_max_temp]);
		}
		else if (rent_max_temp < 5000)
		{
			document.getElementById("slider_rent_max").innerHTML = '$'+format_money(adjusted_rent_max, 0, "", ",");
			document.getElementById("rent_max").value = adjusted_rent_max;
		} 
		else // rent_max_temp >= 6000
		{
			document.getElementById("slider_rent_max").innerHTML = 'Unlimited';
			document.getElementById("rent_max").value = 99999999;
		}
	}
	
<!--   SIZE SLIDER FUNCTIONS   -->
 
	
	
	// SIZE SLIDER FUNCTIONS
	function calc_value_from_size_slider(value)
	{
		if (value <= 1000){
			return value;   // upto 1000     
		}
		else if (value > 1000 && value <= 1500) 
		{
			over = value - 1000;
			return 1000 + (2*over);	// up to 2000
		}
		else if (value > 1500)
		{
			over = value - 1500;
			return 2000 + (4*over);	// upto 4000
		}
		/*else
		{
			over = value - 2000;
			return 2500 + (10*over);	// upto 5000
		}*/
	}
	
	function calc_value_for_size_slider(value)
	{
		if (value <= 1000)
		{
			return value;
		}else if( value > 1000 && value <= 2000)
		{
			over = value - 1000;
			return 1000 + (over/2); 
		}else if( value > 2000 && value <= 4000) {
			over = value - 2000;
			return 1500 + (over/4);
		}
		else return 2050; // max slider value
	}

	function updateSizeValues() 
	{
		size_min_temp = document.getElementById("unit_area_min_temp").value;
		adjusted_size_min = calc_value_from_size_slider(size_min_temp);
		if (size_min_temp <= 200)
		{
			document.getElementById("slider_size_min").innerHTML = '0';
			document.getElementById("unit_area_min").value = 0;
		}
		else
		{
			document.getElementById("slider_size_min").innerHTML = adjusted_size_min;
			document.getElementById("unit_area_min").value = adjusted_size_min;
		}
		
		size_max_temp = document.getElementById("unit_area_max_temp").value;
		adjusted_size_max = calc_value_from_size_slider(size_max_temp);
		if (size_max_temp < 300)
		{
			size_max_temp = 300;
			$("#size-slider").val([null, size_max_temp]);
		}
		else if (size_max_temp < 2050)
		{
			document.getElementById("slider_size_max").innerHTML = adjusted_size_max+ " SQFT";
			document.getElementById("unit_area_max").value = adjusted_size_max;
		} 
		else // size_max_temp >= 2250
		{
			document.getElementById("slider_size_max").innerHTML = 'Unlimited SQFT';
			document.getElementById("unit_area_max").value = 99999999;
		}
		
	}
	
	function format_money(money, decimals, decimal_sep, thousands_sep)
	{ 
	   var n = money,
	   c = isNaN(decimals) ? 2 : Math.abs(decimals), //if decimal is zero we must take it, it means user does not want to show any decimal
	   d = decimal_sep || ',', //if no decimal separetor is passed we use the comma as default decimal separator (we MUST use a decimal separator)
	
	   t = (typeof thousands_sep === 'undefined') ? '.' : thousands_sep, //if you don't want ot use a thousands separator you can pass empty string as thousands_sep value
	
	   sign = (n < 0) ? '-' : '',
	
	   //extracting the absolute value of the integer part of the number and converting to string
	   i = parseInt(n = Math.abs(n).toFixed(c)) + '', 
	
	   j = ((j = i.length) > 3) ? j % 3 : 0; 
	   return sign + (j ? i.substr(0, j) + t : '') + i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + t) + (c ? d + Math.abs(n - i).toFixed(c).slice(2) : ''); 
	}
</script>
