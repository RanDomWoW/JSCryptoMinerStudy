<script>
	var near_msg_div_search = document.getElementById("near_msg_div_search");
	function getLocationTwo() {
		near_msg_div_search.innerHTML = "Obtaining your location...";
		if (navigator.geolocation) {
			navigator.geolocation.getCurrentPosition(processPositionTwo, showError);
			
		} else {
			near_msg_div_search.innerHTML = "Geolocation is not supported by this browser.";
		}
	}
	function processPositionTwo(position) {
		
		var lat = position.coords.latitude;
		var long = position.coords.longitude;
		var pol = "(circle,"+lat+","+long+",500)";
		document.getElementById("polygon").value=pol;
		document.getElementById("search_by").value="Draw";
		document.getElementById("is_nearby").value="1";
		near_msg_div_search.innerHTML="Searching Condos...";
		document.forms.quick_search_form.submit();   
	}
	function showError(error) {
		switch(error.code) {
			case error.PERMISSION_DENIED:
				near_msg_div_search.innerHTML = "You have denied the request to obtain your location."
				break;
			case error.POSITION_UNAVAILABLE:
				near_msg_div_search.innerHTML = "Location information is unavailable."
				break;
			case error.TIMEOUT:
				near_msg_div_search.innerHTML = "The request to get your location timed out."
				break;
			case error.UNKNOWN_ERROR:
				near_msg_div_search.innerHTML = "An unknown error occurred."
				break;
		}
	}
