      // rafraichissement
      function htmlDecode(input){
        var e = document.createElement('div');
        e.innerHTML = input;
        return e.childNodes.length === 0 ? "" : e.childNodes[0].nodeValue;
      }

      function refreshTimeRes() {
        var Auj = new Date();
        Auj = parseInt(Auj.getTime()/ 1000);
        //alert(Auj);
        $( ".clJeuResultat" ).each(function( index ) {
          var liTime = Auj - parseInt($( this ).attr("timeRes"));
          //alert( index + ": " + liTime.toStringDuration() );
          
          $( this ).find('.clTimeResultat').html('<span class="clTimeResultat2"></span> ago');
          $( this ).find('.clTimeResultat2').html(liTime.toStringDuration());
        }); 
        return true; 
      }
      
      Number.prototype.toStringDuration = function () {
        var sec_num = this;
        var lsString = '';
        if (sec_num < 0) {
          lsString += 'less than 1 minute';
        } else {
          var days = Math.floor(sec_num / 86400);
          var hours   = Math.floor(sec_num / 3600);
          var minutes = Math.floor((sec_num - (hours * 3600)) / 60);
          
          //if (hours   < 10) {hours   = "0"+hours;}
          //if (minutes < 10) {minutes = "0"+minutes;}
          //if (seconds < 10) {seconds = "0"+seconds;}
          if (days == 0) {
            if (hours == 0) {
              if (minutes == 0) {
                lsString += 'less than 1 minute';
              } else {
                lsString += minutes + ' min';
              }
            } else {
              lsString += hours + ' h';
            }
          } else {
            lsString += days + ' D';
          }
        }
        return lsString;
    }

