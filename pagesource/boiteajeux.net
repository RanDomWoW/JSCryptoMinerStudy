			 function postFacebookOK() {
			 	$().toastmessage('showToast', {
					            text     : 'TXT_SHARE_FACEBOOK_OK',
					            sticky   : false,
					            position : 'top-right',
					            type     : 'notice',
					            closeText: ''
					        });
			 }
			 
			 
			 function postFacebookKO() {
			 	$().toastmessage('showToast', {
					            text     : 'TXT_SHARE_FACEBOOK_KO',
					            sticky   : false,
					            position : 'top-right',
					            type     : 'warning',
					            closeText: ''
					        });
			 }
			 
			 function postFacebook (action_type, object_type, url,jeu, jeuId,divId) {
					var object = new Object;
					object[object_type] = url;
		
					FB.api(
						'/me/boiteajeux:' + action_type,
						'post',
						object,
						 function(response) {
							 if (!response) {
								 console.log('Error occurred.');
												postFacebookKO();
							 } else if (response.error) {
								console.log(response.error.message);
								console.log(response.error.type);
								console.log("Le partage a échoué");
												postFacebookKO();
							 } else {
									$.post("fb_post.php",
										{ id: jeuId,
											jeu : jeu,
											fb : response.id
										},
										function(data) {// $('#traces').html(JSON.stringify(data));
											if(data.ok) {
												$("#"+divId).hide();
												postFacebookOK();
											} else {
												console.log(data.erreur);
												postFacebookKO();
											}
										},'json'
									);
							 }
						 });
					}
	
					function openGraphCall( action_type, object_type, url,jeu, jeuId, divId) {
						FB.getLoginStatus(function(response) {
							if (response.status === "connected") {
								postFacebook (action_type, object_type, url,jeu, jeuId,divId);
							}
							else {
								if (readCookie('fbsLoginAsk') == null) {
									createCookie('fbsLoginAsk', 1);
									FB.login(function(response) {
										if (response.authResponse) {
											postFacebook (action_type, object_type, url,jeu, jeuId,divId);
										} else {
											console.log('login failed');
												postFacebookKO();
										}
									}, {scope: 'publish_actions'});
								} else {
									console.log('said no');
												postFacebookKO();
								}
							}
						})
					}

