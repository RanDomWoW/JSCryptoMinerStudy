				om_params['evars'][35] = anon_id; 
			} 
			DW.pageParams = dw_params; 
			DW.clear(); 
			setTimeout(function()
			{
				om_params['evars'][16]=$.cookie('spt'); 
				somni = omniture(om_params);
			},delay); 
		});
}
else
{
	DW.pageParams = dw_params; 
	DW.clear(); 
	setTimeout(function()
	{
