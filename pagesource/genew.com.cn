			var y = (e.pageY - this.offsetTop - (h / 2)) * (h > w ? (w / h) : 1);
			var direction = Math.round((((Math.atan2(y, x) * (180 / Math.PI)) + 180) / 90) + 3) % 4;
			var eventType = e.type;
			//var dirName = new Array('上方','右侧','下方','左侧');
			var dirName = new Array(true,false,false,true);
			if(e.type == 'mouseenter'){
			//$(this).html(dirName[direction]+'进入');
                clearTimeout(output);
				$('#menu_sec').css('display','block');
				$('#menu_sec ul').stop(false,false).hide();
				$('#menu_sec .ul0').stop(false,false).slideDown()
			}else{
			//$(this).html(dirName[direction]+'离开');
				if(dirName[direction]){
                    output=setTimeout(function(){
        				$('#menu_sec .ul0').stop(false,false).slideUp()
        				$('#menu_sec').css('display','none');
                    },500);
				}
			}
		});
		$(".listcase").bind("mouseenter mouseleave",function(e) {
			var w = $(this).width();
			var h = $(this).height();
			var x = (e.pageX - this.offsetLeft - (w / 2)) * (w > h ? (h / w) : 1);
			var y = (e.pageY - this.offsetTop - (h / 2)) * (h > w ? (w / h) : 1);
			var direction = Math.round((((Math.atan2(y, x) * (180 / Math.PI)) + 180) / 90) + 3) % 4;
			var eventType = e.type;
			//var dirName = new Array('上方','右侧','下方','左侧');
			var dirName = new Array(true,true,false,false);
			if(e.type == 'mouseenter'){
			//$(this).html(dirName[direction]+'进入');
                clearTimeout(output);
				$('#menu_sec').css('display','block');
				$('#menu_sec ul').stop(false,false).hide();
				$('#menu_sec .ul1').stop(false,false).slideDown()
			}else{
			//$(this).html(dirName[direction]+'离开');
				if(dirName[direction]){
                    output=setTimeout(function(){
    					$('#menu_sec .ul1').stop(false,false).slideUp()
    				    $('#menu_sec').css('display','none');
                    },500);
				}
			}
		});
	}
});
</script>


<script>
$(document).ready(function(e) {
	$('.indBlock3 li.hid').each(function(index, element) {
        if(index == 4){ $(this).remove()}
		$(this).addClass('li'+index)
    });
	/////////////轮播
	var lenThumb = $('#banner li').length;
	for(var i=1;i<=lenThumb;i++){
		if( i == 1){
			$('#banner .spanBox').append('<span class="on"></span>');
		}else{
			$('#banner .spanBox').append('<span></span>');
		}
	}
	var index = 0;
	var playPic = setInterval(function(){
		index++;
		picShow(index);
		if(index==lenThumb){index=0}
	},5000);
	picShow(index);
	$('#banner .prev').click(function(){
		clearInterval(playPic);
		index--;
		if(index==-1){index=lenThumb - 1}
		picShow(index);
		playPic = setInterval(function(){
			picShow(index);
			index++;
			if(index==lenThumb){index=0}
		},6000);
	});
	$('#banner .next').click(function(){
		clearInterval(playPic);
		index++;
		if(index==lenThumb){index=0}
		picShow(index);
		playPic = setInterval(function(){
			picShow(index);
			index++;
			if(index==lenThumb){index=0}
		},6000);
	});
	$('#banner .spanBox span').click(function(){
		clearTimeout(playPic);
		index = $('#banner .spanBox span').index(this);
		picShow(index);
		playPic = setInterval(function(){
			picShow(index);
			index++;
			if(index==lenThumb){index=0}
		},5000);
	});
});
function picShow(i){
	$('#banner li').eq(i).stop(true,true).fadeIn().siblings().fadeOut();
	$('#banner .spanBox span').eq(i).addClass('on').siblings().removeClass('on');
}
</script>
</body>
