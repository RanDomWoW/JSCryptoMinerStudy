<script type="text/javascript">

	function DropdownBuilder(itemName, url, subitems, clss){
		
		var table = document.createElement("TABLE");
		table.className = "menu-dropdown " + clss;
		table.cellPadding = 0;
		table.cellSpacing = 0;
		table.style.position = "absolute";
		
		var tr = document.createElement("TR");
		
		var item = document.createElement("TD");
		item.className = "item";
		
		var subtable = document.createElement("TABLE");
		subtable.cellPadding = 0;
		subtable.cellSpacing = 0;
		var subtr = document.createElement("TR");
		var subtd = document.createElement("TD");
		var a = document.createElement("A");
		a.href = url;
		a.appendChild(document.createTextNode(itemName));
		subtd.appendChild(a);
		subtr.appendChild(subtd);
		subtable.appendChild(subtr);
		item.appendChild(subtable);
		
		tr.appendChild(item);
		
		var space = document.createElement("TD");
		space.onmouseover = function(e) { DropdownMenu.HideActiveDropdown(); };
		tr.appendChild(space);
		
		table.appendChild(tr);
		
		tr = document.createElement("TR");
		
		var subs = document.createElement("TD");
		subs.className = "subitems";
		subs.colSpan = 2;
		
		var ul = document.createElement("UL");
		var li = null;
		
		for (var x = 0; x < subitems.length; x++)
		{
			li = document.createElement("LI");
			a = document.createElement("A");
			a.href = subitems[x].url;
			a.appendChild(document.createTextNode(subitems[x].text));
			li.appendChild(a);
			ul.appendChild(li);
		}
		
		subs.appendChild(ul);
		tr.appendChild(subs);
		
		table.appendChild(tr);
		
		return table;
	}
	
	new DropdownMenu('menu-bar', function(itemName, url, subitems){ return DropdownBuilder(itemName, url, subitems, "menu"); });
	new DropdownMenu('submenu-bar', function(itemName, url, subitems){ return DropdownBuilder(itemName, url, subitems, "submenu"); });
</script>
