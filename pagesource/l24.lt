	mymonth134 = mydate134.getMonth();
	myweekday134= mydate134.getDate();
	myyear134= mydate134.getYear();
	year134 = myyear134;

	if (year134 < 2000) year134 = year134 + 1900;
	myhours134 = mydate134.getHours();
	var mm134 = "am";
	if (myhours134 > 12 + houradjust134)
		mm134 = "pm";
	if (myhours134 > 12 + houradjust134)
		myhours134 -= 12;
	if (myhours134 == 0) myhours134 = 12;
