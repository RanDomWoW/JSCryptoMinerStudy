function clock134() {
	var newdate134 = new Date();
	var day134="";
	var month134="";
	var myweekday134="";
	var year134="";
	var mydate134 = new Date();
	var dston134  = new Date(952822799000);
	dston134.setYear(mydate134.getFullYear());
	var dstoff134 = new Date(973385999000);
	dstoff134.setYear(mydate134.getFullYear());
	dston134.setFullYear(newdate134.getFullYear());
	dstoff134.setFullYear(newdate134.getFullYear());

	var dst134 = "0";
	var myzone134 = newdate134.getTimezoneOffset();

	var zone134 = parseInt(0);

