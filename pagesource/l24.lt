		zoneb134 = -zone134;
	} else {
		zoneb134 = zone134;
	};
	if (mydate134 > dston134 && mydate134 < dstoff134 && dst134 == "1") {
	//date is between dst dates and dst adjust is on.
		zonea134 = zoneb134 - 1;
		var houradjust134 = 0;
	} else {
		zonea134 = zoneb134;
		var houradjust134 = -1;
	};

	newtime134=newdate134.getTime();

	var newzone134 = (zonea134*60*60*1000);

	// convert zone back to negative if it was negative originally
	if (zone134 <= 0) {
		newzone134 = -newzone134;
	};
	newtimea134 = 
	newtime134+(myzone134*60*1000)+newzone134;		
	mydate134.setTime(newtimea134);
