<script type="text/javascript">
$(function(){
    $('li', 'ul#nav > li').hover(function() {
        $(this).parent().siblings('a').addClass('current');
    }, function() {
        $(this).parent().siblings('a').removeClass('current');
    });

    // href 內含中文，只有 IE 不會自動 encodeURIComponent，Chrome、Firefox 皆會自動處理 encodeURIComponent
    // 自行偵測 IE，勿用 $.browser.msie (因 jQuery 1.9 後移除)
    var is_IE = /msie/.test(navigator.userAgent.toLowerCase());
    var is_IE11 = navigator.userAgent.indexOf('Trident/') > 0 && navigator.userAgent.indexOf('rv:11') > 0;
    if (is_IE || is_IE11) {
        $('a[href*=keyword]').each(function() {
            var start_pos = this.href.indexOf('keyword=') + 8;
            var end_pos = this.href.indexOf('&', start_pos);
            var keyword = (end_pos == -1) ? this.href.substr(start_pos) : this.href.substring(start_pos, end_pos);
            if (keyword === decodeURIComponent(keyword)) {// 排除已 encodeURIComponent 字串
                this.href = this.href.replace(keyword, encodeURIComponent(keyword));
            }
        });
    }
    
    // 關鍵字搜尋
    $('#keyword').click(function() {
        if (this.defaultValue == this.value) {
            this.value = '';
        }
    }).blur(function() {
        if (this.value == '') {
            this.value = this.defaultValue;
        }
    }).keydown(function(event) {
        if (event.which == 13) {
            $('#search_button').click();
        }
    });

    $('#search_button').click(function() {
        if (!$('#keyword').val() || $('#keyword').val() == '請輸入關鍵字') {
            alert('請輸入關鍵字');
        }else{
            location.href = 'http://www.net-fashion.net/search?keyword=' + encodeURIComponent($('#keyword').val());
        }
        return false;
    });
});
</script>    
