        <script type="text/javascript">

    checkPlicyCookie();

    function setPlicyCookie() {
        var cvalue="msgboxhide"
        var exdays=90; //cookie expire after 90 days
        var	cname = "dataPolicy";
        var domainName = window.location.host;
        cname = cname+domainName;
        //alert("cname:"+cname + " - cvalue:"+cvalue);
        var d = new Date();
        d.setTime(d.getTime() + (exdays*24*60*60*1000));
        var expires = "expires="+d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
        document.getElementById('CookieMsgBox').style.display = 'none';
    }

    function getPlicyCookie() {
        var	cname = "dataPolicy";
        var domainName = window.location.host;
        cname = cname+domainName;

        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    function checkPlicyCookie() {
        var vp_cookie_val = getPlicyCookie();
        if (vp_cookie_val == "msgboxhide")
        {document.getElementById('CookieMsgBox').style.display = 'none';}
        else {document.getElementById('CookieMsgBox').style.display = 'block';}
    }

</script>

