           $(document).ready(function () {
               var showitem = getUrlVarsNew(window.location.href, "showitem");

               $.ajax({
                    url:"/includes/services.aspx?Serviceid=82&des=",
                    encoding:"UTF-8",
                    dataType:"text", 
                    beforeSend:function(){ 
                        $("#citySource").attr("disabled",true); 
                        $("#citySource").html("");
                    },
                    success:function(res){
                        $("#citySource").attr("disabled",false); 
                        
                        parser = new DOMParser();
                        xmlDoc = parser.parseFromString(res,"text/xml");
                        var len= xmlDoc.getElementsByTagName("Name_FA").length;
                        $("#citySource").append('<option value="">بدون انتخاب</option>') ;
                        for (i=0;i<len;i++){

                                $("#citySource").append('<option value="'+xmlDoc.getElementsByTagName("Code")[i].childNodes[0].nodeValue+'">' + 
                            xmlDoc.getElementsByTagName("Name_FA")[i].childNodes[0].nodeValue+ ' </option>') ;    
                        }
                        if(showitem !=undefined ) {
                            $("#citySource").val(showitem);
                            $("#citySource").change();
                        }
                        
 
                    }
                });  

                $("#citySource").on("change",function(){
                    $.ajax({
                        url:"/includes/services.aspx?Serviceid=82&des="+$(this).val(),
                        encoding:"UTF-8",
                        dataType:"text", 
                        beforeSend:function(){ 
                            $("#citySource").attr("disabled",true); 
                            $("#cityDestination").html("");
                        },
                        success:function(res){
                            $("#citySource").attr("disabled",false); 
                            
                            parser = new DOMParser();
                            xmlDoc = parser.parseFromString(res,"text/xml");
                            var len= xmlDoc.getElementsByTagName("Name_FA").length;
                            $("#cityDestination").append('<option value="">بدون انتخاب</option>') ;
                            for (i=0;i<len;i++){

                                    $("#cityDestination").append('<option value="'+xmlDoc.getElementsByTagName("Code")[i].childNodes[0].nodeValue+'">' + 
                                        xmlDoc.getElementsByTagName("Name_FA")[i].childNodes[0].nodeValue+ ' </option>') ;      
                            }

                        }
                    });    
                });


            })
 
    
    

