           var showitem;
           function getUrlVarsNew(str, parm) {
               var hash = "", outID = "";
               var hashes = str.slice(str.indexOf('?') + 1).split('&');
               for (var i = 0; i < hashes.length; i++) {
                   hash = hashes[i].split('=');
                   if (hash[0] == parm) {
                       outID = hash[1];
                   }//if
               }
               return outID;
           }
    
