{
	e.preventDefault();
	$('#map_container').empty();
	$.ajax(
	{
		url: $(this).attr('href'),
		success: function(data)
		{
			$('#map_container').html($(data));
			var form = $('#map_container').find('form');
			form.each(ajaxContactForm);
		}
	})
	$('#map_container').clearQueue().stop().slideDown(300);
});

var ajaxContactForm = function()
{
	var jForm = jQuery(this).submit(function()
	{
		try
		{
			jQuery.ajax({
				url: jForm.attr('action'),
				type: 'POST',
				data: jForm.serialize(),
				dataType: 'html',
				cache: false,
				processData: false,
				success: function(data, textStatus, XMLHttpRequest)
				{
						$('.chosen_contact_form_container').empty().html($(data));
						$('.chosen_contact_form_container form').each(ajaxContactForm);
				}
			});
		}
		catch(msg)
		{
			alert(msg);
		}
		return false;
	});
};

