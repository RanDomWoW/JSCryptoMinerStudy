<script>
$(function(){
  $(".region .button").on("click", function(){
    if (Modernizr.mq('(max-width: 480px)')) {
      if ($(this).hasClass("active")) {
        $(this).removeClass("active").siblings(".region-member").slideUp();
      } else {
        $(".region .button").removeClass("active");
        // Collapse All .region-member
        $(".region-member").slideUp();
        $(this).addClass("active").siblings(".region-member").slideDown();
      }
    }
  });
  $(".page-title").on("click",function(e){
    // alert('page-title');
    $(this).toggleClass("open");
    if ($(".regions-wrapper").css("overflow") == "hidden") {
      $(".regions-wrapper .spacer").slideToggle("400", function(){
        $(".regions-wrapper").css("overflow","visible");
      });
    } else {
      $(".regions-wrapper").css("overflow","hidden");
      $(".regions-wrapper .spacer").slideToggle("400", function(){});
    }
    // $(".regions-wrapper .spacer").slideToggle("400", function(){});
  });
})
</script>
