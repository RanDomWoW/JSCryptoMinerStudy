        setTimeout(function(){ $(".modalAD").hide(); }, 20000);
		
		$(".modalAD img").click(function() {
			window.open("http://www.carnaval.salvador.ba.gov.br", '_blank');
		});

        
        var loadMore = true;
        var running = false;
        
        $(window).scroll(function() {
            if (loadMore && !running && $(window).scrollTop() >= $(".li-home").last().offset().top + $(".li-home").last().outerHeight() - window.innerHeight) {
                
                running = true;
                
                $(".imgLoader").show();

                $.ajax({
                    method: "POST",
                    url: 'http://bahia.ba/wp-content/themes/bahia_carnaval2018/infiniteScroll.php',
                    data: {
                        post_type: 'bahia,brasil,digital,economia,emprego,entretenimento,esporte,gente,justica,politica,memoria,mundo,saude,servicos,ssa,meioambiente,carnaval2016,carnaval2017,carnaval2018,entrevista,pipocou,exclusivo,eleicoes2016,municipios,salvador,motor',
                        showposts: '10',
                        post__not_in: $("#ids").val(),
                        orderby: 'post_date',
                        order: 'DESC',
                        meta_key: 'ocultar_home',
                        meta_value: '0',
                        editoria: 'false'
                    }
                })
                .done(function (data) {
                    $(".imgLoader").hide();

                    var content = data.split(">>>")[0];
                    var new_ids = data.split(">>>")[1];
                    var count = data.split(">>>")[2];

                    $(".li-home").last().after(content);
                    var ids = $("#ids").val() + ',' + new_ids;
                    $("#ids").val(ids);

                    if(count < 10) {
                        loadMore = false;
                    }
                    
                    running = false;

                });
                
            }
        });
        
        $(".iconAoVivo").hide().repeat().each($).fadeIn($).wait(1000).fadeOut($);
        
                
    });
</script>


