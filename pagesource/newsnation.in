<script>
function get_mega_menu(catId,title,clss,type)
{
	
	$.ajax({
		    method: "GET", 
			url: "http://www.newsnation.in/deskjson/nn-header-menu-json-new.php?catId="+catId+"&type="+type+"&title="+title,
			
    })
	.done(function(data) 
	{	
		var obj = jQuery.parseJSON(data);
		
		$(obj.content).insertAfter("."+clss+":last");
	});
	
}

function dataSearch(e,str)
 {
	var code=e.keyCode? e.keyCode : e.charCode
	
	if(code==13)
	{
		validSearch(str);

	}
 }
 
function validSearch(str)
{
	var str	=	$('#'+str).val();
	if(str=="" || str=="Search..." )
	{
	alert("please enter search text");
	return false;
	}else{

		var res = str.replace(':','');
		res = res.replace('!','');
		res = res.replace('?','');
		res = res.replace("'",'');
		res = res.replace('"','');
		res = res.replace(',','');
		res = res.replace(/ /gi,'-');
		window.location="http://www.newsnation.in/search/"+res;
	}
}
</script>
