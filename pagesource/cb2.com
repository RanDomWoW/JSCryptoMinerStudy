    <script>
        window.onload = function(){
            var locationPathname = '';

            var pathExistsInNavBar = document.querySelectorAll('a[href*=\'' + location.pathname + '\'][class*=\'nav-sub-category\']').length > 0;

            if($('.breadcrumb-list-item').length && $('.breadcrumb-list-item').length >= 2 && !pathExistsInNavBar ){
                locationPathname = $($('.breadcrumb-list-item a')[$('.breadcrumb-list-item').length - 2]).attr('href');
            }
            else if(window.location.pathname.length > 1){
                locationPathname = location.pathname.indexOf('scb') > -1
                    ? location.pathname.substring(4, location.pathname.length)
                    : location.pathname;
            }
            if (window.location.pathname.length > 1) {

                var highlightedElement = document.querySelectorAll('.nav-main a[class*="nav-category-highlight"]');
                var selectedMenuItem = document.querySelectorAll('.nav-main a[href="' + locationPathname + '"]');

                if (highlightedElement.length && selectedMenuItem.length && selectedMenuItem[0].href !== highlightedElement[0].href) {
                    highlightedElement[0].classList.remove('nav-category-highlight');
                    selectedMenuItem[0].classList.add('nav-category-highlight');
                } else if (selectedMenuItem && selectedMenuItem[0]) {
                    selectedMenuItem[0].classList.add('nav-category-highlight');
                }

                var all = document.querySelectorAll('a[href*=\'' + locationPathname + '\'][class*=\'nav-category\']');
                for (var i = 0; i < all.length; i++) {
                    all[i].className += ' nav-active';
                }
            }
        }

    </script>
