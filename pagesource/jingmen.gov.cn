<script>
function replacejgcx(){
var el = [];
_el = document.getElementsByTagName('a');
var _startimedc=$("span[class=dcsj]");
var _startimezj=$("span[class=jzsj]");
$.each(_startimedc,function(n){
			var timedc=_startimedc.get(n).innerText;
			dcTime =  timedc.replace(/-/g,"/");
			newdatedc = getToDay();
			newdatedc = newdatedc.replace(/-/g,"/");				
				if(Date.parse(newdatedc)>Date.parse(dcTime)){
				$(this).html("已截至");
				$(this).css("display","inline-block");
				return true;
			}
				else{
				$(this).html("正在进行");
				$(this).css("display","inline-block");
				return true;
			}		
		});
		$.each(_startimezj,function(n){
			var timezj=_startimezj.get(n).innerText;
			zjTime =  timezj.replace(/-/g,"/");
			newdatezj = getToDay();
			newdatezj = newdatezj.replace(/-/g,"/");				
				if(Date.parse(newdatezj)>Date.parse(zjTime)){
				$(this).html("已截至");
				$(this).css("display","inline-block");
				return true;
			}
				else{
				$(this).html("正在进行");
				$(this).css("display","inline-block");
				return true;
			}		
		});
	}
	 function getToDay(){
           
             var now = new Date();
             var nowYear = now.getFullYear();
             var nowMonth = now.getMonth();
             var nowDate = now.getDate();
             newdate = new Date(nowYear,nowMonth,nowDate);
             nowMonth = doHandleMonth(nowMonth + 1);
             nowDate = doHandleMonth(nowDate);
             return nowYear+"-"+nowMonth+"-"+nowDate;
        }
				//----修改日期格式填充零
        function doHandleMonth(month){
             if(month.toString().length == 1){
              month = "0" + month;
             }
             return month;
        }
</script>
