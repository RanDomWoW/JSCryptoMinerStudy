        <script>

  function ActiveNews(flag) {
                debugger;
                if (flag == 1) {
                    $("#play").attr("class","Active");
                    document.getElementById('play').style.visibility = 'hidden';
                    document.getElementById('stop').style.visibility = 'visible';
                }
                else {
                    $("#play").attr("class", "DeActive");
                    document.getElementById('play').style.visibility = 'visible';
                    document.getElementById('stop').style.visibility = 'hidden';
                }
                 

            }

            function tick() {
                $('#bind_newslist div:first').slideUp(function () { $(this).appendTo($('#bind_newslist')).slideDown(); });
            }


            function dot() {

                var $sample = $("#bind_newslist");

                if ($sample.is(":hover")) {
                    console.log('rr');
                }

                else { if ($('#play').hasClass('Active')) {
                        tick();
                    }
                }
            }
            var myVar = setInterval(function () { dot() }, 2000);
        </script>
        <script type="text/javascript">
            var mn = jQuery.noConflict();

            $(document).ready(function () {
                $.ajax({

                    type: "POST",
                    url: "Rating.asmx/bindNRLnews",
