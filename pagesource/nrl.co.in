<script>
    var fontSize =87;
    function set_font_size(fontType){


        if(fontType == "increase"){
            if(fontSize < 103){
                fontSize = parseInt(fontSize) + 8;
            }
        }else if(fontType == "decrease"){
            if(fontSize > 71){
                fontSize = parseInt(fontSize) - 8;
            }
        }else{
            fontSize = 87;
        }
 
        $("body").css("font-size",fontSize + "%");
        //$("#template_three_column").css("font-size",fontSize + "%");
    }

    

</script>



