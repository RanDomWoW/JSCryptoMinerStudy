		/* SUB-MENU EVENTS */

		/* sub-menu events (click) */
		$('.sub-menu-nav ul li a').on('click', function(){
			var _this= $(this),
				ul   = _this.closest('ul'),
				footer_menu= $('#footer #sub-menu'); // div with menu ul

			if (_this.attr('class') != 'active'){
				
			  	ul.find('li a').removeClass('active');
			  	footer_menu.find('ul li a').removeClass('active');
				
		      	_this.addClass('active');

		      	/* add class active to the same li object on footer */
			  	$('[data-id=' + _this.attr('id') + ']' ).addClass('active');
			}
			/*return false;*/
	  	});

		/* sub-menu-footer events (click) */
		$('#footer #sub-menu ul li a').on('click', function(){
			var _this= $(this),
