<script>
function geraProgramacao(pr){
	var arrayDias=new Array('Domingo','2ª Feira','3ª Feira','4ª Feira','5ª Feira','6ª Feira','Sábado');
	//alert(pr);
	//print_r($arrayHorariosJunt);
	var arrayPr=pr.split('__');
	var output='';
	arrayPr.forEach(function(entry) {
		var arrayIndi=entry.split('+');
		//alert(arrayIndi[0]+' '+arrayIndi[1]);
		var arrayIndi2=arrayIndi[0].split(',');
		output+='<strong>';
		//alert(arrayIndi2.length);
		var contaEste=0;
		arrayIndi2.forEach(function(entryP) {
		contaEste++;
		
		if(arrayIndi2.length>1){
			output+=arrayDias[entryP].replace(' Feira','');
			}else{
				output+=arrayDias[entryP];
				}
		if(contaEste<arrayIndi2.length){
			output+=', ';
			}
		});
		
		
		var arrayIndi3=arrayIndi[1].split('_');
		output+='</strong> às ';
		var contaEste3=0;
		arrayIndi3.forEach(function(entryP3) {
			contaEste3++;
			output+=entryP3;
			if(contaEste3<arrayIndi3.length){
				if(contaEste3<arrayIndi3.length-1){
					output+=', ';
				}else{
					output+=' e ';
					}
			
			}
		});	
		output+='<br>';
		
    //alert(entry);
});
//alert(output);
/*foreach(arrayHorariosJunt as key=>value){
	
}*/
return output;
	}
</script>
