$(".mute").click(function(){
   
   if($(this).hasClass("muted")) {
      $(this).removeClass("muted");
      jQuery('video').prop('muted',false);
   } else {
      $(this).addClass("muted");
      jQuery('video').prop('muted',true);
   }
})



lskeys=Object.keys(localStorage);
var filterednames = lskeys.filter(function(obj) {
    return (obj[0] === "l");
});
var arr= [];

$.each(lskeys, function(idx,key) {
  if (key[0]==='l') {
     lsval=localStorage.getItem(key).split("-");
     if (lsval[2]!==undefined) {
        arr.push({id:key.substring(1)+"-"+lsval[0]+"-"+lsval[1],val:lsval[2]});
     }
  }
})

var lssorted=arr.sort(function(a,b) {
    return b.val-a.val;
});

var lslast = "";
$.each(lssorted, function(idx,val) {
lslast=lslast+val.id+"|"+localStorage.getItem(val.id)+',';
});

lslast=lslast.slice(0, -1);
var $ttt;

$.post("/services/films/get_watch.php",{"movie_ids":lslast},function(data){
        if (data["count"]>0 ) { 
         $(".resume").show();
         $("#resume_playlist").html(data.html);

         $slider=$('#resume_playlist').bxSlider({
              minSlides: 1,
              maxSlides: 4,
              slideWidth:370,
              infiniteLoop: false,
              slideMargin: 10
         });
         
         
         $(".pdelete").click(function(e){
              e.preventDefault();
              localStorage.removeItem('l'+$(this).parent().data("movie_id"));
              //$(this).parent().css("display","none");
              //$('#resume_playlist').jcarousel('reload');
              $(this).parent().remove();
              $slider.reloadSlider();
              
              if($("#resume_playlist li").length==0) {
                    $(".resume").hide();
              }
              
         })

         $(".video_pl").click(function(e) { 
            if(!$(e.target).hasClass("pdelete")) {
               document.location.href="/movies/"+$(this).data("movie_id");
            }
         })

         } else {          
         $(".resume").hide();
         }
},"json");


</script>


