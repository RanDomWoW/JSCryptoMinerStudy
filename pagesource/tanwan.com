        $('.login-zh-dl input').focus(function(){
            this.parentNode.className = 'on';
        }).blur(function(){
            this.parentNode.className = '';
        });

        jQuery('.slideBox').slide({mainCell:'.bd ul',effect:'fold',autoPlay:true,pnLoop:false});
        $('.kf-nav li').hover(function(){
            var _index = $(this).index();
            $(this).addClass('cur').siblings().removeClass('cur');
            $(".kf-list-txt").hide().eq(_index).show();
        }).eq(0).trigger('hover');
        $('.news-nav li').hover(function(){
            var _index = $(this).index();
            $(this).addClass('cur').siblings().removeClass('cur');
            $('.news-list-txt').hide().eq(_index).show();
        }).eq(0).trigger('hover');
        jQuery('.picScroll-left').slide({titCell:'.hd ul',mainCell:'.bd ul',autoPage:true,effect:'leftLoop',vis:6,trigger:'click'});
    });

    // 登录切换
    function loginTab(tab) {
        if(tab) {
            tw_qr_login.init({divQrMain:'.log-erweima'});
            tw_qr_login.get_code();

            $('.loginzh').hide();
            $('.loginewm').show();
        } else {
            tw_qr_login.init({divQrMain:'.log-erweima'});
            tw_qr_login.reset();

            $('.loginzh').show();
            $('.loginewm').hide();
        }
    }
    //热门礼包
    $.getJSON(_protocol+'//www.tanwan.com/api/code.php?a=code_top_list&num=8&callback=?',function(data){
