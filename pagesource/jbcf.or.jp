			$('#sponsors_lower ul').append('<li class="empty"><a href="http://www.jbcf.or.jp/inquiry/office.html">スポンサー募集中</a></li>');
		}

	}
	
	//Guidebook
	$('#menu_guidebook a').featherlight({
		targetAttr: 'href'
	});

});



