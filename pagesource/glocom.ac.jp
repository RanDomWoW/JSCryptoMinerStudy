<script type="text/javascript">
jQuery(function( $ ) {

  window.addEventListener('scroll', function(e){

    var scrolltop = $(window).scrollTop();

    if ( ( (scrolltop >= 100) || ($('#scrollarea').position().top < -100) ) || $('#checked').is(':checked') ) {
      $(".header").addClass("smallHeader");
    }
    else if ( $(".header").hasClass("smallHeader") ) {
      $(".header").removeClass("smallHeader");
    }

    if (scrolltop >= 100) {
      $('.pagetop').fadeIn();
    }
    else {
      $('.pagetop').fadeOut();
    }

    if (scrolltop >= ($(document).height() - $(window).height() - $('.credit').height()) ) {
      $('.pagetop').css({'position':'absolute','bottom':$('.credit').height()});
    }
    else {
      $('.pagetop').css({'position':'fixed','bottom':'0'});
    }

  });

  var scrollpos;
  $('#checked').change(function(){
    if ($(this).is(':checked')) {
      scrollpos = $(window).scrollTop();
      $('#scrollarea').css({top: -scrollpos}).addClass("fixed");
      $('html').css({overflow: 'hidden'});
    } else {
      $('#scrollarea').removeClass("fixed").css({top: ''});
      $('html,body').scrollTop(scrollpos);
      $('html').css({overflow: 'auto'});
    }

  });

  $('a[href^=#]').click(function(){
    var speed = 250;
    var href= $(this).attr("href");
    var target = $(href == "#" || href == "" ? 'html' : href);
    var headerHeight = $(".header").outerHeight();
    if (headerHeight >= 150) {
      headerHeight = headerHeight - 100;
    }
    var position = target.offset().top - headerHeight;
    $("html, body").animate({scrollTop:position}, speed, "swing");
    return false;
  });
});
</script>
</body>

</html>