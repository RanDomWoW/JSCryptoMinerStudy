<script>
var timerRunning = false;
function checkDateTime() {
	var serverHour = 11;
	var difHour = 0;
    if (timerRunning) {
        clearTimeout(updatetime);
        timerRunning = false;
    }
    for (var i = 1; i <= 2; i++) {
        var today = new Date();
        var month = today.getMonth() + 1;
        var date = today.getDate();
        if (i == 2) { //日本时间
            if ((month > 3 || month == 3 && date > 24) && (month < 10 || month == 10 && date < 24)) //夏时制 -6小时
                today = today.dateAdd("h", +1);
            else
                today = today.dateAdd("h", +1);
        }
        var year = today.getYear() + 00;
        month = today.getMonth() + 1;
        date = today.getDate();
        var day = today.getDay();
        var hour = today.getHours();
		if(i==1){
			difHour = serverHour - hour;
		}
        var minute = today.getMinutes();
        var second = today.getSeconds();
        var dateTime = addZero(hour+difHour) + ":" + addZero(minute) + ":" + addZero(second);
        if(hour>=12)
            dateTime+=" PM";
        else
            dateTime+=" AM";
        if (i == 1){
			//北京时间
			$("#bj_time").text(dateTime);
			console.log("北京时间"+hour);
		}            
        else
		{
			//日本时间
            $("#mg_time").text(dateTime);
			console.log("日本时间"+hour);
		}
    }
    updatetime = setTimeout("checkDateTime()", 1000);
    timerRunning = true;
}
function addZero(num, end) {
    return num < 10 ? "0" + num : num;
}
$(function(){
    checkDateTime();
});
/* 得到日期年月日等加数字后的日期 */
Date.prototype.dateAdd = function (interval, number) {
    var d = this;
    var k = {'y': 'FullYear', 'q': 'Month', 'm': 'Month', 'w': 'Date', 'd': 'Date', 'h': 'Hours', 'n': 'Minutes', 's': 'Seconds', 'ms': 'MilliSeconds'};
    var n = {'q': 3, 'w': 7};
    eval('d.set' + k[interval] + '(d.get' + k[interval] + '()+' + ((n[interval] || 1) * number) + ')');
    return d;
} 
