<script>
$(function(){
    $(".styledForm").customSelect();
    tabnum = $.cookie('top_ranktab');
    if(!tabnum || tabnum > 4 || tabnum < 0){
    	tabnum = 1+Math.floor( Math.random() * 3 );
        set_tabno(tabnum);
    }
    if(!$("#tab"+tabnum).length){
        tabnum = 1;
        set_tabno(tabnum);
    }
    $("#tab"+tabnum).attr("checked", true);
    $("#jobfreeword"). keydown(function(e) {
		if ((e.which && e.which === 13) || (e.keyCode && e.keyCode === 13)) {
			return false;
		} else {
			return true;
		}
	});
});
function jobSearch(){
	$('#job_search_cond').attr('method','get');
	$('#job_search_cond').attr('action','/job/job_list.cfm');
    $('#job_search_cond').submit();
}
function set_tabno(num){
	$.cookie('top_ranktab', num);
    $(".tab-content").hide();
	$("#tab-content"+num).show();
}
</script>

