		$('.cadastar').click(function() {
			var useTerms = $('#check1-styler').hasClass('checked');
			
			var error = false;
			
			var email = $("#reg-email").val();
			
			if (email == "") {
				$("#reg-email").val('');
				$("#reg-email").addClass("formError");
				$("#reg-email").attr("placeholder", 'Informe seu e-mail');
				error = true;
			} else if (!IsEmail(email)){
				$("#reg-email").val('');
				$("#reg-email").addClass("formError");
				$("#reg-email").attr("placeholder", 'E-mail inválido');
				error = true;
			}
			
			var firstName = $("#reg-firstName").val();
			
			if (firstName == "" || firstName.indexOf('  ') >= 0 || firstName == " ") {
				$("#reg-firstName").val('');
				$("#reg-firstName").addClass("formError");
				$("#reg-firstName").attr("placeholder", 'Informe seu nome');
				error = true;
			}
			
			var lastName = $("#reg-lastName").val();
			
			if (lastName == "" || lastName.indexOf('  ') >= 0 || lastName == " ") {
				$("#reg-lastName").val('');
				$("#reg-lastName").addClass("formError");
				$("#reg-lastName").attr("placeholder", 'Informe seu sobrenome');
				error = true;
			}
			
			if ($("#reg-cellphone").val() == "") {
				$('.msk-phone').unmask();
				$("#reg-cellphone").addClass("formError");
				$("#reg-cellphone").attr("placeholder", 'Informe seu celular');
				error = true;
			}
			
			var cpf = $("#reg-cpf").val();
			
			if (cpf == "") {
				$('.msk-cpf').unmask();
				$("#reg-cpf").val('');
				$("#reg-cpf").addClass("formError");
				$("#reg-cpf").attr("placeholder", 'Informe seu CPF');
				error = true;
			} else if(!validarCPF(cpf)) {
				$('.msk-cpf').unmask();
				$("#reg-cpf").val('');
				$("#reg-cpf").addClass("formError");
				$("#reg-cpf").attr("placeholder", 'CPF inválido');
				error = true;
			}
			
			var date = $("#reg-dateOfBirth").val();
			
			if (date == "") {
				$('.msk-birthday').unmask();
				$("#reg-dateOfBirth").val('');
				$("#reg-dateOfBirth").addClass("formError");
				$("#reg-dateOfBirth").attr("placeholder", 'Informe a data de nascimento');
				error = true;
			} else if (!isDate(date)){
				$('.msk-birthday').unmask();
				$("#reg-dateOfBirth").val('');
				$("#reg-dateOfBirth").addClass("formError");
				$("#reg-dateOfBirth").attr("placeholder", 'Data de nascimento inválid');
				error = true;
			}
			
			if ($("#reg-gender").val() == "unknown" || $("#reg-gender").val() == null) {
				$("#reg-gender-styler").addClass("formError");
				$('#formProfileRegister .jq-selectbox__select-text').html('Informe o sexo');
				$('#formProfileRegister .jq-selectbox__select-text').addClass("formErrorGender");
				$('#formProfileRegister .jq-selectbox__select').css("border",'none');
				error = true;
			}
			
			var password = $("#reg-password").val();
			
			if (password == "" || password.indexOf(' ') >= 0) {
				$("#reg-password").val('');
				$("#reg-password").addClass("formError");
				$("#reg-password").attr("placeholder", 'Informe a senha');
				error = true;
			} else if ((password.length < 8) || (password.length > 20)){
				$("#reg-password").val('');
				$("#reg-password").addClass("formError");
				$("#reg-password").attr("placeholder", 'informe de 8 a 20 caracteres');
				error = true;
			}
			
			var confirmPassword = $("#reg-CONFIRMPASSWORD").val();
			
			if (confirmPassword == "" || confirmPassword.indexOf(' ') >= 0) {
				$("#reg-CONFIRMPASSWORD").val('');
				$("#reg-CONFIRMPASSWORD").addClass("formError");
				$("#reg-CONFIRMPASSWORD").attr("placeholder", 'Confirme sua senha');
				error = true;
			} else if (confirmPassword != $("#reg-password").val()){
				$("#reg-CONFIRMPASSWORD").val('');
				$("#reg-CONFIRMPASSWORD").addClass("formError");
				$("#reg-CONFIRMPASSWORD").attr("placeholder", 'Confirme sua senha');
				error = true;
			}
			
			if (!useTerms){
				$('.useTerms span').css( "color", "red" );
				$('.useTerms a').css( "color", "red" );
			}
			
			if (!error && useTerms) {
				showPopup();
				$.ajax({
					url : context +"/components/ajax/submit_form_handler.jsp?form=/atg/userprofiling/ProfileFormHandler",
					type : "POST",
					data : $("#formProfileRegister").serialize(),
					success : function(response) {
						if (response.success == 'true') {
							
/* 							Tag facebook para cadastro completo */
							if(facebookPixelId != null && facebookPixelId != ""){
								facebookpixelCompleteRegistrarion();						
							}
/* 							Tag facebook para cadastro completo */					
							
							window.location.replace("/shop/farmrio/");
							
						} else {
							closePopup();
							var qtdFields = response.invalidFields.length;
							for (var i = 0; i < qtdFields; i++) {
								if(response.invalidFields[i].field == 'useTerms'){
									$('.useTerms span').css( "color", "red" );
									$('.useTerms a').css( "color", "red" );
								}
								if(response.invalidFields[i].field == 'gender'){
									$("#reg-gender-styler").addClass("formError");
									$('#formProfileRegister .jq-selectbox__select-text').html(response.messages[i].message);
									$('#formProfileRegister .jq-selectbox__select-text').addClass("formErrorGender");
									$('#formProfileRegister .jq-selectbox__select').css("border",'none');
								}
								if (response.invalidFields[i].field == 'cellphone'){
									$('.msk-phone').unmask();
								}
								
								if (response.invalidFields[i].field == 'cpf'){
									$('.msk-cpf').unmask();
								}
								
								if (response.invalidFields[i].field == 'dateOfBirth'){
									$('.msk-birthday').unmask();
								}
								
								$("#reg-" + response.invalidFields[i].field).val('');
								$("#reg-" + response.invalidFields[i].field).addClass("formError");
								$("#reg-" + response.invalidFields[i].field).attr("placeholder", response.messages[i].message);
							}
						}
					}, error: function(response) {
						console.log("error");
					}
				});
				return false;
			}
		});
		
	</script>

