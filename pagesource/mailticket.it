	//Capslock control sul campo password	
	function capLock(e){
	 kc = e.keyCode?e.keyCode:e.which;
	 sk = e.shiftKey?e.shiftKey:((kc == 16)?true:false);
	 if(((kc >= 65 && kc <= 90) && !sk)||((kc >= 97 && kc <= 122) && sk)){
	  console.log("capslocked");
	  $("input.checkcapslock").addClass("capslock");
	  }
	 else{
	  console.log("tuttok");
	  $("input.checkcapslock").removeClass("capslock");
	  }
	}
