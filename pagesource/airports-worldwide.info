    function adjustPageBody() {
        $("#left-side").width($("#page-body").width() - $("#right-side").width());
    }
    
    function initSearch() {
        var menuOpen = false;
        var menuSelected = false;
        $("#search-field").val("");
        $("#search-clear").hide();
        $("#search-field").autocomplete({
            autofocus: true,
//            focus: function(event, ui) {
//                $(event.target).val(ui.item.label);
//            },
            open: function(event, ui) {
                menuOpen = true;
                menuSelected = false;
            },
            close: function(event, ui) {
                menuOpen = false;
            },
            source: "/search.php?lng=en",
            minLength: 3,
            select: function( event, ui ) {
                $(event.target).val(ui.item.label);
                $("#code").val(ui.item.value);
                $("#search").submit();
                menuSelected = true;
                return false;
            }
        });
        $("#search-field").on("keyup", function(){
            if (!menuOpen && !menuSelected) {
                $("#code").val("");
            }
            if ($(this).val().length === 0) {
                $("#search-clear").hide();
            } else {
                $("#search-clear").show();
            }
        });
        $("#search-clear").on("click", function() {
            $("#search-field").val("");
            $("#code").val("");
            $("#search-field").focus();
            $(this).hide();
        });
    }
    function adjustFooter() {
    h = $('#sheet-footer').height();
    $('#empty').height(h);
    $('#sheet-footer').css('margin-top', -h+'px');
}
