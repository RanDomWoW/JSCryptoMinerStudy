                                        <div class="cart-info">
                                            <h3>{{:Title}}</h3>
                                            <p>{{:AuthorName}}</p>
                                            <p>Isbn: {{:EntityId}}</p>
                                            <p>{{:MaterialType}}</p>
                                            <p class="cart-price">DKK {{:(Price).formatPrice(2,',','.')}}</p>
