                                        <p class="review-price">{{:(Price).formatPrice(2,',','.')}} </p>
                                        DKK  {{:(FinalPrice * Amount).formatPrice(2,',','.')}}
                                    </div>
                                    {{else}}
                                    <div class="cart-action-price">
                                        DKK  {{:(Price * Amount).formatPrice(2,',','.')}}
                                    </div>
                                    {{/if}}
                                </div>
                            </td>
                        </tr>
                        {{/for}}
                    </tbody>
                </table>
            </section>
            <!-- ~~~~~~~~~~~~~~~~~~~~~~~ -->
            <!-- Shopping Basket Desktop -->
