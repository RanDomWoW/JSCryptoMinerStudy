        <script type="text/javascript">
            function ConvertCurrency(element, cena, id) {
                var kurs =119.00;
                var konvertcena = number_format(kurs * cena, 0, ',', ' ');
                var cenaid = '#cena' + id;
                var flag = $(cenaid).data('flag');
                if (flag == 1) {
                    $(cenaid).html(konvertcena + " RSD");
                    $(element).html("<i class='fa fa-exchange pr-10'></i> EUR");
                    $(cenaid).data('flag', 0);

                } else {
                    cena = number_format(cena, 0, ',', ' ');
                    $(cenaid).html(cena + " EUR");
                    $(element).html("<i class='fa fa-exchange pr-10'></i> RSD");
                    $(cenaid).data('flag', 1);
                }
            }
        </script>
