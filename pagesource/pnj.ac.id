    el.scrollLeft = 0;
  };
  el.scrollLeft = el.scrollLeft+speed;
  if(go == 0){
    timeout = setTimeout("scrollFromSide()",50);
  };
}

function stop(){
  go = 1;
  timeout = '';
}

function startit(){
  go = 0;
