	var topDomain = top.location.hostname.replace(domainLookupRegEx, '$2'),
		selfDomain = self.location.hostname.replace(domainLookupRegEx, '$2');
	
	if (topDomain === selfDomain) {
		var antiClickjack = document.getElementById("antiClickjack");
		antiClickjack.parentNode.removeChild(antiClickjack);
	} else {
		top.location = self.location;
	}
</script>


