<script type="text/javascript">
    $(document).on('keydown', '[type="text"]', function (e) {
        var code = e.keyCode || e.which;
        if (code == 13) {
            return false;
        }
    });

    $(document).on('keydown', '.gsm-no', function (e) {
        var code = e.keyCode || e.which;
        if (code == 13) {
            $('.gsm-confirm-btn').trigger("click");
            return false;
        }
    });

    $(document).on('keydown', '.sms-password', function (e) {
        var code = e.keyCode || e.which;
        if (code == 13) {
            $('.gsm-activate').trigger("click");
            return false;
        }
    });

    $(document).on('click', '.gsm-ok-btn', function (e) {
        msg.show('GSM numaranız onaylanmıştır', 'info', 5000);
        return false;
    });

    // ASP.NET Postback Method
    try {
        var PostBackPrm = Sys.WebForms.PageRequestManager.getInstance();
        PostBackPrm.add_endRequest(function (s, e) {
            try {
                if ($('.gsm-confirm-text').val() != "") {
                    $('.gsm-no').attr("readonly", "readonly");
                    $('.gsm-confirm-btn').remove();
                    $('.main-user-btn').css("display", "block");
                }
            }
            catch (Err) {

            }
        });
    }
    catch (err) { }

    function popupStart() {
        var gsmNo = $('.gsm-no');
        if (gsmNo.val() != "") {
            var gsmDurum = isGSM(gsmNo.val().replace(/\(/, '').replace(/\)/, '').replace(/ /, ''));
            if (gsmDurum) {
                $('.gsm-resend').trigger("click");
                return true;
            }
            else {
                msg.show('Lütfen doğru bir GSM numarası giriniz!', 'warning', 10000);
                return false;
            }
        }
        else {
            msg.show('Lütfen GSM numarası giriniz!', 'warning', 10000);
            return false;
        }
    }

    function popupFinish() {
        $('.sms-password').focus();
    }

    function isGSM(phone) {
        var regexp = new RegExp("(5)(?: )?((?: )?[0-9]{1}){9}");
        return regexp.test(phone);
    }

    function runTimer(phone) {
        if (phone != null && phone != "") {
            $('.gsm-no').val(phone);
        }

        timecount.down($('.time'), $('[data-smstime]').attr("data-smstime"), function () {
            $('.sms-activate').hide();
            $('.sms-resend').show();
        });
    }

    function gsmSuccess() {
        $('.gsm-no').attr("readonly", "readonly");
        $('.gsm-confirm-btn').remove();
        //$('.gsm-ok-btn').show();
        $('.main-user-btn').css("display","block");
        $('.gsm-confirm i').trigger("click");

        if ($('[type="password"]').val() != "") $('.main-user-btn').trigger("click");
    }
</script>











