<script>
    onLoadEvent(function () {
        if ($("#new-gallery").length > 0) {
            $(".gallery-container").appendTo("#new-gallery");
        }

        accordionFunction();

        var cattxt = $("meta[name='smk-category']").attr("content");
        var authortxt = $("li[itemprop='author']").text();
        ga('set', 'contentGroup1', cattxt);
        ga('set', 'contentGroup2', authortxt);
        ga('send', 'pageview');

        // Change the value to the form url domain.
        var to_domain = 'http://lp.e-konomista.pt';

        window.onload = iframe_listener();
        var keep_walking = true;

        function iframe_listener() {
            var iframe = $('iframe[src^="' + to_domain + '"]');
            if (iframe.length == 0) {
                setTimeout(iframe_listener, 2000);
                return;
            }
            iframe.each(function () {
                $(this)[0].contentWindow.postMessage($('meta[name=smk-category]').attr("content"), to_domain);
            });
        };

        $(".sidebar form input[type=text]").val('');
        $("body").on('click', '.bt-action-top', function (event) {
            $('html,body').animate({scrollTop: 0}, 'slow');
            return false;
        });

        //shares do facebook
        $("#share-article").click(function (e) { // share end of article
            e.preventDefault();

            FB.ui({
                      method: 'share',
                      picture: $('.box-image-lt img').attr('src'),
                      href: $("link[rel='canonical']").attr('href') + "?utm_source=facebook&utm_medium=site-share"
                  }, function (response) {
                window.console && console.log(response);
                if (response && !response.error_code) {
                    ga('send', 'event', 'Facebook', 'Share', 'Bottom');
                }
            });
        });
        //shares do facebook
    });

    function accordionFunction() {
        $('.accordionMenu > li ').click(
            function () {
                if ($(this).hasClass('active')) {
                    $('.accordionMenu > li').removeClass('active');
                    $('.accordionContent').stop(true, true).slideUp('normal');

                    return;
                }

                $('.accordionMenu > li').removeClass('active');
                $('.accordionContent').slideUp('normal');

                $(this).addClass('active');
                $('.accordionContent', this).stop(true, true).slideDown('normal');
            }
        );

        $('.accordionContent').click(
            function (event) {
                event.stopPropagation();
                $(this).stop;
            }
        );
    }

