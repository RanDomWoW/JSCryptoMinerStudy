<script>
    $(document).ready(function() {

        $('.qrcode').on('click', function(e) {
            e.preventDefault();
            $(this).blur();
            var src = 'http://chart.apis.google.com/chart?chs=328&cht=qr&chl=http://dirarab.net/';
            UIkit.modal.dialog('<div class="uk-modal-body uk-text-center"><img src="' + src + '" alt=""></div><div class="uk-modal-footer uk-text-right"><button class="uk-button uk-button-default uk-modal-close" type="button">إغلق هذة النافذة</button></div>');
        });
        $('.location').click(function() {
            if ("geolocation" in navigator) {
                navigator.geolocation.getCurrentPosition(function(position) {
                    $.ajax({
                            method: "POST",
                            dataType: "json",
                            url: "ajax/get_city_by_geolocation",
                            data: {
                                model: 'prayertimes',
                                lat: position.coords.latitude,
                                lon: position.coords.longitude
                            }
                        })
                        .done(function(msg) {
                            if (msg.goto)
                             {
                                window.location = msg.goto;
                            } else {
                                $(".uk-modal-body").append(msg.html);
                                UIkit.modal("#modal-sections").show();
                            }

                        });
                });
            } else {

                UIkit.modal.alert('متصفحك لا يدعم تحديد موقعك الجغرافي').then();

            }
        });

    });
