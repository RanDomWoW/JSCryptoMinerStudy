current&#39;: account == accountSelectorCtrl.currentAccount}" ng-click="accountSelectorCtrl.accountSelected(account)" ng-if="account.displayName() | isMatching: accountSelectorCtrl.filter" ng-repeat="account in accountSelectorCtrl.accounts"><div class="avatar" ng-style="{&#39;background-image&#39;: (account | avatarURL)}"></div><div class="name" ng-bind="account.displayName()"></div></a></div><p class="create-org" ng-if="accountSelectorCtrl.shouldShowCreateOrganization"><span>...Or if you would like to create an organization, </span><a href="javascript:void(0)" ng-click="accountSelectorCtrl.createOrganizationSelected()">click here</a></p></div></popup><popup model="accountDetailsPopup" ng-controller="AccountDetailsPopupController"><div class="popup-body-account-details"><header><button popup-close=""></button><div class="avatar" ng-style="{&#39;background-image&#39;: account.accountType == &#39;Organization&#39; ? (account.avatarURL | organizationAvatarURL) : (account.avatarURL | userAvatarURL)}"></div><strong ng-bind="account.displayName()" ng-show="!account.hasPendingInvitation"></strong><em ng-bind="account.accountType == &#39;Organization&#39; ? account.billingEmail : account.email"></em><p ng-show="account.accountType == &#39;User&#39; &amp;&amp; account.hasPendingInvitation">Registration pending since: <time datetime=" account.invitedAt | datetimeValue" ng-bind="account.invitedAt | timeInWords" title="{{ account.invitedAt | datetimeValue }}"></time></p></header><article><h2>Test devices</h2><section class="notification" progress="loadAccountTestDevicesProgress"></section><div ng-hide="loadAccountTestDevicesProgress.isInProgress || loadAccountTestDevicesProgress.isError"><section class="notification" ng-show="account.testDevices &amp;&amp; account.testDevices.length == 0">No registered test devices.</section><ul class="test-device-list" ng-show="account.testDevices.length &gt; 0"><li ng-repeat="testDevice in account.testDevices track by $index"><em ng-bind="testDevice.name"></em> <div class="test-device-id" ng-bind="testDevice.deviceId"></div></li></ul></div></article></div></popup><workflow-editor></workflow-editor><popup model="simplePopup" ng-controller="SimplePopupController" ng-init="init(&quot;&quot;, &quot;&quot;, &quot;&quot;)"><div class="popup-body-simple" ng-class="cssClass"><article><h2 ng-bind-html="headerContent"></h2><p ng-bind-html="bodyContent" ng-repeat="bodyContent in bodyContents"></p></article><footer class="actions"><button class="confirm" ng-bind="confirmText" ng-click="confirmSelected()" ng-if="isConfirmable"></button><button initial-focus="" ng-bind="dismissText" ng-click="dismissSelected()" ng-show="dismissText"></button></footer></div></popup></body></html>