<script type="text/javascript">

    function shows(obj, id)
    {
        var objDiv = $("#" + id + "");
        $(objDiv).css("display", "block");
    }
    $(document).ready(function () {
        var openid = "";
        if (openid != "") {
            $("dingbu_none").css("display", "block");
        }
    });
    function show(obj, id) {
        var objDiv = $("#" + id + "");
        $(objDiv).css("display", "block");
        $(objDiv).css("left", event.clientX);
        $(objDiv).css("top", event.clientY + 10);
    }
    function hide(obj, id) {
        var objDiv = $("#" + id + "");
        $(objDiv).css("display", "none");
    }
    //回车键的回车事件
    document.onkeyup = EnterPress;
    function EnterPress(e) {
        var e = e || window.event;
        if (e.keyCode == 13) {
            bookNaClick();
        }
    }


    function htmlEncode(str) {
        var s = "";
        if (str.length == 0) return "";
        s = str.replace(/&/g, "&amp;");
        s = s.replace(/</g, "&lt;");
        s = s.replace(/>/g, "&gt;");
        s = s.replace(/'/g, "&apos;");
        s = s.replace(/"/g, "&quot;");
        return s;
    };


    //关键词（书名）进行搜索
    function bookNaClick() {
        var bookName =$.trim(document.getElementById("dk-text").value);
        if (bookName == "") {
            return;
        } else {
            window.location.href = "search.aspx?book=" + bookName;
            event.returnValue = false;
        }
    }
    function qiandao() {
        var openid = "";
        if (openid == "") {
            alert("请先登录。");
        }
        else {
            $.post(
                "data/data.aspx",
                { openid: openid, type: "qiandao" },
                function (result) {
                    if (result == -1) {
                        alert("今天已经签到，不能重复签到。");
                        style();
                    }
                    else {
                        style();
                        alert("今天获取积分：" + result + "分。");
                    }
                });
        }
    }
    //签到后的样式调整
    function style() {
        document.getElementById("qiandao").style.color = "#8c8c8c";
        document.getElementById("qiandao").style.fontWeight = "400px";
        document.getElementById("qiandao").innerText = "已签到";
    }

    
</script>
