		<script>
		/*<![CDATA[*/
			// Carousel settings
			var numSlides = 3,
				carousel = $("#slider").carousel({interval: 5000});
		
				//carousel = $("#slider").carousel("pause");
				//carousel.carousel("pause");
			
			//create navigation objects and handlers
			var createSlideHandler = function(index){
				return function(){
					carousel.carousel(index);
				};
			};
			for(var i = 0; i < numSlides; i++){
				var a = $("<a/>").click(createSlideHandler(i));
				$(".paging-control-nav").append(a);
				if(i == 0)
					a.addClass("active");
			}
			var pagingControls = $(".paging-control-nav a");
			pagingControls.wrap("<li/>");
			
			//left arrow
			$(".leftarrow").click(function(e){
				$("#slider").carousel("prev");
			});
			
			//right arrow
			$(".rightarrow").click(function(e){
				$("#slider").carousel("next");
			});
			
			//carousel hover handlers
			$("#slider").hover(
				function(e){
					carousel.carousel("pause");
				}, 
				function(e){
					carousel.carousel("cycle");
				}
			);
			
			//carousel events
			$("#slider").bind("slide", function(event) {
			});
	
			$("#slider").bind("slid", function(event) {
				pagingControls.removeClass('active');
				var index = $('#slider .active').index('#slider .item');
				pagingControls.eq(index).addClass('active');
			});
			
		/*]]>*/
		
		// Toggle jumbotron video
		//$("a.toggle-jumbotron-video").click(function() {
		function toggleJumbotronVideo(n) {
			
			// Determine the current state
			var pictureOpacity = $("#jumbotron-" + n + " .picture").css('opacity');
			$("#jumbotron-" + n + " .video").fadeToggle(600);
			
			$("a.toggle-jumbotron-video.text-link").text(pictureOpacity==1 ? 'Hide video' : 'Take a video tour');
			
			// Fade the user greeting; not required if position is relative.
			$("#user-greeting").animate({
				opacity:  (pictureOpacity==1 ? .2 : .86) // Must match initial setting, .86
			}, 600);
			
			$("#signin-reminder").animate({
				opacity:  (pictureOpacity==1 ? .2 : .86) // Must match initial setting, .86
			}, 600);
			
			$("#jumbotron-" + n + " .picture").animate({
				opacity:  (pictureOpacity==1 ? .2 : 1)
			}, 600);
		}
		//});
		
		// Fade/Toggle jumbotron images
		function toggleJumbotronImage(n) {
			
			// Hide the old
			// NOTE: filter required for IE9, .hide required for IE8
			$("[id^='picture-']:not(#picture-" + n + ")").animate({
				opacity: 0
			}, 1200).addClass("hide");
			$("[id^='blurb-']:not(#blurb-" + n + ")").addClass("hide").removeClass("fade-in");
			$("a[id^='link-']:not(a#link-" + n + ")").removeClass("plain");
			
			// Show the new
			$("#picture-" + n).removeClass("hide").animate({
				opacity: 1,
			}, 1200);
			$("#blurb-" + n).removeClass("hide").addClass("fade-in");
			$("a#link-" + n).addClass("plain");
		}
		</script>
		
		
		<script>
			$(function() {
				$FR.register('auth', FundRazr.module.Auth);
				$FR.start('auth');
				
				$FR.register('discover', FundRazr.module.Discover, {
					foundLocation: true
				});
				$FR.start('discover');
				
				$('#campaign-tabs').on('click', 'a[data-toggle="tab"]', function() {
					var $this = $(this),
						type = $this.data('type'),
						state = $this.data('state');
					
					if (!!state)
						return;
					
					$this.data('state', 'loaded');
		           	$.ajax({
						url: $FR.config.apiURL + 'campaigns/featured?type=' + type,
						//findUrl: '/find?type=' + type,
						type: 'GET',
						dataType: 'html',
						success: function(data) {
							var $tabPane = $($this.attr('href'));
							$tabPane.html(data);
							
							// Find more link
							var findType;
							switch(type) {
							    case 'nonprofit':
							        findType = 'Non-profits';
							        break;
							    case 'animals':
							    	findType = 'Animals';
							        break;
							    case 'personal':
							    	findType = 'Personal';
							        break;
							    default:
							    	findType = 'Trending';
							} 
							$tabPane.find('.find-link').attr('href', '/find?category=' + findType);
						},
						error: function(data) {
							var $tabPane = $($this.attr('href'));
							$tabPane.html("Oops, something went wrong. Try again later.");
						}
		           	});
				});
			});
		</script>
		
		
		<script>
		/*<![CDATA[*/
			$(function() {
				
				// When list is shown
			    $('#categories-list').on('show', function () {
			    	$('#categories-menu').addClass('on');
			    });
				
				// When list is hidden
			    $('#categories-list').on('hidden', function (e) {
			    	$('#categories-menu').removeClass('on');
			    });
			    $('#categories-menu').mouseout(function () {
			    	$('#categories-menu').removeClass('off');	// Remove the override
			    });
				
				// Handle clicks inside/outside the expanded list
				$('body').click(function(e) {
					
					// Stop clicks on links in list from closing list
					if ($(e.target).is($('#categories-list a'))) {
						e.stopPropagation();
						return true;
					}
					
					// Stop clicks elsewhere in list from closing list
					else if ($(e.target).is($('#categories-list *'))) {
						return false;
					}
					
					// Clicks elsewhere close the list
					else if ($('#categories-list').hasClass('in')) {
						$('#categories-list').collapse('hide');
					}
				});
			});
		/*]]>*/
		</script>
