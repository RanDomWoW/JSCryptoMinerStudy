.enlace-afiliado.template_1{text-align:left;width:280px;position:relative;overflow:hidden;display:inline-block;border:1px solid #e2e2e2;border-radius:5px;vertical-align:top;text-decoration:none;margin:10px}
.enlace-afiliado.template_1:hover{box-shadow:0 0 5px #ccc}
.enlace-afiliado.template_1 img{vertical-align:middle;border-right:1px solid #e2e2e2}
.enlace-afiliado.template_1 .logo{display:inline-block}
.enlace-afiliado.template_1 .info{display:inline-block;vertical-align:middle;padding:5px;text-align:left}
