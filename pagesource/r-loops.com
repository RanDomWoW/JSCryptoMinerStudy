<script>
    function getFilteredItems() {
        var parameters = "";
        var currentParameters = window.location.href.match(/[a-z\d]+=[a-z\d]+/gi);
        var countParameters  = currentParameters? currentParameters.length : 0;
        $("#checkedFilters div").empty();
        $('.filter input:checkbox:checked').each(function () {
            var val = $(this).val();
            var name = $(this).attr('name');
            var id = $(this).attr('id');
            var filterName = $(this).attr('data-filterName');
            if (countParameters == 0) {
                parameters += "?filter[" + name + "][]=" + val;
            } else {
                parameters += "&filter[" + name + "][]=" + val;
            }
            countParameters++;
            // add checked filter
            var HTML_TAG = '' +
                    '<button  id="uncheck-'+id+'" data-uncheck="'+id+'" class="filterAction tag r_corners" >' +
                    filterName+' <i class="fa fa-times"></i></button>';

            $("#checkedFilters div").append(HTML_TAG);

        });

        if( $('#checkedFilters div').is(':empty') ) {
            $("#checkedFilters").fadeOut(100);
        }else{
            $("#checkedFilters").fadeIn();
        }

        var getParam = "?";
        if (countParameters > 0) {
            getParam = "&";
        }

        var from = $("#price-from").val();
        var to = $("#price-to").val();

        if (to != "" && from != "") {
            var prices = getParam + "filter[price][]=" + from + "&filter[price][]=" + to;
        } else {
            var prices = "";
        }


        return parameters + prices;
    }
    $("body").on("click", ".filterAction", function (e) {
        var id = $(this).closest("div").attr("id");
        var url = $(location).attr('href');

        // remove checked filter
            if (typeof $(this).attr('data-uncheck') != 'undefined') {
                 $("#"+$(this).attr('data-uncheck')).prop("checked", false);
                 $("#uncheck-"+$(this).attr('data-uncheck')).remove();
          }

        var parameters = getFilteredItems();
        setTimeout(function () {
            historyAjax(url + parameters, {}, e, "GET");
        }, 100);
    });

    $("body").on("click", "#filterReset", function (e) {
        var priceFrom = 0;
        var priceTo = 50;

        $("#price-from").val(priceFrom);
        $("#price-to").val(priceTo);
        $('.filter input:checkbox:checked').each(function () {
            $(this).prop("checked", false);
        });
        $("#price-range").slider("option", "values", [priceFrom, priceTo]);
        $("#price-current").text("$"+priceFrom+" - $"+priceTo)


    });

    $("#price-range").slider({
        range: true,
        min: 0,
        max: 200,
        values: [0, 50],
        slide: function (event, ui) {
            var from = ui.values[0];
            var to = ui.values[1];
            $("#price-from").val(from);
            $("#price-to").val(to);
            $("#price-current").text("$" + from + " - $" + to);
        },
        change: function (event, ui) {
            var from = ui.values[0];
            var to = ui.values[1];
            var url = $(location).attr('href');
            var parameters = getFilteredItems();

            setTimeout(function () {
                historyAjax(url + parameters, {}, event, "GET");
            }, 100);
        }
    });
</script>
