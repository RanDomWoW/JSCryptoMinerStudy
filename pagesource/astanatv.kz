	<script>
		$(function () {
			var animationInProgress = false;
			var cur = 0;
			var max = $('.sd-slide').length;
			var obj = [];

			$('.sd-slide').each(function(index) {
				$(this).addClass('sd-slide-' + index);
				$(this).css('left', index * 33.33 + "%");
				var o1 = $(this).clone().css('left', (max + index) * 33.33 + '%');
				var o2 = $(this).clone().css('left', (index - max) * 33.33 + '%');
				obj.push(o1);
				obj.push(o2);
			});

			for (var i=0; i < obj.length; i++) {
				$('.sd-slides').append(obj[i]);
			}

			$('.sd-next').click(function() {
				if (max < 4 || animationInProgress) return; // если всего 3 слайда - ничего не делать
				animationInProgress = true;
				cur --;
				var pos = cur * 33.33333 + "%";
				slideIt(pos, true);
			});

			$('.sd-prev').click(function() {
				if (max < 4 || animationInProgress) return; // если всего 3 слайда - ничего не делать
				animationInProgress = true;
				cur ++;
				var pos = cur * 33.33333 + "%";
				slideIt(pos, false);
			});


			function slideIt(pos, isNext) {
				$('.sd-slides').animate({
					left: pos
				}, 300, function() {
					if (Math.abs(cur) == max) reArrange();
					animationInProgress = false;
				});
			}

			function reArrange() {
				cur = 0;
				$('.sd-slides').css('left', '0');
			}

		});
	</script>

