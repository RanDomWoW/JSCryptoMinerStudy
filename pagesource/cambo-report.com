<script>
    function openColorBox(){
        $.colorbox({
            transition:"elastic",
            width:"50%",
            height:"auto",
            href: "http://cambo-report.com/storage/adverties/SoKvbUGSUH1KtFXctHTRV8MvaQ2ZmzBxi1ixcLwN.jpeg",
            onLoad: function() {
                $('#cboxClose').remove();
                setTimeout(function(){
                    $(window).colorbox.close();
                }, 5000)
            }
        })(jQuery);
    }

    function countDown(){
        seconds--
        $("#seconds").text(seconds);
        if (seconds === 0){
            openColorBox();
            clearInterval(i);
        }
    }

    var seconds = 1,
        i = setInterval(countDown, 1000);
</script>
