			}		
		}
	
		function changeNavLevel($els, inc, fromLevel) { // only first is required; must all be same level
			if ( $els.length > 0 ) {
				inc = inc || -1;
				fromLevel = fromLevel || getNavLevel($els);
				// console.log('changeNavLevel - $els:', $els, 'fromLevel:', fromLevel, 'inc:', inc);
				var toLevel = parseInt(fromLevel) + inc;
				$els.removeClass('nav-level-' + fromLevel).addClass('nav-level-' + toLevel);					
			}
		}
							
		// If cur page is within subsite...		
		if ( $subsiteItem.length > 0 ) {
			$('html').addClass('is-subsite');						
			$subsiteItem.siblings().remove(); // Remove all siblings 
			changeNavLevel($mainNavs.find('li.nav-level-2'), -1, '2'); 
			changeNavLevel($mainNavs.find('li.nav-level-3'), -1, '3'); 
			changeNavLevel($mainNavs.find('li.nav-level-4'), -1, '4'); 
			changeNavLevel($mainNavs.find('li.nav-level-5'), -1, '5'); 
			changeNavLevel($mainNavs.find('li.nav-level-6'), -1, '6'); 
			// $subsiteItem.removeClass('nav-level-2').addClass('nav-level-x'); // hides child link e.g. 'Engagement'
			// $mainNavs.find('li.nav-level-2').removeClass('nav-level-2').addClass('nav-level-x');
			// $mainNavs.find('li.nav-level-3').removeClass('nav-level-3').addClass('nav-level-2');
			// $mainNavs.find('li.nav-level-4').removeClass('nav-level-4').addClass('nav-level-3');
			// $mainNavs.find('li.nav-level-5').removeClass('nav-level-5').addClass('nav-level-4');
			// $mainNavs.find('li.nav-level-6').removeClass('nav-level-6').addClass('nav-level-5');
		} else {
			$('html').addClass('not-subsite');
		}

		// =========================================================
		// =Alt Menu - styles: remove lvl 4
		// (must be AFTER level has been adjusted–since might be subsite and #s might be adjusted with JS)
		// =========================================================

		if ( altMenu ) {			
			
			$mainNavs.find('li.nav-level-2').each(function(){ // top level items
				
				var $this = $(this);
				
				if ( !$this.hasClass('show-grandchildren-if-alt') ) {
					
					// Remove submenus within dropdowns
					$this.find('li.nav-level-3').children('ul').remove();	
												
				} else {
					
					// Remove sub-submenus within dropdowns
					$this.find('li.nav-level-4').children('ul').remove();	
					
					// Split level 3 into cols
					var $level2Submenu = $this.children('ul');
					var childCt = $level2Submenu.children('li:not(.submenu-heading)').length;	
					$this.addClass('has-' + childCt + '-items');		
																
				}
			});
			
		}

		// =========================================================
		// .skip-children
		// =========================================================
		
		var $skipChildren = $mainNavs.find('li.skip-children');
		
		// If .skip-children item exists within #navMain...
		if ( $skipChildren.length > 0 ) {
	
			var parentLevel = getNavLevel($skipChildren);
			var grandchildLevel = (parseInt(parentLevel) + 2).toString(); // More efficient to calculate this now
			var $children2Skip = $skipChildren.children('ul').not('submenu-curated').children('li');
			
			$children2Skip
			.each(function(){
				var $child2Skip = $(this);
				var $grandchildren = $child2Skip.children('ul').children('li');			
				changeNavLevel($grandchildren, -1, grandchildLevel); 			
				$child2Skip.after($grandchildren);			
			}).remove(); // self destruct
			
		}	

		// =========================================================
		// Remove top-level .hide items (helps with :last-child)
		// =========================================================
		$mainNavs.find('li.nav-level-2').filter('.hide').remove();

		$(window).trigger('subsiteModsLoaded');
		subsiteModsLoaded = true;

	}		
})(jQuery);
</script>

