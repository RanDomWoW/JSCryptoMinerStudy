			}
			
		}); 
		
		$('.scrollup').click(function(){
			$("html, body").animate({ scrollTop: 0 }, 700);
			return false;
		});

	});
	
	function submit_form(){
		
		if(document.getElementById('form_input').value != ''){	
		
			var email = document.getElementById('form_input').value	
		
			document.getElementById('form').innerHTML = '<div style="margin-top: 25px; padding-top: 15px; text-align: center;"><img src="images/ajax-loader2.gif" alt="" border="0" \/></div>';
			if(window.XMLHttpRequest){
				xmlhttp=new XMLHttpRequest();
			}else{
				xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
			} 
	    
	    url = 'ajax/nform.php?email='+email+'';
	    xmlhttp.open("GET", url, false);
			xmlhttp.send(null);
			a = xmlhttp.responseText;
	    document.getElementById('form').innerHTML = a;
	    setCookieJS('newsletter_popup', 1, 5);
			return false;
		
		}else{
			return false;
		}
		
	}
	
	function setCookieJS(c_name,value,exdays){
		var exdate=new Date();
		exdate.setDate(exdate.getDate() + exdays);
		var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
		document.cookie=c_name + "=" + c_value;
	}
	
	function hide_nform(){
		$('.nform').slideUp();
		setCookieJS('newsletter_popup', 1, 30);
		document.getElementById('hide').value = 1;
	}

