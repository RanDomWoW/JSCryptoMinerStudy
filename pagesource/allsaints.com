<script>
                    $(function(){

                        function addGCWithSpinner($element,amount){
                            $("#mobile-giftcard-error").hide()
                            contents = $element.html()
                            $element.addClass("pending");
                            $element.html('<img style="width:10px !important;height:10px;margin:auto;" width="10px" height="10px" src="/img/ajax-loading.gif"/>')
                            clear = function(){
                                $element.removeClass("pending")
                                $element.html(contents)
                            }
                            displayError = function(error_message){
                                clear()
                                $("#mobile-giftcard-error").html(error_message)
                                $("#mobile-giftcard-error").show()
                            }
                            addGiftcardToBasket(amount,clear,displayError);
                        }

                        $("input[name=mobile-gifcard-amount]").on("keydown",function(e){
                            if(e.which == 13) {
                                e.preventDefault()
                                if($("input[name=mobile-gifcard-amount]").val()!=""){
                                    addGCWithSpinner($("#mobile-giftcardAdder"),$("input[name=mobile-gifcard-amount]").val());
                                }
                            }
                        })
                        $("#mobile-giftcardAdder").on("click",function(e){
                            e.preventDefault();
                            addGCWithSpinner($(this),$("input[name=mobile-gifcard-amount]").val());
                        })
                        $("#mobile-quickadd-50").on("click",function(e){
                            e.preventDefault();
                            addGCWithSpinner($(this),50)
                        })
                        $("#mobile-quickadd-100").on("click",function(e){
                            e.preventDefault();
                            addGCWithSpinner($(this),100);
                        })
                        $("#mobile-quickadd-150").on("click",function(e){
                            e.preventDefault();
                            addGCWithSpinner($(this),150);
                        })
                        $("#mobile-quickadd-200").on("click",function(e){
                            e.preventDefault();
                            addGCWithSpinner($(this),200);
                        })
                        $("#mobile-quickadd-500").on("click",function(e){
                            e.preventDefault();
                            addGCWithSpinner($(this),500);
                        })
                    })
                    </script>
