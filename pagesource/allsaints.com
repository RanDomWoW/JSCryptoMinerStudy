// Carousel manager module
var CarouselOlapicManager = {
overlayOn: false,
disableTransform: 'disable-transform',
init: function() {
   this.cacheDOM();
   this.setDefaults();
   this.bindEvents();
},
cacheDOM: function() {
   // Pure js selectors
   this.slickList = document.querySelectorAll('.slick-list');
   this.slickTrack = document.querySelectorAll('.slick-track');
   this.olapicInspireElements = document.querySelectorAll('.olapic-inspire');
   this.olapicSlider = document.querySelector('.olapic-campaign');
   // jQuery selectors
   this.$sliders = $(".slick-slider.slick-dotted");
   this.$slides = $('.slides');
},
setDefaults: function() {
   this.elementsArray = Array.prototype.slice.call(this.slickList)
       .concat(Array.prototype.slice.call(this.slickTrack));

   if (/iPhone/.test(navigator.userAgent) && !window.MSStream) {
       this.checkVisitedOlapicCampaign(this.olapicSlider);
   };
},
bindEvents: function() {
   this.slickTrack.forEach(function(eachNode) {
       eachNode.addEventListener('click', this.detectOpenedOverlay.bind(this), false);
   }.bind(this));

   this.olapicInspireElements.forEach(function(each) {
       each.addEventListener('click', this.onOlapicElementClicked.bind(this), false);
   }.bind(this));
},
checkVisitedOlapicCampaign: function(element) {
   var validateVisited = 'olapic_campaign_visited';
   var store = window.localStorage;
   if (!store.getItem(validateVisited)) {
       store.setItem(validateVisited, true);
   };
},
detectOpenedOverlay: function(event) {
   var self = this;
   if (event.target.dataset.opiAs === 'openLayout' && !this.overlayOn) {
       this.$slides.each(function() {
           var slick = $(this).slick('getSlick');
           $(this).find('.slick-track').css('margin-left', (-slick.currentSlide * slick.listWidth - slick.listWidth) + "px");;
       });
       this.elementsArray.forEach(function(node) { node.classList.add('class', this.disableTransform) }.bind(this));
       this.overlayOn = true;
       this.$sliders.each(function(index, element) {
           element.slick.paused = true;
       });
   } else if (event.target.dataset.opiAs === 'closeLayout' && this.overlayOn) {
       setTimeout(function () {
           this.$slides.each(function() {
               var slick = $(this).slick('getSlick');
               $(this).find('.slick-track').css('margin-left', "0px");;
           });
           this.elementsArray.forEach(function(node) { node.classList.remove('class', this.disableTransform) }.bind(this));
           
       }.bind(this), 400);
       this.overlayOn = false;
   };
},
onOlapicElementClicked: function() {
   this.$sliders.each(function(index, element) {
       element.slick.paused = true;
   });
}
};

// CustomYTPlayer prototype
function CustomYTPlayer() {
this.currentScrollDistance = 0;
this.heightOfBrowserViewPort = 0;
this.currentPlayTime = 0;
this.playerObject = null;
this.videosData = [];
}

CustomYTPlayer.prototype.addPlayerReference = function(videoElementReference, mediaCode) {
this.videosData.push({
   elements: {
       playerId: videoElementReference
   },
   videoId: mediaCode,
   onReady: function (event) {
       this.largePlayerObject = event.target;
   }
});
return this;
}

CustomYTPlayer.prototype.getPlayerInstance = function() {
return this.videosData;
}

// FADE
// FadeIn prototype
function FadeIn(cssTargetClass, classesToAdd) {
this.fadeInElements = $(cssTargetClass);
this.enablingClass = 'kiosk-enabled';
this.windowWidth = 1080;
this.bottomOfWindow;

this.addClasses(classesToAdd);
};

FadeIn.prototype.addClasses = function(classString) {
if (!$(document.body).hasClass(this.enablingClass) && window.innerWidth !== this.windowWidth) {
   this.fadeInElements.each(function() {
       $(this).addClass(classString);
   });
}
}

FadeIn.prototype.bindScrollEvent = function() {
$(window).scroll( function(){
   /* Check the location of each desired element */
   this.fadeInElements.each( function(i){

       var topOfObject = $(this).position().top;
       var $hook = $(this).find('.fadein--hook'); 
       this.bottomOfWindow = $(window).scrollTop() + $(window).height();
       /* If the object is completely visible in the window, fade it it */
       if( this.bottomOfWindow > topOfObject ){ 
           $hook.addClass('fade-initial'); 
       }    
   }); 
}.bind(this));
}

FadeIn.prototype.bindResizeEvent = function() {
$(window).on('resize', function() {
   this.bottomOfWindow = $(window).scrollTop() + $(window).height();
}.bind(this));
}

FadeIn.prototype.initEvents = function() {
// start only once at the start
   this.fadeInElements.each( function(i){

   var topOfObject = $(this).position().top;
   var $hook = $(this).find('.fadein--hook'); 
   this.bottomOfWindow = $(window).scrollTop() + $(window).height();
   /* If the object is completely visible in the window, fade it it */
   if( this.bottomOfWindow > topOfObject ){ 
       $hook.addClass('fade-initial'); 
   }    
}); 
this.bindScrollEvent();
this.bindResizeEvent();
return this;
}

