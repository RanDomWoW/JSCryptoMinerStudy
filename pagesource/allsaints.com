var slideChangeCount = 0,
sliderSelector = '.js-custom-slider .slides';
function setSlidesHeight() {
$('.slick-slide').css('height', 'auto');
var highestSlideHeight = 0;
$('.slick-slide').each(function () {
   if(highestSlideHeight < $(this).height()) {
       highestSlideHeight = $(this).height();
   }
});
}
$(function () {
var slideElementsCount = document.querySelectorAll('.slide-el').length;
var slides = $(sliderSelector).find('.content-block');
var slideCount = 0, slideIndex = 0;
var thisSlideIndex, buttonsContainer, sectionButtonsContainer, clonedElement;

// beforeChange
$(sliderSelector).on('beforeChange', function(event, slick, currentSlide, nextSlide){
   if(slides.eq(nextSlide).data('dots') == 'dark') {
       $(sliderSelector).find('.slick-dots, .slick-arrow').addClass('dark-theme');
   }  else {
       $(sliderSelector).find('.slick-dots, .slick-arrow').removeClass('dark-theme');
   }
});

// afterChange
$(sliderSelector).on('afterChange', function(event, slick, currentSlide, nextSlide){
   slideCount++, slideIndex++;

   // Autoplay for carousel slider
   if (slideElementsCount.length !== 0 && slideCount === (slideElementsCount * 3)) {
       $(".slick-slider.slick-dotted").each(function(index, element) {
           element.slick.paused = true;
       });
   }

   // Dynamic button change
   thisSlideIndex = this.querySelector('.slick-active');
   buttonsContainer = this.parentNode.querySelector('.dynamic-buttons');
   sectionButtonsContainer = this.parentNode.querySelector('.button-inject');

   if (buttonsContainer && thisSlideIndex && sectionButtonsContainer) {
       // Removing existing child
       var firstChild = sectionButtonsContainer.firstElementChild;
       sectionButtonsContainer.removeChild(firstChild);

       // Adding cloned (new) element
       clonedElement = buttonsContainer.children[Number(thisSlideIndex.dataset.slickIndex)].cloneNode(true);
       sectionButtonsContainer.appendChild(clonedElement);
   }

});
$(sliderSelector).on('init', function() {

   // Dynamic button initial add
   thisSlideIndex = this.querySelector('.slick-active');
   buttonsContainer = this.parentNode.querySelector('.dynamic-buttons');
   sectionButtonsContainer = this.parentNode.querySelector('.button-inject');

   if (buttonsContainer && thisSlideIndex && sectionButtonsContainer) {
       // Adding clonded node
       clonedElement = buttonsContainer.children[Number(thisSlideIndex.dataset.slickIndex)].cloneNode(true);
       sectionButtonsContainer.appendChild(clonedElement);
   }
});

// onSetPosition event
$(sliderSelector).on('setPosition', function () {
   setSlidesHeight();
});

