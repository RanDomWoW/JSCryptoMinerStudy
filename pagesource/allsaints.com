<script>

    	$(document).ready(function(){
        if ('' == 'jp') {
            hideCountryOverlay();
        }
        else {
            $.ajax({
                "url":"/api/site-selector/",
                "success":function(data){

                    if (data!=undefined && data.length == undefined){

                        if (data["redirect_to"]!=undefined){

                            // close or back to current site
                            $("#disableSiteRedirect").click(function() {
                                tagGoogleAnalytics('GB_visitor_overlay', 'CLICK', 'STAY');
                                hideCountryOverlay();
                            });
                            $("#country-button-close").click(function() {
                                tagGoogleAnalytics('GB_visitor_overlay', 'CLICK', 'CLOSE');
                                hideCountryOverlay();
                            });

                            $('#country-continue-to').click(function() {
                                tagGoogleAnalytics('GB_visitor_overlay', 'CLICK', 'CONTINUE');
                                window.location.href = "//"+data["redirect_to"];
                            });
                        }

                        var $container = $(".persistent-site-selector");

                        $(".ss-call-customer-services").html(data["translations"]["call_our_customer_service_team"]);
                        $container.find(".ss-welcome").html("<h3>" + data["translations"]["welcome_to_allsaints"] + "</h3>");
                        $container.find(".ss-content").html(data["translations"]["site_selector_content"]);
                        $container.find(".ss-content2").html(data["translations"]["site_selector_content_second"]);
                        $container.find("#country-continue-to").html(data["translations"]["continue_to"]+" "+data["redirect_to"]);
                        $container.find("#disableSiteRedirect").html(data["translations"]["back_to_current_site"]);

                        if ($.cookie('disable_redirect') == undefined || $.cookie('disable_redirect') != "yes"){
                            showCountryOverlay();
                        }

                    }
                }
            });
        }
	});

    function tagGoogleAnalytics(category, event, label) {
        // Google Analitics tagging
        if (window._gaq != undefined) {
            window._gaq.push(['_trackEvent', category.toLowerCase(), event, label]);
        }
    }

    function hideCountryOverlay() {
        $.cookie('disable_redirect', 'yes', { expires: 30, path: '/' });
        $('#country-selector-overlay').hide();
    }

    function showCountryOverlay() {
        loadWelcomeImages();
        $('#country-selector-overlay').show();
    }

    /**
     * Replaces placeholder welcome images when popup is displayed.
     */
    function loadWelcomeImages() {
        var welcomeImageM = createImgTag("//images.allsaints.com/static/320/Amazon_Welcome_WW-01.jpg"),
                welcomeImageW = createImgTag("//images.allsaints.com/static/320/Amazon_Welcome_MW-02.jpg"),
                womenPlaceholder = document.getElementsByClassName("welcome-img-placeholder-women")[0],
                menPlaceholder = document.getElementsByClassName("welcome-img-placeholder-men")[0];

        womenPlaceholder.parentNode.replaceChild(welcomeImageW, womenPlaceholder);
        menPlaceholder.parentNode.replaceChild(welcomeImageM, menPlaceholder);
    }

    /**
     * Creates individual the Img tag
     * @param src
     * @returns {Element}
     */
    function createImgTag(src) {
        var welcomeImgTag = document.createElement('img');
        welcomeImgTag.src = src;
        welcomeImgTag.classList.add('adaptive');
        welcomeImgTag.setAttribute('data-adp-pattern', 'static/:width/');
        _adaptiveImage(welcomeImgTag);
        return welcomeImgTag;
    }
