(function(){
 var footerSlider = {
   slickSettings: {
     dots: false,
     infinite: true,
     speed: 500,
     slidesToShow: 1,
     centerMode: true,
     variableWidth: true,
     centerMode: true,
     accessibility: true,
     adaptiveHeight: true,
   },
   init: function() {
     this.cacheDom();
     this.bindEvents();
     this.keyNavigation();
     // this.setSlideDescription();
   },
   cacheDom: function() {
     this.$descWrapper = $('.cslider__res');
     this.$sliderPrev = $('.cslider__prev'); 
     this.$sliderNext = $('.cslider__next'); 
     this.$cslider = $('.cslider'); 
     this.$csliderSection = $('.cslider-section'); 
     this.$doc = $(document); 
   },
   keyNavigation: function(e) {
       var sliderFocus = this.$csliderSection.is(":focus");
       if(sliderFocus) {
           if(e.keyCode === 37) { this.$cslider.slick('slickPrev') }
           if(e.keyCode === 39) { this.$cslider.slick('slickNext') } 
           this.setSlideDescription();
       }
   },
   bindEvents: function() {
     this.$cslider.slick(this.slickSettings);
     this.$sliderPrev.on( 'click', this.changeSlide.bind(this,'slickPrev'));
     this.$sliderNext.on('click', this.changeSlide.bind(this, 'slickNext'));
     this.$doc.on('keydown', this.keyNavigation.bind(this));
   },
   changeSlide: function(dir) {
     this.$cslider.slick(dir);
     this.setSlideDescription();
   },
   toggleDesc: function(ad) {
     ad.addClass('cslider__res-desc--active').siblings().removeClass('cslider__res-desc--active');
   },
   setSlideDescription: function() {
     var activeNum = this.$cslider.find('.slick-center').attr('data-slide-index');
     var $activeDesc = this.$descWrapper.find("[data-slide-index='" + activeNum + "']");
     setTimeout(this.toggleDesc.bind(this, $activeDesc), 500 - 100);
   }
 }
 footerSlider.init(); 
})()



