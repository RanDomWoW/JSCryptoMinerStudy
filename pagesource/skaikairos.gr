	}
	
	function zoomAndCenter(){
	
		var bounds = new google.maps.LatLngBounds;
		for (var i = 0; i < markers_all.length; i++) {
			bounds.extend(markers_all[i].getPosition());
		}
		if (markers_all.length < 2) {
			map.setCenter(markers_all[0].getPosition(), 12);
			return;
		}
		map.fitBounds(bounds)
		return;
	}
	
	


      function showHideMarker() {
        marker.setVisible(!marker.getVisible());
      }


      function pinUnpinMarker() {
        var draggable = marker.getDraggable();
        marker.setDraggable(!draggable);
        marker.setClickable(!draggable);
