<script>

var isCorrect = false;

function checkLogin() {
	if ($.trim($("#user_id").val()) == "" || $("#user_id").val() == "아이디를 입력하세요.") {
		alert("아이디를 입력해 주시기 바랍니다.");
		$("#user_id").focus();
		return;
	}
	
	if ($.trim($("#user_pwd").val()) == "" || $("#user_pwd").val() == "비밀번호를 입력하세요.") {
		alert("비밀번호를 입력해 주시기 바랍니다.");
		$("#user_pwd").focus();
		return;
	}
	
	if (isCorrect) {
		if ($.trim($("#captcha").val()) == "") {
			alert("자동입력 방지문자를 입력해주세요.");
			$("#captcha").focus();
			return;
		}
	}
	
	var uid = $("#user_id").val();
	var pwd = $("#user_pwd").val();
	
	//sslYn : Y
	$.ajax({
	
		
		url: 'https://' + document.domain + '/cmmn/ajax/loginProc.do',
		dataType: 'jsonp',
		jsonp: 'callback',		
		
		
	
		type: 'post',
		data: {
			userId : uid,
			userPwd : pwd,
			isCorrect : isCorrect,
			captcha : $("#captcha").val()
			//userPwd : MD5($("#user_pwd").val()),
		},
		success: function(json) {
			
			if (json) {
				switch(json.resultCode) {
				case "0":
					var backUrl = "/www/intro.do";
					
					if (backUrl.indexOf("regist.do") >= 0) {
						backUrl = "/www/main.do";
					}
					
					location.href = backUrl;
					break;
				case "2":
					var msg = [];
					msg.push('비밀번호를 5회 이상 잘못 입력할 경우, 정보보호를 위해');
					msg.push('자동입력방지문자를 함께 입력하셔야 합니다.');
					msg.push('<img id="captchaImg" src="/cmmn/captcha.do" alt="" style="width:240px; height:50px;" />&nbsp;<input type="text" name="captcha" id="captcha" value="" class="type_text" style="border: solid 1px black" />&nbsp;<a href="javascript:void(0);" onclick="changeCaptcha()">새로고침</a>');
					isCorrect = true;
					
					$("#loginMessage").html(msg.join('<br />'));
					
					$('#captcha').focus();
					if (json.resultMsg != null && json.resultMsg.length > 0) {
						alert(json.resultMsg);
					}
					break;
				case "4444":
					alert("잠시 후 다시 시도해 주십시오.");
					location.href = "/";
					break;
				case "6":
					location.href="/www/member/pwdModify.do";
					break;
				default:
					alert(json.resultMsg);	
				}
			} else {
				alert("잠시 후 다시 시도해 주십시오.");
			}
		},
		error: function() {
			alert("잠시 후 다시 시도해 주십시오.");
		}
	});
	
}

function changeCaptcha() {
  //IE에서 '새로고침'버튼을 클릭시 CaptChaImg.jsp가 호출되지 않는 문제를 해결하기 위해 "?rand='+ Math.random()" 추가 
  //$('#catpcha').html('<img src="/common/captcha/CaptChaImg.jsp?rand='+ Math.random() + '"/>');
	$('#captchaImg').attr('src', '/cmmn/captcha.do?rand='+ Math.random() );
}

$(function() {
	$("#user_id").focus();
	$("#user_pwd").attr("autocomplete", "off");
	
});

function goSsoLogin(){
	var pathname=window.location.pathname;
	var search=window.location.search;
	var backPath = encodeURIComponent(pathname+search);

	location.href="/sso/business.jsp?backUrl="+backPath;

}

