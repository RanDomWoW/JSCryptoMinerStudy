			var Func = {

				// Panel select box Action
				SetSelectBox : function(target) {

					//console.log('### Panel - SetSelectBox');
					//console.log('### Panel - SetSelectBox - ', target);

					var self = this;

					var title = $(target).attr('title');
					var selectValue = target.val();

					//console.log('selectValue', selectValue);

	
					$.each(target.find('option'), function(i, opt) {

				if($(opt).val() != selectValue)
				{
							$(opt).removeAttr('selected');
						}
					});


			if( selectValue != '' )
			{
						title = target.find(':selected').text();
						target.find(':selected').attr('selected', true);
			}
			else
			{
				if(target.attr('id').indexOf('end') > -1)
				{
							title = '도착공항';
				}
				else if(target.attr('id').indexOf('start') > -1)
				{
							title = '출발공항';
				}
				else
				{
							title = '선택하세요.';
						}
					}

			$(target).css({'z-index':10,'opacity':0,'-khtml-appearance':'none'});
					$(target).next('.select').remove();
			
			$(target).after('<span class="select" style="color:#777;font-size:15px;text-align:center;text-indent:0;font-weight:bold;padding-right:15px;">' + title + '</span>');

			$(target).change(function(){
										val = target.find(':selected').text();

				if( val == '' )
				{
					if(target.attr('id').indexOf('end') > -1)
					{
												val = '도착공항';
					}
					else if(target.attr('id').indexOf('start') > -1)
					{
												val = '출발공항';
					}
					else
					{
												val = '선택하세요.';
											}
										}

										$(target).next('.select').text(val);
										//console.log('$(target).next(.select).text()', $(target).next('.select').text());

									});

			if(self.isChange == true)
			{
						target.hide();
			}
			else
			{
						target.show();
					}

				}, //	End SetSelectBox	


		
