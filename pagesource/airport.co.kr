			<script>
			function mainPopupZone(basIdx) {

				var mainRollList = $("#event-popup .popup_tab span");
				var mainRollBtn = $("#event-popup .popup_tab .ico");
				for (i=0;i<mainRollList.length;i++)	{
					mainRollList[i].style.display='none';
				}

				mainRollList[0].style.display='block';
				mainRollBtn[0].src = mainRollBtn[0].src.replace(".png","_on.png");

				var tempNowImg = 0;
				function mainRollInt() {
					for (i=0;i<mainRollList.length;i++)	{
						mainRollList[i].style.display='none';
						if (mainRollBtn[i].src !== undefined) {
							mainRollBtn[i].src = mainRollBtn[i].src.replace("_on.png",".png");
						} else {
							mainRollBtn[i].src = "";
						}
					}

					if (tempNowImg==mainRollList.length - 1) {
						mainRollList[0].style.display = 'block';
						if (mainRollBtn[0].src !== undefined) {
							mainRollBtn[0].src = mainRollBtn[0].src.replace(".png","_on.png");
						} else {
							mainRollBtn[0].src = "";
						}
						tempNowImg=0;
					} else {
						mainRollList[tempNowImg + 1].style.display = 'block';
						if (mainRollBtn[tempNowImg + 1].src !== undefined) {
							mainRollBtn[tempNowImg + 1].src = mainRollBtn[tempNowImg + 1].src.replace(".png","_on.png");
						} else {
							mainRollBtn[tempNowImg + 1].src = "";
						}
						tempNowImg = tempNowImg + 1;
					}
				}
				var mainRollSi = setInterval(mainRollInt,5000);
				$("#event-popup .popup_tab span").mouseover(function() {
					clearInterval(mainRollSi);
				})
				$("#event-popup .popup_tab span").mouseout(function() {
					mainRollSi = setInterval(mainRollInt,5000);
				})
				$(".isstop_" + basIdx).click(function(){
					clearInterval(mainRollSi);
				});
				$(".isplay_" + basIdx).click(function(){
					mainRollSi = setInterval(mainRollInt,5000);
				});
				/* 롤링 관련 부분 끝 */

				/* 온클릭 부분 시작*/
				for (var i=0;i<mainRollList.length;i++) {
					$("#event-popup .popup_tab .link").bind('keyup click' , function() {
						clearInterval(mainRollSi);
						//var liEl = this.parentNode.parentNode.parentNode.parentNode.getElementsByTagName("dd");
						for (j=0;j<mainRollList.length;j++)
						{
							mainRollList[j].style.display='none';
							mainRollBtn[j].src = mainRollBtn[j].src.replace("_on.png",".png");
						}
						mainRollList[this.href.split("popupTab" + basIdx + "_")[1]-1].style.display = 'block';
						mainRollBtn[this.href.split("popupTab" + basIdx + "_")[1]-1].src = mainRollBtn[this.href.split("popupTab" + basIdx + "_")[1]-1].src.replace(".png","_on.png");
						tempNowImg = this.href.split("popupTab" + basIdx + "_")[1]-1;
						return false;
					});
				}
				/* 온클릭 부분 끝 */
			}
			
