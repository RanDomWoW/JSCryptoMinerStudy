					var self = this;
					self.bgImg = new Array();
					self.randomBG();
					self.Event();
				},

				Event : function() {

					var self = this;

			$.each(Obj.air_map.area.find('li'), function(idx, obj) {

				$(obj).find('a:first').click(function() {
					
					var bgImg = '/site/www/common/img/intro/intro_bg_' + $(obj).attr('class') + '.jpg';
					
					$('#intro_main').css('background-image','url(' + bgImg + ')');
					
					var airport	=	$(obj).data('airport');

															// 항공권 예약 국내선 출발지 변경 이벤트
					Obj.air_infor.input.domestic.start_air.val(airport);
