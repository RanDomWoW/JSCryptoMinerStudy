				Submit : function(isTab) {

					var self = this;

					var peopleCount = self.GetPeopleCount(isTab);
					var isProgress = true;

					var area;
					var frm;
					var target;
					var method;
					var actionUrl;
					var round_way;
					var one_way;
					var start_text;
					var start;
					var startDate;
					var change;
					var end_text;
					var end;
					var endDate;
					var adult;
					var child;
					var infant;
					var airline;
					var search;

				if(isTab == 'domestic')
				{

						frm = Obj.air_infor.frm.domestic;

						round_way = Obj.air_infor.round_way;
						one_way = Obj.air_infor.one_way;

						start = Obj.air_infor.input.domestic.start_air;
						startDate = Obj.air_infor.input.domestic.start_date;
						end = Obj.air_infor.input.domestic.end_air;
						endDate = Obj.air_infor.input.domestic.end_date;

						adult = Obj.air_infor.input.domestic.adult;
						child = Obj.air_infor.input.domestic.child;
						infant = Obj.air_infor.input.domestic.infant;

						//target				=	"domestic";
						method = "post";

						var siteIdx = Func.SelectAirport(start.val());

					actionUrl			=	"/" + siteIdx + "/extra/ticketSchedule/domestic2/layOut.do";


				}
				else
				{

						frm = Obj.air_infor.frm.international;

						round_way = Obj.air_infor.round_way;
						one_way = Obj.air_infor.one_way;

						start = Obj.air_infor.input.international.start_air;
						startDate = Obj.air_infor.input.international.start_date;
						end = Obj.air_infor.input.international.end_air;
						endDate = Obj.air_infor.input.international.end_date;

						adult = Obj.air_infor.input.international.adult;
						child = Obj.air_infor.input.international.child;
						infant = Obj.air_infor.input.international.infant;

						//target				=	"international";
						method = "post";

						var siteIdx = Func.SelectAirport(start.val());
					if(siteIdx == '')
					{
							siteIdx = Func.SelectAirport(end.val());
						}

					actionUrl			=	"/" + siteIdx + "/extra/ticketSchedule/international2/layOut.do";

					}

					// 모바일 여부 체크. 모바일인 경우, 항공권 예약 막음
					/*
					if($.browser.mobile == true)
					{
						alert('현재 모바일 브라우저 상에서는 항공권 예매가 불가능합니다.');
						isProgress = false;
					}
					 */

					// 출발공항 체크
				if(isProgress)	
				{
						resultValidate = Validate.Empty(start.val(), false);
					if(resultValidate.errCode != 0) alert('출발공항을 선택해주세요.');
						isProgress = resultValidate.status;
					}

				
					// 도착공항 체크
				if(isProgress)	
				{
						resultValidate = Validate.Empty(end.val(), false);
					if(resultValidate.errCode != 0) alert('도착공항을 선택해주세요.');
						isProgress = resultValidate.status;
						end.css('border-color', '#cc0000');
					}

					// 출발공항 도착공항 동일 여부 확인
				if(isProgress)	
				{
					if( start.val() == end.val() )
					{
							isProgress = false;
							alert('출발지와 도착지가 동일합니다.');
							end.css('border-color', '#cc0000');
						}

					}

					//출발일 체크
				if(isProgress)	
				{ 
						resultValidate = Validate.Empty(startDate.val(), false);
					if(resultValidate.errCode != 0) alert('출발일을 지정해주세요.');
						isProgress = resultValidate.status;
					}

					// 도착일 체크
				if(round_way.prop('checked'))
				{
					if(isProgress)
					{
						resultValidate		=	Validate.Empty(endDate.val(), false);
						if(resultValidate.errCode != 0) alert('돌아오는 날을 지정해주세요.');
							isProgress = resultValidate.status;
						}

					if(isProgress)
					{
						if( startDate.val() > endDate.val())
						{
								isProgress = false;
								alert('출발하는 날보다 도착하는 날이 빠릅니다.');
								endDate.css('border-color', '#cc0000');
							}
						}

					}

				
					// 탑승인원 체크, 0 이상
				if(isProgress)	
				{ 
						resultValidate = Validate.Empty(peopleCount, false);
					if(resultValidate.errCode != 0) alert('탑승 인원수를 지정하셔야 합니다.');
						isProgress = resultValidate.status;
						adult.css('border-color', '#cc0000');
					}
					// 탑승인원 체크, 9이하
				if(isProgress)	
				{ 
						resultValidate = Validate.Max(peopleCount, 10, false);
					if(resultValidate.errCode != 0) alert('총 탑승 인원수는 9명까지 입니다.');
						isProgress = resultValidate.status;
					}

				if(isProgress)
				{
					ga('send', 'event', 'Ticket', 'Intro ' + isTab, 'Ticket Intro ' + isTab);

						frm.attr('method', method);
						//frm.attr('target', target);
						frm.attr('action', actionUrl);
						frm.submit();

					}

				},

				// 성인, 어린이, 유아 탑승 수 합
