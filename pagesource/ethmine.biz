<script type="text/javascript">
function calcthis()
{
	var depo = document.getElementById("money").value;

	if (depo < 0.001) {
        $("#thisbarslide").css({"width": "0%"});
		document.getElementById("inpvar1").innerHTML =  '0.00';
		document.getElementById("inpvar2").innerHTML =  '0.00';
		document.getElementById("inpvar3").innerHTML =  '0.00';
	}
	else if (depo < 5000) {
		var n1 = depo/100*2.6*14.33;
		var m1 = n1.toFixed(2);
		var n2 = depo/100*2.6;
		var m2 = n2.toFixed(2);
        $("#thisbarslide").css({"width": "33%"});
		document.getElementById("inpvar1").innerHTML =  '2.60 %';
		document.getElementById("inpvar2").innerHTML =  m1 +' MH/s';
		document.getElementById("inpvar3").innerHTML =  'USD ' + m2;
	}
	else if (depo < 20000) {
		var n1 = depo/100*2.8*14.33;
		var m1 = n1.toFixed(2);
		var n2 = depo/100*2.8;
		var m2 = n2.toFixed(2);
        $("#thisbarslide").css({"width": "66%"});
		document.getElementById("inpvar1").innerHTML =  '2.80 %';
		document.getElementById("inpvar2").innerHTML =  m1 +' MH/s';
		document.getElementById("inpvar3").innerHTML =  'USD ' + m2;
	}
	else {
		var n1 = depo/100*3*14.33;
		var m1 = n1.toFixed(2);
		var n2 = depo/100*3;
		var m2 = n2.toFixed(2);
        $("#thisbarslide").css({"width": "100%"});
		document.getElementById("inpvar1").innerHTML =  '3.00 %';
		document.getElementById("inpvar2").innerHTML =  m1 +' MH/s';
		document.getElementById("inpvar3").innerHTML =  'USD ' + m2;
	}
};
</script>

