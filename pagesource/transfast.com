<script>

    $(document).ready(function () {

        // Handle the transaction tracker call and animation
        var requestURL = '/api/sitecore/marketing/GetNewTransactions';

        // Initalize variables in closure
        var currentDisplace = 0,
            incrementDisplace = Number(-1 * $('.transaction-box').height()) - 30,
            cashPaymentImg = '/~/media/images/marketing/icons/country-page/send-money.ashx',
            bankPaymentImg = '/~/media/images/marketing/icons/Bank-Transactions.ashx',
            firstTime = true,
            col = true; // true corresponds to right column, false left column of tracker

        function getTransactions() {
            return $.ajax({
                url: requestURL,
                type: 'GET',
                dataType: 'json'
            });

        }

        // Get first set of transactions
        var promise = getTransactions();

        // Poll for transactions every 60 seconds
        var getTransactionInterval = setInterval(function () {
            var promise = getTransactions();
            promise.then(success, failure);
        }, 60000);

        // Success callback
        function success(response) {
            if (response.data && response.data.Transactions && response.data.Transactions.length > 0) {
                
                // Show transactions tracker given that transactions exist
                $('.transaction-tracker').removeClass('hide').show();

                var transactions = response.data.Transactions,
                    paymentModeImg = cashPaymentImg;

                // Iterate over each transaction and fill in the appropriate details
                for (var j = 0; j < transactions.length; j++) {

                    // Create UI box for transaction
                    var box = createUIElement(transactions[j]);

                    // Append to DOM
                    appendTransaction(box, transactions[j].ElapsedTime);

                    // Set interval on first call
                    if (firstTime) {
                        firstTime = false;
                        var animateTransactionsInterval = setInterval(function () {
                            animateTracker();
                        }, 2000);
                    }
                }
            } else {
                failure();
            }
        }

        // Error callback
        function failure(response) {
            $('.transaction-tracker').hide();
            clearInterval(getTransactionInterval);
            if (typeof animateTransactionsInterval !== 'undefined') {
                clearInterval(animateTransactionsInterval);
            }
        }

        // Create UI element for transaction box
        function createUIElement(transaction) {

            var paymentModeImg = '',
                receiveCountryName = '';
            transaction.ReceiverFirstName = transaction.ReceiverFirstName || '';
            transaction.ReceiveAmount = Number(transaction.ReceiveAmount).toFixed(2) || '';
            transaction.ReceiveCurrency = transaction.ReceiveCurrency || '';
            transaction.PaymentMode = transaction.PaymentMode || '';

            // Handle image icon
            if (transaction.PaymentMode == 'BANK DEPOSIT') {
                paymentModeImg = bankPaymentImg;
            } else {
                paymentModeImg = cashPaymentImg;
            }

            // Make country receiving name lowercase and with no spaces for svg
            if (transaction.SendCountry) {
                if (transaction.ReceiveCountry == 'UNITED STATES') {
                    receiveCountryName = transaction.ReceiveCountry.replace(' ', '-').toLowerCase();
                } else {
                    receiveCountryName = transaction.ReceiveCountry.replace(' ', '-').toLowerCase();
                }
            }

            // Compile HTML
            var box = '',
                firstName = transaction.ReceiverFirstName.split(' ')[0];

            if (firstName.length > 10) {
                transaction.ReceiverFirstName = firstName.slice(0, 9) + '...';
            } else {
                transaction.ReceiverFirstName = firstName;
            }

            box += '<div class="transaction-box clearfix">';
            box += '<div class="left-img">';
            box += '<span class="flag">';
            box += '<span class="flag-sprite-container"><svg class="flag-sprite"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#' + receiveCountryName + '"></use></svg></span>';
            box += '</span>';
            box += '<img class="wallet" src="' + paymentModeImg + '" alt="transaction" />';
            box += '</div>';
            box += '<div class="transaction-details">';
            box += '<span class="text">' + transaction.ReceiverFirstName + ' Received</span>';
            box += '<span class="cash-amount" style="font-size:24px;">' + transaction.ReceiveAmount + '</span>';
            box += '<span class="currency">' + transaction.ReceiveCurrency + '</span>';
            box += '<span class="bottom-text">As <span class="method">' + transaction.PaymentMode + '</span></span>';
            box += '</div>';
            box += '</div>';

            return box;

        }

        // Converts elapsed time for display
        // If > 1 hour, return null
        // If not, return back the mins
        function convertTime(elapsedTime) {

            if (elapsedTime) {
                var time = elapsedTime.split(':');

                if (time[0] >= 1) {
                    return null;
                } else {
                    return parseInt(time[1], 10);
                }

            }

        }

        // Append success and failure to promise
        promise.then(success, failure);

        // Append transaction to the flow
        function appendTransaction(box, time) {
            var convertedTime = convertTime(time),
                timeUnit = 'min',
                colName = '.right-tracker', // defaults to right column
                newTransaction = '',
                oval = '<img class="oval-tracker" src="/~/media/images/marketing/icons/tracker-oval.ashx" alt="oval tracker" />',
                ovalTop = '',
                ovalBottom = '',
                ovalBox = '';

            // If converted time is null, then set default 1 hour
            if (!convertedTime) {
                convertedTime = '1';
                timeUnit = 'hour';
            }

            // Decide on right or left column
            // col =
            if (!col) {
                colName = '.left-tracker';
                ovalTop = '';
                ovalBottom = oval;
            } else {
                ovalTop = oval;
                ovalBottom = '';
            }


            // Oval UI
            ovalBox += '<div class="transaction-tracker-circle">';
            ovalBox += ovalTop;
            ovalBox += '<span class="transaction-time-section">';
            ovalBox += '<hr class="line" />';
            ovalBox += '<span class="transaction-time">' + convertedTime + ' ' + timeUnit + '</span>';
            ovalBox += '</span>';
            ovalBox += ovalBottom;
            ovalBox += '</div>';


            // Compile New Transaction box
            newTransaction += '<div class="transaction-container">';
            newTransaction += box;
            newTransaction += ovalBox;
            newTransaction += '</div>';


            // Append new transaction to DOM
            $(colName).append(newTransaction);

            // Switch column flag for next transaction
            col = !col;

        }

        // Animate tracker for vertical shift
        function animateTracker() {
            currentDisplace = currentDisplace + incrementDisplace;
            $('.left-tracker').animate({ 'top': currentDisplace + 'px' }, 80);
            $('.right-tracker').animate({ 'top': currentDisplace + 'px' }, 80);
        }

    });


</script>                    <div id="content-mktg-green" class="row">
