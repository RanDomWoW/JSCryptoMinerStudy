<script type="text/javascript">
  $(function(){
    $(document).click(function(e) {
      var clickId = $(e.target).attr("data-panelid");
      if(!(clickId == "panelSchool" || clickId == "panelCollege" || clickId == "panelBachelor" || clickId == "panelMaster")){
        $(".container-list").slideUp(1000);
      }
    });
    
    // Hide/Show
    var previous_panel;
    $('.degrees').on('click',function(){
      var state = $('.container-list').css("display");      
      var panelId = $(this).attr("data-panelId");
      var panelColor = $(this).css("color");
      if(state == "none"){
        hideAllPanel();
        $("#" + panelId).addClass("show-panel");
        $('.container-list').css("border-style","solid");
        $('.container-list').css("border-color", panelColor);
        $('.container-list').css("border-width", "3px 1px 1px 1px");
        $('.grade-tag-child .circle').css("background-color", panelColor);
        $('.grade-block-child h2').css("color", panelColor);
        $('.container-list').slideDown(1000);
        previous_panel = panelId;
      }else{
        $('.container-list').slideUp(1000, function(){
          if(previous_panel == panelId)
          {
            $("#" + panelId).removeClass("show-panel");
          }
          else
          {
            hideAllPanel();
            $("#" + panelId).addClass("show-panel");
            $('.container-list').css("border-style","solid");
            $('.container-list').css("border-color", panelColor);
            $('.grade-tag-child .circle').css("background-color", panelColor);
            $('.grade-block-child h2').css("color", panelColor);
            $('.container-list').css("border-width", "3px 1px 1px 1px");
            $('.container-list').slideDown(1000);
            previous_panel = panelId;
          }
        });       
      }
    });

    
  
    function hideAllPanel(){
      $('.box').addClass("hide-panel");
      $('.box').removeClass("show-panel");
    }
  
    $("#grade-school h2").on('click', function(){
        $("#container-child-school").toggle(1000);
    });
    $("#grade-high-school h2").on('click', function(){
        $("#container-child-high-school").toggle(1000);
    });
    $("#grade-bachelor h2").on('click', function(){
        $("#container-child-bachelor").toggle(1000);
    });
    $("#grade-master h2").on('click', function(){
        $("#container-child-master").toggle(1000);
    });
    //Make Header automatically stick on top of the page after scrolling up.
    $(window).on('scroll', function () {

            if($(window).width() > 900){
              if ($(window).scrollTop() > 80) {
                  $('#learn-container').addClass('fixed'); //add class to make header on top and fixed positio
                  $('.grade').css("margin-top","90px"); //adjusting the body part
              } else {
                  $('#learn-container').removeClass('fixed'); //removing class from top of the page
                  $('.grade').css("margin-top","0px"); //re-adjusting the body to it's previous position
              }              
              if($('.container-list').css("display") == "none"){
                  if($(window).scrollTop() > 268){
                      $("#trending-header").addClass("fixed-trending-header");
                      $("#lft-ads").addClass("fixed-lft-ads");
                      $("#rlt-ads").addClass("fixed-rlt-ads");
                      $('#main-content').css("margin-left", "15%");
                      $("#content-container").css("margin-top", "58px");
                  }else{
                      $("#trending-header").removeClass("fixed-trending-header");
                      $("#lft-ads").removeClass("fixed-lft-ads");
                      $("#rlt-ads").removeClass("fixed-rlt-ads");
                      $('#main-content').css("margin-left", "0");
                      $("#content-container").css("margin-top", "0px");
                  }
              }else{
                  var con_height = ($('.container-list').height());
                  if($(window).scrollTop() > 268 + con_height){
                        $("#trending-header").addClass("fixed-trending-header");
                        $("#lft-ads").addClass("fixed-lft-ads");
                        $("#rlt-ads").addClass("fixed-rlt-ads");
                        $('#main-content').css("margin-left", "15%");
                        $("#content-container").css("margin-top", "58px !important");
                    }else{
                        $("#trending-header").removeClass("fixed-trending-header");
                        $("#lft-ads").removeClass("fixed-lft-ads");
                        $("#rlt-ads").removeClass("fixed-rlt-ads");
                        $('#main-content').css("margin-left", "0");
                        $("#content-container").css("margin-top", "0px");
                    }
                  }
              }
    });
    $("#playNwin").on('click',function(){
        $("#game-comming").slideToggle(500);
    });
  
  
  });
  
</script>
