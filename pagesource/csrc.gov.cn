<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<HTML>
 <HEAD>

 <BODY>
 <script>
var s=window.location.href;
var domainDatas=["beijing","tianjin","hebei","shanxi","neimenggu","liaoning","jilin","heilongjiang","shanghai","jiangsu","zhejiang","anhui","fujian","jiangxi","shandong","henan","hubei","hunan","guangdong","guangxi","hainan","chongqing","sichuan","guizhou","yunnan","xizang","gansu","qinghai","ningxia","xinjiang","shenzhen","dalian","ningbo","xiamen","qingdao"];
var newdomain="",sr="";
if(s.indexOf("csrc.gov.cn")>0)
{
		sr=s.indexOf(".csrc.gov.cn");
		s=s.substring(0,parseInt(sr));
		if(s.indexOf("www")>0)
		{
			newdomain=s+".csrc.gov.cn/pub/newsite/";
			window.location.href=newdomain;
		}
		else if((s.indexOf("eng")<8)&&(s.indexOf("eng")>-1))
		{
			newdomain=s+".csrc.gov.cn/pub/csrc_en/";	
			window.location.href=newdomain;
		}
		else if(s.indexOf("gkml")>0)
		{
			newdomain=s+".csrc.gov.cn/pub/zjhpublic/";
			window.location.href=newdomain;
		}
		else if(s.indexOf("do_not_delete")>0)
		{
			newdomain="http://www.csrc.gov.cn/pub/do_not_delete/";
			window.location.href=newdomain;
		}
		else if(s.indexOf("shaanxi")>0)
		{
			newdomain="http://shaanxi.csrc.gov.cn/pub/shanxidong/";
			window.location.href=newdomain;
		}
		else
		{
			for(var i=0;i<domainDatas.length;i++)
			{
				if(s.indexOf(domainDatas[i])>0)
				{
					newdomain=s+".csrc.gov.cn/pub/"+domainDatas[i]+"/"
					window.location.href=newdomain;
					break;
				}
			}
		}
}
else
{
	newdomain="http://www.csrc.gov.cn/pub/newsite/";
	window.location.href=newdomain;
}
 </script>
 </BODY>
</HTML>