  function datalayerAddressSubmitted() {
    if (!DL.IS_SITE_MOBILE) {
      var locationArea = document.getElementById('area_id').value
      var locationCity = document.getElementById('city_id').options[document.getElementById('city_id').selectedIndex].text;
    } else {
      var index = document.getElementById('IsSelectedDeliveryAddressCustomYes').checked ? 2 : 1;
      var locationArea = document.getElementById('area_id['+index+']').value
      var locationCity = document.getElementById('city_id['+index+']').options[document.getElementById('city_id['+index+']').selectedIndex].text;
    }

    dataLayer.push({
        'event' : 'address.submitted'
      , 'locationAddress': DL.str_userAddress
      , 'locationCity': locationCity
      , 'locationArea': locationArea
      , 'locationCountry': 'HRV'
      , 'locationMethod': 'manual'
    });
  }

  function datalayerShopListLoaded(categoryNum, sort, category, shopTotal, filter) {
    dataLayer.push({
        'event' : 'shop_list.loaded'
      , 'locationAddress' : DL.str_userAddress
      , 'locationCity' : document.getElementById('city_id').options[document.getElementById('city_id').selectedIndex].text
      , 'locationArea' : document.getElementById('area_id').value
      , 'locationCountry' : 'HRV'
      , 'shopCategorySelectedQuantity' : categoryNum
      , 'shopSortingSelected' : sort
      , 'shopCategorySelected' : category
      , 'shopQuantityTotal' : shopTotal
      , 'shopFilteringSelected' : filter
    });
  }

  function datalayerShopClicked(shopId, shopPosition) {
    var locationCity;
    var locationArea;

    if (!DL.IS_SITE_MOBILE) {
      var selectedCity = document.getElementById('city_id').selectedIndex;
      locationCity = document.getElementById('city_id').options[selectedCity].text;
      locationArea = document.getElementById('area_id').value;
    } else {
      locationCity = DL.str_cityNameSearch;
      locationArea = DL.int_idAreaSearch;
    }

    dataLayer.push({
        'event' : 'shop.clicked'
      , 'locationAddress' : DL.str_userAddress
      , 'locationCity' : locationCity
      , 'locationArea' : locationArea
      , 'locationCountry' : 'HRV'
      , 'shopId' : shopId
      , 'shopPosition' : shopPosition
    });
  }

  function datalayerShopDetailsLoaded() {
    dataLayer.push({
        'event' : 'shop_details.loaded'
      , 'locationAddress' : DL.str_userAddress
      , 'locationCity' : DL.str_cityNameSearch
      , 'locationArea' : DL.int_idAreaSearch
