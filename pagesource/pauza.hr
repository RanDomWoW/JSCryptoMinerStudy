      , 'shopMinimumOrderValue' : DL.fl_minOrderValue
      , 'shopDeliveryFee' : DL.fl_deliveryFee
    });
  }

  function datalayerAddCartClicked(hasMealAdditions, productTypeId, quantity, name, categoryName, price, addressType) {
    if (addressType == 'menu') {
      locationCity = document.getElementById('city_id').options[document.getElementById('city_id').selectedIndex].text;
      locationArea = document.getElementById('area_id').value;
    } else {
      locationCity = DL.str_cityNameSearch
      locationArea = DL.int_idAreaSearch
    }

    dataLayer.push({
        'event' : 'add_cart.clicked'
      , 'locationAddress' : DL.str_userAddress
      , 'locationCity' : locationCity
      , 'locationArea' : locationArea
      , 'locationCountry' : 'HRV'
      , 'shopId' : DL.int_sellerId
      , 'shopName' : DL.str_sellerName
      , 'productDetails' : hasMealAdditions
      , 'productSku' : productTypeId
      , 'productQuantity' : quantity
      , 'productName' : name.replace(/["']/g, "")
      , 'productCategory' : categoryName
      , 'currencyCode' : 'HRK'
      , 'productUnitPrice' : price
    });
  }

  function datalayerCheckoutClicked() {
    var cartValue;
    var locationArea;
    var locationCity;
    var shopId;

    if (DL.gbool_isPageMenu && DL.IS_SITE_MOBILE) {
      var shopId = '';
      var strarr_shopId = document.getElementsByClassName("datalayer-shop-id");
      for (var i=0; i < strarr_shopId.length; ++i) {
        shopId = shopId + strarr_shopId[i].innerHTML;
        if (i + 1 != strarr_shopId.length)
          shopId = shopId + ', ';
      }

      cartValue = 0;
      var strarr_cartValues = document.getElementsByClassName("datalayer-cart-value");
      for (var i=0; i<strarr_cartValues.length; ++i) {
        cartValue = cartValue + parseFloat(strarr_cartValues[i].innerHTML);
      }
    } else {
      shopId = DL.strarr_sellerIds;
      cartValue = DL.fl_cartValue;
    }

    // TODO(kburnik): Extract to method, as it's duplicated as above.
    if (!DL.IS_SITE_MOBILE) {
      var selectedCity = document.getElementById('city_id').selectedIndex;
      locationCity = document.getElementById('city_id').options[selectedCity].text;
      locationArea = document.getElementById('area_id').value;
    } else {
      locationCity = DL.str_cityNameSearch;
      locationArea = DL.int_idAreaSearch;
    }

    dataLayer.push({
        'event' : 'checkout.clicked'
      , 'locationAddress' : DL.str_userAddress
      , 'locationCity' : locationCity
      , 'locationArea' : locationArea
      , 'locationCountry' : 'HRV'
      , 'shopId' : shopId
      , 'currencyCode' : 'HRK'
      , 'cartValue' : cartValue
    });
  }

