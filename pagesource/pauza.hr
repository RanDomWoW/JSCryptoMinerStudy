  function datalayerTransactionClicked() {
    var paymentMethods = [];

    for (var i = 0; i < DL.intarr_idsSellers.length; i++) {
      var paymentMethodCheckbox =
          document.getElementById(
              'PaymentMethod' + DL.PAYMENT_METHOD_CASH + '_seller_id' +
              DL.intarr_idsSellers[i]);
      paymentMethods.push(
          paymentMethodCheckbox.checked ? 'Cash' : 'Credit card');
    }

    dataLayer.push({
        'event' : 'transaction.clicked'
      , 'locationAddress' : DL.str_userAddress
      , 'locationCity' : DL.str_userCityName
      , 'locationArea' : DL.int_userAreaId
      , 'locationCountry' : 'HRV'
      , 'shopId' : DL.strarr_sellerIds
      , 'orderPaymentMethod' : paymentMethods.join(', ')
      , 'coupon' : coupon
      , 'couponValue' : couponDiscount
    });
  }

  function datalayerTransaction(orderId) {
    if (orderId in DL.transactionOrderMap) {
      dataLayer.push(DL.transactionOrderMap[orderId]);
