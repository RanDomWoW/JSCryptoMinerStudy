<script>(function() {
  var missingResources = [];

  function handleMissingResourceError(e) {
    if (e.target && (e.target.href || e.target.src)) {
      var resourceUrl = e.target.href || e.target.src;
      var isSameOrigin = resourceUrl.startsWith(window.location.origin);
      missingResources.push({
        origin: isSameOrigin ? 'same-origin' : 'cross-origin',
        resourceUrl: resourceUrl,
        sourceUrl: window.location.toString(),
        elementType: e.target.tagName
      });
    }
  }

  window.addEventListener(
      'error',
      handleMissingResourceError,
      /*useCapture=*/true);

  window.addEventListener('load', function() {
    // TODO: Currently only report DOC-rendered missing resources, otherwise
    // need to listen to DOM changes for dynamically loaded content.
    window.removeEventListener(
        'error',
        handleMissingResourceError,
        /*useCapture=*/true);

    if (missingResources.length == 0) {
      return;
    }

    console.warn("Reporting", missingResources.length, "missing resources");
    // Use vanilla XHR as we can't really rely on jQuery to be loaded.
    var xhr = new XMLHttpRequest();
    xhr.open('POST', '/api/logs/reportMissingResources');
    xhr.send(JSON.stringify(missingResources));
    missingResources.splice(0);
  });
})();
</script>
