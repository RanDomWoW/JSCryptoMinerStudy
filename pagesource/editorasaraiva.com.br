    <script type="text/javascript">
        function loadAbas(localHash, pageLoad) {
            var idx = -1, subIdx = -1;
            
            switch (localHash.replace("#", ""))
            {
                case 'quem-somos': idx = 0; break;
                case 'selos-editoriais': idx = 1; break;
                case 'selos-editoriais/editora-saraiva': idx = 1; subIdx = 0; break;
                case 'selos-editoriais/atual-editora': idx = 1; subIdx = 1; break;
                case 'selos-editoriais/etico-sistema-de-ensino': idx = 1; subIdx = 5; break;
                case 'selos-editoriais/benvira': idx = 1; subIdx = 3; break;
                case 'selos-editoriais/caramelo': idx = 1; subIdx = 4; break;
                case 'selos-editoriais/formato': idx = 1; subIdx = 2; break;
                case 'selos-editoriais/agora-sistema-de-ensino': idx = 1; subIdx = 6; break;
                case 'nossa-historia': idx = 2; break;
                case 'catalogos': idx = 3; break;
                case 'livraria-saraiva': idx = 4; break;
                case 'relacao-com-investidores': idx = 5; break;
                case 'premiacoes': idx = 6; break;
                case 'trabalhe-conosco': idx = 7; break;
                default: idx = 0; break;
            }
            $(".abasGrupo").tabs({ 
            	selected: idx, 
            	select: function(event, ui) {
            		var hashtag = "";
            		if (ui.panel.id == 'quemSomos') hashtag = 'quem-somos';
            		if (ui.panel.id == 'selosEditoriais') hashtag = 'selos-editoriais';
            		if (ui.panel.id == 'nossaHistoria') hashtag = 'nossa-historia';
            		if (ui.panel.id == 'abaCatalogos') hashtag = 'catalogos';
            		if (ui.panel.id == 'livrariaSaraiva') hashtag = 'livraria-saraiva';
            		if (ui.panel.id == 'relacoesInvestidores') hashtag = 'relacoes-com-investidores';
            		if (ui.panel.id == 'abaPremiacoes') hashtag = 'premiacoes';
            		if (ui.panel.id == 'trabalheConosco') hashtag = 'trabalhe-conosco';
            			
                	window.location.hash = hashtag;
            	}
			});
            
            if (pageLoad) $('.acc_trigger:first').removeClass('active').next().hide();
            
            if (subIdx != -1) {
                $('.acc_trigger:eq(' + subIdx + ')').click();
                document.location = localHash;
            }
        }

        function voltar_ao_topo(callback){
            $('html, body').animate({scrollTop:0}, 'slow', function(){
                if(typeof callback ==="function"){
                    callback();
                } 
            });

        }

        function mostrar_tooltip_cadastro(){
            $("#txtEmailLogin").qtip(
                {
                    content: 
                    {   
                        title:
                        {
                            text: 'Atenção!',
                            button: 'Close' 
                        },
                        text: 'Você precisa estar logado para visualizar este conteúdo'
                    },
                    show: 
                    {
                        event: false,
                        ready: true
                    }, 
                    position:
                    {
                        my: 'top center',
                        at: 'bottom center'
                    },
                    style: {
                        classes: 'loginQtipWrapper'
                    },
                    hide: false
                }
            );
        }
        
        $(document).ready(function () {
        	
            $(window).scroll(function() {
		        // console.log($(document).scrollTop());
		        if ($(document).scrollTop() > 200) {
					$('#backtotop').show('slow');
                    
		        } else {
					$('#backtotop').hide(0);
		        }
		        
		        var posicaoScroll = $(document).scrollTop() + $(window).height();
		        var alturaPagina =$(document).height();  
		        
		        if (posicaoScroll > (alturaPagina - 200)) {
		        	$('#backtotop').css('bottom', - 108 - (alturaPagina - posicaoScroll) + 190 + "px");
                    $('#atualizeSeuCodigoImprimir').css('bottom', - 108 - (alturaPagina - posicaoScroll) + 190 + "px");
		        } else { 
		        	$('#backtotop').css('bottom', '-108px');
                    $('#atualizeSeuCodigoImprimir').css('bottom', '-108px');
	        	}
		        
		    });

			$('#backtotop a').click(function(){
				//$('html, body').animate({scrollTop:0}, 'slow');
                voltar_ao_topo();
				$('#backtotop').hide(); 
				return false; 
			}); 

        	
            $('head').conditionizr({ debug: false });
            $('.fancybox-media')
                .attr('rel', 'media-gallery')
                .fancybox({
                    'width': '730px',
                    'height': 'auto',
                    'type': 'iframe',
                    arrows : false

                });
            
            if (window.location.href.indexOf('o-grupo-saraiva') != -1) {
	            loadAbas(window.location.hash, true);
            }
            
            
            $('.seloClick').click(function() {
                var url = $(this).attr('href');
                loadAbas(url.substr(url.indexOf('#')), false);
                // console.log(url.substr(url.indexOf('#')));
            });

			// Tratamento do banner mini
			$(".miniBanner a").attr('target', '_self'); 
            
            // Tratamento do limite de caracteres do box de produto
            $(".dados .titulo").expander({ slicePoint: 60 });
            $(".dados .autor a").expander({ slicePoint: 45 });
            
            // Adiciona links aos conteúdos do twitter.
            $(".contentTwitter li").linkify({
                handleLinks: function (links) {
                    links
                        .attr('target', '_blank')
                        .css('color', '#fba355');
                }
            });
            
            
            $(".toolTipAutores").qtip(
                {
                    content: 
                    {   
                        title:
                        {
                            button: 'Close' 
                        },
                        text: function(api) {
                            return $(this).parent().parent().find(".qTipText").html();
                        }
                    },
                    show: {
                        event: 'click',
                        solo: true
                    },
                    position:
                    {
                        my: 'bottom center',
                        at: 'top center'
                    },
                    hide: false,
                    style: {
                        classes: 'autorQtipWrapper'
                    }
                }
            );
            
            // Verifico se suporta o atributo placeholder - campos de busca
            var placeholderSupport = "placeholder" in document.createElement("input");
            if (!placeholderSupport) {
            	$("input[name='query']").placeholder();
            }
        });
    </script>




