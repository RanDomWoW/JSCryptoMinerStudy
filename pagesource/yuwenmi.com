<script>
    /*indexBanner*/
    $(document).ready(function() {
        banCont = 0;
        aa = 0;
        aa = $(".banner_icon a").length;
        $(".banner_icon").css("width",45*aa+"px");
        var banGun; banner();
        banGun = setInterval("banner()",3000);
        $(".banner_icon a").click(function(){
            clearInterval(banGun);
            banCont = $(this).attr("value");
            banner();
            banGun = setInterval("banner()",3000);
        });
    });
    function banner(){
        if(banCont <= aa-1){
            $(".banner_pic").animate({left:-100*banCont+"%"});$(".banner_icon em").animate({left:33*banCont+"px"});}
        else{
            $(".banner_pic").animate({left:"0px"});banCont = 0;$(".banner_icon em").animate({left:"0px"});}
        banCont++;
    } </script>
