<script type="text/javascript">
	
(function (global) {
    // jsonp 接收用于请求数据的script dom对象
    global.pushInfoJsonpCallBack = pushInfoJsonpCallBack;
    global.jsonpDom = jp_jsonp("http://app.gd.gov.cn/xxts/pushinfo_json.php");

    function jp_jsonp (url) 
    {
        var jsonp_ = document.createElement('script');
        jsonp_.setAttribute('type', "text/javascript");
        jsonp_.setAttribute('src', url);
        document.body.appendChild(jsonp_);
        return jsonp_;
    }
    // 
    function pushInfoJsonpCallBack (data) 
    {
        var mbDom = document.getElementById('testUl');

        forEach(data, function(json, i) {
            if(i >= 6)return;
            var strA = json.pubDate.split('-');
            strA.shift();
            mbDom.innerHTML += "<li><a href='http://www.gdga.gov.cn/jump.htm?url="+json.link+"' target='_blank'>"+json.title+"</a><span>"+strA.join('-')+"</span></li>";

        });
        
        // 用于遍历数字
        function forEach (array, callBack) 
        {
            for(var i = 0; i < array.length; i++){
                callBack(array[i], i);
            }
        }
        // 请求完成 -- 删除 实现jsonp 的 script
        document.body.removeChild(global.jsonpDom);
    }
})(window)


</script>
