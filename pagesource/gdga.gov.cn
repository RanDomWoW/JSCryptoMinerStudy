function addLink(idName) {
  var wsbsUl = document.getElementById(idName);
  var wsbsLink = wsbsUl.getElementsByTagName('a');
  for(var i = 0; i < wsbsLink.length; i++){
    var url = wsbsLink[i].href;
    var str = "http://www.gdga.gov.cn/jump.htm?url=";
    wsbsLink[i].href = str + url;
  }
}
