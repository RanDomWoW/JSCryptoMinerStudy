                        $("#buca-ricerca-mobile").addClass("hidden");
                        $("#h-lens-mobile").removeClass("bg-bluscuro");
                    }
                });
                $('#mhSearchObjectTypeList li').on('click', function(){
                    $("#mhSearchObjectTypeList li").removeClass("active");
                    $(this).addClass('active');
                    $('#mhSearchObjectTypeLabel').html($(this).text());
                    setSelectedObjectType($(this).children('a:first').data('value'));
                    doSearch('mh');
                });
                $('#mh_search_string').keypress(function(e){
                    var keycode = (e.keyCode ? e.keyCode : e.which);
