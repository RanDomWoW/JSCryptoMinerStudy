<script>
    $(function () {
        $(document).on('click', '.btn-additional-hour', function () {
            var val = $(this).data("val");
            //console.log("btn click ", val);
            $("#AdditionalHour").val(val);
            //console.log(AdditionalHour);
            $('#additional-hour-modal').modal('hide');
            $("#frmSearch").submit();
        });

        $('#frmSearch').submit(function (event) {
            
            var additional_hour = Number($("#AdditionalHour").val());

            if (additional_hour < 0) {

                var start_h = Number($("#HireTimeFromHours").val());
                var start_m =  Number($("#HireTimeFromMinutes").val());
                var end_h =  Number($("#ReturnTimeFromHours").val());
                var end_m = Number($("#ReturnTimeFromMinutes").val());

                var start_d = $("#HireDate").val();
                var end_d = $("#ReturnDate").val();

                var diff = (end_h * 60 + end_m) - (start_h * 60 + start_m);

                //console.log("Hours: ", start_h, start_m, end_h, end_m, diff, start_d, end_d);

                if (diff >= 60 && diff <= 360 && start_d  != end_d) {

                    $('#additional-hour-modal').modal('show');

                    event.preventDefault();

                    return false;

                }

            }

            var form = $('#frmSearch');
            $("[type='submit']", form).attr("disabled", "disabled");

            NProgress.set(0.5);
            NProgress.start();
            
            return true;

        });

    });

</script>

