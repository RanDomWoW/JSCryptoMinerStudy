    $(function () {

        $('body').addClass('add-bg');

        App.initFormValidation($('#frmLogin'), {
            'PhoneOrIdentityNumber': {
                required: true,
                number: true
            },
            'BudgetCardNo': {
                required: true,
                number: true
            }
        }, {
            'PhoneOrIdentityNumber': {
                required: "L&#252;tfen cep Telefonunuzu veya T.C. numaranızı giriniz.",
                number: "L&#252;tfen ge&#231;erli bir cep telefonu veya T.C. numarası giriniz."
