            'BudgetCardNo': {
                required: "L&#252;tfen budget kart numarınızı giriniz.",
                number: "L&#252;tfen ge&#231;erli bir budget kart numarası giriniz."
            }
        });

        var today = new Date();
        var $hireTimeFromHours = $('#HireTimeFromHours');
        var $returnTimeFromHours = $('#ReturnTimeFromHours');
        var hours = today.getHours();

        if (hours === 24) {
            $hireTimeFromHours.val('03');
            $returnTimeFromHours.val('03');
        } else if (hours === 23) {
            $hireTimeFromHours.val('02');
            $returnTimeFromHours.val('02');
        } else if (hours === 22) {
            $hireTimeFromHours.val('01');
            $returnTimeFromHours.val('01');
        } else if (hours === 21) {
            $hireTimeFromHours.val('00');
            $returnTimeFromHours.val('00');
        } else if (hours < 7) {
            $hireTimeFromHours.val('0' + (hours + 3).toString());
            $returnTimeFromHours.val('0' + (hours + 3).toString());
        } else {
            $hireTimeFromHours.val(hours + 3);
            $returnTimeFromHours.val(hours + 3);
        }
    });
</script>

