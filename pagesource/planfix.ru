$(window).load(function() {
	pageTracker.trackPageByPos();
	var isV2 = $('#fixednavbar').hasClass('v2'), fixednavbarIsShow = true;
	if ($(window).scrollTop() > $("#page1").height() ) {
		if($(window).width() > 500) {
			$("#navbar").addClass("show");
		} else {
			$('#fixednavbar').addClass('border');
			if(window.location.hash != '') {
				$.smoothScroll({
					offset: -36,
					scrollTarget: window.location.hash
				});
			}
		}
    }
	if(isV2 && $(window).width() > 960) {
		$('#fixednavbar').hide();
		fixednavbarIsShow = false;
	}
//------------------------------------------------------------------------
//						NAVBAR SLIDE SCRIPT
//------------------------------------------------------------------------
	$(window).scroll(function () {
		if($(window).width() <= 960) {
			if(!fixednavbarIsShow) {
				fixednavbarIsShow = true;
				$('#fixednavbar').show();
			}
			if($(window).scrollTop() > 2) {
				$('#fixednavbar').addClass('border');
			} else {
				$('#fixednavbar').removeClass('border');
			}
		} else {
			// desktop
			if(isV2 && fixednavbarIsShow) {
				$('#fixednavbar').hide();
				fixednavbarIsShow = false;
			} else  {
				if ($(window).scrollTop() > $("#page1").height()) {
					$("#navbar").addClass("show");
					$('#fixednavbar').addClass('border');
				} else {
					$('#fixednavbar').removeClass('border');
					$("#navbar").removeClass("show");
				}
			}
		}

    });

