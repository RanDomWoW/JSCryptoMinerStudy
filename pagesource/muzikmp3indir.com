    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
    background-color: #afd027;

}
ul.mobmen li {float: left;}
ul.mobmen li a {
    display: inline-block;
    color: #f2f2f2;
    text-align: center;
    padding: 14px 35px;
    text-decoration: none;
    transition: 0.3s;
    font-size: 17px;
}
ul.mobmen li a:hover {background-color: #90064c;}
ul.mobmen li.icon {display: none;}
 
  ul.mobmen li:not(:first-child) {display: none;}
  ul.mobmen li.icon {
    float: right;
    display: inline-block;
  }
}
@media screen and (max-width:680px) {
  ul.mobmen.responsive {position: relative;}
  ul.mobmen.responsive li.icon {
    position: absolute;
    right: 0;
    top: 0;
  }
  ul.mobmen.responsive li {
    float: none;
    display: inline;
  }
  ul.mobmen.responsive li a {
    display: block;
    text-align: left;
  }

 
	ul.mobmen {
