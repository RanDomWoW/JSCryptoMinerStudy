<script>
$(window).load(function(){
	var win = $(window);
	var fxel = $('.in-page-nav');
	var eloffset = fxel.offset(30).top;
	
	$(window).scroll(function() {
	    if (eloffset < win.scrollTop()) {
	        fxel.addClass("fixed");
	    } else {
	        fxel.removeClass("fixed");
	    }
	});

		
});

</script>

