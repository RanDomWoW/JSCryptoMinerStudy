<script>
$(document).ready(function(){
	/* Number animation */
	var isanimated = false;
	$(window).scroll(function(){
		function elementsScrolled(elem){
			var docViewTop = $(window).scrollTop();
			var docViewBottom = docViewTop + $(window).height();
			var elemTop = $(elem).offset().top;
			return ((!isanimated) && ((elemTop >= docViewTop)&&(elemTop <= docViewBottom)));
		}
		if (elementsScrolled('.field-name-field-medaille-by-the-numbers')) {
			isanimated = true;
			$('.countx').each(function () {
			  var $this = $(this);
			  jQuery({ Counter: 0 }).stop().delay(500).animate({ Counter: $this.text() }, {
			    duration: 1000,
			    easing: 'swing',
			    step: function () {
			      $this.text(Math.ceil(this.Counter));
			    }
			  });
			});
		}
	});

});	


</script>

