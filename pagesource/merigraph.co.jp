<script type="text/javascript">
	function aj_response_header(param_li){
		var id = param_li.attr("id");
		var url = "html/categoryheader/" + id + ".html";
		jQuery.ajax({
			url:url,
			type:'post',
			async:false,
			success:function(data){
				$("div[id='box_right_category_info']").html(data);
			}
		});
	}

	$("div[id='categories_title']").ready(function(){
		$("div[id='categories_title']").click(function(){
			$("div[id='box_right_categories']").slideToggle();
		});
	});

	$("ul[class='categories_list_ul']").ready(function(){
		var categories_title = $("ul[class='categories_list_ul']").find("div[class='categories_list_big_title']");

		if(categories_title.length > 0){
			categories_title.each(function(){
				var category_title = $(this);
				category_title.click(function(){
//					$("div[class='category_detail']").css({"display":"none"});
//					$("div[class='categories_list_big_title']").find('img').attr("src","templates/item/images/category_list_big_add.png");
					var category_id = category_title.attr('id');
					var category_id_img = '#' + category_id + '_img';
					var category_detail = '#' + category_id + '_detail';
					if($(category_detail).css('display') == 'none'){
						$(category_id_img).attr("src","templates/item/images/category_list_big_minus.png");
					}else{
						$(category_id_img).attr("src","templates/item/images/category_list_big_add.png");
					}
					$(category_detail).slideToggle();
				});
			});

			var categories_list_li = $("ul[class='categories_list_sec_ul']").find('li');

			if(categories_list_li.length > 0){
				categories_list_li.hover(function(){
					var index = categories_list_li.index(this);
					var param_li = categories_list_li.eq(index);
					
					aj_response_header(param_li);
				});
			}

			$("div[id='box_right_category_info']").mouseleave(function(){
				$("div[id='box_right_category_info']").html('');
			});
		}
	});
</script>


