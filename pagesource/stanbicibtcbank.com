function ajaxsubmitForm_Enquiry_Form(){    
	var validator = $( "#Enquiry_Form" ).validate();
	if(validator.form()){ 
		showWait_Enquiry_Form(true);

	// validation perform 
		var str = $("#Enquiry_Form").serialize();

		$.cookie('sbgcscookie', str, { expires: 7 });
	
		var formValues = $("#Enquiry_Form").serialize();
		var postData = formValues + "&dataTable=SBGCS_ENQUIRY_FORM&serviceId=formServiceSaveAndEmail&successMessage=<p>Thank you for your enquiry. A consultant will contact you soon.</p>";
		postData=postData+"&ot_formId=08cbd55b592dd410VgnVCM1000009a90900aRCRD&ot_siteName=Nigeria&ot_PAGE_URL=" + window.location.href;

			$.ajax({   
			  type: "post",   
			  url: '/vgn-ext-templating/sbg/cs/sbgAjaxFormProcess.jsp',   
			  data: postData,
			  success: displayFormResponse_Enquiry_Form,
				error: function(jqXHR, textStatus, errorThrown) {
					showWait_Enquiry_Form(false);
					//alert(' error occured on page submitted using ajax');
						console.log(errorThrown);
					}
			});			
    }
}

function displayFormResponse_Enquiry_Form(resp){
	$('div[id=showFormMessage_Enquiry_Form]').html("");
	var checkError = resp.indexOf('<span');
		showWait_Enquiry_Form(false);
	if(checkError == -1){
		trackFormSuccess();
		$("#Enquiry_Form").hide();
	} else {		
		trackFormFailure();
		tryAnother_Enquiry_Form();
	}

	$('div[id=showFormMessage_Enquiry_Form]').html(resp);
	$('div[id=showFormMessage_Enquiry_Form]').show();
}

