		$.cookie('sbgcscookie', str, { expires: 7 });
	
		var formValues = $("#Callback").serialize();
		var postData = formValues + "&dataTable=sbgcs_callback_form&serviceId=formServiceSaveAndEmail&successMessage=<p>Thank you for your enquiry. A consultant will contact you soon.</p>";
		postData=postData+"&ot_formId=48ad9dfbea6dd410VgnVCM1000009a90900aRCRD&ot_siteName=Nigeria&ot_PAGE_URL=" + window.location.href;

			$.ajax({   
			  type: "post",   
			  url: '/vgn-ext-templating/sbg/cs/sbgAjaxFormProcess.jsp',   
			  data: postData,
			  success: displayFormResponse_Callback,
				error: function(jqXHR, textStatus, errorThrown) {
					showWait_Callback(false);
					//alert(' error occured on page submitted using ajax');
						console.log(errorThrown);
					}
			});			
    }
}

function displayFormResponse_Callback(resp){
	$('div[id=showFormMessage_Callback]').html("");
	var checkError = resp.indexOf('<span');
		showWait_Callback(false);
	if(checkError == -1){
		trackFormSuccess();
		$("#Callback").hide();
	} else {		
		trackFormFailure();
		tryAnother_Callback();
	}

	$('div[id=showFormMessage_Callback]').html(resp);
	$('div[id=showFormMessage_Callback]').show();
}

