<script>


var stats_offset;
var stats_animation_triggered = false;

function create_interval(max_beats, beat_duration, str, stats_no) {
    'use strict';
    $(stats_no).text("0" + str);
    var current_beat = -1;
    var intervalz;
    intervalz = setInterval(function () {
        current_beat += 1;
        $(stats_no).text(current_beat + str);
        if (current_beat === max_beats) {
            clearInterval(intervalz);
        }
    }, beat_duration);
}

function init_animations() {
    'use strict';
    $(".js-a-stat").addClass("animate");

    //$(".js-the-stats").addClass("animate");

    create_interval(20, 100, "m", $(".js-stats-number-20m"));
    create_interval(5, 400, "m", $(".js-stats-number-5m"));
    create_interval(3, 666, "m", $(".js-stats-number-3m"));
    create_interval(6, 333, "m+", $(".js-stats-number-6m"));
    create_interval(55, 36, "%", $(".js-stats-number-women"));
}

function scrollEL() {
    'use strict';
    if (stats_animation_triggered === false) {
        var window_scroll_top = $(window).scrollTop();
        var window_height = $(window).height();

        if (window_scroll_top > stats_offset || window_height > 960) {
            init_animations();
            stats_animation_triggered = true;
        }
    }
}



$(document).ready(function () {
    'use strict';
    stats_offset = ($(".stats-holder").offset().top - 170);

    $(window).scroll(function () {
        scrollEL();
    });
    scrollEL();
});


</script>


