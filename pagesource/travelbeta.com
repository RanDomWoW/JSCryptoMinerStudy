        var currencyUpdateCallBack = function (data) {
            waitingDialog.hide();
            var message = data.message;
            if (data.status === 'SUCCESS') {
                window.location.reload(true);
            } else {
                bootbox.alert('<p style="color:#009688;font-size:1.3em;text-align:center;font-weight:bold">Sorry, unable to process request at the moment.</p>');
            }
            return false;
        }
        function updateCurrency(event, newCurrency, currentCurrency) {
            event.preventDefault();
            if (newCurrency == currentCurrency) {
                bootbox.alert('<p style="color:#009688;font-size:1.3em;text-align:center;font-weight:bold">' + currentCurrency.toUpperCase() + ' is already the selected currency</p>');
            } else {
                waitingDialog.show('Updating currency, please wait...', {dialogSize: 'sm', progressType: 'primary'});
                setTimeout(function () {
                    var formData = {'newCurrencyCode': newCurrency}
                    var currencyUpdateUrl = '/changecurrency';
                    postDataToServer(formData, currencyUpdateUrl, currencyUpdateCallBack);
                }, 100);
                return false;
            }
        }

        var newLetterCallBack = function(data){
            waitingDialog.hide();
            var message = data.message;
            if(data.status === 'SUCCESS'){
                jQuery('#newsLetterErrorText').text(message);
                jQuery('#newsLetterErrorDiv').removeClass('hide').addClass('alert-success').show();
                jQuery('#newLetterEmail').val('');

            }else if(data.status === 'INFO'){
                jQuery('#newsLetterErrorText').text(message);
                jQuery('#newsLetterErrorDiv').removeClass('hide').addClass('alert-info').show();
                jQuery('#newLetterEmail').val('');
            }
            else{
                jQuery('#newsLetterErrorText').text(message);
                jQuery('#newsLetterErrorDiv').removeClass('hide').addClass('alert-error').show();
            }
            return false;
        }

        jQuery("#newsletterForm").submit(function( event ) {
            //alert('test test');
            waitingDialog.show('Processing request, please wait...', {dialogSize: 'sm', progressType: 'primary'});

            setTimeout(function () {
                var formData = $('#newsletterForm').serializeArray();
                jQuery('#newsLetterErrorDiv').removeClass('alert-error alert-success').hide();
                var newLetterUrl = '/sendnewsletter';
                postDataToServer(formData,newLetterUrl,newLetterCallBack);
            }, 400);
            return false;

        });

        var mobileAppLinkCallBack = function(data){
            waitingDialog.hide();
            var message = data.message;
            if(data.status === 'SUCCESS'){
                $('#mobileLinkPhoneNumber').val('');
                bootbox.alert('<b style="color:#009688;font-size:1.3em;">'+message+'</b>');
            }else if(data.status === 'INFO'){
                bootbox.alert('<b style="color:#2823ff;font-size:1.3em;">'+message+'</b>');
                $('#mobileLinkPhoneNumber').val('');
            } else{
                bootbox.alert('<b style="color:red;font-size:1.3em;">'+message+'</b>');
            }
            return false;
        }
        jQuery("#mobileAppLinkForm").submit(function( event ) {
            waitingDialog.show('Processing request, please wait...', {dialogSize: 'sm', progressType: 'primary'});

            setTimeout(function () {
                var formData = $('#mobileAppLinkForm').serializeArray();
                var mobileAppLinkUrl = '/send-mobile-app-links';
                postDataToServer(formData, mobileAppLinkUrl, mobileAppLinkCallBack);
            }, 400);
            return false;

        });

    </script>

    </body>
    </html>