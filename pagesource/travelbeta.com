                }
                return false;
            }
            var callFlightSearchAPI = function(data){
                var message = data.message;
                if(data.status === 'SUCCESS'){
                    var actualData = JSON.parse(data.data);
                    var searchData = {'tripType':actualData.tripType,'toAirport':actualData.toAirport,
                        'fromAirport':actualData.fromAirport,'adult':actualData.adult,
                        'flightClass':actualData.flightClass,'departingDate':actualData.departingDate,
                        'returningDate':actualData.returningDate};
                    var searchParamConstructionUrl = "/flight/process-search-params";
                    postDataToServer(searchData, searchParamConstructionUrl, flightSearchParamProcessingResponse);
                    return false;
                }else{
                    bootbox.alert('<b style="font-size:1.2em;">Sorry, unable to process request at the moment.</b>')
                }
            }
            function searchForTopFlightDeal(dealId){
