    <section class="winners-list">
      <div class="container">
        <div class="row" style="margin-bottom: 30px;">
          <div class="col-md-12">
            <div style="font-size: 20px;" class="lib-row lib-header text-center">
              IIGP 2.0 - 2017 Winners
              <div style="height: 2px; width: 46px; background-color: #14709e; margin: 0px auto 0px auto; margin-top: 10px;" class="lib-header-seperator"></div>
            </div>
          </div>
