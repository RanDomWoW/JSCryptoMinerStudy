<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<script>
var topMapEffect = function(){
  var $mapWrap = $('.mapfig');
  $('.fig00 a').click(function(){
    var thisEq = $('.fig00 a').index(this) + 1;
    $('.fig00').animate( { opacity: 'hide' }, { duration: 80 } );
    $mapWrap.find('div').eq(thisEq).animate( { opacity: 'show'}, { duration: 350} );
    return false;
  });
  $('.mapfig .btnBack').click(function(){
    var thisEq = $('.mapfig .btnBack').index(this) + 1;
    $('.fig00').animate( { opacity: 'show'}, { duration: 350} );
    $mapWrap.find('div').eq(thisEq).hide();
    return false;
  });
};
var subjectSearch = function(){
  $('#subjectSearch button').click(function(){
    var subjectId = $('#subjectSearch input[name=options]:checked').val();
    if(subjectId !== undefined){
      location.href = '/m' + subjectId;
    }
  });
  $('#subjectSearch input[name=options]').change(function(){
    var subjectId = $('#subjectSearch input[name=options]:checked').val();
    $('#areaMap a').each(function(){
      var afterHref = $(this).attr('href').replace(/m\d{1,2}|all/g,'m' + subjectId);
      $(this).attr('href',afterHref);
    });
  });
};
var topSearchWrap = function(){
  $('#topSearchWrap button').click(function(){
    location.href = setSearchURL('#topSearchWrap');
    return false;
  });
};
var headerSearchForm = function(){
  $('#headerSearchForm button').click(function(){
    location.href = setSearchURL('#headerSearchForm');
    return false;
  });
};
var setSearchURL = function(parentBlock){
  var param = [];
  //科目
  var subjectId = $(parentBlock + ' select').val();
  param.push("m" + subjectId);
  // フリーワード
  if((tmp = $(parentBlock + " input[name='free_word']").val())){
    param.push("wd" + encodeURIComponent(tmp));
  }
  //曜日
  var tmpAray = [];
  $(parentBlock + ' [name="visit_days[]"]:checked').each(function(){
    tmpAray.push("vd" + $(this).val());
  });
  if(tmpAray.length > 0){
    param.push(tmpAray.join("-"));
  }
  //時間
  var tmpAray = [];
  $(parentBlock + ' [name="visit_time[]"]:checked').each(function(){
    tmpAray.push("vt" + $(this).val());
  });
  if(tmpAray.length > 0){
    param.push(tmpAray.join("-"));
  }
  //こだわり
  var tmpAray = [];
  $(parentBlock + ' [name="features[]"]:checked').each(function(){
    tmpAray.push("mrt" + $(this).val());
  });
  if(tmpAray.length > 0){
    param.push(tmpAray.join("-"));
  }
  return '/' + param.join("/");
};
//クチコミの読み込み
var loadReview = function(){
  $.ajax({
    url: '/rest/ajax/review/new.json',
    type:'get',
    dataType:'json',
    success: function(data){
      var appHtml = "";
      $(data['review']).each(function(){
        var name = $(this)[0].t_hospital_review_nickname;
        var message = $(this)[0].t_hospital_review_message.slice(0,100);
        var hospitalLink = $(this)[0].m_hospital_estdoc_id;
        var hospitalName = $(this)[0].m_hospital_name;
        var age = $(this)[0].t_hospital_review_age;
        var sex = 'w';
        var sexName = '女性';
        if($(this)[0].t_hospital_review_sex){
          sex = 'm';
          sexName = '男性';
        }
        appHtml += '<div class="reviewItem ' + sex + age + '">';
        appHtml += '<div class="reviewHead">';
        appHtml += '<span class="userName">' + name + 'さんのクチコミ</span>';
        appHtml += '<span class="userAttribute">' + age + '代 ' + sexName + '</span>';
        appHtml += '<span class="hospitalName"><i class="iconHospital"></i><a href="/doctor/' + hospitalLink + '">' + hospitalName + '</a></span>';
        appHtml += '</div>';
        appHtml += '<p class="reviewbody">' + message + '<a href="/doctor/' + hospitalLink + '">&gt;&gt;全文読む</a></p>';
        appHtml += '</div>';
      });
      $('#newReviewSub .reviews').append(appHtml);
    }
  });
};
$(function(){
  topMapEffect();
  subjectSearch();
  headerSearchForm()
  topSearchWrap();
  loadReview();
});
</script>
