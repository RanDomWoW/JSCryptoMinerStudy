<script type="text/javascript" language="javascript">
    $(window).load(function () {
         SetNotLoggedInView();
    });

    function SetLoginName(loginname) {
        if (loginname != "Failed to get user") {

            var usernameLinkContainer = $('#UsernameLinkContainer');

            $("a", usernameLinkContainer).html(loginname);
            $(usernameLinkContainer).show();

            //Hide/show the links depending on what to show
            $("#SignUpListItem").hide();
            $("#LoginListItem").hide();
            $("#UsernameLinkContainer").show();
            $("#LogoutListItem").show();

        }
        else {
            //If data is empty - show the not logged in view
            SetNotLoggedInView();
        }
    }

    function SetNotLoggedInView() {

        //Hide/show the links depending on what to show
        $("#SignUpListItem").show();
        $("#LoginListItem").show();
        $("#UsernameLinkContainer").hide();
        $("#LogoutListItem").hide();
    }

    function getCookieWithName(c_name) {
        if (document.cookie.length > 0) {
            c_start = document.cookie.indexOf(c_name + "=");
            if (c_start != -1) {
                c_start = c_start + c_name.length + 1;
                c_end = document.cookie.indexOf(";", c_start);
                if (c_end == -1) c_end = document.cookie.length;
                return unescape(document.cookie.substring(c_start, c_end));
            }
        }
        return "";
    }

    var Base64 = {

        // private property
        _keyStr: "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=",

        // public method for encoding
        encode: function (input) {
            var output = "";
            var chr1, chr2, chr3, enc1, enc2, enc3, enc4;
            var i = 0;

            input = Base64._utf8_encode(input);

            while (i < input.length) {

                chr1 = input.charCodeAt(i++);
                chr2 = input.charCodeAt(i++);
                chr3 = input.charCodeAt(i++);

                enc1 = chr1 >> 2;
                enc2 = ((chr1 & 3) << 4) | (chr2 >> 4);
                enc3 = ((chr2 & 15) << 2) | (chr3 >> 6);
                enc4 = chr3 & 63;

                if (isNaN(chr2)) {
                    enc3 = enc4 = 64;
                } else if (isNaN(chr3)) {
                    enc4 = 64;
                }

                output = output +
                    this._keyStr.charAt(enc1) + this._keyStr.charAt(enc2) +
                    this._keyStr.charAt(enc3) + this._keyStr.charAt(enc4);

            }

            return output;
        },

        // public method for decoding
        decode: function (input) {
            var output = "";
            var chr1, chr2, chr3;
            var enc1, enc2, enc3, enc4;
            var i = 0;

            input = input.replace(/[^A-Za-z0-9\+\/\=]/g, "");

            while (i < input.length) {

                enc1 = this._keyStr.indexOf(input.charAt(i++));
                enc2 = this._keyStr.indexOf(input.charAt(i++));
                enc3 = this._keyStr.indexOf(input.charAt(i++));
                enc4 = this._keyStr.indexOf(input.charAt(i++));

                chr1 = (enc1 << 2) | (enc2 >> 4);
                chr2 = ((enc2 & 15) << 4) | (enc3 >> 2);
                chr3 = ((enc3 & 3) << 6) | enc4;

                output = output + String.fromCharCode(chr1);

                if (enc3 != 64) {
                    output = output + String.fromCharCode(chr2);
                }
                if (enc4 != 64) {
                    output = output + String.fromCharCode(chr3);
                }

            }

            output = Base64._utf8_decode(output);

            return output;

        },

        // private method for UTF-8 encoding
        _utf8_encode: function (string) {
            string = string.replace(/\r\n/g, "\n");
            var utftext = "";

            for (var n = 0; n < string.length; n++) {

                var c = string.charCodeAt(n);

                if (c < 128) {
                    utftext += String.fromCharCode(c);
                }
                else if ((c > 127) && (c < 2048)) {
                    utftext += String.fromCharCode((c >> 6) | 192);
                    utftext += String.fromCharCode((c & 63) | 128);
                }
                else {
                    utftext += String.fromCharCode((c >> 12) | 224);
                    utftext += String.fromCharCode(((c >> 6) & 63) | 128);
                    utftext += String.fromCharCode((c & 63) | 128);
                }

            }

            return utftext;
        },

        // private method for UTF-8 decoding
        _utf8_decode: function (utftext) {
            var string = "";
            var i = 0;
            var c = c1 = c2 = 0;

            while (i < utftext.length) {

                c = utftext.charCodeAt(i);

                if (c < 128) {
                    string += String.fromCharCode(c);
                    i++;
                }
                else if ((c > 191) && (c < 224)) {
                    c2 = utftext.charCodeAt(i + 1);
                    string += String.fromCharCode(((c & 31) << 6) | (c2 & 63));
                    i += 2;
                }
                else {
                    c2 = utftext.charCodeAt(i + 1);
                    c3 = utftext.charCodeAt(i + 2);
                    string += String.fromCharCode(((c & 15) << 12) | ((c2 & 63) << 6) | (c3 & 63));
                    i += 3;
                }

            }

            return string;
        }

    }

    function getCookie(name) {

        var cookieName = name + "=";
        var startPoint = 0;
        var value = "";
        //alert(cookieName);
        if (document.cookie.indexOf(cookieName) >= 0) {
            startPoint = cookieName.length + document.cookie.indexOf(cookieName);
            value = document.cookie.substring(startPoint);
        }


        return Base64.decode(value);

    }

    function getNumberOfProducts() {

        var strAllProducts = getCookie("webshop2");
        //alert(strAllProducts);
        if (strAllProducts.indexOf("|") >= 1) {

            var allProducts = strAllProducts.split("|");
            return allProducts.length;

        } else if (strAllProducts.indexOf("¤") >= 1) {

            return 1;

        } else {

            return 0;

        }

    }

    function getTotalPrice() {

        var strAllProducts = getCookie("webshop2");
        var total = 0;

        if (strAllProducts.length > 0) {
            var allProducts = strAllProducts.split("|");

            for (var i = 0; i < allProducts.length; i++) {

                var prod = allProducts[i].split("¤");
                try {
                    total += prod[2].replace(",", ".") * prod[4].replace(",", ".");
                }
                catch (err) { }

            }
        }

        if (isNaN(total)) {
            return 0;
        }

        total = Math.round(total * 100) / 100;


        return total;

    }

    function getCurrency() {

        var strAllProducts = getCookie("webshop2");

        if (strAllProducts.length > 0) {
            if (strAllProducts.indexOf("|") > 0) {

                var allProducts = strAllProducts.split("|");
                var prod = allProducts[0].split("¤");
                return prod[5];

            } else {

                var prod = strAllProducts.split("¤");
                return prod[5];

            }
        }

        return "";

    }

    function AjaxRefreshCart() {
        var aGlobal = document.getElementById("AjaxGlobalMenuCart");

        var aRegular = document.getElementById("AjaxRegularCart");
        var totalPriceElement = document.getElementById("AjaxTotalPrice");

        var currency = getCurrency();

        var numberOfProducts = getNumberOfProducts();
        var totalPrice = getTotalPrice();

        if (numberOfProducts > 1) {

            if (aGlobal != null) {
                aGlobal.firstChild.innerHTML = numberOfProducts + ' Itens';
            }

            if (aRegular != null) {
                aRegular.innerHTML = numberOfProducts + ' Itens';
            }

        }
        else if (numberOfProducts == 1) {

            if (aGlobal != null) {
                aGlobal.firstChild.innerHTML = numberOfProducts + ' Item';
            }

            if (aRegular != null) {
                aRegular.innerHTML = numberOfProducts + ' Item';
            }

        }
        else {

            if (aGlobal != null) {
                aGlobal.parentNode.parentNode.style.display = "none";
            }

            if (aRegular != null) {
                aRegular.style.display = "none";
            }

            if (totalPriceElement != null) {
                totalPriceElement.style.display = "none";
            }

            return true;
        }

    if (totalPriceElement != null) {
        totalPriceElement.innerHTML = totalPrice + " " + currency;
    }

    return true;
}
</script>

				
