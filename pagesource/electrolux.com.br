<script type="text/javascript" language="javascript">
    var i = 0;
    var maxExtraFields = 9;
    var previewedOnce = false;

    $(document).ready(
        function () {
            RegisterSendToAFriendInputKeyPress();
        }
    );

    function RegisterSendToAFriendInputKeyPress() {
        $('.mp_sendToAFriend input[type=text]').keypress(SendToAFriendInputKeyPress);
    }

    function SendToAFriendInputKeyPress(e) {
        var key = e.charCode || e.keyCode || 0;
        if (key == 13) {
            var inputs = $('.mp_sendToAFriend input[type=text]');
            var index = inputs.index(this);
            var nextInput = inputs.eq(index + 1);

            if (nextInput.length > 0) {
                nextInput.focus();
            }

            return false;
        }
    }

    // Show Error panel
    function SendToAFriendInit() {
        HidePanels();
    }

    // Add one field for friends emailaddress
    function addField() {
        HidePanels();

        if (i < maxExtraFields) {
            $('#moreFriends').append('<input type="text"  class="cWidth12" name="inputFriends" maxlength="100"' + i + '" id="inputFriends' + i + '" /><br />');
            RegisterSendToAFriendInputKeyPress();
            i++;
        }
        return false;
    }

    // Get all values from form and validate
    function getFieldValuesSendToAFriend() {

        var errorMessage = '';
        
        var fromName = $('#staf_name').val();
        var toMail = $('#staf_femail').val();
        var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;

        HidePanels();

        // Validate input
        if (fromName.length == 0)
            errorMessage += '- Preencha o seu nome<br />';

        if (toMail.length == 0)
            errorMessage += '- Introduza, pelo menos, um endereço de e-mail<br />';
        else {
            if (reg.test(toMail) == false)//
                errorMessage += '- Formato incorreto no endereço de e-mail : ' + toMail + '<br />';
        }

        for (j = 0; j < i; j++) {
            if (typeof toFriendl != 'undefined') {
                var toFriend = $('#inputFriends' + j).val();

                if (toFriend.length > 0) {
                    if (reg.test(toFriend) == true) {
                        //toMail += ';' + toFriend;
                    } else {
                        errorMessage += '- Formato incorreto no endereço de e-mail : ' + toFriend + '<br />';
                    }
                }
            }
        }

        // Send mail or display error message
        if (errorMessage.length > 0) {
            ShowError(errorMessage);
        }
        else {
            SendMail(fromName, toMail);

            for (j = 0; j < i; j++) {
                if (typeof toFriendl != 'undefined') {
                    var toFriend = $('#inputFriends' + j).val();
                    if (toFriend.length > 0) {
                        if (reg.test(toFriend) == true) {
                            SendMail(fromName, toFriend);
                        }
                    }
                }
            }
        }
        grecaptcha.reset(sendToFriendWidgetId);
    }

    // Send Mail
    function SendMail(fromName, toMail) {
        var composedUrl;
        if ($('#product-families .products.compare').length > 0) {
            var compareList = compareModule.getSavedValues() || [];
            var url = window.location.href.split('?')[0];

            var categoryGuid = getParameterByName('category');

            if (!compareList.length || !categoryGuid.length) {
                composedUrl = window.location.href;
            }
            else {
                url += '?category=' + categoryGuid + '&p=';
                _.each(compareList, function (item) {
                    if (item.category.categoryguid === categoryGuid) {
                        var firstItem = true;
                        _(item.category.products).each(function (product) {
                            if (product.shouldberendered) {
                                if (firstItem) {
                                    url += product.productguid;
                                    firstItem = false;
                                } else {
                                    url += ',' + product.productguid;
                                }
                            }
                        });
                    }
                });

                composedUrl = url;
            }

        } else {
            composedUrl = location.href;
        }
        $.get
        (
            '/handlers/SendToAFriend.ashx',
            {
                pId: '00000000-0000-0000-0000-000000000000', url: composedUrl,
                fromName: fromName, toMail: toMail, lang: 'pt-BR', sendType: 'Product',
                PPDLanguageId: '76', pageid: '3',
                reCaptchaResponse: grecaptcha.getResponse(sendToFriendWidgetId)
            },
            function (data) {
                // Success
                if (data == "success") {
                    $("#ResultTextArea").html('Obrigado por divulgar a Electrolux!');
                    $('#ResultArea').show();

                    // Reset form
                    ResetForm();
                }
                else if (data.includes('reCaptcha')) {
                    ShowError(data);
                }
                // Failure
                else {
                    ShowError('Ocorreu um erro');
                }
            }
        );
    }

    var getParameterByName = function (name) {
        name = name.replace(/[\[]/, '\\\[').replace(/[\]]/, '\\\]');
        var regexS = '[\\?&]' + name + '=([^&#]*)';
        var regex = new RegExp(regexS);
        var results = regex.exec(window.location.search);
        if (results == null)
            return '';
        else
            return decodeURIComponent(results[1].replace(/\+/g, ' '));
    };

    // Preview Email
    function previewEmail() {
        var yourName = $("#staf_name").val();
        var previewFooter = $("#emailPreviewFooter");

        if (yourName != "") {
            yourName = $('<div/>').text(yourName).html(); //html encode
            if (!previewedOnce) {
                previewedOnce = true;
                previewFooter.append('Até a próxima, ');
                previewFooter.append('<span id="staf_namePreview">' + yourName + '</span>');
            }
            else {
                $('span#staf_namePreview').html(yourName);
            }
        }
    }

    // Show Error panel
    function ShowError(errorMessage) {
        $("#ErrorMessageTextArea").html(errorMessage);
        $('#ErrorArea').show();
    }

    // Hide Error and Result panel
    function HidePanels() {
        $('#ErrorArea').hide();
        $('#ResultArea').hide();
    }

    // Reset form
    function ResetForm() {
        i = 0;
        $('#moreFriends').html("");
        $('#staf_femail').val("");
    }

</script>

