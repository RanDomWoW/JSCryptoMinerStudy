            send_pass_url: checkHttps("https://www.gullybaba.com/sociallogin/popup/sendPass"),
            create_url: checkHttps("https://www.gullybaba.com/sociallogin/popup/createAcc"),
        };
        Login = new LoginPopup(options);
    });
</script>

