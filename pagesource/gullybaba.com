<script type="text/javascript">
    function checkHttps(requestUrl) {
        if (window.location.href.match('https://') && !requestUrl.match('https://'))
            requestUrl = requestUrl.replace('http://', 'https://');
        if (!window.location.href.match('https://') && requestUrl.match('https://'))
            requestUrl = requestUrl.replace('https://', 'http://');
        return requestUrl;
    }
