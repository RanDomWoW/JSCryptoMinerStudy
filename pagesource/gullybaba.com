				var rl_total = $('.respl-loadmore-btn',$el).parent().attr('data-rl_total');
				var rl_load = $('.respl-loadmore-btn',$el).parent().attr('data-rl_load');
				var rl_allready =  $('.respl-loadmore-btn',$el).parent().attr('data-rl_allready');
