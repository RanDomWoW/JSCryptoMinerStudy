		function wssoshowbox(idbox) {
			jQuery("<div>").attr({'id':'transparant_bg','style':'position:absolute; top:0; left: 0; width: 100px; height: 100px;','class':'transparant'}).appendTo("#global"); //.tx-ebulleinfo-pi1
			var oBulle = jQuery('#'+idbox);
			oBulle.clone().attr({'id':'wssopopup'}).appendTo('#global');
			wssoajustbox();
			jQuery('#wssopopup').css('display','block');
			jQuery(window).resize(function(){wssoajustbox();});
			jQuery(window).scroll(function(){wssoajustbox();});
		}
		
		function wssoajustbox() {
		
			var width = eval (jQuery(window).width());
			var height = eval (jQuery(window).height());
			jQuery('#transparant_bg').css({'width':width+'px','height':+eval(jQuery(window).scrollTop()+height+500)+'px'});
			var wssopopup = jQuery('#wssopopup');
			var nW = (width/2)-(wssopopup.width()/2);
			var nH = jQuery(window).scrollTop()+(height/2)-(wssopopup.height()/2)-100;
			wssopopup.css('left',nW+'px');
			wssopopup.css('top',nH+'px');
		}
		</script>
		
