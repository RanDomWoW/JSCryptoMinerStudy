<h3><div style='color:darkblue;'>Валютный калькулятор:</h3></div>
<form name="calculator" style='display:inline'>
<input type="text" style='text-align:center;font-size:12pt' name="from" value=0 onkeyup="count()" size='8'>
<select name="fromCur" onchange="count()">
<option value="doll" selected>США $
<option value="hkd">Гонконгских $
<option value="euro">Евро
<option value="yuan">Юаней
<option value="rub">Рублей
</select>

=
<span id="to" style='font-size:12pt;width=100px;border:1px solid red;text-align:center;color:blue'>0</span>&nbsp;
<select name="toCur" onchange="count()">
<option value="doll">США $
<option value="hkd">Гонконгских $
<option value="euro">Евро
<option value="yuan">Юаней
<option value="rub" selected>Рублей
</select>

</form>
</div>

<script type='text/javascript'>

var form = document.calculator;

function check(field) {
    var x = field.value;
    field.value = x = x.replace(/ /g, '');
    if (x.match(/^.*,.*$/)) field.value = x = x.replace(',', '.');
    if (!x.match(/^[\d\.,]+$/)) field.value = x.slice(0, -1);
}

function nFormat(nStr) {
    nStr += '';
    nStr = nStr.replace(/ /g, '');
    if (nStr.charAt(nStr.length - 1) != '.') nStr = Math.round(100 * nStr) / 100;
    nStr += '';
    x = nStr.split('.');
    x1 = x[0];
    x2 = x.length > 1 ? '.' + x[1] : '';
    var rgx = /(\d+)(\d{3})/;
    while (rgx.test(x1)) x1 = x1.replace(rgx, '$1' + ' ' + '$2');
    return x1 + x2;
}

function count() {
    check(form.from);
    var fC = form.fromCur.value;
    var tC = form.toCur.value;
    var r = 57.5;
    if (fC == 'doll') {
        if (tC == 'hkd') r = 57.5 / 7.33;
        if (tC == 'euro') r = 57.5 / 70.57;
        if (tC == 'yuan') r = 57.5 / 9.08;
        if (tC == 'rub') r = 57.5;
        if (tC == 'doll') r = 1;
    }
	
    if (fC == 'hkd') {
        if (tC == 'hkd') r = 1;
        if (tC == 'euro') r = 7.33/ 70.57;
        if (tC == 'yuan') r = 7.33/ 9.08;
        if (tC == 'rub') r = 7.33;
        if (tC == 'doll') r = 7.33/ 57.5;
    }
	
    if (fC == 'euro') {
        if (tC == 'hkd') r = 70.57 / 7.33;
        if (tC == 'euro') r = 1;
        if (tC == 'yuan') r = 70.57 / 9.08;
        if (tC == 'rub') r = 70.57;
        if (tC == 'doll') r = 70.57 / 57.5;
    }
    if (fC == 'yuan') {
        if (tC == 'hkd') r = 9.08 / 7.33;
        if (tC == 'euro') r = 9.08 / 70.57;
        if (tC == 'yuan') r = 1;
        if (tC == 'rub') r = 9.08;
        if (tC == 'doll') r = 9.08 / 57.5;
    }
    if (fC == 'rub') {
        if (tC == 'hkd') r = 1 / 7.33;
        if (tC == 'euro') r = 1 / 70.57;
        if (tC == 'yuan') r = 1 / 9.08;
        if (tC == 'rub') r = 1;
        if (tC == 'doll') r = 1 / 57.5;
    }
    var total = form.from.value * r;
    document.getElementById('to').innerHTML = nFormat(total);
    form.from.value = nFormat(form.from.value);
}

</script>

