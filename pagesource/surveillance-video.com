[spec-hint] {
    display: inline-block;
    position: relative;
}
[spec-hint]:before, [spec-hint]:after {
    opacity: 0;
    pointer-events: none;
    position: absolute;
    transition: all 0.3s ease 0s;
    visibility: hidden;
    z-index: 1000000;
}
[spec-hint]:hover:before, [spec-hint]:hover:after {
    opacity: 1;
    visibility: visible;
}
[spec-hint]:before {
    background: none repeat scroll 0 0 transparent;
    border: 6px solid transparent;
    content: "";
    position: absolute;
    z-index: 1000001;
}
[spec-hint]:after {
    background: none repeat scroll 0 0 #fff;
    box-shadow: 4px 4px 8px rgba(0, 0, 0, 0.3);
    color: #000;
    content: attr(spec-hint);
    font-size: 12px;
    line-height: 12px;
    padding: 8px 10px;
    display:block;
    white-space: pre;
}
.spec--top:before {
    /*border-top-color: #494949;*/
    border-top-color: #fff;
}
.spec--top:before {
    margin-bottom: -12px;
}
.spec--top:after {
    margin-left: -18px;
}
.spec--top:before, .spec--top:after {
    bottom: 100%;
    left: 50%; 
}
.spec--top:hover:before, .spec--top:hover:after {
    transform: translateY(-8px);
}
.spec--top:hover{opacity:1;}
