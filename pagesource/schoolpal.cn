        var minutediff = (LocalDate - ServerDate) / 60000;
        if (minutediff > 10 || minutediff < -10) {
            alert("   您的计算机日期时间错误，或导致无法登录！\n\n          (与北京标准时间相差" + minutediff.toFixed(0) + "分钟)\n\n请单击显示器右下角时间并改为：");
