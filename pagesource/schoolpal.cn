        function judgeAutoFill() {
            if ($("#UserName").val() != "") { $("#orgName_tips").hide(); $("#UserName_tips").hide(); $("#UserPW_tips").hide(); };//若字段不为空，则隐藏label
        };

        function judgePW() {
            if ($("#UserPW").val() != "") { $("#UserPW_tips").hide(); };//若字段不为空，则隐藏label
            setTimeout("judgePW()", 1000);
        };

    
        //登录
        function FormSubmit() {
            var loginmode = $("#loginMode").val();
            $(".loginWarning").css("display", "none");
            $(".loginWarning span").css("display", "none");
            $("table div").css("border-color", "#c2c6c8");
            if(loginmode=="computerLogin")
            {
                if($("#orgName").val()=="")
                {
                    $(".MsgIncomplete").text("请输入机构后缀").css("display","block");
                    $(".loginWarning").css("display", "block");
                    $(".computerLogin").find("div:eq(0)").css("border-color", "red");
                    return;
                }
                else  if($("#UserName").val()=="")
                {
                    $(".MsgIncomplete").text("请输入登录名").css("display", "block");
                    $(".loginWarning").css("display", "block");
                    $(".computerLogin").find("div:eq(1)").css("border-color", "red");
                    return;
                }
                else if ($("#UserPW").val() == "") {
                    $(".MsgIncomplete").text("请输入密码").css("display", "block");
                    $(".loginWarning").css("display", "block");
                    $(".computerLogin").find("div:eq(2)").css("border-color", "red");
                    return;
                }
                else{
                    if ("No" == "Yes") {
                        if ($("#validatecode").val() == "") {
                            $(".MsgIncomplete").text("请输入验证码").css("display", "block");
                            $(".loginWarning").css("display", "block");
                            $(".computerLogin").find("div:eq(3)").css("border-color", "red");
                            return;
                        }
                        else {
                            Login.submit();
                        }
                    }
                    else {
                        Login.submit();
                    }
                }
            }
            else
            {
                if ($("#UserTel").val() == "" && $("#UserPW2").val() == "")
                {
                    $(".MsgIncomplete").text("请输入手机号和密码").css("display", "block");
                    $(".loginWarning").css("display", "block");
                    $(".phoneLogin").find("div:eq(0)").css("border-color", "red");
                    $(".phoneLogin").find("div:eq(1)").css("border-color", "red");
                    return;
                }
                else if ($("#UserTel").val() == "") {
                    $(".MsgIncomplete").text("请输入手机号").css("display", "block");
                    $(".loginWarning").css("display", "block");
                    $(".phoneLogin").find("div:eq(0)").css("border-color", "red");
                    return;
                }
                else if ($("#UserPW2").val() == "") {
                    $(".MsgIncomplete").text("请输入密码").css("display", "block");
                    $(".loginWarning").css("display", "block");
                    $(".phoneLogin").find("div:eq(1)").css("border-color", "red");
                    return;
                }
                else
                {
                    if (!CheckPhone($("#UserTel").val()))//请输入正确的手机号
                    {
                        $(".MsgIncomplete").text("请输入正确的手机号").css("display", "block");
                        $(".loginWarning").css("display", "block");
                        //$(".phoneLogin").find("div:eq(0)").css("border-color", "red");
                        return;
                    }
                    if("No"=="Yes")
                    {
                        if ($("#validatecode2").val()!="")
                            Login.submit();
                        else
                        {
                            $(".MsgIncomplete").text("请输入验证码").css("display", "block");
                            $(".loginWarning").css("display", "block");
                            $(".phoneLogin").find("div:eq(2)").css("border-color", "red");
                            return;
                        }
                    }
                    else
                    {
                        Login.submit();;
                    }
                    
                }
            }
      
        };

        function initLoginCover() {
            var coverImageUrl = 'url()';
            var coverImageLink = '';
            var coverShowTime = '';
            $("#divLoginCover").children().find(".schoolhomeBG").eq(0).css("background-image",coverImageUrl);
            $("#divLoginCover").children().find(".schoolhomeBG").eq(0).attr("href", coverImageLink);
            $("#divLoginCover").show();
            setTimeout('hideLoginCover()', isNaN(coverShowTime) || coverShowTime == '' ? 5000 : coverShowTime * 1000);
        };

        function hideLoginCover() {
            $("#divLoginCover").hide();
        };

        $(function () {
            function doSetLoginMode() {
                if ($("#loginMode").val() == "computerLogin" && !$(".loginTab a[mode='computerLogin']").hasClass("current"))
                {
                    $("#loginMode").val("phoneLogin"); 
                }                
            }
            function getErrorMsg(errorMsg) {
                switch (errorMsg) {
                    case "OrgCrmVersionNotExist": return "版本不存在";
                    case "PaymentNotConfirmed": return "校宝财务未确认到款";
                    case "Expired": return "您的系统已到期";
                    case "UserNotMatch": return "登录名或密码错误，请重新输入";
                                            case "UserDisabled": return "该账号已被禁用，请联系最高权限。";
                    case "CodeNotMatch": return "验证码错误，请重新输入";
                    case "BothNotExist": return "没有相关登录信息，请重新输入手机号或者使用普通登录";
                    case "PhoneNotMatch": return "手机号或密码错误，请重新输入！<br/>如果忘记密码，可用<a href='/SchoolPal/ForgetPassword' style='color:#0000FF'>手机号重置密码</a>";
                    case "OrgNotExist": return "机构后缀不存在，请重新输入";
                    case "AccountLocked": return "登录失败次数已达5次，请5分钟后再尝试登录";
                    case "SignError": return "登录失败，请重试";
                    case "PersonalLoginWayError": return "该机构账号暂不支持电脑版，如要使用请登录移动端。<br/>如有其他机构账号，请点击确定关闭提示框，选择并保存其他机构账号即可进入电脑版";
                    case "NoFunctionAuthority": return "该账号无任何权限，请联系相关人员配置";
                    case "LoginTimeError": return "";
                    default: return errorMsg;
                }
            }
            $(".MsgError").html(getErrorMsg(''));
            doSetLoginMode();
            $(".loginTab a").click(function () {
                var mode = $(this).attr("mode");
                if ($("#loginMode").val() != mode)
                {
                    $(".loginWarning").css("display", "none");
                }
                $(".loginTableBox").css("display", "none");
                $(".loginTab a").removeClass("current");
                $(this).addClass("current");
                $("#loginMode").attr("value", mode);
                $("." + mode).css("display", "block");
            });

            $(document).keydown(function (event) {
                if (event.keyCode == 13)
                {
                    FormSubmit();
                }
            });
            $("#themeOption").bind('mouseover', function () {
                $("#theme_items").css({ 'display': 'block' });
                msov = true;
            });//鼠标停留themeOption，item可见

            $("#theme_items").bind('mouseover', function () {
                $("#theme_items").css({ 'display': 'block' });
            });//鼠标停留theme_items，item可见

            $("span em").bind('mouseover', function () {
                $(this).addClass('item_selected');
            });//鼠标停留theme_items，添加选中效果

            $("span em").bind('click', function () {
                $("span em").removeClass('item_selected');
                $(this).addClass('item_selected');
                if ($(this).hasClass('item1')) {
                    item = "item1"; $("#theme").val(8);
                    if (theme_style == "") { $("#HeadLine").addClass("theme_style1"); theme_style = "theme_style1"; }
                    else { $("#HeadLine").removeClass(theme_style); $("#HeadLine").addClass("theme_style1"); theme_style = "theme_style1"; }
                };
                if ($(this).hasClass('item2')) {
                    item = "item2"; $("#theme").val(9);
                    if (theme_style == "") { $("#HeadLine").addClass("theme_style2"); theme_style = "theme_style2"; }
                    else { $("#HeadLine").removeClass(theme_style); $("#HeadLine").addClass("theme_style2"); theme_style = "theme_style2"; }
                };
                if ($(this).hasClass('item3')) {
                    item = "item3"; $("#theme").val(10);
                    if (theme_style == "") { $("#HeadLine").addClass("theme_style3"); theme_style = "theme_style3"; }
                    else { $("#HeadLine").removeClass(theme_style); $("#HeadLine").addClass("theme_style3"); theme_style = "theme_style3"; }
                };
            });//记住添加选中效果的theme_items，并给theme设值

            $("span em").bind('mouseleave', function () {
                if (!$(this).hasClass(item)) { $(this).removeClass('item_selected'); };
            });//鼠标移开，若目标未被记录添加选中效果，则移除选中效果

            $("#themeOption").bind('mouseleave', function () {
                setTimeout("if (!msov) { $('#theme_items').css({ 'display': 'none' }) }", 1000); //若1000ms后，鼠标不在#themeOption上，则执行函数
                msov = false;
            });
        

      
            //---------------------------------------------

            $("#orgName").blur(function () {
                if ($(this).val() == "") {
                    $("#orgName_tips").show();
                }
                else {
                    $("orgName_tips").hide();
                }
                //if ($("#UserPW").val() != "") $("#UserPW_tips").hide(); else $("#UserPW_tips").show();//针对IE9以下版本change事件对IE自动填充不敏感
            }).focus(function () {
                $("#orgName_tips").hide();
                //if ($("#UserPW").val() != "") $("#UserPW_tips").hide(); else $("#UserPW_tips").show();//针对IE9以下版本change事件对IE自动填充不敏感
            })
            $("#orgName_tips").click(function () {
                $("#orgName").focus();
            });
            if ($("#orgName").val() == "") {
                $("#orgName_tips").show();
            }
            else {
                $("#orgName_tips").hide();
            }


            $("#UserName").blur(function () {
                if ($(this).val() == "") {
                    $("#UserName_tips").show();
                }
                else {
                    $("UserName_tips").hide();
                }
                //if ($("#UserPW").val() != "") $("#UserPW_tips").hide(); else $("#UserPW_tips").show();//针对IE9以下版本change事件对IE自动填充不敏感
            }).focus(function () {
                $("#UserName_tips").hide();
                //if ($("#UserPW").val() != "") $("#UserPW_tips").hide(); else $("#UserPW_tips").show();//针对IE9以下版本change事件对IE自动填充不敏感
            })

            $("#UserName_tips").click(function () {
