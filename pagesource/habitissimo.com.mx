<script type="text/javascript">
  jQuery(function($) {

    $('.do-subscribe').on('click', function(ev) {
      ev.preventDefault();
      var $button = $(ev.target);

      if (!$button.attr('data-album-id')) {
        $button = $button.closest('button');
      }

      if ($button.hasClass('disabled')) {
        return;
      }
      $button.addClass('disabled');

      Login.pop({
        'title': "Nos encanta que te guste el \u00e1lbum<small>Necesitamos que entres a tu cuenta o est\u00e9s registrado para poder seguir el \u00e1lbum<\/small>",
        'source_page': '',
        'source_action' : 'subscribe_album_aside',
        'onsuccess': function () {
          var album_id = $button.data('album-id');
          var album_url = $button.data('album-url');

          $.ajax({type:"GET",
            url:     '/do_ajax/add_subscribe_album/' + album_id + '?show_alert=true',
            success: function(){
              window.location.href = album_url;
            },
            statusCode: {
              401: function() {
                bootbox.alert("Debes estar registrado como usuario");
              },
              405: function() {
                bootbox.alert("Ya est\u00e1s siguiendo el \u00e1lbum");
              }
            },
            complete: function() {
              $button.removeClass('do-subscribe');
            }
          });
        }
      });
    });
  });
</script>
