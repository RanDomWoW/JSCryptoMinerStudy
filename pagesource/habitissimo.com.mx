  jQuery(function($) {
    var menu_selector_classes = [".menu .menu-toggle",".menu .accordion-menu-toggle"];

    menu_selector_classes.forEach(function(menu_selector_class) {
      var $navbar = $('#main-navbar');

      $navbar.on('touchstart', menu_selector_class , function(ev) {
        var $menu = $(this).closest('.menu'),
          $hover = !$menu.hasClass('active hover');

        $menu.toggleClass('active hover', $hover);

        if ($hover) {
          $menu.add($menu.parentsUntil($navbar, '.menu')).siblings().removeClass('active hover');
        }

        if (!$(this).hasClass('noncollapsible')) {
          ev.preventDefault();
        }

        var onBodyClick = function(ev) {
          if (!$(ev.target).closest('.menu.hover').length) {
            $navbar.find('.menu').removeClass('hover');
            $(document.body).off('touchstart', onBodyClick);
          }
        };

        $(document.body).on('touchstart', onBodyClick);
      });

      $navbar.on('mouseenter touchenter', menu_selector_class, function() {
        var $menu = $(this).closest('.menu');

        $menu.addClass('hover');
        $menu.add($menu.parentsUntil($navbar, '.menu')).siblings('.menu').removeClass('hover');
      });

      $navbar.on('mouseleave', '.menu.hover', function() {
        $(this).removeClass('hover');
      });

      $navbar.on('click', menu_selector_class, function() {
        var $menu = $(this).closest('.menu');

        $menu.toggleClass('active');

        if ($menu.hasClass('active') || $menu.hasClass('hover')) {
          $menu.add($menu.parentsUntil($navbar, '.menu')).siblings('.menu').removeClass('active hover');
        }
      });
    })
  });
</script>
