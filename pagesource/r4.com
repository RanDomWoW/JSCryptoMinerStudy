			function throwLogin(form){
									
				$("#T_LOGIN_FORM_P [type=submit]").attr("disabled", "disabled").val("Haciendo login");
				
				$.ajax({
					type:"POST",
					url: "https://www.r4.com/ajax/login",
					data: $(form).serialize(),
					async: true,
					dataType: "json"
				})
				.done(function(postData){
					$(".campo-captcha").remove();
					$("#T_LOGIN_FORM_P [type=submit]").removeAttr("disabled").val("Acceder");
					
					if(postData.valido){
						var returnUrl = "TX=goto&FWD=LOGIN&PAG="+$("#T_LOGIN_FORM_P [name=PAG]").val();
						if(typeof postData.returnUrl != "undefined" &&
							postData.returnUrl != ""){
							returnUrl = postData.returnUrl;
						}
						$("#T_LOGIN_FORM_P").attr("action", "https://www.r4.com/portal?"+returnUrl);
						$(".general-error").html("");
						$(".general-error").removeClass("active");
						
						document.forms["T_LOGIN_FORM_P"].submit();
					}else{
						if(postData.errCode && postData.errCode != "10136") {
							$("#T_LOGIN_FORM_P input[type=\"password\"]").val("");
						}

						if(postData.mensaje && postData.mensaje != ""){
							$(".general-error").html(postData.mensaje);
						}else{
							$(".general-error").html("Ha habido un error al hacer login");
						}
						
						$(".general-error").addClass("active");		
							
						if(postData.urlCaptcha && postData.urlCaptcha != ""){
							var contenedorCaptcha = $("<div class=\"campo campo-captcha\"></div>");
							
							$("#T_LOGIN_FORM_P input[type=submit]").before(contenedorCaptcha);
							
							contenedorCaptcha.append("<label>Escribe los siguientes caracteres:</label>");
							contenedorCaptcha.append("<img src=\""+postData.urlCaptcha+"\">");
							contenedorCaptcha.append("<div class=\"captcha-btn js-launch-captcha-login\"><div class=\"sprite reload-captcha\"></div></div>");
							contenedorCaptcha.append("<div class=\"campo-elem\"><input type=\"text\" name=\"CAPTCHA\" autocomplete=\"off\"></div>");
							contenedorCaptcha.append("<div class=\"error-txt\"></div>");
						}
					}
				})
				.fail(function(){
					$("#T_LOGIN_FORM_P [type=submit]").removeAttr("disabled").val("Acceder");
				})
			}
		

			function createCustomAlert(txt) {
				var container = $("<div class=\"custom-alert-container\" style=\"display:none;\"></div>").appendTo("body");
				var fondo = $("<div class=\"fondo\"></div>").appendTo(container);
				
				var contMensaje = $("<div class=\"cont-mensaje\"></div>").appendTo(container);
				var cerrar = $("<div class=\"cerrar\">&times;</div>").appendTo(contMensaje);
				if (txt.indexOf("<titulo-mensaje>")!=-1)
				{
					var titulo= $("<div class=\"titulo-mensaje\">"+txt.substring(txt.lastIndexOf("<titulo-mensaje>")+16,txt.lastIndexOf("</titulo-mensaje>"))+"</div>").appendTo(contMensaje);
					var mensaje = $("<div class=\"mensaje\">"+txt.substring(txt.lastIndexOf("<cuerpo-mensaje>")+16,txt.lastIndexOf("</cuerpo-mensaje>"))+"</div>").appendTo(contMensaje);
				}else{
					var titulo = $("<div class=\"titulo-mensaje\">Mensaje de Renta 4 Banco</div>").appendTo(contMensaje);
					var mensaje = $("<div class=\"mensaje\">"+txt.replace(/(\r\n|\n|\r)/gm, "<br>")+"</div>").appendTo(contMensaje);
				}
				var boton = $("<button class=\"boton\">Aceptar</button>").appendTo(contMensaje);
				
				openAlert();
				bindEvents();
			}
						
			function bindEvents(){
				$(".custom-alert-container .fondo, .cont-mensaje .boton, .cont-mensaje .cerrar").on("click", function(){
