				$( "#home-content .pills-container .pill-image a" ).click( function( obj ) {
					var arr = $( "#home-content .pills-container .pill-image a" );
					var pos = -1;
					for( var i = 0; i < arr.length; i++ )
					{
						if( arr[i] == this )
						{
							pos = i;
							break;
						}
					}
					if( pos > -1 )
