<script type="text/javascript">
    // <![CDATA[
    var parser;
    var slideDuration = 3.0;
    var titleLimit = 22;
    var descriptionLimit = 70;

    $(document).ready(function() {
        parser = new contentParser("./main/xml/contents.xml");
        parser.parse(parseComplete);
        $( '#footbar' ).css("position","absolute");
        $( '#footbar' ).scrollFollow();
    })
    
    function getUrlVars()
    {
      var vars = [], hash;
      var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
      for(var i = 0; i <hashes.length; i++) {
        hash = hashes[i].split('=');
        vars.push(hash[0]);
        vars[hash[0]] = hash[1];
      }
      return vars;
    }


    function generateSlides(){
        var contents = parser.data.mainvisual.contents;
        var str = "";
        var durations = [];
        for (var i = 0; i < contents.length; i++) {
          var content = contents[i];
          var link = content.link;
          var image = content.image;
          var duration = content.duration;
          var target = content.target;

          if(target == null){
            target = "_self";

          }

          if(link!=null){
            str+='<a href="'+link+'" target="'+target+'">'
          }else{
            str+='<a>'
          }
          str+='<img src="/main/img/'+image+'" />';
          if(link!=null){
            str+='</a>'
          }else{
            str+='</a>'
          }
          if(duration==null){
            duration = slideDuration;
          } 
          durations.push(duration * 1000);
        }
        $(".slides_container").append(str);
        $('#mainvisual').slides({
            preload: true,
            play: 3000,
            pause: 10,
            hoverPause: true,
            playIntervals:durations
        });
        var str ='<p><a href="/main/contents/">一覧</a></p>';
        $page = $('.pagination');
        $page.wrap("<div class='pagination'></div>");
        $page.removeAttr("class");
        $('.pagination').append(str);
    }
    function genreFromPref(genrePref){
      var genre;
      var genres = parser.data.genreList;
      for (var i = 0; i < genres.length; i++) {
        var tmpGenre = genres[i];
        if(tmpGenre.name == genrePref.name){
          genre = tmpGenre;
          break;
        } 
      };
      return genre;
    }

    function contentFromId(contentId,genre){
      var contents = genre.contents;
      var content;
      for (var i = 0; i < contents.length; i++) {
        var tmpContent = contents[i];
        if(tmpContent.id == contentId){
          content =  tmpContent;
          break;
        }
      };
      return content;
    }

    function parseComplete(){
        var genres = parser.data.genreList;
        var pref = parser.data.preferences;
        var $doc = $("#content");
        var genrePrefList = pref.genre;
        for (var i = 0; i < genrePrefList.length; i++) {
          var genrePref = genrePrefList[i];
          if(genrePref.name == "news"){
            $doc.append($("#news"));
          }else if(genrePref.name == "shopping"){
            $doc.append($("#shopping"));
          }else if(genrePref.name == "ynn"){
            $doc.append(writeYnn(true));
          }else{
            var genre = genreFromPref(genrePref);
            var contentIdList = genrePref.content;
            var str ="";
            str +='<div id="'+genre.name+'" class="boxContent type'+ genrePref.layout +'">';
            str +='<div class="ttl">';
            str +='<h2>'+genre.title+'</h2>';
            str +='<p><a href="contents/index.html#box'+upperCaseTitle(genre.name)+'">一覧</a></p>';
            str +='</div>';
           for (var j = 0; j < contentIdList.length; j++) {
              var contentid = contentIdList[j]
              var content = contentFromId(contentid,genre);
              if(genrePref.layout=="1" || genrePref.layout== "2"){
                var col = j % 3;
                if(col == 0){
                  str +='<div class="boxItem">';
                }
                str += contentStringType1(content.title,content.description,content.contentImage,content.contentLink,genrePref.layout);
                if(col == 2){
                  str +='</div>';
                }
              }else{
                str += contentStringType2(content.title,content.description,content.contentImage,content.contentLink);
              }
            };
            str +='<!-- //'+genre.name+' --></div>'
            $doc.append(str);
          }
        };
        generateSlides();
    }

    function upperCaseTitle(string){
        var cap = string;
        var cha = cap.substring(0,1).toUpperCase();
        var result = cha +  cap.substring(1,cap.length);
        return result;
    }

    function contentStringType1 (title,description,indexImage,contentLink,type){
        var cutted = description;
        if(cutted.length > descriptionLimit){
            cutted = cutted.substring(0,descriptionLimit - 1);
            cutted += "..."
        }
        
        var cuttedTitle = title;
        if(cuttedTitle.length > titleLimit){
            cuttedTitle = cuttedTitle.substring(0,titleLimit - 1);
            cuttedTitle += "..."
        }
        var str ="";
        str+= '<div class="item">';
        str+= '<div class="thumb"><a href="'+ contentLink +'" target="_blank"><img src="http://www.yoshimoto.co.jp//contents/img/'+ indexImage+'" title="'+title+'" width="135" height="100"/></a></div>';
        str+= '<h3><a href="'+ contentLink +'" target="_blank">'+cuttedTitle+'</a></h3>';
        if(type == "2"){
          str+= '<p>'+cutted+'</p>'
        };
        str+= '</div>';
        return str;
    }
    function contentStringType2 (title,description,indexImage,contentLink){
        var cutLength = 70;
        var cutted = description;

        if(cutted.length > cutLength){
            cutted = cutted.substring(0,cutLength - 1);
            cutted += "..."
        }
        var str ="";
        str+= '<div class="boxLive">';
        str+= '<div class="thumb"><a href="'+ contentLink +'" target="_blank"><img src="http://www.yoshimoto.co.jp//contents/img/'+ indexImage+'" title="'+title+'" width="135" height="100"/></a></div>';
        str+= '<dl>';
        str+= '<dt><a href="'+ contentLink +'">'+title+'</a></dt>';
        str+= '<dd>'+ cutted+'</dt>';
        str+= '</dl>';
        str+= '</div>';
        return str;
    }
    // ]]>
</script>

</head>
