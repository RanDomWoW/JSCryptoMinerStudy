<script type="text/javascript" charset="utf-8">

    function search(e, textarea) {
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 13) { //Enter keycode
            if ($(textarea).val().trim().length != 0) {

                window.location = '/Search/Show?keyword=' + $(textarea).val().trim();

            }
        }
    }

    var initColorBox = function () {
        $(".iframe").colorbox({ iframe: true, width: "80%", height: "80%" });
    }

    $(document).ready(function () {

        var date = new Date();

        date.setTime(date.getTime() + (1 * 24 * 60 * 60 * 1000));
        $.cookie.raw = true;
        $.cookie('goplextor', 'Language=en', { path: '/', expires: date, domain: '.goplextor.com' });
      
        var checkBrowser = function () {

            //console.log(navigator.userAgent);

            var os = $.client.os;
            var browser = $.client.browser;
            var version = $.client.version;

            //console.log("browser:" + browser);
            //console.log("version:" + version);

            var ie9 = (browser == "Explorer" || browser == "Mozilla") && version < 10;

            if (ie9) {
                var url = '/Error/browserIE';
                location.replace(url);
            }

            var check = browser == "Explorer" || browser == "Mozilla" || browser == "Chrome" || browser == "Safari" || browser == "Firefox";

            var cookie = $.cookie("PlextorBrowser");

            if (check == false && cookie == undefined) {

                var url = '/Error/browserOther';
                location.replace(url);
            }

        }

        if(isMobile() == false){
            checkBrowser();
        }

        initColorBox();

    });

    function isMobile() { return navigator.userAgent.toLowerCase().indexOf("mobile")>=0; }


</script>

