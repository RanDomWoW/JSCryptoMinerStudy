	<script type="text/javascript">
		(function($){
			var finding = '';
			var GET = {};

			$(document).ready(function($){
				var $chic = $('#chiclist');
				var $chicsingle = $('.single.chiclist');

				if( window.icegram_data != undefined ) {
					finding = setInterval(function(){
						findIcegram();

						if($("#icegram_message_43275").length > 0) {
							clearInterval(finding);
							populateIcegram($("#icegram_message_43275"));
							$("#icegram_message_43275").on('submit', 'form', function ( event ) {
								event.preventDefault();
								register( $(this) );
							});
						}
					}, 1000);
				}

				//Single post
				if ( $chicsingle.length > 0 ) {
					$chicsingle.find('form').submit(function ( event ) {
						event.preventDefault();
						register( $chicsingle.find('form') );
					});
				};

				//Modal & page
				if ( $chic.length > 0 ) {
					$chic.find('form').submit(function ( event ) {
						event.preventDefault();
						register( $chic.find('form') );
					});
				};

				if( $('.subscribe-template').length > 0 ) {
					$('.subscribe-template').find('form').on('submit',function ( event ) {
						event.preventDefault();
						register( $('.subscribe-template').find('form') );
					});
				};
			});

			function parserGet(input) {
				if (input.length > 1) {
					var params = input.slice(1).replace(/\+/g, ' ').split('&'),
						plength = params.length,
						tmp,
						key,
						val,
						obj,
						p;

					for (p = 0; p < plength; p += 1) {
						tmp = params[p].split('=');
						key = decodeURIComponent(tmp[0]);
						val = decodeURIComponent(tmp[1]);
						if (GET.hasOwnProperty(key)) {
							obj = GET[key];
							if (obj.constructor === Array) {
								obj.push(val);
							} else {
								GET[key] = [obj, val];
							}
						} else {
							GET[key] = val;
						}
					}
				}
			};

			function populateIcegram($ice) {
				parserGet(window.location.search);
				JSON.stringify(GET);

				$ice.find('input[name="SITESOURCE"]').val(window.location.href);
				$ice.find('input[name="UTMSOURCE"]').val(GET.utm_source);
				$ice.find('input[name="UTMMEDIUM"]').val(GET.utm_medium);
				$ice.find('input[name="UTMCAMP"]').val(GET.utm_campaign);
			}

			function replaceText($ice) {
				var text = $ice.find('.ig_message').text();

				if(text != '') {
					clearInterval(finding);

					text = text.replace('//thechrisellefactor.us11.list-manage.com//subscribe/post-json?u=8c6cf690ebd7fa990cd83ac28&id=97eecade9f&c=??', '');
					text = text.replace('(', '');
					text = text.replace(')', '');

					text = $.parseJSON(text);

					if(text.result != "success") {
						$ice.find('.ig_message').html( text.msg.replace("0 - ", "") );

						setTimeout(function() {
							$ice.find('.ig_content').fadeOut(function() {
								$ice.find('.ig_message').html('');
							});
						}, 5000);
					} else {
						$ice.find('.ig_message').html( text.msg );

						setTimeout(function() {
							$ice.find('.ig_close').trigger('click');
						}, 5000);
					}

					$ice.find('.ig_content').fadeIn();
				}
			}

			function findIcegram($ice_selector, $button_selector) {

				$ice_selector = $ice_selector || '#icegram_message_32433';
				$button_selector = $button_selector || '.ig_button';
				var $ice = $($ice_selector);

				if( $ice.length > 0 ) {
					clearInterval(finding);

					if(window.location.search != '') {
						populateIcegram($ice);
					}

					$ice.find('form ' + $button_selector).click(function ( event ) {
						finding = setInterval(function() {
							replaceText($ice);
						}, 1000);
					});
				}
			}

			function register($form) {
				$form.find('button').blur();

				$.ajax({
					type        : $form.attr('method'),
					url         : $form.attr('action'),
					data        : $form.serialize(),
					cache       : false,
					dataType    : 'jsonp',
					contentType : "application/json; charset=utf-8",
					error       : function(err) {
						console.log(err)
					},
					success     : function(data) {
						if(typeof(ga) != "undefined") {
						    ga('send', 'event', { eventCategory: 'Newsletter', eventAction: 'submit', eventLabel: 'signup', eventValue: 'Lead'});
						}
						else if(typeof(__gaTracker) != "undefined") {
						    __gaTracker('send', 'event', { eventCategory: 'Newsletter', eventAction: 'submit', eventLabel: 'signup', eventValue: 'Lead'});
						}
						if(typeof fbq !== 'undefined') fbq('track', 'Lead');

						if (data.result != "success") {
							var $response = $form.find('#mce-responses .response');
							// Something went wrong, do something to notify the user. maybe alert(data.msg);
							$response.html(data.msg.replace("0 - ", ""));

							setTimeout(function() {
								$response.parent().fadeOut();
							}, 5000);
						} else {
							var $response = $form.find('#mce-thankyou .response');
							// It worked, carry on...
							$response.html(data.msg);
							if(typeof(ga) != "undefined") {
							    ga('send', 'event', { eventCategory: 'Newsletter', eventAction: 'submit', eventLabel: 'signup', eventValue: 'CompleteRegistration'});
							}
							else if(typeof(__gaTracker) != "undefined") {
							    __gaTracker('send', 'event', { eventCategory: 'Newsletter', eventAction: 'submit', eventLabel: 'signup', eventValue: 'CompleteRegistration'});
							}
							if(typeof fbq !== 'undefined') fbq('track', 'CompleteRegistration');
							/*
							setTimeout(function() {
								$form.find('input').val('');
								$response.parent().hide();
							}, 15000);
							*/
						}

						$response.parent().fadeIn();
					}
				});
			}

		})(jQuery);
	</script>

