<script type='text/javascript'>//<![CDATA[
$(window).load(function(){
	var sideslider = $('[data-toggle=collapse-side]');
	var sel = sideslider.attr('data-target');
	var sel2 = sideslider.attr('data-target-2');
	sideslider.click(function(event){
		$(sel).toggleClass('in');
		$(sel2).toggleClass('out');
	});
		
	//alert($(window).width());
	if ($(window).width() >= 753 && $(window).width() <= 976) {   
		$expand = 455;   
	} else if ($(window).width() >= 977 && $(window).width() <= 1184) {   
		$expand = 553;   
	} else if ($(window).width() >= 1185 && $(window).width() <= 1424) {   
		$expand = 620;   
	} else { 
		$expand = 640; 
	}
	
	$('#search').on('focus',function(){
		var $w=$(this).width();
		$(this).animate({width:$w+$expand+'px'},300);
		document.getElementById("pc-menu").style.display = "none";
	})
	.on('blur',function(){
		var $w=$(this).width();
		if($(this).val() == '') {
			$(this).animate({width:$w-$expand+'px'},300);
			$("#pc-menu").delay(300).fadeIn(200);
		}
	});
	
	$(".mb-searchbar").click(function(){
        $("#mb-search-bar").slideToggle('fast');
    });
    
	// Add slideDown animation to dropdown
	$('.dropdown').on('show.bs.dropdown', function(e){
		$(this).find('.dropdown-menu').first().stop(true, true).slideDown();
	});

	// Add slideUp animation to dropdown
	$('.dropdown').on('hide.bs.dropdown', function(e){
		$(this).find('.dropdown-menu').first().stop(true, true).slideUp();
	});

});//]]> 
