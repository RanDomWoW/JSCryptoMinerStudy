<script type="text/javascript">



var rolling01 = new ObjectRolling();
var rolling02 = new ObjectRolling();

// Window load후 롤링 이미지 처리
addEventObserve(window, 'load', function(){

	rolling01.setObj(document.getElementById('top_img1'), document.getElementById('top_bt1'));
	rolling01.setObj(document.getElementById('top_img2'), document.getElementById('top_bt2'));
	rolling01.setObj(document.getElementById('top_img3'), document.getElementById('top_bt3'));
	rolling01.setObj(document.getElementById('top_img4'), document.getElementById('top_bt4'));
	rolling01.setObj(document.getElementById('top_img5'), document.getElementById('top_bt5'));
	

	setTimeout("rolling01.startRolling()", 3000);
	//$j('.promotion_view img').load(rolling01.startRolling());

	rolling02.setObj(document.getElementById('mid_img1'), document.getElementById('mid_btn1'));
	rolling02.setObj(document.getElementById('mid_img2'), document.getElementById('mid_btn2'));
	rolling02.startRolling();
});

$j(document).ready(function() {
	// 상단 프로모션 롤링 13개
	$j("div#MainContents div.promotion_img img:gt(0)").addClass("hidden");
	$j("div#MainContents ul.promotion_btn img").each(function(i) {
		var cnt = parseInt(i + 1);
		$j(this).attr( "id", "top_bt" + cnt );
		$j(this).hover(
			function() { rolling01.stopRolling(cnt); },
			function() { rolling01.startRolling(); }
		);
	});
	$j("div#MainContents div.promotion_img img").each(function(i) {
		var cnt = parseInt(i + 1);
		$j(this).attr( "id", "top_img" + cnt );
		$j(this).hover(
			function() { rolling01.stopRolling(cnt); },
			function() { rolling01.startRolling(); }
		);
	});

	// 중단 롤링 2개
	$j("div#MainContents div.promotion_main05 img.mid_view:eq(1)").addClass("hidden");
	$j("div#MainContents div.promotion_main05 span.main05_btn img").each(function(i) {
		var cnt = parseInt(i + 1);
		$j(this).attr( "id", "mid_btn" + cnt );
		$j(this).hover(
			function() { rolling02.stopRolling(cnt); },
			function() { rolling02.startRolling(); }
		);
	});
	$j("div#MainContents div.promotion_main05 img.mid_view").each(function(i) {
		var cnt = parseInt(i + 1);
		$j(this).attr( "id", "mid_img" + cnt );
		$j(this).hover(
			function() { rolling02.stopRolling(cnt); },
			function() { rolling02.startRolling(); }
		);
	});
});

</script>
