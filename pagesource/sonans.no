    <svg class="eyes" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 34 10" width="34" height="10"><title>eyes</title><g><path class="fill" d="M32 5a3 3 0 0 0-3-3V0a5 5 0 1 0 5 5zM8 5a3 3 0 0 0-3-3V0a5 5 0 1 0 5 5z"/></g></svg>
    
    <svg class="eye  eye--left" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 10 10" width="10" height="10"><title>eye</title><path class="fill" d="M8 5a3 3 0 0 0-3-3V0a5 5 0 1 0 5 5z"/></svg>
    <svg class="glasses" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 44 14" width="44" height="14"><title>glasses</title><path class="fill" d="M40 3v2h1.94a8 8 0 0 1-15.88 0H28V3h-2.14a4 4 0 0 0-7.72 0H16v2h1.94A8 8 0 0 1 2.06 5H4V3H0v1a10 10 0 0 0 20 0 2 2 0 0 1 4 0 10 10 0 0 0 20 0V3z"/></svg>
    
    <svg class="eye  eye--right" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 10 10" width="10" height="10"><defs></defs><title>eye</title><g id="" data-name=""><g id="" data-name=""><path class="fill" d="M8,5A3,3,0,0,0,5,2V0a5,5,0,1,0,5,5Z"/></g></g></svg>
 
    
    <svg class="ear-right" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 10.46 6.94" width="10.46" height="6.94"><defs></defs><title>ear-right</title><g><path class="fill" d="M0 5.21l9.85 1.73A40.62 40.62 0 0 0 10.46 0h-10A29.78 29.78 0 0 1 0 5.21z"/></g></svg>
    
    <svg class="mouth" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 51.42 17.02" width="51.42" height="17.02"><defs></defs><title>mouth</title><g><path class="fill" d="M25.71 7A29.85 29.85 0 0 1 6.43 0L0 7.66A39.83 39.83 0 0 0 25.71 17a39.85 39.85 0 0 0 25.71-9.34L45 0a29.89 29.89 0 0 1-19.29 7z"/></g></svg>
 
    <svg class="mouth-right-3" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 15.44 14.62" width="15.44" height="14.62"><title>mouth-right</title><path class="fill" d="M0 5.22l3.42 9.4a39.81 39.81 0 0 0 12-7L9 0a30.06 30.06 0 0 1-9 5.22z"/></svg>
 
 
              
          </div>
      </div>
      <div id="chat-close" ng-click="hideChat()"><i class="icon-close"></i></div>
    </div>
    
    <div class="chatbody">
      <div id="chatMessage">
        <div>
          <div class="row" ng-repeat="step in steps track by $index">
            <div class="message">
              <div class="chatbodyResponse">
                <!-- Response in writing -->
                <div ng-if="step.type == 'write'" class="row">
                  <div>
                    <section ng-repeat="response in step.response track by $index" class="jump_here">
                      <div class="chat chatbot">
                         <!--<div class="user-photo"><img src="/sites/all/themes/sonans/images/sonans-chatbot.png" alt=""></div>-->
                          <span class="chat-bubble sannachat"><a ng-show="!response.show_reply && response.heading" ng-click="show_hidden_answer($parent.$index, $index)">Klikk her for info om: {{ response.heading }}</a>
                        <div ng-if="response.show_reply" ng-bind-html="response.body | ashtml"></div>
                        <section ng-if="response.type == 'search'">
                          <angucomplete id="ex1" placeholder="Søk på fag" pause="100" selectedobject="default_search_option" localdata="search_subjects" searchfields="title" titlefield="title" minlength="1" inputclass="form-control form-control-small" />
                        </section>

                        <section ng-if="response.use_button">
                          <a href='/radgivning'>
                            {{ response.button_text }}
                          </a>
                        </section>

                        <section ng-if="response.use_second_button">
                          <a ng-if="!show_stuff.cancel_booking" ng-click="do_show_this(response.second_button_function)">
                            {{ response.second_button_text }}
                          </a>

                          <div ng-if="show_stuff.cancel_booking">
                            Dersom du ønsker å endre time, avbestille time eller ikke har blitt kontaktet bør du ta <a href="/skolene">kontakt med din skole</a>.
                          </div>
                        </section>

                        </span>
                      </div>
                    </section>

                    <div ng-if="$index > 0 && steps.length-1 == $index && !step.hide_vite" class="chat chatbot">

                      <span class="chat-bubble sannachat sanna-thumb">
                        Fant du svar på det du lette etter?
                        <section>
                          <!-- Not voted jet -->
                            <span ng-if="!step.voted" ng-click="vote_up()" class="sanna-vote ico-like-yellow"></span>
                            <span ng-if="!step.voted" ng-click="vote_down()" class="sanna-vote ico-dislike-yellow"></span>
                          <!-- Voted up! -->
                            <span ng-if="step.voted == 'up'" class="sanna-vote ico-like-black"></span>
                            <span ng-if="step.voted == 'up'" ng-click="vote_down()" class="sanna-vote ico-dislike-yellow"></span>
                          <!-- Voted down! -->
                            <span ng-if="step.voted == 'down'" ng-click="vote_up()" class="sanna-vote ico-like-yellow"></span>
                            <span ng-if="step.voted == 'down'" class="sanna-vote ico-dislike-black"></span>
                        </section>
                      </span>
                    </div>

                    <div class="chat chatbot" ng-if="(max_questions < asked_questions && steps.length-1 == $index && 
                                                     !show_stuff.lesson_plan && !show_stuff.booking && !step.responded_to) || step.voted == 'down' || queried_human">
                         <!--<div class="user-photo"><img src="/sites/all/themes/sonans/images/sonans-chatbot.png" alt=""></div>-->
                      <span class="chat-bubble sannachat">
                        <span ng-if="available_human">
                          Jeg ser forresten at {{ available_human.name }} er tilgjengelig på chatten vår, hvis du ikke finner svarene du leter etter kan jeg 
                          <a ng-click="talk_to_human()">
                             sette deg over?
                          </a>
                        </span>

                        <span ng-if="!available_human">
Hvis du ikke finner svar på det du ser etter kan du <a href="/kontakt-skole?from=chat" ng-click="log_contact_us()">legge igjen en beskjed</a>. Du får svar så fort en av våre veiledere ser den.
                         </span>
                      </span>
                    </div>

<!--
                    <section class="chat-bubble-user" ng-if="$index > 0 && steps.length-1 == $index && !step.responded_to && !show_stuff.lesson_plan && !show_stuff.booking">
                      <a ng-click="go_intent_less(step)">
                        Jeg spurte om noe annet
                      </a>
                    </section>
-->
                  </div>
                </div>

                <!-- Followup question select -->
                
                <div ng-if="step.type == 'select'" class="chat chatbot">
                   <!--<div class="user-photo"><img src="/sites/all/themes/sonans/images/sonans-chatbot.png" alt=""></div>-->
                    <p class="chat-bubble sannachat">{{ step.heading }}</p>
                </div>
              </div>

              <div ng-if="booking_parameters && show_stuff.booking && steps.length-1 == $index" class="showbooking">
                <div ng-if="found_an_open_slot">
                  <p>
                    Jeg ser en av våre rådgivere {{ booking_school_description }}har ledig tid {{ description_of_date }}, hvis du vil kan jeg lage en avtale for deg {{ description_of_time }}?
                  </p>

                  <button ng-click="reserve_this_spot(booking_parameters.slot)" class="btnOrder bookbutton bookconsulting">
                    {{ description_of_time }} {{ description_of_date }} passer fint
                  </button>
                  <a ng-click="no_go_on_spot()">
                    Hvis det ikke passer da kan du søke i tilgjengelige tidspunkter ved å klikke her
                  </a>
                </div>

                <div ng-if="!found_an_open_slot">
                  <p>
                    Hvilken dato kan passe for rådgivning?
                  </p>

                  <div datepicker ng-model="actual_booking.date" class="ui-datepicker" show-weeks="false" starting-day="1" year-range="2" ng-click="set_the_date()" min-date="minDate" max-date="maxDate" date-disabled="disabled(date, mode)">
                  </div>

                  <div ng-if="!actual_booking.selected_spot">
                    <div ng-repeat="item in available_slots"  class="centered_alignedtext chatbuttons booking_date_button_time_selection">
                      <button class="btnOrder bookbutton" ng-click="reserve_this_spot(item)">
                        {{ item.start | date:'shortTime' }} - {{ item.end | date:'shortTime' }}
                      </button>
                    </div>

                    <div>{{ missing_slots }}</div>
                  </div>
                </div>

                <div ng-if="insert_details" class="insert_details" id='start_booking_schema'>
                  <label>
                    <input placeholder="Ditt navn" type="text" ng-model="actual_booking.name" name="name"/>
                  </label>

                  <label>
                    <input placeholder="E-post" type="email" ng-model="actual_booking.email" name="email"/>
                  </label>

                  <div class="row">
                    <label>
                      <input placeholder="Mobil" type="tel" ng-model="actual_booking.phone" name="phone"/>
                    </label>
                  </div>

                  <div class="row">
                    <div class="chatContacttype">
                    <input type="radio" ng-model="actual_booking.booking_form" value="digital"> Via videochat
                    <br>
                    <input ng-if="booking_parameters.agreement_alias == 'sonng'" type="radio" ng-model="actual_booking.booking_form" value="phone"> <span ng-if="booking_parameters.agreement_alias == 'sonng'">Over telefon</span>
                    <br ng-if="booking_parameters.agreement_alias == 'sonng'">
                    <input ng-if="booking_parameters.agreement_alias != 'sonng'" type="radio" ng-model="actual_booking.booking_form" value="on_school">
                    <span ng-if="booking_parameters.agreement_alias != 'sonng'">
                              Oppmøte {{ school_descrition() }}
                            </span>

                    <div ng-if="booking_parameters.agreement_alias == 'sonng'">
                      <input type="radio" ng-model="actual_booking.booking_form" value="sonpriber_ng">
                      <span>Oppmøte i Bergen</span>
                    </div>
                    </div>
                  </div>

                  <div class="row">
                    <label>
                      <textarea placeholder="Kommentar" class="booking_comments" ng-model="actual_booking.comment" name="comment"></textarea>
                    </label>
                  </div>

                  <div>
                    {{ booking_error }}
                  </div>

                  <div ng-if="booking_in_progress">
                    <button class="btnOrder bookbutton bookconsulting">
                      Vennligst vent
                    </button>
                  </div>

                  <div ng-if="!booking_in_progress">
                    <button class="btnOrder bookbutton bookconsulting" ng-click="make_reservation()">
                      Reserver {{ description_of_time }} {{ description_of_date }}
                    </button>
                  </div>
                </div>

                <br />
                <br />
                <br />
                <br />
              </div>

              <div ng-if="show_stuff.lesson_plan && steps.length-1 == $index">
                <div ng-if="customer_type == 0" class="row">
                  <div class="col-xs-12 col-md-6">
                    <a ng-click="set_customer_type(2)">Jeg er elev hos Sonans</a>
                  </div>

                  <div class="col-xs-12 col-md-6">
                    <a ng-click="set_customer_type(1)">Jeg er ikke elev hos Sonans</a>
                  </div>
                </div>

                <div ng-if="customer_type == 1">
                  <a>Vis neste timeplan</a>
                  <div>
                    <div timetable data="lesson_plan_items"></div>
                  </div>
                  <button class="btnOrder">
                    Bestill denne timeplanen
                  </button>
                </div>

                <div ng-if="customer_type == 2">
                  Ditt brukernavn:
                  <input>
                  <a>Vis timeplan</a>
                  <div ng-if="lesson_plan_items">
                    <div timetable data="lesson_plan_items"></div>
                  </div>
                </div>
              </div>
            </div><!-- .message -->

            <div class="row chattextarea">
              <!-- New question in writing -->
              <div ng-if="step.type == 'write'">
                <textarea ng-trim="false" ng-if="steps.length-1 == $index && !show_stuff.lesson_plan && !show_stuff.booking" maxlength="128" rows="1" ng-keypress="pressed_key($event)" ng-model="step.selected" ng-change="check_for_enter(step.selected)" id="write_question_here_{{ $index }}" placeholder="Spørsmålet ditt her"></textarea>

                <section ng-if="steps.length-1 != $index" class="chat user">
                   <!--<div class="user-photo"><img src="/sites/all/themes/sonans/images/user.jpg" alt=""></div>-->
                    <p ng-if="step.selected" class="chat-bubble user">{{ step.selected }}</p>
                </section>
                <p ng-if="steps.length-1 == $index && step.responded_to && !show_stuff.lesson_plan && !show_stuff.booking">
                  <!--<span class="chat-spinner chat-spinner-1"></span>-->
                  <!--<img src="/sites/all/themes/sonans/images/chat.gif" alt="chat">-->
                </p>
              </div>

              <!-- New question in selection -->
              <section ng-if="step.type == 'select' && step.selected" class="chat user">
                 <!--<div class="user-photo"><img src="/sites/all/themes/sonans/images/user.jpg" alt=""></div>-->
                  <p ng-if="step.selected.description" class="chat-bubble user">{{ step.selected.description }}</p>
              </section>

              <p ng-if="step.type == 'select' && steps.length-1 == $index && step.selected && !booking_parameters">
                <!--<span class="chat-spinner chat-spinner-1"></span>-->
                <!--<img src="/sites/all/themes/sonans/images/chat.gif" alt="chat">-->
              </p>

              <div ng-if="steps.length-1 == $index">
                <button ng-click="cancel_step()" ng-if="(step.type == 'select' && !step.selected) || show_stuff.booking" class="bookbutton cancelbutton">
                   <span>Fortsett å chatte</span>
                </button>
              </div>

              <div ng-if="step.type == 'select' && steps.length-1 == $index && !step.selected" class="chatoptions">
                <div ng-repeat="option in step.options | orderBy:'description'" class="centered_alignedtext chatbuttons">
                  <button class="btnOrder bookbutton"  ng-click="answer_question(step, option)">
                    {{ option.description }} 
                  </button> <span class="bookplace"> - </span>
                </div>
              </div>
            </div>
          </div>
        </div>

      </div><!--#chatMessage-->
    </div>
  </div>
</div>
    
    <!--chatbot end-->  
  


<script>// <![CDATA[
(function (tos) {
 window.setInterval(function () {
 tos = (function (t) {
 return t[0] == 50 ? (parseInt(t[1]) + 1) + ':00' : (t[1] || '0') + ':' + (parseInt(t[0]) + 10);
 })(tos.split(':').reverse());
 window.pageTracker ? pageTracker._trackEvent('time on page', 'Log', tos) : ga('send', 'event', 'time on page', 'Log', tos, {'nonInteraction': 1});
 }, 10000);
})('00');
// ]]></script>


</body>
</html>