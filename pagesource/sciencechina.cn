<script language="javascript">
	function kscheck(){
	if (document.ksjs.inputword.value==""){
		alert("快速检索时，检索词不能为空!");
		return false;
	}
	if (document.ksjs.inputword.value!=""){
		var str1,sql1;
		str1=sql1="";
		str1=document.ksjs.inputword.value;
		sql1 = "((ISSN:" 
				+ str1 + ") OR (JOURNAL_NAME_GF:" 
				+ str1 + ") OR (SUPERSCRIPTION_CN:"
				+ str1 + ") OR (KEYWORD_CN:"
				+ str1 + ") OR (KEYWORD_EN:"
				+ str1 + ") OR (AUTHOR_NAMES:"
				+ str1 + ") OR (ORGANIZATIONS:"
				+ str1 + ") OR (TABLOID_CN:"
				+ str1 + ") OR (YEAR:"
				+ str1 + ")) AND (CITE:1)"				
		document.ksjs.searchword.value=sql1;
		document.ksjs.dbtype.value="1"
		document.ksjs.searchmode.value="1"
		
	}
	document.ksjs.submit();
	return true;
}
</script>


