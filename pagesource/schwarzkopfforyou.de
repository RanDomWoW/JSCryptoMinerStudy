<script type="text/javascript">
    $(document).ready(function() {
        $('#cookie_policy_overlay').find('.close').on('click', function(e) {
            e.preventDefault();
            var d = new Date();
            d.setTime(d.getTime() + (365 * 24 * 60 * 60 * 1000));
            document.cookie = "sk4uPersonalized[cookiesAccepted]=1; expires=" + d.toUTCString() + '; path=/';
            $('#cookie_policy_overlay').slideUp('slow');
        });
    });
</script>
