<script type="text/javascript">
    $(document).on('ready', function() {
        var rowCount = 1;
        var currentRow = 1;

        // Call it two times, because we want to show two rows from the beginning.
        showActionsRow();
        showActionsRow();

        $('.show-more-items').on('click', function(e) {
            e.preventDefault();
            showActionsRow();
        });

        function showActionsRow() {
            var itemsInRow = $('.row-' + currentRow).length;
            if (currentRow > 1) {
                previousRow = currentRow - 1;
                $('.row-' + previousRow).show(0);
            }
            if (itemsInRow == 1) {
                $('.row-' + currentRow).show(0);
                $('.placeholder-a').show(0);
                $('.placeholder-b').show(0);
                $('.show-more').hide(0);
            } else if (itemsInRow == 2) {
                $('.row-' + currentRow).show(0);
                $('.placeholder-b').show(0);
                $('.show-more').hide(0);
            } else if (itemsInRow == 3 && currentRow < rowCount) {
                $('.row-' + currentRow).each(function(i, el) {
                    if (i < 2) $(el).show(0);
                });
                //$('.row-'+currentRow).show(0);
                $('.show-more').show(0);
            } else if (itemsInRow == 3 && currentRow == rowCount) {
                $('.row-' + currentRow).show(0);
                $('.show-more').hide(0);
            }
            if (currentRow == rowCount) return false;
            currentRow++;
        }
    });
</script>        <!--Section Five-->
