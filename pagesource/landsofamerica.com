<script type="text/javascript">
					//-----------------------------------------------------------------------
					var urlPropertyType = "all-land";
					var handleSearchAddress = function(result, status) {
						var bSendForm = false;
						var searchObj = handleGeocodeAddress(result, status);
						if (searchObj.success) {
							switch (searchObj.searchType) {
								case "zip" : $('input[name="locURL"]').val(searchObj.u + urlPropertyType + "/");
									bSendForm = true;
								break;
								case "city" : $('input[name="locURL"]').val(searchObj.u + urlPropertyType + "/");
									bSendForm = true;
								break;
								case "county" : $('input[name="locURL"]').val(searchObj.u + urlPropertyType + "/");
									bSendForm = true;
								break;
								case "state" : $('input[name="locURL"]').val(searchObj.u + urlPropertyType + "/");
									bSendForm = true;
								break;
								default :
									$('input[name="locURL"]').val("/United-States/" + urlPropertyType + "/");
									bSendForm = true;
								break;
							}
						}
						else {
							var foundProp = false;
							var valCheck = $("#pageTopperSearch").val().trim();
							// check if it is a property id
							if ($.isNumeric(valCheck)) {
								$.ajax({
									type: "POST",
									url: '/webservices/remote.cfc?method=validatePropertyID&returnFormat=json',
									async: false,
									data: {
										propID: valCheck
									},
									dataType: 'json',
									success: function( checkresponse, request, settings ){
										foundProp = checkresponse.PROPFOUND;
									}
								});
							}

							if (foundProp) {
								var t=setTimeout("window.location.href = '/listing/" + valCheck + "'", 250);
							}
						}
						if (bSendForm) {
							sendSearch();
						}
					}
					//-----------------------------------------------------------------------
					var handleResult = function(e, row){
						if(row){
							$.ajax({
								type: 'POST',
								url: '/webservices/remote.cfc?method=saveHomeSearch&returnFormat=json',
								async: true,
								data: {
									argumentCollection: JSON.stringify( { searchRow: row } )
								},
								dataType: 'json',
								success: function( responseVariable, request, settings ){
								}
							});
							if (typeof row.G !== "undefined") {
								$('input[name="locURL"]').val(row.G + row.P);
							}
							else {
								var searchDir = "";
								$.ajax({
									type: 'GET',
									url: '/webservices/remote.cfc?method=getSearchTypeDirectory&returnFormat=json',
									async: false,
									data: {},
									dataType: 'json',
									success: function( responseVariable, request, settings ){
										searchDir = responseVariable;
									}
								});
								var locURL = 'america/land-for-sale';
								switch(row.T){
									case 'inv':// INVENTORY
										var t=setTimeout("window.location.href = '/listing/" + row.ID + "'", 250);
										return false;
										break;
									case 'zip':// ZIP CODE
										$('input[name="locURL"]').val("/" + locURL + searchDir + "/?hs=1&search=zip&zip=" + row.ZIP + "&state_id=" + row.SID + "&location=" + row.L);
										break;
									case 'ct':// CITY
										$('input[name="locURL"]').val("/" + locURL + searchDir + "/?hs=1&search=city&city_id=" + row.ID + "&location=" + row.L);
										break;
									case 'cy':// COUNTY
										$('input[name="locURL"]').val("/" + locURL + searchDir + "/?hs=1&search=county&county_id=" + row.ID + "&location=" + row.L);
										break;
									case 'lk':// LAKE
										$('input[name="locURL"]').val("/" + locURL + searchDir + "/?hs=1&search=lake&lake_id=" + row.ID + "&location=" + row.L);
										break;
									case 's':// STATE
										$('input[name="locURL"]').val("/" + row.L.replace(/ /g, '_').toLowerCase() + "/");
										break;
								}
							}
							$('#pageTopperSearch').data( "doFocus", 0 );
							shiftFocus( 2 );
						}
						else{
							alert('We couldn\'t find the location you entered. Please re-enter the name, and choose the location from the list.');
						}
					}
					/*-----------------------------------------------------------------------*/
					/**/
					/**/
					$(document).ready(function() {
						$('#state-search').tooltip();
						$('#pageTopperSearch').on({
							"focus": function(){
								var doFocus = $('#pageTopperSearch').data( "doFocus" );
								if( typeof doFocus == 'undefined' ){
									doFocus = 1;
								}
								if( doFocus == 1 ){
									$(this).data('placeholder', $(this).attr('placeholder'));
									$(this).attr('placeholder', '');

									/**/

									/**/
								}
								else{
									$('#pageTopperSearch').data( "doFocus", 1 );
								}
							},
							"blur": function(){
								$(this).attr('placeholder', $(this).data('placeholder'));
								$(this).data('placeholder', '');
							},
							"keydown":function(e){
								/**/
								if ( ( e.which == 8 ) || ( e.which == 32 ) || ( e.which == 46 )
									|| ( (e.which >= 48) && (e.which <= 90) )
									|| ( (e.which >= 96) && (e.which <= 111) )
									|| ( (e.which >= 186) && (e.which <= 192) )
									|| ( (e.which >= 219) && (e.which <= 222) ) ) {

									$('input[name="locURL"]').val('');
								}
							},
							"keyup": function(){
								if ($("#searchSubmit").text().indexOf("CONTINUE") >= 0) {
									$("#searchSubmit").html($("#searchSubmit").html().replace("CONTINUE LAST SEARCH", "SEARCH"));
								}
							}
						});// END $('#pageTopperSearch').on({

					}); // END $(document).ready(function(){

				</script>
