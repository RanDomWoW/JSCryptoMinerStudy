<style type='text/css'>/* MENU ICONS */

#navigation li {
    line-height: 1;
}
#navigation ul li ul li a:before {
  color:#10874f;
  content:"\f105";
  font-family:FontAwesome !important;
  padding-right:5px;
}
#navigation ul li ul li{
  padding:7px 5px 5px 7px !important;
  background-color: transparent !important;
}
#navigation ul li ul li a{
  display:inline-block;
  font-family: "Rockwell",helvetica,arial,sans-serif !important;
  font-size:14px !important;
  font-weight:normal;
}

/* OTHER SUBMENU STYLING */
#navigation ul li ul{
  background-color:#f8f8f8 !important;
  border-top:0px !important;
}
.navigation ul ul li:nth-of-type(5) a,.navigation ul ul li:nth-of-type(10) a{
  border-bottom:none !important;
}
.navigation ul ul li{
  background-color:transparent !important;
}
.navigation ul ul li:nth-of-type(1) a,.navigation ul ul li:nth-of-type(6) a{
  padding-top:7px !important;
}
.navigation ul ul{
  margin-left:11px !important;
}

#navigation ul ul li a{
  color:#333 !important;
  padding-top:10px!important;
  padding-bottom:18px !important;
}
.navigation ul ul a:hover{
  opacity:0.7;
  transition: all 0.2s;
}
.navigation ul ul a{
  transition: all 0.2s;
}
/* OTHER MENU STYLING */

.menu .mobile{
display:none;
}
@media screen and (max-width:960px){
/* MOBILE MENU */
 
  nav{
    position:absolute;
    top:0px;
  }
  header .container{
    max-width:100% !important;
    width:100% !important;
  }
  header .three.columns{
    width:140px !important;
  }
  header .nine.columns{
    margin:0px !important;
    width:100% !important;
  }
  header{
    box-shadow:0px 0px 2px rgba(0,0,0,0.2) !important;
    height:36px;
    padding-top:15px !important;
    position:relative !important;
  }
  header button{
    margin-top:-5px !important;
    padding-top:0px !important;
  }
  header .button.small.green{
    display:none;
  }
  header .header-right{
    display:none;
  }
  header #logo{
    margin-bottom:0px !important;
  }
  header .dl-active{
    border:none !important;
  }
  .dl-menu-wrapper{
    max-width:100% !important;
    width:100% !important;
  }
  #rm-dl-menu{
       margin-top:38px !important;
  }
  #rm-dl-menu li, #rm-dl-menu {
    border:none !important;
  }
    #rm-dl-menu li{
    padding:7px;
  }
  #rm-dl-menu li a{
    font-family:'Rockwell',arial,helvetica,sans-serif !important;
    font-size:18px !important;
    cursor:pointer;
    text-align: left !important;
  }

.dl-menuwrapper .dl-submenu li{
    width:100% !important;
  }
.dl-submenu {
  background-color:#efefef !important;
  border:none !important;
  width:100% !important;
  
}
.dl-back{
  display:none;
}
  .dl-menuwrapper button::after {
    background: #10874f !important;
    box-shadow: 0 7px 0 #10874f,0 14px 0 #10874f !important;
}
.menu .mobile{
display:block;
}
}</style>

