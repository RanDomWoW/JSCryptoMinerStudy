<script type="text/javascript">
	$(function () {
		// Показываем окно через 30 сек.
		setTimeout(function() {
			$("#modalCatcher").modal();
			}, 45000);
			
		// Меняем стандартную анимацию появления окна
		$("#modalCatcher").on('show.bs.modal', function() {
			$(this).animateCss('flipInX')
			});
		// Меняем стандартную анимацию скрытия окна
		$("#modalCatcher").on('hide.bs.modal', function() {
			//$(this).animateCss('flipOutX')
			});
		
		// Заполняем [name=NAME] для названия Лида
		$('#catcher-input').on('input',function() {
			$('#modalCatcher [name=NAME]').val($(this).val());
			// Одновременно проверяем на заполненность номера
			var numberClear = $(this).val().replace(/_/g,"").replace(/ /g,"").replace(/-/g,"");
			if(numberClear.length == 12)
			{
				$("#catcher-form [type=submit]").attr("disabled",false);
			}
			else
			{
				$("#catcher-form [type=submit]").attr("disabled",true);
			}
		});

			
			
		$("#catcher-form").on('submit', function(event){
				event.preventDefault();
				
				// Деактивирем кнопку сразу после отправки
				$(this).find("[type=submit]").attr("disabled",true);

				var $that = $(this);
				$.ajax({
						type: $that.attr('method'),
						url: '/LEADGEN/sendLead.php',
						data: $that.serialize(),
						dataType: "json",
						cache: false,
						beforeSend: function(){
						},
						complete: function(data){
						},
						success: function(json){
								console.log('Успешно');
								console.log(json);
								$that.get(0).reset();
								$("#modalCatcher").modal("hide");
								$("#modalSendMess").modal();
						},
						error: function(data) {
								console.log('Ошибка');
								console.log(data);
						}
				});
			});
				
    });
</script>

