<script>
	function customSize() {
		var w	= resize.getBrowserWidth( );
		if( w < 1000 ) {
			jQuery( ".image-inner" ).css( "width", "1000px");
		}
		else {
			jQuery( ".image-inner" ).css( "width", "100%");
		}
	}
	jQuery(document).ready(
		function(){
			resize.onCustom = customSize;
			resize.onInitsize();
		}
	);
	
	jQuery(window).resize(
		function(){
			resize.onResize( );
		}
	);
</script>
