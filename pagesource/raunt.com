<script>
	var bannerHref = "";
	var text = "";
	var dontShowBanner = getCookie("dontShowMobileBanner");
	if (bowser.tablet){
		text = "Raunt<br>Tablet Uygulaması";
		if (bowser.ios){
			bannerHref = "https://itunes.apple.com/us/app/raunt-tablet-ygs-lys-haz-rl-k/id1095667080?l=tr&ls=1&mt=8";
		}
		else if (bowser.android){
			bannerHref = "https://play.google.com/store/apps/details?id=com.sebit.raunt.tablet";
		}
	}
	else if (bowser.mobile){
		text = "Raunt<br>Cep Uygulaması";
		if (bowser.ios){
			bannerHref="https://itunes.apple.com/tr/app/raunt/id1271952494?l=tr&mt=8"
		}
		else if (bowser.android){
			if(bowser.windowsphone){
				bannerHref = "https://www.microsoft.com/tr-tr/store/p/raunt-cep/9nblggh514p1";
			}
			else{
				bannerHref = "https://play.google.com/store/apps/details?id=com.sebit.raunt";				
			}
		}
	}
	
	else{
		$("#mobile-banner-header").css("display","none");
	}
	
	if (bowser.name == "Internet Explorer"){
		$("#mobile-banner-header").css("display","none");
	}
	
	var mobileLink = '<a href="'+bannerHref+'" target="_blank" class="button button-border mobile-banner-links" style="background-color: #feb715;border-radius: 5px;color:#000; width: 115px; padding-left: 10px;">GÖRÜNTÜLE</a>'
	
	$("#mobile-link-container").append(mobileLink);
	$("#mobile-banner-logo-text").html(text);
	
	$("#mobile-banner-close-button").on("click", function (){
		setCookie("dontShowMobileBanner", "true", 10);
		$("#mobile-banner-header").css("display","none");
		/*var sliderHeight = parseInt($("#slider").css("height").split("px")[0]);
		$("#slider").css("height",(sliderHeight+80)+"px");*/
	})
	
	function setCookie(cname, cvalue, exdays) {
	    var d = new Date();
	    d.setTime(d.getTime() + (exdays*24*60*60*1000));
	    var expires = "expires="+ d.toUTCString();
	    document.cookie = cname + "=" + cvalue + "; " + expires;
	}
	
	function getCookie(cname) {
	    var name = cname + "=";
	    var ca = document.cookie.split(';');
	    for(var i = 0; i <ca.length; i++) {
	        var c = ca[i];
	        while (c.charAt(0)==' ') {
	            c = c.substring(1);
	        }
	        if (c.indexOf(name) == 0) {
	            return c.substring(name.length,c.length);
	        }
	    }
	    return "";
	}
</script>



