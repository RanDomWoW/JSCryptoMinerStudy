    <script type="text/javascript">
        //Scoped to the PDP, as there is a similar function on the PLP
        Handlebars.registerHelper("pdpWillHideSwatches", function(items, options) {
            if (items.length > 8) {
                return options.fn(this);
            } else {
                return options.inverse(this);
            }
        });

        (function($) {
            $(function () {
                if (aligentFredhopper.campaign) {
                    aligentFredhopper.campaign.events.beforeCampaignPush = function($container, $xml_data) {
                        var result = {
                                $container: $container,
                                $xml_data: $xml_data,
                                continue: true
                            },
                            $static_content = $xml_data.find('static-content');
                        if (typeof $static_content[0] !== "undefined" && !$container.is(':empty')) {
                            result.continue = false;
                        }
                        return result;
                    }
                    aligentFredhopper.campaign.events.afterSuccess = function($xml_data) {
                        if (aligentFredhopper.campaign.fh_view && aligentFredhopper.campaign.fh_view=='detail') {
                            // CUSTOM PDP: Colour Swatches
                            $item = $xml_data.find('universes universe[name="' + aligentFredhopper.campaign.universe + '"] items-section items item:first');
                            // If there is an item
                            if ($item[0]) {
                                var // item's url_key attribute
                                    url_key = $item.find('attribute[name="url_key"] value').text(),
                                    // item's colour_switch attribute
                                    // Colour swatches comes from FH as a string
                                    // url_key;;colour_group;;colour_fashion||url_key;;colour_group;;colour_fashion||etc
                                    // where || is delimiter between variants and ;; is delimiter between attributes in one variant
                                    colour_swatches = $item.find('attribute[name="colour_switch"] value').text().split("||"),
                                    // prepared JSON for Handlebars
                                    json_data = {
                                        items: []
                                    };
                                if (colour_swatches.length) {
                                    $.each(colour_swatches, function (j, value) {
                                        // split into attributes url_key, colour_group and colour_fashion
                                        colour_swatch = value.split(";;");
                                        // if on any reason one of attributes was lost, we skip this variant here
                                        if (!colour_swatch[0] || !colour_swatch[1] || !colour_swatch[2]) {
                                            return;
                                        }
                                        // populate data
                                        json_data.items.push({
                                            url_key: colour_swatch[0],
                                            colour_group: colour_swatch[1].replace(/\W/, '_').toLowerCase(),
                                            colour_fashion: colour_swatch[2],
                                            colour_fashion_class: colour_swatch[2].toLowerCase().replace(/ /g, '_'),

                                            // check if colour swatch is current product actually
                                            selected: url_key == colour_swatch[0]
                                        });
                                    });
                                    // template source code - specifically different for Product
                                    var source = $('#custom_fredhopper-handlebars-colour_swatches_pdp').html();
                                    if (source) {
                                        // compile source code with Handlebars
                                        var template = Handlebars.compile(source);
                                        var html = template(json_data);
                                        // inject rendered Colour Swatches template into DOM of current product
                                        $('.product-main-info').after(html);
                                    }

                                    tbSwatchSlider.setSwatchContainerWidth();
                                }
                            }

                            // CUSTOM PDP: We Recommend
                            if ($xml_data.find('universes universe[name="'+aligentFredhopper.campaign.universe+'"] themes theme custom-field[name="'+aligentFredhopper.campaign.fh_campaign_type+'"]:contains("pdp_werecommend")')[0]) {
                                $('.pdp__upsell-title[data-toggle="youmightlike"]').show();
                                $('#aligent_fredhopper-campaign-pdp_werecommend').parent().show();
                            } else {
                                $('.pdp__upsell-title[data-toggle="youmightlike"]').hide();
                                $('#aligent_fredhopper-campaign-pdp_werecommend').parent().hide();
                            }

                            // CUSTOM PDP: You May Also Like
                            if ($xml_data.find('universes universe[name="'+aligentFredhopper.campaign.universe+'"] themes theme custom-field[name="'+aligentFredhopper.campaign.fh_campaign_type+'"]:contains("pdp_youmightlike")')[0]) {
                                $('#aligent_fredhopper-campaign-pdp_youmightlike').parent().show();
                            }

                            var tbPDP = new TBPDP();
                            tbPDP.init();
                        }

                        // CUSTOM Themes: Colour Swatches for items themes like Zero Results (excluding We Recommend, You May Also Like Themes)
                        // Get themes from FH response
                        $themes = $xml_data.find('universes universe[name="'+aligentFredhopper.campaign.universe+'"] themes > theme');
                        if ($themes[0]) {
                            $themes.each(function () {
                                campaign_slot = $(this).find('custom-field[name="campaign_slot"]').text();
                                if (campaign_slot == 'pdp_youmightlike' || campaign_slot == 'pdp_werecommend') {
                                    return;
                                }
                                $theme = $(this);
                                // Get items from theme
                                $items = $theme.find('items item');
                                // If there are items => then it is a theme with products
                                if ($items[0]) {
                                    $items.each(function () {
                                        // item
                                        var $item = $(this),
                                            // item's url_key attribute
                                            url_key = $item.find('attribute[name="url_key"] value').text(),
                                            // item's colour_switch attribute
                                            // Colour swatches comes from FH as a string
                                            // url_key;;colour_group;;colour_fashion||url_key;;colour_group;;colour_fashion||etc
                                            // where || is delimiter between variants and ;; is delimiter between attributes in one variant
                                            colour_swatches = $item.find('attribute[name="colour_switch"] value').text().split("||"),
                                            // prepared JSON for Handlebars
                                            json_data = {
                                                items: []
                                            };

                                        if (false && colour_swatches.length) { //Colour swatches have been temporarily disabled from the zero search results page
                                            $.each(colour_swatches, function (j, value) {
                                                // split into attributes url_key, colour_group and colour_fashion
                                                colour_swatch = value.split(";;");
                                                // if on any reason one of attributes was lost, we skip this variant here
                                                if (!colour_swatch[0] || !colour_swatch[1] || !colour_swatch[2]) {
                                                    return;
                                                }
                                                // populate data
                                                json_data.items.push({
                                                    url_key: colour_swatch[0],
                                                    colour_group: colour_swatch[1].replace(/\W/, '_').toLowerCase(),
                                                    colour_fashion: colour_swatch[2],
                                                    colour_fashion_class: colour_swatch[2].toLowerCase().replace(/ /g, '_'),

                                                    // check if colour swatch is current product actually
                                                    selected: url_key == colour_swatch[0]
                                                });
                                            });

                                            // template source code - specifically different for themes
                                            var source = $('#custom_fredhopper-handlebars-colour_swatches_themes').html();
                                            if (source) {
                                                // compile source code with Handlebars
                                                var template = Handlebars.compile(source);
                                                var html = template(json_data);
                                                // inject rendered Colour Swatches template into DOM of current product
                                                $('#' + $theme.find('custom-field[name="' + aligentFredhopper.campaign.fh_campaign_type + '"]').text() + '_' + $item.find('attribute[name="secondid"] value:first').text() + ' .item-image').after(html);
                                            }
                                        }
                                    });
                                }
                            });
                        }

                        document.dispatchEvent(new Event('fredHopperDone'));
                    }
                }
            });
        })(jQuery);

        var tbSwatchSlider = new TBSwatchSlider(".product-view", 6);
    </script>

<script type="text/javascript">
    /**
     * This helper essentially adds logical if statements
     *
     * Usage:
     *  {{compare @variable1 '>' @variable2}} <div>variable1 is greater than variable2</div> {{/compare}}
     *
     *  Reference:
     *   https://gist.github.com/pheuter/3515945#gistcomment-1378171
     *
     * @param lvalue   The first value to compare
     * @param operator The comparison operator to use
     * @param rvalue   The second value to compare
     * @param options  Handlebars options hash
     */
    Handlebars.registerHelper('compare', function (lvalue, operator, rvalue, options) {
        var operators, result;

        if (arguments.length < 3) {
            throw new Error("Handlerbars Helper 'compare' needs 2 parameters");
        }

        if (options === undefined) {
            options = rvalue;
            rvalue = operator;
            operator = "===";
        }

        operators = {
            '==': function (l, r) { return l == r; },
            '===': function (l, r) { return l === r; },
            '!=': function (l, r) { return l != r; },
            '!==': function (l, r) { return l !== r; },
            '<': function (l, r) { return l < r; },
            '>': function (l, r) { return l > r; },
            '<=': function (l, r) { return l <= r; },
            '>=': function (l, r) { return l >= r; },
            'typeof': function (l, r) { return typeof l == r; }
        };

        if (!operators[operator]) {
            throw new Error("Handlerbars Helper 'compare' doesn't know the operator " + operator);
        }

        result = operators[operator](lvalue, rvalue);

        if (result) {
            return options.fn(this);
        } else {
            return options.inverse(this);
        }

    });
</script>
