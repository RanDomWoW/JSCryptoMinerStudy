    <script type="text/javascript">
        /**
         * Auxiliary function to show Results
         *
         * @param {array} data The suggestion
         */
        window.aligentFredhopper['autocomplete'].callbacks.results_show = function(data) {
            var $fredhopperSearchResults = jQuery(aligentFredhopper.autocomplete.selectors.results);

            var $keywordContainer    = jQuery("#js-aligent_fredhopper-autocomplete-results-1keywords");
            var $categoriesContainer = jQuery("#js-aligent_fredhopper-autocomplete-results-2categories");
            var $productsContainer   = jQuery("#js-aligent_fredhopper-autocomplete-results-3products");
            var $storesContainer     = jQuery("#js-aligent_fredhopper-autocomplete-results-4stores");

            if (typeof data !== "undefined") {
                var keywords = data[0].suggestions;
                var categories = data[1].suggestions;
                var products = data[2].suggestions;
                var stores = data[3].suggestions;

                // Check if there are any search suggestions
                if (keywords.length === 0 && categories.length === 0 && products.length === 0 && stores.length === 0) {
                    $fredhopperSearchResults.toggleClass("no-results", true).html("No results");
                    // Hide Results, if no suggestions
                    aligentFredhopper.autocomplete.load.show = false;
                }

                if (keywords.length === 0) {
                    $keywordContainer.toggleClass("no-results", true);
                }

                if (categories.length === 0) {
                    $categoriesContainer.toggleClass("no-results", true);
                }

                if (products.length === 0) {
                    $productsContainer.toggleClass("no-results", true);
                }

                if (stores.length === 0) {
                    $storesContainer.toggleClass("no-results", true);
                }
            }

            if ($categoriesContainer[0]) {
                var $keywordsUlContainer = $keywordContainer.find('ul');
                $keywordsUlContainer.html(
                    $keywordsUlContainer.html() +
                    $categoriesContainer.find('ul').html()
                );
                $categoriesContainer.remove();
            }

            $fredhopperSearchResults.toggle(aligentFredhopper.autocomplete.load.show);
            $fredhopperSearchResults.parent().toggleClass('hide-autocomplete', !aligentFredhopper.autocomplete.load.show);

            jQuery(".wrapper").addClass('enable-overlay');

        };

        var tbHeader = new TBHeader();
        tbHeader.init();

        var tbCarousel = new TBCarousel();
        tbCarousel.init();

