<script type="text/template" id="aligent_fredhopper-handlebars-campaign-image">
    {{#each items}}
        <a href="{{url}}"><img src="{{image}}" alt="{{alt}}" class="img-responsive" /></a>
    {{/each}}
</script>
<script type="text/template" id="aligent_fredhopper-handlebars-campaign-image-map">
    <div style="margin-bottom: 15px;">
        <img src="{{img}}" usemap="#{{id}}" class="img-responsive">
        <map name="{{id}}">
        {{#each areas}}
        <area href="{{url}}" shape="rect" coords="{{x1}},{{y1}},{{x2}},{{y2}}" alt="{{alt}}" title="{{alt}}" target="{{target}}">
        {{/each}}
        </map>
    </div>
</script>
<script type="text/template" id="aligent_fredhopper-handlebars-campaign-items">
    {{#each items}}
        <li class="item type-configurable js-aligent_fredhopper-instant-item-container" data-index="{{index}}" id="{{../slot}}_{{secondid}}">
            <a class="item-image thumbnail directLink" title="{{name}}" href="/{{url_key}}.html">
                <div class="content">
                    <img
                        alt="{{name}}"
                        src="{{#if _thumburl}}{{_thumburl}}{{else}}{{_imageurl}}{{/if}}"
                        data-rollover-src="{{#if thumb_rollover}}{{thumb_rollover}}{{else}}{{_imageurl}}{{/if}}"
                    >
                </div>
            </a>
            <p class="product-name"><a title="{{name}}" href="/{{url_key}}.html" class="directLink">{{name}}</a></p>
            <p class="product-color">{{colour_fashion}}</p>
            {{#xif "rrp != current_price"}}
            <div class="price-box">
                <p class="special-price">
                    <span class="price-label">Special Price</span>
                    <span id="product-price-{{id}}" class="price">${{current_price}}</span>
                </p>
                <p class="old-price">
                    <span class="price-label">Regular Price:</span>
                    <span id="old-price-{{id}}" class="price">${{rrp}}</span>
                </p>
            </div>
            {{else}}
            <div class="price-box">
                <span id="product-price-{{id}}" class="regular-price">
                    <span class="price">${{current_price}}</span>
                </span>
            </div>
            {{/xif}}
        </li>
    {{/each}}
</script>

