<script type="text/javascript">

var _gaq = _gaq || [];

// Put your analytics code here
_gaq.push(['_setAccount', 'UA-20710450-1']);

// This Section passes URLs for pages during the Order Process.  B2BSeller has the same page name for shipbill and order confirmation, so this code passes unique URLS
if ($(".ShoppingCart_DescriptionText").length !=0) {_gaq.push(['_trackPageview', '/default.aspx?page=shopping_cart']);}
else if ($(".ShipBillInformation_ShipMethodLabel").length !=0) {_gaq.push(['_trackPageview', '/default.aspx?page=shipping_information']);}
else if ($(".OrderSummary_DescriptionText").length != 0) {_gaq.push(['_trackPageview', '/default.aspx?page=order_confirmation']);}

// This Is the Code to Pass Order Information on the Order Summary Page to Analytics
else if ($(".OrderConfirmation_HeaderDescription").length != 0) {
    _gaq.push(['_trackPageview', '/default.aspx?page=order_complete']);
    if ($(".OrderConfirmation_OrderNumberText").text().length != 0) {
        oN = $('.OrderConfirmation_OrderNumberText').text().trim();
    }
    else {
        oN = $('.OrderConfirmation_WebReferenceText').text().trim();
    }    
    
    iT = $('#InvoiceTotal').text().replace(/\$|,/g, '');
    _gaq.push(['_addTrans',oN,'ICC',iT,'','','','','']);
    
    $(".OrderConfirmation_Table tr").each(function(index) {
        if (index == 0) {
            return true; 
        }
        var itemCode = $(".OrderConfirmation_ItemCode", this).text();
        var itemDesc = $(".OrderConfirmation_Description", this).text();
        var itemQty = $(".OrderConfirmation_Quantity", this).text();
        var itemPrice = $(".OrderConfirmation_Price", this).text().replace(/\$|,/g, '');
        _gaq.push(['_addItem',oN,itemCode,itemCode, 'NA',itemPrice,itemQty]);
        
    });

    _gaq.push(['_trackTrans']);
}
else {_gaq.push(['_trackPageview']);}

(function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl.google-analytics.com/ga.js' : 'http://www.google-analytics.com/ga.js');
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
})();

</script>
