    $(document).ready(function() {

        /*Consumir mosaicoAsistenciasTorneo para asistencia por AJAX*/
        // URL Produccion
        var urlAsistencia = 'http://siidadmin.ligamx.net/webservices/prtl_web_jsondata.ashx?hsah=|a090b36be13c6b89f155d9c4aeea5b61c2f3172983397b6152f0f2b61b320bd232fcc1331fbaa4ff042243e4210f39d8195d28b6a166526b2890b410f17e2ce3|&psWidget=PRTL_LIGA_MosaicoEstadistica&objTipo=AsistenciasTorneo&objIDTemporada=<IDTEMPORADA>&objIDTorneo=<IDTORNEO>&objIDDivision=<IDDIVISION>';
        LMX.consultarServicio( urlAsistencia, {"time":91}, function(data){
            var htmlAsist="";
            htmlAsist += '<span class="titulo">Asistencia</span>';
            if(data['DatosJSON'][0].TOTAL>0){
                htmlAsist += '<span class="big">' + data['DatosJSON'][0].TOTAL + '</span>';
                htmlAsist += '<small>Asistentes</small>';
            }
            $('#secc_asistencia').html(htmlAsist);
        });


        /*Consumir mosaicoLiderTorneo para Líder Torneo por AJAX*/
        // URL Produccion
        var urlLiderTorneo = 'http://siidadmin.ligamx.net/webservices/prtl_web_jsondata.ashx?hsah=|a090b36be13c6b89f155d9c4aeea5b61c2f3172983397b6152f0f2b61b320bd232fcc1331fbaa4ff042243e4210f39d8195d28b6a166526b2890b410f17e2ce3|&psWidget=PRTL_LIGA_MosaicoEstadistica&objTipo=LiderTorneo&objIDTemporada=<IDTEMPORADA>&objIDTorneo=<IDTORNEO>&objIDDivision=<IDDIVISION>';
        LMX.consultarServicio( urlLiderTorneo, {"time":91}, function(data){
            var htmlLT="";
            if(data['DatosJSON'].length >0 && data['DatosJSON'][0].JJ>0 ){
                htmlLT += '<img height="40" width="40" src="https://s3.amazonaws.com/lmxwebsite/docs/archdgtl/AfldDrct/logos/'+ data['DatosJSON'][0].IDClub+'/'+ data['DatosJSON'][0].IDClub+'.png" alt="'+data['DatosJSON'][0].Nombre+'" class="img-responsivimg img-responsive">';
            }
            htmlLT += '<p><span class="titulo">Líder Torneo</span><span>'+( (data['DatosJSON'].length >0 && data['DatosJSON'][0].JJ>0) ? data['DatosJSON'][0].Nombre : "")+'</span></p>';
            $('#secc_lidertorneo').html(htmlLT);
        });


        /*Consumir mosaicoGolesAnotados para asistencia por AJAX*/
        // URL Produccion
        var urlGoles = 'http://siidadmin.ligamx.net/webservices/prtl_web_jsondata.ashx?hsah=|a090b36be13c6b89f155d9c4aeea5b61c2f3172983397b6152f0f2b61b320bd232fcc1331fbaa4ff042243e4210f39d8195d28b6a166526b2890b410f17e2ce3|&psWidget=PRTL_LIGA_MosaicoEstadistica&objTipo=GolesAnotados&objIDTemporada=<IDTEMPORADA>&objIDTorneo=<IDTORNEO>&objIDDivision=<IDDIVISION>&objFase=1&objTop=1';
        LMX.consultarServicio( urlGoles, {"time":91}, function(data){
            var htmlGoles="";
            htmlGoles += '<span class="titulo">Anotados</span>';
            htmlGoles += '<span>' + (data["DatosJSON"][0].TOTAL > 0 ? data["DatosJSON"][0].TOTAL + " Goles" : "") + '</span>';
            $('#secc_golesanotados').html(htmlGoles);
        });


        /*Consumir mosaicoAsistenciasTorneo para asistencia por AJAX*/
        // URL Produccion
        var urlCociente = 'http://siidadmin.ligamx.net/webservices/prtl_web_jsondata.ashx?hsah=|a090b36be13c6b89f155d9c4aeea5b61c2f3172983397b6152f0f2b61b320bd232fcc1331fbaa4ff042243e4210f39d8195d28b6a166526b2890b410f17e2ce3|&psWidget=PRTL_Cociente&objIdDivision=<IDDIVISION>&objIdTemporada=<IDTEMPORADA>&objIDTorneo=<IDTORNEO>';
        LMX.consultarServicio( urlCociente, {"time":91}, function(datas){
            var data =datas["DatosJSON"];
            var ulti=1;
            if ( "ASCENSO MX" == "LIGA MX") {
                ulti = data.length - 1;
                ////alert("ulti: " + ulti);
            }
            if ( "ASCENSO MX" == "ASCENSO MX") {
                ulti = data.length - 1;
            }
            if ( "2" == "3") {
                ulti = data.length - 1;
            }
            if ( "2" == "4") {
                ulti = data.length - 1;
            }
            if ("2" == "5") {
                ulti = data.length - 1;
            }
            if ("2" == "6") {
                ulti = data.length - 1;
            }
            var htmlCoc="";
            if(data.length>0 && ulti<=data.length && data[ulti].cociente > 0 && parametros.actual.idTorneo!=47) {
                htmlCoc += '<img height="40" width="40" src="https://s3.amazonaws.com/lmxwebsite/docs/archdgtl/AfldDrct/logos/'+ data[ulti].idClub +'/'+ data[ulti].idClub+'.png" alt="'+data[ulti].nombreClub+'" class="img-responsivimg img-responsive">';
                htmlCoc += '<p><span class="titulo">Cociente</span><span>'+ data[ulti].cociente + '</span></p>';
            } else if(parametros.actual.idTorneo!=47) {
                htmlCoc += '<p><span class="titulo">Cociente</span></p>';
            }
            $('#secc_cociente').html(htmlCoc);
        });


        /*Consumir tablaFairPlay para FAIR-PLAY por AJAX*/
        // URL Produccion
        var urlFair = 'http://siidadmin.ligamx.net/webservices/prtl_web_jsondata.ashx?hsah=|a090b36be13c6b89f155d9c4aeea5b61c2f3172983397b6152f0f2b61b320bd232fcc1331fbaa4ff042243e4210f39d8195d28b6a166526b2890b410f17e2ce3|&psWidget=PRTL_FairPlay_new&objIdDivision=<IDDIVISION>&objIdTemporada=<IDTEMPORADA>&objIDTorneo=<IDTORNEO>';
        LMX.consultarServicio( urlFair, {"time":91}, function(data){
            var htmlFair="";
            if (data['DatosJSON'].length>0 && data['DatosJSON'][0].totalJJ>0 ){
                htmlFair += '<img height="40" width="40" src="https://s3.amazonaws.com/lmxwebsite/docs/archdgtl/AfldDrct/logos/'+data['DatosJSON'][0].idClub+'/'+data['DatosJSON'][0].idClub+'.png" alt="'+data['DatosJSON'][0].nombreClub+'" class="img-responsivimg img-responsive">';
                htmlFair += '<p><span class="titulo">Fair-play</span><span>Faltas</span></p>';
            }else {
                htmlFair += '<p><span class="titulo">Fair-play</span></p>';
            }
            $('#secc_fairplay').html(htmlFair);
        });


        /*Consumir minutosjugados para Participacion por AJAX*/
        // URL Produccion
        var urlParticipacion = 'http://siidadmin.ligamx.net/webservices/prtl_web_jsondata.ashx?hsah=|a090b36be13c6b89f155d9c4aeea5b61c2f3172983397b6152f0f2b61b320bd232fcc1331fbaa4ff042243e4210f39d8195d28b6a166526b2890b410f17e2ce3|&psWidget=PRTL_TtalJgdrActvxDvsn&objIDDivision=<IDDIVISION>&objIDTemporadas=<IDTEMPORADA>&objIDTorneo=<IDTORNEO>';
        LMX.consultarServicio( urlParticipacion, {
            IDDivision:'2'
            , IDTemporadas:'68'
            , IDTorneo:'2'
            , "time":3600}, function(data){
            var htmlPart="";
            htmlPart += '<span class="titulo">Registro</span>';
            if (data['DatosJSON'].length>0){
                data['DatosJSON'].forEach(function(datos, index) {
                    //alert(2);
                    if( Number(datos.IDDivision) == 2 && datos.idTorneo ==2 ){
                        htmlPart += '<span class ="big">' + datos.Total + '</span>';

                        if ("2" == "14") {
                            htmlPart += '<small>jugadoras</small>';
                        }
                        else {
                            htmlPart += '<small>Jugadores</small>';
                        }

                    }
                });
            }
            $('#secc_participacion').html(htmlPart);
        });
        var countmosaicolideres = 0;
        var url2 = "http://mxfeed.matchanalysis.com/player-ranking-data.jsp?statset=0-0-0-705&stat=completepasses";
        url2 = btoa(url2).replace('/','@');
        $.ajax({
            type: "GET", async: true, url:"ws/xml/"+url2,
            success: function(xml){
                if(xml['DatosJSON']['doc']!='') {
                    $.each(xml['DatosJSON']['doc']['ROWSET']['ROW'], function (key, val) {
                        var vnID = val['extpersonid'];
                        var vsNombre = val['name'];
                        var vnValor = val['value'];
                        if (countmosaicolideres == 0) {
                            //$('#DatoMosaicoEMA').html(vsNombre + '<br/> ' + vnValor + ' pases');
                        }
                        countmosaicolideres = 1;
                    });
                }
            }
        });

        var countmosaicoema = 0;
        var url2 = "http://mxfeed.matchanalysis.com/player-ranking-data.jsp?statset=0-0-0-705&stat=distance";

        url2 = btoa(url2).replace('/','@');
        $.ajax({
            type: "GET", async: true, url:"ws/xml/"+url2,
            success: function(xml){
                if(xml['DatosJSON']['doc']!='') {
                    $.each(xml['DatosJSON']['doc']['ROWSET']['0']['ROW'], function (key, val) {
                        var vnID = val['extpersonid'];
                        var vsNombre = val['name'];
                        var vnValor = val['value'];
                        vnValor = (vnValor / 1000).toFixed(2);
                        if (countmosaicoema == 0) {
                            $('#DatoMosaicoLideres').html(vsNombre + '<br/> ' + vnValor + ' Km');
                        }
                        countmosaicoema = 1;
                    });
                }
            }
        });

        //


    });
</script>





