            checkMenuSize = function() {
                if (horizontal_menu.is(':visible')) {
                    var block_menu_width = block_menu.innerWidth();

                    if (menu.innerWidth() > block_menu_width) {
                        decreaseMenuSize();
                    } else {
                        if (block_menu_width <= $(window).width() && old_width < $(window).width()) {
                            increaseMenuSize();
                        }
                    }
                    block_menu.css('visibility', 'visible');
                    old_width = $(window).width();
                }
            };
        checkMenuSize();

        $(window).on( "resize", function() {
            checkMenuSize();
        });
