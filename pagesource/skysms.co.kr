/************************************************************
* 받는사람 수 체크
************************************************************/
function checkDestination()
{
	var form	= document.msg_form;
	var regexp	= /\n/g;
	var lines	= form.destination.value.split(regexp);

	form.destination_count.value = lines.length;

	if(trim(form.destination.value) == "")
		form.destination_count.value = 0;
}

/*******************************************************************************
* 입력창 초기화
*******************************************************************************/
function initDestination(obj)
{
	if(obj.defaultValue == obj.value)
		obj.value = "";

	checkDestination();
}

/*******************************************************************************
* 짧은메세지 전송
*******************************************************************************/
function checkSend()
{
	var form = document.msg_form;

	var str_chk = form.body.value;
	str_chk = str_chk.replace(/\#/g,"＃");
	form.body.value = str_chk;

	var userid = "";

	if( !userid || userid == "" ){
		alert('로그인 후 이용하세요');
		return;
	}

	// 중복전송 방지
	if(form.check_attack.value == 0)
	{
		if(trim(form.body.value) == "" || form.body.value == "여기에 메세지를 입력하세요.")
		{
			alert("메세지를 입력하시기 바랍니다.");
			form.body.value = "";
			form.body.focus();
			return;
		}

		if(trim(form.destination.value) == "" || form.destination.value == form.destination.defaultValue)
		{
			alert("받는사람 전화번호를 입력하세요.");
			form.destination.focus();
			return;
		}

		if(trim(form.callback.value) == "")
		{
			alert("보내는사람 전화번호를 입력하시기 바랍니다.");
			form.callback.focus();
			return;
		}

		form.check_attack.value = 1;
		setTimeout("clearCheckAttack()", 3000);
		form.submit();
	}
	else
	{
		alert("이미 전송중에 있습니다.\n잠시만 기다려 주시기 바랍니다.");
		return;
	}
}							
function last_telnum_open()
{
	var url = "/send_s/again_send_telnum.php";
	window.open(url, "last_tel_num", "top=100, left=100, width=100, height=100, scrollbars=no, status=no,toolbars=no");
}


function clearCheckAttack()
{
    document.msg_form.check_attack.value = 0;
}

function lastTelnumToServer(top,left)
{
	//var params = "name="+encodeURIComponent(document.f.name.value);
	var params=null;
	var lastTelnumView = document.getElementById("lastTelnumView");
	lastTelnumView.style.display = "";
	lastTelnumView.style.top	= event.clientY - top;
	lastTelnumView.style.left	= event.clientX - left;


	sendRequest("/ajax/last_telnum.php", params, lastTelnumFromServer, "POST");
}

function lastTelnumFromServer()
{
	var lastTelnumView = document.getElementById("lastTelnumView");
	if (httpRequest.readyState == 4)
	{
		if (httpRequest.status == 200)
		{
			//alert("서버응답:"+httpRequest.responseText);
			lastTelnumView.innerHTML = httpRequest.responseText;

		}
	}
	else
	{
		lastTelnumView.innerHTML = "처리중..";
	}
}

function lastTelnumHide()
{
	var lastTelnumView = document.getElementById("lastTelnumView");
	lastTelnumView.style.display = "none";
}

function lastTelnumInsertAddr(hp)
{
	var msg_form2 = document.msg_form;

	if(msg_form2.destination.value == msg_form2.destination.defaultValue)
		msg_form2.destination.value = "";


	msg_form2.destination.value += hp + "\n";
	checkDestination();
	lastTelnumHide();
}

function lastTelnumDel(value)
{
	if(confirm('최근 보낸번호 : '+value +'\n삭제하시겠습니까?'))
	{
		var params="destination=" + encodeURIComponent(value);
		sendRequest("/ajax/last_telnum_del.php", params, lastTelnumFromServer, "POST");
		sendRequest("/ajax/last_telnum.php", params, lastTelnumFromServer, "POST");
	}
}

function clearDest()
{
	var form = document.msg_form;

	form.destination_count.value = 0;
	form.destination.value = "";
}


function mms_save()
{
	document.all.save_menu.style.top = 405;
	document.all.save_menu.style.left = 600;

	if(document.all.save_menu.style.display == "")
		document.all.save_menu.style.display = "none";
	else
	{
		document.all.save_menu.style.display = "";

		var form = document.msg_form;

		if(form.body.value != "" && form.body.value != form.body.defaultValue)
			document.all.save_menu_sms.disabled = false;
		else
			document.all.save_menu_sms.disabled = true;

	}
}

function open_savewin(obj_id)
{
	var url = "/send_lms/savemms_win.php?mode=" + obj_id;

	document.all.save_mms.src = url;
	document.all.save_mms.style.top = 300;
	document.all.save_mms.style.left = 430;
	document.all.save_mms.style.display = "";
}

function check_mms_menu(menu_name, evt)
{
	var obj = eval("document.all." + menu_name);

	if(obj.disabled == false)
	{
		if(evt == "over")
			obj.style.backgroundColor = "#DDDDDD";
		else
			obj.style.backgroundColor = "#EEEEEE";
	}
}

function my_mms_view()
{
	window.open("/send_lms/my_mms_box_lms.php?add_url=main","my_mms","top=50, left=50, width=820, height=600");
}

