* 예약일시 체크
*******************************************************************************/
function checkRDate()
{
	var form = document.msg_form;

	var year	= form.year.options[form.year.selectedIndex].value.toString();
	var month	= form.month.options[form.month.selectedIndex].value.toString();
	var day		= form.day.options[form.day.selectedIndex].value.toString();
	var hour	= form.hour.options[form.hour.selectedIndex].value.toString();
	var minute	= form.minute.options[form.minute.selectedIndex].value.toString();
	
	// 설정된 날짜
	if(month.length < 2) month = '0' + month;
	if(day.length < 2) day = '0' + day;
	if(hour.length < 2) hour = '0' + hour;
	if(minute.length < 2) minute = '0' + minute;
	
	var selected_date = year + month + day + hour + minute;
	
	if("201803250814" > selected_date)
	{
		alert("예약일시가 이전날짜 입니다. 예약일시를 정확하게 선택해 주시기 바랍니다.");
		initReserveDate();
	}
}
	
/*******************************************************************************
* 예약일시 초기화
*******************************************************************************/
function initReserveDate()
{
	var form = document.msg_form;
	
	var year	= 2018;
	var month	= 03;
	var day		= 25;
	var hour	= 08;
	var minute	= 14;

	makeReserveDay();

		form.year.value		= year;
		form.month.value	= month;
		form.day.value		= day;
		form.hour.value		= hour;
		form.minute.value	= minute;
}

/*******************************************************************************
* 일자 생성
*******************************************************************************/
function makeReserveDay()
{
	var form = document.msg_form;

	var year	= form.year.options[form.year.selectedIndex].value;
	var month	= form.month.options[form.month.selectedIndex].value;
	var obj_day	= form.day;

	var days	= month_days[month - 1];

	if(isLeap(year, month))
		days++;

	if(obj_day.length < days)
	{
		var gap = days - obj_day.length;

		for(var i = obj_day.length + 1; i <= days; i++)
		{
			var new_option = new Option(i, i);
			obj_day.options[i - 1] = new_option;
		}
	}
	else
	{
		for (var i = obj_day.length; i > days; i--)
			obj_day.options[i - 1] = null;
	}
}

/*******************************************************************************
* 윤달 체크
*******************************************************************************/
function isLeap(year, month)
{
	if(month == 2)
		if(((year % 4 == 0) && (year % 100 != 0)) || (year % 400 == 0))
			return true;
	else
		return false;
}

/*******************************************************************************
* 예약 일시 보임/숨김
*******************************************************************************/
function changeDivRserve(type)
{
	document.all.div_reserve.style.display = type;
}

