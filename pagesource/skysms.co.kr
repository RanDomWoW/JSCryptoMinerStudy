/*******************************************************************************
* 처음이용자 관련
*******************************************************************************/
function step1(){go_step(1);}
function step2(){go_step(2);}
function step3(){go_step(3);}
function step4(){go_step(4);}

function go_step(type)
{
	var url = "";

	if(type == 1)		url = "/step2.php";
	else if(type == 2)	url = "/step4.php";
	else if(type == 3)
	{
		url = "/address/grp_manage_form.php";
		location.href = url;
		return false;
	}
	else if(type == 4)	url = "/step1.php";

	if(type == 4)	window.open(url, "step", "top=100, left=100, width=400, height=400, scrollbars=yes");
	else 			window.open(url, "step", "top=100, left=100, width=400, height=400");
}

/*******************************************************************************
* 스크롤 관련
*******************************************************************************/
function change_scroll_pixel(obj_name, pixel)
{
	eval(obj_name + "_scroll_pixel = " + pixel);
}

function scroll(obj_name, idx1, idx2)
{
	var obj1 = eval(obj_name + "_" + idx1);
	var obj2 = eval(obj_name + "_" + idx2);

	var scroll_pixel = eval(obj_name + "_scroll_pixel");
	var height = eval(obj_name + "_height");

	obj1.style.pixelTop -= scroll_pixel;

	if(obj1.style.pixelTop < -height)
	{
		obj1.style.pixelTop = height;

		idx1++;
		idx2++;

		setTimeout("start_scroll_pixel('" + obj_name + "', " + idx1 + ")", eval(obj_name + "_wait_time"));
	}
	else
	{
		obj2.style.pixelTop -= scroll_pixel;
		setTimeout("scroll('" + obj_name + "', " + idx1 + ", " + idx2 + ")", eval(obj_name + "_scroll_speed"));
	}
}

function start_scroll_pixel(obj_name, idx1)
{
	var obj = eval(obj_name);
	var idx2;

	if(idx1 >= obj.length)	idx1 = 0;
	idx2 = idx1 + 1;

	if(idx2 >= obj.length)	idx2 = 0;

	scroll(obj_name, idx1, idx2);
}

/*******************************************************************************
* 컨텐츠 출력
*
* obj_name	- array 명
* w			- 컨텐츠 폭
* h			- 컨텐츠 높이
* s			- 스크롤 간격
* wt		- 정지시간
*******************************************************************************/
function init_contents(obj_name, w, h)
{
	var scroll_pixel	= eval(obj_name + "_scroll_pixel");
	var wait_time		= eval(obj_name + "_wait_time");

	var obj = eval(obj_name);
	var length = obj.length;

	for (var i = 0; i < length; i++)
	{
		if(i == 0)
			document.write("<div id='" + obj_name + "_" + i + "' style='position:absolute; top:0; width:" + w + "; height:" + h + ";' onmouseover='change_scroll_pixel(\"" + obj_name + "\", 0)' onmouseout='change_scroll_pixel(\"" + obj_name + "\", " + scroll_pixel + ")'>" + obj[i] + "</div><BR>");
		else
			document.write("<div id='" + obj_name + "_" + i + "' style='position:absolute; top:" + h + "; width:" + w + "; height:" + h + ";' onmouseover='change_scroll_pixel(\"" + obj_name + "\", 0)' onmouseout='change_scroll_pixel(\"" + obj_name + "\", " + scroll_pixel + ")'>" + obj[i] + "</div><BR>");
	}

	setTimeout("start_scroll_pixel('" + obj_name + "', 0)", wait_time);
}

/******************************************************************************/
var XMLHTTP_OBJS = new Array("Microsoft.XMLHTTP", "Msxml2.XMLHTTP.5.0", "Msxml2.XMLHTTP.4.0", "MSXML2.XMLHTTP.3.0", "MSXML2.XMLHTTP");
var	xmlHttp = null;

function get_emoticon(type, grp)
{

	if(xmlHttp == null)
	{
		for(var i = 0; i < XMLHTTP_OBJS.length; i++)
		{
			try
			{
				xmlHttp = new ActiveXObject(XMLHTTP_OBJS[i]);
				break;
			}
			catch(e)
			{
				xmlHttp = null;
			}
		}
	}

	if(xmlHttp == null)
	{
		location.href = "/send_s/?grp=" + grp;
		return;
	}

	xmlHttp.open("GET", "/main_html3/_get_emoticon.php?type=" + type + "&grp=" + grp, true);
	xmlHttp.onreadystatechange = function ()
	{
		if(xmlHttp.readyState == 4)
		{
			//alert(xmlHttp.status);
			if(xmlHttp.status == 200)
				init_emoticon(type, grp);
			else
				location.href = "/send_s/?grp=" + grp;
		}
	}
    xmlHttp.send(null);
}

function init_emoticon(type, grp)
{
	var items = xmlHttp.responseXML.getElementsByTagName("item");
	var items_length = items.length;

	var seq, body;

	if(items_length == 4)
	{
		for(var i = 0; i < items_length; i++)
		{
			var item = items[i];

			seq		= item.getElementsByTagName("seq")[0].firstChild.nodeValue;
			body	= item.getElementsByTagName("body")[0].firstChild.nodeValue;


			eval("document.all." + type + "_body_" + i).innerHTML = "<textarea cols=\"16\" rows=\"5\" readonly class=\"screen_emoticon\" onclick=\"selectEmoticonMain(" + seq + ", this)\">" + body + "</textarea>";

		}
	}
	else
		location.href = "/send_s/?grp=" + grp;
}

/******************************************************************************/

var cid = true;
var txt_scroll_delay = 300;


function init_txt_scroll(idx)
{
	clearInterval(cid);
	cid = true;

	eval("document.all.msg_" + idx).scrollTop = 0;
}

function txt_scroll(idx)
{
	eval("document.all.msg_" + idx).scrollTop += 5;

	if(cid == false)
		cid = setInterval("txt_scroll(" + idx + ")", txt_scroll_delay);
}


//-->
</SCRIPT>

