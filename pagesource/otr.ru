	<script>$(document).ready(function(){
		
		function applySidebarSubnavigation() {
			var currentPopover = null;
			$.each($('.b-sidebar-menu .nav-main ul li'), function(i, o){
				if ($(o).find('.b-subnav-list').length > 0) {
					$(o).popover({
			    		template: '<div class="popover b-sidebar-subnav"><div class="arrow"></div><div class="popover-inner"><h2 class="popover-title"></h2><div class="popover-content"><p></p></div></div></div>',
			    		trigger: 'manual',
			    		html : true,
			    		placement: "right",
			    		container: 'body',
			    		title: function() {
			    			return $(o).data('title');
			    		},
			    		content: function() {
			    			return $(o).find('.b-subnav-list').html();
			    		}
			    	});
				}
				$(o).find('a.nav-item').on("click", function () {
					if ($(o).find('.b-subnav-list').length <= 0 || null != currentPopover) {
						return true;
					}
					return showSubnav(o);
				});
				$(o).on("mouseenter", function () {
					return showSubnav(o);
				});
				$('.b-overlay').on("click", function () {
					$('.b-overlay').fadeOut();
					currentPopover.hide();
					currentPopover = null;
	        		$(o).removeClass('active');
					return false;
				});
				
				function showSubnav(o) {
					$('.b-sidebar-menu .nav-main ul li').removeClass('active');
					$('.b-sidebar-subnav').hide();
					if ($(o).find('.b-subnav-list').length > 0) {
						$('.b-overlay').fadeIn();
						$(o).addClass('active');
			        	$(o).popover('show');
			        	$('.b-sidebar-subnav .arrow').css('top', ($(o).offset().top - $(window).scrollTop() + $(o).outerHeight() / 2)  + 'px');
			        	$(".popover").on("mouseleave", function () {
							$('.b-overlay').fadeOut();
			        		$(o).popover('hide');
			        		currentPopover = null;
			        		$(o).removeClass('active');
			        	});
			        	currentPopover = $('.b-sidebar-subnav:visible').first();
			        	return false;
					} else {
						$('.b-overlay').fadeOut();
					}
		        	return true;
				}
			});
		}
	});
