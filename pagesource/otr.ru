	<script>
		var isMoving = false;
		
		function adjustWindowSize() {
			 var footerHeight = jQuery('footer').height();
			    var actionPanelHeight = jQuery('#action-panel').height();
				var stickFooterPush = jQuery('.push').height(footerHeight + actionPanelHeight);	
				jQuery('.page-wrap').css({'marginBottom':'-' + (footerHeight + actionPanelHeight) + 'px'});
				$('[adjust-height]').each(function() {
					var totalHeight = 0;

					$(this).children().each(function(){
					    totalHeight = totalHeight + $(this).outerHeight(true);
					});
					if (totalHeight < ($(window).height() - footerHeight - actionPanelHeight)) {
						$(this).css('min-height', ($(window).height() - footerHeight - actionPanelHeight) + 'px');
					}
				});
		}
		
		function showSearchPanel() {
			if ($('.b-floating-search').css('left') == '85px' || isMoving) {
				return;
			}
			
			if ($('.b-floating-search').css('display') == 'none') {
				$('.b-floating-search').show();
			}
			
			isMoving = true;
			$('.b-floating-search').stop().animate({
	            left: '90px'
	        }, 'slow',function() {
	        	$(this).css('z-index', '1000');
	        	$(this).animate({left: '85px'}, 150, function(){
	        		isMoving = false;
	        		$('.b-floating-search').show();
	        	});
	        });			
		}
		
		jQuery(window).resize(function(){
			adjustWindowSize();
		});
