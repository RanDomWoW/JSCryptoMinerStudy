                    $('.body').css({"margin-left":(width-170)+"px", "position":"relative"});
                } else {
                    $('#right_fixed_bunner').css({"right":(width/2-170)+"px"});
                    $('.body').css({"margin-left":"auto"});
                }
                $('#right_fixed_bunner').show();
            }
        }
    </script>
    <style>
        #right_fixed_bunner {
            position: fixed;
            right:0px;
            top:0px;
        }
    </style>
</div>
</body>
</html>