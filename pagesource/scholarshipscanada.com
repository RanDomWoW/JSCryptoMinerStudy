    <script type="text/javascript">
        function getCookieonIndex(contest) {
            var SCCanada = contest + "=";
            var ca = document.cookie.split(';');
            
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(SCCanada) == 0) {
                    return c.substring(SCCanada.length, c.length);
                }

            }
            return "";
        }

        function checkCookieonIndex() {
            var link = document.getElementById("canada150-contest").text;
            
            var contestIndex = getCookieonIndex("SCCanada");
            
            if (contestIndex == link) {
                document.getElementById("pnlCanada150").style.display = "none";
                //return false;
            } else {
                document.getElementById("pnlCanada150").style.display = "block";
                //return true;
            }

        }

        function setCookie(contest, cvalue, exdays) {
            var link = document.getElementById("canada150-contest").text;

            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = contest + "=" + link + "; " + expires;
            
        }

        $(document).ready(function ($) {
            $(".Canada150-Close").bind("click", function () {
                $(".Page-Canada-150").hide();
            });
        })
    </script>
