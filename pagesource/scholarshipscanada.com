<script lang="javascript">
    function setCookie(vote, cvalue, exdays) {

        var anyItemselected;

        var link = document.getElementById("PollingDataControl1_hypPollName");

        //Init var
        anyItemselected = false;

        var RB1 = document.getElementById("PollingDataControl1_rbVotingOptionslist");
        var listItems = RB1.getElementsByTagName("input");

        for (var i = 0; i < listItems.length; i++) {
            if (listItems[i].checked) {
                anyItemselected = true;
            }
        }

        //If any option was selected then set the cookie
        if (anyItemselected == true) {

            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = vote + "=" + link.innerText + "; " + expires;
        }
    }

    function getCookie(vote) {
        var SCPolling = vote + "=";
        var ca = document.cookie.split(';');

        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(SCPolling) == 0) {
                return c.substring(SCPolling.length, c.length);
            }

        }
        return "";
    }

    function checkCookie() {

        var link = document.getElementById("PollingDataControl1_hypPollName");
        
        var vote = getCookie("SCPolling");
        if (vote == link.innerText) {
            alert("You have already voted!");
            return false;
        } else {
            setCookie("SCPolling", "1", 1);
            return true;
        }

    }
</script>
