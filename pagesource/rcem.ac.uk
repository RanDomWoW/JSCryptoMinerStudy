            <script type="text/javascript">
                //<![CDATA[
                    // Executes when a query is clicked in the pop-up menu.    
                    // Add or replace the selected query key in the url query string, and reload the page.
                    function QueryClickedctl01_TemplateBody_WebPartManager1_gwpciNewQueryMenuCommon_ciNewQueryMenuCommon(key)
                    {
                        var newlocation = "";
                        var hash = window.location.hash; //need to check for anchors/hashtags and handle them carefully
                        if (window.location.search.length == 0)
                        {
                            newlocation = window.location + "?" + "QueryMenuSelectedKeyctl01_TemplateBody_WebPartManager1_gwpciNewQueryMenuCommon_ciNewQueryMenuCommon" + "=" + key;
                        }
                        else
                        {
                            // Add the key to the query string, or replace it if it already exists in the query string
                            var queryString = window.location.search.substring(1);
                            var paramPairs = queryString.split("&");
                            var newQueryString = "";
                            var isFound = new Boolean(false);
                            for (var i=0;i<paramPairs.length;i++)
                            {
                                if (newQueryString.length > 0)
                                    newQueryString += "&";
                                var pair = paramPairs[i].split("=");
                                if (pair[0] == "QueryMenuSelectedKeyctl01_TemplateBody_WebPartManager1_gwpciNewQueryMenuCommon_ciNewQueryMenuCommon")
                                {
                                    newQueryString += "QueryMenuSelectedKeyctl01_TemplateBody_WebPartManager1_gwpciNewQueryMenuCommon_ciNewQueryMenuCommon" + "=" + key;
                                    isFound = true;
                                }
                                else
                                {
                                    newQueryString += paramPairs[i];
                                }
                            }
                            if (isFound == false){
                                // Param wasn't in query string, add it.
                                if(hash.length == 0)
                                {
                                    newlocation = window.location + "&" + "QueryMenuSelectedKeyctl01_TemplateBody_WebPartManager1_gwpciNewQueryMenuCommon_ciNewQueryMenuCommon" + "=" + key;
                                }
                                else{
                                    var url = window.location.href.substring(0, window.location.href.indexOf("?"));
                                    newlocation = url + "?" + newQueryString + "&" + "QueryMenuSelectedKeyctl01_TemplateBody_WebPartManager1_gwpciNewQueryMenuCommon_ciNewQueryMenuCommon" + "=" + key + hash;
                                }
                            }
                            else
                            {
                                var url = window.location.href.substring(0, window.location.href.indexOf("?"));
                                newlocation = url + "?" + newQueryString + hash;
                            }                                                      
                        }
                        window.location = newlocation;
                    }
                //]]>
            </script>
                
