<script>
  var kraut = window.kraut || {};
  kraut.imgixErrorHandler = {
    development: function() {
      var imgClone = this.cloneNode(true);
      imgClone.removeAttribute("srcset");
      imgClone.setAttribute("src", imgClone.getAttribute("ix-path"));
      this.parentNode.replaceChild(imgClone, this);
      return true;
    },
    staging: function() {
      if(!this.hasAttribute("errorHandlerExecuted")) {
        ["src", "srcSet"].forEach(function(attr) {
          this.setAttribute(attr, this.getAttribute(attr).replace(/staging-krautreporter.imgix.net/g, "krautreporter.imgix.net"));
        }.bind(this));
        this.setAttribute("errorHandlerExecuted", "true");
      }
      return true;
    }
  };
  
  kraut.initImgix = function() {
    imgix.config.host = 'krautreporter.imgix.net';
    // IE 11 does not support srcset and relies on src.
    // Therefore we need to explicitly set the `w` attribute for each imgix-image
    // so that IE 11 can load an appropriate image version.
    var isIE11 = !!navigator.userAgent.match(/Trident\/7\./);
    if(isIE11) {
      if(window.ENV.environment === "development") {
        console.log("Applying IE11 hack for imgix loading");
      }
      var imgTags = Array.from(document.querySelectorAll("img[ix-params]"))
      imgTags.forEach(function(imgTag) {
        var ixParams = '{"fit":"crop","auto":"format","q":"40","w":"' + imgTag.width + '"}';
        imgTag.setAttribute("ix-params", ixParams);
        imgTag.setAttribute("width", imgTag.width);
      });
    }
  
    imgix.config.includeLibraryParam = false;
    imgix.init({
      srcAttribute: "data-src",
      srcsetAttribute: "data-srcset",
      sizesAttribute: "data-ignored-sizes",
    });
  
    var imgTags = Array.from(document.querySelectorAll("[ix-initialized=ix-initialized]:not(.lazyloaded)"))
    imgTags.forEach(function(imgTag) {
      imgTag.classList.add("lazyload");
      imgTag.classList.remove("kr-lazysizes-placeholder");
  
      if(window.ENV && kraut.imgixErrorHandler[window.ENV.environment]) {
        imgTag.onerror = kraut.imgixErrorHandler[window.ENV.environment];
      }
    });
  };
  document.addEventListener("DOMContentLoaded", kraut.initImgix);
</script>

