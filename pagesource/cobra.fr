<script type="text/javascript">
    
    var amlabel_selector = '.amlabel-selector, .amlabel-div';

    (function($) {
        $(document).ready(function(){
                var productids = [];
                var reg = /[0-9]+$/;

                $('[id^=product-id-]').each(function (key, value) {
                    var amlabel_mode = 'category';
                    var idProductDiv = value.id;
                    var match = reg.exec(idProductDiv);

                    if ($(value).hasClass('amlabel-mode-product')) {
                        amlabel_mode = 'product';
                    }
                    if (match.length) {
                        // productid including mode allows one page to handle both
                        // of category and product mode for differents <div>
                        productids.push([match[0], amlabel_mode]);
                    }
                });

                var postData = {
                    "productids" : productids
                };

                $.ajax({
                    "url" : "http://www.cobra.fr/amlabelajax/ajax/getlabels/",
                    "data" : postData,
                    "method" : "POST",
                    "dataType" : "json",
                    "success" : function (result) {
                        for (var id in result) {
                            amlabel_product_ids[id] = result[id];
                        }
                        amlabel_init();
                    }
                });
        });
    })(jQuery);

    
