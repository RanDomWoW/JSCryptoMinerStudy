function fixcomfychair() {

	// BLF - comfychair Image / Link fixer
	// description:  Fix comfychair's images on BLF
	// author:       wight

	// Quick hack using code grabbed from Robusto on stackexchange - 
	// http://stackoverflow.com/questions/3232992/rewrite-parts-of-a-url-in-greasemonkey-and-firefox/3233046#3233046
	// as well as from Spencer May to get the code running at the appropriate time (after the DOM is loaded)
	// http://stackoverflow.com/questions/4842590/run-function-when-page-is-loaded/25984032#25984032

	var images = document.getElementsByTagName("img"); //array
	for (var i=0,imax=images.length; i<imax; i++) {
	   images[i].src = images[i].src.replace("75.65.123.78/","photo.jesusthepirate.com/blf/comfychair/");
	}

	var links = document.getElementsByTagName("a"); //array
	for (var i=0,imax=links.length; i<imax; i++) {
	   links[i].src = links[i].href.replace("75.65.123.78/","photo.jesusthepirate.com/blf/comfychair/");
	}		
}

document.addEventListener('DOMContentLoaded', fixcomfychair, false);

