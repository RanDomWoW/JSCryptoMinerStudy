        var errorSpan = $sns_jq('#topLinkLoginForm span.error');
        $sns_jq('#topLinkLoginForm').ajaxForm(function(result) {
          if (!result) {
            errorSpan.text('Login Error');
          } else {
            if (result.success) {
              $sns_jq('.sns-quickaccess li.login').html(result.data.replaceHtml);

              if (window.location.pathname === '/' ||
                  window.location.pathname === '/index.php/') {
                window.location.reload();
              } else {
                if (window._MB && window._MB.setIsLoggedIn) {
                  window._MB.setIsLoggedIn();
                }
                $sns_jq('#modal_login').modal('hide');
              }
            } else {
              if (result.error === 'bad request') {
                window.location.reload();
              }
              errorSpan.text(result.error);
            }
          }
        });
      });

    </script>
