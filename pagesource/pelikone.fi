    <script>
        getCurrentUser(function(user) { // success
            $('.userbox').addClass('logged-in').append(
                $('<div/>', {'class': 'userbox-username nav-drop'}).append(
                    $('<a/>', {'href': user.url}).append(
						$('<img>', {'src': user.avatar_small_url, 'alt': ''}),
						$('<span/>').text(user.name)
					),
                    $('<ul/>').append(
                        $('<li/>').append(
                            $('<a/>', {'href': user.url}).text('Profiili')
                        ),
                        $('<li/>').append(
                            $('<a/>', {'href': '/pelilistat/' + user.name + '/suosikit'}).text('Suosikit')
                        ),
                        $('<li/>').append(
                            $('<a/>', {'href': '/saavutukset'}).text('Saavutukset')
                        ),
                        $('<li/>').append(
                            $('<a/>', {'href': '/pelilistat/' + user.name}).text('Omat pelilistat')
                        ),
                        $('<li/>').append(
                            $('<a/>', {'href': 'http://pelikone.fi/ihmiset/' + user.name + '/kaverit/online'}).text('Kaverit')
                        ),
                        $('<li/>').append(
                            $('<a/>', {'href': 'http://pelikone.fi/ihmiset/' + user.name + '/yksityisviestit'}).text('Viestit')
                        ),
                        $('<li/>').append(
                            $('<a/>', {'href': '/pankki'}).text('Pankki')
                        ),
                        $('<li/>', {'class': 'userbox-logout'}).append(
                            $('<a/>', {'href': '/logout'}).text('Kirjaudu ulos').click(function() { return logoutpost(); })
                        )
                    )
                ),
                $('<div/>', {'id': 'news', 'class': 'news'}).append(
                    $('<a/>', {'class': 'news-icon', 'href': ''})
                )
            ); // userbox append
            if (window.location.href == user.url) {
                $('.nav-drop.selected').removeClass('selected');
                $('.userbox-username.nav-drop').addClass('selected');
            }
            $.getJSON('/ajax/user-notifications', {limit: 5, page: 1}, function(result) {
                var count = result.notifications.length, news;
                news = $('<ul/>');
                if (count === 0) {
                    news.append(
                        $('<li/>').append(
                            $('<p/>', {'class': 'news-item-link'}).html('Ei viestejä'),
                            $('<a/>', {'class': 'news-close', 'href': ''}).html('&times;')
                        )
                    );
                }
                $.each(result.notifications, function() {
                    news.append(
                        $('<li/>').append(
                            $('<a/>', {'class': 'news-item-link', 'href': this.url}).append(
                                $('<img>', {'src': this.image, 'alt': ''}),
                                $('<p/>').html(this.body)
                            ),
                            $('<a/>', {'class': 'news-close', 'href': ''}).html('&times;')
                        )
                    );
                });
                var $newsList = $('<div/>', {'class': 'news-list news-flash'}).append(news);
                if (count > 0) {
                    $newsList.append($('<a/>', {'class': 'news-show-all', 'href': user.url + '/ilmoitukset', 'text': 'Näytä kaikki'}));
                }
                $('#news').append($newsList);
                if (result.unread > 0) {
                    $('.news-icon').append(
                        $('<span/>', {'class': 'news-count'}).text(result.unread)
                    ).addClass('news-icon-hilite');
                    $('.news-count').fadeIn();
                }
                $('.news-icon').live('click', function(e) {
                    $('#news .news-list').removeClass('news-flash').toggle();
                    $(this).toggleClass('news-icon-active');
                    $('#news .news-list li').show();
                    if (result.unread > 0) {
                        $('.news-count').fadeOut();
                        $.post('/ajax/user-notifications');
                    }
                    e.preventDefault();
                });
                $('.news-close').live('click', function(e) {
                    $(this).closest('li').hide();
                    $('.news-flash').hide().removeClass('news-flash');
                    e.preventDefault();
                });
/*                else {
                    $('.news-icon').live('click', function(e) {
                        e.preventDefault();
                    });
                    $('.news-icon').addClass("news-icon-faded");
                }*/
            }); // user-notifications
        }, function() { // error
            $('.userbox').addClass('logged-out').append(
                $('<div/>', {'class': 'userbox-join'}).append(
                    $('<a/>', {'class': 'userbox-join-a kiss-header-join', 'href': '/liity'}).text('Liity nyt')
                ),
                $('<div/>', {'class': 'userbox-login'}).append(
                    $('<a/>', {'href': ''}).text('Kirjaudu sisään'),
                    $('<div/>', {'class': 'userbox-login-options'}).append(
                        $('<a/>', {'class': 'userbox-login-option userbox-login-fb', 'href': '/facebook/login'}).html('Kirjaudu <strong>Facebook</strong>-tunnuksella'),
                        $('<a/>', {'class': 'userbox-login-option userbox-login-pk', 'href': ''}).html('Kirjaudu <strong>Pelikone</strong>-tunnuksella'),
                        $('<form/>', {'class': 'userbox-form', 'method': 'post', 'action': 'https://pelikone.fi/login', 'onsubmit': 'return loginpost()'}).append(
                                                        $('<ul/>').append(
                                $('<li/>').append(
                                    $('<label/>', {'for': 'user-login-name'}).text('Käyttäjänimesi'),
                                    $('<br>'),
                                    $('<input/>', {'class': 'input-text', 'id': 'user-login-name', 'name': 'user_login', 'type': 'text'})
                                ),
                                $('<li/>').append(
                                    $('<label/>', {'for': 'user-login-pwd'}).text('Salasana ').append(
                                        $('<span/>', {'class': 'userbox-login-forgot'}).text('Unohtuiko salasana?').click(function() { location.href="/content/forgot"; })
                                    ),
                                    $('<br>'),
                                    $('<input/>', {'class': 'input-text', 'id': 'user-login-pwd', 'name': 'user_password', 'type': 'password'})
                                ),
                                $('<li/>', {'class': 'last'}).append(
                                    $('<button/>', {'type': 'submit'}).text('Kirjaudu'),
                                    $('<div/>', {'class': 'remember-me'}).append(
                                        $('<input/>', {'name': 'user_stickylogin', 'type': 'checkbox', 'checked': 'checked'}),
                                        $('<label/>').text('Muista minut ')
                                    )
                                )
                            )
                        )
                    )
                )
            );
        });
