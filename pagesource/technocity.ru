<script id="basket-specitems-sections-table" type="text/x-jquery-tmpl"> 
{{each specSections}}
	<table cellspacing="0" cellpadding="0" border="0" class="basket" data-section="${ID}"><tbody>
		{{if NAME}}
			<tr style="height: 23px;">
				<td class="prev-image"></td>
				<td colspan="2">
					${NAME}:
				</td>
			</tr>
		{{/if}}
		{{if RADIO_NONE}}
			<tr style="height: 23px;">
				<td class="prev-image"></td>
				<td colspan="2">
					<div class="field"><label><input type="radio" name="SPEC_ITEMS" value="0" checked="checked">${RADIO_NONE}</label></div>
				</td>
			</tr>
	    {{/if}}

	    {{if RADIO_NONE}}
			{{each specItems}} 
			<tr class="{{if FILTER}}filter ${FILTER}{{else}}filter-any{{/if}}">
				<td class="prev-image"></td>
				<td class="info">
					<div class="field"><label><input type="radio" name="SPEC_ITEMS" value="${ID}" {{if IN_BASKET}}checked="checked"{{/if}}>${NAME}</label></div>
				</td>
				<td class="price"><div class="price"><span class="number">${$item.formatPrice(PRICE)}</span> руб.</div></td>
			</tr>
			{{/each}}
	    {{else}}
			{{each specItems}} 
			<tr class="{{if FILTER}}filter ${FILTER}{{else}}filter-any{{/if}}">
				<td class="prev-image"></td>
				<td class="info">
					<div class="field"><label class="checkbox"><input type="checkbox" name="ADD_ITEMS_QUANTITY_${ID}" value="1" data-id="${ID}"{{if IN_BASKET}} checked="checked"{{/if}}> ${NAME}</label></div>
				</td>
				<td class="price"><div class="price"><span class="number">${$item.formatPrice(PRICE)}</span> руб.</div>{{if OLD_PRICE}}<span style="text-decoration:line-through">${$item.formatPrice(OLD_PRICE)} руб.</span>{{/if}}</td>
			</tr>
			{{/each}}
		{{/if}}
	</tbody></table>
{{/each}}
</script>

<div class="popup-centered-overlay">
	<div class="balloon balloon-callme popup-centered"><b class="ar3"></b><span class="balloon-close" title="Закрыть"></span>
		<div id="callme-res">
			<form>
				<p>Продавец перезвонит вам на указанный номер:</p>
				<p class="input">
					+7<input class="textInput" id="callme-phone" value="" style="width: 270px; margin-left: 6px;">
					<span id="phone-error" style="display: none; margin-left: 20px;">пожалуйста, введите 10 цифр телефона</span>
				</p>
								
								<p class="submit"><button class="button button-on-grey" type="submit">Перезвоните мне!</button></p>
                <p class="asterisk">Нажимая кнопку "Перезвоните мне!" я подтверждаю своё согласие на <a href="/politics/">обработку персональных данных</a>.</p>			</form>
		</div>
		<script>
			$(function() {
								
				$("body").on('click', '.handler-callme', function(){
					var b = $(".balloon-callme");
					var c = b.parent();
					if (b.is(':visible')) {
						c.fadeOut(200);
						$(document).unbind('keyup');
					} else {
						c.fadeIn(200);
						b.find('input').focus();
						$(document).keyup(function(e){
							if (e.which == 27) {
								c.fadeOut(200);
								$(document).unbind('keyup');
								return false;
							};
						});
					};
					return false;
				});

				$("body").on("click", '.popup-centered .balloon-close', function() {
					$(".popup-centered-overlay").fadeOut(200);
				});

				$("body").on("click", '.popup-centered-overlay', function(event) {
					if ($(event.target).closest($(this).find('.popup-centered')).length == 0) {
						$(this).fadeOut(200);
					}
				});

				$("body").on("change keyup input click", '#callme-phone', function() {
					if (this.value.match(/[^0-9]/g)) {
						this.value = this.value.replace(/[^0-9]/g, '');
					}
				});
				
				$("body").on('submit', '.balloon-callme form', function(){
					if ($('#callme-phone').val().length == 10){
					    yaCounter2326996.reachGoal('goal-callme');
					    ga('send', 'event', 'goal', 'goal-callme');
					    $.post('/ajax/callme.php', {
					        phone: $('#callme-phone').val(),
					        time: $('#callme-time').val()
					    }, function(data) {
					        $('#callme-res').html(data);
					    });
					}else{
					    $('#callme-res').addClass('error');
					    $('#phone-error').slideDown();
					    $('#callme-phone').focus();
					}
					return false;
				});

				if (location.hash.substring(1) == 'callme') {
					$('a.handler-callme').click();
				}
			});
		</script>
	</div>
</div>
<div id="callme_widget">
	<a class="callme handler-callme callme-green"></a>
</div>
	<div class="main-announces">
	</div><script type="text/javascript">
	$(function() {
			});
</script></body>
</html>