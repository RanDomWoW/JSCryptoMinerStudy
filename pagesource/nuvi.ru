<script type="text/javascript">
    $(document).ready(function() {
        var listWidth = $("#carousel-items > div").size() * 170;
        $("#carousel-items").css({width: listWidth});

        $("#carousel .next, #carousel .prev").click(function() {
            var scrollValue = $("#carousel .wrap").scrollLeft();
            var maxScrollValue = listWidth - 4 * 170;
            var clickValue = $(this).hasClass('next') ? 170 : -170;

            if(scrollValue + clickValue > maxScrollValue) scrollValue = -170;
            if(scrollValue + clickValue < 0) scrollValue = maxScrollValue;
            else scrollValue += clickValue;

            $("#carousel .wrap").animate({scrollLeft: scrollValue});

            return false;
        });

        //slideShow();
    });
</script>
