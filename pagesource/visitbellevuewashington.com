				    var slideshowBefore = function(e,i) {

				    	if (i > (this.getSize()-1)) {
					    	var elm = '#slideshow .slide-0';

					    	$(elm).fadeTo(1000, 1);
					    	fadeSlideOut((this.getSize()-1));
						} else if (i < 0){
							var elm = '#slideshow .slide-' + (this.getSize()-1);

					    	$(elm).fadeTo(1000, 1);
					    	fadeSlideOut(0);
						} else {
					    	var elm = '#slideshow .slide-' + i;
					    	var lastelm = '#slideshow .slide-' + (i-1);
					    	//var content = '<h3>' + $(elm).attr('title') + '</h3>' + $(elm).attr('data-desc');

					    	$(elm).fadeTo(1000, 1);
					    	fadeSlideOut(i-1);
						}

						var title = $(elm).attr('title');
				    	var content = $(elm).attr('data-desc');
						var link = $(elm).attr('data-url');
						var linktxt = $(elm).attr('data-linkTxt');
						if (link != 'undefined' && linktxt != 'undefined')
							content = content + ' <a href="' + link + '">' + linktxt + '</a>';

						content = content;
				    	$('#slideshow-caption .caption-content').html(content);
						$('#slideshow-caption .caption-title').html(title);
						// preload the NEXT slide? just uncomment this little gem
						updateSlideSrc(i+1);
				    }
				    
				    var fadeSlideOut = function(n) {
				    	$('#slideshow .slide-'+n).fadeTo(1000, 0.3);
					}

				    var updateSlideSrc = function(n) {
				    	var elm = $('#slideshow .slide-'+n);
				    	var src = $(elm).attr('src');
				   		var dsrc = $(elm).attr('data-src');

				   		fadeSlideOut(n);

				   		if (src != dsrc) {
				    		$(elm).attr('src',dsrc);
				    	}
				    }

				    var addExtraClones = function() {
