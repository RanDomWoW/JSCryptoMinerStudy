  <script type='text/javascript'>
    // AJAX Call to get the user's information
    $(function() {
      // Call API
      $.get('/api/wpmenu').success(function(data) {
        // Log API Response
        console.log(data)
        // If the user is logged in show menu
        if (data.session) {
          $('a.subscribe-link').each(function() {
            $(this).attr('href', '/users/subscriptions');
          });
          // Add API data to the HTML div.
          $('#header-user-menu-email').text(data.email);
		  $('#header-user-menu-email-mobile').text(data.email);
          $('#header-user-menu-credits').text(data.credits);
		  $('#header-user-menu-credits-mobile').text(data.credits);
          $('#header-user-menu-plan').text(data.plan);
		  $('#header-user-menu-plan-mobile').text(data.plan);
          $('#header-user-menu').show();
		  $('#header-user-menu-mobile').show();
		  $('#masthead').addClass(data.plan);
        } else {
          // IF the user is not logged in show the login link
          $('#header-login-link').show();
		  $('#header-login-link-mobile').show()
        };
      });
    });
  </script>
