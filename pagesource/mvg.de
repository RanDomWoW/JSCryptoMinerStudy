<div class="map-slider" style="display:none;">
	<div id="haltestellenMap" data-eval="initMapOnDemand()">
    <div id="haltestellenMapInternal" class="haltestellenMap"></div>
<script type="text/javascript">
var _selectedPosition;
function initClick(){
  OpenLayers.Control.Click = OpenLayers.Class(OpenLayers.Control, {                
    defaultHandlerOptions: {
        'single': true,
        'double': false,
        'pixelTolerance': 3,
        'stopSingle': false,
        'stopDouble': false
    },

    initialize: function(options) {
        this.handlerOptions = OpenLayers.Util.extend(
            {}, this.defaultHandlerOptions
        );
        OpenLayers.Control.prototype.initialize.apply(
            this, arguments
        ); 
        this.handler = new OpenLayers.Handler.Click(
            this, {
                'click': this.trigger
            }, this.handlerOptions
        );
    }, 
    trigger: function(evt) {
    	if (_eventTime){
    		var diff = new Date().getTime() - _eventTime;
    		if (diff < 500){
    			//console.log(diff);
    			return;
    		}
    	}
        var lonlat = _map.getLonLatFromPixel(evt.xy);
        _selectedPosition = lonlat;
    	var content="<div id='mapPopupLabel'>Position übernehmen?</div><div class='mapPopupOk'>OK</div>";
    	popup = new OpenLayers.Popup.FramedCloud("clickPopup", lonlat,
    			   new OpenLayers.Size(100,50),
    			   content, null, false);
    	_map.addPopup(popup, true);
    	$('#clickPopup_contentDiv').click(selectPositon);
        //console.log("You clicked near " + lonlat.lat + " N, " + lonlat.lon + " E");
    }
  });
}
var _map;
var _haltestellen;
var _click;
var _lon;
var _lat;
var _umgebungskarte;
var _eventTime;
var _fahrt;
var _strecken;
var _zoom;
function initMapOnDemand() {
	
	_fahrt = false;
	if (typeof sidebarLink !== 'undefined'){
		var id = $(sidebarLink).attr('data-stationID');
		_umgebungskarte = id;
		if (id){
			_lat = allStationsMap[id].latitude;
			_lon = allStationsMap[id].longitude;
		}
		if ($(sidebarLink).attr('data-fahrt')) 
			_fahrt = true;
	}
	_zoom = "";
	if ($(sidebarLink).attr('data-zoom'))
		_zoom = "?zoom";
	if (typeof OpenLayers === 'undefined') {
		$.getScript(themepath+"/js/vendor/OpenLayers.js", continueInit);
	} else continueInit();
}
function continueInit(){
	$('#haltestellenMapInternal').height($('#sidebar-right').height()-90);
	$(window).smartresize(function(){
			$('#haltestellenMapInternal').height($('#sidebar-right').height()-90);
			_map.updateSize();
	});
	OpenLayers.ImgPath = themepath+"/img/ol/";
	var munich = new OpenLayers.Bounds(1202139.375, 6059902, 1355648.75, 6194309);
    //var munich = new OpenLayers.Bounds(10.799, 47.717, 12.178, 48.523);
    var osm = new OpenLayers.Layer.OSM( "OSM Map","//maps.swm.de/tiles/osm/${z}/${x}/${y}.png", {
        'attribution' : 'Kartendaten &copy; <a target="_blank" href="//www.openstreetmap.org/copyright" target="_blank">OpenStreetMap</a>'
    });
    $('#haltestellenMapInternal').html('');
    //console.log("INIT");
    var template = {
    	    strokeColor: "#0000FF",
    	    strokeOpacity: 1,
    	    strokeWidth: 3,
    	    fillColor: "#00AAFF",
    	    fillOpacity: 1,
    	    pointRadius: 5,
    	    pointerEvents: "visiblePainted",
    	    externalGraphic: themepath+"/img/icons/png/station.png",
    	    graphicXOffset :  -17,
    	    graphicYOffset :  -50,
    	    graphicWidth   : 51,
    	    graphicHeight  : 48,
    	    rotation:0,
    	    label : "${name}", //label from attribute name
    	    labelXOffset:  0,
    	    labelYOffset: 0,
    	    fontColor: "black",
    	    fontSize: "12px",
    	    fontFamily: "PT Sans",
    	    fontWeight: "bold",
    	    labelAlign: "cm"
    	};
    var aStyleMap = new OpenLayers.StyleMap(new OpenLayers.Style(template)); 
	_map = new OpenLayers.Map({
		div: "haltestellenMapInternal",
		projection: "EPSG:900913",
        //displayProjection : new OpenLayers.Projection("EPSG:4326"),
		maxExtent: munich,
		restrictedExtent: munich,
		theme: null,
		numZoomLevels: 18
	});
	_haltestellen = new OpenLayers.Layer.Vector("Haltestellen", {
		styleMap: aStyleMap, 
		projection: "EPSG:4326",
		strategies: [new OpenLayers.Strategy.BBOX({resFactor: 1.1, ratio: 1})],
		protocol: new OpenLayers.Protocol.HTTP({
		   url: contextpath+"/haltestellenService"+_zoom,
		   format: new OpenLayers.Format.GeoJSON()
		})
	});
	if (_fahrt){
		_strecken = new OpenLayers.Layer.Vector("Strecken");
		var features = [];
		allStationsList.forEach(function(station){
			//console.log("STATION", station);
			var from = station.from;
			var to = station.to;
			var startPt=new OpenLayers.Geometry.Point(from.longitude, from.latitude);
			var endPt=new OpenLayers.Geometry.Point(to.longitude, to.latitude);
			startPt.transform(new OpenLayers.Projection("EPSG:4326"),new OpenLayers.Projection("EPSG:900913"));
			endPt.transform(new OpenLayers.Projection("EPSG:4326"),new OpenLayers.Projection("EPSG:900913"));
			var line=new OpenLayers.Geometry.LineString([startPt, endPt]);
			var style={strokeColor:"#0500bd", strokeWidth:3};
			var fea=new OpenLayers.Feature.Vector(line, {}, style);
			features.push (fea);
		});
		_strecken = new OpenLayers.Layer.Vector();
		_strecken.addFeatures(features);
		//console.log("Strecken", _strecken);
	}
	initClick();
	_click = new OpenLayers.Control.Click();
	// Interaction; not needed for initial display.
	selectControl = new OpenLayers.Control.SelectFeature(_haltestellen);
	_map.addControl(selectControl);
	selectControl.activate();
    _map.addControl(_click);
    if (!_umgebungskarte) _click.activate();
	_haltestellen.events.on({
		'featureselected': onStationSelect,
		'loadend': loadEnd
	});
	if (_fahrt)
		_map.addLayers([osm, _strecken]);	
	else
		_map.addLayers([osm, _haltestellen]);
	if (_umgebungskarte)
		_map.zoomTo(18);
	else
		_map.zoomTo(16);
	if (_lat && _lon)
		setCenterInternal(_lon, _lat);
	else
		setCenterInternal(11.575,48.137);
	//$(_map.layerContainerDiv).css('width', '100%'); // why was this needed ??? it breaks pinch zoom
}
function setCenter(lon, lat){
	_lon = lon;
	_lat = lat;
}
function setCenterInternal(lon, lat){
	//console.log("setCenter "+lat+","+lon)
	_map.setCenter(new OpenLayers.LonLat(lon,lat).transform(
			new OpenLayers.Projection("EPSG:4326"),
			new OpenLayers.Projection("EPSG:900913")));
}
function loadEnd(){
	//console.log("END");
}
var _selectedStation;
var x;
function onStationSelect(evt){
	if (_umgebungskarte) return;
	_selectedStation = evt.feature;
	var content="<div id='mapPopupLabel'>Haltestelle übernehmen?</div><div class='mapPopupOk'>OK</div>";
	popup = new OpenLayers.Popup.FramedCloud("featurePopup",
			 evt.feature.geometry.getBounds().getCenterLonLat(),
			 new OpenLayers.Size(120,50),
			 content, null, false);
	_map.addPopup(popup, true);
	$('#featurePopup_contentDiv').click(selectStation);
	_eventTime = new Date().getTime();
	//_haltestellen.events.fallTrough(false);
	//OpenLayers.Event.stop(evt);
	//evt.cancelBubble = true;
}
function selectStation(){
	jQuery("#sidebar-right").trigger("close");
	updateStationFinderWithMapID(_selectedStation.attributes.id,_selectedStation.attributes.name,_selectedStation.attributes.place,_selectedStation.attributes.products);
}
function selectPositon(){
	jQuery("#sidebar-right").trigger("close");
/* 	var g = evt.feature.geometry; */
	//console.log("before", _selectedPosition.lon,_selectedPosition.lat);
	var lonlat = new OpenLayers.LonLat(_selectedPosition.lon,_selectedPosition.lat).transform(
			new OpenLayers.Projection("EPSG:900913"),
			new OpenLayers.Projection("EPSG:4326"));
	//console.log("updateStationFinderWithMapLoc", lonlat.lon, lonlat.lat);
	updateStationFinderWithMapLoc(lonlat.lat,lonlat.lon);
}
</script>
	</div>
