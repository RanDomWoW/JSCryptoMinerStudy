											{{#if zoomInfoRolltreppe}}<span class="zoomInfoRolltreppe" title="Rolltreppen Status"></span>{{/if}}{{#if zoomInfoFahrstuhl}}<span class="zoomInfoFahrstuhl" title="Fahrstuhl Status"></span>{{/if}}
									</a>
								{{/if}}
							{{/if}}
						</span>
					</td>
				</tr>
			{{/if}}
			{{#unless footwaySpecial}}
				<tr tabindex="0" onkeypress="return TabKeyPress(event)">
					<td class="transfer-data-left">
						<div class="relative-wrapper">
							<span class="info-bubble-left"><span><a tabindex="-1"  class="sidebar-button sidebar-wide" data-sidebar-heading-iconclass="map" data-sidebar-heading-text="Umgebungskarte" href="#haltestellenMap" data-stationID="{{from.id}}">Umgebungskarte</a></span></span>
						</div>
						<span class="transfer-type-icon">
						</span>
						<span class="transfer-time">
							{{formattedDeparture}} {{#if this.delay}}<br /><span class="yellow">+ {{this.delay}}</span>{{/if}}
						</span>
						{{#if label }}
						<span class="transfer-means-icon">
									<span class="transport-line svg-icon {{product}} {{product}}{{label}}">
									<span class="transport-type"></span>
									<span class="transport-number">
										<span>{{label}}</span>
									</span>
								</span>
								<span class="print-icon">{{productName}}{{label}}</span>
						</span>
						{{else}}
						<span class="transfer-means-icon icon-walk-weiss">
								{{!--<img src="/.resources/mvg-theme/themes/mvg/img/icons/icon_mann_weiss.svg" class="svg-icon" alt="icon walk" />--}}
								<img src="/.resources/mvg-theme/themes/mvg/img/icons/icon_mann_schwarz.svg" class="print-icon" alt="Fußweg" />
						</span>
						{{/if}}
					</td>
					<td class="transfer-data-right">
						<div class="relative-wrapper">
							<span class="info-bubble-right info-bubble-mobile"><span><a tabindex="-1" class="sidebar-button sidebar-wide" data-sidebar-heading-iconclass="map" data-sidebar-heading-text="Umgebungskarte" href="#haltestellenMap" data-stationID="{{from.id}}">Umgebungskarte</a></span></span>
						</div>
						<span class="transfer-type_text">
							{{activity}} {{#if isFootway}} {{formattedWalkingTime}} (ca. {{formattedWalkingDistance}})  {{/if}}
						</span>

                        {{#if product}}
								<span class="transfer-to">
									{{#if isUorSBoolean}}
										<span class="transfer-info_text">in</span> {{productName}}{{label}}
									{{else}}
										<span class="transfer-info_text">in</span> {{productName}} {{label}}
									{{/if}}
								</span>
                        {{/if}}
						{{#if to.name}}
							<span class="transfer-to">
