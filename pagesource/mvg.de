	<script id="template-station-finder-station" type="text/x-handlebars-template">
		{{#each this}}
		<li data-hs-name="{{name}}" data-hs-street="{{street}}" data-loc-houseNumber="{{houseNumber}}"  data-hs-place="{{place}}" data-hs-id="{{id}}" data-loc-products="{{products}}" data-loc-type="{{type}}" data-loc-latitude="{{latitude}}" data-loc-longitude="{{longitude}}" data-loc-haslivedata="{{hasLiveData}}" data-loc-haszoomdata="{{hasZoomData}}" data-loc-isfavourite="{{isFavourite}}" data-loc-fahrinfoID="{{id}}{{street}}{{houseNumber}}{{place}}">
			<span class="hsf-icon {{#if street}}icon-address{{/if}}">
				{{#if name}}
						{{#if products}}
								{{#each this.products}}
										<img src="/.resources/mvg-theme/themes/mvg/img/icons/product-icon-{{this}}.svg" alt="{{this}}" />
								{{/each}}
						{{else}}
								<!--<img src="/.resources/mvg-theme/themes/mvg/img/icons/product-icon-b.svg" alt="icon b" />-->
								<strong>(H)</strong>
						{{/if}}		
				{{/if}}
				{{!--#if street}}
						<img class="location-icon-address" src="/.resources/mvg-theme/themes/mvg/img/icons/product-icon-address.svg" alt="icon address" />
				{{/if--}}		
		    </span>
			<span tabindex="0" onkeypress="event.stopPropagation(); return TabKeyPress(event)" class="hsf-station" style="width:{{#if distance}}50%{{else}}60%{{/if}}">
				{{name}}{{street}}{{#if houseNumber}} {{houseNumber}}{{/if}}, {{place}}
				{{!--<span class="hsf-station-gradient"></span>--}}
		    </span>
			{{#unless hideFavourites}}
				<span tabindex="0" onkeypress="event.stopPropagation(); return TabKeyPress(event)" class="hsf-favourite {{#if isFavourite}} is-favourite {{else}} no-favourite {{/if}}"><span class="readOnly">Favorit</span></span>
			{{/unless}}	
			{{!--#if calculatedDistance}}
				<span class="hsf-distance">{{calculatedDistance}}</span>
			{{/if--}}
			{{#if distance}}
				<span class="hsf-distance">{{distance}} m</span>
			{{/if}}
		</li>
		{{/each}}
	</script>
	
	<script id="template-location-finder-results" type="text/x-handlebars-template">
		{{#each this}}
		<li tabindex="0" onkeypress="return TabKeyPress(event)" data-loc-type="{{type}}" data-hs-name="{{name}}" data-hs-street="{{street}}" data-loc-housenumber="{{houseNumber}}" data-hs-place="{{place}}" data-hs-id="{{id}}" data-loc-products="{{products}}" data-loc-type="{{type}}" data-loc-latitude="{{latitude}}" data-loc-longitude="{{longitude}}" data-loc-haslivedata="{{hasLiveData}}" data-loc-haszoomdata="{{hasZoomData}}" data-loc-isfavourite="{{isFavourite}}" data-loc-fahrinfoid="{{id}}{{street}}{{houseNumber}}{{place}}">
			<span class="hsf-icon {{#if street}}icon-address{{/if}}">
				{{#if name}}
						{{#if products}}
								{{#each this.products}}
										<img class="location-icon-product" src="/.resources/mvg-theme/themes/mvg/img/icons/product-icon-{{this}}.svg" alt="{{this}}" />
								{{/each}}
						{{else}}
								<strong>(H)</strong>
						{{/if}}
				{{/if}}
		    </span>	
			<span class="hsf-station" style="width:{{#if distance}}50%{{else}}60%{{/if}}">
				{{name}}{{street}}{{#if houseNumber}} {{houseNumber}}{{/if}}{{#if place}}, {{place}} {{/if}}
				<span class="hsf-station-gradient"></span>
		    </span>
			{{#unless hideFavourites}}
				<span tabindex="0" onkeypress="return TabKeyPress(event)" class="hsf-favourite {{#if isFavourite}} is-favourite {{else}} no-favourite {{/if}}"><span class="readOnly">Favorit</span></span>
			{{/unless}}	
			{{#if distance}}
				<span class="hsf-distance">{{distance}}</span>
		    {{/if}}
		</li>
		{{/each}}
	</script>
	
	<script id="template-printed-timetables-names" type="text/x-handlebars-template">
		{{!--  iterate over ARRAY names inside OBJ locationNames  --}}
		{{#each names}}
		<li data-hs-name="{{this}}" data-hs-place="">
				<a href="javascript:void(0)" >{{this}}</a>
		</li>
		{{/each}}
	</script>
	
	<script id="template-departures" type="text/x-handlebars-template">
		{{#each this}}
		<tr>
			<td class="current-departures transfer-means-icon">
				{{#if product}}
						<span class="transport-line {{product}} {{product}}{{label}}">
							<span class="transport-type"></span>
							<span class="transport-number">
								<span>{{label}}</span>
							</span>
						</span>
				{{/if}}
			</td>
			<td class="current-departures destination">
				{{destination}}
			</td>
			<td class="current-departures remaining-time">
				<span class="{{#if blink}}blink{{/if}} minutesTillDeparture">{{formattedDepartureTime}}</span>
				<span class="{{#if blink}}blink{{/if}} timeOfDeparture hiddenTime">{{formattedDepartureClocktime}}</span>
			</td>
		</tr>
		{{/each}}
	</script>

