<script>
//make a case-insentitive "Contains"
//http://stackoverflow.com/questions/187537/is-there-a-case-insensitive-jquery-contains-selector
jQuery.expr[":"].Contains = jQuery.expr.createPseudo(function(arg) {
	return function( elem ) {
		return jQuery(elem).text().toUpperCase().indexOf(arg.toUpperCase()) >= 0;
	};
});
</script>

<script>
$(function(){
//***** SECTION: CALENDAR DATE ITEMS *****

	var $sourceDateTable = $("#dateSection table:first");

	//cleanup
	$sourceDateTable.html($sourceDateTable.html().replace(/&nbsp;/g, " "));  //no nbsp's
	$sourceDateTable.find("p").contents().unwrap();  //no P's
	$sourceDateTable.find("th:first").closest("tr").remove();  //no header row
	
	//assign classes to list items
	$sourceDateTable.find("tr").each(function(){
		if($(this).children(":eq(2):Contains('Y')").length > 0){ $(this).addClass("itemEvents"); } else { $(this).addClass("itemDates"); } //assign class for what type of item this is
	});

	//remove items which have passed
	$sourceDateTable.find("tr").each(function(){
		var endingDateText = $(this).children(":eq(1)").text();
		if(endingDateText.indexOf("/") > -1){  //ignore "ongoing" situations (cell will be blank)
			var today = new Date();
			today.setHours(0, 0, 0, 0);  //must clear time portion otherwise comparison will be off
			var testdate = new Date(Date.parse(endingDateText));
			if(today > testdate){ $(this).remove(); };
		}
	});

	//output first X calendar items
	var $ul = $("<ul>");
	$sourceDateTable.find("tr:lt(7)").each(function(){
		var $li = $("<li>").attr("class", $(this).attr("class")).append($(this).find("a"));
		$li.find("a").contents().wrap("<div class='textPart'><div class='overflowStop'></div></div>");
		var $datePart = getDateHTML($.trim($(this).find("td:eq(0)").text()), $.trim($(this).find("td:eq(1)").text()), $("<div class='datePart'></div>"));
		$li.find("a").prepend($datePart)
		$ul.append($li);
	});
	$("#dateSection").html($ul).show();
	$("#dateSection li:contains('NaN')").each(function(){ console.log("Date item has invalid date and has been hidden: " + $(this).text()); $(this).hide(); });
	
	//add item for all events
	$("#dateSection ul").append("<li class='viewMore'><a href='/?6457-College-Calendar'>View More</a></li>");

});

function getDateHTML(beg, ending, $container){
//http://stackoverflow.com/questions/8579861/how-to-convert-milliseconds-into-a-readable-date
//input: date string, like 1/20/2017

	var months = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
	
	//ordinarily, there is no beginning date to show
	if(beg.length === 0){
		
		//the ending date may be empty, too, in cases such as "ongoing" events
		if(ending.length === 0){
			var date = new Date();
			return $container.addClass("MMM").append(months[date.getMonth()]);
		} else {
			//return string format, like Jan 20
			var date = new Date(Date.parse(ending));
			return $container.append("<span class='bigD'>" + date.getDate() + "</span><br><span class='bigM'>" + months[date.getMonth()] + "</span>");
		}
		
	} else {
	//else, there is a beginning date to show	
		if(ending.length !== 0){
			var date1 = new Date(Date.parse(beg));
			var date2 = new Date(Date.parse(ending));
			return $container.append("<div class='multiDate'><span class='littleD'>" + date1.getDate() + "</span><br><span class='littleM'>" + months[date1.getMonth()] + "</span></div>")
							 .append("<div class='hyphen'>-</div>")
							 .append("<div class='multiDate'><span class='littleD'>" + date2.getDate() + "</span><br><span class='littleM'>" + months[date2.getMonth()] + "</span></div>");
		}
	}
}
</script>
<script>
$(function(){
//***** SECTION: NEWS FEATURE "SPOTLIGHT" ITEMS *****

	var $source = $("#spotlightSection table:first");
	if($source.length === 0){ return false; }
	
	//cleanup
	$source.find("p").contents().unwrap();  //no P's
	$source.html($source.html().replace(/&nbsp;/g, " "));  //no nbsp's
	$source.find("th:first").closest("tr").remove();  //no header row

	//save items to variables
	var $wide1 = saveToSpotlight($source.find("td:nth-child(1):Contains('1')").closest("tr"));
	var $wide2 = saveToSpotlight($source.find("td:nth-child(1):Contains('3')").closest("tr"));
	var $narrow1 = saveToSpotlight($source.find("td:nth-child(1):Contains('2')").closest("tr"));
	var $narrow2 = saveToSpotlight($source.find("td:nth-child(1):Contains('4')").closest("tr"));
	
	//delete source HTML
	$source.remove();
	
	//output saved items
	$("#spotlightSection").append($wide1).append($narrow1).append($narrow2).append($wide2);
	
	//add background in case of missing image (to eliminate flicker of background color before image loads: not added to CSS & using a delay)
	setTimeout(function(){
		$("#spotlightSection a").css({"background-color":"#686868"});
	}, 2000);

});

function saveToSpotlight(elemTR){

	var $tr = $(elemTR);
	var outHTML = "";
	
	var hrefLink = $tr.find("td:eq(1) a").attr("href");
	var srcImage = decodeURIComponent($tr.find("td:eq(2) img").attr("data-src"));  //prevent double escaping
	var textTitle = $tr.find("td:eq(1)").text();
	
	//remove it so it isn't selected again
	$tr.remove();
	
	outHTML += "<a class='content' href='" + hrefLink + "' style='background-image:url(" + srcImage + ")'><div class='text'>" + textTitle + "</div></a>";
	
	if(hrefLink === undefined || hrefLink === "" || srcImage === undefined || srcImage === "" || textTitle === undefined || textTitle === "" ){
		console.log("Could not output the item due to incorrect attribute or text.");
	} else {
		return $(outHTML);
	}
	
}
</script>

<script>
$(function(){
//***** SECTION: ANNOUNCEMENT ITEMS *****

	$("#athletics li").each(function(){
		
		//convert HTML for each LI
		$(this).find("th:first").closest("tr").addClass("screenReaderOnly");
		$(this).find("table").wrap("<div class='minScoreWidth'>");
		
		//replace an announcement with it
		var $openSlot = ($("#announcementSection li:not(.athletics):last"));
		
		$($openSlot).html($(this).html()).addClass("athletics");
		
	});
	
	$("#athletics").remove();

});
</script>


