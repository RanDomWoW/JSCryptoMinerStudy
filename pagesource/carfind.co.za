<script>
	var bannerout = null
	var bannerout_large = null
	var bannerout_small = null
	var currbannernum = 0
	var canclosebanner = false
	var bannerclosetimer = null

	addevent(window, 'mousemove', function () {
		if (bannerout && canclosebanner) {
			bannerout.style.height = '0px'; bannerout.style.height = '0px'; bannerout_small.style.boxShadow = ''; bannerout.style.display = 'none'; bannerout = null
		}
	})
	addevent(window, 'click', function () {
		if (bannerout) { bannerout.style.height = '0px'; bannerout.style.height = '0px'; bannerout_small.style.boxShadow = ''; bannerout.style.display = 'none'; bannerout = null }
	})
	function enlargebanner(elem, bannernum, smallbanner) {
		var image = elem.getAttribute('data-popupimage')
		var usepopup = elem.getAttribute('data-usepopupimage')

		if (bannerout) { bannerout.style.height = '0px'; bannerout.style.display = 'none'; bannerout_small.style.boxShadow = ''; bannerout = null } //close other open banner 
		if (usepopup == 'False') {
			elem.src = elem.getAttribute('data-popupimage'); // old rollover on small banners
			return
		}
		var popdiv = doc('banner' + bannernum + 'div')
		popdiv.style.display = 'block'
		popdiv.style.opacity = '1'
		bannerout = popdiv
		bannerout_small = elem
		var leftpos = getOffset(elem).absleft
		if (leftpos + 435 > doc_width()) leftpos = leftpos - 435 + elem.offsetWidth
		if (leftpos < 10) leftpos += 100
		popdiv.style.width = '435px';
		popdiv.style.height = '290px'
		popdiv.style.left = leftpos + 'px'
		popdiv.querySelector('img').src = image //elem.src.replace('_still', '') + '_roll'; //'http://test.carfind.co.za/boats/banners/2117_home1.jpg' 
		popdiv.querySelector('a').href = elem.parentElement.href;
		elem.style.boxShadow = '5px 5px 10px gray'
		canclosebanner = false //must wait till they mouseover the banner itself to hide
		bannerclosetimer = window.setTimeout(function () {canclosebanner=true },1500)
	}
	function poplargebanner(bannernum) {
		if (bannerout) { bannerout.style.height = '0px'; bannerout.style.display = 'none'; bannerout_small.style.boxShadow = ''; bannerout = null } //close other open banner 
		var popdiv = doc('bigbanner_' + bannernum)
		popdiv.style.display = 'block'
		popdiv.style.opacity = '1'
		bannerout_large = popdiv
		currbannernum = parseInt(bannernum)
		doc('blackbg').style.display = 'block'
	}
	function hidelargebanner() {
		doc('blackbg').style.display = 'none'
		bannerout_large.style.display = 'none';
	}
	function nextbanner() {
		hidelargebanner()
		var maxbannernum = parseInt(doc('numbanners').value)
		if (currbannernum == maxbannernum) currbannernum = 0
		poplargebanner(currbannernum + 1)
	}
	function prevbanner() {
		hidelargebanner()
		var maxbannernum = parseInt(doc('numbanners').value)
		if (currbannernum == 1) currbannernum = maxbannernum + 1
		poplargebanner(currbannernum - 1)
	}
</script>
