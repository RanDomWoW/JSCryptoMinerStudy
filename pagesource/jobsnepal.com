		function sendPasswordButton_click(e)
		{
			email = $('#UserEmail').val();

			if(!email) 
			{
				$('#UserEmail').focus();
				return false;
			}
      		
      		msgC.addClass('ajax-loader-big');
      		msgC.html('');
      		msgC.fadeIn(150);
      		
      		formW.slideUp(150);
      		sbtn.button("disable");
			dlg.resize();

			$.ajax({
				url: 'http://www.jobsnepal.com/index.php?method=ForgotPassword',
				type: 'POST',
				data: ({'UserEmail' : email}),
				success: function(x){
					msgC.removeClass('ajax-loader-big');
					msgC.html(fp_messages[x]);
					if(x== 1)//success
					{
						setTimeout(closeDialog, 2000);
					}
					else
					{
						formW.show(200);
						sbtn.button("enable");
					}

				},
				error: function(){
					dlg.dialog( "close" );
				}					
			});
		}
