    function FuncSetProductImg(p_type, p_itemcd, p_num, p_url) {
        var product_url = "";

        var hosturl = 'http://image.wconcept.co.kr' + "/productimg";

        if (p_type == "01" || p_type == "05") {
            product_url = hosturl + "/image/img" + p_num + "/" + Right(p_itemcd, 2) + "/" + p_url;

        } else {
            product_url = p_url;
        }

        return product_url;
    }

    function Right(target, length) {
        if (length <= target.length) {
            return target.substring(target.length - length);
        }
        return target;
    }

    // 히스토리 전체 삭제
    function SetHistoryDelAll(seq, menu) {
        if (!confirm("전체 히스토리를 삭제 하시겠습니까?("+seq+","+menu+"")) {
            return;
        }

        $.ajax({
            type: "POST",
            url: "/Layout/_Ax/Quick_HistorySet.cshtml",
            data: {
                seq: 0,
                menu: "ALL"
            },
            success: function (result) {

                if (result == 1) {
               //     alert("전체 삭제 되었습니다.");
                    $("#historyCnt").text(0);
                    setHistoryNoData();
                } else {

                    if (result == "404.0") {
                        alert("로그인 후 이용해 주세요.");
                    } else if (result == "404.1") {
                        alert("잘못된 요청 입니다.");
                    } else {
                        alert("삭제 되지 않았습니다.");
                    }
                }

                _quickMenu.init();
                _quickMenu.resize();
            },
            error: function (response) {
                console.log("error : " + response.responseText);
                return false;
            }
        });
    }

    // 히스토리 삭제
    function SetHistoryDel(seq, menu, obj) {

        if (seq == "" || menu == "") {
            alert("삭제 할 히스토리를 선택해 주세요.");
            return;
        }

        if (!confirm("선택하신 히스토리를 삭제 하시겠습니까?")) {
            return;
        }

        $.ajax({
            type: "POST",
            url: "/Layout/_Ax/Quick_HistorySet.cshtml",
            data: {
                seq: seq,
                menu: menu
            },
            success: function (result) {

                if (result == 1) {
               //     alert("삭제 되었습니다.");

                    var historyCnt = $("#historyCnt").text();
                    if (typeof (historyCnt) == "undefined" || historyCnt == "") {
                        historyCnt = "0";
                    }
                    historyCnt = parseInt(historyCnt) - 1;

                    if (historyCnt <= 0) {
                        setHistoryNoData();
                        $("#historyCnt").text(0);
                    } else {
                        obj.parent("dd").remove();
                        $("#historyCnt").text(historyCnt);
                    }

                    _quickMenu.resize();
                } else {

                    if (result == "404.0") {
                        alert("로그인 후 이용해 주세요.");
                    } else if (result == "404.1") {
                        alert("잘못된 요청 입니다.");
                    } else {
                        alert("삭제 되지 않았습니다.");
                    }
                }
            },
            error: function (response) {
                console.log("error : " + response.responseText);
                return false;
            }
        });
    }

