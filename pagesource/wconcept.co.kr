    function getHistoryList() {
        var param = {
            "maxdisp": $("#frmHistory").find("input[name=maxdisp]").val(),
            "pageIndex": $("#frmHistory").find("input[name=pageIndex]").val()
        }

        $.ajax({
            type: "POST",
            url: "/Layout/_Partial/Quick_HistoryList.cshtml",
            data: param,
            success: function (data) {
                appendHistory(data);

                if (parseInt($("#frmHistory").find("input[name=pageIndex]").val()) === 1) {
                    _quickMenu.init();
                    _quickMenu.resize();
                }
            },
            error: function (response) {
                console.log("error : " + response.responseText);
                return false;
            },
            beforeSend: function () {
            }
        });

    }

    function appendHistory(data) {
        data = JSON.parse(data);

        // var totalCnt = data.TotalRecordCount;
        var totalCnt = $("#historyCnt").text();
        var table = data.dsList.Table;

        $("#frmHistory").find("input[name=totalCount]").val(totalCnt);

        if (totalCnt > 0) {
            $("#quick_content_history").removeClass("nodata");

            $.each(table, function (index, item) {
                var $title = setHistoryTitle(item);

                setHistoryContent(item, $title);
            });
        } else {
            setHistoryNoData();
        }
    }

    function setHistoryTitle(data) {
        var $Title = $("#divHistory").find("[history-title='" + data.HISTORYDAY + "']");

        if ($Title.length == 0) {
            var titleHtml = "";
            if (data.ISTODAY == "Y") {
                titleHtml += "<dl class=\"day_list today\" history-title=\"" + data.HISTORYDAY + "\">";
                titleHtml += "<dt>TODAY</dt>";
            } else {
                titleHtml += "<dl class=\"day_list\" history-title=\"" + data.HISTORYDAY + "\">";
                titleHtml += "<dt>" + data.HISTORYDAY + "</dt>";
            }
            titleHtml += "</dl>";

            $("#divHistory").append(titleHtml);

            return $("#divHistory").find("[history-title='" + data.HISTORYDAY + "']");
        } else {
            return $Title;
        }
    }

