    <script type="text/javascript">
        $(document).ready(function () {
            var $vt = $('.carousel .txt');
            for (var i = 0; i < $vt.length; i++) {
                var $height = $vt.eq(i);
                var helf = $height.height() / 2;
                $vt.eq(i).css('margin-top', '-' + helf + 'px');
            };
            $('.key_visual_type2').ixSlideMax();
            $('.key_visual_type3').ixSlideMax();

            var $slide = $('.carousel').ixSlideMax();
            $(window).on('resize', function (e) {
                $slide.ixSlideMax('resize');
            });

            // lookbook
            var $flipbook = $('#flipbook'),
                $prev = $flipbook.siblings('.prev'),
                $next = $flipbook.siblings('.next');

            var length = parseInt($flipbook.find('.item').length / 2),
                flipIndex = 0;

            var size = getSize();
            var flipbook = $flipbook.turn({
                page: 2,
                width: size.width,
                height: size.height,
                elevation: 50,
                gradients: true,
                disabled: true,
                autoCenter: true,
                duration: 1000,
                when: {
                    start: function (event, pageObject, corner) {
                        if (pageObject.next == 1)
                            event.preventDefault();
                    },
                    turning: function (event, page, view) {
                        if (page == 1)
                            event.preventDefault();
                    }
                }
            });

            $prev.click(function (e) {
                e.preventDefault();
                flipbook.turn('previous');
                flipIndex--;
                if (flipIndex < 0) {
                    flipIndex = 0;
                }
                toggleControl(flipIndex);
            });

            $next.click(function (e) {
                e.preventDefault();
                flipbook.turn('next');
                flipIndex++;
                if (flipIndex > length - 1) {
                    flipIndex = length - 1;
                }
                toggleControl(flipIndex);
            });

            //document.body.addEventListener('touchmove', function (e) {
            //    e.preventDefault();
            //});

            $(window).on('resize', function (e) {
                var size = getSize();

                if (size.width > size.height) { // landscape
                    $flipbook.turn('display', 'double');
                } else {
                    $flipbook.turn('display', 'double');
                }

                $flipbook.turn('size', size.width, size.height);
            });

            function toggleControl(idx) {
                if (idx == 0) {
                    $prev.hide();
                    $next.show();
                } else if (idx == length - 1) {
                    $prev.show();
                    $next.hide();
                } else {
                    $prev.show();
                    $next.show();
                }
            }

            function getSize() {
                var width = document.body.clientWidth;
                var height = $(window).height();
                return {
                    width: width,
                    height: height
                }
            }

            toggleControl(flipIndex);
            $(window).trigger('resize');


            var $linkButton = $('.designer .link_group li button');
            var $carousel = $('.designer .carousel').ixSlideMax();

            $linkButton.click(function () {
                $linkButton.trigger('change', { index: $linkButton.index(this) });
            });

            $linkButton.on('change', function (event, data) {
                $carousel.ixSlideMax('changeIndex', data.index);
            })

            $carousel.on('ixSlideMax:change', function (event) {
                var $target = $linkButton.eq(event.currentIndex);
                $target.parent().parent().children('li').removeClass('active');
                $target.parent().addClass('active');
            });

            $('#jumbotron.carousel').find('.ix-list-item a').on('click', function (e) {
                var $this = $(this);
                var idx = $this.parent().data('origin-index');

                var id = $this.data('promo-id');
                var name = $this.attr('href').replace(/http:\/\//gi, '');
                var creative = $this.data('promo-creative');
                var position = $this.data('promo-position');

                ga('ec:addPromo', {
                    'id': id,
                    'name': name,
                    'creative': idx,
                    'position': position + idx
                });

                // Send the promo_click action with an event.
                ga('ec:setAction', 'promo_click');
                ga('send', 'event', 'Jumbotron', 'click', name);
            });
        });
    </script>

    
