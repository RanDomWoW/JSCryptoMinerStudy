    function setHistoryNoData() {
        $("#quick_content_history").addClass("nodata");

        var html = "";
        html += "<div class=\"day_list\">";
        html += "<h3>쇼핑 히스토리가 없습니다. </h3>";
        html += "<p>W concept 이곳 저곳을 둘러보세요. </p><p> 다양한 소식과 상품들을 만나실 수 있습니다.</p>";
