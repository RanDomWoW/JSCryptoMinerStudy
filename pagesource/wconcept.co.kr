                }
            }
        });


        //알림설정 버튼 text
        $("#is_heartpush, #is_shoppush").click(function () {
            if ($(this).hasClass("active")) {
                $(this).text("on");
            } else {
                $(this).text("off");
            }
        });


        $("#divHistory").on("scroll", function () {
            var divHeight = $("#divHistory").innerHeight();
            var scrollTop = $("#divHistory").scrollTop();
            var scrollHeight = $("#divHistory").prop("scrollHeight");
            var buffer = 0;

            var maxdisp = parseInt($("#frmHistory").find("input[name=maxdisp]").val());
            var pageIndex = parseInt($("#frmHistory").find("input[name=pageIndex]").val());
            var totalCount = parseInt($("#frmHistory").find("input[name=totalCount]").val());

            if ((divHeight + scrollTop + buffer) >= scrollHeight) {
                if ((maxdisp * pageIndex) < totalCount) {
                    console.log(pageIndex);
                    $("#frmHistory").find("input[name=pageIndex]").val(pageIndex + 1);
                    getHistoryList();
                }
            }
        });
    });

    function onScrollTop() {
        $(document).scrollTop(0);
