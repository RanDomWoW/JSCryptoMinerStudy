    //알림삭제
    function delUserNotice(idx, noticecd, noticedate, obj) {

        if (idx == -1){
            if (!confirm("알림 내역을 삭제하시겠습니까?")) {
                return;
            }
        }

        $.ajax({
            type: "POST",
            url: "/Layout/_Ax/Quick_AlarmDel.cshtml",
            data: { "idx": idx, "noticecd": noticecd, "noticedate": noticedate },
            success: function (data) {
                var jsonData = JSON.parse(data);

                if (jsonData.Key === 0) {

                    if (idx == -1) {
                        $("#btnQuickAlarm").click();
                    } else {
                        obj.parent().remove();
                    }

                    $('#alarmCnt').text(jsonData.Value);
                } else if (jsonData.Key === 400) {
                    if (confirm("로그인이 필요한 서비스 입니다.\n로그인 페이지로 이동하시겠습니까?")) {
                        rUrl = WCK_COMMON_PRT._protocal + "//" + WCK_COMMON_PRT._hostname + WCK_COMMON_PRT._pathname + WCK_COMMON_PRT._param;
                        location.href = WCK_COMMON_PRT._ssl_domain + "/Member/Login.cshtml?rUrl=" + encodeURIComponent(rUrl);
                    }
                } else {
                    alert(jsonData.Value);
                }
            },
            error: function (response) {
                console.log("error : " + response.responseText);
            }
        });
    }

    //알림설정
    function setUserNotice () {

        if (!confirm("설정을 저장하시겠습니까?")) {
            return;
        }

        var is_heartpush = $("#is_heartpush").hasClass("active") ? "Y" : "N";
        var is_shoppush = $("#is_shoppush").hasClass("active") ? "Y" : "N";

        $.ajax({
            type: "POST",
            url: "/Layout/_Ax/Quick_AlarmSet.cshtml",
            data: { "is_heartpush": is_heartpush, "is_shoppush": is_shoppush },
            success: function (data) {
                var jsonData = JSON.parse(data);

                if (jsonData.Key === 1) {
                    $(".layer_alarm_set").removeClass("active");
                    setNoticeCondition();
                } else if (jsonData.Key === 400) {
                    if (confirm("로그인이 필요한 서비스 입니다.\n로그인 페이지로 이동하시겠습니까?")) {
                        rUrl = WCK_COMMON_PRT._protocal + "//" + WCK_COMMON_PRT._hostname + WCK_COMMON_PRT._pathname + WCK_COMMON_PRT._param;
                        location.href = WCK_COMMON_PRT._ssl_domain + "/Member/Login.cshtml?rUrl=" + encodeURIComponent(rUrl);
                    }
                } else {
                    alert(jsonData.Value);
                }
            },
            error: function (response) {
                console.log("error : " + response.responseText);
            }
        });


    }

    //알림설정조회
    function setNoticeCondition() {

        var is_heartpush = $("#is_heartpush").hasClass("active") ? "Y" : "N";
        var is_shoppush = $("#is_shoppush").hasClass("active") ? "Y" : "N";

        $(".no_alram").slideUp();

        if (is_heartpush == "Y" && is_shoppush == "Y") {
            //$(".alarm_off").hide();
        } else if (is_heartpush == "Y" && is_shoppush == "N") {
            $("#noticeCont").text("쇼핑");
            $(".no_alram").slideDown();
        } else if (is_heartpush == "N" && is_shoppush == "Y") {
            $("#noticeCont").text('MY♥');
            $(".no_alram").slideDown();
        } else {
            $("#noticeCont").text("모든");
            $(".no_alram").slideDown();
        }
    }


