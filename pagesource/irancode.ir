<script>
    var oldIndex = 0;
    var duration = 500;

    function showContextMenu(index) {
        $('.menu-context').css({ 'margin-right': (index * 100) + '%' });
        $('.menu-context').animate(
                    { 'height': 209 }, {
                        duration: 300,
                        easing: 'easeOutSine',
                        complete: function () {
                            $('.menu-context').removeClass('state-closed');
                            $('.menu-context').addClass('state-opened');
                            $('.content-item:nth-child(' + (index + 1) + ')').addClass('active');
                        }
                    });
    }
    function hideContextMenu() {
        $('.menu-context').animate(
                    { 'height': 0 }, {
                        duration: 300,
                        easing: 'easeOutSine',
                        complete: function () {
                            $('.menu-context').removeClass('state-opened');
                            $('.menu-context').addClass('state-closed');
                        }
                    });
    }
    function initialmenu() {
        $('.top-menu').bind('mouseleave', function () {
            setTimeout('hideContextMenu();', 300);
        });
        $('#topMenu li.t-item').click(function () {
            $('.content-item').removeClass('active');
            var index = -($(this).attr('index') - 1);
            if (index === undefined) return;

            if ($('.menu-context').height() == 0)
                showContextMenu(index);
            else if (oldIndex == index || !$(this).attr('index'))
                hideContextMenu();
            else {
                $('.menu-context').animate(
                    { 'margin-right': (index * 100) + '%' }, {
                        duration: duration,
                        easing: 'easeOutQuart'
                    });
            }
            if ($('.menu-context').height() > 0) {
                $('.content-item:nth-child(' + ((index) + 1) + ')').addClass('active');
            }
            oldIndex = index;

        });
    }
</script>

