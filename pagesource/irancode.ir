<script>
    function onSelect(e) {
        var item = $(e.item);
        var s = '.t-content.t-state-active #KeyWord';
        var href;

        $('div.mainContent#SearchResult').css('display', 'none');
        $('div#suggestion-panel').css('display', 'none');
        $('.icdsm').css('display', 'none');
        $('#sRm').css('display', 'none');
        $('#searchKeyWordResult').css('min-height', '50px');

        if ($(s).attr('value') != undefined) {
            if ($(s).attr('value') == 'نام یا کد کالا را اینجا درج نمایید' || $(s).attr('value').trim() == '') {
                $('.icdsm').hide();
                return;
            }
        }

        if (item.find('> .t-link').text() == 'تامين كننده') {
            href = '/Search/SupplierSearch?IsProduct=False&SupplierType=False&SearchBy=SupplierName&KeyWord=*&Exact=False&PageNumber=1&PageSize=10';
        } else if (item.find('> .t-link').text() == 'محصول') {
            href = '/Search/ProductSearch?IsProduct=False&ProductionType=False&KeyWord=*&Exact=False&PageNumber=1&PageSize=10';
        } else if (item.find('> .t-link').text() == 'كاتالوگ') {
            href = '/Search/ECatalogueSearch?IsProduct=False&ProductionType=True&KeyWord=*&Exact=False&PageNumber=1&PageSize=10';
        } else if (item.find('> .t-link').text() == 'تناظرات کدهای بین المللی') {
            href = '/Search/CorrespondenceSearch?IsProduct=False&CorrespondenceCodeType=All&SearchBy=Title&KeyWord=*&Exact=False&PageNumber=1&PageSize=10';
        }

        window.location = href.replace('*', $(s).val());
        $('.message-header').show();
        e.preventDefault();
    }
</script>

