<script type="text/javascript">
var saml_login_url = "https://www.insead.edu/saml_login";
function isInseadUser(user_email) {
if($('#cboxOverlay:visible').length == 0)
{
}
else {
return /@insead.edu\s*$/i.test(user_email)  || /@insead.edutest\s*$/i.test(user_email);
}
}
jQuery(".login-details #login-name").on( "blur", function(e) {
if(isInseadUser($(this).val())) {
$(".suggestion-text").show();
window.location.href= saml_login_url;
}
});
jQuery(".login-details #register-email").on( "blur", function(e) {
if(isInseadUser($(this).val())) {
$(this).addClass('error');
$(".adfs_hint").show().find('a').attr("href", saml_login_url);
} else {
$(this).removeClass('error');
$(".adfs_hint").hide();
}
});
function get_url_param() {
var vars = [], hash;
var q = document.URL.split('?')[1];
if(q != undefined){
q = q.split('&');
for(var i = 0; i < q.length; i++){
hash = q[i].split('=');
vars[hash[0]] = hash[1];
}
}
return vars;
}
var colorbox_Dimensions = function(is_large) {
var width = '640';
var height = '480';
var docWidth = jQuery(document).width();
if (docWidth < 480) {
height = '60%';
width = '100%';
} else if (docWidth < 960) {
if (is_large) {
height = '100%';
width = '100%';
}
} else  {
if (is_large) {
height = '100%';
width = '940px';
}
}
var response = { width: width, height: height };
return response;
};
function load_colorbox() {
$param=get_url_param();
$message_obj = {};
var dimensions = colorbox_Dimensions(true);
if($param.activation=='success') {
$message_obj['success'] = 'Thank you for verifing your e-mail address.';
message_display('success', $message_obj, 'login-message-block');
$( "#login-name" ).val(decodeURIComponent($param.username));
$.colorbox({inline:true, width: dimensions.width, height: dimensions.width, open:true, href:"#inline-content-login" });
} else if($param.activation=='error') {
if($param.errorcode==1) {
$message_obj['error'] = 'Invalid verify link.';
message_display('error', $message_obj, 'login-message-block');
$.colorbox({inline:true, width: dimensions.width, height: dimensions.width, open:true, href:"#inline-content-login" });
} else if($param.errorcode==2) {
$message_obj['error'] = 'Link is expired.';
message_display('error', $message_obj, 'login-message-block');
$.colorbox({inline:true, width: dimensions.width, height: dimensions.width, open:true, href:"#inline-content-login" });
}
}
}
jQuery( document ).ready(function() {
$( "#user-login-form .form-submit" ).click(login_submit);
$( "#user-register .form-submit" ).click(register_submit);
load_colorbox();
jQuery('#user-login-form').ajaxForm({
dataType:  'json',
success: login_result,
});
jQuery('#user-register').ajaxForm({
dataType:  'json',
beforeSubmit:  loding_show,
success: register_result,
});
});
jQuery('#user-login-form').validate({
errorPlacement: function(error, element) {},
});
jQuery('#user-register').validate({
errorPlacement: function(error, element) {},
rules: { repass: { equalTo: "#register-password" }}
});
function login_result($data){
if($data.error===true) {
message_display('error', $data.error_data, 'login-message-block');
} else {
jQuery('#user-login-form').clearForm();
window.location = '/'+$data.location;
}
}
function login_submit() {
if(isInseadUser($('#login-name').val())) {
window.location.href= saml_login_url;
}
else{
jQuery('#user-login-form').trigger('submit');
}
}
function register_submit() {
jQuery('#user-register').trigger('submit');
}
function loding_show() {
$("#register-loading").removeClass('hide');
}
function register_result($data){
$("#register-loading").addClass('hide');
if($data.error===true) {
if(Object.keys($data.error_data).length == 1 && $data.error_data["register-repassword"] != '') {
message_display('error', $data.error_data, 'password-error-block');
jQuery("#register-password").focus();
} else {
message_display('error', $data.error_data, 'register-message-block');
}
// $("#register-fname").focus();
} else {
message_display('success', $data.success_data, 'register-message-block');
jQuery('#user-register').clearForm();
jQuery('#user-register').hide();
}
}
function message_display($message_type, $message_data, $message_id) {
if($message_type=='error') {
$message_class = 'msg-error';
} else if($message_type=='success') {
$message_class = 'msg-success';
}
if(Object.keys($message_data).length > 0) {
$message_block = '<ul class="'+$message_class+'">';
jQuery.each( $message_data, function( key, value ) {
$message_block += '<li>'+value+'</li>';
});
$message_block += '</ul>';
jQuery("#"+$message_id).html($message_block);
}
}
