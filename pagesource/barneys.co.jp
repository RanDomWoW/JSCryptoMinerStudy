        if(api) {
          api.onSeek(function(){
            if(this.getSize() - this.getIndex() <= 6) {
            $(this.getNaviButtons()[1]).addClass('disabled');
            } else {
            $(this.getNaviButtons()[1]).removeClass('disabled');
            }
          });
        };
        $(".box_adore_pics .items a").mouseenter(function(e) {
          $(this).addClass('hover');
          $tooltip_title.html($(this).find('.product-name').html());
          $tooltip_desc.html($(this).find('.product-desc').html());
        }).mouseleave(function(e){
          $(this).removeClass('hover');
        }).tooltip({
          tip: '#tooltip',
          position: 'top center',

          delay: 0
        });
            $(".search_txt input").focus(function() {
          if($(this).attr('value') == 'SEARCH') {
            $(this).attr('value','');
          }
        }); 
        $(".search_txt input").blur(function() {
          if($(this).attr('value') == '') {
            $(this).attr('value','SEARCH');
          }
        });
            $(".navigation ul li").mouseenter(function() {
          $(this).find("div.dropdown_content").show();
        });
        $(".navigation ul li").mouseleave(function() {
          $(this).find("div.dropdown_content").hide();
        });
            
            
            
            
            
             
            
      });
    </script>





