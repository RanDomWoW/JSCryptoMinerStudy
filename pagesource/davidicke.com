            });

            // when a user has focus on the mailing list subscribe field, we want to show the prompt.
            // once shown, then we keep the message shown.
            $('#input-mailing-list-nav-bar-email').focus(function () {
                var $target = $('#prompt-mailing-list-nav-bar');
                if ($target.is(":hidden"))
                    $target.removeClass('hidden').hide().show(200);
            });
        });
    </script>
<script type="text/javascript">



    var players = plyr.setup();

    // If the player is playing a preview video, we want to display the full video overlay once finished
    $(players).each(function(i,v)
    {
        v.on('ended', function(event)
        {
            var instance = event.detail.plyr;
            var $videoPlayer = $(instance.getContainer()).parents('.media-player');
            if($videoPlayer)
            {
                if($videoPlayer.data('preview-type') == 'preview')
                {
                    $videoPlayer.find('.full-version-overlay').show();
                }
            }
        });
    });


    // once the advert has been loaded, we want to display the tag to the user.
    $('.advert').on('advertLoaded',function()
    {
        var $target = $(this);

        //determine if we have an advert loaded.
        //The only reason why we wouldn't is because of an error, or because no ads have been assigned.
        //if($target.height()>0)
        $target.find('.tag').show(500);
    });


    $(document).ready(function()
    {
        //this event will fire when all of the javascript has been run on the page
        $(document).trigger("afterLastDocumentReady");
    });


</script>
</body>
</html>