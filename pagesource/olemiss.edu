<script type="text/javascript">

// Search
function makeSearch() {
	var q = document.getElementById('q');
	var c = document.getElementById('search-category');
	var category = 'all';
	if (c.selectedIndex > -1) {
		category = c.options[c.selectedIndex].value;
	}
	var url = 'http://olemiss.edu/search/' + category + '/' + q.value;
	window.location.href = url;
	return false;
}

// Cycle Search Placeholder Text
var ph = ['A - Z','People','Policies','Directories','Organizations','Courses','Programs','Committees','Technology','UM'];
var phctr = 0;
var timeoutID;
timeoutID = setInterval(rotateph, 1000);
function rotateph() {
	$('input#q').attr('placeholder','Search '+ph[phctr]);
	if (phctr < (ph.length - 1)) {
		phctr++;
	} else {
		timeoutID = clearInterval();
	}
}

//FlexSlider
$(window).load(function() {
  $('.flexslider').flexslider({
    animation: "slide",
	slideshowSpeed: 10000,
	pauseOnHover: true,
	keyboard: false,
//	randomize: true,
	start: function() {
		$('figcaption').hide();
		$('.flex-active-slide figcaption').fadeIn(300);
	},
	after: function() {
		$('.flex-active-slide figcaption').fadeIn(300);
	},
	before: function() {
		$('.flex-active-slide figcaption').fadeOut(300);
	}
  });
});
/**
 * Equal Heights Plugin
 * Equalize the heights of elements. Great for columns or any elements
 * that need to be the same size (floats, etc).
 * 
 * Version 1.0
 * Updated 12/10/2008
 *
 * Copyright (c) 2008 Rob Glazebrook (cssnewbie.com) 
 *
 * Usage: $(object).equalHeights([minHeight], [maxHeight]);
 * 
 * Example 1: $(".cols").equalHeights(); Sets all columns to the same height.
 * Example 2: $(".cols").equalHeights(400); Sets all cols to at least 400px tall.
 * Example 3: $(".cols").equalHeights(100,300); Cols are at least 100 but no more
 * than 300 pixels tall. Elements with too much content will gain a scrollbar.
 * 
 */
(function($) {
	$.fn.equalHeights = function(minHeight, maxHeight) {
		tallest = (minHeight) ? minHeight : 0;
		this.each(function() {
			if($(this).height() > tallest) {
				tallest = $(this).height();
			}
		});
		if((maxHeight) && tallest > maxHeight) tallest = maxHeight;
		return this.each(function() {
			$(this).height(tallest).css("overflow","auto");
		});
	}
})(jQuery);

var pagesizes  = new Array(320, 600, 768, 1024, 1280);
var photosizes = new Array(320, 600, 728,  948, 1200);
var devicewidth = 0;
function getDeviceWidth() {
	if(typeof window.innerWidth==="undefined") {
		devicewidth = $(window).innerWidth();
	} else {
		devicewidth = Math.max($(window).innerWidth(), window.innerWidth);
	}	
}

$(document).ready(function() {
// Give Constituencies Active State for Mobile Dropdowns
	$('#constituencies dl').click(function() {
		$(this).toggleClass('hover');
	});

	$('#constituencies dl').mouseenter(function() {
		$(this).addClass('hover');
	});
	$('#constituencies dl').mouseleave(function() {
		$(this).removeClass('hover');
	});

// Sublinks show/hide
	$('nav#main').addClass('showmore');

	$('#moresublinks').click(function() { 
		$('nav#main').removeClass('showmore').addClass('showfewer');
	});
	$('#fewersublinks').click(function() {
		$('nav#main').removeClass('showfewer').addClass('showmore');
	});

// Mouseover for MoreInfo icons 
	$('.icons-main-info, .moreinfo').on({
		mouseenter:function() {
			$(this).parent().children('.moreinfo').show();
		},
		mouseleave:function() {
			$(this).parent().children('.moreinfo').hide();
		}
	});

// Main Nav Buttons with Dropdowns 	
var navtimer;
var navdelay = 300;
	$('#aboutum, #aboutum-push').hover( function() {
			navtimer = setTimeout(function() {
				$('body').addClass('push-aboutum');
			}, navdelay);
		}, function() {
			clearTimeout(navtimer);
			$('body').removeClass('push-aboutum');
	});
	$('#academics, #academics-push').hover( function() {
			navtimer = setTimeout(function() {
				$('body').addClass('push-academics');
			}, navdelay);
		}, function() {
			clearTimeout(navtimer);
			$('body').removeClass('push-academics');
	});
	$('#campuslife, #campuslife-push').hover( function() {
			navtimer = setTimeout(function() {
				$('body').addClass('push-campuslife');
			}, navdelay);
		}, function() {
			clearTimeout(navtimer);
			$('body').removeClass('push-campuslife');
	});
	$('#newsevents, #newsevents-push').hover( function() {
			navtimer = setTimeout(function() {
				$('body').addClass('push-newsevents');
			}, navdelay);
		}, function() {
			clearTimeout(navtimer);
			$('body').removeClass('push-newsevents');
	});
	
// AutoScroll Announcements
	var period     = 6000;
	var transition = 600;
	function scrollUp() {
		getDeviceWidth();
		if (devicewidth >= pagesizes[2]) {
			var itemHeight  = $('#announcements li').height();
			var totalHeight = $('#announcements li').size() * itemHeight;
			var currentTop  = parseInt($('#announcements li').css('top'), 10);
			var newTop      = currentTop - itemHeight;
			if (-newTop < totalHeight) {
				$('#announcements li').animate({top: newTop}, transition);
			} else {
				newTop = 0;
				$('#announcements li').css({top: itemHeight}).animate({top: newTop}, transition);
			}
		} else {
			$('#announcements li').css({top: 0});
		}
	}
	setInterval(function() {scrollUp()}, period);
	
// Fix column heights for #newsannouncements and #eventphototweet
	setEqualHeights = function() {
		getDeviceWidth();
		$('#news, #announcements, #events, #photos, #tweets').css('height', 'auto');
		 if (devicewidth >= pagesizes[2]) {
			$('#news, #announcements').equalHeights();
		}
		if (devicewidth >= pagesizes[3]) {
			$('#events, #photos, #tweets').equalHeights();
		}
	}
	$(document).ready(setEqualHeights);
	$(window).bind('resize', setEqualHeights);

// Give hero images an adaptive background
	setHeroPhotos = function() {
		getDeviceWidth();
		$('#herophoto li img').each( function() {
			var datasrc = "&f=" + $(this).data('src');
			var imagemagic = "/includes/images/?w=";
			var photourl = imagemagic+photosizes[0]+datasrc;
			
			for(var i=0; i<5; i++) {
				if (devicewidth >= pagesizes[i]) {
					photourl = imagemagic+photosizes[i]+datasrc;
				}
			}
			$(this).attr('src',photourl);
		});
	}
	$(document).ready(setHeroPhotos);
	$(window).bind('resize', setHeroPhotos);

// Handle Alerts
	if($('#alert').length) {
		$('body').addClass('alert');
	}

// Flickr Description Ellipsis
	$('.desc').dotdotdot({
		watch: true
	});

});
</script>
