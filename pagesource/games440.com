			if(data == "") return;
			//$("#games_page").html(data);
			$("#games_page").append(data);
			
			//alert(data.length)
			if (data.length<50)
			{
				currentPage=totalPages;
			}
			tooltip();
			//isLoading = 0;
			setTimeout("isLoading = 0;",500);
		});

	}
	//$("#games_page").append("<br>page="+page);
}

$(document).ready(function() {
 	$(window).scroll(function(){
		var scrollTop = $(window).scrollTop();			
		if (scrollTop>500)
		{
			$("#topbtn").show();
		}
		else
		{
			$("#topbtn").hide();
		}
		if  (scrollTop >= ($(document).height() - $(window).height() - 300)) 
		{
			if(isLoading == 0) {
				//alert($(document).height()+" currentPage="+currentPage);
				currentPage++;
				if (currentPage<totalPages)
				{
					loadGames(currentPage);
				}
			}
		}
	}); 
});

this.tooltip = function(){	
		if (this.t!=null)
	{
		return;
	}
	xOffset = 10;
	yOffset = 20;		
	$("a.tooltip").hover(function(e)
	{						
		//alert(this.t);
		if (this.title=="")
		{
			return;
		}
		this.t = this.title;
		this.title = "";									  
		$("body").append("<p id='tooltip'>"+ this.t +"</p>");
		$("#tooltip")
			.css("top",(e.pageY - xOffset) + "px")
			.css("left",(e.pageX + yOffset) + "px")
			.fadeIn("fast");				
    },
	function()
	{
		this.title = this.t;		
		$("#tooltip").remove();
    });	
	$("a.tooltip").mousemove(function(e){
		$("#tooltip")
			.css("top",(e.pageY - xOffset) + "px")
			.css("left",(e.pageX + yOffset) + "px");
	});			
};


// starting the script on page load
$(document).ready(function(){
	tooltip();
	//setTimeout("tooltip()",500);
});


</script>



