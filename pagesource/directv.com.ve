<script>

	function search() {
		var q = $('.searching').val();
		var RegExPattern = /\S/;
		if ((RegExPattern.test(q)) && (q != '') && (q != 'Buscar')) {
			q.replace("&#32;", "+");
			window.location.href = "/busqueda.aspx?&q=" + q;
			$('a.search').removeAttr('href');
		}
		//return false;
	}

	function searchCDS() {
		$('#form1').submit(function () { return false; });
		var q = $('#searchTerms').val();
		var RegExPattern = /\S/;
		if ((RegExPattern.test(q)) && (q.value != '')) {
			q.replace("&#32;", "+");
			window.location.href = "Home.aspx?x=0&y=0&Buscar=si&searchTerms=" + q;
		}
	}

	function searchNewCDS() {
		$('#form1').submit(function () { return false; });
		var q = $('#searchTermsNew').val();
		var RegExPattern = /\S/;
		if ((RegExPattern.test(q)) && (q.value != '')) {
			q.replace("&#32;", "+");
			var str=window.location.host;
			var cant=str.indexOf("localhost");
			var url;
			if (cant>-1){
				url="http://"+window.location.host + "/Search.aspx?&q=" + q;
				window.location.href = url;
			}else{
				url ="http://"+window.location.host + "/ayuda/Search.aspx?&q=" + q;
				window.location.href = url;
			}
		}
	}

	function searchNewCDSMobile() {
		$('#form1').submit(function () { return false; });
		var q = $('#searchTermsMobileNew').val();
		var RegExPattern = /\S/;
		if ((RegExPattern.test(q)) && (q.value != '')) {
			q.replace("&#32;", "+");
			var str=window.location.host;
			var cant=str.indexOf("localhost");
			var url;
			if (cant>-1){
				url="http://"+window.location.host + "/Search.aspx?&q=" + q;
				window.location.href = url;
			}else{
				url ="http://"+window.location.host + "/ayuda/Search.aspx?&q=" + q;
				window.location.href = url;
			}
		}
	}

	$(document).ready(function () {

		$("input[type=image][title=Buscar][class=image]").bind('click', function () { searchCDS(); });

		/*Add and Remove focus function to search textbox*/
		$('.QueryBoxText').focus(function () {
			$(this).keypress(
                function (event) {
                	if (event.keyCode != 13) {
                		return true;
                	} else {
                		search();
                	}
                }
            );
		});

		$('#searchTerms').focus(function () {
			$(this).keypress(
                function (event) {
                	if (event.keyCode != 13) {
                		return true;
                	} else {
                		searchCDS();
                	}
                }
            );
		});

		$('#searchTermsNew').focus(function () {
			$(this).keypress(
				function (event) {
					if (event.keyCode != 13) {
						return true;
					} else {
						searchNewCDS();
					}
				}
			);
		});

		$('#searchTermsMobileNew').focus(function () {
			$(this).keypress(
				function (event) {
					if (event.keyCode != 13) {
						return true;
					} else {
						searchNewCDSMobile();
					}
				}
			);
		});

	});
