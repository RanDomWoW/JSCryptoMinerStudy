<script>
    $(document).ready(function () {
        $('#switcherPlanes').switcher({
            onChange: function (elem, value) {

                gtm_sendEvent("Encuentra el plan ideal para ti", "Switch", "Prepago | Pospago");

                if (value) {
                    $('#postpago-in').clearQueue();
                    $('#prepago-in').clearQueue();
                    $('#postpago-in').fadeOut('fast');
                    $('#prepago-in').delay(200).fadeIn('fast');

                    $("#postpago").addClass("cyan");
                    $("#prepago").removeClass("cyan");
                }
                else {
                    $('#postpago-in').clearQueue();
                    $('#prepago-in').clearQueue();
                    $('#prepago-in').fadeOut('fast');
                    $('#postpago-in').delay(200).fadeIn('fast');

                    $("#prepago").addClass("cyan");
                    $("#postpago").removeClass("cyan");
                }


            }
        });

        //Evento cambio opciones
        $("#prepago, #postpago").bind("click", function () {
            $(this).hasClass("cyan") ? $('#switcherPlanes').click() : null;
        });

        if ($("#switcherPlanes").siblings(".switcherCheckHandle").position().left > 0) {
            $('#switcherPlanes').click()
        }

    });
</script>
