<script src="/newhomepage/js/jquery.bxslider.js"></script>
<script>

    var SlidersBox;
    var porcentajeTolerancia = 80;

    function isVisiblePorcentaje(porcentajeVisible, contenedorCarrousel) {

        if (($(window).scrollTop() - contenedorCarrousel.offset().top) <= ((contenedorCarrousel.height() + contenedorCarrousel.offset().top) * ((100 - porcentajeVisible) / 100))) {
            return true;
        }
        else {
            return false;
        }

    }

    $(document).ready(function () {
        SlidersBox = $('.bxslider').bxSlider({
            pagerCustom: '#bx-pager',
            onSliderLoad: function () {

                if (isVisiblePorcentaje(porcentajeTolerancia, $(".bx-viewport"))) {
                    dataLayer.push({ 'event': 'impresiondebanner', 'name': $($(".container-content-slider")[1]).find("h1").text(), 'posicion': 1 });
                }

                $('.slider.bannerLock').css('visibility', 'visible');
            },
            onSlideAfter: function (slideElement, oldIndex, newIndex) {

                if (isVisiblePorcentaje(porcentajeTolerancia, $(".bx-viewport"))) {
                    dataLayer.push({ 'event': 'impresiondebanner', 'name': $($(".container-content-slider")[SlidersBox.getCurrentSlide() + 1]).find("h1").text(), 'posicion': (SlidersBox.getCurrentSlide() + 1) });
                }
            }
        });

        // Trackeo de evento Swipe
        $(".bxslider li").on("swipe", function () { trackPageEvent('Cambio de banner Carrusel', 'Home No Clientes') });

        $(".bx-controls-direction").on("click", function () {
            trackPageEvent('Cambio de banner Carrusel', 'Home No Clientes');
        });
    });
</script>
<script>
    $('.cargar').click(function (e) {
        e.preventDefault();
        if ($(window).width() <= 980) {
            if (!$(this).siblings('.content').is(":visible")) {
                $('.content').slideUp('300');
                $(this).siblings('.content').slideDown('300');
            }
            else {
                $('.content').slideUp('300');
                $(this).siblings('.content').slideUp('300');
            }
        }
    });

    nover = function () {
        if ($(window).width() > 980) {
            $("a.cargar").removeAttr('href');
            $('.content').slideDown('300');
        }
        else {
            $("a.cargar").attr('href', 'javascript:void();');
            if ($('.content:visible').length != 1) {
                $('.content').slideUp('300');
            }
        }
    };

    $(document).ready(function () {

        var thisPage = window.location.href;
        var destino = window.location.protocol + "//" + window.location.hostname + "/clientes"

        $('#switcherClientes').switcher({
            onChange: function (elem, value) {

                if (value && destino != thisPage) {
                    gtm_sendEvent("Cliente", "Empty", "Empty");
                    $('body').fadeOut('slow', function () {
                        $('#switcherClientes').click();
                        //$('#switcherPlanes').click();
                    });
                    window.location.href = destino;
                }
            }
        });

        nover();
        $(window).resize(function (e) {
            e.preventDefault();
            nover();
        });
    });
</script>
