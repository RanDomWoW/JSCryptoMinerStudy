    $('.content-slider').click(function (e) {
        e.stopPropagation();

        var bannerTitle = $(this).attr("data-title");
        var bannerPos = Number($(this).attr("data-position"));
        dataLayer.push({ 'event': 'clickenbanner', 'name': bannerTitle, 'posicion': bannerPos });

        if (e.target.nodeName != 'A') {
            var suscribete = $(this).find('.btn-suscribete');
            if (suscribete.length > 0) {
                var attr = suscribete.attr('target');
                var href = suscribete.attr('href');
                if (typeof attr !== typeof undefined && attr !== false) {
                    if (attr == '_blank') {
                        window.open(href, '_blank');
                    }
                }
                else {
                    window.location = href;
                }
            }
        }

    });
</script>
