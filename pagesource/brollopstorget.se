window.addEventListener('jquery:loaded', function() {
    jQuery(document).ready(function(event) {
        setTimeout(function () {
            var tag = document.createElement('script');
            tag.src = "//www.youtube.com/player_api";
            var firstScriptTag = document.getElementsByTagName('script')[0];
            firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
        }, 500);
    });

    jQuery(document).on('click', '[data-youtube-embed-id], [data-tv4play-embed-id]', function (e) {
        var container;
        if ($(this).parents('.pgwSlider').length > 0 && $(this).parents('.ps-list').length === 0) {
            e.preventDefault();
            var slider = $(this).parents('.pgwSlider').get(0).slajder;
            if (slider) {
                slider.stopSlide();
            }
            jQuery('.arrow.left, .arrow.right').css({ display: 'none' });
            jQuery('.ps-caption').css({ display: 'none' });

            container = $(this).parents('li').first();

            if ($(this).data('youtube-embed-content')) {
                container.html($(this).data('youtube-embed-content'));

                setTimeout(function () {
                    initYouTubePlayer($(container).find('div[data-video-id]').first().get(0), true);
                }.bind(this), 100);
            } else if ($(this).data('tv4play-embed-content')) {
                container.html($(this).data('tv4play-embed-content'));
            }
        } else if ($(this).is('.articleTeaser')) {
            e.preventDefault();

            container = $(this);
            if ($(this).data('youtube-embed-id')) {
                var trackingAttr = '';
                if ($(this).data('youtube-tracking-link')) {
                    trackingAttr = 'data-play-tracker="' + $(this).data('youtube-tracking-link') + '"';
                }
                container.html(
                    '<div class="video-container">' +
                            '<div id="custom-youtube-player" ' + trackingAttr + ' " data-video-id="' + $(this).data('youtube-embed-id') + '" data-title="' + $(container).find('h3').text() + '"></div>' +
                    '</div>'
                );

                setTimeout(function () {
                    initYouTubePlayer($(container).find('div[data-video-id]').first().get(0), true);
                }.bind(this), 100);
            } else if ($(this).data('tv4play-embed-id')) {
                container.html(
                    '<div class="video-container">' +
                        '<iframe src="http://www.tv4play.se/iframe/video/'+$(this).data('tv4play-embed-id')+'?starttime=0" width="100%" height="100%" scrolling="no" frameborder="no"></iframe>' +
                    '</div>'
                );
            }
        }
    });
}, false);

var flm_youtube_player;
function initYouTubePlayer(t, autoplay) {
    var onReady = function(){};
    if (autoplay === true) {
        onReady = function (event) {
            event.target.playVideo();
        }
    }

    flm_youtube_player = new YT.Player(t, {
        height: jQuery(t).data('height'),
        width: jQuery(t).data('width'),
        videoId: jQuery(t).data('videoId'),
        events: {
            'onReady': onReady,
            'onStateChange': onPlayerStateChange
        }
    });
};
function onYouTubePlayerAPIReady() {
    jQuery('div[data-video-id]').each(function () {
        initYouTubePlayer(this, !!$(this).data('autoplay'));
    });
}
function onPlayerStateChange(event) {
    var title = jQuery(event.target.getIframe()).data('title');
    var playTracker = jQuery(event.target.getIframe()).data('play-tracker');
    var slider = jQuery('.pgwSlider');
    if (slider.length > 0 && slider.get(0) && slider.get(0).slajder) {
        slider = slider.get(0).slajder;
    } else {
        slider = null;
    }
    if (event.data == YT.PlayerState.PLAYING) {
        if (slider) {
            slider.stopSlide();
        }
        jQuery('.arrow.left, .arrow.right').css({ display: 'none' });
        jQuery('.ps-caption').css({ display: 'none' });
        ga('send', 'event', 'Videos', 'Play', title, undefined, {'nonInteraction': 1});
        if (playTracker) {
            var img = document.createElement('img');
            img.src = playTracker;
        }
    } else {
        jQuery('.arrow.left, .arrow.right').css({ display: 'inline' });
        jQuery('.ps-caption').css({ display: 'inline' });
    }
    if (event.data == YT.PlayerState.ENDED) {
        ga('send', 'event', 'Videos', 'Watch to End', title, undefined, {'nonInteraction': 1});
    }
}
