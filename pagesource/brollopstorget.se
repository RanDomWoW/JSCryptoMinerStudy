    adformContainer.style.display = 'block';

    container.appendChild(adformContainer);

    var adformUrl = window.mfl.getAdformUrl(adformId);

    if (window.mfl.debug) {
      console.log('Adform: ' + name + ': Loading tag with mid ' + adformId + '... (' + adformUrl + ')');
