        <script>
    (function() {
        // An example of measuring promotion views. This example assumes that
        // information about the promotions displayed is available when the page loads.
        var promotions = [];
        for (var i = 0; i < window.mfl.puffImpressions.length; i++) {
            promotions.push({
                id: window.mfl.puffImpressions[i].url,
                name: window.mfl.puffImpressions[i].title,
                position: window.mfl.puffImpressions[i].position,
                creative: window.mfl.puffImpressions[i].creative
            });
        }

        window.mfl.puffImpressions = {
            push: function(data) {
                promotions.push({
                    'id': data.url,
                    'name': data.title,
                    'position': data.position,
                    'creative': data.creative
                });
            },
        };

        var sendImps = function(){
            // Only send 30 at a time (keep below 8KB per event)
            for (var i = 0; i < promotions.length; i += 30) {
                dataLayer.push({
                  'event': 'promotionView',
                  'ecommerce': {
                    'promoView': {
                      'promotions': promotions.slice(i, i+30)
                    }
                  }
                });
            }

            promotions = [];

            setTimeout(sendImps, 500);
        };

        setTimeout(sendImps, 500);
    })();
