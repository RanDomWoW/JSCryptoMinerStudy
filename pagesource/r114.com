<script type="text/javascript">
	var rolling_right_ad;
	var const_page=1;
	
	$('input[type=text], textarea').placeholder();

	function ch_paging(cur_page,_flag){

		const_page = parseInt(cur_page);
		for(var i=1 ; i <= 4 ; i++){
			if(i == parseInt(cur_page)){
			//	document.getElementById("ad_" + i).style.display = "";
			//	document.getElementById("button_" + i).className = "active";
				$("#ad_" + i).css("display", "");
				$("#button_" + i).addClass("active");
			}else{
			//	document.getElementById("ad_" + i).style.display = "none";
			//	document.getElementById("button_" + i).className = "";
				$("#ad_" + i).css("display", "none");
				$("#button_" + i).removeClass("active");
			}
		}
		if(_flag == "off"){
			end_rol();
		}else{
			start_rol();
		}
	}

	function start_rol(){
		if(const_page > 3) {
			const_page=1;
			rolling_right_ad = setTimeout("ch_paging('" + const_page + "')",10000);
		}else{
			rolling_right_ad = setTimeout("ch_paging('" + (const_page+1) + "')",10000);
		}
	}

	function end_rol(){
		clearTimeout(rolling_right_ad);
	}

	ch_paging(3,'on');


	function chgRightFloatingBanner(_onoff, _imgSrc) {

		if (_imgSrc != "") {
			var _obj = document.getElementById("obj_right_floating_banner");

			if (_onoff == "on") {
				_obj.innerHTML = "<img src='" + _imgSrc + "' />";
				_obj.style.display = "";
			}
			else {
				_obj.innerHTML = "";
				_obj.style.display = "none";
			}
		}

	}


	$(document).ready(function() {

		// 중앙 배너 롤링
	    $('.slide_type60 > ul > li').css("display", "");
	    
    	if($.browser.msie && parseInt($.browser.version) <= parseInt(8)){
    	}
    	else {
			if($('.slide_type60 > ul').length>0){
				var slider60 = $('.slide_type60 > ul').bxSlider({
					auto: true,
					pause: 8000,
					minSlides: 1,
					pager: true,
					controls: true,
					touchEnabled: false
				});
			};
		}

		$(document).on('mouseover','.bx-pager',function() {
			slider60.stopAuto();
			slider60.startAuto();
		});	

	});

	var postCount = parseInt($("#adsMainCount").val());

</script>

