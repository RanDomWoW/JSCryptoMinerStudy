    <script>
function buscar(){
	var res = $("#busca").val();
	
	//var busca = res.replace("/", "|"); 
	var busca = res.split("/").join("|");
	
	var m = $("#buscaTipo").val();
	if (m == 'galerias'){
		window.location="/galerias/1/"+busca; 
	}
	if (m == 'galeria'){
		window.location="/galerias/1/"+busca; 
	}
	if (m == 'noticia'){
		window.location="/noticias/1/"+busca; 
	}
	if (m == 'noticias'){
		window.location="/noticias/1/"+busca; 
	}
	if (m == ''){
		window.location="/noticias/1/"+busca; 
	}
	if (m == 'editais'){
		window.location="/editais/1/"+busca; 
	}
	if (m == 'normativas'){
		window.location="/normativas/1/"+busca; 
	}
	if (m == 'parcerias'){
		window.location="/parcerias/1/"+busca; 
	}
	if (m == 'videos'){
		window.location="/videos/1/"+busca; 
	}
	if (m == 'biblioteca'){
		window.location="/biblioteca/1/"+busca; 
	}
	if (m == 'treinamentos'){
		window.location="/treinamentos/1/"+busca; 
	}
	if (m == 'concursos'){
		window.location="/concursos/1/"+busca; 
	}
}
	
function searchKeyPress(e)
{
    // look for window.event in case event isn't passed in
    e = e || window.event;
    if (e.keyCode == 13)
    {
     buscar();
    return false;
    }
    return true;
}
</script>
                  
                
