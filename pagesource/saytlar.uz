<script language="JavaScript">
    function setDimensions() {
        if ($('.super').height() != $('.super').width()) {
            $('.super').height($('.super').width());
            $('.superImgDiv').height($('.superImgDiv').width() - $('.superLink').height());
            $('.superImg').height($('.superImgDiv').height());
        }
        
        if ($('.subLi').height() != $('.subLi').width()) {
            $('.subLi').height($('.subLi').width());
            $('.subImgDiv').height($('.subImgDiv').width() - $('.subLink').height());
            $('.subImg').height($('.subImgDiv').height());
        }
        
        if ($('.catalog').height() != $('.catalog').width()) {
            $('.catalog').height($('.catalog').width());
            $('.catalogImgDiv').height($('.catalogImgDiv').width() - $('.catalogLink').height() - $('.catalogInfo').height());
            $('.catalogImg').height($('.catalogImgDiv').height());
        }
        
        if ($('.catalogType').width() != $('.catalog').width()) {
            $('.catalogType').width($('.catalog').width());
            $('.catalogTypeImg').width($('.catalog').width());
        }
    }
    setInterval(setDimensions, 100);
</script>

<script type="text/javascript">
    $('#check-tas-ix').on ('submit', function (){
        var domain = this.domain.value;
		if (domain == '') return;
        var $this = this;
        $.ajax({
          method: "POST",
          url: "/catalog/check",
          data: { domain: domain }
        })
          .done(function( msg ) {
            if (msg.indexOf("НЕ входит") !== -1 || msg.indexOf("Не удалось определить") !== -1){
				$this.domain.value = '';
                $($this.domain).addClass ('ph-red');
                $($this.domain).removeClass ('ph-green');
                $($this.domain).attr ('placeholder', 'Сайт вне tas-ix');
            }else{
                $this.domain.value = '';
                $($this.domain).addClass ('ph-green');
                $($this.domain).removeClass ('ph-red');
                $($this.domain).attr ('placeholder', 'Сайт в tas-ix');
            }
            
          });
    });
</script>

