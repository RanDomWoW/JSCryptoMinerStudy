$(function() {
  // bind switch search type
  $('a.f_dropdown_type').bind('click', function(e){
    e.preventDefault();
    $('span.f_dropdown_text').attr('data-dropdown-value', $(this).attr('data-dropdown-type'));
    $('span.f_dropdown_text').text($(this).text());
  });
  // bind search button click
  $('button.f_dropdown_search').bind('click', function(e){
    e.preventDefault();
    var keyword = keywordProcess($.trim($('input#f_dropdown_keyword').val()));
    if (keyword) {
      var url;
      var searchType = $('span.f_dropdown_text').attr('data-dropdown-value');
      switch (searchType) {
        // case 'people':
          // TODO: do we have the public search people feature
          // return alert(searchType);
          // break;
        case 'suppliers':
          url = "/SUPPLIERS/" + encodeURI(keyword.toUpperCase()) + '-MANUFACTURERS.html';
          break;
        case 'shipments':
          if ((window.location.href).indexOf('/Import-') == -1) {
            url = "/Customs-Data/Export-" + encodeURI(keyword);
          } else {
            url = "/Customs-Data/Import-" + encodeURI(keyword);
          }
          break;
        case 'products':
        default:
          url = '/catalog/all/' + encodeURI(keyword) + '-manufacturers';
          break;
      };
      // TODO: get lang from url query, we never put lang into cookie?
      var lang = $.cookie('lang');
      if (lang == 'zh-CN') {
        url += '?lang=' + lang;
      }
      window.location.href = url;
      return false;
    }
  });
  // bind "enter" key
  $('input#f_dropdown_keyword').bind('keydown', function(e){
    if (e.keyCode === 13) {
      $('button.f_dropdown_search').click();
    }
  });
  //Method to process keyword
  function keywordProcess(keyword) {
    if (keyword) {
      keyword = keyword.replace(/\b\w+\b/g, function(keyword) {
        return keyword.substring(0, 1).toUpperCase() + keyword.substring(1);
      });
      //Replace single hyphen with '--' and replace blank character with '-'
      keyword = keyword.replace(/-+/g, "--");
      keyword = keyword.replace(/\s+/g, "-");
      keyword = keyword.replace(/-{3,}/g, "--");
    }
    return keyword;
  }

  //Method to process keyword
  function supplierKeywordProcess(keyword) {
    if (keyword) {
      keyword = keyword.replace(/\s+/g, '-').toUpperCase();
    }
    return keyword;
  }

  // Register Bar - Sticky to the top.
  var  mn = $("#regBar");
      mns = "is_scrolled";
      hdr = $('header').height();
      
      hide_last = $("#regBar-desc-last")
      last_hidden = "is_hidden";

      btn = $("#regBar-btn")
      btn_show = "is_shown";

      

  $(window).scroll(function() {
    if( $(this).scrollTop() > hdr ) {
      mn.addClass(mns);
      btn.addClass(btn_show);
      hide_last.addClass(last_hidden);
    } else {
      mn.removeClass(mns);
      btn.removeClass(btn_show);
      hide_last.removeClass(last_hidden);
    }
  });
});
