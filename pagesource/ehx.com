    $(document).ready(function(){
        $("#nav-mobile").html($("#nav-main").html());
	
		if($('#nav-mobile').css('display') == 'none'){$("#nav-mobile").html("");} else {$("#nav-main").html("");}
		
		$(window).bind('orientationchange', function(event) {
			
			if($( window ).width() > 949 ){
				$("#nav-main").html($("#nav-mobile").html());	
				$("#nav-mobile").html("") ;
			}else{
				if($("#nav-mobile").html() == ""){
					$("#nav-mobile").html($("#nav-main").html());
					$("#nav-main").html("") ;
				}				
			}
		});
	

        $("#nav-trigger span").click(function(){
            if ($("nav#nav-mobile ul").hasClass("expanded")) {
                $("nav#nav-mobile ul.expanded").removeClass("expanded").slideUp(250);
                $(this).removeClass("open");
            } else {
                $("nav#nav-mobile ul").addClass("expanded").slideDown(250);
                $(this).addClass("open");
            }
        });
		if($(window).width() >= 768){
			var rightH = $(".colRight").height();
			var leftH = $(".colLeft").height();
			if(leftH < rightH){$(".colLeft").css('min-height',rightH);}else {$(".colRight").css('min-height',leftH);}	
		}
    });
