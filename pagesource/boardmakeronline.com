<form id="loginForm" class="login-modal">
<div id="LoginModal">
    <div class="form">
        
        <div class="alert alert-error" id="loginErrDiv" style="display: none;">
        </div>
        <h3>
            Member Login</h3>
        <div class="form-section first no-padding">
            <div class="control-group">
                <label class="control-label" for="loginUname">
                    Username or Email</label>
                <div class="controls">
                    <input name="loginUname" type="text" id="loginUname" class="text" tabindex="1" />
                </div>
            </div>
            <div class="control-group">
                <label class="control-label" for="password">
                    Password</label>
                <div class="controls">
                    <input name="loginPwd" type="password" id="loginPwd" class="text" tabindex="2" />
                </div>
            </div>
            <div class="control-group">
                <a class="btn mr-20" id="LoginBtn" href="#" tabindex="3">Log In</a>
                <label class="checkbox inline">
                    <input id="chkKeepSignedIn" class="ignore-valdation" type="checkbox" name="chkKeepSignedIn"
                        tabindex="4" />
                    Keep Me Signed In?</label>
            </div>
            <div class="control-group">
                <a href="/ForgotPassword.aspx" tabindex="5">
                    I forgot my password</a></div>
        </div>
