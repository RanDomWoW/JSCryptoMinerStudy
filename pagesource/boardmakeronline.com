            <a class="btn" id="LoginRegisterBtn" href="/Registration/Register"
                tabindex="6">Register</a>
        </div>
        -->
    </div>
</div>
</form>
<script type="text/javascript">
    $(document).ready(function () {
        $(".login-modal").validate({
            rules: {
                loginUname: 'required',
                loginPwd: 'required'
            },
            ignore: '.ignore-valdation',
            highlight: function (element) {
                $(element).closest('.control-group').removeClass('success').addClass('error');
                //setTimeout(function () { ResizeColorbox(); }, 15); // delay makes sure content is added before resize;
            },
            success: function (element) {
                element.closest('.control-group').removeClass('error').addClass('success');
                //setTimeout(function () { ResizeColorbox(); }, 15); // delay makes sure content is added before resize;
            },
            errorClass: 'help-block'
        });

        $('#LoginBtn').click(function (e) {
            e.preventDefault();
            tryLogin();
        });

        $("#loginPwd").keydown(function (event) {
            if (event.keyCode == 13) {
                tryLogin();
            }
        });

    });

    function tryLogin() {

        if ($('#loginForm').valid() == true) {
            SubmitLoginInfo();
        }
        return false;
    }

    function SubmitLoginInfo() {
        if ($('#LoginFlag').val() == "False") {
            var username = $('#loginUname').val();
            var pwd = $('#loginPwd').val();
            var keepSignedIn = $('#chkKeepSignedIn').is(':checked');
            var postData = { userName: username, password: pwd, keepSignedIn: keepSignedIn };
            var goToPage = $('#gotoUrl').val();
            $.ajax({
                type: 'POST',
                url: '/Subscription/Login',
                data: postData,
                beforeSend: function () {
                    $.blockUI({ baseZ: 20000, message: '<h3 style="padding-top:20px;">Processing Login.....</h3><img src="https://bmoimages.azureedge.net/content/Images/loader.gif" />' });
                },
                success: function (data) {
                    if (data.isSuccessful) {
                        $('#loginErrDiv').html('').hide();
                        $('#LoginDetailsTopDiv').html('myDynaVox:<strong> ' + data.displayName + '</strong>|<a href="/You/Home">My Dashboard</a>|<a href="/Logout.aspx"> Log out</a>');
                        if (invokedFrom == "StartNowBtn") {
                            ShowAppropriateMessage();
                            $.unblockUI();
                        } 
                        else {
                        if(data.isDmcUser){
                            window.location.href = '/Organization';//take to AccountManagement page
                        }
                        else{
                            window.location.href = '/You/Home';
                        }
                        }
                    }
                    else {
                        $('#loginErrDiv').html(data.errMsg).show().colorbox.resize();
                        resetValidation();
                        $.unblockUI();
                    }
                },
                error: function (xhr, textStatus, errorThrown) {
                    $.unblockUI();
                    HandleAjaxError(xhr, textStatus, errorThrown, 'Select Subscription', 'There was an issue logging in.');
                }
            });
        }
    }

    function resetValidation() {
        $('.control-group')
            .removeClass('error, success')
            .find('label.help-block').text('');
    }

    function ShowAppropriateMessage() {
        $.ajax({
            type: 'GET',
            url: '/Subscription/SubscriptionMessaging',
            success: function (res) {
                var resultType = typeof res;
                if (resultType == 'string') {
                    $("#LoginModal").html(res);

                    $.fn.colorbox({ width: "700px", href: '#LoginModal', open: true, scrolling: false, inline: true, onComplete: function () {
                        $.fn.colorbox.resize();
                    }
                    });
                }
                else {
                    if (!res.isLoggedIn) {
                        invokedFrom = "StartNowBtn";
                        openLoginModal();
                    }
                }

            },
            error: function (xhr, textStatus, errorThrown) {
                HandleAjaxError(xhr, textStatus, errorThrown, 'Trial Start', 'There was an issue starting the trial.');
            }
        });
    }



</script>

