 <html>
        <head>
	<script>
		var userLocale = navigator.language || navigator.userLanguage;
		if (userLocale.toLowerCase() == 'zh-tw') 
			{ document.location.href="Ch"; }
		else if (userLocale.toLowerCase() == 'zh-cn')
				{ document.location.href="Sch"; }
		else { document.location.href="Eng";}
        </script>
        </head>
        </html>