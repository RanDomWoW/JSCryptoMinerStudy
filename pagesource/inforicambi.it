<script>

function set_geoloc()
{
	$.ajax({
	  type: "get",
	  url: 'http://inforicambi.it/ajax/geoloc.html',
	  data:{'str':$('#geoLocal').val() },
	  dataType: 'script',
	  Async: true,
	  cache: false,
	  beforeSend : function () {
			$('#geoLoader').html('<i class="fa fa-refresh fa-spin"></i>');
		},
	  success: function(result){
			document.location.reload();
			$('#geoLoader').html('');
			//chiudi_geo_loc();
		}
	 });
}

function autocomplete_init() {
    var options = {
     types: ['(cities)'],
     componentRestrictions: {country: "it"}
    };

    var input = document.getElementById('geoLocal');
    var autocomplete = new google.maps.places.Autocomplete(input, options);

	$(input).keypress(function(e) {
	   if(e.which == 13) {
			set_geoloc()
		}
	});

}

autocomplete_init();

</script>

