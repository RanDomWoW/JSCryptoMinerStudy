										url: $auto_complete_url.replace('__query__', request.term),
										dataType: 'JSONP',
										success: function(data) {
											response($.map(data[1], function(item) {
												return item[0];
											}));
										}
									});
								},
								select: function(event, ui) {
									if ( !ui.item ) {
										return;
									}

									var $url = 'https://www.vic.gov.au/search-results.html?q=__query__';

									window.location.href = $url.replace('__query__', ui.item.label);
								}
							});
						});
