	<script type="text/javascript">

		function get_new_tweets()
		{
			var last_tweet_id = $('.list-lawers li').first().attr('tweet_id');
			$url = 'https://www.vic.gov.au/elements/tweets.elm.html?last_tweet_id=#last_tweet_id#';
			$url = $url.replace('#last_tweet_id#', last_tweet_id);

			$.post(
				$url,
				null,
				function ($data) {
					if ( $data.length > 22 ) {
						$('.list-lawers').animate({opacity: 0 }, 1000, function() {
							$('.list-lawers li').animate({opacity: 0 }, 0);
							$('.list-lawers').animate({opacity: 1 }, 0);
							$('.list-lawers').prepend($data);
							$('.list-lawers li').animate({opacity: 1 }, 1000);
						});
					}
					schedule_get_new_tweets();
				}
			);
		}

		function schedule_get_new_tweets()
		{
			setTimeout(get_new_tweets, 30000);
		}

		$(document).ready(function(){
			schedule_get_new_tweets();
		});
	</script>

