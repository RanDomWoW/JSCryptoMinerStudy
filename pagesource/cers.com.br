<script type="text/javascript">

	window.onload = function(){
		botaoChatAtivo();
	}

	var idButtonAtendimento = "liveagent_button_online_5731J0000004IjC";
	var idButtonSuporte = "liveagent_button_online_5731J0000004IjH";
	var idButtonVenda = "liveagent_button_online_5731J0000004Ij7";

	function selecionarChat(sel){
		var valorSelecionado = $(sel).val();
		
		if(valorSelecionado == "venda"){		
			if(isVisible(idButtonVenda)){
				clickButtonChat("Vendas");
			} else {
				showAlertaOffLine();
			}
		 }
		
		else if(valorSelecionado == "supor"){
			if(isVisible(idButtonSuporte)){
				clickButtonChat("Suporte");
			} else {
				showAlertaOffLine();
			}
		 }
		
		else{
		   if(isVisible(idButtonAtendimento)){
				clickButtonChat("Atendimento");
			} else {
				showAlertaOffLine();
			}
		 }
	}
	
	function clickButtonChat(texto){
		showAlertaOnLine();
		var elAtendimeto = $("[id*='liveagent_button']:contains('" + texto + "')");
		var onClickValue = elAtendimeto.attr("onclick");
		$(".btn-enviar").attr("onclick", onClickValue);
	}
	
	function showAlertaOnLine(){
		$("#hidden_div").hide();
		$("#prechat_submit").show();
	}
	
	function showAlertaOffLine(){
		$("#hidden_div").show();
		$("#prechat_submit").hide();
		$("#escolhaOffline").hide();
	}
	
	function isVisible(id){
		return $("#" + id ).css("display") == 'inline';
	}
	
	function isOline(){
		return (isVisible(idButtonAtendimento) || 
				isVisible(idButtonSuporte) || 
				isVisible(idButtonVenda));
	}
	
	function botaoChatAtivo(){
		if(isOline() == true){
			$("#chatAtivo").show();
		} else {
			$("#chatInativo").show();
		}
	}

</script>





