    <p>
      <button type="button" class="btn btn-new next" name="button" ng-disabled="disable()" ng-click="pushGoal()">
        <i class="fa fa-plus"></i> Adicionar uma nova meta</button>
    </p>
  <div ng-include="templateButtons"></div>
</form>
</script> 
   
