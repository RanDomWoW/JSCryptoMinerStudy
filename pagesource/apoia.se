    <script type="text/ng-template" id="/views/directives/user/contact/fields.html"><div>
  <div class="form-group" ng-if="!outOfBrazil">
    <label for="contactPhone" class="required">
      Telefone para contato
    </label>
    <div>
      <div class="input-group">
        <input type="text" name="contactPhone" id="contactPhone" ng-model="contact.value" placeholder="(00) 0000-0000" ui-br-phone-number ng-required="true">
        <span class="message error" ng-show="showError('contactPhone','required')">Telefone é obrigatório</span>
        <span class="message error" ng-show="showError('contactPhone','brPhoneNumber')">O numero está incorreto</span>
      </div>
      <small>Ponha seu telefone com o DDD da sua cidade</small>
    </div>
  </div>
  <div class="form-group international-phone" ng-if="outOfBrazil">
    <label for="contactPhone">
      Telefone para contato
      <span class="message error message-inline" ng-show="showError('contact','value')">Telefone é obrigatório</span>
    </label>
    <div class="grid">
      <div class="grid__col">
        <div class="input-group">
          <input type="text" class="ddi" name="contactDDI" id="contactDDI" ng-model="contact.ddi" ng-required="true">
          <input type="text" name="contactPhone" id="contactPhone" ng-model="contact.value" ng-required="true">
          <span class="message error" ng-show="showError('contactPhone','required')">Telefone é obrigatório</span>
        </div>
        <small>Utilizar o código internacional (DDI) somente o numero.</small>
      </div>
    </div>
  </div>
</div>
</script> 
   
