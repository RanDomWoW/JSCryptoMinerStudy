    <script type="text/ng-template" id="/views/directives/user/campaign/rewards.html"><form name="Form" novalidate ng-submit="submit()">

  <div class="reward" ng-repeat="reward in data.rewards">
    <h4>
      <i class="fa fa-star"></i> Recompensa {{reward.value|numeral:"$0,0"}}
      <a class="btn btn-new btn-xs btn-primary" ng-click="delete()">
        <i class="fa fa-trash"></i>
        Excluir
      </a>
    </h4>

    <div class="grid">
      <div class="grid__col grid__col--5-of-8">
        <div class="form-group">
          <label for="rewardTitle_{{$index}}">
            T&iacute;tulo
          </label>
          <div class="input-group">
            <input class="form-control" id="rewardTitle_{{$index}}" name="rewardTitle_{{$index}}" ng-model="reward.title" type="text"
              ng-disabled="disable()" focus-me="!reward.title"></input>
          </div>
        </div>
      </div>
      <div class="grid__col grid__col--3-of-8">
        <div class="form-group">
          <label for="rewardValue_{{$index}}">
            Valor (R$)
            <span class="message error" ng-show="showError('rewardValue_'+$index)">Apoio com um valor mímimo de R$1</span>
          </label>
          <div class="input-group">
            <input class="form-control" id="rewardValue_{{$index}}" name="rewardValue_{{$index}}" ng-model="reward.value" require type="text"
              required ng-disabled="disable()" ui-money-mask="0" min=1 focus-me="!reward.desc"></input>
          </div>
        </div>
      </div>
    </div>


    <div class="form-group">
      <label for="rewardDesc_{{$index}}">
        Descri&ccedil;&atilde;o
        <span class="message error message-inline" ng-show="showError('rewardDesc_'+$index,'required')">Como você vai reconpensar seu apoiador?</span>
      </label>
      <div class="input-group">
        <input class="form-control" ta-max-text="{{descMaxLength}}" id="rewardDesc_{{$index}}" name="rewardDesc_{{$index}}" ng-model="reward.desc"
          type="hidden" required ng-disabled="disable()"></input>
        <div text-angular ta-toolbar="[['h1','h2','h3','bold','italics','underline','quote','ul','ol'],['justifyLeft','justifyCenter','justifyRight','insertImage','insertLink','insertVideo', 'charCount']]"
          ng-model="reward.desc"></div>
        <span class="message error message-inline" ng-show="showError('rewardDesc_'+$index)">A descrição deve ter um máximo de {{ descMaxLength }} caracteres.</span>
      </div>
    </div>
  </div>
  <p>
    <button type="button" class="btn btn-new next" name="button" ng-disabled="disable()" ng-click="pushReward()">
      <i class="fa fa-plus"></i> Adicionar uma nova recompensa</button>
  </p>
  <div ng-include="templateButtons"></div>
</form>
</script> 
   
