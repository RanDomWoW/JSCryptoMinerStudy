  <div class="grid__col" ng-cloak ng-class="{'grid__col--2-of-6':outOfBrazil,'grid__col--4-of-8':!outOfBrazil}">
    <div class="form-group">
      <label for="addressCity" class="required">
        Cidade
      </label>
      <div class="input-group">
        <input type="text" name="addressCity" id="addressCity" ng-model="address.city" ng-required="addressRequired">
        <span class="message error" ng-show="showError('addressCity','required')">Cidade é obrigatório</span>
        <!-- <small>Nos buscamos a cidade atavés do seu CEP verifique se é ele que está errado</small> -->
      </div>
    </div>
  </div>
  <div class="grid__col grid__col--1-of-8" ng-if="!outOfBrazil">
    <div class="form-group">
      <label for="addressState" class="required">
        UF
      </label>
      <div class="input-group">
        <input type="text" name="addressState" id="addressState" max-length="2" min-length="2" ng-pattern="stateValidate" ng-model="address.state" ng-required="addressRequired && !outOfBrazil">
        <span class="message error" ng-show="showError('addressState','required')">UF inválido</span>
      </div>
    </div>
  </div>
  <div class="grid__col grid__col--2-of-6" ng-if="outOfBrazil">
    <div class="form-group">
      <label for="addressState">Estado</label>
      <div class="input-group">
        <input type="text" name="addressStates" id="addressState" ng-model="address.state" ng-pattern="false">
        <span class="message error" ng-show="showError('addressState','required')">Estado é obrigatório</span>
      </div>
    </div>
  </div>
</div>
</script> 
   
