    <script type="text/ng-template" id="/views/directives/search-list.html"><div class="results">
    <h4 ng-show="results.campaigns.length">Campanhas</h4>
    <ul>
        <li ng-repeat="item in results.campaigns" ng-class="{selected:item.selected}" ng-click="goTo(item)">
