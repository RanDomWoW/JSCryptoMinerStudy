      <input class="form-control" id="campaignDesc" name="campaignDesc" ng-model="data.campaign.about.desc" require type="hidden"
        required ng-disabled="disable()"></input>
      <div text-angular ta-toolbar="[['h1','h2','h3','bold','italics','underline','quote','ul','ol'],['justifyLeft','justifyCenter','justifyRight','insertImage','insertLink','insertVideo']]"
        ng-model="data.campaign.about.desc"></div>
    </div>
