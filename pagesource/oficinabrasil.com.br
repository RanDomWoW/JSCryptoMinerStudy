<script>
function centerModal (modalSelector) {
  var $modal = $(modalSelector)
  $modal.css({
    marginLeft: ($modal.width() / -2 )+ 'px',
    marginTop: ($modal.height() / -2) + 'px',
  })
}

function toggleModal(modalSelector) {
  centerModal(modalSelector)
  if (parseInt($('.modals').css('top')) > 0) {
    $('.modals').css({
      top: '0px'
    })
    $(modalSelector).fadeIn();
  } else {
    $('.modals').css({
      top: '100%'
    })
    $(modalSelector).fadeOut();
  }
}

window.toggleModal = toggleModal
</script>

