<script>
    /**
     * Показ/скрытие блока с онлайн тв на главной странице
     */
    inlineScripts.push(function() {
        $(document).ready(function() {
            //плавно показываем блоки скроллим к нему.
            $('.js-show-tv-online').on('click', function() {
                var $tvBox = $(this).closest('.tv-box');
                $tvBox.addClass('open');
                // инициализация трансляции
                $('.js-main-translation-block').trigger('init-online-translation');
                $('html, body').animate({
                    scrollTop: $tvBox.offset().top
                }, 700);

                //скрываем при сколле вверх выше двух третей экрана.
                var  heightWindow = $(window).height(),
                    heightScroll = window.pageYOffset;
                $(window).on('scroll', function() {
                    var tempScroll = window.pageYOffset;
                    if (heightScroll > tempScroll && tempScroll < heightWindow/3 && heightScroll > heightWindow/3){
                        $('.tv-box').removeClass('open');
                        $('.js-main-translation-block').trigger('stop-online-translation');
                    }
                    heightScroll = tempScroll;
                });
            });

            //скрываем по нажатию на кнопку "Наверх"
            $('.js-top-btn').on('click', function() {
                // при нажатии на кнопку "Наверх" скролл срабатывает в течение 700 мс
                setTimeout(function() {
                    $('.tv-box').removeClass('open');
                }, 700);
                // остановить трансляцию
                $('.js-main-translation-block').trigger('stop-online-translation');
            });
        });
    });
</script>
