<script>
        inlineScripts.push(function() {

            var $timer = $(".js-second-counter");
            //обновление таймера
            function updateTimer() {
                var time = $timer.html();
                var ss = time.split(":");
                var dt = new Date();
                dt.setHours(0);
                dt.setMinutes(ss[0]);
                dt.setSeconds(ss[1]);

                var dt2 = new Date(dt.valueOf() - 1000);
                var temp = dt2.toTimeString().split(" ");
                var ts = temp[0].split(":");
                $timer.html(ts[1]+":"+ts[2]);
                if (ts[1] == 0 && ts[2] == 0) {
                    $('.js-forgot-form').find('[type="submit"]').removeAttr('disabled');
                } else {
                    setTimeout(updateTimer, 1000);
                }
            }
            $('.js-forgot-form').submit(function(e) {
                e.preventDefault();
                $('.js-forgot-form').find('[type="submit"]').attr('disabled', 'disabled');
                $.ajax($(this).attr('action'), {
                    dataType: 'json',
                    method: 'post',
                    data: $(this).serialize(),
                    success: function (data) {
                        if (data['SUCCESS']) {
                            $('.js-forgot-form label,input[type=text]').hide();
                            $('.js-success-message').text(data['MESSAGE']).show();
                            $('.js-status-message').parent().hide();
                            $('.js-timer-container').show();
                            $timer.val('5:00');
                            setTimeout(updateTimer, 1000);
                        } else {
                            $('.js-status-message').text(data['MESSAGE']).parent().show();
                            $('.js-forgot-form').find('[type="submit"]').removeAttr('disabled');
                        }
                    }
                });
            })
        });
    </script>
