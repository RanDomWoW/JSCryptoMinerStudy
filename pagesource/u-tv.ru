    inlineScripts.push(function () {

        /**
         * При размерах окна более 992 инициализируем bxSlider
         * */

        function sliderInit() {
            if ($(window).width() < 992) {

                // подгружаем маленькие картинки
                $('.slider__img').each(function(i, elem) {
                    var slideSrc = $(elem).data('src');
                    $(elem).attr('src', slideSrc);
                });

                if (window.slider) {
                    window.slider.destroySlider();
                    $('.js-main-slide').css('width', '');
                } else {
                    return false;
                }
            }

            if (window.slider) {
                return false;
            }

            $(document).trigger('calcBorder');

            $('.slider__slide--today').remove();

            window.slider = $('.slider').bxSlider({
                slideSelector: '.slider__slide',
                pagerCustom: '.slider__pager',
                controls: false,
                pause: 15000,
                speed: 1000,
                auto: true,
                onSlideAfter : function ($slideElement, oldIndex, newIndex) {
                    //Статистика просмотра слайда
                    var slideId = $slideElement.data('slide-id');
                    if (slideId) {
                        sliderStatistic('view', slideId);
                    }
                }
            });

            //Статистика просмотра первого слайда
            var firstSlideId = $(slider.getCurrentSlideElement()).data('slide-id');
            if (firstSlideId) {
                sliderStatistic('view', firstSlideId);
            }
        }
        $(function() {
