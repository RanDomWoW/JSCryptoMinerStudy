<script>
jQuery(document).ready(function($) {

	$('#btn_login').click(function() {

		$.login.clearErrors();

		if (!$.login.validateNotEmpty($("#email").val())){ $.login.showErrorMessage($("#email"), "Por favor, digite um email válido.", "bottom"); return false;}
        if (!$.login.validateEmail($("#email").val())){ $.login.showErrorMessage($("#email"), "Por favor, digite um email válido.", "bottom"); return false;}
        if (!$.login.validateNotEmpty($("#password").val())){ $.login.showErrorMessage($("#password"), "Por favor, digite uma senha.", "bottom"); return false;}

		$.login.setLoading();

        dataLayer.push({'event': '_trackVirtualview', 'category': 'Login', 'action': 'normalLogin'});

        $.login.login($('#email').val(), $('#password').val(), null, $('#rememberMe').is(':checked'), $.successCallback, $.errorCallback);

        return false;
	});

    $("#btn_loginwithfb").click(function() {

    	$.login.clearErrors();

    	$.login.setLoading();

        dataLayer.push({'event': '_trackVirtualview', 'category': 'Login', 'action': 'fbLogin'});

        $.facebook.login($.successCallback, $.errorCallback);

        return false;
    });

    $("#btn_forgot_password").click(function(event) {

    	event.preventDefault();

    	$.login.clearErrors();

    	$.login.forgotpwd($("#forgot_email").val(), $.onForgotPwdSuccess, $.onForgotPwdError);
    });

    // Clear errors on any component focus.
    $('#email,#password').focus(function() { $.login.clearErrors(); });

	// Enter on some fields executes forms.
	$('#email,#password').keyup(function(e) {
		if (e.keyCode == 13) {
			$('#btn_login').click();
			$('#email,#password').blur();
		}
	});


    $.successCallback = function(newUser) {

    	$.login.clearLoading();

		if (!newUser)
			window.location.reload();
	}

	$.errorCallback = function(errorCode) {

		$.fancybox.close();

		$.login.clearLoading();

		if (errorCode == null || errorCode == $.sw.errorCode.INTERNAL_ERROR)
		{
			$.login.showErrorMessage($("#btn_login"), "Oops, algo saiu errado. Tem sido exploradas e que vai atender a que, logo que possível.", "bottom");
			return;
		}

	    switch(errorCode) {
	        case $.sw.errorCode.SESSION_EXPIRED:
	        case $.sw.errorCode.INVALID_PARAM:
	        	$.login.showErrorMessage($("#btn_login"), "Oops, alguma coisa deu errado.<br />Por favor atualize seu navegador e tente novamente. (" + errorCode + ")", "bottom");
				break;
	        case $.sw.errorCode.INTEGRATION_COMMUNICATION_FAILURE:
	        	$.login.showErrorMessage($("#btn_loginwithfb"), "There was a problem communicating with Facebook, please refresh and try again. (" + errorCode + ")", "bottom");
	        	break;
		    case $.sw.errorCode.DO_NOT_HAVE_PERMISSION:
	        	$.login.showErrorMessage($("#btn_login"), "Você não tem as permissões necessárias para completar esta ação. (" + errorCode + ")", "bottom");
	        	break;
	        case $.sw.errorCode.LOGIN_INVALID_DETAILS:
	        	$.login.showErrorMessage($("#btn_login"), "Não foi possíl fazer login com estas informações.", "bottom");
	            break;
	        case $.sw.errorCode.LOGIN_ACCOUNT_BANNED:
	        	$.login.showErrorMessage($("#email"), "A sua conta foi bloqueada, verifique seu email para saber mais.", "bottom");
				break;
	        case $.sw.errorCode.USER_NOT_FOUND:
	        	$.login.showErrorMessage($("#btn_login"), "A conta do Facebook que você está usando não está conectada ao MiniMundos.", "bottom");
	            break;
	        case $.sw.errorCode.LOGIN_EXPIRED_TOKEN:
	        	$.login.showErrorMessage($("#btn_forgot_password"), "O link de redefinição de senha que você utilizou está expirado. Por favor clique aqui para enviar um novo e-mail com um novo link.", "bottom");
				break;
	        default:
		        $.login.showErrorMessage($("#btn_login"), "Oops, something went wrong. ("  + errorCode + ")", "bottom");
	    }
	}

	var pwdResetToken = $.login.getUrlVar("pwrt");
	if (pwdResetToken != undefined)
		$.login.resetPwd(pwdResetToken, $.errorCallback);
});
</script>

