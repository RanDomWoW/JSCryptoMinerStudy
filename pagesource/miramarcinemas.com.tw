<script>
    //dash 選單設定


    function set_sel_dash_cinema() {
        $('#sel_dash_cinema option').remove();
        $('#sel_dash_movie option').remove();
        $('#sel_dash_date option').remove();
        $('#sel_dash_time option').remove();

        $('#sel_dash_cinema').append('<option  value="" > 選擇影城 </option>');
        $('#sel_dash_movie').append('<option  value="" > 選擇電影 </option>');
        $('#sel_dash_date').append('<option  value="" > 選擇日期 </option>');
        $('#sel_dash_time').append('<option  value="" > 選擇場次 </option>');

        var isDrShow = false;//是否有大直皇家影城
        var htmlDrShow = "";
        var isTrShow = false;//是否有台茂皇家影城
        var htmlTrShow = "";
        $.each(dash_CinemaList.Data.CinemaGroup, function (idx, c) {

            if (c.CinemaCName  =="美麗華大直皇家影城") {
                isDrShow = true;
                htmlDrShow = '<option value="' + c.CinemaId + '_' + c.CinemaCName + '">' + c.CinemaCName + '</option>';
                return;
            }

            if (c.CinemaCName  == "美麗華台茂皇家影城") {
                isTrShow = true;
                htmlTrShow = '<option value="' + c.CinemaId + '_' + c.CinemaCName + '">' + c.CinemaCName + '</option>';
                return;
            }

            $('#sel_dash_cinema').append('<option value="' + c.CinemaId + '_' + c.CinemaCName + '">' + c.CinemaCName + '</option>');
        });

        if (isDrShow) {
            $('#sel_dash_cinema').append(htmlDrShow);
        }

        if (isTrShow) {
            $('#sel_dash_cinema').append(htmlTrShow);
        }
    }

    function set_sel_dash_movie() {
        $('#sel_dash_movie option').remove();
        $('#sel_dash_date option').remove();
        $('#sel_dash_time option').remove();

        $('#sel_dash_movie').append('<option  value="info" > 選擇電影 </option>');
        $('#sel_dash_date').append('<option  value="" > 選擇日期 </option>');
        $('#sel_dash_time').append('<option  value="" > 選擇場次 </option>');


        var sel_CinemaCName = $('#sel_dash_cinema').val();


        $.each(dash_CinemaList.Data.CinemaGroup, function (idx, c) {
            if (sel_CinemaCName === (c.CinemaId + '_' + c.CinemaCName)) {
                $.each(c.MovieInfo, function (idx2, m) {
                    var MovieCName = m.MovieCName.replace(/\(.*?\)/g, '') ;
                    $('#sel_dash_movie').append('<option value="' + c.CinemaId + '_' + MovieCName + '">' + MovieCName + '</option>');
                });
                return false;
            }
        });
    }

    function set_sel_dash_date() {

        $('#sel_dash_date option').remove();
        $('#sel_dash_time option').remove();


        $('#sel_dash_date').append('<option value="">選擇日期</option> ');
        $('#sel_dash_time').append('<option  value="" > 選擇場次 </option>');


        var CinemaIdName = $('#sel_dash_cinema').val();
        var MovieIdName = $('#sel_dash_movie').val();


        $.each(dash_CinemaList.Data.CinemaGroup, function (idx, c) {

            if (CinemaIdName === (c.CinemaId + '_' + c.CinemaCName)) {
                $.each(c.MovieInfo, function (idx2, m) {
                    if (MovieIdName === (c.CinemaId + '_' + m.MovieCName.replace(/\(.*?\)/g, '') )) {
                        $.each(m.ShowDateList, function (idx3, s) {
                            var arrDate = s.ShowDateCht.split('-');
                            $('#sel_dash_date').append('<option value="' + s.ShowDateCht + '">' + arrDate[0] + '月' + arrDate[1] + '日' + ' 星期' + arrDate[2] + '</option>');
                        });
                        return false;
                    }
                });
                return false;
            }

        });

    }

    function set_sel_dash_time() {
        var CinemaIdName = $('#sel_dash_cinema').val();
        var MovieIdName = $('#sel_dash_movie').val();
        var DateList = $('#sel_dash_date').val();

        $('#sel_dash_time option').remove();
        $.each(dash_CinemaList.Data.CinemaGroup, function (idx, c) {
            if (CinemaIdName === (c.CinemaId + '_' + c.CinemaCName)) {
                $.each(c.MovieInfo, function (idx2, m) {
                    if (MovieIdName === (c.CinemaId + '_' + m.MovieCName.replace(/\(.*?\)/g, '') )) {
                        $.each(m.ShowDateList, function (idx3, s) {
                            if (DateList === s.ShowDateCht) {
                                $.each(s.ShowTimeList, function (idx4, st) {

                                    //重新設定影廳名稱-------------start
                                   // debugger;
                                    var sel_cinema_val = $('#sel_dash_cinema  option:selected').text();
                                    var m_MovieHallCht = st.MovieHallCht;
                                    if (sel_cinema_val === "美麗華大直皇家影城") {
                                        if (st.MovieHallCht == "標準") {
                                            m_MovieHallCht = "大直皇家影城";
                                        }

                                    } else if (sel_cinema_val === "美麗華台茂皇家影城") {
                                        if (st.MovieHallCht == "美麗華皇家影城") {
                                            m_MovieHallCht = "台茂皇家影城";
                                        }
                                    } else if (sel_cinema_val === "美麗華台茂影城") {
                                        if (st.MovieHallCht == "美麗華皇家影城") {
                                            m_MovieHallCht = "台茂皇家影城";
                                            return;
                                        }

                                        if (st.MovieHallCht == "標準") {
                                            m_MovieHallCht = "標準廳";
                                        }
                                    } else {
                                        if (st.MovieHallCht == "標準") {
                                            m_MovieHallCht = "標準廳";
                                        }
                                    }
                                    if (st.MovieHallCht == "IMAX") {
                                        m_MovieHallCht = "IMAX 廳";
                                    }

                                     //重新設定影廳名稱-------------end


                                    $('#sel_dash_time').append('<option value="">' + m_MovieHallCht + '</option>');
                                    $.each(st.SessionList, function (idx5, sl) {
                                        $('#sel_dash_time').append('<option value="' + c.CinemaId + '_' + sl.SessionId + '">' + sl.ShowTime + '</option>');
                                    });
                                });
                                return false;
                            }
                        });
                        return false;
                    }
                });
                return false;
            }

        });
    }



    set_sel_dash_cinema();


    $('html').click(function (e) {
        if (!$(e.target).parents('.dash').length) {
           // toggleDashDeatil();
            hide_detail();
        }
    });



    function dash_go_Booking() {
        var tmp = $('#sel_dash_time').val();
        tmp = tmp.split('_');
        var mCid = tmp[0];
        var mSid = tmp[1];

        if (mCid == "") {
            alert("請選擇電影場次");
            return;
        }

        var m_name = $('#sel_dash_movie :selected').text();
        go_page("/Booking/booking_data?cid=" + mCid + "&sid=" + mSid + "&m_name=" + m_name  );
    }


    $(".fancybox")
        .attr('rel', 'gallery')
        .fancybox({
            beforeLoad: function () {
                this.title = $(this.element).attr('caption');
            }
        });




</script>


