        $('#special').hide();
        $('#news').hide(); 

        $('#' + type).fadeIn(200);
         
     
        console.log(type);
        detectSubNavRows();

     
       
    }


    function detectSubNavRows() {
        var sub_nav_rows = $('.sub_nav').find('.sub_nav_row').filter(function () {
            return $(this).css('display') != 'none';
        });

        if (sub_nav_rows.length > 0) {
            if ($('#pc_logo_div').css('display') != 'none') {
                $('.sub_nav').fadeIn();
            }
        }


    }


    function set_sub_nav_select(obj) {




        if ($(window).width() < 1030) {
            if (pre_sub_nav !== null) {
                $(pre_sub_nav).css('border-bottom', '3px solid #cccccc').css('color', 'black');
            }
            $(obj).css('border-bottom', '3px solid red').css('color', 'black');
            pre_sub_nav = obj;

        } else {
            if (pre_sub_nav !== null) {
                $(pre_sub_nav).css('border-bottom', '6px solid #cccccc').css('color', 'black');
            }
            $(obj).css('border-bottom', '6px solid red').css('color', 'black');
            pre_sub_nav = obj;

        }

    }



