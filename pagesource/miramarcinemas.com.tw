    <style>
        /* Animation keyframes - you need to add prefixes */
        @keyframes spin {
            from {
                transform: rotate(0deg);
            }

            to {
                transform: rotate(360deg);
            }
        }

        /* Loading animation container */
        .loading {
            position: absolute;
            top: 50%;
            left: 50%;
            width: 28px;
            height: 28px;
            margin: -14px 0 0 -14px;
        }

            /* Spinning circle (inner circle) */
            .loading .maskedCircle {
                width: 20px;
                height: 20px;
                border-radius: 12px;
                border: 3px solid #f72929;
            }

            /* Spinning circle mask */
            .loading .mask {
                width: 12px;
                height: 12px;
                overflow: hidden;
            }

            /* Spinner */
            .loading .spinner {
                position: absolute;
                left: 1px;
                top: 1px;
                width: 26px;
                height: 26px;
                animation: spin 1s infinite linear;
            }

        #load_mask {
            width: 100%;
            height: 100vh;
            background-color: #808080bf;
            position: fixed;
