if (currentUrl.length < 3){currentUrl = location.href;}
/* Custom function probeURL added Oct 1, 2012 per Mongoose/Geary/Randy request
Custom function probeURL modified by jking on Jun 4, 2013 accept a parameter (theUrl) */
function probeURL (theUrl)
{
var checkURL = theUrl;
checkURL = checkURL.split("?")[0].split("#")[0];
return encodeURIComponent(checkURL);
}
/* custom1 = utm_campaign if it exists in query string */
if (getURLVar('utm_campaign')){
var custom1=getURLVar('utm_campaign');
}
/* custom2 = site entry page URL: do not set if custom2 cookie exists */
if(!get_c('MM_custom2',null)) {
var custom2=probeURL(currentUrl);
}
/* custom3 = current page URL */
var custom3=probeURL(currentUrl);
function printVars(){
var custom1Value = '';
var custom2Value = '';
if(!get_c('MM_custom1',null)) {
var custom1Value = custom1;
}
else{
custom1Value = get_c('MM_custom1');
}
if(!get_c('MM_custom2',null)) {
var custom1Value = custom2;
}
else{
custom2Value = get_c('MM_custom2');
}
variables = "custom1 = " + custom1Value + " <br>custom2 = " + decodeURIComponent(custom2Value) + " <br>custom3= " + decodeURIComponent(custom3);
document.getElementById('mongooseCustomVars').innerHTML = variables;
}
</script>
