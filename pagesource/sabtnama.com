    <script>


        var url = "";

        function clean(obj) {
            var propNames = Object.getOwnPropertyNames(obj);
            console.log(propNames);
            for (var i = 0; i < propNames.length; i++) {
                var propName = propNames[i];
                if (obj[propName] === null || obj[propName] === undefined) {
                    delete obj[propName];
                }
            }
            return obj;
        }


        $(document)
            .on('focus',
                'input[id$="Date"]',
                function () {
                    $(this).datepicker();
                });
        $("#btnLoadmore")
            .click(function () {
                var pageIndex = parseInt($("#PageIndex").val()) + 1;
                $("#PageIndex").val(pageIndex);

                console.log(pageIndex)
                console.log(parseInt($("#PageCount").val()));
                console.log(parseInt($("#PageIndex").val()));
                if (parseInt($("#PageIndex").val()) <= parseInt($("#PageCount").val())) {


                    getresult('/Home/GetEvents', pageIndex);
                } else {
                    $("#btnLoadmore").addClass("hidden");
                }
            })


        function getresult(url, pageIndex) {


            $.ajax({
                url: url,
                type: "GET",
                data: {
                    pageIndex: pageIndex,
                    takeAmount: 5
                },
                beforeSend: function () {
                    $('.blocker').show();

                },
                complete: function () {
                    $('.blocker').hide();
                },
                success: function (data) {
                    $("#result").append(data);

                    //console.log("PageIndex: " + parseInt($("#PageIndex").val()));
                    //console.log("Page Count: " + parseInt($("#PageCount").val()));
                    if (parseInt($("#PageIndex").val()) + 1 > parseInt($("#PageCount").val())) {
                        $("#btnLoadmore").addClass("hidden");
                    }
                },
                error: function () { }
            });
        }


        $(document).ready(function () {
