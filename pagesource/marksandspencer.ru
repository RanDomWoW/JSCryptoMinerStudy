    function addFavorite(idForm, aFavorite, full, quik){
        var arResult;

        var href = '/local/components/morizo/favorite.add/component.php?ajax_send=1';
		
		if( countJsonArray(localStorage["idFavorites"]) >= 100 ){
			$('#favorite_limit').lightbox_me({
                centered: true,
                closeSelector: ".close_default",
                onClose: function() {
                    $('.lb_overlay').remove();
                }
            });
			
			return false;
		} 
		
				else if( $.cookie('MS_AUTH_AFTER_FAV') == undefined ){
			
			$.cookie('MS_AUTH_AFTER_FAV', 'Y', {expires: 30, path: '/'});
			
			$('.quick_view .close_popup').trigger('click');
			
			$('.popup.auth_for_favorite').lightbox_me({
			    centered: true,
			    closeSelector: ".close_default",
			    onLoad: function() {},
			    onClose: function() {
			        $('.lb_overlay').remove();
			    }
			});
			
		}
				
        var qString = $(idForm).formSerialize();

        var jqxhr = $.post(
            href,
            qString,
            function(data){
                arResult = data;
                return false;
            },
            "json"
        );
        jqxhr.always(function () {
			
            $(aFavorite).removeClass('add_to_fav');
            $(aFavorite).addClass('dell_favorite checked');

            if(full === true)
            {
                $(aFavorite).next().html('В избранном');
                $(aFavorite).next().removeClass('add_to_fav');
                $(aFavorite).next().addClass('dell_favorite');
            }
            if(quik !== false)
            {
                $(quik).removeClass('add_to_fav');
                $(quik).addClass('dell_favorite checked');
            }
            
            if(supports_html5_storage())
            {
                var id = $_GET("id",qString);

                if(parseInt(id) > 0)
                {
                    var obIdsFavorite = {};
                    var idsFavoriteLoc = localStorage["idFavorites"];
                    
                    if(typeof(idsFavoriteLoc) !== "undefined" )
                    {
                        obIdsFavorite = JSON.parse(idsFavoriteLoc);
                        obIdsFavorite[parseInt(id)] = parseInt(id);
                    }
                    else
                    {
                        obIdsFavorite[parseInt(id)] = parseInt(id);
                    }
                    
                    localStorage.setItem("idFavorites", JSON.stringify(obIdsFavorite));

                    var i = 0;
                    for(var p in obIdsFavorite) {
                        i = i + 1;
                    }
                  
                        $("#favorite_num").html('<span class="num">'+i+'</span>');
                        $('#mobile_favorite_num').html(i);
                    
                }
            }
            if( $('.popup.quick_view:visible') ){
				create_fav_labels('quick_view');
			}
        });
        return false;
    }

