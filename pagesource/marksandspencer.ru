    function dellFavorite(id, iblock_id, full, quik){
        var arResult;
        var href = "/local/components/morizo/favorite.dell/component.php?subscribe_action=dell&ajax_send=1&id=" + id +"&iblock_id=" + iblock_id;

        $.getJSON(
