    function create_fav_labels(sourse){	

    	
	    if(supports_html5_storage())
        {
            var idsFavoriteLoc = localStorage["idFavorites"];

            if (typeof(idsFavoriteLoc) !== "undefined")
            {
            	
            	if(sourse=='refrash'){
	            	$('.dell_favorite').each(function(){
		            	$(this).next(".dell_favorite").html('Добавить в избранное');
	                    $(this).next(".dell_favorite").addClass('add_to_fav');
	                    $(this).removeClass('dell_favorite').removeClass('checked');
	                    $(this).next(".dell_favorite").removeClass('dell_favorite');
	                    $(this).addClass('add_to_fav');
	            	});
            	}
            	
                var obIdsFavorite = JSON.parse(idsFavoriteLoc);
                for(var p in obIdsFavorite) {
                	
                	var idfav;
                	
					idfav = "#favorite_"+obIdsFavorite[p];
					
					
                    $(idfav).next(".add_to_fav").html('В избранном');
                    $(idfav).next(".add_to_fav").addClass('dell_favorite');
                    $(idfav).removeClass('add_to_fav');
                    $(idfav).next(".add_to_fav").removeClass('add_to_fav');
                    $(idfav).addClass('dell_favorite checked');
                    
                    
                    if(sourse=='refrash' && $('.popup.quick_view:visible') || sourse=='quick_view' && $('.popup.quick_view:visible') ){
                    	idfav = ".quick_view #favorite_"+obIdsFavorite[p];
						$(idfav).next(".add_to_fav").html('В избранном');
						$(idfav).next(".add_to_fav").addClass('dell_favorite');
						$(idfav).removeClass('add_to_fav');
						$(idfav).next(".add_to_fav").removeClass('add_to_fav');
						$(idfav).addClass('dell_favorite checked');
					} 
                }
            }
        }
        
        $(".add_to_fav").attr("style", "block");
		$(".dell_favorite").attr("style", "block");
    }
    
	function remove_fav_labels(){
		$('a.to_favorite').remove();
	}
	
