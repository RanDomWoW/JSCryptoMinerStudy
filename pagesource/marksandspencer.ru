                arResult = data;

                var aFavorite = '#favorite_'+id;

                $(aFavorite).removeClass('dell_favorite');
                $(aFavorite).removeClass('checked');
                $(aFavorite).addClass('add_to_fav');


                if(full === true)
                {
                    $(aFavorite).next().html('Добавить в избранное');
                    $(aFavorite).next().removeClass('dell_favorite');
                    $(aFavorite).next().addClass('add_to_fav');
                }
                if(quik !== false)
                {
                    $(quik).removeClass('dell_favorite');
                    $(quik).removeClass('checked');
                    $(quik).addClass('add_to_fav');
                }
                if(supports_html5_storage())
                {
                    var idsFavoriteLoc = localStorage["idFavorites"];
                    var obIdsFavorite = JSON.parse(idsFavoriteLoc);
                    delete obIdsFavorite[id];

                    localStorage.setItem("idFavorites", JSON.stringify(obIdsFavorite));

                    var i = 0;
                    for(var p in obIdsFavorite) {
                        i = i + 1;
                    }
                    
                    $("#favorite_num").html('<span class="num">'+i+'</span>');
                    $('#mobile_favorite_num').html(i);
                    
                }
                
                // refrash page  in favorite_list
                
                if( $('.personal_favorite').length || $('.public_favorite').length ){ 
					refrash_fav_list('delete');
				}
				
				if( $('.popup.quick_view:visible') ){
					create_fav_labels('quick_view');
				}
				 
                return false;
            }
            
        );

		
		
		
        return false;
