            var strFirstName = $("#txtname").val();
            var strLastName = $("#txtcname").val();
            var strEmail = $("#txtemail").val();
            var strMobileNo = $("#txtmobile").val();
            var strNoticePeriod = $("#txtmsg").val();
            // Make Ajax request with the contentType = false, and procesDate = false
            // alert("before ajax ")


            $.ajax({

                url: '/api/callapi/sendGetmail',
                type: 'get',
                dataType: 'json',
                data: { name: strFirstName,cname:strLastName,mob:strMobileNo,email:strEmail,msg:strNoticePeriod }

            })
            .done(function (data, textStatus, jqXHR) {
                alert("Your form has been submitted");
                $("#txtname").val("");
                $("#txtcname").val("");
                $("#txtemail").val("");
                $("#txtmobile").val("");
                $("#txtmsg").val("");
            })
        .fail(function (jqXHR, textStatus, errorThrown) {
            alert("Error");
        })
        .always(function (jqXHROrData, textStatus, jqXHROrErrorThrown) {
             location.reload();

        });




         
        }

    });
</script> 


