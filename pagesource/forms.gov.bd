    <script>
        $(document).ready(function () {
            $("#back-top").hide();
            $(function () {
                $(window).scroll(function () {
                    if ($(this).scrollTop() > 100) {
                        $('#back-top').fadeIn();
                    } else {
                        $('#back-top').fadeOut();
                    }
                });
                $('#back-top a').click(function () {
                    $('body,html').animate({scrollTop: 0}, 800);
                    return false;
                });
            });
        });

        $(document).ready(function () {
            $("#topintro").hide();
            $(function () {
                $(window).scroll(function () {
                    if ($(this).scrollTop() > 65) {
                        $(function () {
                            $("#topintro").show()
                            $("#topintro").animate({
                                opacity: 1
                            }, {
                                duration: 150,
                                queue: false
                            });
                            $("#topintro").animate({
                                "margin-top": "0px"
                            }, {
                                duration: 100,
                                specialEasing: {
                                    "margin-top": "easeOutCirc"
                                },
                                queue: false
                            });
                        });
                    } else {
                        $(function () {
                            $("#topintro").animate({
                                opacity: 0
                            }, {
                                duration: 150,
                                queue: false
                            });
                            $("#topintro").animate({
                                "margin-top": "45px"
                            }, {
                                duration: 50,
                                specialEasing: {
                                    "margin-top": "easeOutCirc"
                                },
                                queue: false
                            });

                        });
                    }
                });
            });
        });

    </script>
