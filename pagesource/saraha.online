<script>
		jQuery(document).ready(function(){
						jQuery(":file").filestyle({input: false});
			jQuery(":file").filestyle('buttonText', 'تحميل');
			jQuery('#dardacha').animate({'opacity': 0}, 1000, function () {
				jQuery(this).text('دردشة');
			}).animate({'opacity': 1}, 1000);
			jQuery('#sendie').on('keydown', function (event) {
				if (event.keyCode == 8 || event.keyCode == 46 || event.keyCode == 16 || event.keyCode == 17 || event.keyCode == 18 || event.keyCode == 27) {
					if(jQuery('#sendie').val().length < 2)
						jQuery(this).removeAttr("dir");
					else
						jQuery(this).attr('dir', 'auto');
				}else{
					jQuery(this).attr('dir', 'auto');
				}
				
			});
			jQuery('#submitregister').on('click', function () {
				jQuery('.registerloading').show();
			});
			jQuery('#sendie').on('focus', function () {
				jQuery(this).removeAttr("dir");
			});
			jQuery('#sendie').on('blur', function () {
				jQuery(this).removeAttr("dir");
			});
			jQuery('#sendie').on('focusout', function () {
				jQuery(this).removeAttr("dir");
			});
			var isMobile = window.matchMedia("only screen and (max-width: 768px)");

			if (isMobile.matches) {
				jQuery('#sendie').on('focus', function () {
					jQuery(".mobile-menu, #site-header").hide();
					jQuery(".chat-footer").css({"bottom":"-5px"});
					jQuery(".chat-header").css({"top":"0px"});
					jQuery("#chat-wrap").css({"top":"30px"});
				});
				jQuery('#sendie').on('focusout', function () {
					jQuery(".mobile-menu, #site-header").show();
					jQuery(".chat-footer").css({"bottom":"35px"});
					jQuery(".chat-header").css({"top":"initial"});
					jQuery("#chat-wrap").css({"top":"80px"});
				});
			}
		});
		(function() {

			var dardacha = $(".fading");
			var dardachaIndex = -1;
			
			function showNext() {
				++dardachaIndex;
				dardacha.eq(dardachaIndex % dardacha.length)
					.fadeIn(1000)
					.delay(1000)
					.fadeOut(1000, showNext);
			}
			
			showNext();
			
		})();
		(function() {

			var dardacham = $(".fadingm");
			var dardachaIndexm = -1;
			
			function showNextm() {
