            <script type="text/html" id="simple-tmpl">
                <![CDATA[

                <h2 class="title">Önnek ajánljuk</h2>

                <div class="swiper-container top-100-item-swiper">

                    <div class="swiper-wrapper interested-in-carousel">

                        {{ for (var i=0; i < SC.page.products.length; i++) { }}
                        <div class="swiper-slide item">
                            {{ var p = SC.page.products[i]; }}
                            {{ var categoryList = SC.page.products[i].category.split(">"); }}
                            <div class="inner row small-collapse item"
                                 data-list="Főoldali legnépszerűbb termékek"
                                 data-price="{{=p.price }}"
                                 data-id="{{= p.id }}"
                                 data-pos="{{=i+1 }}"
                                 data-brand="{{= p.brand }}"
                                 data-category="{{= categoryList.slice(-1)[0] }}"
                            >
                                <a href="{{=p.link}}" class="btn-more product-more-details">
                                    <div data-scarabitem="{{= p.id }}">
                                        <div class="photo columns small-5">
                                            <img class="swiper-lazy" src="https://webassets.praktiker.hu/images/100x100.png" data-src="{{=p.image.replace('600x600', '100x100')}}" alt="{{=p.title}}">
                                            <div class="swiper-lazy-preloader"></div>
                                        </div>
                                        <div class="excerpt columns small-7">
                                            <h3 class="overflow-text product-name">{{=p.title }}</h3>
                                            <div class="clearfix">
                                                {{ p.msrp = Math.round(p.msrp); }}
                                                {{ p.price = Math.round(p.price); }}
                                                {{ if(p.price < p.msrp) { }}
                                                <div class="old-price">{{=p.msrp.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1.") }} Ft</div>
                                                {{ } else { }}
                                                <div class="old-price">&nbsp;</div>
                                                {{ } }}
                                                <div class="price">{{=p.price.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1.") }} Ft</div>
                                            </div>
                                        </div>
                                    </div>
                                </a>
                            </div>
                        </div>
                        {{ } }}
                    </div>

                    <div class="swiper-button-next top100next swiper-button-black"></div>
                    <div class="swiper-button-prev top100prev swiper-button-black"></div>

                </div>

                ]]>
            </script>

        </div>

    </div>

</section>





