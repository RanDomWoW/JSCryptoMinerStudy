<script type="text/javascript" src="http://static.020.com/pc/js/libs/jquery-1.11.3.min.js"></script>
<script type="text/javascript" src="http://static.020.com/pc/js/libs/underscore-min.js"></script>
<script type="text/javascript">
    jQuery.fn.FocusIMG = function (opt) {
        var jQueryThis = $(this);
        var set = {
            showPreNext:true, //是否展示上一页下一页
            showTitle:true, //是否展示标题
            timespan:5000,  //时间间隔
            scrollBox:null,
            onChanngeEnd:function(i){}
        }
        $.extend(set, opt);
        var sWidth = jQueryThis.width(); //获取焦点图的宽度（显示面积）
        if(set.scrollBox!=null){
            sWidth=set.scrollBox.width();
        }
        var len = $("ul li", jQueryThis).length; //获取焦点图个数
        var index = 0;
        var picTimer;
        set.onChanngeEnd(0);
        //以下代码添加数字按钮和按钮后的半透明条，还有上一页、下一页两个按钮
        var btn = "<div class='btn'>";
        for (var i = 0; i < len; i++) {
            var curClass = '';
            if (i == 0) {
                curClass = 'cur';
            }
            btn += "<span class='" + curClass + "'></span>";
        }
        btn += "</div><div class='preNext pre'></div><div class='preNext next'></div>";
        jQueryThis.append(btn);
        //本例为左右滚动，即所有li元素都是在同一排向左浮动，所以这里需要计算出外围ul元素的宽度
        $("ul", jQueryThis).css("width", sWidth * (len));
        //标题
        if (!set.showTitle) {
            $("h3", jQueryThis).css("visibility", "hidden");
            jQueryThis.hover(function () {
                $("h3", jQueryThis).css("visibility", "visible");
            }, function () {
                $("h3", jQueryThis).css("visibility", "hidden");
            });
        }
        //上一页、下一页按钮透明度处理
        if (!set.showPreNext) {
            $(".preNext", jQueryThis).css("opacity", "0");
            jQueryThis.hover(function () {
                $(".preNext", jQueryThis).css("opacity", "0.7");
            }, function () {
                $(".preNext", jQueryThis).css("opacity", 0);
            });
        } else {
            $(".preNext", jQueryThis).css("opacity", "0.4").mouseenter(function () {
                $(this).stop(true, false).animate({"opacity":"0.7"}, 300);
            }).mouseout(function () {
                $(this).stop(true, false).animate({"opacity":"0.4"}, 300);
            });
        }
        if (/*Modernizr.touch*/false) {
            // if (true) {
            //iScroll for touch version
            var myScroll;
            var domId = $(this).attr('id'),scroll_domId=domId;
            if(set.scrollBox!=null){
                scroll_domId = set.scrollBox.attr('id');
            }
            var myScroll = new iScroll(scroll_domId, {
                vScroll:true,
                snap:true,
                momentum:false,
                hScrollbar:false,
                onScrollEnd:function () {
                    var currentLI = document.getElementById(domId).querySelector('div.btn > span:nth-child(' + (this.currPageX + 1) + ')');
                    if (currentLI) {
                        document.getElementById(domId).querySelector('div.btn > span.cur').className = '';
                        currentLI.className = 'cur';
                        set.onChanngeEnd(this.currPageX);
                    }
                }
            });
            //上一页按钮
            $(".pre", jQueryThis).click(function () {
                index -= 1;
                if (index == -1) {
                    index = len - 1;
                }
                myScroll.scrollToPage('prev', 0);
            });

            //下一页按钮
            $(".next", jQueryThis).click(function () {
                index += 1;
                if (index == len) {
                    index = 0;
                }
                myScroll.scrollToPage('next', 0);
            });
        } else {
            //pc version
            //为小按钮添加鼠标滑入事件，以显示相应的内容
            $(".btn span", jQueryThis).mouseenter(function () {
                index = $(".btn span", jQueryThis).index(this);
                showPics(index);
            });
            //上一页按钮
            $(".pre", jQueryThis).click(function () {
                index -= 1;
                if (index == -1) {
                    index = len - 1;
                }
                showPics(index);
            });

            //下一页按钮
            $(".next", jQueryThis).click(function () {
                index += 1;
                if (index == len) {
                    index = 0;
                }
                showPics(index);
            });
            //鼠标滑上焦点图时停止自动播放，滑出时开始自动播放
            jQueryThis.hover(function () {
                clearInterval(picTimer);
            },function () {
                picTimer = setInterval(function () {
                    index++;
                    if (index == len) {
                        index = 0;
                    }
                    showPics(index);
                }, set.timespan);
            }).trigger("mouseleave");

            //显示图片函数，根据接收的index值显示相应的内容
            function showPics(index) { //普通切换
                var nowLeft = -index * sWidth; //根据index值计算ul元素的left值
                $("ul", jQueryThis).stop(true, false).animate({"left":nowLeft}, 300); //通过animate()调整ul元素滚动到计算出的position
                $(".btn span", jQueryThis).removeClass('cur').eq(index).addClass('cur'); //为当前的按钮切换到选中的效果
                set.onChanngeEnd(index);
            }
        }
        return this;
    }
    $('#focus-image').FocusIMG({});

    $('#J_TabNav1 span').on('click',function(){
        var $this = $(this);
        $('#J_TabNav1 span.active').removeClass('active');
        $this.addClass('active');
        $('#J_TabContent1 .tab-content.active').removeClass('active');
        $('#J_TabContent1 .tab-content').eq($this.index()).addClass('active');
    });

    $('#J_TabNav3 span').on('click',function(){
        var $this = $(this);
        $('#J_TabNav3 span.active').removeClass('active');
        $this.addClass('active');
        $('#J_TabContent3 .tab-content.active').removeClass('active');
        $('#J_TabContent3 .tab-content').eq($this.index()).addClass('active');
    });

    var curTabContent = $('#J_TabContent2 .tab-content').eq(0);
    $('#J_TabNav2 span').on('click',function(){
        var $this = $(this);
        $('#J_TabNav2 span.active').removeClass('active');
        $this.addClass('active');
        $('#J_TabContent2 .tab-content.active').removeClass('active');
        curTabContent = $('#J_TabContent2 .tab-content').eq($this.index()).addClass('active');
    });

    var getListByColumnsAjax = false;
    function getListByColumns(columnIds){
        if(getListByColumnsAjax){
            return;
        }
        getListByColumnsAjax = true;
        curTabContent.find('.news-list-more').html('加载中...');
        $.ajax({
            url:'/api/article/getListByColumns.do',
            type:'GET',
            data:{
                columnIds:columnIds,
                ltPublishTime:window['column'+columnIds+'LtPublishTime']
            },
            dataType:'json',
            success:function(data){
                if(data.ret == 0){
                    if(data.list.length>0){
                        window['column'+columnIds+'LtPublishTime'] = data.ltPublishTime;
                        var compiled = _.template($('#tmpl-news-list-item').html());
                        var html = compiled(data);
                        curTabContent.find('.news-list').append(html);
                        curTabContent.find('.news-list-more').html('点击浏览更多');
                    }else{
                        curTabContent.find('.news-list-more').attr('href','javascript:;').html('没有更多');
                    }
                }else{
                    curTabContent.find('.news-list-more').html('加载失败，请稍后重试');
                }
                getListByColumnsAjax = false;
            },
            error:function(){
                curTabContent.find('.news-list-more').html('加载失败，请稍后重试');
                getListByColumnsAjax = false;
            }
        });
    }
</script>
