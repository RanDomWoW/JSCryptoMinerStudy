<script>
        $(document).ready(function(){
            $("#empresasBuscaButton").click( function (e){ e.preventDefault(); validabuscaempresa(); });
            
            $('#formempresas #keyword').focus(function(){
            	if($(this).val() == 'Digite o termo da pesquisa aqui') $(this).val('');
            });
            $('#formempresas #keyword').blur(function(){
            	if($(this).val() == '') $(this).val('Digite o termo da pesquisa aqui');
            });
        });
    
        function validabuscaempresa(){
        
            if($('#categoria').val().length < 1 || $('#categoria').val() == null){
                if($('#keyword').val().length < 1 || $('#keyword').val() == 'Digite o termo da pesquisa aqui'){
                    return false;
                }
                $('#categoria2').remove();
                $('#categoria').remove();
            }
            else{
                if($('#keyword').val().lenght < 1 || $('#keyword').val() == 'Digite o termo da pesquisa aqui'){
                    window.location = $('#categoria').val();
                    return false;
                }
                
                var tempcat = $('#categoria').val().split("/");
                $('#categoria2').val(tempcat[tempcat.length-1]);
                $('#categoria').remove();
            }   
            $('#formempresas').submit();
        }
    </script>
