$(document).ready(function(){



$("#frmCreate").on('submit',(function(e) {
	e.preventDefault();
	// $("#message").empty();
	// $('#loading').show();
	jQuery('#loading-mask').toggle();
	$.ajax({
	url: "http://web.bnndev.go.id:88/user/xhr_ajax?state=ubah_profil",
	type: "POST",
	data: new FormData(this),
	contentType: false,
	cache: false,
	processData:false,
	statusCode: {
		400: function (response) {
			alert('Code 400: Bad Request. Coba beberapa saat lagi.');
			jQuery('#loading-mask').hide();
		},
		404: function (response) {
			alert('Code 404: Not Found. Coba beberapa saat lagi.');
			jQuery('#loading-mask').hide();
		},
		408: function (response) {
			alert('Code 408: Request Timeout. Coba beberapa saat lagi.');
			jQuery('#loading-mask').hide();
		},
		500: function (response) {
			alert('Code 500: Internal Server Error. Coba beberapa saat lagi.');
			jQuery('#loading-mask').hide();
		},
		504: function (response) {
			alert('Code 504: Gateway Timeout. Coba beberapa saat lagi.');
			jQuery('#loading-mask').hide();
		}
	},
	success: function(response)   // A function to be called if request succeeds
	{
		// jQuery('#loading-mask').hide();
		response = response.substr(0, response.length-1);
		response = response.split('[[SPLIT]]');
		//jQuery('#loading-mask').hide();			
		jQuery('div#xhr_adapter').html(response[1]);
	}
	});
}));




	
