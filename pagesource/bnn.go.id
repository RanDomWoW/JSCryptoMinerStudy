<script>
		
			/* Drop Down */
			$(document).ready(function() {
				var hidden = true;
				$('.dd-link').hover(function(e) {
					e.preventDefault();
					if (hidden){
					   $(this).next('.dd-dropdown').fadeToggle(200, function(){hidden = false;});
					}
				});

				$('html').click(function() {
					if (!hidden) {
						$('.dd-dropdown').fadeOut();
						hidden=true;
					}
				});

				$('.dd-dropdown').click(function(event) {
					event.stopPropagation();
				});
				
				
				
