    <script type="text/javascript">
        $(function () {




            $('.bxslider').bxSlider({
                mode: 'horizontal',
                captions: false,
                pager: false,
                auto: true,
                autoHover: true
            });

            if (useNoticePop == 1) {
                var haveSeenNotice = Cookies.get('haveSeenNotice' + noticeCode);

                HLog("haveSeenNotice: " + haveSeenNotice);

                if (!haveSeenNotice) {
                    // 7일간 보지않기를 누르지 않더라도 3회이상 노출되면 노출되지 않도록 조치한다.
                    var noticeViewCount = Cookies.get('noticeViewCount' + noticeCode);
                    if (!noticeViewCount) {
                        noticeViewCount = 1;
                    } else {
                        noticeViewCount = parseInt(noticeViewCount) + 1;
                    }

                    if (noticeViewCount < 4) {
                        Cookies.remove('noticeViewCount' + noticeCode);
                        Cookies.set('noticeViewCount' + noticeCode, noticeViewCount, { expires: 7 });
                        $('#noticeModal').modal('show');
                    }
                }
            }
        });

        function setNoticeCloseCookie(noticeCode) {
            //HLog("setNoticeCloseCookie. noticeCode:" + noticeCode);
            //HLog("setNoticeCloseCookie. notice-close-check:" + $("#notice-close-check").is(":checked"));
            if ($("#notice-close-check").is(":checked")) {
                Cookies.set('haveSeenNotice' + noticeCode, '1', { expires: 7 });
            }
        }
    </script>

