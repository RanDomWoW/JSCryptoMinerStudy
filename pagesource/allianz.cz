  });

  $("#js-float").click(function(){
    $("html, body").animate({scrollTop:0},600);
    return false;
  });

  $(window).on("scroll resize", function(){
    var a = $("#js-float");

    if ($(document).height() > 1.3 * $(window).height()){
      if($(this).scrollTop()>400){
        a.css('opacity', '1');
      }
      else {
        a.css('opacity', '0');
      }
    } else {
        a.css('opacity', '0');
    }
  });

