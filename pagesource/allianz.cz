<script type="text/javascript" src="/public/d0/45/da/42674_147778_form_utils.js"></script>
<script>
var LFQ = LFQ || {};

LFQ.initDisplay = function () {
	window.console && console.log("LFQ display initialization - begin");
	LFQ.DisplayPointer = null;
	LFQ.DisplayTitles = new Array();
	LFQ.DisplayDiscounts = new Array();

	var mainUl = document.getElementById("js-display-items");
	if (mainUl) {
		// getting values
		var title, discount;
		var data = mainUl.getElementsByTagName("li");
		for (i = 0; i < data.length; i++) {
			title = data[i].getElementsByTagName("strong")[0].textContent;
			LFQ.DisplayTitles.push(title);
			discount = data[i].getElementsByTagName("em")[0].textContent;
			LFQ.DisplayDiscounts.push(discount);
		}
		// setting pointer
		LFQ.DisplayPointer = 0;
		// binding events
		document.getElementById("js-display-next").addEventListener("click", LFQ.nextViewOnDisplay);
		document.getElementById("js-display-previous").addEventListener("click", LFQ.previousViewOnDisplay);
		// open first item
		LFQ.setDisplayValue();
		window.console && console.log("LFQ display initialization - end");
	} else {
		window.console && console.log("LFQ display was not found");
	}
};

LFQ.setDisplayValue = function () {
	var newValue;
        var newText;

	if (LFQ.DisplayDiscounts[LFQ.DisplayPointer] <= 0) {
		newValue = "0%";
		newText = "Nesplňujete podmínky pro udělení slevy.";
	} else {
		newValue = "─&thinsp;" + LFQ.DisplayDiscounts[LFQ.DisplayPointer] + "%";
		newText = "Kolik bude vaše sleva?";
	}

	var text = document.getElementById("js-hp-text");
	text.textContent = newText;

	var display = document.getElementById("js-display");
	display.innerHTML = newValue;

	// setting title
	var mainUl = document.getElementById("js-display-items");
	var data = mainUl.getElementsByTagName("li");
	for (i = 0; i < data.length; i++) {
		if (i == LFQ.DisplayPointer) {
			data[i].className = "life__display-item active";
		} else {
			data[i].className = "life__display-item";
		}
	}
};

LFQ.nextViewOnDisplay = function () {
	if (LFQ.DisplayPointer < (LFQ.DisplayDiscounts.length - 1)) {
		LFQ.DisplayPointer++;
	} else {
		LFQ.DisplayPointer = 0;
	}

	LFQ.setDisplayValue();
};

LFQ.previousViewOnDisplay = function () {
	if (LFQ.DisplayPointer > 0) {
		LFQ.DisplayPointer--;
	} else {
		LFQ.DisplayPointer = (LFQ.DisplayDiscounts.length - 1);
	}

	LFQ.setDisplayValue();
};
</script>
<script type="text/javascript">
document.addEventListener('DOMContentLoaded', function() {
	var carousels = document.querySelectorAll('[data-mmkCarID]');
