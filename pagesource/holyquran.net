var blurVal = 51;
var acc = 2;
var strength = 30;
var stopAt;
var maxSeconds = 60;
var moveInterval;
var closeInterval;

function Initialize()
{
	popnews.style.filter = 'progid:DXImageTransform.Microsoft.Shadow(color=#BBBBBB, Direction=135, Strength=6) progid:DXImageTransform.Microsoft.MotionBlur(strength=' + strength + ', direction=0) alpha(opacity=80)';
	popnews.style.fontFamily = 'Arabic Transparent';
	popnews.style.fontSize = '11pt';
	popnews.style.pixelLeft = (document.body.clientWidth - popnews.style.pixelWidth) / 2;
	stopAt = (document.body.clientHeight - popnews.style.pixelHeight) / 2 - strength;
	StartTimer();
}

function Fly(id)
{
	acc = acc * 2;
	if (id.style.pixelTop + acc >= stopAt)
	{
		id.style.pixelTop = stopAt + 1;
		clearInterval(moveInterval);
		moveInterval = '';
		Unblur(popnews);
	}
	else
	{
		id.style.pixelTop = id.style.pixelTop + acc; 
	}
}

function StartTimer()
{
	moveInterval = window.setInterval("Fly(popnews)",50);
}

function Unblur(id)
{
	blurVal = 0;
	id.style.filter = 'progid:DXImageTransform.Microsoft.Shadow(color=#BBBBBB, Direction=135, Strength=6); progid:DXImageTransform.Microsoft.MotionBlur(strength=' + blurVal + ', direction=0) alpha(opacity=95)';
	id.style.pixelTop = id.style.pixelTop + strength;
	timerID.innerHTML = maxSeconds;
	closeInterval = window.setInterval('ShowTime()', 1000);
}

function displayWindows()
{
	alert(document.body.clientWidth);
}

function CloseDiv()
{
	popnews.style.display = 'none';
	closeTimeout = "";
}
function ShowTime()
{
	maxSeconds -= 1;
	if (maxSeconds <= 0 )
	{
		clearInterval(closeInterval);
		closeInterval = '';
		CloseDiv();
	}
	else
	{
		timerID.innerHTML = maxSeconds;
	}
}


