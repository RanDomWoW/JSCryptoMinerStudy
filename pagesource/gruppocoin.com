<script>
  function CookieManager($, options) {
    var DEFAULTS = {
      scroll_accept: false,
      script_kinds: [],
      debug: false,
    };
  
    var _options = {}, _scripts = {}, _cookie_values = {}, _this = this;
    _options = $.extend( true, {}, DEFAULTS, options );
    var _main_domain = document.domain.split('.').slice(-2).join('.');
  
  
    if(_options.debug) console.log('Starting CookieManager...');
  
    _this.cookie_policy_accepted = false;
  
    collectScripts();
    readCookies();
  
    if(!_this.cookie_policy_accepted) {
      $("#cookie-manager-banner").fadeIn('fast');
  
      $(document).on('click'+(_options.scroll_accept?' scroll':''), function(e){
        if(!_this.cookie_policy_accepted) {
          e.stopPropagation();
          if (!$(e.target).closest('.cookie-policy').length) {
            _this.acceptAllCookies();
          }
        }
      });
  
      $(".cookie-manager .cookies-ok").click(function(e){
        e.preventDefault();
        _this.acceptAllCookies();
      });
  
    } else {
      enableAllScripts();
    }
  
    //private methods
    function collectScripts() {
  
      //sorting them in order to have remote scripts loaded before local ones
      for(var idx in _options.script_kinds){
        var kind = _options.script_kinds[idx];
        _scripts[kind] = $("script.cookie-manager." + kind).sort(function(a,b){
          return $(a).prop("src") ? a : b;
        }).toArray();
      }
    }
  
    function readCookies(){
      _this.cookie_policy_accepted = ($.cookie('cookie_manager_policy_accepted') == "true");
  
      for(var idx in _options.script_kinds){ //loading actual values
        var kind = _options.script_kinds[idx];
        if(_options.debug) console.log('Reading cookie '+ kind + ": " +$.cookie("cookie_manager_cookie_" + kind  +"_enabled"));
        _cookie_values[kind + "_enabled"] = ($.cookie("cookie_manager_cookie_" + kind  +"_enabled") == "true");
      }
    }
  
    function retrieveJS(src, callback) {
       if(_options.debug) console.log('Loading async '+ src);
       var s = document.createElement('script');
       s.src = src;
       s.async = true;
       s.status = "enabled";
       s.onreadystatechange = s.onload = function() {
           var state = s.readyState;
           if (!callback.done && (!state || /loaded|complete/.test(state))) {
               callback.done = true;
               callback();
           }
       };
       document.getElementsByTagName('head')[0].appendChild(s);
     }
  
    function enableScripts(kind){
  
      if(!_cookie_values[kind + "_enabled"]){
        if(_options.debug) console.log("Not enabling " + kind +" scripts");
        return;
      }
  
  
      if(_scripts[kind].length > 0){
        $("body").append("<!-- Dinamically restored " + kind +" script -->");
        var script = _scripts[kind].shift(); //dequeue scripts
        var $script = $(script);
        var name = $script.data("alias") || $script.prop("src");
  
        if($script.prop("src")){
          if(_options.debug) console.log('Loading remote script '+ name);
          retrieveJS($script.prop("src"),function(){
            if(_options.debug) console.log('Remote script '+ name + ' loaded');
            $script.remove(); //remove commented script
            enableScripts(kind)
          });
        }
        else{
  
          if(_options.debug) console.log('Loading in page script '+ name);
          $("body").append($("<script />", {
            html: $script.html(),
            status: "enabled"
          }));
  
          if(_options.debug) console.log('Page script '+ name + ' inserted');
          $script.remove();
          enableScripts(kind);
        }
  
      }
    }
  
    function enableAllScripts(){
      if(_options.debug) console.log('Enabling scripts...');
  
      $("#cookie-manager-banner").fadeOut('fast');
  
      for(var idx in _options.script_kinds)
        enableScripts(_options.script_kinds[idx]);
    }
  
    //public methods
    this.acceptAllCookies = function() {
      $.cookie('cookie_manager_policy_accepted', true, { domain: _main_domain, expires: 365 });
  
      //rewrite all cookies based on user changes
      var cookie_kinds = _options.script_kinds;
      for(var _id = 0; _id < cookie_kinds.length; _id++){
        var cookie_kind = cookie_kinds[_id];
        $.cookie("cookie_manager_cookie_" +cookie_kind + "_enabled", true, { domain: _main_domain, expires: 365 });
      }
  
      readCookies();
  
      enableAllScripts();
    }
  
  }
  
  jQuery(function($) {
    new CookieManager($, {});
  });
</script>
