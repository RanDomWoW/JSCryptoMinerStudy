function airlinesSearch(alsValue, alsValueHidden){
	$('.airLinesList a img').each(function() {
		$(this).attr('src', $(this).attr('src').replace('_on', '_off'));
	});
	if(alsValue){
		$("."+alsValue+" a img").attr('src', $("."+alsValue+" a img").attr('src').replace('_off', '_on'));
		$("#s_airs").val(alsValueHidden);
	}
	var alsValueNow = alsValue;
	depArrChange(alsValue);
	SearchInputCheck();
}

function depArrChange(daValue){
	$("#departure").empty();
	$("#arrival").empty();
	$.each(depArrListArray[daValue], function(i, value) {
	var splitTerm = value.split( ':' );
		$('#departure').append($('<option>').html(splitTerm[1]).val(splitTerm[0]));
		$('#arrival').append($('<option>').html(splitTerm[1]).val(splitTerm[0]));
	});
}

