function SearchInputCheck(){
	var inputFlg = 0;

	if(!$('#departure').val()){
		$("#departure").css("color","#999999");
		$("#departure option:nth-of-type(n+2)").css("color","#333333");
		$("#departure").css("border-color","#cccccc");
		$("#departure").css("background","url(img/rn/select-box-arrow_off.png) #ffffff right center no-repeat");
		var inputFlg = inputFlg + 1;
	}else{
		$("#departure").css("color","#333333");
		$("#departure").css("border-color","#999999");
		$("#departure").css("background","url(img/rn/select-box-arrow.png) #ffffff right center no-repeat");
	}
	if(!$('#arrival').val()){
		$("#arrival").css("color","#999999");
		$("#arrival option:nth-of-type(n+2)").css("color","#333333");
		$("#arrival").css("border-color","#cccccc");
		$("#arrival").css("background","url(img/rn/select-box-arrow_off.png) #ffffff right center no-repeat");
		var inputFlg = inputFlg + 1;
	}else{
		$("#arrival").css("color","#333333");
		$("#arrival").css("border-color","#999999");
		$("#arrival").css("background","url(img/rn/select-box-arrow.png) #ffffff right center no-repeat");
	}
	if(!$('#depDate').val()){
		$("#depDate").css("border-color","#cccccc");
		$("#depDate").css("background","url(img/rn/select-box-icon01_off.gif) #ffffff right center no-repeat");
		var inputFlg = inputFlg + 1;
	}else{
		$("#depDate").css("border-color","#999999");
		$("#depDate").css("background","url(img/rn/select-box-icon01.gif) #ffffff right center no-repeat");
	}

	if(inputFlg >= 1){
		$("#searchBtn").attr("src", "img/rn/search_btn01_off.jpg");
		$(".searchBtn_on").css("display", "none");
		$(".searchBtn_off").css("display", "block");
	}else{
		$("#searchBtn").attr("src", "img/rn/search_btn01.jpg");
		$(".searchBtn_on").css("display", "block");
		$(".searchBtn_off").css("display", "none");
	}
}
