<script type="text/javascript">
	$(document).ready(function ()
	{
		$('.updates_widget_js').updatesWidget({"host":"stream.aif.by","chanel":"updates","debug":false,"itemsTypes":["content","textbroadcast_post"]});
	});
</script>
<script id="updates_content_tpl_js" type="text/x-jquery-tmpl">
	<li class="updates_item_js updates_content_${id}_js" data-type="content" data-id="${id}">
		<a href="#" class="closer close_js"></a>
		<div class="time">${published_time}</div>
		<a href="${url}" class="img_box item_link_js" target="_blank">
			<img src="${img}" />
		</a>
		<a href="${url}" class="title item_link_js" target="_blank">
			<h2>${title}</h2>
			<span class="link_spanner"></span>
		</a>
		<div class="lead">${lead}</div>
		<!-- <a href="#" class="blue_link item_link_js" target="_blank">Читать далее</a> -->
	</li>
</script>
<script id="updates_comment_tpl_js" type="text/x-jquery-tmpl">
	<li class="updates_item_js updates_comment_${id}_js" data-type="comment" data-id="${id}">
		<a href="${url}" class="closer close_js"></a>
		<div class="time">${published_time}</div>
		<div class="author_avatar">
			<img src="${userAvatar}" />
		</div>
		<div class="user_name">
			<b>${userName}</b> <span>только что оставил комментарий:</span>
		</div>
		<div class="comment_text">${text}</div>
		<a href="${url}" class="item_link_js blue_link" target="_blank">
			Ответить ${userName}
			<span class="link_spanner"></span>
		</a>
	</li>
</script>
<script id="updates_textbroadcast_post_tpl_js" type="text/x-jquery-tmpl">
	<li class="updates_item_js updates_textbroadcast_post_${id}_js" data-type="textbroadcast_post" data-id="${id}">
		<a href="#" class="closer close_js"></a>
		<div class="time">${published_time}</div>
		<a href="${url}" class="img_box item_link_js" target="_blank">
			<img src="${img}" />
		</a>
		<a href="${url}" class="title item_link_js" target="_blank">
			<h2>${title}</h2>
			<span class="link_spanner"></span>
		</a>
		<div class="lead">${text}</div>
		<a href="#" class="blue_link item_link_js" target="_blank">Читать трансляцию</a>
	</li>
</script>

