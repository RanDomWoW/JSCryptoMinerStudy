<!-- 获取Uc的用户信息开始 -->
 <script>
    window.Intercom && window.Intercom("boot", {
	    app_id:"t38a3kta"
    });
    var iframe = document.createElement('iframe');
    iframe.style.position = 'absolute';
    iframe.style.top = '-9999px';
    iframe.src = 'https://uc.eztalks.com/static/html/website.html';
    document.body.appendChild(iframe);
    iframe.onload = function () {
      iframe.contentWindow.postMessage(JSON.stringify({
        type: 'uc-user'
      }), '*');
    };
    window.addEventListener('message', function (e) {
      if (e.origin.indexOf('eztalks') < 0 && e.origin.indexOf('test') < 0 && e.origin.indexOf('192.168.32.124') < 0) {
        return;
      };
      var data;
      try {
        data = JSON.parse(e.data);
      } catch (e) {
        return;
      }
      if (data.type === 'uc-user-back') {
        var obj = data.data;
      	console.log("UC返回的用户信息" + JSON.stringify(obj));
        if (obj === null) {
			if (document.querySelector("#country_contact") != null) {
			 hanlderPhoneNum("0");
          }
          return
        }
        if (obj.email != null && obj.email != '') {
			// 处理电话页面的显隐
			hanlderPhoneNum(obj.audio);
			
			var headDiv = document.querySelector("#loginHeader");
			var noLoginHeadDiv = document.querySelector("#noLoginHeader");
			var nameImg = document.querySelector(".signIcon");
			headDiv.style.display = "block";
			noLoginHeadDiv.style.display = "none";
			
			// about/sales页面替换用户邮箱
			var ucEmail = document.querySelector("input[name='agile-form[ucEmail]']");
			if (ucEmail != null) {
				ucEmail.value = obj.email;
			}
			
			if (obj.name != null && obj.name != '') {
				nameImg.innerText = obj.name.charAt(0).toUpperCase();
			}

	        window.Intercom && window.Intercom("update", {
			  app_id:"t38a3kta",

	          email: obj.email,
	          phone: obj.phone,
	          created_at: obj.created_at,
	          name: obj.name,
	          user_id: obj.user_id,
	          meetings_plan: obj.meetings_plan,
	          rooms_plan: obj.rooms_plan,
	          active_time: obj.active_time,
	          company: {
	          	id: obj.company.id,
	          	name: obj.company.name,
	          	size: obj.company.size,
	          	industry: obj.company.industry,
	          	website: obj.company.website
	          }
	        });
        }
      }
    });
	
	// 处理电话号码页面
    function hanlderPhoneNum(val) {
		 if(val==="0"){
	        $('#country_contact .free').show()
	        $('#country_contact .basic').hide()
	        $('#country_contact .pro').hide()
	      }else if(val==="1"){
	        $('#country_contact .basic').show()
	        $('#country_contact .free').hide()
	        $('#country_contact .pro').hide()
	      }else if(val==="2"){
	        $('#country_contact .pro').show()
	        $('#country_contact .free').hide()
	        $('#country_contact .basic').hide()
	      }
    }
  </script>
<!-- 获取Uc的用户信息结束 -->


