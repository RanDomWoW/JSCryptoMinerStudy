 $(".theChampTCBackground").on('DOMSubtreeModified', function() {
        var currentCounter = $(this).find(".theChampTotalShareCount").html();
        if(typeof currentCounter == "undefined" || currentCounter == ""){
            currentCounter = 0;
        }
        $(this).closest('.post').find(".share_count").html(currentCounter);
        //console.log("evento");
    });

    setTimeout(function(){
        $('.share_count').each(function() {
            $(this).html("0");
        });
        $('.the_champ_sharing_container').each(function() {
            var currentCounter = $(this).find(".theChampTotalShareCount").html();
            if(typeof currentCounter != "undefined"){
                if(currentCounter == ""){
                    currentCounter = 0;
                }
                $(this).closest('.post').find(".share_count").html(currentCounter);
            }
        });
    }, 1000);

    });
