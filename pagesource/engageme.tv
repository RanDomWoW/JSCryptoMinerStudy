<!-- Include js plugin -->
<script src="/assets/owl-carousel/owl.carousel.js"></script>
<script type="text/javascript" charset="utf-8">
var way_to_earn = $('.way-to-earn-section').html();
  function mobile() {
    var checkWidth = $(window).width();
    var owl = $("#owl-demo");
    if(checkWidth <=768){
      owl.owlCarousel({
        navigation : false, // Show next and prev buttons
        slideSpeed : 300,
        paginationSpeed : 400,
        singleItem:true,
        items : 1,
        itemsMobile : true,
        itemsDesktop : false,
        itemsTablet: true,
        singleItem:true,
        center:true,
        itemsDesktopSmall : false,
      });
      $('.owl-controls').show();
    }else{
	  owl.trigger('destroy.owl.carousel');
	  $('.way-to-earn-section').html(way_to_earn);
	  owl = $("#owl-demo"); 
      owl.find('.owl-wrapper').prop("style", "");
      owl.find('.owl-item').prop("style", "");
      owl.removeClass('owl-carousel');
      $('.owl-controls').hide();
    }
  }
  $(document).ready(mobile);
  $(window).resize(mobile);


</script>