	<script>
				function convertcurrency(value)
					{
						var mycurrency	=	$(".mycurrency").val();
							switch(mycurrency)
								{

																						case "CAD": case "cad":  
														var finalvalue	=	Number(value)*1.27;
														finalvalue= Number(finalvalue); 
														return finalvalue.toFixed(2);
													break;
																						case "AUD": case "aud":  
														var finalvalue	=	Number(value)*1.3;
														finalvalue= Number(finalvalue); 
														return finalvalue.toFixed(2);
													break;
																						case "USD": case "usd":  
														var finalvalue	=	Number(value)*1;
														finalvalue= Number(finalvalue); 
														return finalvalue.toFixed(2);
													break;
																						case "GBP": case "gbp":  
														var finalvalue	=	Number(value)*0.75;
														finalvalue= Number(finalvalue); 
														return finalvalue.toFixed(2);
													break;
																						case "EUR": case "eur":  
														var finalvalue	=	Number(value)*0.84;
														finalvalue= Number(finalvalue); 
														return finalvalue.toFixed(2);
													break;
																						case "NZD": case "nzd":  
														var finalvalue	=	Number(value)*1.42;
														finalvalue= Number(finalvalue); 
														return finalvalue.toFixed(2);
													break;
																						case "CHF": case "chf":  
														var finalvalue	=	Number(value)*0.99;
														finalvalue= Number(finalvalue); 
														return finalvalue.toFixed(2);
													break;
																						case "DKK": case "dkk":  
														var finalvalue	=	Number(value)*6.27;
														finalvalue= Number(finalvalue); 
														return finalvalue.toFixed(2);
													break;
																						case "NOK": case "nok":  
														var finalvalue	=	Number(value)*8.33;
														finalvalue= Number(finalvalue); 
														return finalvalue.toFixed(2);
													break;
																						case "PLN": case "pln":  
														var finalvalue	=	Number(value)*3.53;
														finalvalue= Number(finalvalue); 
														return finalvalue.toFixed(2);
													break;
																						case "SEK": case "sek":  
														var finalvalue	=	Number(value)*8.35;
														finalvalue= Number(finalvalue); 
														return finalvalue.toFixed(2);
													break;
																		default: 
										return value;
									break;
								}
					}

				function symbolcurrency()
					{
						var mycurrency	=	$(".mycurrency").val();	
							return mycurrency;
					}
	
				function old_convertcurrency(value)
					{
						var countryname	=	$(".getcountryname").val();
							switch(countryname)
								{
									case "Australia": case "Australia":
										var finalvalue	=	Number(value)*1.30;
										finalvalue= Number(finalvalue); 
										return finalvalue.toFixed(2);
									break;
									case "Canada": case "canada":  
										var finalvalue	=	Number(value)*1.27;
										finalvalue= Number(finalvalue); 
										return finalvalue.toFixed(2);
									break;
									case "United States": case "united states":  case "united States": case "United states":  
										return value;
									break;
									case "United Kingdom": case "united kingdom":  case "United kingdom": case "united Kingdom":  
										var finalvalue	=	Number(value)*0.75;
										finalvalue= Number(finalvalue); 
										return finalvalue.toFixed(2);
									break;
									default: 
										/* var finalvalue	=	Number(value)*0.87;
										finalvalue= Number(finalvalue); 
										return finalvalue.toFixed(2); */
										return value;
									break;
								}
					}
					
				function old_symbolcurrency()
					{
						var countryname	=	$(".getcountryname").val();
						switch(countryname)
							{
								case "Canada": case "canada": 
									return "CAD";
								break;
								case "United States": case "united states":  case "united States": case "United states": 
									return "USD";
								break;
								case "Australia": case "Australia":  
									return "AUD";
								break;
								case "United Kingdom": case "united kingdom":  case "United kingdom": case "united Kingdom":  
									return "GBP";
								break;
								default: 
									return "USD";
								break;
							}
					}

				function stripeamount(value)
					{
						value = value.toString();
							value = value.replace(".", "");
								value = Number(value)*100;
									value = value/100;
										value = value.toString();
											return value = value.replace(".", "");
					}
					
						
