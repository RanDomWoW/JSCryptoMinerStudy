    $(window).bind("load", function() { 
        var loadVideo = false;
        var topVideo = $('.toradex-video').offset().top - $(window).height();
        if(topVideo < 500) {
            topVideo = 768;
        }
        topVideo += 100;

        var htmlVideo = '<video id="bgvid" autoplay loop muted><source src="https://docs.toradex.com/103722-home-page-video-webm.webm" type="video/webm"><source src="https://docs.toradex.com/103721-home-page-video-mp4.mp4" type="video/mp4"></video>';
        if($(window).scrollTop() > topVideo) {
            if(!loadVideo) {
                $('#video-section').html(htmlVideo);
            }
            loadVideo = true;
        } else {
            $(window).scroll(function() {
                var scroll = $(this).scrollTop();
                if(scroll > topVideo) {
                    if(!loadVideo) {
                        $('#video-section').html(htmlVideo);
                    }
                    loadVideo = true;
                }
            });
        }
    });
    $(function(){
        $('#popup').hide();
        setTimeout(function(){
            $('#popup').show();
            if (!$.cookie('contact_popup') || $.cookie('contact_popup') == 'open'){    
                $('#popup-form').slideDown('fast');
            } else {
                $('#popup-form').slideUp('fast');
            }
        },15000);

    });

    // Toggle Panel and Set Cookie //
    $('#questions').click(function(){        
        $('#popup-form').slideToggle('fast', function(){
            if ($(this).is(':hidden')) {
                $.cookie('contact_popup', 'closed');
            } else {
                $.cookie('contact_popup', 'open');
            }
        });
    });
});</script>
