<script>
    $(document).ready(function () {

        var url = '/News/CallRss';
        $.getJSON(url,
            function (data) {


                var xmlDoc = $.parseXML(data);

                var $xml = $(xmlDoc);
                var $xmlcontent = $xml.find("item");
                var count = 0;
                $xmlcontent.each(function (i, el) {
                    if (count < 5) {
                        var $categories = $(el).find("category");
                        $categories.each(function (index, element) {
                            if (element.innerHTML.includes('imp')) {
                                makeNews(el);
                                count++;
                            }
                        });
                    }
                });
            });
    });
    function makeNews(el) {
        var i = $(el);
        var baseDiv = $('#newsList');
        var articlesDiv = $('<div class="articles-list__item"></div>');
        var link = $('<a href="' + i.find('link').text() + '"></a>');
        var description = $.parseHTML(i.find('description').text());
        var imgsrc = description[0].src;
        var image = '<img class="articles-list__thumbnail pull-right" src="' + imgsrc + '" alt="' + i.find('title').text() + '" />';
        var pullright = $('<div class="articles-list__data pull-right"></div>');
        var h3title = '<h3 class="articles-list__article-title newsTitle">' + i.find('title').text() + '</h3>';
        /*var dateDiv = '<div class="articles-list__date">' + i.find('pubDate').text() + '</div>';*/
        var clarifix = '<div class="clearfix"></div>';

        //concat
        var pullrightcomplete = pullright.append(h3title);//.append(dateDiv)
        var linkcomplete = link.append(image).append(pullrightcomplete).append(clarifix);
        var articlecomplete = articlesDiv.append(linkcomplete);
        baseDiv.append(articlecomplete);
    };
</script>
