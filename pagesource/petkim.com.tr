<script type="text/javascript">

    $(document).ready(function () {

        //#region Property

        $('ul.sf-menu').superfish();

        //#endregion

        //#region User Define Functions

        function pagerFactory(idx, slide) {
            var s = idx ? ' ' : '';
            return '<li' + s + ' title="' + (idx + 1) + '"><a href="#">' + (idx + 1) + '</a></li>';
        };

        //#endregion

        //#region Events

        $("#Ara").click(function (e) {

            var kw = $("#txtSearch").val();
            var Dil = $("#Header1_Ara").attr("lang");
            if (kw != "Sitede Ara..." && kw != "Search" && kw != "") {
                window.location.href = "/search.aspx?kw=" + kw + "&Id_Lang=" + Dil; ;

            }

        });

        $(document).keyup(function (event) {
            var key = event.keyCode || event.charCode || 0;
            if (key == 13) {
                var kw = $("#txtSearch").val();
                var Dil = $("#Header1_Ara").attr("lang");
                if (kw != "Sitede Ara..." && kw != "Search" && kw != "") {
                    window.location.href = "/search.aspx?kw=" + kw + "&Id_Lang=" + Dil;

                }
            }
        });

        $('.MainTop li').hover(
		                                    function () {
		                                        //show onlineislemler submenu
		                                        $('ul', this).fadeIn("fast");

		                                    },
		                                    function () {
		                                        //hide onlineislemler submenu
		                                        $('ul', this).fadeOut("fast");
		                                    }
		                            );

        $('.MainTop li input').click(
		function () {
		    if ($(this).val() == "Sitede Ara..." && $(this).val() != "Search") {
		        $(this).val("");
		    }
		}
		);

        $('.MainTop li input').blur(
		    function () {
		        if ($(this).val() == "Sitede Ara..." && $(this).val() != "Search") {
		            $(this).val('');
		        }
		    }
		);

        var originalFontSize = $('#maincontent').css('font-size');

        $("#Font-Reset").click(function () {

            $('#maincontent').css('font-size', originalFontSize);

        });

        $("#Font-Asc").click(function () {

            var currentFontSize = $('#maincontent').css('font-size');

            var currentFontSizeNum = parseFloat(currentFontSize, 10);

            var currentLineHeight = $('#maincontent').css('line-height');

            var currentLineHeightNum = parseFloat(currentLineHeight, 10);

            var newFontSize = currentFontSizeNum + 1;
            if (newFontSize < 24) {
                $('#maincontent').css('font-size', newFontSize);
                $('#maincontent').css('line-height', currentLineHeightNum + 1 + "px");
            }
            return false;

        });

        $("#Font-Desc").click(function () {

            var currentFontSize = $('#maincontent').css('font-size');

            var currentFontSizeNum = parseFloat(currentFontSize, 10);


            var currentLineHeight = $('#maincontent').css('line-height');

            var currentLineHeightNum = parseFloat(currentLineHeight, 10);

            var newFontSize = currentFontSizeNum - 1;
            if (newFontSize > 8) {
                $('#maincontent').css('font-size', newFontSize);
                $('#maincontent').css('line-height', currentLineHeightNum - 1 + "px");
            }
            return false;

        });

        $('.fancybox-media').fancybox({
            openEffect: 'none',
            closeEffect: 'none',
            helpers: {
                media: {}
            }
        });

        if (location.hash.substring(1) == "anar-mammadov") { $(".anar-mammadov").trigger('click') }

        //#endregion

    });
                   
</script>
