  <section id="acciones" class="container">
    <div class="row aligned-row">
      <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
        <!--          <div class="textAccion">-->
        <h3>Quiero hacer un envío</h3>
        <!--</div>-->
        <a href="https://www2.mrw.es/yatelollevo/" target="_blank" class="btn btn-default btn-ambito" role="button">NACIONAL</a>
        <a href="https://www.mrwinternacional.es/ytll/" target="_blank" class="btn btn-default btn-ambito" role="button">INTERNACIONAL</a>
      </div>
      <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
        <h3>¿Dónde está mi envío?</h3>
        <form class="form-inline" action="/seguimiento_envios/validar_seguiment.asp" method="post" id="seguimiento" name="seguimiento">
          <div class="form-group">
            <div class="input-group">
              <input type="text" class="form-control" name="mrw-finder-follow-code" placeholder="N&uacute;mero de envío" id="mrw-finder-follow-code" pattern="[^*'%()/<>[\]\\,'|\x22]{12,25}" required />
              <span class="input-group-btn">
                <button type="submit" class="btn btn-default btn-action" value="Encontrar" id="mrw-finder-follow-btn"><img src="/img/action.png" /></button>
              </span>
            </div>
          </div>
        </form>
      </div>
      <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
        <h3>Localizador de oficinas</h3>
        <form class="form-inline" action="/oficina_transporte_urgente/MRW_buscador_franquicia.asp" method="post" id="oficinas">
          <div class="form-group">
            <div class="input-group">
              <input type="text" class="form-control" name="mrw-finder-offices-code" placeholder="CP o poblaci&oacute;n" id="mrw-finder-offices-code" pattern="[0-9A-Za-z]+" required />
              <span class="input-group-btn">
                <button type="submit" class="btn btn-default btn-action" value="Encontrar" id="mrw-finder-offices-btn"><img src="/img/action.png" /></button>
              </span>
            </div>
          </div>
        </form>
      </div>
      <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
        <h3>Concertar una entrega</h3>
        <!--<a href="/concertar.asp?id=ES" class="btn btn-default btn-ambito" role="button">CONCERTAR</a>-->
        <!--www.mrw.es/concertar.asp?clave=006971796172&id=ES&valida=647750632-->
        <form class="form-inline" action="concertar.asp" id="concertarEntrega">
          <div class="form-group">
            <input type="text" class="form-control" name="clave" id="clave" placeholder="N&uacute;mero de envío" pattern="[0-9A-Za-z]{12}" required />
            <div class="input-group">
              <input type="hidden" class="form-control" name="id" id="id" value="ES" />
              <input type="text" class="form-control" name="valida" id="valida" placeholder="Nº tel&eacute;fono" pattern="[0-9]{9,12}" required />
              <span class="input-group-btn">
                <button type="submit" class="btn btn-default btn-action"><img src="/img/action.png" /></button>
              </span>
            </div>
          </div>
        </form>
      </div>
    </div>

  </section>

