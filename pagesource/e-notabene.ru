	function updateLetterByLetter(blockNum, blockVal)
	{
		var currentNewsNumber = document.getElementById('cur_number').value;
	
		var obj1 = document.getElementById( currentNewsNumber + '_part_1');
		var obj2 = document.getElementById( currentNewsNumber + '_part_2');
		var obj3 = document.getElementById( currentNewsNumber + '_part_3');
		  
		  
		if (obj1 != null && obj2 != null && obj3 != null)
		{
			resetLetterByLetterEffect();
			
			
			if (blockNum == 1)
			{
				var pos = parseInt(window.text_1.length*blockVal);
				
				obj1.innerHTML = window.text_1.substr(0,pos);
				
				obj2.innerHTML = "";
				obj3.innerHTML = "";
			}
			else if (blockNum == 2)
			{
				var pos = parseInt(window.text_2.length*blockVal);
				
				obj2.innerHTML = window.text_2.substr(0,pos);
				
				obj1.innerHTML = window.text_1;
				obj3.innerHTML = "";
			}
			else if (blockNum == 3)
			{
				var pos = parseInt(window.text_3.length*blockVal);
				
				obj3.innerHTML = window.text_3.substr(0,pos);
				
				obj1.innerHTML = window.text_1;
				obj2.innerHTML = window.text_2;
			}	
		}
		
	}
	
	function updateMove(blockNum, blockVal)
	{
		var currentNewsNumber = document.getElementById('cur_number').value;
		
		var obj1 = document.getElementById( currentNewsNumber + '_move_1');
		var obj2 = document.getElementById( currentNewsNumber + '_move_2');
		var obj3 = document.getElementById( currentNewsNumber + '_move_3');
		
		if (obj1 != null && obj2 != null && obj3 != null)
		{
			//resetMoveEffect();
			
			if (blockNum == 1)
			{
				obj1.style.left = '0px';
			}
			else if (blockNum == 2)
			{
				obj1.style.transition = 'none';
				obj1.style.left = '0px';
				obj2.style.left = '0px';
			}
			else if (blockNum == 3)
			{
				obj1.style.transition = 'none';
				obj2.style.transition = 'none';
				obj1.style.left = '0px';
				obj2.style.left = '0px';
				obj3.style.left = '0px';
			}
			
			
		}
		
	}
	
	function effectControllerStart()
	{
		document.resetEffectController = 0;
		
		var millSec = new Date().getTime();
		
		setTimeout("effectController("+millSec+",0);", 100);
	}
	
	function effectController(lastMillSec, lastDelay)
	{
	//debugger;
		if (document.resetEffectController == 1)
			return;
			
		var millSec = new Date().getTime();
		
		var delay = millSec - lastMillSec;
		
		lastDelay += delay;
		
		// 0...15000(эффект)...25000(реклама)
		
		var curStep = Math.floor(lastDelay / 15000);
		
		if (typeof (newsChange) !== "function")
		{
			curStep = 0;
		}
			
			
		//console.log("curStep " + curStep);
		//console.log("lastDelay " + lastDelay);

		//временной промежуток на эффекте			
		if (curStep == 0 && lastDelay < 15000)
		{				
			var blockNum = Math.floor(lastDelay / 5000) + 1;
			var blockVal = (lastDelay % 5000) / 5000;			
			
			//console.log("blockVal " + blockVal);
			//console.log("lastDelay" + lastDelay);
			
			var animation = getAnimationNumber();
			
			console.log("animation" + animation);
			
			if (animation == 2)
			{
				updateOpacity(blockNum, blockVal);
			}
			else if (animation == 3)
			{
				updateLetterByLetter(blockNum, blockVal);
			}
			else if (animation == 4)
			{
				updateMove(blockNum, blockVal);
			}
		}
		else
		{
			if (lastDelay > 24400)
			{
				if (typeof (newsChange) == "function")
					newsChange();
					
				lastDelay = 0;
				//resetMoveEffect();
			}
		}
		
		setTimeout("effectController(" + millSec + ", " + lastDelay + ");", 100);
	}
	
	function resetMoveEffect()
	{
		var currentNewsNumber = document.getElementById('cur_number').value;
		
		var obj1 = document.getElementById( currentNewsNumber + '_move_1');
		var obj2 = document.getElementById( currentNewsNumber + '_move_2');
		var obj3 = document.getElementById( currentNewsNumber + '_move_3');
		
		if (obj1 != null && obj2 != null && obj3 != null)
		{
			obj1.style.left = '-200px';
			obj2.style.left = '-200px';
			obj3.style.left = '-200px';
			
			obj1.style.transition = '2s left';
			obj2.style.transition = '2s left';
			obj3.style.transition = '2s left';
		}
	}

	function resetOpacity()
	{
		var cur_number = document.getElementById('cur_number').value;
			
		var obj1 = document.getElementById( cur_number + '_opacity_1');
		var obj2 = document.getElementById( cur_number + '_opacity_2');
		var obj3 = document.getElementById( cur_number + '_opacity_3');
			  
			  
		if (obj1 != null && obj2 != null && obj3 != null)
		{
			obj1.style.opacity = 0;
			obj2.style.opacity = 0;
			obj3.style.opacity = 0;
		}
	}

	function resetLetterByLetterEffect()
	{
		var cur_number = document.getElementById('cur_number').value;
				
		var obj1 = document.getElementById( cur_number + '_part_1');
		var obj2 = document.getElementById( cur_number + '_part_2');
		var obj3 = document.getElementById( cur_number + '_part_3');
					  
					  
		if (obj1 != null && obj2 != null && obj3 != null)
		{
			if (window.text_1 == "")
				window.text_1 = obj1.innerHTML;
				
			obj1.innerHTML = "";
			
			if (window.text_2 == "")		
				window.text_2 = obj2.innerHTML;
				
			obj2.innerHTML = "";
			
			if (window.text_3 == "")		
				window.text_3 = obj3.innerHTML;
				
			obj3.innerHTML = "";
		}
	}

	function getAnimationNumber()
	{
		var cur_number = document.getElementById('cur_number').value;
		
		var an2 = document.getElementById(cur_number + '_animation_2');
		var an3 = document.getElementById(cur_number + '_animation_3');
		var an4 = document.getElementById(cur_number + '_animation_4');
		
		if (an2 !=null)
			return 2;
		else if (an3 !=null)
			return 3;
		else if (an4 !=null)
			return 4;
			
		return 1;	
	}
	

