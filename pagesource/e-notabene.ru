	function updateOpacity(blockNum, blockVal)
	{
	  var currentNewsNumber = document.getElementById('cur_number').value;
	
	  var obj1 = document.getElementById( currentNewsNumber + '_opacity_1');
	  var obj2 = document.getElementById( currentNewsNumber + '_opacity_2');
	  var obj3 = document.getElementById( currentNewsNumber + '_opacity_3');
	  
	  
	  if (obj1 != null && obj2 != null && obj3 != null)
	  {
		if (obj1.style.opacity == '')
		{
			obj1.style.opacity = 0;
		}
		
		if (obj2.style.opacity == '')
		{
			obj2.style.opacity = 0;
		}
		
		if (obj3.style.opacity == '')
		{
			obj3.style.opacity = 0;
		}
		
		
		if (blockNum == 1)
		{
		//alert("blockNum");
		
			obj1.style.opacity = parseFloat(blockVal);
			
			obj2.style.opacity = 0;
			obj3.style.opacity = 0;
