			obj2.style.opacity = parseFloat(blockVal);
			
			obj1.style.opacity = 1;
			obj3.style.opacity = 0;
		}
		else if (blockNum == 3)
		{
			obj3.style.opacity = parseFloat(blockVal);
			
			obj1.style.opacity = 1;
