<script type="text/javascript">
wordpressAsync = (function(){
    
	var list      = [];
	var isLoading = false;
	var isReady   = false;
    
    
	addEvent(window, "load", function(){

		if (isReady === false) {

			isReady = true;

			setTimeout(function(){

				lazyLoadBlock();
				refresh();
			}, 0);
		}
	});
    
                //"js", string
                //"css", string, string
                //"func", function
    
	return function(type, value, media){
        
		list.push({
			"type"  : type,
			"value" : value,
			"media" : media
		});

		refresh();
	};

	function refresh() {
		
		if (isReady === false) {
			return;
		}

		if (isLoading === true) {
			return;
		}

		if (list.length < 1) {
			return;
		}

		var elementLoad = list.shift();

		if (elementLoad.type === "js") {

			isLoading = true;

			loadJs(elementLoad.value, function(){

				isLoading = false;
				setTimeout(refresh, 0);
			});
        
        } else if (elementLoad.type === "css") {

			loadCss(elementLoad.value, elementLoad.media, function(){

				setTimeout(refresh, 0);
			});

        } else if (elementLoad.type === "func") {
            
			elementLoad.value();
            setTimeout(refresh, 0);
            
		} else {
            
			throw Error("Nieznany typ");
		}
	}

	function addEvent(element, event, callback) {

        if (element.addEventListener)
            element.addEventListener(event, callback, false);

        else if (element.attachEvent)
            element.attachEvent('on' + event, callback);

        else
            throw Error('nieprawidlowe odgalezienie');
    }

    function loadCss(src, media, callback) {

		var node   = document.createElement('link');

		node.type  = 'text/css';
		node.rel   = 'stylesheet';
		node.href  = src;
		node.media = media;

		head_add(node);
		callback();
    }

    function loadJs(src, callback) {

		var node    = document.createElement('script');
		var exec    = false;

		node.type   = 'text/javascript';
		node.onload = callbackLoad;
		node.src    = src;
		node.async  = true;
		node.defer  = true;

		node.onreadystatechange = function () {
		    
		    if (node.readyState === 'loaded' || node.readyState === 'complete')
		        callbackLoad();
		    };

		head_add(node);

		function callbackLoad() {

			if (exec === false) {
				exec = true;
				callback();
			}
		}
    }

    function head_add(node) {
        
        var head = document.getElementsByTagName('head');
        
        if (head.length !== 1)
            throw Error('Expected to exactly one element "head" -> ' + head.length);
        
        head = head[0];
        head.appendChild(node);
    }

    function lazyLoadBlock() {

    	var list = document.querySelectorAll(".lazyLoadBlock");

    	for (var i=0; i<list.length; i++) {
			runBlock(list[i]);
    	}

    	function runBlock(block) {
            
            block.style.opacity = "0";
            
    		block.innerHTML = getText(block);
            
            var imgList = block.querySelectorAll("img");
            
            if (imgList.length > 0) {
                
                addEvent(imgList[0], "load", function(){

                    block.style.opacity = "1";
                });
            
            } else {
            
                block.style.opacity = "1";
            }
    	}
    }
    
	function getText(node) {

		var out = "";

        if (typeof(node.textContent) === "string")
        	return node.textContent;

        for (node = node.firstChild; node; node = node.nextSibling)
        	out += getText(node)

	    return out
	}

}());
</script>



