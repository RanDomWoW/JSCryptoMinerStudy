require(["jquery"], function($){
	$('.box-tocart .action.tocart').hover(function(){
		$(this).toggleClass('hovered');
	});
});
require(["jquery", "jquery/ui"], function($){
	$('.search-toggle').click(function(){
		if ($(window).width() >= 768) {
			$('#search_mini_form').toggle("slide", {direction: "right"}, 300);
			$(this).toggleClass('soHotRightNow');
		} else {
			if ($('html').hasClass('nav-open')) {
				$('html').removeClass('nav-open').removeClass('nav-before-open');
			}
			$('#search_mini_form').toggle("slide", {direction: "up"}, 300);
		}
	});
	$('.footer-top h4').click(function(){
		if ($(window).width() < 768) {
			$(this).find('.fa').toggle();
			$(this).next('ul').toggle();
		}
	});
});
require(["jquery", "owl"], function($){
	if ($('button.button').length) {
		$('button.deduct').click(function(){
			var oldValue = $('#qty').val();
			// Don't let #qty get lower than 0
			if (oldValue > 1) {
				var newVal = parseInt(oldValue - 1);
				
			} else {
				newVal = 1;
			}
			$('#qty').val(newVal);
			return false;
		});
		$('button.add').click(function(){
			var oldValue = parseInt($('#qty').val());
			var newVal = parseInt(oldValue + 1);
			$('#qty').val(newVal);
			return false;
		});
	}
	if ($('.cms-index-index .product-items').length) {
		$('.cms-index-index .product-items').owlCarousel({
			loop: true,
			nav: true,
			navText: ['',''],
			dots: false,
			responsive: {
				0: {
					items: 1
				},
				768: {
					items: 3
				},
				1000: {
					items: 4
				}
			}
		});
	}
	if ($('.upsell .product-items').length) {
		var owlRot = $('.upsell ol.product-items');
		owlRot.owlCarousel({
			nav: true,
			navText: ['',''],
			dots: false,
			responsive: {
				0: {
					items: 1
				},
				768: {
					items: 3
				},
				1000: {
					items: 4
				}
			}
		});
	}
	$('.stockNotifierDiv').insertAfter('#product-options-wrapper');
	$('.hide-share').hideshare({
		linkedin: false,
		googleplus: false
	});
	if ($('.level0.nav-8').length) {
		$('.level0.nav-8 .mega-menu-2-div a').each(function(){
			$(this).hover(function(){
				var thisHtml = $(this).next('.hover-hidden').html();
				$('.level0.nav-8 .model-hover-container').html(thisHtml);
			});
		});
	}
	if ($('.crumbsetc').length) {
		if ($('.crumbsetc .category-description').length) {
			return true;
		} else {
			$('.crumbsetc').addClass('no-desc');
		}
	}
});


