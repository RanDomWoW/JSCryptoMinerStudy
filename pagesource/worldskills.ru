	<script>
	    $(document).on('click', '#primary-menu li.sub-menu', function(e) {
            e.preventDefault();
            e.stopPropagation();
            
            if (e.target != this) return;
            
            console.log($(this));

            var pWidth = $(this).innerWidth();
            var pOffset = $(this).offset();
            var x = e.pageX - pOffset.left;

            var link = $(this).children('a');

            if (x / pWidth * 100 > 75 || $(link).hasClass('disabled')) {
                e.preventDefault();
                $(this).siblings('.item').addClass('inactive');
                $(link).next().show();
                $(link).addClass('inactive');
                $(this).find('ul > li > a').show();

                $('#menu-back').removeClass('hidden');
            } else {
                window.location.href = $(link).attr('href');
            }

            return false;
	    });
	    
	    $(document).on('click', '#menu-back', function() {
	        $('#primary-menu > ul li, #primary-menu > ul li a').removeClass('inactive').css('display', 'block');
	        $('#primary-menu > ul li > ul').hide();

            $('#menu-back').addClass('hidden');
	    });	 

	    $(document).on('click', '#menu-close', function() {
	        $('#primary-menu > ul li, #primary-menu > ul li a').removeClass('inactive').css('display', 'block');
	        $('#primary-menu > ul li > ul').hide();

            $('#menu-back').addClass('hidden');

			if( $('#primary-menu').find('ul.mobile-primary-menu').length > 0 ) {
				$( '#primary-menu > ul.mobile-primary-menu, #primary-menu > div > ul.mobile-primary-menu' ).toggleClass("show");
			} else {
				$( '#primary-menu > ul, #primary-menu > div > ul' ).toggleClass("show");
			}

			$('body').toggleClass("primary-menu-open");

			return false;
	    });

        jQuery.fn.dataTableExt.oSort['extractdate-asc'] = function (x, y) {
            var x = x.split('.');
            x = new Date(x[2], x[1] - 1, x[0]).getTime();
            var y = y.split('.');
            y = new Date(y[2], y[1] - 1, y[0]).getTime();

            return ((x < y) ? -1 : ((x > y) ? 1 : 0));  
        };  
        
        jQuery.fn.dataTableExt.oSort['extractdate-desc'] = function (x, y) {
            var x = x.split('.');
            x = new Date(x[2], x[1] - 1, x[0]).getTime();
            var y = y.split('.');
            y = new Date(y[2], y[1] - 1, y[0]).getTime();

            return ((x < y) ? 1 : ((x > y) ? -1 : 0));  
        };

	    $('#dt').DataTable({
	        paging: false,
            "aoColumnDefs": [{
                "sType": "extractdate",
                "bSortable": true,
                "aTargets": [0, 1]
            }, {
                "searchable": false,
                "targets": [0, 1]
            }],
            colReorder: {
                order: [3, 2, 0, 1],
            },
	        language:
	            {
                    "processing": "Подождите...",
                    "search": "",
                    "lengthMenu": "Показать _MENU_ записей",
                    "info": "Записи с _START_ до _END_ из _TOTAL_ записей",
                    "infoEmpty": "Записи с 0 до 0 из 0 записей",
                    "infoFiltered": "(отфильтровано из _MAX_ записей)",
                    "infoPostFix": "",
                    "loadingRecords": "Загрузка записей...",
                    "zeroRecords": "Записи отсутствуют.",
                    "emptyTable": "В таблице отсутствуют данные",
                    "paginate": {
                    "first": "Первая",
                    "previous": "Предыдущая",
                    "next": "Следующая",
                    "last": "Последняя"
                },
                    "aria": {
                        "sortAscending": ": активировать для сортировки столбца по возрастанию",
                        "sortDescending": ": активировать для сортировки столбца по убыванию"
                    }
                }
	    });
	    
	    //$.datepicker.setDefaults($.datepicker.regional['ru']);
	    
        $('.input-daterange .before, .input-daterange .after').datepicker({
            dateFormat: 'dd.mm.yyyy',
        });
        
        $('.input-daterange .before').datepicker('setDate', new Date(2017, 1));
        $('.input-daterange .after').datepicker('setDate', new Date());
        
        var regions = [];
        $('#dt td:nth-child(1)').each(function(i, item) {
            regions.push($(item).text());
        });
        
        for (var i = 0; i < regions.length; i++) {
            $('#region').append('<option>' + regions[i] + '</option>');
        }
        
        $('#region').change(function(e) {
            filterRows();
        });
        
        function checkDates(first, second, elem) {

            var since = Date.parse($('.input-daterange .before').val().split(".").reverse().join("-"));
            console.log('since val is',$('.input-daterange .before').val().split(".").reverse().join("-"),'till val is',$('.input-daterange .after').val().split(".").reverse().join("-"));
            var till = Date.parse($('.input-daterange .after').val().split(".").reverse().join("-"));
            
            console.log('first text is',$(first).text().split(".").reverse().join("-"),'second text is',$(second).text().split(".").reverse().join("-"));
            var start = Date.parse($(first).text().split(".").reverse().join("-"));
            var end = Date.parse($(second).text().split(".").reverse().join("-"));
            
            console.log('since is',since,'till is',till,'start is',start,'end is',end);
            
            if (start>=since && end<=till) {
                $(elem).removeClass('hidden').removeClass('date');
            } else {
                $(elem).addClass('hidden').addClass('date');
            }
        }

        function showLong() {
        	$('.championship .loadmore').removeClass('visible-xs').hide();
            $('#dt tbody tr.long').removeClass('hidden').removeClass('long');
        }
        
        function filterRows() {
            showLong();

            var option = $('#region option:selected');
            console.log('region value is ',$(option).val());
            if ($(option).is($('#region option:first'))) {
                $('#dt tbody tr:not(.date)').removeClass('hidden').removeClass('region');
            } else {
                $('#dt tbody td:nth-child(1)').each(function(i, item) {
                console.log($(option).val()==$(item).text());
                    if ($(item).text() != $(option).val()) {
                    console.log('hide to ',$(item).text());
                        $(item).parents('tr:not(.date)').addClass('hidden').addClass('region');
                    } else {
                    console.log('show to ',$(item).text());
                        $(item).parents('tr:not(.date)').removeClass('hidden').removeClass('region');
                    }
                });
            }

            $('#dt tbody tr:not(.region)').each(function(i, item) {
                checkDates($(item).find('td:nth-child(3)'), $(item).find('td:nth-child(4)'), item);
            });
        }

        $('.input-daterange').change(function() {
            filterRows();
        });
	    
	    $('form.advsea').on('submit', function() {
            var $input = $(this).find('input[type=text]');
            if ($input.val() == '') {
                $input.addClass('error');
                return false;
            } else {
                $input.removeClass('error');
            }
	    });
	    
	    $('.championship .loadmore').click(function() {
	        showLong();
	    });
	    
        $('#primary-menu button.search').on('click', function() {
            $('#primary-menu .search-input').toggleClass('active');
        });
        
        $('.championship #sort').change(function() {
	        showLong();
            $('#dt').DataTable().order([$(this).val(), 'asc']).draw();
        });
        
        $(function() {
            if ($(window).width() < 769) {
                $('#primary-menu ul').removeClass('sf-js-enabled');
            }
        });
	</script>
