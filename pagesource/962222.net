function toLongIDCard(sShortID) {

      var sLongID = "";

      var j, k, sum = 0;

      power = new Array(9, 7, 3, 6, 1, 2, 4, 8, 5, 10, 9, 7, 3, 6, 1, 2, 4);

      

      sLongID = sShortID.substring(0, 6) + "19" + sShortID.substring(6);

      for(i = 0; i < 17; i++) {

         j = 16 - i;

         if (j == 16) k = parseInt(sLongID.substring(j));

         else k = parseInt(sLongID.substring(j, j + 1));

         sum += (k * power[i]);

      }

      sum++;

      k = sum % 11;

      if(k == 10) sLongID = sLongID + "X";

      else sLongID = sLongID + k;

      

      return sLongID;

   }

 

