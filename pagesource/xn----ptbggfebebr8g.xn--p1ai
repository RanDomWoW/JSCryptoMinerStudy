<script>
    function _checkhash(){
        var hash = window.location.hash;
        var pathname = window.location.pathname;
        var a = $('.mdl-navigation a');
        $('.mdl-navigation .active').removeClass('active');

        if (hash){
            a.each(function(){
                var $this = $(this);
                if ($this.prop('hash') === hash){
                    $this.addClass('active');
                }
            });
        } else if (pathname && pathname !== '/'){
            a.each(function(){
                var $this = $(this);
                if ($this.prop('pathname') === pathname){
                    $this.addClass('active');
                }
            });
        }
    }

    $(function () {
        $(window).bind('hashchange', _checkhash);
        var currentHash = _checkhash();
    });
</script>
