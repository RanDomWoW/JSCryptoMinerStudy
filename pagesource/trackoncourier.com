// Removes leading whitespaces
function LTrim( value ) {
	
	var re = /\s*((\S+\s*)*)/;
	return value.replace(re, "$1");
	
}

// Removes ending whitespaces
function RTrim( value ) {
	
	var re = /((\s*\S+)*)\s*/;
	return value.replace(re, "$1");
	
}

// Removes leading and ending whitespaces
function trimValue( value ) {
	return LTrim(RTrim(value));
	
}

function validateNumbers(consLength, site) {
    hidAWB=document.getElementById("ctl00_ContentPlaceHolder1_Tracking1_hidAWB");
    
    if(site=='trackon') {
        trackconsno=document.getElementById("ctl00_ContentPlaceHolder1_Tracking1_trackno");
        consnoLength=10*consLength;
        
        if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)){ //test for MSIE x.x;
             var ieversion=new Number(RegExp.$1) // capture x.x portion and store as a number
             if (ieversion>=9)
                consnoLength = 329;
             else if (ieversion>=8)
                consnoLength = 329;
             else if (ieversion>=7)
                consnoLength = 329;
             else if (ieversion>=6)
                consnoLength = 329;
             else if (ieversion>=5)
                consnoLength = 329;
        }
    
    } else {
        trackconsno=document.getElementById("ctl00_ContentPlaceHolder1_Tracking1_primetrackno");
        consnoLength=8*consLength;
    }
    
    if(trackconsno.value.length==0) {
        alert("Tracking Number should be filled");
        return false;
    }
    else if(!IsNumeric1(trackconsno.value)) {
        alert("Tracking Number should be numeric only");
        return false;
    }
    else if(trackconsno.value.length > consnoLength) {
        alert("Tracking Number should be less than " + consLength + " numbers");
        return false;
    }
    
    isConsignmentPresent=0;
    var consNumber='1';
    var myArray=new Array();
	var newConsignmentNumbers='';
	var isConsignmentPresent=0;
    var iArrayNumber=0;

	for(counter=0; counter<trackconsno.value.length; counter++) {
		if(('\n' != trackconsno.value.substring(counter, counter+1))&&('\r' != trackconsno.value.substring(counter, counter+1))) {
			consNumber+=trackconsno.value.substring(counter, counter+1);

		}  else {
			if(4<trimValue(consNumber).length) {
				myArray[iArrayNumber]=trimValue(consNumber);
				iArrayNumber=iArrayNumber+1;
				isConsignmentPresent=1;
			}
			consNumber='';
		}
	}
	
	if(4<trimValue(consNumber).length) {
		myArray[iArrayNumber]=consNumber;
	} else if(1==isConsignmentPresent){
		iArrayNumber-=1;
	}
			
	for(var iTempCount=0;iTempCount<=iArrayNumber;iTempCount++) {
		isConsignmentPresent=0;
		for(var iCounter=0; iCounter<iTempCount; iCounter++) {
		    if(myArray[iTempCount]==myArray[iCounter]) {
				isConsignmentPresent=1;
			} 
		}
		if(0==isConsignmentPresent) {
			newConsignmentNumbers+=myArray[iTempCount]+",";
		}
	}
	
	newConsignmentNumbers=newConsignmentNumbers.substring(0,newConsignmentNumbers.length-1);
	
	hidAWB.value=newConsignmentNumbers;
		
    return true;
}
</script>

