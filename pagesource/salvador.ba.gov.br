    function displayVals(opc) {
		
      var opcao = opc;
      if(opcao != '0'){
	  
        $.ajax({
          cache: false,
          type: 'post',
          dataType: 'text',
          url: 'http://salvador.ba.gov.br/templates/salvador/processamento.php',
          data: {opcao: opcao},
          beforeSend: function() {},
          success: function(data) {
            
			var dado = $.parseJSON(data);                        			
            $('#interior1').html(dado.nom_servico);			
            $('#interior2').html(dado.nom_popular);
            $('#interior3').html(dado.des_servico);
            $('#interior4').html(dado.documentacao);
            $('#interior5').html(dado.val_taxa );
            $('#interior6').html(dado.qtd_prazo + dado.des_unid_prazo);
            $('#interior7').html(dado.atendimento);
			
          },
          error: function() {
            alert('PHP nao encontrado');
          }
        });

        limpaModal();		      
	    
		$("#imprimir").attr("title",opc);
		$("#enviar").attr("title",opc);
		
		
		
        $("#myModal").modal();
		
		
      }//fim if
    }

   });
   
   
   
   
   function limpaModal(){
    for ( var i = 1 ; i < 8 ; i++ ) {        
