      }
    }

    function Enviar(opc) {
      if ( opc != 0 ){
      window.open ("templates/salvador/enviar.php?opcao="+opc , "Enviar informações do Serviço", "left=400px,top=250px,location=0, status=0, scrollbars=1, width=400, height=200 , titlebar=0 , menubar=0" , "");
      }
    }   
   //fim código luquinhas

//teste
function checkThis(y) {
    var isVisible = document.getElementById("interior" + y);
    

    
    if (isVisible.style.display == "none"){
        for ( var i = 1 ; i < 8 ; i++ ) {        
		  document.getElementById("interior" + i).style.display = "none";       
		}
	  
	  document.getElementById("interior" + y).style.display = "block";
      
      
    }
    else{
      document.getElementById("interior" + y).style.display = "none";
    } 
    
