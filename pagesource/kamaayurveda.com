							jQuery('#popupregister #username').val(isEmail); 
							jQuery('#popupregister #email_address').val('');
							
						}else{
							
							jQuery('#popupregister #email_address').val(isEmail);
							jQuery('#popupregister #username').val('');
						}
							
							
						
							
							
						
						
						/*if(isEmail.indexOf('@')>0){
							
						}else{

							
						}*/
							
						

                        if(data=='emailexist'){

                            jQuery('#cemail-div').hide();

                            jQuery('#clogin-div').show();

                        } else if(data=='emailnotexist'){

                            jQuery('#onepage-cemail').after("<p style='margin-top: 10px;' class='validation-advice'>Account Doesn't exist Pleas Signup!</p>");

							setTimeout(function(){
								jQuery('#cemail-div').hide();
								jQuery('#register').show();
							}, 2000);
							
                           

                            //curOpenId = "register";

                        }

                        jQuery('#cemail .wloader').hide();

                    }

                })

                return false;

            })



            jQuery('#clogin').submit(function(){

