				jQuery('#cemail-div p.validation-advice').remove();
                if(!cemail.validator.validate()) return;

                jQuery('#cemail .wloader').show();

                //console.log(login_email);

                jQuery.ajax({

                    url:jQuery('#cemail').attr('action'),

                    type:'post',

