jQuery('#discountcode-form').submit(function(e) 
{ 
    jQuery('.minicart-wrapper').addClass('loading');
    e.preventDefault(); 
    jQuery.ajax(
    { 
        url:jQuery('#discountcode-form').attr('action'), 
        type:'POST', 
        data:'ajax=true&'+jQuery('#discountcode-form').serialize(), 
        success:function(data) 
        { 
            //alert(data);
            var obj = jQuery.parseJSON(data);
            jQuery('#minicart-ajax-loader').hide();
            jQuery('#minicart-success-message').html("").hide();
            jQuery('#minicart-error-message').html("").hide();

            if(obj.success == 1){
                jQuery('#minicart-success-message').html(obj.msg).show();
                jQuery('.minicart-totals').html(obj.contents);
            }else{
                jQuery('#minicart-error-message').html(obj.msg).show();
                jQuery('.minicart-totals').html(obj.contents);
           // jQuery('.minicart-couponcode .minicart-code-result').html(data);
            } 
        jQuery('.minicart-wrapper').removeClass('loading');
        }
    }); 
});

function clearcart()
{
    if (confirm('Are you sure you want to clear the cart?')) 
    {
        jQuery('.minicart-wrapper').addClass('loading');

        jQuery.ajax(
        { 
            url:jQuery('#clearminicart').attr('clearurl'), 
            type:'POST', 
            data:'test=test', 
            success:function(data) 
            { 
                window.location.href = "https://www.kamaayurveda.com/checkout/cart/";
            }
        }); 
    } 
} 
