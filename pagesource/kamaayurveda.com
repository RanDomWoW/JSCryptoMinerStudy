                            else
                                location.reload();
                        }

						else if(data=='wrongpassword'){
							jQuery('#onepage-clogin').after("<p style='margin-top: -102px;' class='validation-advice'>Wrong Password!</p>");
							jQuery(".input-text.validation-passed").css("border", "1px solid red");
							jQuery('#clogin .wloader').hide();
						}
						
                        else if(data=='wronglogin,'){

                            jQuery('#onepage-clogin').after("<p style='margin-top: 10px;' class='validation-advice'>Invalid User Please Signup!</p>");
							jQuery('#clogin .wloader').hide();
							
							setTimeout(function(){
								hideTab();
								jQuery('#cemail-div').show();
								jQuery("#clogin-div").hide();
								showTab('register');
