                    success:function(data){

                        //console.log(jQuery('#useremail').val());

                        jQuery('#login-useremail').val(jQuery('#useremail').val());

                        //jQuery('#register-useremail').val(jQuery('#useremail').val());
						var isEmail = jQuery('#useremail').val();
						
						var intRegex = /^\d+$/;
