            {{/each}}
        </ul>
    {{/if}}
</script>

<script id="hamburger-mobile-template" type="text/x-handlebars-template">
    {{#ifEquals navigationStrategy "sibling-children"}}
    		{{#each hamburgerLinks.primary}}
        		<div class="panel panel-default">
            		<div class="panel-heading" role="tab" id="levelTwoLinks_{{math @index '+' '1'}}">
                		<h4 class="panel-title">
                    		<a role="button" href="{{this.url}}{{#stringDoesntContain this.url '.html'}}.html{{/stringDoesntContain}}" 
                       		aria-expanded="false" aria-controls="levelThreeLinks_{{math @index '+' '1'}}">
                        		{{this.text}}
                    		</a>
                    		<span></span>
                		</h4>
            		</div>
            		<div id="levelThreeLinks_{{math @index '+' '1'}}" class="panel-collapse collapse" role="tabpanel"
                 		aria-labelledby="levelTwoLinks_{{math @index '+' '1'}}">
                		<div class="panel-body">
                    		<ul>
                        		{{#each this.children}}
                        			<li>
                        				<a href="{{this.url}}{{#stringDoesntContain this.url '.html'}}.html{{/stringDoesntContain}}">{{this.text}}</a>
                        			</li>
                        		{{/each}}
                    		</ul>
                		</div>
            		</div>
        		</div>
    		{{/each}}
    {{/ifEquals}}
	{{#ifEquals navigationStrategy "manual"}}
        {{#if hasSectionTitle}}
        <span class="sectiontitle"><a href="{{sectionPagePath}}.html">{{sectionTitle}}</a></span>
        {{/if}}
        <div class="row">
            {{#if hamburgerLinks.primary.length}}
            <div class="col-sm-6" id="inPageNavigation">
                <ul>
                    {{#each hamburgerLinks.primary}}
                    <li>
                        <a href="{{this.url}}{{#stringDoesntContain this.url '.html'}}.html{{/stringDoesntContain}}" class="inPageLink"
                           target="{{this.targetType}}"><span>{{this.text}}</span></a>
                    </li>
                    {{/each}}
                </ul>
            </div>
            {{/if}}
            {{#if hamburgerLinks.secondary.length}}
            <div class="col-sm-6" id="furtherArticles">
                <ul>
                    {{#each hamburgerLinks.secondary}}
                    <li>
                        <a href="{{this.url}}{{#stringDoesntContain this.url '.html'}}.html{{/stringDoesntContain}}"
                           target="{{this.targetType}}"><span>{{this.text}}</span></a>
                    </li>
                    {{/each}}
                </ul>
            </div>
            {{/if}}
        </div>
    {{/ifEquals}}
</script>

<script id="social-share-template" type="text/x-handlebars-template">
    {{#each socialShareConfiguration}}
        {{#ifEquals this "Facebook"}}
        <a href="https://www.facebook.com/sharer/sharer.php?u={{socialURL}}">
            <span class="sti-btn sti-facebook-btn circle-icon">FB</span></a>
        {{/ifEquals}}
        {{#ifEquals this "Twitter"}}
        <a href="https://twitter.com/intent/tweet/?url={{socialURL}}">
            <span class="sti-btn sti-twitter-btn circle-icon">twitter</span></a>
        {{/ifEquals}}
        {{#ifEquals this "Linkedin"}}
        <a href="https://www.linkedin.com/shareArticle?mini=true&url={{socialURL}}">
            <span class="sti-btn sti-linkedin-btn circle-icon">Linkedin</span></a>
        {{/ifEquals}}
        {{#ifEquals this "Googleplus"}}
        <a href="https://plus.google.com/share?url={{socialURL}}"><span
                class="sti-btn sti-google-btn circle-icon">GooglePlus</span></a>
        {{/ifEquals}}
        {{#ifEquals this "Weibo"}}
        <a href="http://v.t.sina.com.cn/share/share.php?url={{socialURL}}">
            <span class="sti-btn sti-weibo-btn circle-icon">Weibo</span></a>
        {{/ifEquals}}
    {{/each}}
</script>


