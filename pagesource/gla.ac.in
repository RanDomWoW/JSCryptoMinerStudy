<!-- contact form start -->
<script type="text/javascript">
$(document).ready(function(){ 
	var _scroll = true, _timer = false, _floatbox = $("#contact_form"), _floatbox_opener = $(".contact-opener") ;
	_floatbox.css("right", "-300px"); //initial contact form position
	
	//Contact form Opener button
	_floatbox_opener.click(function(){
		if (_floatbox.hasClass('visiable')){
            _floatbox.animate({"right":"-300px"}, {duration: 300}).removeClass('visiable');
        }else{
           _floatbox.animate({"right":"0px"},  {duration: 300}).addClass('visiable');
        }
	});
	
	//Effect on Scroll
	$(window).scroll(function(){
		if(_scroll){
			_floatbox.animate({"top": "150px"},{duration: 300});
			_scroll = false;
		}
		if(_timer !== false){ clearTimeout(_timer); }           
			_timer = setTimeout(function(){_scroll = true; 
			_floatbox.animate({"top": "150px"},{easing: "linear"}, {duration: 500});}, 400); 
	});	
	
	//Ajax form submit
    $("#submit_btn").click(function() { 
        var proceed = true;
        //simple validation at client's end
        $("#contact_form input[required=true], #contact_form textarea[required=true]").each(function(){
            $(this).css('border-color',''); 
            if(!$.trim($(this).val())){ //if this field is empty 
                $(this).css('border','#f00'); //change 
                proceed = false; //set do not proceed flag
            }
            //check invalid email
            var email_reg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
            if($(this).attr("type")=="email" && !email_reg.test($.trim($(this).val()))){
                $(this).css('border','#f00'); //change border color to red   
                proceed = false; //set do not proceed flag              
            }   
        });
       
        if(proceed) //everything looks good! proceed...
        {
			var frm_mobile 	 = jQuery('#frm_mobile').val();
			var frm_state    = jQuery('#state').val();
			var frm_district = jQuery('#district').val();
			var frm_name 	 = jQuery('#frm_name').val();
			var frm_course 	 = jQuery('#frm_course').val();
			var frm_branch 	 = jQuery('#branch').val();
			var frm_email 	 = jQuery('#frm_email').val();
			var email_reg 	 = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
			
				if(frm_name==''){
					form_name = '<div class="error">Please Enter Name.</div>';
					$("#contact_form #contact_results").hide().html(form_name).slideDown();
					return false;
				}
				
				if(frm_course==''){
					form_course = '<div class="error">Please Select Course.</div>';
					$("#contact_form #contact_results").hide().html(form_course).slideDown();
					return false;
				}
				
				if(frm_branch==''){
					form_branch = '<div class="error">Please Select Branch.</div>';
					$("#contact_form #contact_results").hide().html(form_branch).slideDown();
					return false;
				}
				
				if(frm_email==''){
					form_email = '<div class="error">Please Enter Email.</div>';
					$("#contact_form #contact_results").hide().html(form_email).slideDown();
					return false;
				}
				
				if(!email_reg.test(frm_email)){
					form_email = '<div class="error">Enter Valid Email.</div>';
					$("#contact_form #contact_results").hide().html(form_email).slideDown();
					return false;
				}
				
				if(frm_state==''){
					form_state = '<div class="error">Please Select State.</div>';
					$("#contact_form #contact_results").hide().html(form_state).slideDown();
					return false;
				}
				
				if(frm_district==''){
					form_district = '<div class="error">Please Select District.</div>';
					$("#contact_form #contact_results").hide().html(form_district).slideDown();
					return false;
				}
			
				if(frm_mobile.length<10){
					form_mobile = '<div class="error">Enter 10 Numeric digits.</div>';
					$("#contact_form #contact_results").hide().html(form_mobile).slideDown();
					return false;
				} else if(isNaN(frm_mobile)) {
					form_mobile = '<div class="error">Mobile No. must be numeric.</div>';
					$("#contact_form #contact_results").hide().html(form_mobile).slideDown();
					return false;
				}else {	
					jQuery.ajax({
						type: "POST",
						url: '/page/savefloatingformdata/',
						data: jQuery('#float_form').serialize(),
						success: function (data) {
							  if(data=='success') {
									output = '<div class="success">Your query has been sent successfully.</div>';
									jQuery('#float_form').trigger("reset");
									jQuery("#frm_mobile").css("border-color", "");
									} else {
									output = '<div class="error">Something went wrong.</div>';
									jQuery('#float_form').trigger("reset");
							}
							$("#contact_form #contact_results").hide().html(output).slideDown();
					   }
					});
				}	
			return false;
        }
    });
	
});
</script>

