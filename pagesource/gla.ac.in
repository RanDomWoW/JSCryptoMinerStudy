<script>
jQuery('#submit').click(function(){
	var error=0;
		if(jQuery('#name').val()==""){			
			$('#name').attr("placeholder","This field is required....!");
			error=1;
		}
		if(jQuery('#mobile_no').val()==""){
			$('#mobile_no').attr("placeholder","This field is required....!");
			error=1;
		}
		if(jQuery('#email_id').val()==""){
			$('#email_id').attr("placeholder","This field is required....!");
			error=1;
		}
		if(jQuery('#query').val().trim()==""){
			jQuery("#query").html('');
			$('#query').attr("placeholder","This field is required....!");
			error=1;
		}
		
		if(error == 1){
			return false;
		}
		
		
		jQuery.ajax({
		  type: "POST",
		  url: "http://gla.ac.in//page/subscribe",		  
		  data: { name: jQuery('#name').val(), email_id: jQuery('#email_id').val(), mobile_no: jQuery('#mobile_no').val(), query: jQuery('#query').val() }
		})
		.done(function( msg ) {			
			if(msg=="success"){	
				$('#name').hide();
				$('#mobile_no').hide();
				$('#email_id').hide();
				$('#query').hide();
				$('#submit').hide();
				$('#sucess').show();
				//jQuery('#email_id').val(msg );		  
			}else if(msg=="email"){
				jQuery('#email_id').val("Invalid Email...!");
			}else if(msg=="mobile"){
				jQuery('#mobile_no').val("Invalid Mobile Number...!");
			}else if(msg=="both"){
				jQuery('#email_id').val("Invalid Email...!");
				jQuery('#mobile_no').val("Invalid Mobile Number...!");
