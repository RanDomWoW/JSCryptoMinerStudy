<script type="text/javascript">
    /*отправляем условия сотрудничества*/
    $("#get_termsForm_button").on('click',function(){

        //отменяем отправку формы
        $('#get_termsForm').submit(function(){
            return false;
        });

        //получаем имя пользователя
        var user_name = $('#terms_email_registr').val();

        //получаем e-mail для отправки письма
        var user_email = $('#terms_password_registr').val();

        //отправляем данные скрипту
        $.post(
            "/ajax/terms_cooperation.php",
            {
                status:'send',
                user_name:user_name,
                user_email:user_email
            },
            function(data){
                //обработка полученных данных
                if(data.result){
                    $('.modalConditionsOverlay').removeClass('modalVisible');
                    $('.alertTitle').html("Условия сотрудничества были отправлены вам на почту.");
                    $('.modalAlertOverlay').addClass('modalVisible');
                }else{
                    if(data.error == 'notValidEmail'){
                        $('.alertTitle').html("Вы ввели некорректный e-mail.");
                        $('.modalAlertOverlay').addClass('modalVisible');
                    }
                    if(data.error == 'emptyField'){
                        $('.alertTitle').html("Вы не заполнили обязательные поля");
                        $('.modalAlertOverlay').addClass('modalVisible');
                    }
                }
            },
            "json"
        )
    });
    /*отправляем условия сотрудничества*/
</script>
