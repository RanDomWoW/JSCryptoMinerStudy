    $("div.productPreviewFavorite").live('click',function(){
        var element = $(this);

        var idProd = element.attr('data-id-product');
        var ipUser = element.attr('data-ip-user');

        if(!$(this).hasClass('active')){
            $.post(
                "/ajax/favorite.php", {status:'add',idProd:idProd,ipUser:ipUser},
                function(data){
                    if(data.result){
                        element.addClass('active');
