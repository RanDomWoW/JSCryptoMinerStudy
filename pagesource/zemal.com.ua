<script type="text/javascript">
    /* регистрация */
    $("#checkInFormSubmit").on('click', function(){
        var form = $('#checkInForm');

        form.submit(function(){
            return false;
        });
        var msg = form.serialize();


        //отправляем данные скрипту
        $.post(
            "/ajax/checkin.php",
            {
                status:'send',
                data:msg
            },
            function(data){
                var form = $('#checkInForm');
                form.find('.textAlertBlock').html('');
                //обработка полученных данных
                if(data.result){
                    form.find('.textAlertBlock').html(data.alert);
                    form.find('.inputFields').val('');

                    if (data.login && data.password) {
                        $('#loginAuthorization').val(data.login);
                        $('#passwordAuthorization').val(data.password);
                        $('#loginForm').submit();
                    }
                }else{
                    if(data.error){
                        form.find('.textAlertBlock').html(data.error);
                    }
                }
            },
            "json"
        )
    });
    /* регистрация */

    /* авторизация */
    $("#loginButton").on('click', function(){
        var form = $('#loginForm');

        var msg = form.serialize();
        //отправляем данные скрипту
        $.post(
            "/ajax/login.php",
            {
                status:'send',
                data:msg
            },
            function(data){
                var form = $('#loginForm');
                form.find('.textAlertBlock').html('');
                //обработка полученных данных
                if(data.result){
                    form.submit();
                }else{
                    if(data.error){
                        form.find('.textAlertBlock').html(data.error);
                    }
                }
            },
            "json"
        )
    });
    /* авторизация */

    /* восстановление пароля */
    $("#recoveryPasswordButton").on('click', function(){
        var form = $('#recoveryPasswordForm');

        form.submit(function(){
            return false;
        });
        var msg = form.serialize();


        //отправляем данные скрипту
        $.post(
            "/ajax/recoveryPassword.php",
            {
                status:'send',
                data:msg
            },
            function(data){
                var form = $('#recoveryPasswordForm');
                form.find('.textAlertBlock').html('');
                //обработка полученных данных
                if(data.result){
                    form.find('.textAlertBlock').html(data.alert);
                    form.find('.inputFields').val('');
                }else{
                    if(data.error){
                        form.find('.textAlertBlock').html(data.error);
                    }
                }
            },
            "json"
        )
    });
    /* восстановление пароля */

    $('.regLink').click(function () {
        var nav = $(this).attr('data-nav');
        var tab = $(this).attr('data-tab');
        $('.nav-item').removeClass('active');
        $('.tab-pane').removeClass('active');
        $('#'+nav).addClass('active');
        $('#'+tab).addClass('active');
    });
</script>

