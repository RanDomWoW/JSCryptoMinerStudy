<script type="text/javascript">
    // hide #back-top first
    $("#back-top").hide();

    // fade in #back-top
    $(function () {
        $(window).scroll(function () {

            if ($(this).scrollTop() > 500) {
                $('#back-top').fadeIn();
            } else {
                $('#back-top').fadeOut();
            }

            var heightPage = $(document).height();
            var top = $("div.itemsWeekBlock").offset().top;
            var bottom = heightPage-top;
            var neadeElem = $("div.itemsWeekBlock");
            if(bottom < 800){
                if(!neadeElem.hasClass('itemsWeekBlockBottom')){
                    $("div.itemsWeekBlock").addClass('itemsWeekBlockBottom');
                }
            }
            if(bottom > 1200){
                if(neadeElem.hasClass('itemsWeekBlockBottom')){
                    $("div.itemsWeekBlock").removeClass('itemsWeekBlockBottom');
                }
            }
        });

        // scroll body to 0px on click
        $('#back-top').click(function () {
            $('body,html').animate({
                scrollTop: 0
            }, 800);
            return false;
        });
    });
</script>
