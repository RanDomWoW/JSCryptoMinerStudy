<!-- Mail menbers registration form（非ログイン ONLY） by Hiroomi Izumi -->
<div>
    <div id="monoco-mailuser-slide">
        <div id="monoco-mailuser-slide-in">
            <div id="monoco-mailuser-close-btn"></div>
            <iframe seamless sandbox="allow-same-origin allow-forms allow-scripts" src="/contact/mailuserform" width="100%" height="100%" frameborder="0" scrolling="no" marginwidth="0"></iframe>
        </div>
    </div>
    <script type="text/javascript">
        var monoco_mailuser_slide_trigger_bottom_pos = $(document).height() - $(window).height() - 500;
        $(function() {
            var slide = $('#monoco-mailuser-slide');
            var closeBtn = $('#monoco-mailuser-close-btn');
            var btnOpenFlag = false;
            var openFlag = false;
            var panelSwitch = function() {
                // 閉じる
                if (openFlag === true ) {
                    slide.removeClass("show"); 
                }
                // 開く
                else if (openFlag === false) {
                    slide.addClass("show"); 
                }
            };
            // 閉じるボタンクリックしたら
            closeBtn.click(function(){
                panelSwitch();
                openFlag = !openFlag;
                btnOpenFlag = true;
            });
            // 画面下位置を取得
            $(window).scroll(function () {
                monoco_mailuser_slide_trigger_bottom_pos = $(document).height() - $(window).height() - 500;
                if (!btnOpenFlag) {
                    if ($(this).scrollTop() >= monoco_mailuser_slide_trigger_bottom_pos) {
                        if (openFlag === false) {
                            panelSwitch();
                            openFlag = true;
                        }
                    } else {
                        if (openFlag) {
                            panelSwitch();
                            openFlag = false;
                        }
                    }
                }
            });
        });
    </script>
</div>
