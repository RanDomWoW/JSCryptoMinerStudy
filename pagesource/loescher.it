<script type="text/javascript">
    var static_isadmin= 'false';    
    var static_preview_mode = true;
    var static_canedit = 'false';
    var static_candelete = 'false';
    var static_asmx = 'Admin/CMS/UserControls/StaticContent/cms_staticcontent.asmx';

    $(document).ready(function () {
        if (static_isadmin == "true") {
            $('#static_edit').show();
        }
        else {
            $('#static_edit').hide();
        }
        $('input[type=button],button,submit').button();

        var control_url = "/Admin/CMS/UserControls/StaticContent/staticcontent_admin.aspx";
        $('.static_cms_toolbar').LoeCMSCtrlToolbar({ ctrlprefix: 'static', editimg: 'Admin/CMS/Common/images/badge_edit-64.png', delimg: 'Admin/CMS/Common/images/badge_cancel-64.png', newimg: 'Admin/CMS/Common/images/1373478750_new-24.png', canedit:false, candelete:false, cancreate:true, canpublish:false });

        $('.static_toolbar_ctrl')
            .fadeTo("slow",0.5)
            .mouseover(function() {$(this).fadeTo("fast",1.0);})
            .mouseout(function() {$(this).fadeTo("fast",0.5);});

        //aggiungiamo le funzioni per gestire gli eventi di pressione dei pulsanti edit e delete del controllo toolbar CMS Loescher
        $('.static_cms_edit').each(function(k,v) {
            $(v).attr("href",control_url+"?id="+$(v).attr("data-item-id")+"&m=edit");
            $(v).attr("data-fancybox-type", "iframe");
        });               

        $('.static_cms_delete').click(function (e) {
            var p=Object();
            p.id = $(this).attr("data-item-id");
            if (confirm("Eliminare questo elemento? (i dati non saranno recuperabili)")) {
                $.ajax({
                url:static_asmx+'/Delete',
                dataType:'json',
                type:'POST',
                data: JSON.stringify(p),
                contentType:'application/json',
                success: function(r) { 
                    CM_ShowModalMsg(500,300,"Cancellazione effettuata!"); 
                    window.location.reload(true);
                },
                error: function (j,x,e) {CM_ShowModalError(500,300,e);}
                });
            }

            e.preventDefault();
            return false;
        });

        //*** BEGIN ANDIAMO AD IMPOSTARE GLI ATTRIBUTI CORRETTI PER USARE FANCYBOX ***
        $('.static_cms_new').attr("href", control_url );
        $('.static_cms_new').attr("data-fancybox-type", "iframe");

        //forzatura applicazione fancybox
        $(".static_cms_new,.static_cms_edit").fancybox({
            //            maxWidth: 800,
            //            maxHeight: 600,            
            fitToView: false,
            width: '99%',
            height: '99%',
            autoSize: false,
            closeClick: false,
            openEffect: 'fade',
            closeEffect: 'fade'
        });
        
        $(".static_cms_publish").click(function(e) {
            e.preventDefault();
            if (confirm("Pubblicare questo contenuto rendendolo visibile a tutti?")) {
                //TODO - richiamare metodo di pubblicazione del webservice
                var p = new Object();
                p.id = $(this).attr("data-item-id");
                p.utente = '';
                $.ajax({
                url:static_asmx+'/Pubblica',
                type:'POST',
                dataType:'json',
                contentType:'application/json',
                data: JSON.stringify(p),
                success: function(data) {},
                error: function(j,x,e) {CM_ShowModalError(500,300,e);}
                });
            }
        });
        //*** END ***

        //nascondiamo il superfluo
        if (static_canedit != 'true') 
            $('.static_cms_edit').hide();

        if (static_candelete != 'true') 
            $('.static_cms_delete').hide();

    });

</script>

