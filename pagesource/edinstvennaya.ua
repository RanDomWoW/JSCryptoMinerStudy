                + '<h1 class="items-list-itm-title">'
                    + '<a class="items-list-itm-lnk" href="' + article.url + '">' + article.title + '</a>'
                + '</h1>'
            + '</section>'
            + '</div>';
        } else {
            htmlArticle += '<section class="items-list-itm latest-alticles">'
                + '<a class="items-list-img zoom-img" href="' + article.url + '">'
                    + '<img src="' + article.photos.photo_norm + '">'
                + '</a>'
                + '<h1 class="items-list-itm-title">'
                    + '<a class="items-list-itm-lnk" href="' + article.url + '">' + article.title + '</a>'
                + '</h1>'
            + '</section>';
        }
    }

    $.ajax({
        type: 'POST',
        url: '/article/index/latest',
        data: { start: start, limit: limit, type: 'latest'}
    })

    .done(function(data) {
        var data = JSON.parse(data);
        data.forEach(addArticles);
        $('div.latest-alticles-list').html(htmlArticle);
        if (data.length < limit) {
            $('#readmore-btn').hide();
        }
            });

    $( window ).resize(function() {
        oldLimit = limit
        if ($(window).width() < 900) {
            limit = 2;
        } else {
            limit = 3;
        }
        if (oldLimit !== limit) {
            start = 0;
            $.ajax({
                type: 'POST',
                url: '/article/index/latest',
                data: { start: start, limit: limit, type: 'latest'}
            })

            .done(function(data) {
                var data = JSON.parse(data);
                data.forEach(addArticles);
                $('div.latest-alticles-list').html(htmlArticle);
                $('#readmore-btn').show();
                            });
        }
    });

    $('#readmore-btn').click(function() {
        start = $(".latest-alticles").length;
        $.ajax({
            type: 'POST',
            url: '/article/index/latest',
            data: { start: start, limit: limit, type: 'latest'}
        })

        .done(function(data) {
            var data = JSON.parse(data);
            data.forEach(addArticles);
            $('div.items-list-row:last').after(htmlArticle);
            if (data.length < limit) {
                $('#readmore-btn').hide();
            }
        });

        return false;
    });
});
