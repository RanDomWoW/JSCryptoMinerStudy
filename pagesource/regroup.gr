<script type="text/javascript">
 function setCookie_Policy(pName, pValue, exdays) {
 var d = new Date();
 d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
 var expires = "expires=" + d.toUTCString();
 document.cookie = pName + "=" + pValue + ";" + expires;
}
function getCookie_Policy(pName) {
 var name = pName + "=";
 var ca = document.cookie.split(';');
 for (var i = 0; i < ca.length; i++) {
 var c = ca[i];
 while (c.charAt(0) == ' ') c = c.substring(1);
 if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
 }
 return "";
 }

        jQuery(document).ready(function () {
             var cookiesAccept = getCookie_Policy("cookie_accepted");
             if (typeof cookiesAccept == "undefined" || cookiesAccept == null || cookiesAccept == "") {
               jQuery("#cookieWrap").removeClass("no-display");
               jQuery("#cookieWrap").addClass("block-display");
               } else {
               jQuery("#cookieWrap").removeClass("block-display");
               jQuery("#cookieWrap").addClass("no-display");
             }
            return false;
   });
  function AcceptCookiesPopUp() {
      jQuery("[id=cookieWrap]").click(function (e) {
           e.preventDefault();
         setCookie_Policy("cookie_accepted", Date.now(), 365);
        jQuery("#cookieWrap").fadeOut(600);
       jQuery("#cookieWrap").removeClass("block-display");
       jQuery("#cookieWrap").addClass("no-display");
  	});
		return false;
	}
</script>
