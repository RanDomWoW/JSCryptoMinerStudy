				}
			}	
		});
	   });
	
	function startCountDown(abcd){
		var total = $('.flex-control-nav li').length;
		var avgWidth = ($(".flexslider").width() / total) - 2;
		TweenLite.killTweensOf(".timerAnim");
		$(".flex-control-nav li").css( "width", avgWidth + "px");
		$(".timerAnim").css( "width", "0px");		
		$(".timerAnim").css( "left", (abcd * (avgWidth + 1)) + "px");
		TweenLite.to (".timerAnim", 5, {css: { width: avgWidth + "px"  }, ease:Sine.easeInOut});	
	}
	$( window ).resize(function() {
		var total = $('.flex-control-nav li').length;
		var avgWidth = ($(".flexslider").width() / total) - 2;
		$(".flex-control-nav li").css( "width", avgWidth + "px");
		$(".timerAnim").css( "left", (curSlide * avgWidth) + "px");
	});
	


	
</script>
