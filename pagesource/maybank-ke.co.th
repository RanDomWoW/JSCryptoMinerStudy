				dragClickables:true,
				onClick: onElementClick,
				onDragEnd:function(e) {
					if(prevX > this.x) {
						//console.log("left :"+ e.pageX);
						onRightComplete();
						prevX = this.x;
					}
					else if(prevX < this.x) { // dragged right
						//console.log("right :"+ e.pageX);
						onLeftComplete();			
						prevX = this.x;
						
					}
					e.stopImmediatePropagation();
					e.stopPropagation();
