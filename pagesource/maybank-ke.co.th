			Draggable.get('#container').disable();
			$('#main').unbind('touchmove');
		}else{
			Draggable.get('#container').enable();
			initDragable();
			$('#main').bind(
			  'touchmove',
			  function(e) {
			  e.preventDefault();
			  }
			);
		}
			
		window.addEventListener("orientationchange", function() {
			//location.reload();
		}, false);
		
					$("#nav_right").bind(Evt.PUSH, function(e){
