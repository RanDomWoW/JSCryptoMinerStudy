						$('#hd_worldmapimg img').attr('src',$('#ddl_ww_select option:selected').attr('imgurl'));
						$('#hd_ww_link').attr('href',$('#ddl_ww_select option:selected').attr('link'));
						$('#hd_ww_button').text($('#ddl_ww_select option:selected').val());
						    window.location.href=$(this).attr('href');
						});
					}
					else
					{
						$("#ddl_ww_select").selectbox({
							onOpen: function (inst) {								
								$(".hd_dropdownlist .sbHolder").css("position","absolute");
								$("#hd_worldmapimg").css("margin-top","60px");
								
							},
							onClose: function (inst) {								
								
								
							},
							onChange: function (val, inst) {								
								ddlOnChange(val);
								
							}
						
						});							
							
							function ddlOnChange(val){
								var itemimg = $('#ddl_ww_select option[value="'+val+'"]').attr("imgurl");
								var itemvalue = $('#ddl_ww_select option[value="'+val+'"]').attr("value");
								var itemsrc = $('#ddl_ww_select option[value="'+val+'"]').attr("link");
								
