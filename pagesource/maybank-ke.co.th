		function switchLanguageUrl(lang)
		{
			
			var url = location.href;
			if(lang=="en")
			{
				
				if(url.indexOf("/en") > 0 || url.indexOf("/th") > 0)
				{
				  url = url.replace("/th","/en");
				}
				else
				{
				  url = url.replace(".co.th",".co.th/en");	
				}
			}
			else 
			{
				url = url.replace("/en","/th");	
			}
			location.href = url;
		}

