	function onRightCompleteCheck(){
		var currentPosisi = $('#container').position().left;
		var lastpoistion = $("#container").width()-$("body").width();		
		if(currentPosisi <= -lastpoistion){			
			onCheckLocation();
		}
		
	}

	function onLeftCompleteCheck(){
		var currentPosisi = $('#container').position().left;
		var lastpoistion = 0;		
		if(currentPosisi > lastpoistion){			
			onCheckLocation();
		}
		
	}

	function onCheckLocation(){
		var currentPosisi = $('#container').position().left;
		var endingpoistion = $("#container").width()-$("body").width();
		var startposistion = 0;

		if(currentPosisi <= -endingpoistion){
			onRightHide();
			//console.log('right hide');
		}else{
			onRightShow();
			//console.log('right show');
		}
		
		if(currentPosisi >= startposistion){
			onLeftHide();
			//console.log('left hide');
		}else{
			onLeftShow();
			//console.log('left show');
		}
	}

	function onRightShow(){
		TweenLite.to ("#nav_right", 0.5, {css: { right: "-23px"  }, ease:Sine.easeInOut});
		TweenLite.to ("#nav_left", 0.5, {css: {transform:"rotate(180deg)", left: "-23px"  }, ease:Sine.easeInOut});
		$("#nav_right").addClass("nav_hide");
		
	}

	function onRightHide(){
		TweenLite.to ("#nav_right", 0.5, {css: {right: "-180px"  }, ease:Sine.easeInOut});		
		$("#nav_right").addClass("nav_hide");		
	}
	
	function onLeftShow(){
		TweenLite.to ("#nav_left", 0.5, {css: {transform:"rotate(180deg)", left: "-23px"  }, ease:Sine.easeInOut});
		$("#nav_left").removeClass("nav_hide");
	}

	function onLeftHide(){		
		TweenLite.to ("#nav_left", 0.5, {css: {transform:"rotate(180deg)", left: "-180px"  }, ease:Sine.easeInOut});
		$("#nav_left").addClass("nav_hide");
		
	}
	
	
	function initDragable(){
		var width = 0;
		$('#bc_back ul li').each(function() {
				var $this = $(this);
				width += $this.outerWidth()+40;
		});
				 
		var widWidth = 0;
			$('#widget .item').each(function() {
			var $this = $(this);
			widWidth += $this.outerWidth()+20;
		});
						
						
						
		if(widWidth > width){
			width = widWidth;
		}
						
		if(width < 1500){
			width = 1500;
		}
		$('#container').css("width",(width+40)+"px");
		glowidth=$('#container').width();
		origlowidth=$('#container').width();		
	}
	
	function getRightPosition(){
		var currentPosisi = $('#container').position().left;
		var lastposition = $("#container").width()-$("body").width();
		var toposisiX = currentPosisi - 600;
		
		if(toposisiX < -lastposition){
			toposisiX = -lastposition;
		}
		return toposisiX;
	}
	
	function getLeftPosition(){
		var currentPosisi = $('#container').position().left;
		var lastposition = 0;
		var toposisiX = currentPosisi + 600;
		
		if(toposisiX > 0){
			toposisiX = 0;
		}
		return toposisiX;
	}
	
