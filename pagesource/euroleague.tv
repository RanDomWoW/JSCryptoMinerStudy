															<span class="shortTime">{{shortDate}}</span>
														</span>
													{{/if}}
												</div>
												{{#if sportHomeFirst}}
													<div class="home">
														<span class="logo"><img border="0" src="{{awayLogo}}"/>
														</span><span class="name" title="{{awayTeam.code}}">{{awayTeam.code}}</span>
													</div>
												{{else}}
													<div class="home">
														<span class="logo"><img border="0" src="{{homeLogo}}" /></span>
														<span class="name" title="{{homeTeam.code}}">{{homeTeam.code}}</span>
													</div>
												{{/if}}
											</div>
										{{else}}
											<div class="event-game">
												<li>{{name}}</li>
											</div>
										{{/if}}
									</div>
								</a>
							</div>
						{{/each}}
					</div>
				</script>
