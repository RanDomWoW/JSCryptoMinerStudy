<script>
$(function(){
    var url = '//'+location.host+'/_ajax/todayGame.php';
    var now = new Date();
	var y = now.getFullYear();
	var m = ('00' + (now.getMonth() + 1) ).slice( -2 );
	var d = ('00' + now.getDate() ).slice( -2 );
	var h = ('00' + now.getHours() ).slice( -2 );
	var mi = ('00' + now.getMinutes() ).slice( -2 );
	var s = ('00' + now.getSeconds() ).slice( -2 );
	var ymdhmis = y+'-'+m+'-'+d+' '+h+':'+mi+':'+s;

// Ajax通信を開始
    $.ajax({
        type: 'GET',
        url: url,
        cache: false,
        dataType: 'json',
        success: function(data){
            if(data){
                var twoHours = data["gameDate"]+" "+data["twoHours"];
                if (data["homeTeamCode"] == "2005001" && (h >= 1 && ymdhmis < twoHours)) {//午前1時過ぎ
                    var html = '<p class="pl_btnBlock01 pl_mb0"><a href="http://wing2.softbankhawks.co.jp/ex/door/" class="pl_btnT01 pl_w100p" target="_blank">当日券情報　vs　<img src="'+data["vsIcon"]+'"></a></p>';
                    $("#pl_ticketPurchase").find("dd").html(html);
                }else{
                    $("#pl_ticketPurchase").remove();
                }
            }else{
            	$("#pl_ticketPurchase").remove();
            }
        },
        error: function(data){
            // 通信失敗時の処理を記述
            console.log("errorrrrrrrr");
        }
    });
});
</script>

<script>
$(function(){
    var url = '//'+location.host+'/_ajax/farmTicket.php';

// Ajax通信を開始
