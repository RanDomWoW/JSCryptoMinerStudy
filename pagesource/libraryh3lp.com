    <script type="text/javascript">
      $(function() {
        $('#region option[value="' + location.hostname + '"]').prop('selected', true);
        $('#region').on('change', function() {
          window.location = 'https://' + $('#region :selected').val() + '/free-trial';
        });

        $('#trial-registration-form input[name="password"]')
          .change(function() {
            var password = $('#trial-registration-form input[name="password"]').val(),
                confirmation = $('#trial-registration-form input[name="password_confirmation"]').val();
            if (password !== confirmation) {
              $('#trial-field-alert')
                .html('<p>Passwords do not match.<\p>')
                .show();
            } else {
              $('#trial-field-alert').hide();
            }
          });
        $('#trial-registration-form input[name="password_confirmation"]')
          .change(function() {
            var password = $('#trial-registration-form input[name="password"]').val(),
                confirmation = $('#trial-registration-form input[name="password_confirmation"]').val();
            if (password !== confirmation) {
              $('#trial-field-alert')
                .html('<p>Passwords do not match.<\p>')
                .show();
            } else {
              $('#trial-field-alert').hide();
            }
          });

        $('#trial-registration-form')
          .submit(function(e){
            e.preventDefault();
            $.ajax({
              type: 'POST',
              url: 'https://' + location.hostname + '/2016-02-10/registrations',
              data: $('#trial-registration-form').serialize(),
              dataType: 'json',
              statusCode: {
                201: function() {
                  $('.trial-registration').hide();
                  $('#trial-success-alert').show();
                }
              },
              success: function(data) {
                if (data && data.error) {
                  $('#trial-field-alert')
                    .html('<p>' + data.error + '<\p>')
                    .show();
                }
              },
              error: function(xhr) {
                if (xhr.status !== 201) {
                  $('.trial-registration').hide();
                  $('#trial-failure-alert').show();
                }
              }
            });
          });
      });
    </script>
