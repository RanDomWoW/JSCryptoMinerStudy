   <script type="text/javascript" language="JavaScript">

     var villaDests = {
     "Egypt": [ "Hurghada" ],
     "England": [ "London" ],
     "Frankrike": [ "Cote d'Azur", "Nice", "Paris" ],
     "Hellas": [ "Chania", "Heraklion", "Kreta", "Korfu", "Preveza" ],
     "Irland": [ "Dublin" ],
     "Italia": [ "Milano", "Roma", "Venezia" ],
