laga", "Mallorca", "Murcia", "Tenerife" ],
/*     "Sri Lanka": [ "Sri Lanka" ], */
     "Tsjekkia": [ "Praha" ],
     "Tyrkia": [ "Istanbul" ],
     "Tyskland": [ "Berlin", "Hamburg", "Munchen" ],
     "Ungarn": [ "Budapest" ],
     "Østerrike": [ "Wien" ]
     };

     function selectVilla(country, destIndex) {
       list = $("#villaDest").get(0);
       list.options.length = 1;
       if(villaDests[country]) {
         $.each(villaDests[country], function(index, text) {
           list.options[list.options.length] = new Option(text, text);
         });
       }
       $("#villaCountry").val(country);
       list.selectedIndex = destIndex;
     }

     function quoteText(text)
     {
       return text.match(/\s/) != null ? '\"' + text + '\"' : text;
     }

     $("#villaCountry").change(function() {
       selectVilla(this.value, 0);
       $("#villaWords").val(quoteText(this.value));
     });

     $("#villaDest").change(function() {
       $("#villaWords").val(quoteText(this.value == '' ? $("#villaCountry").val() : this.value));
     });

     list = $("#villaCountry").get(0);
     $.each(villaDests, function(key, text) {
       list.options[list.options.length] = new Option(key, key);
     });

     $(list.form).removeAttr('onsubmit').submit(function (event) {
        event.preventDefault();
       $(".villaDD").each(function() { this.disabled = true; });
       this.submit();
     });

     words = $("#villaWords").val().replace(/\"/g, "");
     $.each(villaDests, function(key, value) {
       if(words == key) {
         selectVilla(key, 0);
         return false;
       } else {
         hit = 0;
         $.each(villaDests[key], function(index, val) {
           if(words == val) {
             hit = index + 1;
             return false;
           }
         });
         if(hit != '')
         {
           selectVilla(key, hit);
           return false;
         }
       }
     });

</script>

