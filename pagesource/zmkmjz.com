<script type="text/javascript">
                

    (function () {
        function addClass(eles,attr) {
            eles.each(function(){
                var anim = $(this).attr(attr);
                var animClass = anim + ' animated';
                var delay = $(this).attr('data-animated-delay');
                $(this).css({
                    '-webkit-animation-delay':delay,
                    'animation-delay':delay,
                    '-moz-animation-delay':delay
                }).addClass(animClass)
            })
        }

        function removeClass(eles,attr){
            eles.each(function(){
                var anim = $(this).attr(attr);
                var animClass = anim + ' animated';
                $(this).removeClass(animClass)
            })
        }

        var slider = new HopeSlide({
            basePath:'../img/',
            enableDots:true,
            onBeginAnimation:function (origin, newCurr, currIndex, nextIndex) {
                //删除origin上的进入动画类
                var o_sliderTxt = origin.find('.sTxt');
                if(o_sliderTxt.length > 0){

                    removeClass(o_sliderTxt,'data-animated-in');
                    addClass(o_sliderTxt,'data-animated-out');
                }

                //删除离开动画类
                var sliderTxt = newCurr.find('.sTxt');
                if(sliderTxt.length > 0){

                    removeClass(sliderTxt,'data-animated-out');
                    addClass(sliderTxt,'data-animated-in');
                }
            }
        })
    })();
</script>
