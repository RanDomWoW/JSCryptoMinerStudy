<script>
jQuery("document").ready(function(){
    jQuery(".glyphicon-menu-hamburger, .menu-hamburge-pre-home-aberto a").click(function(){
        if (jQuery(".glyphicon-user").hasClass('active-menu-mobile')){
            checkOpenClose('user');       
        }
        checkOpenClose('hamburger');
    });

    jQuery(".glyphicon-user").click(function(){
        if (jQuery(".glyphicon-menu-hamburger").hasClass('active-menu-mobile')){
            checkOpenClose('hamburger');
        }
        checkOpenClose('user');
    });

    function checkOpenClose(menu){

        if (jQuery(".background-protect").is(":visible")){
            jQuery(".background-protect").hide();
            jQuery("#wj_prehome").removeClass("wj-top-menu-fixed");
            if (menu == 'hamburger'){
                jQuery(".glyphicon-menu-hamburger").removeClass("active-menu-mobile");
                jQuery(".menu-hamburge-pre-home-aberto").hide(300);
            }
            else {
                jQuery(".glyphicon-user").removeClass("active-menu-mobile");
                jQuery(".botoes-esquerdo-mobile-dropdown").hide(300);
            }
        }
        else {
            jQuery(".background-protect").show();
            jQuery("#wj_prehome").addClass("wj-top-menu-fixed");
            if (menu == 'hamburger'){
                jQuery(".glyphicon-menu-hamburger").addClass("active-menu-mobile");
                jQuery(".menu-hamburge-pre-home-aberto").show(300);
            }
            else {
                jQuery(".glyphicon-user").addClass("active-menu-mobile");
                jQuery(".botoes-esquerdo-mobile-dropdown").show(300);
            }
        }

        if (menu == 'all'){
            jQuery(".menu-hamburge-pre-home-aberto").hide(300);
            jQuery(".botoes-esquerdo-mobile-dropdown").hide(300);
        }
    }

    jQuery(window).resize(function(evt){
        if (jQuery(this).width() <= 768 && jQuery(".background-protect").is(":visible")){
            checkOpenClose('all');
        }
    });
});
