<script type="text/javascript">
    var submitAction = "/recherche/multicriteres",
        refSubmitAction = "/recherche/reference",
        kwSubmitAction = "/recherche/litterale";


$(document).ready(function() {
    $('#searchMobile').bind('submit', function(event) {
        event.preventDefault();
        var refVal = $('#reference').val().trim(), kwVal = $('#keyword').val().trim();
        if (!refVal && !kwVal) {
            $(this).unbind('submit').submit();
        }
        else if(refVal != '' && refVal !== undefined && refVal !== null) {
            $('#keyword').attr('name', 'keyword');
            $(this).attr('action', refSubmitAction).unbind('submit').submit();
        }
