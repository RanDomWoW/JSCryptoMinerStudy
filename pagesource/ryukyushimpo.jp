        // 太字
        if (!j)
            $('#weather_' + j + '_date').addClass('active');

        // 天気
        $('#weather_' + j + '_weather').html(forecast.weather_str_short);

        // 予報
        html  = '<span class="mgr10">AM ' + ($.isPlainObject(forecast['pop_06-12']) ? 'ー' : forecast['pop_06-12'].replace(/\s/g,'')) + ' / ';
