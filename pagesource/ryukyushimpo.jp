<script>

    // グローバルナビカレント追加
    var pathName = location.pathname;
    var query = location.search;
    $('.js-currentLink').find('a').each(function(){
        var _a = $(this);
        var href = _a.attr('href');

        // トップへは完全一致
        if(href == '/') {
            if(pathName == '/') {
                _a.parent().addClass('active');
            }
            return true;
        }

        // まずgnav と比較
        var archives = ['event', 'archive'];
        if(pathName.indexOf(href) > -1) {
            _a.parent().addClass('active');
            return true;
        } else if(query != '' && href.indexOf(query) > -1) {
            _a.parent().addClass('active');
            return true;
        }

        for(var i=0; i < archives.length; i++) {
            if(pathName.indexOf(archives[i]) > -1) {
                if(href.indexOf(archives[i]) > -1) {
                    _a.parent().addClass('active');
                    return true;
                }
            }
        }

        // gnavと一致しない場合、sub_nav と比較
        $('#sub_nav').find('a').each(function(){
            var shref = $(this).attr('href');
            if(href == shref) {
                _a.parent().addClass('active');
            }
        });
    });

    //検索窓用
    $(function(){
        $("#search-scope").on('click', function () {
            if($('#search-box').is(':visible'))
            {
                var searchKeyword = $('#search-box').val();
                window.location.href = '/search.html?keyword=' + encodeURI(searchKeyword);
            }
            else
            {
                $("#search-box").show(150, 'easeInOutQuad');
                $("#search-scope").addClass("search-open");
            }
        });
    });

    $('html').on('click', function () {
        $('#search-box').fadeOut();
        $("#search-scope").removeClass("search-open");
    });

    $('#searchFormNavi').on('click', function(event) {
        event.stopPropagation();
    });

</script>



