    function ajaxPopup(url,temp,element){
        if(element == null){
            affPopup(null);
        }else{
            affPopup(element);
        }
        if(temp==null){
            new Ajax.Request(url, {
             method: 'get',
             onComplete: function(response) {
               insertHtml(response.responseText);
             }
            });
         }else{
            setTimeout(function(){
                insertHtml(temp);
            }, 1000); 
            
         }
    }
</script>    </div>
