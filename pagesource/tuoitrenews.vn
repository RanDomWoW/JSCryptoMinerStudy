function loading() {
    var startTime = new Date().getTime();
    $('<div id="loading"><img src="https://static.tuoitrenews.vn/assets/ttnew/taDf1OZ1wQ/v1/images/loading.gif" /></div>').appendTo('body');
    var allImages = $('img'),
    imgLength = allImages.length,
    countImage = 0;
    for(var i = 0; i < imgLength; i++){
        var img = new Image();
        img.onload = function(){
            countImage++;
            if(countImage == imgLength){
                var endTime = new Date().getTime();
                if(endTime - startTime > 500){
                    onReady();
                }else{
                    setTimeout(function(){
                        onReady();
                    },500 - (endTime - startTime));
                }
            }
        };
        img.onerror = function(){
            countImage++;if(countImage == imgLength){
               var endTime = new Date().getTime();
                if(endTime - startTime > 2000){
                    onReady();
                }else{
                    setTimeout(function(){
                        onReady();
                    },1000 - (endTime - startTime));
                }
            }
        };
        img.src = allImages.eq(i).attr('src');
    }
}

