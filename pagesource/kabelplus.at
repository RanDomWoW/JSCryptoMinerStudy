<div ng-controller="smallinputController" id="VerfuegbarkeitsrechnerSmall">
    <div ng-form="VerfuegbarkeitsrechnerSmall">
        <h3>
            Verfügbarkeit<a href="CMSWebParts/KabelPlus/Availability/#" id="ctl00_ctl12_availabilityCheck_aExpander" title="Verfügbarkeit Formular anzeigen"></a>
        </h3>
        <p>
            Bitte prüfen Sie, welche Produkte an Ihrer Wohnadresse verfügbar sind.
        </p>

        <div class="availability">
            <div ng-form="addresssmall">
                <span class="fields clearfix">
                    <input id="Zip_small_popup"
                        ng-readonly="goingtoProducts"
                        name="Zip_small_popup"
                        maxlength="4"
                        class="zip"
                        type="text"
                        ng-init=""
                        placeholder="PLZ *"
                        ng-model="adress.Zip"
                        ng-enter="checkadress()"
                        ng-required="!adress.City"
                        ng-model-options="{ updateOn: 'keyup blur' }"
                        maxlength="4"
                        ng-pattern="/(^\d{4}$)|^$/" />
                    <input id="City_small_popup"
                        ng-readonly="goingtoProducts"
                        name="City_small_popup"
                        class="city"
                        type="text"
                        ng-init=""
                        placeholder="Ort *"
                        ng-enter="checkadress()"
                        ng-model="adress.City"
                        ng-required="!adress.Zip" />
                </span>

                <span class="fields clearfix">
                    <input ng-init=""
                        ng-readonly="goingtoProducts"
                        placeholder="Straße"
                        id="Street_small_popup"
                        name="Street_small_popup"
                        class="street"
                        type="text"
                        required="required"
                        ng-enter="checkadress()"
                        ng-model="adress.Street" />
                    <input id="Number_small_popup"
                        ng-readonly="goingtoProducts"
                        name="Number_small_popup"
                        class="number"
                        type="text"
                        required="required"
                        ng-model="adress.Housenumber"
                        ng-init=""
                        ng-enter="checkadress()"
                        placeholder="Nr." />
                    <input id="Stair_small_popup"
                        ng-readonly="goingtoProducts"
                        name="Stair_small_popup"
                        class="stair"
                        type="text"
                        ng-model="adress.Stair"
                        ng-init=""
                        ng-keyup="$event.keyCode == 13 && checkadress()"
                        placeholder="Stiege" />
                </span>
                <p class="error ng-hide" ng-show="VerfuegbarkeitsrechnerSmall.$submitted && !VerfuegbarkeitsrechnerSmall.$valid">
                    <span class="ng-hide" ng-show="addresssmall.Zip_small_popup.$error.required || addresssmall.City_small.$error.required ">
                        PLZ und/oder Ort erforderlich
                        <br />
                    </span>
                    <span class="ng-hid" ng-show="addresssmall.Zip_small_popup.$error.pattern">
                        PLZ muss aus vier Zahlen bestehen 
                        <br />
                    </span>
                    <span class="ng-hide" ng-show="addresssmall.Street_small_popup.$error.required ">
                        Geben Sie eine Straße ein
                        <br />
                    </span>
                    <span class="ng-hide" ng-show="addresssmall.Number_small_popup.$error.required">
                        Geben Sie eine Hausnummer ein
                    </span>
                </p>
                <p ng-show="error" class="error ng-hide">{{error}}</p>
            </div>

            

            <div ng-show="goingtoProducts">
                <span>
                    Wir leiten Sie jetzt zu den Produkten weiter
                </span>
            </div>

            <a class="btnLink AvailabilityCheck"
                ng-click="checkadress()"
                ng-class="{disabled: checking}"
                id="Submit_Small_popup"
                onmousedown="ET_Event.eventStart('Verfugbarkeitsabfrage','Sidebar','Gestartet','')">

                Prüfen
            </a>
            <img src="/App_Themes/kabelplus/angular/res/loading.gif" alt="loading" class="loading pull-right" ng-show="checking" />
            <br class="clr">
        </div>
