	<script>
		$(function () {
            var $heroAccordion = $('.hero_accordion'),
                interval, timeout, auto, active = 0;

            if (!$heroAccordion || $heroAccordion.length == 0) return;

            function stopInterval () {
                if (interval) window.clearInterval(interval);
                if (timeout) window.clearTimeout(timeout);
            }

            function startInterval () {
                stopInterval();
                interval = window.setInterval(function () {
                    if (++active > 3) active = 0;
                    auto = true;
                    $heroAccordion.find('li').removeClass('ui-state-focus');
                    $heroAccordion.tabs( "select", active );
                    auto = false;
                }, 9000);
            }

            $heroAccordion.tabs({
                fx: [
                    {opacity:'toggle', duration:'fast'},   // hide option
                    {opacity:'toggle', duration:'fast'}
                ],
                select: function( event, ui ) {
                    active = ui.index;
                    if (!auto) { // user click
                        stopInterval();
                        timeout = window.setTimeout(startInterval, 5000);
                    }
                }
            });

            startInterval();
		});
	</script>

