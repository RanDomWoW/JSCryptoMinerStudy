<script>
    (function(mapsApp) {
        var input = (document.getElementById('store-location'));
        var urlStorefinder = 'https://www.steiff.com/de-de/storefinder/';

        mapsApp.setupAutocomplete = function() {
            var autocomplete = new google.maps.places.Autocomplete(input);

            google.maps.event.addListener(autocomplete, 'place_changed', function () {
                var place = autocomplete.getPlace();

                if(!place.geometry && document.querySelector('.pac-container').childElementCount >= 1){
                    mapsApp.enterKeyFallback(input.value);
                }
                else if (!place.geometry) {
                    return;
                } else {
                    window.location.href =urlStorefinder + '#!' + place.geometry.location.lat() + '/' + place.geometry.location.lng();
                }
            });
        };

        mapsApp.enterKeyFallback = function (searchString) {
            function getSuggestions(predictions, status) {
                if (status != google.maps.places.PlacesServiceStatus.OK) {
                    alert('Sorry, something went wrong :( '+ status);
                    return;
                }

                if (predictions.length >= 1){
                    var placeId =  predictions[0].place_id;

                    var geocoder = new google.maps.Geocoder;

                    geocoder.geocode({'placeId': placeId}, function(results, status) {
                        if (status == google.maps.GeocoderStatus.OK) {
                            window.location.href =urlStorefinder + '#!' + results[0].geometry.location.lat() + '/' + results[0].geometry.location.lng();
                        }else {
                            alert('Google Service is unavailable, sorry.');
                        }
                    });
                }else {
                    alert('Please check your input');
                }
            }

            var service = new google.maps.places.AutocompleteService();
            service.getQueryPredictions({ input: searchString }, getSuggestions);

        };

        mapsApp.loadGoogleMapsScript = function () {
            var callbackFunction = (window.hasOwnProperty('mapContainer'))?'initMap':'mapsApp.setupAutocomplete';
            var script = document.createElement('script');
            script.type = 'text/javascript';
            script.src = '//maps.googleapis.com/maps/api/js?v=3&libraries=places&sensor=FALSE&key=AIzaSyAgcVALmdv_DH3LR1fHJPMShnZUYQvfqJA&callback='+callbackFunction;
            document.body.appendChild(script);

            if (callbackFunction === 'initMap'){
                var footerSearch = document.querySelector('#store-location');
                document.querySelector('.store-search .input-box').removeChild(footerSearch);
            }
        };

    }(window.mapsApp = window.mapsApp || {}));
    window.onload = mapsApp.loadGoogleMapsScript;
</script>
                                



