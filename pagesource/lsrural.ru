                var valide = validateInteger(val);
            }
            return valide;
        }

        function defineTypeAndValide(el, val) {
            var valueType = $(el).attr('data-format');
            if (valueType === "float") {
                var valide = validateInput(val, 'float');
            } else if(valueType === "integer") {
                var valide = validateInput(val, 'int');
            }
            return valide;
