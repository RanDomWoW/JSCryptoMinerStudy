<script>
  document.write('<div id="poll-content"></div>');
$('#poll-content').load('/polls/latest', function(response, status, xhr) {
	form = $('#poll-content form')[0];
	if(form) {
		form = $(form);
		form.submit(function(event) {
			/* stop form from submitting normally */
			event.preventDefault();
			/* Send the data using post and put the results in a div */
			$.post( form.attr('action'), form.serialize(),
			function( data ) {
				$('#poll-content').empty().append(data);
			}
			);
		});
	}
});
</script>


