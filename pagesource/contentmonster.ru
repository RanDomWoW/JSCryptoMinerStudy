<script>
    $(function () {
        resizeVideo();
        revScroll.init();
        setTimeout(revScroll.init, 300);
        $(window).on('resize', function () {
            resizeVideo();
            revScroll.init();
        });
    });

    function resizeVideo() {
        var videoWidth = $('.video-block').width();
        $('iframe').prop('height', videoWidth / (16 / 9));
    }
    var revScroll = {
        spPage: 1,
        loading: false,
        pane: $('.scroll-pane'),
        spUl: $('.scroll-pane').find('ul'),
        optins: {
            verticalDragMinHeight: 48,
            verticalDragMaxHeight: 48,
            showArrows: true
        },
        init: function () {
            revScroll.pane.jScrollPane(revScroll.options).bind(
                    'jsp-scroll-y', $.proxy(revScroll.scrollHandler, revScroll));
        },
        loadItems: function () {
            $.post('/review/load', { page: ++revScroll.spPage }, function (ret) {
                        $('#revloading').remove();
                        if (!ret.items.length) {
                            revScroll.scrollHandler = null;
                            return;
                        }
                        $.each(ret.items, $.proxy(revScroll.appendItem, revScroll));
                        revScroll.loading = false;
                        revScroll.init();
                    }
            )
        },
        appendItem: function (i, e) {
            if (e.fname || e.lname) {
                e.login = e.fname + ' ' + e.lname;
            }
            this.spUl.append($('<li> <p>' + e.text + '</p><em class="nickName">' + e.login + '</em></li>'));
        },
        scrollHandler: function (event, scrollPositionY, isAtTop, isAtBottom) {
            if (isAtBottom && !this.loading) {
                this.loading = true;
                this.spUl.append($('<li id="revloading"><img src="/css/img/circle-apple-small.gif"></li>'));
                this.loadItems();
                this.init();
            }
        }
    }
</script>
