<html lang="ja">


	<head>
		<meta http-equiv="content-type" content="text/html;charset=utf-8">
		<title>Plentycom Systems</title>

		<SCRIPT LANGUAGE="JavaScript"><!--

  var lng = getBrowserLANG()

  if( lng.substr(0,2)=='ja' )	window.location.href="index_j.html"
  else 							window.location.href="en/index.html"

	function getBrowserLANG()
	{
		if(document.all)                  
    		return navigator.browserLanguage      //IE用
  		else if(document.layers) 
    		return navigator.language             //N4用
  		else if(document.getElementById) 
    		return navigator.language.substr(0,2) //N6,Moz用
	 }
//-->

</SCRIPT>
