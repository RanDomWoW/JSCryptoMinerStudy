      // 출발월 설정
      function setStartMonth(){
        var nowDt = new Date();
        var dt = new Date();
        var nowYear, nowMonth, num, i;
        var optionStr = '';

        for (i=1;i<13;i++){

          num = nowDt.getMonth() + i;
          if (num == 12){ // 12를 더하면 0월이 되어서
            nowYear = dt.getFullYear();
            nowMonth = '12';
          }else if(num > 12){
            dt.setMonth(num-12);
            nowYear = dt.getFullYear() + 1;  
            //nowMonth = dt.getMonth();
            
            nowMonth = num-12;
            if(nowMonth.length < 2){
              nowMonth = '0'+nowMonth
            }
          }else{
            dt.setMonth(num);
            nowYear = dt.getFullYear();
            nowMonth = dt.getMonth();
          }
          // 한자리수 월이면 0 추가
          if ( String(nowMonth).length == 1){
            nowMonth = "0" +  nowMonth;
          }
          optionStr = optionStr + '<option value="' + nowYear + '-' + nowMonth + '">' +  nowYear + '년' +  nowMonth +  '월</option>';
        }

        $('#om_text_tour_date').append(optionStr);
      }


