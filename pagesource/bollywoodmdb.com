    <script>
      function menuReady() {
        if (AUTH.isUserlogin()) {
          var d = AUTH.getSessionStorage('auth-data');
          $("#loginMenu").find("span").text(d.gurl)
        } else {
          $("#loginMenu").find("span").text('Login / Register')
        }


        setCityMenu();
      }
      function setCityMenu() {
        var l = localStorage.getItem('city_name');
        var c = localStorage.getItem('city');

        if (l !== '' && l !== null && typeof l !== 'undefined') {
          c = c.toLowerCase();
          l = l.replace(/\b[a-z]/g, function (letter) {
            return letter.toUpperCase();
          });
          $("#cityinfo").append('<img class="w3-hide" width="16" height="16" src="http://cdn.bollywoodmdb.com/images/location.svg">&nbsp;<a class="cus-none titlecolor" href="http://www.bollywoodmdb.com/showtimes/city-' + c + '" class="icon">' + l + '</a>');
          $("#mob-city").append('<a class="cus-none titlecolor" href="http://www.bollywoodmdb.com/showtimes/city-' + c + '" class="icon"><img class="w3-hide" width="16" height="16" src="http://cdn.bollywoodmdb.com/images/location.svg">&nbsp;<strong>' + l + '</strong></a>');
          $("#cityinfo,#mob-city").find('img').removeClass('w3-hide');
        } else {
          $("#mob-city").hide();
        }
      }

      function pageReady() {
        $.ajaxSetup({
          headers: {'X-CSRF-Token': $('meta[name=_token]').attr('content')}
        });

        $(".mo-menu").click(function () {
          $(".nav-menu").animate({"left": "0px"}, "fast");
        });
        $("#closebtn").click(function () {
          $(".nav-menu").animate({"left": "-250px"}, "fast");
        });
        $(".dropdown").click(function () {
          if ($(this).hasClass('active')) {
            $(this).removeClass('active')
            $(this).find('.sub-menu').hide();
          } else {
            $(this).addClass('active')
            $(this).find('.sub-menu').show();
          }
        });

        $(".dropdown").hover(function () {
                  $(this).children('.sub-menu').show();
                },
                function () {
                  $(this).children('.sub-menu').hide();
                });
        $("#showtimeMenu").on('click', function () {
          $("#showtimeBox").show();
          $(".nav-menu").animate({"left": "-250px"}, "fast");
        })
        /* User login system*/
        $("#loginMenu").on('click', function () {
          if (!AUTH.isUserlogin()) {
            $("#login").show();
          } else {
            $("#userMenu").show();
          }
        })
        $("#forgetpassword").click(function () {
          $("#sociallogin").addClass('w3-hide').removeClass('w3-show');
          $(".forget").removeClass('w3-hide').addClass('w3-show');
        })
        $("#registerbtn").click(function () {
          $("#loginMain").addClass('w3-hide').removeClass('w3-show');
          $("#register").removeClass('w3-hide').addClass('w3-show');

        })

        $("#signupbtn").click(function () {
          $("#loginMain").removeClass('w3-hide').addClass('w3-show');
          $("#register").removeClass('w3-show').addClass('w3-hide');
          $("#sociallogin").addClass('w3-show').removeClass('w3-hide');
          $(".forget").addClass('w3-hide').removeClass('w3-show');
        })

        $("#signupauth").on('click', function () {
          $.ajax({
            url: 'http://www.bollywoodmdb.com//login/bollywoodmdb',
            type: "POST",
            data: {userid: $("#userid").val(), password: $("#password").val()},
            success: function (data) {
              var response = JSON.parse(data);
              if (response.isSuccess) {
                localStorage.setItem("auth-data", JSON.stringify(response));
                window.location.reload();
              } else {
                $("#messageID").removeClass('w3-hide').html(response.error_message);
              }
            }
          });
        });
      }

    </script>
