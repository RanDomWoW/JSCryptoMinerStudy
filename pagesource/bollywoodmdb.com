    <script>
      if(!isMobileDevice()) {
        $(document).ready(function () {
          var obj = $('#sticky');
          var offset = obj.offset();
          var topOffset = offset.top;

          var obj_r = $('#stickyright');
          var offset_r = obj_r.offset();
          var topOffset_r = offset_r.top;


         if($('#sticky').length>0) {
           $(window).scroll(function () {
             var scrollTop = $(window).scrollTop();
             if (scrollTop >= topOffset) {
               obj.css({
                 top: 0,
                 position: 'fixed',
               });
               obj_r.css({
                 top: 0,
                 position: 'fixed',
               });
             } else if (scrollTop < topOffset) {
               obj.css({
                 top: 200,
                 position: 'absolute',
               });
               obj_r.css({
                 top: 200,
                 position: 'absolute',
               });
             }
           });
         }


        });
      }

    </script>


