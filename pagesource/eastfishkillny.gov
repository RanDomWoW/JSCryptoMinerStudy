<!-- BEGIN East Fishkill Public Alerts Script -->
<script src="http://www.eastfishkillny.gov/PublicAlerts/Scripts/jquery.cookie.js"></script>

 <script>
        var i = 0;
        var MuniCode = '';
        var IsInternetPage = 'true';
        var webURL =''
        //New cookie code added
        var now = new Date();
        var time = now.getTime();
        time += 1200 * 1000;  //(#min * 60) * 1000 1200 =20 minutes
        now.setTime(time);
	//Enter the URL of the server where this will be running
        webURL = 'http://www.eastfishkillny.gov/'
        //webURL = "http://testmydci.dcny.gov"
        if ($.cookie('TimeUp') == null) {
            $.cookie('TimeUp', 'true', { expires: now });
        }
        //Enter the MUNIPOICode here
        MuniCode = 'EF';
        // if IsInternetPage is true NO Banner will be displayed (banner will be internet banner and the responsibility of WebTeam) 
        IsInternetPage = 'true';
        
        //below resets cookie
        //$.cookie('TimeUp','true',{expires: 6000});
        //End of New Cookie code
        // $.get("http://10.42.196.242/PublicAlerts/PAHandler.ashx?munipoicode=" + MuniCode, function (data, status) {
        // comment above and uncomment below for publish version
        // $.get("CBCPublicAlerts.ashx", function (data, status) {
        //$.get("PAHandler.ashx?munipoicode=" + MuniCode, function (data, status) {
       // $.get("http://testmydci.dcny.gov/PublicAlerts/PAHandler.ashx?munipoicode=" + MuniCode, function (data, status) {

        $(document).ready(function () {
                // this is for developement environment
           // $.get("PAHandler.ashx?munipoicode=" + MuniCode, function (data, status) {
                // this is for production
                $.get(webURL + "/PublicAlerts/PAHandler.ashx?munipoicode=" + MuniCode, function (data, status) {
                    $.each(data, function (key, value) {
                    $('<li>', { text: value }).appendTo($('#PublicAlerts'));
                    if (i == 0) {
                        $(".carousel-inner-alerts ").append($('<div class="item active" style="text-overflow: ellipsis; white-space:nowrap;"><p>' + value + '</p></div>'));
                    }
                    else {
                        $(".carousel-inner-alerts").append($('<div class="item" style="text-overflow: ellipsis; white-space:nowrap;"><p>' + value + '</p></div>'));
                    }
                    i++;

                });


                if (i > 0) {
                    // if there are published alerts  make sure you can see the red bar
                    $('#AlertCtnr').removeClass('hidden');
                    //changed for cookie code
                    if ($.cookie('TimeUp') == 'true') {
                        //  if (window.name != "foo"  || $.cookie('TimeUp')==true ) {
                        //before opening modal set to false so refreshing doesn't open modal again
                        $.cookie('TimeUp', 'false', { expires: now });
                        $('#myModal').modal('show');
                        // window.name = "foo";
                        //Set cookie... expire it in amount of time set

                    };
                    $("a#AddOfAlertsLarge").attr('href', webURL+'/Government/PublicAlerts.htm' );
                    $("a#AddOfAlertssmall").attr('href', webURL + '/Government/PublicAlerts.htm' );
                    //$("a#AddOfAlertsLarge").attr('href', 'http://10.42.196.242/PublicAlerts/browsealerts2.aspx?munipoicode=' + MuniCode);
                    //$("a#AddOfAlertssmall").attr('href', 'http://10.42.196.242/PublicAlerts/browsealerts2.aspx?munipoicode=' + MuniCode);

                }
                else {
                    // if no alerts hide the red panel
                    $('#AlertCtnr').addClass('hidden');
                    //comment out line below if after there are no alerts more come and you want the modal pop again
                    window.name = "";
                }
            });
        });

    </script>
    
<!-- END East Fishkill Public Alerts Script -->
   

