    $(window).on('cartProductRemoved.avanticart', function(event, data){

        var productID = data.product_id;
        var productQty = data.product_quantity;

        ga('ec:addProduct', {
            'id': productID,
            'quantity': productQty
        });

        ga('send', 'event', 'UX', 'click', 'remove from cart');
        ga('ec:setAction', 'remove');
    });
</script>

