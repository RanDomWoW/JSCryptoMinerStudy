    <script>
        window.setInterval(function(){
            var newsletter_subscribe_time = parseInt(jQuery.cookie('newsletter_subscribe_time'));
            var now = getCurrentTimestamp();

            if (newsletter_subscribe_time + 3600 * 24 > now){
                setCountdown(newsletter_subscribe_time + 3600 * 24, now);
            } else {
                $('#newsletter_expire_time').hide();
            }

        }, 1000);

        function setCountdown(expire_time, now){

            var diff = expire_time - now;
            var hour = parseInt(diff / 3600);
            var rest = diff % 3600;
            var minutes = parseInt(rest / 60);
            var sec = rest % 60;

            if (sec < 10){
                sec = "0" + sec;
            }

            if (minutes < 10){
                minutes = "0" + minutes;
            }

            $('#countdown').text(hour + ':' + minutes + ':' + sec);
            $('#newsletter_expire_time').show();
        }

        function getCurrentTimestamp(){

            var now = parseInt(Date.now()/1000) | 0;
            return now;
            /*
             if (typeof Date.now == "undefined") {
             Date.now = function(){return new Date().getTime()};
             }
             */

        }

    </script>
    
    
