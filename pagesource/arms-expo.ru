    $(document).ready(function(){

        var inspectMobile = function() {

            var mobile = false;
            var browser = navigator.userAgent;
            var mobileBrowsers = ["iPhone", "iPad", "iPod"];

            for (var item of mobileBrowsers) {

                var regExp = new RegExp(item);

                if (regExp.test(browser)) {

                    mobile = true;

                };

            };

            return mobile;

        };

        if (inspectMobile() == true) {

            var elems = {
                menu: document.getElementsByClassName("main-menu")[0],
                bgMenuPart: document.getElementsByClassName("bg-menu-part")[0],
