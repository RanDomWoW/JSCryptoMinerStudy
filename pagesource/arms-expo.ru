            }

            if (elems.censorship) {
                elems.censorship.style.backgroundImage = "url(/images/icons-s2f36198cb6.png?1521416943&v2)";
            }

            if (elems.banners) {

                for (var i = 0; i < elems.banners.length; i++) {

                    var banner = elems.banners[i];

                    if (banner.classList.contains("content")) {

                        var img = banner.getElementsByTagName("img")[0];
                        var imgSrc = img.getAttribute("src");

