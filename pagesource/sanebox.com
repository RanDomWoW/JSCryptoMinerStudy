function get_video_id(yp) {
  var video_url = yp.getVideoUrl();
  var index = video_url.indexOf("v=");
  var video_id = video_url.substring(index + 2);
  var extra_content = video_id.indexOf("&");
  if (extra_content > 0) {
    video_id = video_id.substring(0, extra_content);
  }
  return video_id;
}

function initTrack(yp) {
  var video_id = get_video_id(yp);
  if(sbYTStore.hasOwnProperty(video_id) == false) {
    sbYTStore[video_id] = {'played': [], 'total': Math.round(yp.getDuration()), 'element_timer': null, continue: true, 'fired_off': 10};
  }
}

function keepTrack(yp) {
  var video_id = get_video_id(yp);
  if (sbYTStore[video_id].continue) {
    var position = Math.round(yp.getCurrentTime());
    if (sbYTStore[video_id].played.indexOf(position) == -1) {
      sbYTStore[video_id].played.push(position);
    }
    var p = percentPlayed(sbYTStore[video_id]);
    if (p >= sbYTStore[video_id].fired_off) {
      window.ga('send', 'event', sendYTGAEvent(null, 'play_0' + sbYTStore[video_id].fired_off, video_id));
