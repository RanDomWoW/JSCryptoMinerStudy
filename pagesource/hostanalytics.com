<script>
    jQuery(document).ready(function() {
        var promoPopup = jQuery('#promoPopup');

        var promoContainer = jQuery('#desktopPromoPopupContainer');

        jQuery('#promoPopupClose').on('click', function() {
            promoContainer.hide();
            var d = new Date();
            d.setTime(d.getTime() + (2*24*60*60*1000));
            var expires = "expires="+ d.toUTCString();
            document.cookie = "promo_seen=true;" + expires + ";path=/";
        });

        jQuery('#promoPopupCloseMobile').on('click', function() {
            jQuery('#mobilePromoContainer').hide();
            var d = new Date();
            d.setTime(d.getTime() + (2*24*60*60*1000));
            var expires = "expires="+ d.toUTCString();
            document.cookie = "promo_seen=true;" + expires + ";path=/";
        });

        showPromoPopup();
        jQuery(window).resize(function() {
            showPromoPopup();
        });

        function showPromoPopup() {
            if(jQuery(window).width() > 991)
            {
                promoContainer.delay(2500)
                    .queue( function(next){
                        jQuery(this).css('visibility', 'visible');
                        var promoPopupOffset = promoPopup.height()/2 + 'px';
                        promoPopup.css('top', 'calc(50vh - ' + promoPopupOffset);
                        promoPopup.delay(200)
                            .queue(function(next) {
                                jQuery(this).slideDown('slow');
                                next();
                            });
                        next();
                    });
            }
        }
    })
</script>

