    (function ($) {
        var navbarVisible = false;

        $(window).scroll(function () {

            var ht = $('header').height() + 480;

            if ($(this).scrollTop() >= ht) {

                if (!navbarVisible) {
                    $("#navbar-main").addClass("navbar-fixed-top")
                        .hide()
                        .fadeTo('2000', '1');

                    $(".so-panel:first").css("padding-top", "30px");
                    navbarVisible = true;
                }
            }
            else {
                $("#navbar-main").removeClass("navbar-fixed-top").removeAttr('style');

                $(".so-panel:first").css("padding-top", "0px");
                navbarVisible = false;
            }

        });
    })(jQuery);
</script>
<script>
    (function ($) {
        $('a[href^="#"]').on('click', function (e) {
            e.preventDefault();

            var target = this.hash;
            var $target = $(target);
            if ($target.length) {

                $('html, body').stop().animate({
                    'scrollTop': $target.offset().top
                }, 900, 'swing', function () {
                    window.location.hash = target;
                });
            }
        });
    })(jQuery);
</script>
<script>
    (function ($) {
        $(window).scroll(function () {
            if ($(this).scrollTop() > 50) {
                $('#back-to-top').fadeIn();
            } else {
                $('#back-to-top').fadeOut();
            }
        });
        // scroll body to 0px on click
        $('#back-to-top').click(function () {
            $('#back-to-top');
            $('body,html').animate({
                scrollTop: 0
            }, 800);
            return false;
        });

        $('#back-to-top');

    })(jQuery);
