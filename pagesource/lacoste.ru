<style>
  .sale-popup {
    display: flex;
    width: 739px; background-color: #fff;
  }
  .sale-popup__img-container {
    flex-basis: 50%;
    flex: 0 0 auto;
  }
  .sale-popup__img {
    display: block;
  }
  .sale-popup__descr {
    text-align: center;
    display: flex;
    flex-direction: row;
    align-items: center;
    position: relative;
  }
  .sale-popup__descr-inner {
    padding: 0 40px;
  }
  .sale-popup__title {
    font: normal 24px/30px 'FuturaFuturisC-Bold';
    text-transform: uppercase;
    color: #303030;
  }
  .sale-popup__conditions {
    font: normal 16px/16px 'FuturaFuturisC';
    color: #000;
    margin-top: 13px;
  }
  .sale-popup__coupon {
    font: normal 20px/30px 'FuturaFuturisC';
    text-transform: uppercase;
    color: #303030;
    margin-top: 12px;
  }
  .sale-popup__btn {
    color: #fff;
    background-color: #303030;
    text-transform: uppercase;
    font: normal 16px/16px 'FuturaFuturisC';
    padding: 10px 18px;
    margin: 15px 0;
    display: inline-block;
    transition: all 0.2s;
    cursor: pointer;
    text-decoration: none;
  }
  .sale-popup__btn:hover,
  .sale-popup__btn--used {
    color: #303030;
    background-color: #fff;
    box-shadow: inset 0 0 0px 2px #303030;
  }
  .sale-popup__disclaimer {
    font: normal 12px/16px 'FuturaFuturisC';
    color: #b6b6b6;
    position: absolute;
    bottom: 18px;
    padding: 0 20px;
    width: calc(100% - 40px);
  }

  .sale-popup .popup__close {
    position: absolute;
    opacity: .8;
		right: -55px;
    top: 0;
    cursor: pointer;
    width: 16px;
    height: 16px;
    background: url('/img/popup-close.png') no-repeat 0 0;
    background-size: contain!important;
    z-index: 10005;
  }

	.sale-popup__title-comment {
		font: normal 16px/16px 'FuturaFuturisC';
	  color: #000;
		text-transform: none;
		margin-top: -2px;
		}
</style>
