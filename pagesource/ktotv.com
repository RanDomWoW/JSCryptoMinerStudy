                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="exampleModalLabel">Email</h4>
            </div>
            <div class="modal-body">
                <form id="mailForm" action="/sendMail" method="POST">
                    <div class="form-group">
                        <label for="to-name" class="control-label">*Destinataire:</label>
                        <input type="text" class="form-control" name="to_name" id="to-name">
                    </div>
                    <div class="form-group">
                        <label for="from-name" class="control-label">*Expéditeur:</label>
                        <input type="text" class="form-control" name="from_name" id="from-name">
                    </div>
                    <div class="form-group">
                        <label for="subject-name" class="control-label">*Sujet:</label>
                        <input type="text" class="form-control" name="subject" id="subject-name" value="">
                    </div>
                    <div class="form-group">
                        <label for="url-name" class="control-label">*URL:</label>
                        <input type="text" class="form-control" id="url-name" name="url" value="">
                    </div>
                    <div class="form-group">
                        <label for="message-text" class="control-label">Message:</label>
                        <textarea class="form-control" name="message" id="message-text"></textarea>
                    </div>
                </form>
            </div>

            <div class="modal-footer">
                <button id="close_email_popup" type="button" class="btn btn-default" data-dismiss="modal">Fermer
                </button>
                <button type="button" class="btn btn-primary" onclick="sendMail();">Envoyer le message</button>
            </div>
        </div>
    </div>
</div>

<script>

    function sendMail() {

        if (check_form()) {
            $.ajax({
                url: $('#mailForm').attr('action'),
                type: $('#mailForm').attr('method'),
                data: $('#mailForm').serialize() + "&type=mail",
                dataType: "json",
                success: function(res) {
                    alert("Message envoyé.");
                    $("#close_email_popup").click();
                },
                error: function(res) {
                    alert("Erreur lors de l'envoie du mail.");
                }
            });
        } else {
            alert("Merci de remplir les champs obligatoires.");
        }
    }

    function check_form() {
        var tmp = true;
        $('#mailForm input, #mailForm select').each(
                function(index){
                    var input = $(this);
                    if (input.val() == "") {
                        tmp = false;
                        return false;
                    }
                }
        );

        if (!tmp)
            return false;
        return true;
    }

</script>

        


