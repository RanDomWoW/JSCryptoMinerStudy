                                        var str = txt.value;
                                        if (str != "") {
                                            var rex = /[0-9]{1,}/gi;
                                            var arr = str.match(rex);
                                            if (arr != null && arr.length > 0) {
                                                var AA = new Array("零", "一", "二", "三", "四", "五", "六", "七", "八", "九");
                                                var BB = new Array("", "十", "百", "千", "萬", "億", "圓", "");
                                                for (var j = 0; j < arr.length; j++) {
                                                    var a = arr[j], k = 0, re = "";
                                                    for (var i = a.length - 1; i >= 0; i--) {
                                                        switch (k) {
                                                            case 0: re = BB[7] + re; break;
                                                            case 4: if (!new RegExp("0{4}//d{" + (a.length - i - 1) + "}$").test(a)) re = BB[4] + re; break;
                                                            case 8: re = BB[5] + re; BB[7] = BB[5]; k = 0; break;
                                                        }
                                                        if (k % 4 == 2 && a.charAt(i) == "0" && a.charAt(i + 2) != "0") re = AA[0] + re;
                                                        if (a.charAt(i) != 0) re = AA[a.charAt(i)] + BB[k % 4] + re; k++;
                                                    }
                                                    str = str.replace(a, re);
                                                }
                                            }
                                        }
                                        txt.value = str;
                                    }
                                    catch (e) { }
                                }
