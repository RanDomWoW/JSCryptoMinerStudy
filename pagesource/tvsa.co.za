	<script type="text/javascript">
        //<![CDATA[
        function HideAdd(id)
        {
            document.getElementById(id).style.display = 'none';
        }
        function OpenSearchBox()
        {
            try
            {
                var searchBox = document.getElementById('searchContainer'), inputBox = document.getElementById('txtGSearch');
                inputBox.style.width = searchBox.style.width = '1px';
                searchBox.style.display = 'block';

                var containerPos = GetElementPositionById('headercontainer');
                var searchBoxPos = GetElementPosition(searchBox), inputBoxPos = GetElementPosition(inputBox)
                searchBox.style.width = (containerPos.right - searchBoxPos.left - 20) + 'px';
                inputBox.style.width = (containerPos.right - inputBoxPos.left - 90) + 'px';

                document.getElementById('txtGSearch').focus();

                if (window.addEventListener)
                {
                    
                    window.setTimeout('document.addEventListener(\'click\', window[\'CloseSearchBox\'], false);', 100);
                    
                }
                else if (window.attachEvent)
                {
                    
                    window.setTimeout('document.attachEvent(\'onclick\', window[\'CloseSearchBox\']);', 100);
                    
                }
            }
            catch (ex) { }
        }

        function CloseSearchBox()
        {
            document.getElementById('searchContainer').style.display='none';

            if (window.addEventListener)
            {
                
                document.removeEventListener('click', CloseSearchBox, false);
                
                window.removeEventListener('resize', CloseSearchBox, false);
            }
            else if (window.attachEvent)
            {
                
                document.detachEvent('onclick', CloseSearchBox, false);
                
                window.detachEvent('onresize', CloseSearchBox, false);
            }
        }

        function DoClickSearchBox(event)
        {
            if (event.stopPropagation)
                event.stopPropagation();

            else
                event.cancelBubble = true;
        }

        function DoGSearch()
        {
            var value = document.getElementById('txtGSearch').value.replace(/^\s+|\s+$/g, '');

            if (value.length > 0) window.location.href = '/searchresults.aspx?cx=013944795887846508942%3Ac1bowe8cwei&cof=FORID%3A11&q=' + encodeURIComponent(value) + '&sa=Search+TVSA&siteurl=tvsa.co.za%2F&ref=&ss=';
        }

        function ToggleProfileMenu()
        {
            var menu = document.getElementById('profileMenu');

            if (menu.style.display = menu.style.display == 'block')
                menu.style.display = 'none';

            else
            {
                document.getElementById('mainMenu').style.display = 'none';
                menu.style.display = 'block';
            }
        }

        function CloseProfileMenu()
        {
            document.getElementById('profileMenu').style.display = 'none';
        }

        function ToggleMainMenu()
        {
            var menu = document.getElementById('mainMenu');

            if (menu.style.display = menu.style.display == 'block')
                menu.style.display = 'none';

            else
            {
                document.getElementById('profileMenu').style.display = 'none';
                menu.style.display = 'block';
            }
        }

        function CloseMainMenu()
        {
            document.getElementById('mainMenu').style.display = 'none';
        }
        //]]>
