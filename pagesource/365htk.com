        <script>
            function timeCN() {
                //获得显示时间的div
                t_div = document.getElementById('DateCN');
                var now = new Date()
                //替换div内容 
                t_div.innerHTML = now.getFullYear()
                 + "-" + (now.getMonth() + 1) + "-" + now.getDate()
                + " " + shapetime(now.getHours(), now.getMinutes());
                //等待一秒钟后调用time方法，由于settimeout在time方法内，所以可以无限调用
                setTimeout(timeCN, 1000);
            }
            function timeEN() {
                //获得显示时间的div
                t_div = document.getElementById('DateEN');
                var now = new Date(new Date() - 12 * 60 * 60 * 1000)
                //替换div内容 
                t_div.innerHTML = now.getFullYear()
                 + "-" + (now.getMonth() + 1) + "-" + now.getDate()
                 + " " + shapetime(now.getHours(), now.getMinutes());

                //等待一秒钟后调用time方法，由于settimeout在time方法内，所以可以无限调用
                setTimeout(timeEN, 1000);
            }
            function shapetime(vhrs, vmin) {
                if (vhrs <= 9) {
                    vhrs = "0" + vhrs;
                }
                if (vmin <= 9) {
                    vmin = "0" + vmin;
                }
                return vhrs + ":" + vmin;
            }
            timeCN();
            timeEN();
        </script>
