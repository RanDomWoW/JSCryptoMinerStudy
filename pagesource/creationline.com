		if(slno==1){
			if(wi<=1000){
				slideAnimeSet(slno,44,10,57,71);
			}else{
				slideAnimeSet(slno,44,490,367,490);
			}
		}
		if(slno==2){
			if(wi<=1000){
				slideAnimeSet(slno,22,4,40,56);
			}else{
				slideAnimeSet(slno,22,430,278,396);
			}
		}
		if(slno==3){
			if(wi<=1000){
				slideAnimeSet(slno,22,0,17,31);
