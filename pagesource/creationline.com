	recNews();
	$(window).scroll(function(){
		wi = $(window).width();
		if($(this).scrollTop()>500){
			$('#recNewsBox').fadeOut();
			tg=1;
		}else if(tg&&!tc){
			$('#recNewsBox').fadeIn();
			recNews();
			tg=0;
		}
	});
	$('#recNewsClose').click(function(){
		 wi = $(window).width();
		 if(wi<=1000){
			$('#recNews').fadeOut();
			$('#recNewsBox').removeClass('smart');
			$('#top_images').removeClass('set');
		 }else{
			$('#recNewsBox').fadeOut();
		 	tc=1;
		 }
	});
	var blogUrl = '/category/news/feed/';
	$.ajax({
		url : blogUrl,
		type : 'GET',
		error: function(){
		console.log('NEWS READ ERROR');
		},success: function(xml){
				var blogupdate = $(xml);
				blogupdate.find('item').each(function(i) {
				if(i<tp){
				var entry_item = $(this);
				var entry_item_title = entry_item.find('title').text();
				var pubDate = entry_item.find('pubDate').text();
				var timeStamp = parseInt( new Date(pubDate) /1000);
				var nowTimeStamp = parseInt( new Date()/1000);
				pubDate = dateFormat(new Date(pubDate));
				var entry_link = entry_item.find('link').text();
				var li = $('<li />');
				var newitem = '';
				var New = '';
				if(nowTimeStamp < parseInt(timeStamp)+(60*60*24*14)){
					New = '<span class="new">NEW</span>';
				}
				li.append('<dl><dt>' + pubDate + '</dt><dd>'+New+newitem+'<a href="'+entry_link+'">' +entry_item_title + '</a></dd></dl>');
				
				$('#recNews ul').append(li);
				if(wi>1000){
					$('#recNewsClose').delay(1200).animate({'opacity':1});
				}
				i++;
			}
		});$('#loading').remove();
	}
});
});
function recNews(){"use strict"
	var rcls = $('#recNews li');
	var h_rcls = $('#recNews').height();
	rcls.css({'opacity':0});
	if(wi>1000){
		$('#recTr').delay(1000).animate({'top':'-20px'});
	}else{
		$('#recTr').delay(1000).animate({'top':'auto'});
	}
	for(var j=0;j<rcls.length;j++){
		$(rcls[j]).css({'top':((j+1)*h_rcls*-1)});
		$(rcls[j]).delay(1200+(j*200)).animate({'opacity':1,'top':0},{'duration':1200,'easing':'easeInOutElastic'});
	}
	tg =0;
}
var dateFormat = function(my_date){
	var year = my_date.getFullYear();
	var month = my_date.getMonth() + 1;
	var date = my_date.getDate();
	format_date = year + "." + month + "." + date;
	return format_date;
};
