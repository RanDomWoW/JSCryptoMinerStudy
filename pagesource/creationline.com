	function re_size(){"use strict";
		var douga_hi = 0.5625;
		wi = $(window).width();
		var wh = 0;
		if(((wi-1000)/2*-1)<0){
			wh =((wi-1000)/2*-1);
		}
		$('#slide3 .box .w,#slide2 .box .w,#slide1 .box .w').css({'width':wi,'left':wh});
		var hi = (wi*douga_hi);
		vwi = wi;
		if(hi<650){
			hi = 650;
			vwi = (hi/douga_hi);
		}
		if(wi<=600 && !$('#top_slide ul.slidebody').hasClass('ss')){
			slidebodys = $('#top_slide ul.slidebody li.theslide');
			setTimeout(function(){
				$('#top_slide ul.slidebody li.theslide .box').remove();
			},1000);
			$('#top_slide ul.slidebody').addClass('ss');
			$('#top_slide,#backVideo').remove();
		}else if($('#top_slide ul.slidebody').hasClass('ss') && wi>600){
			$('#top_slide ul.slidebody li.theslide').remove();
			for(var si=0;si<slidebodys.length;si++){
				$('#top_slide ul.slidebody').append(slidebodys[si].innerHTML);
			}
			$('#top_slide ul.slidebody').removeClass('ss');
		}
		$('#backVideo video').css({'height':hi,'width':vwi});
		$('#smart_top_back').css({'backgroundSize':vwi+'px '+hi+'px','height':hi});
	}
	
