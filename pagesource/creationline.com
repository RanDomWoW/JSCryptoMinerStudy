				slideAnimeSet(slno,352,150,96,224);
			}
		} 
	}
	
	function slideAnimeSet(slno,title,ul,servicename,servicesubname){
		wi = $(window).width();
		var par = "";
		if(wi<=1000){
			par = "%";
		}
		$('#slide'+slno+' .title').css({'top':'-650px'});
		$('#slide'+slno+' .title').animate({'top':title},{'duration':900,'easing':'easeInOutCubic'}).fadeIn();
		
		$('#slide'+slno+' ul').css({'left':wi});
		$('#slide'+slno+' ul').animate({'left':ul+par},{'duration':1100,'easing':'easeInOutCubic'}).fadeIn();
		
		$('#slide'+slno+' .servicename').css({'top':'-650px'});
		$('#slide'+slno+' .servicename').animate({'top':servicename+par},{'duration':500,'easing':'easeInOutCubic'}).fadeIn();
		
		$('#slide'+slno+' .servicesubname').css({'top':'-650px'});
		$('#slide'+slno+' .servicesubname').animate({'top':servicesubname+par},{'duration':1200,'easing':'easeInOutCubic'}).fadeIn();
	}
	
