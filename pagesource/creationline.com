	var cat_id = $($this).attr('data-'+cat_name);
	if(cat_id==0){
		$('#top_service .right ul li').fadeIn();
	}else{
		var sv = $('#top_service .right ul li');
		var is=0;
		for(is=0;is<sv.length;is++){
			var cat_ids = sv[is].dataset[cat_name];
			if(cat_ids){
				var cat_id_sp = cat_ids.split(',');
				var view = $.inArray(cat_id,cat_id_sp);
				if(view >= 0){
					$(sv[is]).fadeIn();
				}else{
					$(sv[is]).css('display','none');
				}
			}else{
				$(sv[is]).css('display','none');
			}
		}
	}
	$('#cat_select112 li.active,#tag_select112 li.active').removeClass('active');
	$($this).addClass('active');
	page_move('#top_service_mv');
}
function service_active(id){
	if(!id){
		id=1;
	}
	$('#top_service #top_service_box .sv').css('display','none').removeClass('active');
	$('#top_service #top_service_box .sv[data-id="'+id+'"]').fadeIn('800').addClass('active');
	page_move('#top_service_detail');
}
function page_move(tag){
	var win = $(window).width();
	if(win<=700){
		var position=$(tag).offset().top;
		var bx;if($('body')[0].scrollTop>0){bx='body';}else{bx='html';}
		$(bx).animate({scrollTop:position},{duration:800,easing:'swing'});
	}
}
</script>
