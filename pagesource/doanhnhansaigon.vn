<script>
    var domain_cross = 'doanhnhansaigon.vn';
    document.domain = domain_cross;

    $('.layer_video').click(function () {
        var currentvideoid = $(this).find('div[data-component-type="video"]').data('component-value');
        var idiframe = $(this).find('iframe').attr('id');
        var iframeWin = document.getElementById(idiframe).contentWindow;
        iframeWin.postMessage(['play', currentvideoid], '*');
    });

    $( ".wrap_video" ).mouseenter(function() {
        var objhtml = $(this).closest('.owl-item');
        if(objhtml.find(".thumb_video").hasClass( "is_played" )){
            objhtml.find(".title_video" ).css('display','block');
            objhtml.find('.thumb_video').addClass('shadow_title');//layer_video
        };
    }).mouseleave(function() {
        var objhtml = $(this).closest('.owl-item');
        if(objhtml.find(".thumb_video").hasClass( "is_played" )){
            objhtml.find( ".title_video" ).css('display','none');
            objhtml.find('.thumb_video').removeClass('shadow_title');//layer_video
        }
    });

    var messageHandle = function (e) {
        if (e.origin.indexOf(domain_cross) > -1) {
            var video_id = e.data[1];
            switch (e.data[0]){
                case "paused":
                    $('.wrap-' + video_id + ' .icon-play-video').css('display', 'block');
                    $('.wrap-' + video_id + ' .title_video').css('display', 'block');
                    $('.wrap-' + video_id + ' .thumb_video').addClass('shadow_title layer_video');
                    $('.wrap-' + video_id + ' .thumb_video').removeClass('is_played');
                    break;

                case "played":
                    $('.wrap-' + video_id + ' .icon-play-video').css('display', 'none');
                    //$('.wrap-' + video_id + ' .title_video').css('display', 'none');
                                    $('.wrap-' + video_id + ' .thumb_video').addClass('is_played');
                    $('.wrap-' + video_id + ' .thumb_video').removeClass('layer_video');
                    break;
            }
        }
    };

    window.addEventListener ? window.addEventListener('message', messageHandle, false) : window.attachEvent('message', messageHandle, false);

    $('.inner_video').on('click', '#caretRight, .next_thumb', function(){
        stopVideo();
    });
    $('.inner_video').on('click', '#caretLeft, .prev_thumb', function(){
        stopVideo();
    });
</script>

