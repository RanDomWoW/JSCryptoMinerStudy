            stopVideo();
            list_video.trigger('prev.owl.carousel');
            removeThumbNext();
        });
        removeThumbNext();
        function removeThumbNext() {
            $('.view_more').css('display', 'none');
            $('.next').css('display', 'block');
            $('.prev').css('display', 'block');

            $('.btnnext').css('display', 'block');
            $('.btnprev').css('display', 'block');

            if (true == $('.first_active').closest('div.owl-item').hasClass('active')) {
                $('.prev').css('display', 'none');
                $('.btnprev').css('display', 'none');
            }
            if (true == $('.last_active').closest('div.owl-item').hasClass('active')) {
                $('.next').css('display', 'none');
                $('.btnnext').css('display', 'none');
                $('.view_more').css('display', 'block');
            }
        }
    });

