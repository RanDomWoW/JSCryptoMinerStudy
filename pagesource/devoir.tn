            $('#errormsg_0_sub').html('').hide();
            $('#errormsg_0_Message').html('').hide();
            $('#errormsg_0_Captch').html('').hide();


            textUserUsername = UserUsername.val().length;
            if (textUserUsername < 4) {
                UserUsername.addClass('c-red');
                $('#errormsg_0_user').append('<span class="zmdi zmdi-alert-circle"></span> Saisissez votre Nom & Prénom').show();

                repa = false;
            } else {
                UserUsername.removeClass('c-red');
                repa = true;
            }


            if (UserMail.val() == '' || !validateEmail(UserMail.val())) {
                UserMail.addClass('c-red');
                $('#errormsg_0_email').append('<span class="zmdi zmdi-alert-circle"></span> Veuillez entrer une adresse électronique valable.').show();

                repb = false;
            } else {
                UserMail.removeClass('c-red');
                repb = true;
            }

            textUserSub = UserSub.val().length;
            if (textUserSub < 4) {
                UserSub.addClass('c-red');
                $('#errormsg_0_sub').append('<span class="zmdi zmdi-alert-circle"></span> Saisissez votre Objet').show();

                repc = false;
            } else {
                UserSub.removeClass('c-red');
                repc = true;
            }

            textUserMessage = UserMessage.val().length;
            if (textUserMessage < 4) {
                UserMessage.addClass('c-red');
                $('#errormsg_0_Message').append('<span class="zmdi zmdi-alert-circle"></span> Saisissez votre Message').show();

                repd = false;
            } else {
                UserMessage.removeClass('c-red');
                repd = true;
            }


            textUserCaptch = UserCaptch.val().length;
            if (textUserCaptch < 4) {
                UserCaptch.addClass('c-red');
                $('#errormsg_0_Captch').append('<span class="zmdi zmdi-alert-circle"></span> Saisissez le code de vérification').show();

                repe = false;
            } else {
                UserCaptch.removeClass('c-red');
                repe = true;
            }


            if (repa && repb && repd && repc && repe) {
                console.log("sending...");
                $.ajax({
                    type: "POST",
