        function getLastStats(){

            $.ajax({
                crossDomain: true,
                type: "GET",
                url: "https://www.devoir.tn/xml/last.txt", // change to full path of file on server
                dataType: "html",
                success: function(data) {

                    var lignes =   data.match(/[^\r\n]+/g);
                    var elem = "";

                    for (var value in lignes){
                        elem = "";
                        var nl = lignes[value];
                        l = decodeURI(nl).replace("https://www.devoir.tn/","");
                        var params = l.split("/");
                        console.log(params);
                        index = params.length-1;
                        if ( (params[index]) == ""){
                            index--;
                        }
                        var color = "bgm-grey";
                        color = $('#s-main-menu [name="get_'+params[0]+'"]').attr('color');
                        if (color != undefined || color != "" )
                            color = "bgm-"+color;

                        elem = '<a target="_blank" href="'+nl+'" class="list-group-item media ">'+
                            '<div class="pull-left">'+
                            '<div class="avatar-char '+color+'">'+params[0][0]+'</div>'+
                            '</div>'+
                            '<div class="media-body">'+
                            '<div class="lgi-heading">'+decodeURI(params[index].replace('.html',''))+'</div>'+
                            '<small class="lgi-text">'+decodeURI(nl)+'</small>'+
                            '</div>'+
                            '</a>';
                        if ( params[0][0] != 'f')
                            $('#mCSB_2_container').append(elem);
                    }

                },
                failure: function(data) {
                    console.log("TXT File could not be found");
                }
            });


        }

