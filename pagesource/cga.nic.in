<script type="text/javascript">
    //<![CDATA[
    function Search() {

        var emailPat = /^[^*|\".:>[\]{}`\\()';^!%#@\$]+$/;

        var emailid = document.getElementById('ctl00_Top_txtSearch').value;
        if (emailid == " Enter Your Text") {
            emailid = "";
        }
        else {
            emailid = document.getElementById('ctl00_Top_txtSearch').value;
        }


        var matchArray = emailid.match(emailPat);

        if (emailid == "") {
            alert("Enter text to search");
            document.getElementById("ctl00_Top_txtSearch").focus();
            return false;
        }
        else if (matchArray == null) {
            alert("Please enter valid text, only enter alpha numbers.");
            document.getElementById('ctl00_Top_txtSearch').focus();
            return false;
        }
        else if (($('<div/>').text(emailid).html().indexOf("&lt;") >= 0) || ($('<div/>').text(emailid).html().indexOf("&amp;") >= 0)) {
            alert("Please enter valid text, only enter alpha numbers.");
            document.getElementById('ctl00_Top_txtSearch').focus();
            return false;
        }
    return true;
}



