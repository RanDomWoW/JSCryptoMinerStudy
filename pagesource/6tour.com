    <script type="text/javascript" src="//static3.6tour.com/Scripts/jquery.carouFredSel-5.6.4.min.js"></script>
    <script type="text/javascript">
        $(function () {
            $('#slider').carouFredSel({
                width: '100%',
                items: 3,
                auto: {
                    delay: 5000
                },
                scroll: {
                    items: 1,
                    duration: 2000,
                    pauseDuration: 5000
                },
                prev: '#prev',
                next: '#next'
            });

            setTimeout(function () {
                $("#slider img").each(function (idx, item) {
                    setTimeout(function () {
                        var elem = $(item);
                        //elem.hide();
                        var src = elem.data('src');
                        elem.attr('src', src);
                        elem.fadeIn('slow');
                    }, 500 * idx);
                });
            }, 500);

            $("#timezoneoffset").val(new Date().getTimezoneOffset());
            $('#Username').focus();
        });

        function addUsernameToQs() {
            var username = $('#Username').val();
            var form = $('#loginForm');
            form.attr('action', form.attr('action') + '?u=' + username);
        }
    </script>

