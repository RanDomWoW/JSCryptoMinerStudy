<script language='javascript'>
var eg_scroll_time = 3500;
var eg_ck = 0;
function banner_roll(div_id, banner_height, banner_delay, banner_speed, this_height)
{
    var div_tag = document.getElementById(div_id);
    var a_tag, i;   
    this_height++;
	if(eg_ck > 0){
		if(this_height < banner_height){
			div_tag.style.top = - this_height;
			eg_stop = setTimeout("banner_roll('" + div_id + "', " + banner_height + ", " + banner_delay + ", " + banner_speed + ", " + this_height + ");", banner_speed);
		} else {
			a_tag = div_tag.getElementsByTagName("A");
			div_tag.appendChild(a_tag[0]);
			div_tag.style.top = 0;
			eg_stop = setTimeout("banner_roll('" + div_id + "', " + banner_height + ", " + banner_delay + ", " + banner_speed + ", 0);", banner_delay);
		}
	}else{
			a_tag = div_tag.getElementsByTagName("A");
			div_tag.appendChild(a_tag[9]);
			div_tag.style.top = 0;
			eg_stop = setTimeout("banner_roll('" + div_id + "', " + banner_height + ", " + banner_delay + ", " + banner_speed + ", 0);", banner_delay);
			eg_ck++;
	}
	return true;
}
function stop_banner_roll(v){
	if(v == 1){
			clearTimeout(eg_stop);
	}else banner_roll("banner_1", 18, eg_scroll_time, 20, 0);
}
