				"overflow":"hidden",    //默认隐藏
				"wordWrap":"break-word",
				"wordBreak":"break-all",
				"width": "582px",
				"height":"48px"
			});

			var timeRoll = document.getElementById("rollContent_roll"),
				_h = 49;
                                console.log(_h);
				timeoutRoll = function(){
					var _h = 49,
						_t = parseInt(timeRoll.style.top,10),
						//是否将top值设置为0
						_tt = _h>Math.abs(_t)||_t>=0?_t-10:(_h||10);
						setCss(timeRoll,{
							"top":_tt+"px"
						});
						setTimeout(timeoutRoll,200);//定时调用，模拟动画
				};

			if(_h>48){//判断滚动高度是否大于 data-rhight
				timeoutRoll();
				setCss(timeRoll,{
					"position":"relative",
