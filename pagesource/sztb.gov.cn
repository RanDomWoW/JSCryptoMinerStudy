<script type="text/javascript">
	var rootpath = 'http://www.e511.com';
	var rootpath_js = 'http://www.e511.com';
	
	/*
		公交线路
	*/
	 function dtLine(){
		var fromsubwayStart = document.getElementById("fromsubwayStart").value;
		if(fromsubwayStart == "" || typeof fromsubwayStart == "undefined" || fromsubwayStart == null){
			alert("请输入完整地铁线路!");
			return false;
		}
		var tosubwayEnd = document.getElementById("tosubwayEnd").value;
                if(tosubwayEnd == "" || typeof tosubwayEnd == "undefined" || tosubwayEnd == null){
			alert("请输入完整地铁线路!");
			return false;
		}
                window.open("http://www.e511.com/shineiTraffic.html?type=metro&start="+encodeURI(encodeURI(fromsubwayStart))+"&end="+encodeURI(encodeURI(tosubwayEnd)));
	}
	
	
	/*
		公交线路
	*/
	function busLine(){
		var fromroadStart = document.getElementById("fromroadStart").value;
		if(fromroadStart == "" || typeof fromroadStart == "undefined" || fromroadStart == null){
			alert("请输入公交线路!");
			return false;
		}
		var toroadEnd = document.getElementById("toroadEnd").value;
		if(toroadEnd == "" || typeof toroadEnd == "undefined" || toroadEnd == null){
			alert("请输入公交线路!");
			return false;
		}
window.open("http://www.e511.com/shineiTraffic.html?type=bus&start="+encodeURI(encodeURI(fromroadStart))+"&end="+encodeURI(encodeURI(toroadEnd)));
	}
	
	/*
		公交换乘
	*/
	function busStation(){
		var keywordTwo = document.getElementById("keywordTwo").value;
		if(keywordTwo == "" || typeof keywordTwo == "undefined" || keywordTwo == null){
			alert("请输入公交站点!");
			return false;
		}
		document.getElementById("bussearch").value = encodeURI(keywordTwo);
		document.getElementById("ty").value = "2";
		
		var myForm = document.getElementById("myForm");
		myForm.action = rootpath+"/jsp/bus/bus.jsp";
		myForm.submit();
	}
	
	/*

		出租出行
	*/
	function taxiTravel(){
		var fromroadStart = document.getElementById("fromroadStart2").value;
		var toroadEnd = document.getElementById("toroadEnd2").value;
		if(fromroadStart == "" || typeof fromroadStart == "undefined" || fromroadStart == null){
			alert("请输入出租出行起点!");
			return false;
		}
		if(toroadEnd == "" || typeof toroadEnd == "undefined" || toroadEnd == null){
			alert("请输入出租出行止点!");
			return false;
		}
		window.open("http://www.e511.com/shineiTraffic.html?type=taxi&start="+encodeURI(encodeURI(fromroadStart))+"&end="+encodeURI(encodeURI(toroadEnd )));
		
	}
	
	function taxiDistribution(){
		var keywordTo = document.getElementById("keywordTo").value;
		if(keywordTo == "" || typeof keywordTo == "undefined" || keywordTo == null){
			alert("请输入您要查看的出租分布位置!");
			return false;
		}
		document.getElementById("keyword").value = encodeURI(keywordTo);
		var myForm = document.getElementById("myForm2");
		myForm.action = rootpath+"/jsp/taxi/taxiGo.jsp";
		document.getElementById("t").value ="2"
		myForm.submit();
	}
	/*
		出港
	*/
	function toPort(){
		var endcity = document.getElementById("endcity").value;
		url = rootpath_js+"/jsp/intercitytravel/shiptravel.jsp?endcity="+endcity;
		url = encodeURI(encodeURI(url));
		window.open(url,"gangkouselect");
	}
	
	/*
		到港
	*/
	function fromPort(){
		var startcity = document.getElementById("startcity").value;
		url = rootpath_js+"/jsp/intercitytravel/shiptravel.jsp?startcity="+startcity;
		url = encodeURI(encodeURI(url));
		
		window.open(url,"gangkouselect");
	}

</script>
