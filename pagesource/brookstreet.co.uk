    <script type="text/javascript">
        var evtlocation = document.createEvent("HTMLEvents");

        evtlocation.initEvent("click", true, true);

        function setText(val, e) {
            document.getElementById(e).value = val;
        }
        function insertText(val, e) {
            document.getElementById(e).value += val;
        }
        var nav = null;
        function requestPosition() {
            if (nav == null) {
                nav = window.navigator;
            }
            if (nav != null) {
                var geoloc = nav.geolocation;
                if (geoloc != null) {
                    geoloc.getCurrentPosition(successCallback, errorCallback);

                }
                else {
                    console.log("Geolocation not supported");
                }
            }
            else {
                console.log("Navigator not found");
            }
        }
        function successCallback(position) {
            setText(position.coords.latitude, "hfLatitude");
            setText(position.coords.longitude, "hfLongitude");
            document.getElementById("ctl00_cphContainer3_lnkBranchSearch").click();
        }
        function GetCoordinates(vLatLng) {
            setText(vLatLng.lat(), "hfLatitude");
            setText(vLatLng.lng(), "hfLongitude");
            document.getElementById("ctl00_cphContainer3_lnkBranchSearch").click();
        }
        function errorCallback(error) {
            var message = "";
            // Check for known errors
            switch (error.code) {
                case error.PERMISSION_DENIED:
                    message = "This website does not have permission to use " +
                      "the Geolocation API";
                    break;
                case error.POSITION_UNAVAILABLE:
                    message = "The current position could not be determined.";
                    break;
                case error.PERMISSION_DENIED_TIMEOUT:
                    message = "The current position could not be determined " +
                      "within the specified timeout period.";
                    break;
            }
            // If it's an unknown error, build a message that includes 
            // information that helps identify the situation, so that 
            // the error handler can be updated.
            if (message == "") {
                var strErrorCode = error.code.toString();
                message = "The position could not be determined due to " +
                  "an unknown error (Code: " + strErrorCode + ").";
            }
            console.log(message);
        }

    </script>
    <script type="text/jscript">

        var iMaxValue = 60000;
        var iMinSalary= 10000;
        var iMaxSalary = 60000;
        var iSteps = 500;
   
        $(function () {
            $("#slider-salary").slider({
                range: true,
                min: 0,
                max: iMaxValue,
                values: [iMinSalary, iMaxSalary],
                step: iSteps,
                slide: function (event, ui) {
                 
                $("#lblSalaryMin").text("£" + addCommas(ui.values[0]));
                
                if (ui.values[1] == iMaxValue) {
                    $('#lblSalaryMax').text('£' + addCommas(ui.values[1]) + "+");
                } else {
                    $('#lblSalaryMax').text('£' + addCommas(ui.values[1]));
                }

                document.getElementById('hfSalaryMin').value = ui.values[0]; 
                document.getElementById('hfSalaryMax').value = ui.values[1]; 
                                       
            }
        });

        $("#lblSalaryMin").text("£" + addCommas($("#slider-salary").slider("values", 0)));

        if ($("#slider-salary").slider("values", 1) == iMaxValue) {
            $("#lblSalaryMax").text("£" + addCommas($("#slider-salary").slider("values", 1)) + "+");
        } else {
            $("#lblSalaryMax").text("£" + addCommas($("#slider-salary").slider("values", 1)));
        }

        
        document.getElementById('hfSalaryMin').value = $("#slider-salary").slider("values", 0); 
        document.getElementById('hfSalaryMax').value = $("#slider-salary").slider("values", 1); 
