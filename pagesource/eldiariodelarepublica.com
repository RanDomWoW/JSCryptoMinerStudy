	<script type="text/javascript">
	  //tiempo transcurrido fecha
	  function tiempo_transcurrido(tiempo){
	    var actual = new Date().getTime();
	    var diasDif = actual - tiempo;
	    var minutos = Math.round(diasDif/(1000 * 60));
	    var horas = Math.round(diasDif/(1000 * 60 * 60));
	    var dias = Math.round(diasDif/(1000 * 60 * 60 * 24));
	    var texto_dias = ' días';
	
	    if(dias=='1')
	      texto_dias = ' día';
	
	    if(minutos<=10)
	      return "Hace un momento";
	    if(horas<=2)
	      return "Hace 1 hora";
	    if(horas<23)
	      return "Hace " + horas + " horas";
	    else
	      return "Hace " + dias + texto_dias;
	  }
	  //tiempo transcurrido anuncios
	  $('.opinion_div .opinion_fecha').each(function(index,elem){
	    var fecha_c = $(this).data('fecha-c');
	    $(this).html(tiempo_transcurrido(fecha_c)+" ");
	  });
	</script>

