				var itemCount = jQuery('.sidebarSlideshowItem').length;
				jQuery('.arrowLeft').click(function () {
					var sidebarSlideshowElLeft = parseInt(jQuery('.sidebarSlideshow').css('left'));
					if ((sidebarSlideshowElLeft + 206) <= 0) {
						jQuery('.sidebarSlideshow').css({'left': (sidebarSlideshowElLeft + 206) + 'px'});
						jQuery('.arrowRight').show();
					}
					if (parseInt(jQuery('.sidebarSlideshow').css('left')) == 0) {
						jQuery('.arrowLeft').hide();
					}
					else {
						jQuery('.arrowLeft').show();
					}
				});

				jQuery('.arrowRight').click(function () {
					var sidebarSlideshowElLeft = parseInt(jQuery('.sidebarSlideshow').css('left'));
		
					if ((sidebarSlideshowElLeft - 206) >= (itemCount - 1) * (-206)) {
						jQuery('.sidebarSlideshow').css({'left': (sidebarSlideshowElLeft - 206) + 'px'});
						jQuery('.arrowLeft').show();
					}
					if (parseInt(jQuery('.sidebarSlideshow').css('left')) == (itemCount - 1) * (-206)) {
