			$(function(){
				var search_text = $("#search_box input");
				var text_label = $("#text_label");
				
				if (search_text) {
					search_text.removeAttr('readonly');
				}
				if (search_text && text_label) {
					search_text.on("focus", removePlaceholder);
					search_text.on("blur", setPlaceholder);
					text_label.on("click", setTextFocus);
					search_text.on("keydown", removePlaceholder);
					setPlaceholder();
				}
			});
			
			function removePlaceholder(){
				var text_label = $("#text_label");
				text_label.text(" ");
                text_label.addClass('width0');
            }
            function setPlaceholder(){
				var search_text = $("#search_box input");
				var text_label = $("#text_label");
				if(search_text.val() != ""){
					text_label.text(" ");
                    text_label.addClass('width0');
                }else{
					text_label.text("調べたいキーワードを入力してください");
                    text_label.removeClass('width0');
                }
            }
			
            function setTextFocus(){
				var search_text = $("#search_box input");
                search_text.focus();
            }
        </script>
