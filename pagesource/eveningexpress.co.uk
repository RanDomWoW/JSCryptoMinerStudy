	/***
	Body
	***/

	body,
	.site-content {
		background-color: #ffffff;
	}

	.block > .wrap,
	.site-content > .wrap,
	.thumbnail .title--caps {
		background-color: #ffffff;
	}

	.package--top__synopsis .signup { border-color: #ffffff; }

	/*****
	Header
	*****/

	.site-header { background-color: #333333; }

	/* Header > Datestamp / Verticals */
	.verticals__wrap { border-color: #555555; }

	.datestamp,
	.verticals a,
	.verticals__btn {
		color: #ffffff;
	}

	/* Header > Search bar */
	.search-form--header {
		border-color: #555555;
		background-color: transparent;
	}

	.search-form--header .search-form__text,
	.search-form--header .search-form__submit {
		color: #ffffff;
	}

	/*****
	Footer
	------
	* Putting this CSS here for first-load visibility.
	*****/

	.site-footer {
		background-color: #555 !important;
		color: #fff;
	}

	/*********
	Navigation
	*********/

	/* Navigation > Tabs */
	.nav-bar > .menu-item > a { color: #ffffff; }

	/* Navigation > Show Tabs */
	.show-tabs .nav-bar .sub-menu {
		background-color: #333333;
	}

	.show-tabs .nav-bar > .menu-item > a { background-color: #333333; }
	.show-tabs .all-sections-btn { background-color: #333333; }

	/* Navigation > Hide Tabs */
	.hide-tabs .nav-bar .sub-menu { background-color: #333333; }

	.hide-tabs .current_page_item,
	.hide-tabs .menu-item-type-taxonomy.current-menu-item,
	.hide-tabs .menu-item-type-taxonomy.current-menu-parent,
	.hide-tabs .menu-item-type-taxonomy.current-post-ancestor,
	.hide-tabs .menu-item-type-taxonomy.current-fp-ancestor {
		border-color: #e21533;
	}

	.hide-tabs .nav-bar .sub-menu { border-color: #555555; }

	/* Navigation > Buttons */
	.header-btn,
	.all-sections-btn,
	.nav-bar .sub-menu a {
		color: #ffffff;
	}

	/* Navigation > Pager */
	.pagination .current { background-color: #e21533; }

	/*****
	Blocks
	*****/

	/* Blocks > Carousel Block */
	.block--carousel .title--block__link { color: #e21533; }

	
	/* Blocks > Gallery Block */
	.gallery__page.js-pager-active {
		background-color: #e21533;
		color: #ffffff;
	}

	.gallery__page.js-pager-active:before { color: #e21533; }
