    (function($) {
      $.each(academicFieldsJson, function(i, item){
        $('#step1 .popup-menu').append('<li class="item" data-step-id="1" data-tid="'+item.tid+'">'+item.name+'</li>');
      });
      var $tooltip1 = $('#tooltip1');
      var $step1_answer = $('#step1-answer');
      var $step2_answer = $('#step2-answer');
      var $step3_answer = $('#step3-answer');

      var tooltip1_top = parseInt($tooltip1.css('top'));

      $tooltip1.css('top', tooltip1_top+10);
      $tooltip1.animate({
        top: tooltip1_top,
        opacity: 1
      }, 600, function(){
        setTimeout(function(){
          $tooltip1.fadeOut('fast').fadeIn('fast');
        }, 150);
      });

      $step1_answer.hover(function(){
        $tooltip1.animate({
          top: tooltip1_top+10,
          opacity: 0
        }, 300, function(){
          $tooltip1.remove();
        });
      });

      $('.answer-button').click(function(e){
        var step_id = $(this).attr('id').substr(4, 1);
        $('#step'+step_id+' .popup-menu').css('visibility', 'visible');
      });

      $('.statement .popup-menu').mouseleave(function(e){
        $(this).css('visibility', 'hidden');
      });

      $('.statement .popup-menu').on('click', 'li', function(){
        var text = $(this).html();
        var step_id = $(this).attr('data-step-id');
        var next_step_id = parseInt(step_id)+1;
        $('#step'+step_id+' .popup-menu').css('visibility', 'hidden');
        if(!($(this).hasClass('selected'))){
          $('#step'+step_id+' .answer-button').html(text);
          $('#step'+step_id+' .popup-menu li').removeClass('selected');
          $(this).addClass('selected');

          $('#progress .step-'+next_step_id).addClass('active');

          if(step_id < 3){
            if($('#step'+next_step_id).hasClass('not-visible')) {
              $('#step'+next_step_id).animate({
                opacity: 1
              }, 250, function(){
                $('#step'+next_step_id).removeClass('not-visible');
              });

              $('#field-selector-wrapper').animate({
                paddingTop: parseInt($('#field-selector-wrapper').css('padding-top'))-20,
                paddingBottom: parseInt($('#field-selector-wrapper').css('padding-bottom'))+20
              }, 300);

            }
          }
          if(step_id == 1){
            $('#step2 .popup-menu').empty();
            $('#step3 .popup-menu').empty();
            $step2_answer.html('انتخاب کنید');
            $step3_answer.html('انتخاب کنید');
            $.each(academicFieldsJson[$(this).attr('data-tid')].childs, function(i, item){
              $('#step2 .popup-menu').append('<li class="item" data-step-id="2" data-tid="'+item.tid+'">'+item.name+'</li>');
            });
          }
          else if(step_id == 2) {
            $('#step3 .popup-menu').empty();
            $step3_answer.html('انتخاب کنید');
            $.each(academicFieldsJson[$('#step1 .popup-menu li.selected').attr('data-tid')].childs[$(this).attr('data-tid')].childs, function(i, item){
              $('#step3 .popup-menu').append('<li data-target="'+item.target_url+'" class="item" data-step-id="3" data-tid="'+item.tid+'">'+item.name+'</li>');

            });
          }
          else if(step_id == 3) {
            $.cookie('Parseh_academicFieldTid', $(this).attr('data-tid'), {expires: 365, path: Drupal.settings.basePath});
            $.cookie('Parseh_academicFieldGroupTid', $('li[data-step-id="2"].selected').attr('data-tid'), {expires: 365, path: Drupal.settings.basePath});
            $.cookie('Parseh_academicFieldGradeTid', $('li[data-step-id="1"].selected').attr('data-tid'), {expires: 365, path: Drupal.settings.basePath});
            if($(this).attr('data-target') != '') {
              window.location.replace($(this).attr('data-target'));
            }
          }
        }
      });

    })(jQuery);
  </script>
