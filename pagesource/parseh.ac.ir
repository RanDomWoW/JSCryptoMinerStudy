            clearInterval(interval);
            interval = setInterval(function(){

              var currentSlideNum = parseInt($('.slides-pager li.active').attr('data-slide-num'));
              var nextSlideNum = 0
              if((currentSlideNum+1) < slidesCount) {
                nextSlideNum = (currentSlideNum+1);
              }

              slideTo(nextSlideNum);
            }, slideInterval);
          }

          $('.slides-pager li').click(function(){
            if(!$(this).hasClass('active') && !sliding) {
