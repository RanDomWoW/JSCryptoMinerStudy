<script>
    function _trackTourSearch(tourType) {
        var category = 'GrandTop_Click';
        var action;
        var label;
        switch (tourType) {
            case 'kokunaiStay':
                action = 'SR1_国内宿泊 条件検索';
                var areaObj = document.getElementById('cat1');
                var prefObj = document.getElementById('cat2');
                var thirdSelectObj = document.getElementById('cat3');
                var fourthSelectObj = document.getElementById('cat4');
                label = areaObj.options[areaObj.options.selectedIndex].text + '>' + 
                    prefObj.options[prefObj.options.selectedIndex].text + '>' + 
                    thirdSelectObj.options[thirdSelectObj.options.selectedIndex].text + '>' + 
                    fourthSelectObj.options[fourthSelectObj.options.selectedIndex].text;
                break;
            case 'kokunaiTour':
                action = 'SR2_国内ツアー 条件検索';
                var departObj = document.getElementsByName('dprtplccd')[0];
                var destObj = document.getElementsByName('catid')[0];
                label = destObj.options[destObj.options.selectedIndex].text + '_' + 
                    departObj.options[departObj.options.selectedIndex].text;
                break;
            case 'kaigaiTour':
                action = 'SR3_海外ツアー 条件検索';
                var departObj = document.getElementsByName('StartAt')[0];
                var destAreaObj = document.getElementById('firstPull');
                var destCountryObj = document.getElementById('selectCountry');
                var destCityObj = document.getElementById('selectCity');
                label = destAreaObj.options[destAreaObj.options.selectedIndex].text + '>' +
                    destCountryObj.options[destCountryObj.options.selectedIndex].text + '>' +
                    destCityObj.options[destCityObj.options.selectedIndex].text + '_' + 
                    departObj.options[departObj.options.selectedIndex].text;
                break;
        }
        dataLayer.push({'event': 'trackEvent', 'category': category, 'action': action, 'label': label});
    }
</script>


