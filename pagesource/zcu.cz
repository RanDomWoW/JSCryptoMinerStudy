<script type="text/javascript">


    function switch_tab(id) {
        if (CURRENT_TAB != id) {
            document.getElementById(id).style.display = 'block';
            document.getElementById(CURRENT_TAB).style.display = 'none';
            document.getElementById('a_' + id).className = 'active';
            document.getElementById('a_' + CURRENT_TAB).className = '';
            CURRENT_TAB = id;
        }
    }

    


    var firstval = 1;
    var opacity = 1;
    var si = null;
    var sc = null;
    var elms = document.getElementsByName('fsa');

    for (var i = 0; i < elms.length; ++i) {
        var e = elms[i];
        e.addEventListener('click', function (event) {
            event.preventDefault();
            event.stopPropagation();
            event.stopImmediatePropagation();
        });
    }

    function changeP(id) {
        clearTimeout(si);
        clearTimeout(sc);
        for (i = 1; i < 7; i++) {

            document.getElementById('foto_' + i).style.display = 'none';
            if (document.getElementById('fs_' + i) != null)
                document.getElementById('fs_' + i).style.display = 'none';
        }
        document.getElementById('foto_' + id).style.display = 'block';
        document.getElementById('foto_' + id).style.opacity = 1;
        if (document.getElementById('fs_' + id) != null)
            document.getElementById('fs_' + id).style.display = 'block';

    }

    function Carousel() {
        for (i = 1; i < 6; i++) {
            document.getElementById('foto_' + i).style.display = 'none';
            if (document.getElementById('fs_' + i) != null)
                document.getElementById('fs_' + i).style.display = 'none';
        }

        if (firstval == 6) {
            firstval = 1;
        }
        else {
            firstval++;
        }

        document.getElementById('foto_' + firstval).style.display = 'block';
        document.getElementById('foto_' + firstval).style.opacity = 0.1;
        si = swapI();

//                setTimeout(Carousel, 5000);

    }

    function swapC() {
        if (document.getElementById('foto_' + firstval).style.opacity > 0) {
            document.getElementById('foto_' + firstval).style.opacity = (document.getElementById('foto_' + firstval).style.opacity - 0.1).toFixed(2);
            sc = setTimeout(swapC, 35);
        }
        else {

            Carousel();
            return;
        }
    }

    function swapI() {
        if (document.getElementById('foto_' + firstval).style.opacity < 1) {
//                    console.log(console.log(document.getElementById('foto_' + firstval).style.opacity));
            document.getElementById('foto_' + firstval).style.opacity = (parseFloat(document.getElementById('foto_' + firstval).style.opacity) + 0.1).toFixed(2);
            if (document.getElementById('fs_' + firstval) != null)
                document.getElementById('fs_' + firstval).style.display = 'block';
            si = setTimeout(swapI, 35);
        }
        else {
//                    console.log('bbbb');

            sc = setTimeout(swapC, 5000);
        }
    }

    sc = setTimeout(swapC, 5000);

    
</script>



