<script>
    // Globale Variabel zur Prüfung ob AJAX durchgeführt wurde
    var done = true;

    // Document Ready, Funktionsaufrufe
    $(document).ready(function () {

        // Scroll Event um weitere Inhalte zu laden
        $(window).scroll(debounce(function () {
            // Wenn nach unten gescrollt wird.
            if ($(this).scrollTop() + $(this).innerHeight() >= $(document).height() - 3000) {
                if (done === true) {
                    ajaxTimelineLoad($('#btnLoadNextPosts'));
                }
            }
        }, 200));

        // Button Klick um weitere Inhalte zu laden
        $('#timeline').on('click', '#btnLoadNextPosts', function () {
            if (done === true) {
                ajaxTimelineLoad($(this));
            }
            return false;
        });
    });

    // Funktion zum laden des Contents
    function ajaxTimelineLoad(btn) {
        done = false;
        var url = btn.prop('href');
        btn.html('<i class="fa fa-spinner fa-pulse fa-fw fa-2x"></i>');

        // AJAX Call
        $.ajax({
            url: url,
            type: 'GET',
            dataType: 'html',
            success: function (data) {
                $('.timeline-loading-button').remove();
                $(data).insertBefore($('#end-of-timeline'));
                done = true;
            },
            error: function (data) {
                btn.html('<i class="fa fa-warning fa-2x"></i>');
                done = true;
            }
        });
    }

