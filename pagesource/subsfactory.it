var emailfromprofileall = function(element) {
    element.attr('disabled', true);
    $.ajax({
        url : '/services/email.php',
        type : 'post',
        data : {
            method : 'insertall'
        },
        success : function(data) {
            if (data == 'true') {
                $('.profile-content .favorites .banner .follow').each(function() {
                    if ($(this).find('.unfollowbutton').is(":visible")) {
                        $(this).find('.emailbutton').hide();
