                    setTimeout(circleFlip, 3000);
                }, 3000);
            }
        }

        /*
         Move Cloud
         */
        if ($('.cloud').get(0)) {
            var moveCloud = function() {
                $('.cloud').animate({
                    'top': '+=20px'
                }, 3000, 'linear', function() {
                    $('.cloud').animate({
                        'top': '-=20px'
                    }, 3000, 'linear', function() {
                        moveCloud();
                    });
                });
            };

            moveCloud();
        }

    }).apply(this, [jQuery]);
