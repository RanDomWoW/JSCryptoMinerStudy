        if (parseInt(data) > 99)
        {
            setTimeout(function(){
                $(".mail_span").append("<a href='member.php?act=mail_transfer' id='my_letter'><img class='mail_img' src='/themes/default/img/header/letters.png' width='22' height='16' border='0'/><div class='my_letter_num' id='my_letter_num'><span style='color: white !important;'>99</span></div></a>");
            }, 2000);
        }
        else
        {
            if (parseInt(data) > 0)
            {
                setTimeout(function(){
                    $(".mail_span").append("<a href='member.php?act=mail_transfer' id='my_letter'><img class='mail_img' src='/themes/default/img/header/letters.png' width='22' height='16' border='0'/><div class='my_letter_num' id='my_letter_num'><span style='color: white !important;'>" + data + "</span></div></a>");
                }, 2000);
