    function getrate_callback(result)
    {
        if(result)
        {
            document.getElementById('currency_rate').innerHTML = result;
        }
        else
        {
            document.getElementById('currency_rate').innerHTML = html_no;
        }
    }
    function getname_callback(result)
    {
        result = result.replace(/^(\s+)|(\s+)$/,'');
        var infoText = "";
        if(result)
        {
            infoText = "<a>"+welcom + "，"+result+"！</a><a class='line'>|</a>"
            + "<a href='index.php?act=member_account'>"
            + member_center
            + "</a><a class='line'>|</a>"
            + "<a href='javascript:void(0);' onClick=\"Quit_login();alert(login_to_index);window.location='index.php'\">"
            + login_out
            + "</a>";
