            infoText = "<a href='/member_login'>"
            + login
            + "</a><a class='line'>|</a>"
            + "<a href='/member_register'>"
            + register
            + "</a><a class='line'>|</a>"
            + "<a href='/download'>"
            + download_app
            + "</a>";
        }
        document.getElementById('user_name').innerHTML = infoText;
