<script>

	$(".news").closest(".box").addClass("news-box");
	
	function incrementPhoto(container_id){
		var idx = Cookies.get(container_id);
		if (!parseInt(idx)){
			idx = $("#"+container_id+" .carousel-inner .active").index();
		}
		
		var len = $("#"+container_id+" .carousel-inner .item").length;	
		idx++;
		idx = idx % len;		
			
		$("#"+container_id+" .carousel-inner .item").removeClass("active");
		$("#"+container_id+" .carousel-inner .item:nth-child("+ (1+idx) +")").addClass("active");
		
		Cookies.set(container_id,idx);
	}
	
	incrementPhoto("discovery-innovation");
	incrementPhoto("liu-life");
	incrementPhoto("notable-alumni");
	
</script>
    
