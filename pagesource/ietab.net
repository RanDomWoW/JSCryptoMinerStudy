<script>

    $(document).foundation();

    var HomePage = {
        NUM_USERS: 4562666,

        log10: function(val) {
            return Math.log(val) / Math.LN10;
        },

        animateNumberWorker: function(el, nPos) {
            var finalValue = parseInt(this.NUM_USERS.toString().substr(nPos, 1));

            var theInterval = window.setInterval(function() {
                var nCurrent = parseInt(Math.random() * 10);
                el.innerText = nCurrent;
            }, 40);
            window.setTimeout(function() {
                window.clearInterval(theInterval);
                el.innerText = finalValue;
            }, 500);
        },

        animateNumber: function(el, nPos, numDigits) {
            window.setTimeout(function() {
                this.animateNumberWorker(el,nPos);
            }.bind(this), (numDigits - 1 - nPos) * 200);
        },

        animateUsers: function() {
            var numDigits = parseInt(this.log10(this.NUM_USERS)) + 1;
            var allNumbers = $('.counter-number');
            for (var i=0; i<allNumbers.length; i++) {
                this.animateNumber(allNumbers[i], i, numDigits);
            }
        },

        init: function() {
            $('#homepage-counter').waypoint(function() {
                this.animateUsers();
            }.bind(this),
                {
                    offset: '95%',
                    triggerOnce: true
                });
        }

    }
    HomePage.init();
    // HomePage.animateUsers();




</script>

