<!--This script initializes any Magnific photo galleries. 
The script allows multiple data-groups of photos, or Solo photos.
Use data-group="1" and so on.-->
  
  <script src='/jquery.js'></script>

  <script src='/magnific-popup.js'></script>

  <script>
    var groups = {};
$('.galleryGroup').each(function () {
    var id = parseInt($(this).attr('data-group'), 10);
    if (!groups[id]) {
        groups[id] = [];
    }
    groups[id].push(this);
    }
);
$.each(groups, function () { 
    $(this).magnificPopup({
        type: 'image',
        closeOnContentClick: true,
        closeBtnInside: false,
        gallery: { enabled: true }
    });
});

$('.gallerySolo').each(function () {
    $(this).magnificPopup({
        type: 'image',
        closeOnContentClick: true,
        closeBtnInside: false,
        gallery: { enabled: true }
    });
});
  </script>
  
<!--Make submit buttons say "Loading" and add a progress wheel to the cursor when clicked.-->
<script type='text/javascript'>
document.addEventListener('DOMContentLoaded', function() {
    var bns = document.getElementsByTagName("input");
  	for (i = 0; i < bns.length; i++) {
    	if(bns[i].getAttribute('type')=='submit'){
    		bns[i].addEventListener("click", function() {
    			this.value = "Loading...";
    			document.body.style.cursor = "progress";    		
    		});
    	}
   	}
 }, false);
 </script>

<!--Toggle between sub-tabs-->
<script type='text/javascript'>

function toggle(selectName, selectNumber) {
	
	var i = 0;	
	
	while(document.getElementById(selectName + i) != null){
		document.getElementById(selectName + i).style.display = 'none';
		document.getElementById("select-" + selectName + i).className = 'deselected';
		i++;
	};
	
	document.getElementById(selectName + selectNumber).style.display = 'block';
	document.getElementById("select-" + selectName + selectNumber).className = 'selected';
	
};
  </script>

