<script> 
    
    /*
     *   Check if URL contains ?dealkamer=1 and set a cookiw with bame 'dealmaker' valid for 14 days.
     *   Box content is showed when cookie is active
    **/  
    var sfp_deal = (function(){        
        var sfp_deal = {},
            active_cookies = [];    
        
        function setCookie(cname, exdays) {
            var value;
            var expires;
            
            var d = new Date();
            value = d.getTime();
            //value = 11223344;
            d.setTime(d.getTime() + (exdays*24*60*60*1000));
            expires = "expires="+d.toUTCString();
            document.cookie = cname + "=" + value + "; " + expires;
            
        };
        
        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for(var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        };
        
        function getCookiesByPrefix(prefix) {
            var cookies = [],
                all_cookies = document.cookie.split(';'),
                c, cname, cvalue,
                obj = {};
                
            //console.log('all_cookies:', all_cookies);
            for(var i = 0, max = all_cookies.length; i < max; i++) {
                c = all_cookies[i];
                // trim
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(prefix) == 0) {
                    name = c.split('=')[0];
                    value = parseInt(c.split('=')[1]);
                    obj = { 'name' : name , 'value' : value };
                    cookies.push(obj);
                }
            }
            //console.log('cookies:' , cookies);
            return cookies;
        };
        
        function showContentByCookieName(cname) {
            if (getCookie(cname)){
                var elements = document.getElementsByClassName(cname);
                Array.prototype.forEach.call(elements, function(elem){   // or [].forEach.call
                    elem.classList.add("visible");
                });
            }
        }
        
        sfp_deal = {
            update : function(name){   
                
                if (getCookie(name) ){
                    console.log(name+'cookie already set');
                }else{
                    setCookie(name,14);
                    console.log('cookie '+ name +' set to 14 days');
                }
            },
            showContent : function(){
                active_cookies = getCookiesByPrefix("sfp_deal");
                active_cookies.sort(function(a,b){  // Sort from bigger to smaller (newest first)
                    if (a.value < b.value){
                        return 1
                    } 
                    if (a.value > b.value){
                        return -1
                    }
                    return 0
                });
                if (active_cookies.length > 0){
                   switch(active_cookies[0].name){
                        case 'sfp_deal_danskebank':
                            showContentByCookieName('sfp_deal_danskebank');
                            break;
                        default :   
                    } 
                }
            }
        }
        return sfp_deal;
    })();  
    
    //sfp_deal.update("sfp_deal_danskebank",1);
    
    document.addEventListener("DOMContentLoaded", function(event) {
        sfp_deal.showContent();
    });

</script>








        

