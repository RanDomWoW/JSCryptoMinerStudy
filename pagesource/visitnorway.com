	var facets = {};
  $.each(customSettings.types, function(i,v){
    facets[v.name] = v.label;
  });

  var searchLayout = new search.SearchLayout({facets : facets});  
  sb.data("sv_searchLayout", searchLayout);
  
  var runSearch = function(getObj) {
    var query = getObj;
    // @T236955 ignoring extra whitespace
    query.q = $.trim(query.q).replace(/\s+/g, ' ');
    var origQ = query.q;
    // @T235663 / @T233700 add wildcard to the last word for predictive matches
    // @T236955 additional search tweaks to ensure AND logic and boost exact phrase matches in title
    var spellcheckQ = origQ.toLowerCase();
    var qArr = spellcheckQ.split(/\s+/);
    var lastWord = qArr.pop();
    var q = '';
    qArr.forEach(function (term) {
      q += '+' + term + ' ';
    });
    
    q += '+(' + lastWord + ' ' + lastWord + '*)';

    // on page load, repopulates form[data-sv-searchForm] 1-1 with variables present in the URL
    /*$.each(curUrl.get, function(key, val) {
      form.find('[name="'+key+'"]').val(val);
    });*/
    
    // filter query
    var fq = [];
    
    //@T215360: limit results by site
    // filters for items which belong on this site
    fq.push("+sites:"+sv_site.site);
    if (query.type) {
      // filters for items of a single type (pages, listings, events, blog, etc)
      fq.push("+type:"+query.type);
    }
    
    // @T208172 - Local search for premium partners
    if (premiumPartners) {
			//@T215539: product ownership
			// filters for items matching db_owner
			// example : ?q=hotel&owner=Bergen Reiselivslag or ?q=hotel&owner=Visit Sandefjord
			premiumPartners.split(',').forEach(function(premium_partner) {
				fq.push('+custom_arr_string_premium_partners:"'+premium_partner.trim()+'"');
			});
		}
		
    // query fields ( list of fields to test against search term )
    // @T241678 increasing boosting on paid keywords
    var qf = 'custom_text_paid_keywords^100 titleExact^5 title^2 decodedURL^1.5 content altcontent_0 altcontent_1 custom_string_municipality custom_string_destination keywords^0.5';
        
	// @T238910 mobile header search
    if(mobile) {
      qf = qf.replace(/title\^\d+/, 'title^10');
      qf = qf.replace(/titleExact\^\d+/, 'titleExact^15');
    }
    
    var solrArgs = {
      lang: $('html').attr('lang'),
      start : query.start,
      q : q,
      'spellcheck.q' : spellcheckQ, // @T233700 add wildcard to the last word for predictive matches
      fq : fq.join(" "),
      qf : qf,
      pf : qf, // phrase fields, usually identical to qf,
      mm : "100%" //T@222833 // 2 word search, 100% are required. 3 word search, 100% are required, 4+ words are 100% required. this could be tweaked forever ( ref : http://lucene.apache.org/solr/4_10_1/solr-core/org/apache/solr/util/doc-files/min-should-match.html ) 
    };
    
    searchLayout.search(solrArgs, function(err, rtn) {
      rtn.processed.request.q = origQ;
      
      delete rtn.processed.request.type;
      if (query.type) {
        rtn.processed.request.type = { label : facets[query.type], value : query.type };
      }
      
      postSearch(err, rtn);
    });
  };
  
  var postSearch = function(err, rtn) {
    if (err) { console.log(err); alert(err); }
    var resultCount = rtn.processed.response.docs.length;

    // @T273588 create return obj to pass locale code in addition to request and response data to template
    var rtnObj = { request : rtn.processed.request, response : rtn.processed.response, locale_code : locale_code };

    var html = goatee.fill(templates.itemsTemplate, rtnObj);
    var mhtml = goatee.fill(templates.mitemsTemplate, rtnObj);
    items.html(html);
    mitems.html(mhtml);
    
    /* @T235892: UI adjustments to give the partner search results better context */
    if(searchLabel) {
      sb.find('.search-all').removeClass('show');
      items.find('.search-all').addClass('show').click(function() { $(this).prop('href', '/search/?q=' + form.find(".searchInput").val()); });
      items.find('.search-results-view-all .search-label').html('<span data-i18n="search.in">in</span> ');
      items.find('.search-results-view-all .search-label').append(searchLabel);
      
      if(!resultCount) {
        sb.find('.search-all').addClass('show');
        items.find('.search-all').removeClass('show');
      }
    }
    
    $(document).trigger('translate');

    sb.find(".search_result_hero img").error(function() {
      var img = $(this);
      var src = img.prop("src");

      if(src.match(/media\.tellus\.no\/images\/.+t%3D4/)) {
        src = src.replace("t%3D4", "t%3D1");
        img.prop("src", src);
      } else {
        img.remove();
      }
    });
  };

  // waits to call the keyup callback function until 250ms
  // after the last keyup was encountered.
  // prevents querying every single letter as the user types
  // @T237513 - autosuggest not kicking in when typing special characters
  var mobile;
  var prevAutosuggestQuery = '';
  inp.add(minp).keyup(lodash.debounce(function(e) {
    mobile = $(this).is(minp);
    if(mobile) inp.val(minp.val());
    var newQuery = inp.val().trim().replace(/\s+/g, ' ');
  	if(!newQuery) {
      items.add(mitems).html("");
      prevAutosuggestQuery = '';
  	} else {
      if(e.keyCode === 13) {
        sb.find(".searchBtn").click();
      } else if (newQuery !== prevAutosuggestQuery) {
        // remember what we last queried to avoid submitting if the q hasn't actually changed
        prevAutosuggestQuery = newQuery;
        form.submit();
      }
  	}
  }, 250));
	
	form.submit(function(e) {
		var action = $(this).attr("action");
		if(!action) {
			e.preventDefault();
			runSearch({ q : inp.val() });
		}
	});

  sb.on("click", ".searchBtn", function() {
    form.attr("action", form.attr("data-action"));
    form.submit();
  });

	sb.on("click", "[data-search-view-all]", function() {
		form.attr("action", form.attr("data-action"));
		form.submit();
	});

  sb.on("click", "[data-sv-searchLink]", function() {
    var query = $(this).attr("data-href");

    window.location.href = window.location.origin + "/search" + query;
  });
});
