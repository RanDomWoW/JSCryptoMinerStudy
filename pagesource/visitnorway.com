<script type="text/javascript">
	require(["jquery", "async", "plugins_dynamic_content", "sv_stringLib", "sv_clientLib", "domReady!"], function($, async, dynamic, stringLib, clientLib) {
		var root = $("[data-guid=7fdef09d-fdd4-43df-b00c-bcd2801708eb]");
		var switcher = root.find("[data-dynamic-switcher]");
		var switcherSel = switcher.find("select");
		var profiles = [];
		var profilesRaw = [];
		var calls = [];
		
		var index = "default";
		
		var pickVariation = function(index) {
			var div = index === "default" ? root.find("[data-dynamic-default]") : root.find("[data-dynamic-profiles] [data-dynamic-profile]").eq(index);			
			var html = stringLib.decodeScriptTags(div.html());
			root.find("[data-dynamic-chosen]").html(html).attr("data-chosen-profile", index);
			$(document).trigger('translate');
			switcherSel.val(index);
		}
		
		root.find("[data-dynamic-profiles] [data-dynamic-profile]").each(function(i, div) {
			var title = $(div).attr("data-title");
			var option = $("<option />").val(i).text(title);
			switcherSel.append(option);
			
			var attr = $(div).attr("data-dynamic-profile");
			profilesRaw.push(attr);
			profiles.push.apply(profiles, attr.split(","));
		});
		
		switcher.find("select").change(function() {
			pickVariation($(this).val());
		});
		
		// if no profile panels have been added we don't need to check
		if (profiles.length > 0) {
			calls.push(function(cb) {
				dynamic.getProfile({ profiles : profiles.join(",") }, function(err, data) {
					if (err) { throw err; }
					
					if (data.success === true) {
						profilesRaw.some(function(val, i) {
							if (val.indexOf(data.profile.id) > -1) {
								index = i;
								return true;
							}
						});
					}
					
					cb(null);
				});
			});
		}
		
		clientLib.checkCmsLogin({}, function(err, data){
			if (data.loggedIn === true) {
				switcher.addClass("active");
			}
		});
		
		/*
		* @T206013: If a language cookie is found use that instead of the ip to load dynamic content
		*/
		function getCookie(cname) {
	    var name = cname + "=";
	    var ca = document.cookie.split(';');
	    
	    for(var i = 0; i < ca.length; i++) {
	        var c = ca[i];
	        while (c.charAt(0) == ' ') c = c.substring(1);
	        if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
	    }

	    return "";
		}

		var cookieCheck = getCookie("dcOverwrite");
		
		if(cookieCheck !== "") {
			var index = "default";
			
			if(cookieCheck !== "default") {
				var content = root.find("[data-dynamic-profiles] [data-dynamic-profile='" + cookieCheck + "']");

				if(content.length !== 0) {
					index = root.find("[data-dynamic-profiles] [data-dynamic-profile]").index(content);
				}
			}

			pickVariation(index);
		} else {
			async.series(calls, function() {
				pickVariation(index);
			});
		}
		
		/* @T223398 */
		switcher.find('select').each(function () {
	    var $t = $(this);
	    var numberOfOptions = $t.children('option').length;

	    $t.addClass('s-hidden');
	    $t.wrap('<div class="select"></div>');
	    $t.after('<div class="styledSelect"></div>');

	    var $styledSelect = $t.next('div.styledSelect');
	    $styledSelect.text($t.children('option').eq(0).text());

	    var $list = $('<ul />', { 'class': 'options' }).insertAfter($styledSelect);

	    for (var i = 0; i < numberOfOptions; i++) {
	    		var opt = $t.children('option').eq(i);
					$('<li />', { text: opt.text(), rel: opt.val() }).appendTo($list);
	    }

	    var $listItems = $list.children('li');

	    $styledSelect.click(function (e) {
	        e.stopPropagation();
	        $('div.styledSelect.active').each(function () {
	            $(this).removeClass('active').next('ul.options').hide();
	        });
	        $(this).toggleClass('active').next('ul.options').toggle();
	    });

	    $listItems.click(function (e) {
	        e.stopPropagation();
	        $styledSelect.text($(this).text()).removeClass('active');
	        $t.val($(this).attr('rel')).trigger("change");
	        $list.hide();
	    });

	    $(document).click(function () {
	        $styledSelect.removeClass('active');
	        $list.hide();
	    });
		});
	});
</script></div>
				

				
