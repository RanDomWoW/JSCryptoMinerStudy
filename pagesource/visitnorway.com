			return;
		}
		resetMenu();
		var video = document.getElementById("bgvid");
		if(video !== null) {
			video.play();
		}
	});

	//reset menu and close drop down
	function resetMenu(){
		$('body, html').removeClass('active-drop');
		
		// @T318778 - recall and restore previous scroll position
		var scrollTop = $(window).data('previousScrollOffset');
		$('body').css('top', 0);
		$(window).scrollTop(scrollTop);
		
		$('body').removeClass('norwaynow-drop'); /* @T258812 ensure black bg does not show unless ig is open*/

		$('[data-header-btn-dropdown]').removeClass('showSitemap'); /* @T278817 If sitemap visible when menu closes => hide sitemap to display curated nav */
		$('[data-sv-listcontainer]').removeClass('showItems'); /* close mobile accordion navigation menus*/

		

		$navbtn.removeClass('active');
		$root.find("[data-header-btn='menu']").attr("style", "padding-right: 20px;");
		
		//set a slight delay so we don't see the css animation
		/*setTimeout(function(){  
			closeMenuItem();
		}, 300);*/	
	}

	/*
	*	@T201431: GET PAGE DATA FOR RECENT PAGES
	*	Used for building the data for the recent pages collections widget
	*/
	window.hasLocalStorage = $('html').hasClass('localstorage') ? true : false;

  if (window.hasLocalStorage) {
  	$("html").bind("saveRecentPage", function(){
	    var cp = window.currentPage;
	    
	    // if we never got a title, then use the fist h1 found
	    if(cp.title === "" || cp.title === undefined) {
	    	if($("h1").length > 0) {
	    		cp.title = $("h1").first().text();
	    	} else {
	    		var headTitle = $("head title").text();
	    		cp.title = headTitle.substring(0, headTitle.indexOf(" - "));
	    	}
	    }

	    if(cp.description === "" || cp.description === undefined) {
	    	if($(".contentRender_name_plugins_core_textbox").length > 0) {
	    		// @T224838 @T283732 filter out style and script to remove CSS and JS from being shown in the recent pages description
	    		cp.description = $(".contentRender_name_plugins_core_textbox").first().clone().children('style, script').remove().end().text();
	    	} else {
	    		var headDescription = $("head meta[name='description']").attr("content");
	    		cp.description = headDescription;
	    	}
	    }

      var rpStr = localStorage.recentpages;
      var rpArray = rpStr.length === 0 || rpStr === undefined ? [] : JSON.parse(rpStr);

      // if current page already exists in the array, then remove from the array
      for(var i = 0; i < rpArray.length; i++) {
        var page = rpArray[i];

        if(cp.url === page.url) {
          rpArray.splice(i, 1);
          i--;
        }
      }

      // add current page to the array if page doesn't 404
		  // we use the document.title for this, so if the <title> of the nav404 page changes, change this also
		  if (document.title !== "404") {
				rpArray.push(cp);
		  }

      // if array has more than 16 values then remove the oldest one
      if(rpArray.length > 17) {
        rpArray.splice(0, 1);
      }

      // save to localStorage only if we're not in a preview
      if (window === window.top) {
        localStorage.recentpages = JSON.stringify(rpArray);
      }
	  });

    if(localStorage.recentpages === undefined) {
      localStorage.recentpages = '';
    }

    var rpStr = localStorage.recentpages;
    var rpArray = rpStr.length === 0 ? [] : JSON.parse(rpStr);

    // check for expired items and remove them
    for(var i = 0; i < rpArray.length; i++) {
    	var page = rpArray[i];

    	if(page.visited !== undefined) {
    		if(moment(page.visited).diff(moment(), "days") <= -30) {
    			rpArray.splice(i, 1);
    			i--;
    		}
    	}
    }

    localStorage.recentpages = JSON.stringify(rpArray);

    // build the current page
    var title = "Visit Norway";
    var image = "";
    
    
    	image = "https://res.cloudinary.com/simpleview/image/upload/c_fill,f_auto,h_280,q_75,w_280/v1/clients/norway/maxresdefault_3c053b1e-3de8-4639-ab5f-3ef3b2b275ef.jpg"
    
    var description = "";
    
    	description = "Plan your holiday in Norway with free guides and videos. Learn about fjords, northern lights, midnight sun, where to stay, walking, fishing and more.";
    

    var currentPage = {
      title : title,
      image : image,
      description : description,
      url : window.location.href,
      visited : moment()
    }

    window.currentPage = currentPage;
    $("html").trigger("saveRecentPage");
  }

  
  

});
</script></div>
