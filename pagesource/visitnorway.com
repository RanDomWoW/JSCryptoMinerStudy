				/*
				* Sets and displays the map. 
				* This will show all pins for a specific group panel if pins have been provided
				*
				*
				* $pins: all pin items that are to be plotted on the map.
				*/
				function showMap($pins) {
					// begin: Google Maps logic
					var placemarks = [];
					//add map data only if we have pins
					if($pins !== undefined && $pins.length > 0) {
						$pins.each(function(){
							//pin data variables:
							var $this = $(this),
								dataType = $this.attr("data-type"), 
								lat = ($this.attr("data-sv-lat"))*1,
								long = ($this.attr("data-sv-long"))*1,
								recid = $this.attr("data-recid"), 
								image = $this.find("img"),
								fnugg_id = $this.attr('data-fnugg-id'),
								placemarkerData;

								setSrc(image);
								var windowcontent = $this.find("[data-sv-map-infowindow]").html();
								
							//insert map data only if we hav lat and long values
							if (lat && long) {
								placemarkerData = {
									id : dataType + '_' + recid, 
									marker : {
										position : [lat,long]
									},
									infoWindow : {
										content : '<div class="info-window">'+windowcontent+'</div>'
									}
								};
								
								// @T245684 add custom icon image options
								var icon_image = $this.attr('data-icon-image');
								
								// matt's note:
								// if more icon options are needed, i'd change this logic to first check
								// if icon_image !== "default" then add a switch statement within that
								// or possibly come up with a consistent file name convention so that
								// it can just be automated
								if (icon_image === "black_sheep") {
									placemarkerData.marker.icon = {
										url: "/includes/client_public/shared/black_sheep_marker.png"
										
									}
								}
								
								
								placemarks.push(placemarkerData);
							}
						});
						
					}
					placemarks = lodash.uniq(placemarks, 'id'); 

					mapObj.setState({
						placemarksAdded : false,
						placemarks : placemarks,
						open : true
					});
					
					$(document).trigger('translate');
					
					// @T251044 listen for map pin click events
					mapObj.getMap(function(map) {
						var placemarks = map.getPlacemarks();
						lodash.forEach(placemarks, function (placemark) {
							map.placemarkOn(placemark.data.id, 'click', function(e) {
								// @T289064 fnugg snow reports
								var type = placemark.data.id.match(/^[^_]+/);
								var recid = placemark.data.id.match(/[^_]+$/);
								setTimeout(function () {
									var $infowindow = $('[data-gmapcontainer] [data-type=' + type + '][data-recid=' + recid + ']');
									var fnugg_id = $infowindow.attr('data-fnugg-id');
									var $fnuggContainer = $infowindow.find("[data-fnugg-container]");
									fnugg_map_infowindow.render($infowindow, $fnuggContainer, fnuggTemplate, fnugg_id);
								}, 200);
								
								map.setCenter(placemark.data.marker.position); // set map center using placemark coordinates
								if(window.innerWidth <= 640){
									// on mobile click, return to active menu item
									closePanel();
									$nonmapitem.removeClass("hidden");
									return
								} else if (window.innerWidth > 640 && window.innerWidth <= 1024){
									mapOptions.panBy(0, -300); //adjust map position for tablet
								} else {
									mapOptions.panBy(-260, -200); //adjust map position for desktop
								}
							});
						});
					});
				}
				
				/*
				* displays the info window on a map for a given pin
				* 
				*
				* $this: the current active pin
				*
				*/
				function displayMapInfo($this) {
					// @T251044 a nasty hack to style the info window -- this should be replaced with something better if there's time (Matt)
					$('.gm-style-iw').siblings().eq(0).children().eq(1).css({
					  boxShadow: 'none'
					}).end().eq(3).css({
					  background: 'pink'
					});
					$root.find("[data-click-placemark-map]").removeClass('active');
					$this.addClass('active');

					var $pinItem = $this.closest('[data-sv-map-group-item]');

					var dataType = $pinItem.attr("data-type");
					var recid = $pinItem.attr("data-recid");
					var lat = $pinItem.attr('data-sv-lat')*1;
					var long = $pinItem.attr('data-sv-long')*1;

					//make sure we have valid values before displaying map info window
					if(recid.length > 0 && lat != "" && long != "") {
						
						if (dataType === "offer") {
							recid = "offer_" + recid; //update for offers
						}
						else if (dataType === 'event') {
							recid = "event_" + recid; //update for events
						}
						else if (dataType === 'listing') {
							recid = "listing_" + recid; //update for listings
						}
						else if (dataType === 'nav') {
							recid = "nav_" + recid; //update for navItems
						}
						else if (dataType === 'slide') {
							recid = "slide_" + recid; //update for slides
						}
						else {
							recid = "unknown_" + recid;
						}

						mapObj.setState({ open : true }, function() {
							mapObj.getMap(function(map) {
								map.clickPlacemark(recid);
								 if(window.innerWidth >= 641 && window.innerWidth <= 1024){
									//close menu
									mapOptions.closeMenu();
								}
							});
						});
					}
					else {
						closePanel(true);
					}
				}

				/*
				* closes active group panel and map info boxes.
				* 
				*
				* closeInfoOnly: only close the map pin info window
				*
				*/
				function closePanel(closeInfoOnly) {
					$root.find("[data-click-placemark-map]").removeClass('active');
					if(closeInfoOnly === undefined || closeInfoOnly === false) {
						$groupPanel.removeClass('active-group');
						$root.removeClass("display-mobile-map");
						$root.find('[data-sv-group-panel].active-group').removeClass('active-group');
					}

					mapObj.getMap(function(map) {
						if(map._curInfoWindow != null){
							map._curInfoWindow.close();
						}
					});
				}



				/*
				* sets the source of an image: brought over from related content (shell)
				* 
				*
				* nodes: image
				*
				*/
				function setSrc(node) {
					var src = node.attr("src");
					var dataSrc = node.attr("data-src");
					
					if(src !== dataSrc) {
						node.attr("src", dataSrc);
					}
				}

				/*
				* ===========================================
				*  Functionality for sliders (mobile/tablet)
