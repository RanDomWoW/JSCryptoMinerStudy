				////////////////////////////////


				/*
				* Creates drop down items based off a group's title.
				* Loads images for group buttons - desktop version: Only load images if we are under maxSlides.
				* Places items into the drop down content area.
				* -- these items are the buttons used for displaying different sliders on mobile and tablet
				*
				*/
				function mobileSetup(index, flag){
					/* iterate through each group button */
					$groupBtn.each(function(i) {
						var $this = $(this);
						var $groupPanel = $this.closest('[data-sv-group-panel]'); //this is the parent div
						var groupID = $groupPanel.attr('data-sv-map-guid'); //id from the parent div
						var title = $this.attr("data-sv-group-title");

						//get each item/slide that is going to be placed into this slider
						var slides = $groupPanel.find('[data-sv-map-group-item]');
						
						if(flag) { // only do if this is the initial load
							var dropItem = '<li class="item" data-sv-mgroup-id="' + groupID + '">' + title + '</li>';
							
							//insert item into the drop down area
							$groupDropdownItems.append(dropItem);

							// this will be used within desktop version as well
							//fade content in
							$this.addClass('fadein');
						}

						//set the first element to active
						if(i === index){
							$groupSlider.find('.toggle-btn span.text').text(title);
							
							//add slide count
							var slideCount = '<span data-current-slide>1</span> of ' + slides.length + '</span>';
							$root.find('[data-slide-count]').html(slideCount);
							
							createSlider(slides, groupID, true);
						}
					});
				}

				/*
				* Create a slide and implement slick.
				* Slide gets inserted into the content dynamically.
				*
				*
				* slides:  the slides/items that are going to be inserted into this "slide"
				*
				* groupID: the id of the slide. This is used for setting the active slide from a click event
				*          the id of the slide and button id attribute should match.
				*
				* isFirst: true/false - used to set the first slider to active 
				*/
				function createSlider(slides, groupID, isFirst) {
					var $slider;

					if(!$("#slide-" + groupID).length > 0) {
						//clone slides
						var slides = slides.clone();
						var slideID = "slide-" + groupID;
						var classStr = "slides";

						if(isFirst) {
							classStr = "slides active";
						}

						//create slider div
						$slider = $("<div>", {class: classStr, id: slideID}).append(slides);

						//append to content area
						$root.find('[data-sv-group-slider] [data-sv-group-mobile-slides]').append($slider);
					} else {
						$slider = $("#slide-" + groupID);
					}

					$slider.on("init", function() {
						setSrc($slider.find(".map-group-item[data-slick-index='0'] img"));
					})
					
					// @T289064 fnugg snow reports
					$slider.find('[data-sv-map-infowindow]').each(function (infowindow) {
						
						var $infoWindow = $(this);
						var $fnuggContainer = $infoWindow.find('.fnugg-container');
						$infoWindow.on('click', '[data-fnugg-mobile-show]', function () {
							$infoWindow.addClass('show-fnugg-data');
							
							if (!$infoWindow.data('fnugg-loaded')) {
								$infoWindow.data('fnugg-loaded', true);
								var fnugg_id = $infoWindow.find('[data-fnugg-id]').attr('data-fnugg-id');
								fnugg_map_infowindow.render($infoWindow, $infoWindow.find('[data-fnugg-container]'), fnuggTemplate, fnugg_id);
							};
						});
						
						$infoWindow.on('click', '[data-fnugg-mobile-hide]', function () {
							$infoWindow.removeClass('show-fnugg-data');
						});
					});

					//create slick slider
					$slider.slick({
						prevArrow: $root.find(".slick-prev"),
						nextArrow: $root.find(".slick-next"),
						adaptiveHeight: false
					});

					$slider.on("beforeChange", function(event, slick, current, next) {
						setSrc($slider.find(".map-group-item[data-slick-index=" + next + "] img"));
							$('[data-current-slide]').text(next + 1);
						closePanel();
					});
				}

				/*
				* Sets the height of the info-box container to the larget info-box container within a slider item
				* This helps keep the content from jumping up/down.
				* 
				*
				* nodes:  info-box containers within a slider item.
				*
				*/
				function setContentHeight(nodes){
					var elementHeights = nodes.map(function(){
						return $(this).outerHeight(true);
					}).get();

					var maxHeight = Math.max.apply(null, elementHeights);
					// Set each height to the max height
					nodes.height(maxHeight);
				}

				function checkMobile(){

					//If total amount of groups is less than maxSlides, load image.
					// we don't want to load any images if we are in "show-tabs" mode.
					//load images for desktop only
					if(groupBtnCount > 0){
						if(groupBtnCount < maxSlides){
							$groupBtn.each(function(){
								setSrc($(this).find('.map-group-button-int img'));
							});	
						}	
					}
				}
			});
	</sv_script>
 
