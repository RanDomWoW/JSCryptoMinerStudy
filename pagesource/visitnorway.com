<script>
require(["jquery", "moment", "sv_site", "domReady!"], function($, moment, site) {
	var $root = $('[data-header-sv-root]');
	var $navbtn = $root.find('[data-header-btn]');
	var $signinIframe = $('.signin-content iframe');

	var isTouch = $("html").hasClass("touch") || window.innerWidth < 1025;
	var paddingRight = isTouch ? 20 : Math.abs($(window).width() - window.innerWidth) + 20;
	
	// @T287919 pinterest blocked in China, script times out
	if(site.site !== "cn") {
		$('<script>').prop('src', '//assets.pinterest.com/js/pinit.js').data('pin-build', 'parsePins').appendTo('head');
	}
	
	$(document).keyup(function(e) {
		if(e.keyCode === 27) {
			resetMenu();
		}
	});

	// @T251553 - Fade in Header Partner Logo - If no slideshow, position to the right of Norway logo
	var $slideshow = $('.custom_header_slideshow_interior, .custom_header_slideshow_homepage');
	var $hasArticleStyle = $slideshow.hasClass('custom_header_article');
	var $headerLogo = $('.headerpartnerlogo');
	
	// @T251553 Remove this code and related logo wrapper html/css once premium partners migrate their logos to the header panel
	if($headerLogo.length){
		$headerLogo.addClass('fadeIn');
	} else {
		$('[data-sv-logo-wrapper]').css('display', 'block');
	}

	if($headerLogo.length && (!$slideshow.length || $hasArticleStyle)){
		$headerLogo.addClass('noSlideshow');
		$root.addClass('headerNoSlideshowParterLogo');
	}

	//menu buttons
	$navbtn.on('click', function() {
		var $clickedBtn = $(this);

		$('body').removeClass('norwaynow-drop'); /* @T258812 ensure black bg does not show unless ig is open*/

		if(!$clickedBtn.hasClass("nearme")) {
			if($clickedBtn.is('.active')) { //close dropdown/reset
				resetMenu();
				$("html").trigger("playVideo");
			}
			else { //open dropdown
				$("html").trigger("pauseVideo");

				$root.find("[data-header-btn='menu']").attr("style", "padding-right: " + paddingRight + "px;");

				//set active button
				if(!$clickedBtn.is('.language')) {
					$navbtn.removeClass('active');
				}
				if(!$clickedBtn.is('.signin')) {
					$navbtn.removeClass('active');
				}

				$clickedBtn.addClass('active');
				//set active content
				var buttonVal = $clickedBtn.data('header-btn');
				$('[data-button-content]').removeClass("active");
				$('[data-button-content="' + buttonVal + '"]').addClass('active');
				if(buttonVal === 'signin') {
					if(!$signinIframe.prop('src')) {
						var src = $signinIframe.data('src');
						$signinIframe.prop('src', src);
						$signinIframe.css('display', 'block');
					}
					$('.signin-content').scrollTop(0);
				}
				
				//set active drop down
				// @T318778 - disable body scrolling, maintain position for recall
				var previousScrollOffset = $(window).scrollTop();
				$(window).data('previousScrollOffset', previousScrollOffset);
				$('body, html').addClass('active-drop');
				$('body').css('top', (0 - previousScrollOffset) + 'px');
				
				if(buttonVal === 'ig') {
					$('body').addClass('norwaynow-drop'); /* @T258812 ensure black bg shows when ig is open*/
				}

				

				if($clickedBtn.is('.ig')) {
					$("html").trigger("loadInstagram");
				}
				
				if($clickedBtn.is('.book')) {
					$('.button-dropdown-int').scrollTop(0);
				} else if($clickedBtn.is(".search")) {
					$(".search_widget_headerbox [data-sv-searchinput]").focus();
				}
			}
		}
	});
	
	$signinIframe.on('load', function() {
		$('.signin-content').scrollTop(0);
		var height = $signinIframe.contents().find('.content').height() + 100;
		$signinIframe.height(height);
	});

	if(window.location.search.match(/\?signin=\//)) {
		var src = window.location.search.replace('?signin=', '');
		$signinIframe.prop('src', src);
		$signinIframe.css('display', 'block');
		$navbtn.filter('[data-header-btn="signin"]').click();
	}
	
	$("[data-signin-close]").on("click", function() {
		$(this).closest("[data-button-content]").removeClass("active");
		$root.find("[data-header-btn='signin']").removeClass("active");
		$("[data-button-content='menu']").addClass("active");
		$("[data-header-btn='menu']").addClass("active");
	});

	$("[data-language-close]").on("click", function() {
		$(this).closest("[data-button-content]").removeClass("active");
