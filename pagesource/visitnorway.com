<script>
require(["jquery", "goatee", "domReady!"], function($, gt) {
	// T223791 - Cache the nav template which means we have open/select active nav paths
	var navProps = JSON.parse($("meta[name=sv-nav-properties]").attr("content"));
	var self = $("li[data-id="+navProps.id+"]"); // Find current nav item

	self.addClass("current-item").addClass("selected");

	self.parents("li").addClass("dl-subview").addClass("selected");
	self.parents("ul").addClass("dl-subviewopen");
	self.parents("ul.mainmenu > li.selected").addClass("active-section");
	
	// @T284594 run js to create breadcrumbs if the current page is in the main section
	if (navProps.section.indexOf('main') >= 0) {
		var $root = $('[data-guid=41ea4d40-e040-433e-aba3-55cb2e045fff]');
		
		var showBreadcrumbs = true;

		var $breadcrumbContainer = $('[data-breadcrumb-target]');
		var breadcrumbTemplate = $('[data-breadcrumb]').html();
		var navItemProperties = JSON.parse($('meta[name=sv-nav-properties]').attr('content'));
		var parentIds = navItemProperties.parents_ids || [];

		var breadcrumbItems = [];

		var addToBreadcrumb = function(node) {
			var breadcrumbItem = {};
			breadcrumbItem.href = node.attr('href');
			breadcrumbItem.title = node.html();
			breadcrumbItems.push(breadcrumbItem);
		}

		parentIds.forEach(function(parentId){
			var parentItems = $('li[data-id="' + parentId + '"] > a');
			if (!parentItems.length) {
				// some nav item within the path is hidden, don't show breadcrumbs
				showBreadcrumbs = false;
			}
			addToBreadcrumb(parentItems.first());
		});

		// add the current page (this way accounts for items not set to show in nav)
		var currentPageTitle = $('.root_panel').data('sv-title');
		breadcrumbItems.push({
			title: currentPageTitle,
			href: document.location.href
		});
		
		var isHomePage = window.location.pathname === '/';
		var isPremiumDetailPage = location.pathname.indexOf('/listings-') >= 0;

		// check content owner against blacklisted content owners
		var contentOwner = $('.root_panel').data('sv-contentowner');
		// var breadcrumbBlacklist = ['geilo', 'trondheim']; // examples for future use if needed
		var breadcrumbBlacklist = [];

		// hide breadcrumb from homepage, premium partner detail pages, and pages with content owners in blacklist
		if(isHomePage || isPremiumDetailPage || breadcrumbBlacklist.indexOf(contentOwner) != -1){
			showBreadcrumbs = false
		}
		
		// don't show on first level pages, e.g. /places-to-go
		if (breadcrumbItems.length < 2) {
			showBreadcrumbs = false;
		}
		
		// T284594 prevent templating more than one breadcrumb
		if(showBreadcrumbs && !$('[data-breadcrumb-target]').children().length) {
			/* @T313788 insert breadcrumb after 360-tour header on pages without header slideshows
			where the 360 tour widget is dropped within the first rendered panel */
			var hasHeaderSlide = $('.custom_header_slideshow_interior, .custom_header_slideshow_homepage').length > 0;
			if (!hasHeaderSlide){
				var $firstDivInContent = $('.root_panel > .content > .contentRender:first-child');
				var isBreadcrumbAppended = $firstDivInContent.siblings('[data-breadcrumb-target]').length > 0;
				var has360TourHeader = $firstDivInContent.find('.tour-360').length > 0;
				if(has360TourHeader && !isBreadcrumbAppended){
					$breadcrumbContainer.insertAfter($firstDivInContent);
				}
			}
			
			// fill empty breadcrumb div
			$breadcrumbContainer.html(gt.fill(breadcrumbTemplate, {data: breadcrumbItems}));
		}
	}
});
</script></div>
