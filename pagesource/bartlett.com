  <section class="rd-parallax service-news">
    <div data-speed="0.3" data-type="media" data-url="/images/parralax_2-10.jpg" class="rd-parallax-layer"></div>
    <div data-speed="0" data-type="html" class="rd-parallax-layer">
      <div class="section-80">
        <div class="services">
          <div class="shell-wide section-80">
