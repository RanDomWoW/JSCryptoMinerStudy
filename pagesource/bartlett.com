<script type="text/javascript">
			$(function(){
				var $eventSelect = $("#updateregion");
				$eventSelect.on("change", function (e) {
					var r = $eventSelect.val();
					if (r == 'ie') {
						$(".zipFields").hide();
					} else {
						$(".zipFields").show();
					}
				});

				
				
				//Custom validators
				$.validator.addMethod("validZipCode", function(value, element) {
					//check to see if the region is US
					var reg = $("#updateregion").val();
					$.validator.messages.validZipCode = "Please provide a valid postal code.";
					if ( reg == 'az' || reg == 'mw' || reg == 'ne' || reg == 'south' || reg == 'tx' || reg == 'cal') {
						return this.optional(element) || /^\d{5}?$/.test(value);
					} else if (reg == 'gb') {
						$.validator.messages.validZipCode = "Please specify a valid UK postcode";
						return this.optional( element ) || /^((([A-PR-UWYZ][0-9])|([A-PR-UWYZ][0-9][0-9])|([A-PR-UWYZ][A-HK-Y][0-9])|([A-PR-UWYZ][A-HK-Y][0-9][0-9])|([A-PR-UWYZ][0-9][A-HJKSTUW])|([A-PR-UWYZ][A-HK-Y][0-9][ABEHMNPRVWXY]))\s?([0-9][ABD-HJLNP-UW-Z]{2})|(GIR)\s?(0AA))$/i.test( value );
					} else if (reg == 'bc' || reg == 'on'){
						return this.optional( element ) || /^[ABCEGHJKLMNPRSTVXY]\d[ABCEGHJKLMNPRSTVWXYZ] *\d[ABCEGHJKLMNPRSTVWXYZ]\d$/i.test( value );
					}else {
						return true;
					}
				}, "");
				
				// Validation
				$("#regionform").validate(
				{
					submitHandler: function(form) {
						var $regionSelector = $('.footer-region');
						$regionSelector.removeClass('active');
						toast('The region has successfully been updated.', { delay: 3000, className: 'toast-success' });
						setTimeout(function(){document.getElementById('regionform').submit();}, 2000);
						return false;
					},

					// Rules for form validation
					rules:
						{
							updateregion: 'required',
							regionZip: {
								required: true,
								validZipCode: true
							}
						},

					// Messages for form validation
					messages:
					{
						updateregion:
						{
							required: 'Please select a region.'
						},

						regionZip:
						{
							required: 'Please enter a postal code.'
						}
					},

					// Do not change code below
					errorPlacement: function(error, element)
					{
						error.insertAfter(element.parent());
					}
				});
			});
		</script>

