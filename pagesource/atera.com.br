function buscachange(e){
	if(!e){
		e=window.event;
	}
	if(e.keyCode == 38 || e.keyCode == 40){//seta para cima
		try{
			window.clearInterval(interMove);
		}catch(e){}
		try{
			window.clearTimeout(inter);
		}catch(e){}
		andando=false;
	}else{
		if(e.keyCode == 13 && selBusca!=0){
			document.getElementById('tabelaBusca').rows[selBusca-1].onclick();
		}else{
			window.clearTimeout(inter);
			inter = window.setTimeout('buscaSpec3()',400);
		}
	}
}

function moveBusca(e){
	if(!e){
		e=window.event;
	}
	if(!andando){
		if(e.keyCode == 38){//seta para cima
			if(selBusca>0){
				paraCima();
				window.clearTimeout(inter);
				inter = window.setTimeout("new function(){interMove = window.setInterval('paraCima()',130)}",250);
				andando=true
			}
		}else if(e.keyCode == 40){//seta para baixo
			if(selBusca<document.getElementById('tabelaBusca').rows.length){
				paraBaixo();
				window.clearTimeout(inter);
				inter = window.setTimeout("new function(){interMove = window.setInterval('paraBaixo()',130)}",250);
				andando=true
			}
		}
	}
}
function paraCima(){
	if(selBusca>0){
		selBusca--;
		onselBuscaChange();
	}
}
function paraBaixo(){
	if(selBusca<document.getElementById('tabelaBusca').rows.length){
		selBusca++;
		onselBuscaChange();
	}
}
function setselBusca(n){
		selBusca=n;
		onselBuscaChange();
	}

function onselBuscaChange(){
	tab = document.getElementById('tabelaBusca');
	if(tab){
		for(i=0;i<tab.rows.length;i++){
			tab.rows[i].style.background='#ffffff';
		}
		if(selBusca>=1 && selBusca-1<tab.rows.length){
			tab.rows[selBusca-1].style.background='#F0F0E0';
			document.getElementById('linha').value = tab.rows[selBusca-1].attributes.getNamedItem('name').value;
		}
	}
}

