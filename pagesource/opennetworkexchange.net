	<script type="text/javascript">
		window.onload = function() {
		  document.getElementById("txtUsername").focus();
		  
		  var next = ""
		  if (!next && location.hash) {
		      next = "/bap/" + location.hash;
		  }
		  $("input[name=next]").val(next);
		  
		  // If user is at bap.aws.opennetworkexchange.net (the auth pool of servers), or at a singledigits.net URL 
		  // then redirect them to the admin pool at console.aws.opennetworkexchange.net
	      var awsAuthUrl = "bap.aws.opennetworkexchange.net";
		  var sdUrl = "singledigits.net";
		  if ((location.host.indexOf(awsAuthUrl) >= 0) || (location.host.indexOf(sdUrl)) >= 0) {
	          window.location = ("https://console.aws.opennetworkexchange.net" + location.pathname + location.search);
		  }
		};
	</script>
				
