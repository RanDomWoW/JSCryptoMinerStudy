0%{transform:translateY(0%) rotate(5deg)}
75%{transform:translateY(70vh) translateX(-3vh) rotate(5deg)}
100%{transform:translateY(80vh) rotate(5deg)}
}
.stem{width:2px;height:60%;margin-left:7px;background:linear-gradient(to bottom,rgba(255,255,255,0),rgba(255,255,255,0.25));animation:stem .5s linear infinite}
@keyframes stem {
0%{opacity:1}
65%{opacity:1}
75%{opacity:0}
100%{opacity:0}
}

