			$('.apex-m-source').attr('srcset','https://s7d1.scene7.com/is/image/TheNorthFaceBrand/w09_rainwear_hero_m-placeholder?$SCALE-ORIGINAL$');
		}
		function addGifs(){
			$('.apex-d-source').attr('srcset','https://s7d1.scene7.com/is/content/TheNorthFaceBrand/Europe/WEBSITE%20UPDATES/SS18/W09/w09_rainwear_hero_d_rain.gif');
			$('.apex-t-source').attr('srcset','https://s7d1.scene7.com/is/content/TheNorthFaceBrand/Europe/WEBSITE%20UPDATES/SS18/W09/w09_rainwear_hero_t_rain.gif');
			$('.apex-m-source').attr('srcset','https://s7d1.scene7.com/is/content/TheNorthFaceBrand/Europe/WEBSITE%20UPDATES/SS18/W09/w09_rainwear_hero_m_rain.gif');
		}

		$(window).load(function() {
			addGifs();
			makeItRain();
			$('.rain').fadeIn(2000);

			var el1 = $('.element1');
			var el2 = $('.element2');
			var el3 = $('.element3');
			var el4 = $('.element4');
			var el5 = $('.element5');
			var product = $('.product');
			var bg = '.apexflex picture img';
			var rain = '.rain';

			TweenMax.set('.element1', {y:'+=150%'});
			TweenMax.to('.element1', 0.5, {autoAlpha:1, delay: 0.5});

			var tl = new TimelineMax({paused:false, delay:2})
			.to(el1, 0.3, {y:'-=150%'} ,0)
			.to(el2, 0.3, {autoAlpha:1} ,0.3)
			.to(el3, 0.3, {autoAlpha:1} ,1)
			.to(el4, 0.3, {autoAlpha:1} ,1.2)
			.to(el5, 0.3, {autoAlpha:1} ,1.5)
			.to(product, 0.5, {autoAlpha:1} ,1.5)
			.set(bg,{className:"+=blurred"} ,1.5);

		});
		$(window).resize(function() {
			removeGifs();
		});

		// tl.timeScale(1);

	});
