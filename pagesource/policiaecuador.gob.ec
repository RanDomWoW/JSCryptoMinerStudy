  <script type="text/javascript" language="javascript">




var podeDiminuir = true;
var podeAumentar = true;

$j(document).ready(function() {
	$j(".th_lang").mouseover(function(){
		$j("#idiomas").css("right","70px");
	})
	
	$j(".th_lang").mouseout(function(){
		$j("#idiomas").css("right","30px");
	})

})
function aumentaFonte()
{
    if(podeAumentar == true)
    {
        $j("body,html,h1,h2,h3,h4,h5,input,p,span,ul,li,table,td,select").css({
            "font-size": function(index, value) {
                return parseFloat(value) + 1;
            }
        });
        podeAumentar = false;
        podeDiminuir = true;
    }
}

function resetarFonte()
{
    $j("body,html,h1,h2,h3,h4,h5,input,p,span,ul,li,table,td,select").each(function(){
        $j(this).css("font-size","");
    })   
    podeDiminuir = true;
    podeAumentar = true;
}

function diminuirFonte()
{
	if(podeDiminuir == true)
    {
        $j("body,html,h1,h2,h3,h4,h5,input,p,span,ul,li,table,td,select").css({
            "font-size": function(index, value) {
                return parseFloat(value) - 1;
            }
        });
        podeDiminuir = false;
        podeAumentar = true;
	}
}


</script>

