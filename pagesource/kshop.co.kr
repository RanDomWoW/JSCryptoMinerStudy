        function onAirCommonBanner(){
        	//TV쇼핑인경우 타이머 작동안되게 처리 
        	if(!location.href.match('/tvshop')) {
            	Maintimer = setInterval('mainTvTimer()', 1000);
            }
        }
        function mainTvTimer(){
            var _SECOND = 1000,
                        _MINUTE = _SECOND * 60,
                        _HOUR = _MINUTE * 60,
                        _DAY = _HOUR * 24,
                        _UNIT_STRING = {
                                        SECOND: '초',
                                        MINUTE: '분',
                                        HOUR: '시간',
                                        DAY: '일'
            };
        var vodLength = $(".tvVodSt_header").length;
        
        var currentTime = new Date();   
        var startTime = '';
        var endTime = '';
        var startTime2 = '00:00:00';
        var adver = '0';
        var i = '';
        
        if(vodLength <= '0'){
            clearInterval(Maintimer);
            return ;
        }
        
        for(i=1;i<vodLength+1;i++){
            endTime = $("#endVodHeader"+i).val();
            startTime = $("#startVodHeader"+i).val();
            if(!endTime || !startTime) return; 
            var endDate = new Date(
                                    endTime.substring(0, 4),
                                    endTime.substring(5, 7) -1,
                                    endTime.substring(8, 10),
                                    endTime.substring(11, 13),
                                    endTime.substring(14, 16),
                                    endTime.substring(17, 19)
            );
            var startDate = new Date(
                                        startTime.substring(0, 4),
                                        startTime.substring(5, 7) -1,
                                        startTime.substring(8, 10),
                                        startTime.substring(11, 13),
                                        startTime.substring(14, 16),
                                        startTime.substring(17, 19)
            );
            var j = i+1;
            if(i != vodLength){
                startTime2 = $("#startVodHeader"+j).val();
                if(!startTime2)return;
                var startDate2 = new Date(
                                        startTime2.substring(0, 4),
                                        startTime2.substring(5, 7) -1,
                                        startTime2.substring(8, 10),
                                        startTime2.substring(11, 13),
                                        startTime2.substring(14, 16),
                                        startTime2.substring(17, 19)
                );
            }else{
                startTime2 = '00:00:00';
            }
            if(endDate < startDate){
                clearInterval(Maintimer);
                return ;
            }
            //다음 방송 시작시간이 x 오늘 하루 방송 모두 종료
            if(startTime2 == '00:00:00'){
                startTime2 = 'end';    
                break;
            }
            //방송중인 상품 시작, 종료시간을 가지고 빠져나가게 함
            if(currentTime < endDate && currentTime > startDate){
                break;
            }
            //광고중일때 adver을 1로 광고임을 파악하게 함
            if((currentTime < endDate && currentTime < startDate)){
                adver = '1';
                break;
            }
        }
        //0 방송, 1 광고
        if(adver == 0){
            var distance = endDate - new Date();
            var days = Math.floor(distance / _DAY);
            var hours = (days * 24) + Math.floor((distance % _DAY) / _HOUR);
            var minutes = Math.floor((distance % _HOUR) / _MINUTE);
            var seconds = Math.floor((distance % _MINUTE) / _SECOND);
               
            var hoursString = hours + "";
            var minutesString = minutes + "";
            var secondsString = seconds + "";
               
            var remainedTime = "";
                
            if(hours && hours > 0){
                if(hoursString.length < 2){
                    remainedTime += "0" + hoursString;
                }else{
                    remainedTime += hoursString;
                }
            }else{
                remainedTime += "00";
            }
            remainedTime += ":";
            if(minutes && minutes > 0){
                if(minutesString.length < 2){
                    remainedTime += "0" + minutesString;
                }else{
                    remainedTime += minutesString;
                }
            }else{
                remainedTime += "00";
            }
            remainedTime += ":";
            if(seconds && seconds > 0){
                if(secondsString.length < 2){
                    remainedTime += "0" + secondsString;
                }else{
                    remainedTime += secondsString;
                }
            }else{
                remainedTime += "00";
            }
            //방송중
            jQuery(".nonAir").css("display","none");
            jQuery(".onAirArea").css("display","none");
            jQuery("#nowOnAirHeader"+i).css("display","block");
            
            // TV쇼핑일 땐 타이머 세팅되지 않도록(TV쇼핑과 sync 맞추기 위함)
            //if(!location.href.match('/tvshop')) {
                jQuery("#remainTimeHeader"+i).text(remainedTime);
            //}
            
            i='';
            //마지막 방송 종료
            if(startTime2 == 'end' && remainedTime == '00:00:00'){
                      
                //jQuery(".onAirArea").css("display","none");
                clearInterval(Maintimer);
            }
        }else{
            jQuery(".onAirArea").css("display","none");
            jQuery(".nonAir").css("display","");  
        }
        
        //정말 필요한 코드인지 파악 중
//        if( (remainedTime == "00:00:00") && remainedTime != "00:00:00"){
//            jQuery(".time > strong").eq(1).html("aaa광고중입니다.");            
//        }
        
        //하루 방송이 모두 종료되었을때..필요없을듯
//        if(startDate2 == "end" && remainedTime == "00:00:00"){
//            jQuery(".time > strong").eq(1).html("00:00:00");   
//            clearInterval(Maintimer);
//        }
    }
        </script>
        
