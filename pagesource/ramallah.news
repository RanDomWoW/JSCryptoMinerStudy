<script>
    var $breaking_ids = [];
    $.pluck = function (obj, key) {
        var ret = [];
        $.each(obj, function (k, value) {
            ret[k] = value[key];
        });
        return ret;
    }
    $.arraysEqual = function (a1, a2) {
        return JSON.stringify(a1) == JSON.stringify(a2);
    }
    $.arr_diff = function (a1, a2) {
        var a = [], diff = [];
        for (var i = 0; i < a1.length; i++) {
            a[a1[i]] = true;
        }
        for (var i = 0; i < a2.length; i++) {
            if (a[a2[i]]) {
                delete a[a2[i]];
            } else {
                a[a2[i]] = true;
            }
        }
        for (var k in a) {
            diff.push(k);
        }
        return diff;
    };
    function play_breaking() {
        $('.breaking-news .marquee').marquee('destroy');
        $('.breaking-news .marquee').marquee({
            duration: 9000,
            gap: 50,
            delayBeforeStart: 0,
            direction: 'right',
            duplicated: false,
            pauseOnHover: true
        });
    }
    $(document).ready(function () {
        setInterval(function () {
            getBreakings()
        }, 9000);
        getBreakings();
    });
    function getBreakings() {
        $.get('https://ramallah.news/index.php?ajax=breaking', function (data) {
            if (data != false) {
                var $breaking = $('#breaking'), $new_ids = $.pluck(data, 'id');
                if (data.length) {
                    $breaking.removeClass('hidden');
                    if ($.arr_diff($breaking_ids, $.pluck(data, 'id')).length > 0) {
                        $('#breaking audio').trigger('play');
                        $breaking_ids = $new_ids;
                        $('.breaking-news .marquee').marquee('destroy');
                        $breaking.find('.marquee a').remove();
                        $.each(data, function (index, element) {
                            var link = element.link ? 'href="' + element.link + '"' : '';
                            $breaking.find('.marquee').prepend('<a data-id="' + element.id + '" ' + link + ' class="item breaking-' + element.id + '"><span class="date">' + element.created_at + '</span> ' + element.title + '</a>');
                        });
                        play_breaking();
                    }
                } else {
                    $breaking_ids = [];
                    $breaking.addClass('hidden');
                }
            } else {
                $breaking_ids = [];
                $('#breaking').addClass('hidden');
            }
        }, 'json');
    }
</script><!-- Start New breaking marquee java code -->
