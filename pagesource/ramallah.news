        function loadVideo() {
            $(window).on('scroll', function () {
                if (videoContainer.length > 0 && videoContainer.offset().top - $(window).scrollTop() <= 400) {
                    if (isTimerStart) {
                        $.get('index.php?ajax=ad&do=increment&id=' + videoAdId, function (result) {
                        }, 'json');
                        videoPlayer = document.getElementById("videoPlayer");
                        if (videoPlayer != null) {
                            var clickCount = 1;
                            if (isMobile) {
                                videoPlayer.play();
                                videoContainer.on('click', function (event) {
                                    // event.preventDefault();
                                    if (event.srcElement.className != 'controls aty-close') {
                                        window.location.href = '/banner/' + videoAdId;
                                    }
                                });
                            } else {
                                videoPlayer.play();
                                videoContainer.on('click', function (event) {
                                    // event.preventDefault();
                                    if (event.srcElement.className != 'controls aty-close') {
                                        window.location.href = '/banner/' + videoAdId;
                                    }
                                });
                            }
                            videoPlayer.volume = volume;
                            videoPlayer.addEventListener('ended', function () {
                                videoContainer.animate({'height': '0px'}, 500, function () {
                                    $(this).remove();
                                });
                            }, false);
                            videoPlayer.addEventListener('timeupdate', function seektimeupdate() {
                                var progressValue = videoPlayer.currentTime * (100 / videoPlayer.duration);
                                videoContainer.find('.progress .bar').css({'width': progressValue + '%'});
                            }, false);
                        }
                        isTimerStart = false;
                        videoContainer.find('.aty-close').css({'display': 'none'});
                        videoContainer.find('.count-down').css({'display': 'block'});
                        videoContainer.on('mouseenter', function () {
                            setVolume('+');
                            videoContainer.find('.aty-volume_off').stop(true, false).fadeOut(500);
                        }).on('mouseleave', function () {
                            setVolume('-');
                            videoContainer.find('.aty-volume_off').stop(true, false).fadeIn(500);
                        });
                        var newHeight = (videoContainer.width() * 9 / 16);
                        videoContainer.animate({'height': newHeight + 'px'}, 1000);
                        setInterval(function () {
                            if (countDown > 0) {
                                $('.count-down').text(countDown--);
