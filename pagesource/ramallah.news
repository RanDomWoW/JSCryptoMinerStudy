                            });
                        });
                    }
                }
            });
            function setVolume(operation) {
                if (onVolumeAction == false) {
                    onVolumeAction = true
                    var timeInterval = setInterval(function () {
                        if (operation == '+') {
                            if (volume.toFixed(2) < 0.99) {
                                volume += 0.01;
                                videoPlayer.volume = volume;
                            } else {
                                videoPlayer.volume = 1;
                                clearInterval(timeInterval);
                                onVolumeAction = false;
                            }
                        } else {
                            if (volume.toFixed(2) > 0.01) {
                                volume -= 0.01;
                                videoPlayer.volume = volume;
                            } else {
                                videoPlayer.volume = 0;
                                clearInterval(timeInterval);
                                onVolumeAction = false;
                            }
                        }
                    }, 10);
                }
            }
        }
