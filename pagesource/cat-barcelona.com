    function check_send() {
        if( document.getElementById('nombre').value == '' || document.getElementById('email').value == '' || document.getElementById('comentarios').value == '' ){
            mostrar_error('Debes rellenar los campos del formulario');
            return false;
        }
        if( isEmail(document.getElementById('email')) == 0 ){
            mostrar_error('Debes especificar un correo válido');
            return false;
        }

        return true;
   }
   function mostrar_error(html) {
        $('#errorform').css({'display':'block'});
        if (document.getElementById('errorformServer')!=null) 
			$('#errorformServer').css({'display':'none'});
   }

    function isEmail(fld) {

        var email;
        var parts;
        var esOk = false;
        email = fld.value;
        var m = email.match(/^\s*(\S+(\s+\S+)*)\s*$/);
        parts = email.split(/@/);
        esOk = ((m[2] == null) || (m[2] == ""));
        esOk &= (parts.length == 2);
        if (esOk) {
          var dot = parts[1].split(/\./);
          esOk = (dot.length > 1);
        }
        return esOk;
    }

