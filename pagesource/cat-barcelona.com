  		function rwdOcultar(){
			jQuery(".mobile-hide").css("display","none");	
			jQuery(".mobile-show").css("display","block");				
			
		}
		function rwdMostrar(){
			jQuery(".mobile-hide").css("display","block");	
			jQuery(".mobile-show").css("display","none");	
		}
  
  		jQuery(document).ready(function(){			
			if(jQuery(window).width() < 650) rwdOcultar();
			else rwdMostrar();		
			
			// BOTON MENU
			jQuery("a.menu-button").click(function(){
				var estado = jQuery("#mobile-menu").css("display");
				jQuery(".submenu").css("display","none");
				if(estado == "none")	jQuery("#mobile-menu").css("display","block");
				else jQuery("#mobile-menu").css("display","none");
			});
			
			// SUBMENUES
			jQuery("a.desplegable").click(function(){
				var target = "#"+jQuery(this).attr("rel");	
				var estado = jQuery(target).css("display");
				jQuery("ul.submenu").css("display","none");
				if(estado == "none"){
					jQuery(target).css("display","block");
					jQuery(this).addClass("desplegado");
				}
				else
				{
					jQuery(this).removeClass("desplegado");
					jQuery(target).css("display","none");
					
				}
			});
		});  
		
		jQuery(window).resize(function(){
			if(jQuery(window).width() < 650) rwdOcultar();
			else rwdMostrar();
		});
		
		
