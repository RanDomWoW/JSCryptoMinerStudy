    /**
     * This function shows the exit modal when pages redirect to DB.com
     * @return
     */
    function showDBIModal(targetURL, modalType){

        if(targetURL == undefined || targetURL == '') {
            targetURL = "www.davidsbridal.com"
        }
        if(modalType == undefined || modalType == '' || modalType.toUpperCase() == 'EXIT') {
            modalType = "Exit";
        }
        var dbiModalTargetURLId = 'dbi_' + modalType + '_ModalTargetURL';
        var exitDialog = dijit.byId('dbi_Exit_Modal');

        exitDialog.onShow = function(){
            this.redirectTimeOutCaller = setTimeout(function(){
                window.location.replace(targetURL);
            }, "3000");
        };
        exitDialog.onHide = function(){
            clearTimeout(this.redirectTimeOutCaller);
        };
        exitDialog.onBlur = function(){
            this.hide();
        }
        exitDialog.onFocus = function(){
            exitDialog.resize();
        }

        exitDialog.show();

        var pageName = "";
        if (targetURL.indexOf("invitations.davidsbridal.com") !== -1) {
            pageName = "Visit Invitations Website Modal";
            
                    pageName = "Visit Invitations Website Modal - UK";
                
        } else if(targetURL.indexOf("Content_StyleandFashionGuide_styleguide") !== -1) {
            
                    pageName = "Visit Inspiration & Guidance Modal - UK";
                
        } else if(targetURL.indexOf("davidsbridal.com") !== -1) {
            pageName = "Visit US Site Modal";
            
                    pageName = "Visit US Website Modal - UK";
                
        }

        utag.view({
            "page_name": pageName,
            "page_type": "Modal",
            "site_type": utag_data.site_type,
            "visitor_device": utag_data.visitor_device,
            "event_type": "virtualPageview"
        });
    }

