<script>
$(document).ready(function(){

  function parseFeed(xmlData){

    var counter = 0;
    $(xmlData).find("item").each(function(){

      var title = $(this).find("title")[0].textContent;
      var pubdate = new Date($(this).find("pubDate")[0].textContent);

      var description = String($(this).find("description")[0].textContent).split("<p>");

      var image = $.parseHTML(description[0]);
      var srcset = $(image[0]).attr("srcset").split(", ");
      var image_src = String(srcset[0]).split(" ");
      image = image_src[0];

      var revised_description = String(description[1]).replace("<\/p>","");

      var link = $(this).find("link")[0].textContent;

     var months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];

      var item = '<div class="entry-wrapper">';
      item += '<div class="entry-image" style="background-image:url(' + image + ')"><a href="' + link + '"><img src="' + image + '" / style="display: none;"></a></div>';
      item += '<div class="entry-date">' + pubdate.getDate() + " " + months[pubdate.getMonth()] + " "  + pubdate.getFullYear() +'</div>';
      item += '<div class="entry-title"><h4><a href="' + link + '">' + title + '</a></h4></div>';
      item += '<div class="entry-content"><p>' + revised_description;
      item += '<a target="_blank" class="read" href="' + link+ '">Read&nbsp;More</a>   ';
      item += '</p></div>';
      item += '</div';

      $("#feed").append(item);

      counter++;

      if(counter>=3){
        return false; 
      }

    });

  }
  $.ajax({
      method: "GET",
      url: "//cdn.shopify.com/s/files/1/1889/5061/t/16/assets/unwound.xml?9503225194529990449"
  }).done(function(data){
      parseFeed(data);
  }).fail(function(error){
      console.log(error)
  });
});


</script>
 
	



