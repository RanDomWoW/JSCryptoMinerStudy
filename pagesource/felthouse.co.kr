    <script type="text/javascript">
(function($){
	var MainBnr = function(){
		this.id = "#mainBnr";
		this.btn= "#bnrBtn";
		this.width="";
		this.select=0;
		this.timer=null;
		this.speed=1000;
		this.fade=650;
		this.imgFile=[];
		this.imgSw=[];
	}
	MainBnr.prototype={
		setting:function(){
			var _this = this;
			var imgExt = /(\.\w{3,4}$)/;
			var url=[];
			reSize();
			$(window).resize(function(){
				reSize();
			});
			this.len = $('>li',this.id).length;
			for(var i=0; i<this.len; i++){
				if(i > 0) $('>li:eq('+i+')',this.id).css({'z-index':'0'}).hide();
				this.imgFile[i] = $('>li:eq('+i+')',this.btn).find('img').attr('src');
				this.imgSw[i] = new Image();
				this.imgSw[i].src = this.imgFile[i].replace(imgExt,'_r$1');
				url[i] = $('>li:eq('+i+')',this.id).find('a').attr('href');
			}
			$('>li:first',this.btn).find('img').attr('src',this.imgSw[0].src);
			$(this.id).parent('div').on({
				mouseenter:function(){
					_this.stop();
				},
				mouseleave:function(){
					_this.start();
				}
			});
			$('>li',this.btn).on({
				mouseenter:function(){
					_this.select = ($(this).index()-1);
					_this.moving();
				},
				click:function(){
					document.location.href=url[$(this).index()];
				}
			});
		},
		moving:function(){
			this.select++;

			if(this.select >= this.len) this.select=0;
			var viewBnr = $('>li:eq('+this.select+')',this.id);
			viewBnr.css({'z-index':'1'}).stop(true,true).fadeIn(this.fade);
			$('>li',this.id).not(viewBnr).css({'z-index':'1'}).stop(true,true).fadeOut(this.fade);
			$('>li:eq('+this.select+')',this.btn).find('img').attr('src',this.imgSw[this.select].src);
			for(var i=0; i < this.len; i++){
				if(this.select != i){
					$('>li:eq('+i+')',this.btn).find('img').attr('src',this.imgFile[i]);
				}
			}
		},
		start:function(){
			var _this=this;
			this.timer=setInterval(function(){
				_this.moving();
			},this.speed);
		},
		stop:function(){
			clearInterval(this.timer);
		},
		init:function(){
			this.setting();
			this.start();
		}
	}
	var reSize=function(){
		var winW = $(window).width();
		if(winW < 1150){
			$('#mainBnrSec').css({'width':'1150px'});
		}else{
			$('#mainBnrSec').css({'width':'100%'});
		}
	}
	$(document).ready(function(){
		var mainBnr = new MainBnr();
		mainBnr.id="#mainBnr";
		mainBnr.btn= "#bnrBtn";
		mainBnr.speed=3500;
		mainBnr.fade=700;
		mainBnr.init();
	});
})(jQuery);
</script> 
