var iedom_event=document.all||document.getElementById
var actualheight_event=''
var cross_scroler, ns_scroler
var pausespeed_event=(pauseit_event==0)? copyspeed_event: 0
 
function populate_event(){
if (iedom_event){
cross_scroler=document.getElementById? document.getElementById("iescrollerEvent") : document.all.iescrollerEvent
cross_scroler.style.top=parseInt(scrollerheight_event)+8+"px"
cross_scroler.innerHTML=scrollercontent_events
actualheight_event=cross_scroler.offsetHeight
}
else if (document.layers){
