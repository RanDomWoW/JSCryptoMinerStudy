function scrollscroller_event(){
 
if (iedom_event){
if (parseInt(cross_scroler.style.top)>(actualheight_event*(-1)+8))
cross_scroler.style.top=parseInt(cross_scroler.style.top)-copyspeed_event+"px"
else
cross_scroler.style.top=parseInt(scrollerheight_event)+8+"px"
}
else if (document.layers){
if (ns_scroler.top>(actualheight_event*(-1)+8))
ns_scroler.top-=copyspeed_event
else
ns_scroler.top=parseInt(scrollerheight_event)+8
}
}
 
