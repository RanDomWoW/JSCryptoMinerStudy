<script src="/export/system/modules/ar.com.puntal/resources/js/jquery.galeriacarousel.js"></script>
<script src="/export/system/modules/ar.com.puntal/resources/js/jquery.marquesinacarousel.js"></script>
<script src="/export/system/modules/ar.com.puntal/resources/js/jquery.cinecarousel.js"></script>
<script src="/export/system/modules/ar.com.puntal/resources/js/jquery.easing.1.3.js"></script>
<script src="/export/system/modules/ar.com.puntal/resources/js/jquery.mousewheel.js"></script>

<script>  
	$(document).ready(function(e) {
		var nav = $(".header");
		var posicion = nav.offset().top;	
			
       function fixedTop (){
			var posicionScroll = $(window).scrollTop();
			if ( posicionScroll > posicion){
					$(".header").addClass('fixHeader');
	
			} else {
					$(".header").removeClass('fixHeader');
			};			
		};
				
		$(window).scroll( function(){
			fixedTop();
			console.log(posicion);
		});
		
		var $menuDesplegado = $(".menuDesplegado");
		var $iconNav = $("span.nav");
		var $closeNav = $(".sideNav .close-nav");
		
		if ($(window).width() > 990){	
					$iconNav.click(function(){
							$menuDesplegado.slideToggle();
					});					
					$menuDesplegado.mouseleave(function(){
							$(this).slideUp();
					});					
			} else {
					$iconNav.click(function(){
							$menuDesplegado.addClass('active');
					});
					$closeNav.click(function(){
							$menuDesplegado.removeClass('active');
					});
			}
		
		$( window ).resize(function() {
			if ($(window).width() > 990){			
					$iconNav.click(function(){
							$menuDesplegado.slideToggle();
					});
					
					$menuDesplegado.mouseleave(function(){
							$(this).slideUp();
					});
					
			} else {
					$iconNav.click(function(){
							$menuDesplegado.addClass('active');
					});
					$closeNav.click(function(){
							$menuDesplegado.removeClass('active');
					});
			}
		});
	  	
		$(".srh").click(function(){
			$(".searchInput").fadeIn();
		});
		
		$(".closeSearch").click(function(){
			$(".searchInput").fadeOut();
		});
		
		$(document).mouseup(function (e)
		{
			var container = $(".searchInput");
		
			if (!container.is(e.target) // if the target of the click isn't the container...
				&& container.has(e.target).length === 0) // ... nor a descendant of the container
			{
				container.fadeOut();
			}
		});
		
		$("input").focusin(function(){
			$(this).attr('placeholder','');
		});
	});
</script>

<script type="text/javascript">
	$('#ca-container-cine').cinecarousel();
</script>
<script type="text/javascript">
	$('#ca-container-marquesina').marquesinacarousel();
</script>

</html>