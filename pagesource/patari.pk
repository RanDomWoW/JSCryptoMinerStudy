    <meta name="viewport" content="width=device-width,initial-scale=1.0, maximum-scale=1.0, target-densityDpi=device-dpi">
    <meta name="description" content="{{ meta.desc }}">
    <meta name="author" content="">
    <meta name="apple-itunes-app" content="app-id=1011704215">
    <meta name="google-play-app" content="app-id=com.centsol.csongs">
    <!-- HTML5 Base Url -->
    <base href="/">
    <!-- For Search Engines -->
    <meta name="fragment" content="!">

    <!-- iOS Applinks -->
    <meta property="al:ios:url" content="patariApp://" />
    <meta property="al:ios:app_store_id" content="1011704215" />
    <meta property="al:ios:app_name" content="Patari" />

    <!-- Android Applinks -->
    <meta property="al:android:package" content="patari://patari.pk/#/home/new" />

    <!-- Facebook related metatags -->
    <meta property="fb:app_id" content="{{ meta.fb.fb_app_id }}">
    <meta property="og:url" content="{{ meta.fb.fb_url }}">
    <meta property="og:title" content="{{ meta.fb.fb_title }}">
    <meta property="og:image" content="{{ meta.fb.fb_image }}">
    <meta property="og:site_name" content="{{ meta.fb.fb_site_name }}">
    <meta property="og:type" content="{{ meta.fb.fb_type }}">

    <meta property="og:description" content="{{ meta.desc }}">

    <meta ng-if=" meta.fb.fb_type=='music.song'" property="music:duration" content="219" />
    <meta ng-if=" meta.fb.fb_type=='music.song'" property="music:preview_url:secure_url" content="{{ meta.fb.fb_song_link }}" />
    <meta ng-if=" meta.fb.fb_type!='music.musician'" property="music:musician" content="{{ meta.fb.fb_musician }}" />
    <meta ng-if=" meta.fb.fb_type=='music.album'" property="music:release_date" content="{{ meta.fb.fb_album_release }}" />
    <meta ng-repeat="tags in meta.fb.album_tags" property="{{tags.property}}" content="{{tags.content}}" />


    <!-- Twitter Card -->
    <meta name="twitter:card" content="{{ meta.tw.tw_card_type }}" />
    <meta name="twitter:site" content="@patarimusic" />
    <meta name="twitter:title" content="{{ meta.tw.tw_title }}" />
    <meta name="twitter:description" content="{{ meta.tw.tw_description }}" />
    <meta name="twitter:image" content="{{ meta.tw.tw_image }}" />
    <meta name="twitter:url" content="{{ meta.tw.tw_url }}" />
    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->

