	//生成二维码
	var _qrcode_create = function(_moban_id){
		var _that = $("#wwei_qrcode_create");
		//内容
		var _qrdata = _getQrcodeData();
		if(!_qrdata){
			return false;
		}
		var _data = {
			"qrid" : _qrid,
			"data" : _qrdata,
			"moban_id":_moban_id,
			"size" : $("#form-size").val(),
			"level" : $("#form-level").val()
		};
		//样式设置
		var _style = {
			"protype" : $("#form-protype").val()
		};
		if(_moban_id<=0)//set moban
		{
			if($("#setting-base").hasClass("in"))//颜色
			{
				_style.ptcolor = $("#ptcolor").val();
				_style.inptcolor = $("#inptcolor").val();
				_style.fcolor = $("#fcolor").val();
				_style.bcolor = $("#bcolor").val();
				_style.mbtype_hb = $("#mbtype_hb").prop("checked") ? 1 : 0;
			}
			if($("#setting-logo").hasClass("in"))//logo
			{

				_style.logo_id = $("#picker_logo").data("id");
				_style.logo_width = _parseInt($("#logo_width").val());
				_style.logo_height = _parseInt($("#logo_height").val());
				_style.logo_border = $("#logo_border").prop("checked") ? 1 : 0;

				if(_style.logo_id !="" && (isNaN(_style.logo_width) || _style.logo_width<=0 || isNaN(_style.logo_height) || _style.logo_height<=0)){
					$.alert("请设置LOGO大小");
					return false;
				}
			}
			if($("#setting-bg").hasClass("in"))//bg
			{
				_style.bg_id = $("#picker_bg").data("id");
				_style.bgqrsize = _parseInt($("#bgqrsize").val());
				_style.bgx = _parseInt($("#bgx").val());
				_style.bgy = _parseInt($("#bgy").val());
				_style.rotate = $("#rotate").val();
				
				
				if(_style.bg_id !="" && (isNaN(_style.bgqrsize) || _style.bgqrsize <=0)){
					$.alert("请设置二维码在背景图的位置和大小");
					return false;
				}
			}
			if($("#setting-fg").hasClass("in"))//fg
			{
				_style.fg_id = $("#picker_fg").data("id");
			}
		}
		
		_data.style_setting = _style;


		//loading
		_that.html('<img src="/static/images/load_qrcode.gif"/> 生成中...').attr("disabled",true);
		
		$("#view-qrcode-img").html('<img src="/static/images/load_qrcode.gif" />');

		//ajax
		$.ajax({
			type: "POST",
			dataType: "json",
			url: _create_url,
			data: _data,
			error: function(XMLHttpRequest, textStatus, errorThrown) {
				_that.html('生成二维码').attr("disabled",false);
				$.alert('<span class="red">错误：'+ XMLHttpRequest.status +' '+ textStatus +'，请您重试</span>');
				return false;
			},
			success: function(rsData){
				_that.html('修改二维码').attr("disabled",false);
				
				if(rsData.code !=1){
					$.alert('<span class="red">错误：'+ rsData.msg +'</span>');
					return false;
					
				}
				_qrid = rsData.qrid;
				$("#view-qrcode-img").html('<img src="'+ rsData.data +'" />');
				
				$("#png_down").attr("href",rsData.png_url);
				$("#svg_down").attr("href",rsData.svg_url);
				$("#eps_down").attr("href",rsData.eps_url);
				$("#down-btngroup").show(500);
				$("#wwei_qrcode_meihua").show(500);
			}
		});
		
		
	};
