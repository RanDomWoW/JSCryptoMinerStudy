		set_mobanma = function(page_list){
			
			if(_qrid<=0){
				return false;
			}
			//page set
			$("#mobanma_prev").show();
			$("#mobanma_next").show();
			if(mobanma_page.page_index <=1){
				$("#mobanma_prev").hide();
			}else{
				$("#mobanma_prev").show();
			}
			if(mobanma_page.page_index >= mobanma_page.page_total){
				$("#mobanma_next").hide();
			}else{
				$("#mobanma_next").show();
			}
			$("#msck_page_info").html('共 ' +mobanma_page.count +'  个，第 ' +mobanma_page.page_index +' / ' + mobanma_page.page_total + ' 页');
			//console.log(page_list);
			var _html = '';
			for(var i in page_list){
				_html+='<tr><td>'+ page_list[i].id +'</td>';
				if(page_list[i].cover)
				{
					_html+='<td><img src="'+ page_list[i].cover +'" style="width:45px;height:45px;" class="cover-popover" data-html="true" data-content="<img src=\''+ page_list[i].cover +'\'/ style=\'max-width:250px;max-height:250px;\'>" data-trigger="hover"/></td>';
				}else{
					_html+='<td>无图</td>';
				}
				_html+='<td><div style="width:70px;height:20px;overflow:hidden" title="'+ page_list[i].title +'">'+ page_list[i].title +'</div></td>';
				_html+='<td><a class="text-info mobanma_create"  data-id="'+ page_list[i].id +'" href="javascript:;" target="_blank">使用模板</a></td></tr>';
			}
			if(_html ==''){
				_html = '<tr><td colspan="4">未找到模板，请先领取 - 付费/免费 模板</td></tr>';
			}else
			{
				_html = $(_html);
				//create
				_html.find(".cover-popover").hover(function(){
					$(this).popover('show');
				});
				_html.find(".mobanma_create").click(function(){

					$("#mobanma_modal").modal("hide");

					var _moban_id = $(this).data("id");
					_qrcode_create(_moban_id);

					return false;
				});
			}
			
			//set show
			
			$("#mobanma_list").html(_html);
			$("#mobanma_modal").modal("show");
		},
		get_mobanma = function(){

			if(_qrid<=0){
				return false;
			}
			var _page_index = mobanma_page.page_index;
			var _kwd = $("#mobanma_kwd").val();
			if(!typeof(mobanma_list[_page_index]) == "undefined"){
				set_mobanma(mobanma_list[_page_index]);//cache set
			}else{
				//ajax
				$("#mobanma_loading").show();
				$.ajax({
				   type: "POST",
				   url: "http://www.wwei.cn/qrcode-index-mobanma_me.html",
				   data: {"page_index":mobanma_page.page_index,"kwd":_kwd},
				   dataType:"json",
				   success: function(rsData){
						$("#mobanma_loading").hide();
						if(rsData.status!=1){
							$.alert(rsData.msg);
							return false;
						}
						mobanma_page = rsData.data.page_info;
						mobanma_list[_page_index] = rsData.data.list;//cache
						set_mobanma(rsData.data.list);//set
				   }
				});


				
			}
			
	};
