							$preview.attr("src",_file_path + "&size=150x150");
						}
					}else if(upkey =='bg'){
						if(resporse.width<100 || resporse.height<100){
							$msg.html('<span class="red">图片尺寸不能小于100PX</sapn>');
						}else
						{
							_pick.data("width",resporse.width);
							_pick.data("height",resporse.height);
							$preview.attr("src",_file_path + "&size=100");
							/*$preview.load(function(){
								$("#btn_jcrop").click();
							});*/

							$preview.on("load",function(){
								$("#btn_jcrop").click();
							});
						}
						
					}else if(upkey == 'vcard' || upkey == 'weixin'){
						$preview.attr("src",_file_path + "&size=64x64");
					}else if(upkey == 'album'){
						//$preview.attr("src",_file_path + "&size=64x64");
						var _max_albums = _parseInt($("#max-albums").text()) + 1;
						$("#max-albums").text(_max_albums);
						if(_max_albums<= fnum)
						{
							var _album_ids = _pick.data("id") + "," + resporse.id_key;
							_pick.data("id",_album_ids);
							//size
							var _album_size = _pick.data("size");
							if(_album_size ==""){
								_album_size = [];
							}else{
								//_album_size = JSON.parse(_album_size);
							}
							_album_size.push({
								"id" : resporse.id_key,
								"width" : resporse.width,
								"height" : resporse.height
							});

							//_album_size = JSON.stringify(_album_size);
							_pick.data("size",_album_size);

							$preview.append('<img src="'+ _file_path +'&size=64x64" style="max-width:64px;max-height:64px; margin:2px; border:1px solid #ccc"/>');
						}
						if(_max_albums >=fnum)
						{
							_pick.hide();
						}
					}
				}else
				{
					console.log(resporse);
					$msg.html('<span class="red">上传失败，请重新上传</sapn>');
				}
			});

			uploader.on( 'uploadError', function( file ) {
				uploader.removeFile( file );
				$msg.html('<span class="red">上传出错</sapn>');
			});
			uploader.on("error", function (type) {
				if (type == "Q_TYPE_DENIED") {
					$.alert("请上传JPG、PNG、GIF格式文件");
				} else if (type == "Q_EXCEED_SIZE_LIMIT" || type=="F_EXCEED_SIZE") {
					$.alert("图片大小不能超过2M");
				}else {
					$.alert("上传出错！请检查后重新上传！错误代码:"+type);
				}
			});


			uploader_arr[i] = uploader;


		})(uparr,i)

	}


});
</script>

