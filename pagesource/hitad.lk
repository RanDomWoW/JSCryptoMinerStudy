<script>
	$(document).ready(function(){
	
		$(".category_indicator").click(function(){			
			update_icon();
			$(".mobile_filter").toggle();
		});	
		
		$("#collapse_bottom").click(function(){			
			update_icon();
			$(".mobile_filter").toggle();
		});	
		
		$(".category_ul").click(function(){			
			update_icon();
			$(".mobile_filter").toggle();
		});	
		
		$("#cat_id").on("change", function(){
			update_icon();
		});		
		
		 
		$(".mobile_filter ul li").click(function(){			
			$("#cat_id").val($(this).attr('id'));			
			update_icon();
			$("html, body").animate({ scrollTop: $('.option_container').offset().top }, 1000);
			
			$(".option_container").html("<center><br><br><img src='http://www.hitad.lk/template/resources/img/icons/ajax-loader.gif'/></center>");
			$(".mobile_filter ul li").css({"background":"#fff"});
			$(this).css({"background":"#ccc"});
			$.ajax({
				url:"http://www.hitad.lk/ajaxdata/get_category_data_mobile",
				type:"POST", 
				data:{id:$(this).attr('id'), text:$("#search").val()},
				success:function(data){
					$(".option_container").html(data);					
				},
				error:function(){
					$(".option_container").html("");
				}  
			});
		});		
		
	});
	
	function update_icon(){
		if($("#cat_id").val()!="0"){
			$(".category_indicator").attr("src", "http://www.hitad.lk/template/resources/img/icons/category_selected.jpg");
		}else{
			$(".category_indicator").attr("src", "http://www.hitad.lk/template/resources/img/icons/category_notselected.jpg");
		}
	}
	
	function toggle_options(obj){
		var state_obj = $(obj).children("span");		
		if(state_obj.attr("title") == "collapse"){			
			$(document.getElementById(state_obj.attr("class"))).hide();
			$(document.getElementById(state_obj.attr("class")+"_hr")).hide();
			state_obj.html('<img src="http://www.hitad.lk/template/resources/img/icons/expland_icon.jpg"/>');
			state_obj.attr("title", "expand");
		}else{
			$(document.getElementById(state_obj.attr("class"))).show();
			$(document.getElementById(state_obj.attr("class")+"_hr")).show();
			state_obj.html('<img src="http://www.hitad.lk/template/resources/img/icons/collaps_icon.jpg"/>');
			state_obj.attr("title", "collapse");
		}		
	}
