       $(document).ready(function(){
            $('#cat_id').change(function(){
                    var id = $('#cat_id').val();
                    $('#left_filter').html('<img align="center" src="http://www.hitad.lk/template/resources/img/ajax-loader.gif">');
                    $.ajax({
                        type: 'POST',
                        url: 'http://www.hitad.lk/ajaxdata/get_category_data',
                        data: {"id": id},
                       // dataType: 'json',
                        success: function(sdata){                         
                            $('#left_ct').html(sdata);
							$('input').iCheck({
								checkboxClass: 'icheckbox_square-blue',
								radioClass: 'iradio_square-green',
								increaseArea: '20%'
							});
							$('.tmp').removeClass('filter-body').addClass('filter-body');
                        }
                    });
            });
			
			update_icon();
			
			//GKM :: Checkboxes duplicate to search
			$('input').on('ifChecked', function(){				
				var name 	= $(this).attr('name');
				var value 	= $(this).val(); 
				var ref 	= name.replace('[]', '_')+value;
				$('#tmp_options').append('<input type="hidden" id="'+ref+'" name="'+name+'" value="'+value+'"/>');
			});
			$('input').on('ifUnchecked', function(){
				var name 	= $(this).attr('name');
				var value 	= $(this).val(); 
				var ref 	= name.replace('[]', '_')+value;
				$('#'+ref).remove();
			});
			$('#Price_from').change(function(){$('#h_from').val($(this).val())});
			$('#Price_to').change(function(){$('#h_to').val($(this).val())});
			$('#duration').change(function(){$('#h_duration').val($(this).val())});
			$('#Year_from').change(function(){$('#h_year_from').val($(this).val())});
			$('#Year_to').change(function(){$('#h_year_to').val($(this).val())});
        }); 
		
		function suggestions(value){
			console.log(value);
			$.ajax({
				url:'http://54.255.159.185/ajaxdata/suggestions/'+value,
				type : 'GET',
				success:function(result){					
					if(result != ''){
						result = JSON.parse(result);
						$('#suggestions').html('');
						$.each(result, function(i, item) {
							$('#suggestions').append('<li>'+item+'</li>');
						});
					}
				}
			});
		}	
