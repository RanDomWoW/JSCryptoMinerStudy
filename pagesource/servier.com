<script>
            (function($){
                window.onload=function(){
                  $('.slider').slick({
                  autoplay:true,
                  autoplaySpeed:1500,
                  });
                };
                
                jQuery('.wpsisac-slick-slider-1 ').on('beforeChange', function(event, slick, currentSlide, nextSlide){
                    $( ".slider-content, .slick-dots" ).finish();
                    console.log("opacity down");
                    console.log(event.currentTarget);
                    $( ".slider-content" ).animate({ opacity: 0 }, 300 );
                    $( ".slider-content" ).animate({ marginLeft: -800 }, 300 );
                    $( ".slick-dots" ).animate({ opacity: 0}, 200);
                });
                jQuery('.wpsisac-slick-slider-1 ').on('afterChange', function(event, slick, currentSlide, nextSlide){
                    $( ".slider-content, .slick-dots" ).finish();
                    console.log("opacity up");
                    console.log(event.currentTarget);
                    $( ".slider-content" ).delay(800).animate({ marginLeft: 0 }, { duration: 400, queue: false });
                    $( ".slider-content" ).delay(2300).animate({ opacity: 1 }, { duration: 1500, queue: false } );
                    $( ".slick-dots" ).delay(2700).animate({ opacity: 1},  { duration: 1500, queue: false });
                });
                
                
                $(document).on('click', '.control-articles', function(event){
                    event.preventDefault();
                    $direction = $(this).attr('id');
                    $total_blocs = 10;
                    $last = $('#next').attr('last');
                    $next = $('#next').attr('next');
                    $('.article').addClass('none');
                    if($direction == 'next'){
                        if($next == $total_blocs){   
                            $next = 1;
                            $nextone = 2;
                            $nexttwo = 3;
                            $last = $total_blocs-1;
                            $('.article-'+$next).removeClass('none');
                            $('.article-'+$nextone).removeClass('none');
                            $('.article-'+$nexttwo).removeClass('none');
                            $('#next').attr('next', '3');
                            $('#next').attr('last', '1'); 
                        } 
                        else{
                            $lastmore = parseFloat($last)+1;
                            $nextone = parseFloat($next)+1;
                            $('.article-'+$last).addClass('none');
                            $('.article-'+$next).removeClass('none');
                            $('.article-'+$nextone).removeClass('none');
                            $('.article-'+$lastmore).removeClass('none');
                            $('#next').attr('next', parseFloat($next)+1);
                            $('#next').attr('last', parseFloat($last)+1);
                        }
                    }
                    if($direction == 'prev'){
                        if($last == '1'){
                            $last = parseFloat($total_blocs)-1;
                            $lasttwo = parseFloat($total_blocs)-2;
                            $next = parseFloat($total_blocs)+1;
                            $lastmore = $last+1;
                            $lastmoretwo = $last+2;
                            $('.article-'+$last).removeClass('none');
                            $('.article-'+$lasttwo).removeClass('none');
                            $('.article-'+$lastmore).removeClass('none');
                            $('#next').attr('next', parseFloat($total_blocs));
                            $('#next').attr('last', parseFloat($total_blocs)-1);
                        }
                        else{
                            $previous = $last-1 ;
                            $previousbefore = $last+1 ;
                            $('.article-'+$last).removeClass('none');
                            $('.article-'+$previous).removeClass('none');
                            $('.article-'+$previousbefore).removeClass('none');
                            $last = $last-1;
                            $next = $last+2;
                            $('.article-'+$next).removeClass('none');
                            $next = $next+1;
                            $('#next').attr('last', $last);
                            $('#next').attr('next', $next);
                        }
                    }
                });
            })(jQuery);
    
</script>