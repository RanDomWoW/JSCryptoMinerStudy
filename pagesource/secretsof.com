function show_cat(cat,subcat){
	$("#prodpopup").popup("hide");
	if(cat!=last_id || $("#catwin"+cat).is(":hidden")){ 
		console.log ("sessionStorage.catpopup="+sessionStorage.catpopup);
		console.log ("sessionStorage.catpopupid="+sessionStorage.catpopupid);
		console.log ("sessionStorage.subcatpopupid="+sessionStorage.subcatpopupid);
		if(sessionStorage.catpopupid==cat && sessionStorage.catpopup){
			$("#catwin"+cat).html(sessionStorage.catpopup);
			$("#catwin"+cat).show();
			if(sessionStorage.subcatpopupid>0){
				$(".subcat").show();
				$(".subcat").hide();
				$(".subcatp").hide();
				$("."+sessionStorage.catpopupid+"_"+sessionStorage.subcatpopupid).show();
			}else{
				$(".subcat").show();
				$(".subcatp").hide();
			}
		}else{
			$("#catwin"+cat).html("<img align='center' style='margin:50px;' src='https://www.secretsof.com/images/ajax-loader.gif'>");
			$("#catwin"+cat).show();
			var dataString="designercode=&cat="+cat;
			$.ajax({
				 type: "POST",
				 url: "//www.secretsof.com/categories.php",
				 data: dataString,
				cache: false,
				success: function(result){			
					parts=result.split("~");
					if (parts[0] != "success") {
						$("#catwin"+cat).hide();
						alert(result);
					}else{						
						sessionStorage.catpopupid=cat;
						sessionStorage.catpopup=parts[1];
						$("#catwin"+cat).html(parts[1]);
						$("#catwin"+cat).show();
						if(subcat){
							$(".subcat").show();
							$(".subcat").hide();
							$(".subcatp").hide();
							$("."+cat+"_"+subcat).show();
							sessionStorage.subcatpopupid=subcat;
						}else{
							sessionStorage.subcatpopupid=0;
							$(".subcat").show();
							$(".subcatp").hide();
						}
						$("#catwin"+cat).css('max-height', (window.innerHeight-50)+'px');
						$(".dropdown-menu").hide();
					}
				} 
			});	
		}
	}
}


