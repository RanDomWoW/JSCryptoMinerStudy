		$("#paginationNext").click(function() {	
			if (correntPage != (pagesCount-1) * limit && correntPage < ((pagesCount-1) * limit)) {
				correntPage += 1 * limit;
				$('#paginationScroll').animate({marginRight: (-(correntPage) * aWidth) + "px"}, 1000);
			} else {
				correntPage = 0;
				$('#paginationScroll').animate({marginRight: "0px"}, 2000);
			}
		});
				
		$("#paginationPrevious").click(function() {	
			if (correntPage != 0 && correntPage >=0) {
				correntPage -= 1 * limit;
				$('#paginationScroll').animate({marginRight: (-(correntPage) * aWidth) + "px"}, 1000);
			} else {
				correntPage = (pagesCount - 1) * limit;
				$('#paginationScroll').animate({marginRight: (-(correntPage) * aWidth) + "px"}, 2000);
			}
		});
		
		function pagination() {
			id = (Math.floor(0 / limit));
			id += ((0 % limit) == 0 ? -1 : 0); 
			correntPage += 1 * (limit) * id;
			$('#paginationScroll').animate({marginRight: (-(correntPage * 2) - aWidth) + "px"}, 1000);
		}
			});
