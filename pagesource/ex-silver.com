    function InitSlider(sloderindex) {
        var ul = $('.itemCon:eq(' + sloderindex + ') .itemTxt ul');
        var prev = $(".prev");
        var next = $(".next");
        if (ul.children().length <= 1) {
            return false;
        }
        var blen = true;
        var timer = setInterval(nextf, 2000);

        prev.click(function () {
            if (!blen) {
                return false;
            }
            blen = false;
            clearInterval(timer);
            ul.children().last().prependTo(ul);
            ul.css('marginTop', -30);
            ul.animate({ marginTop: 0 }, 500, function () {
                blen = true;
            });
            timer = setInterval(nextf, 2000);
        });

        next.click(function () {
            if (!blen) {
                return false;
            }
            blen = false;
            clearInterval(timer);
            nextf();
            timer = setInterval(nextf, 2000);
        })

        function nextf() {
            ul.animate({ marginTop: -30 }, 500, function () {
                ul.children().first().appendTo(ul);
                ul.css('marginTop', 0);
                blen = true;
            })
        }
    }
    $(function () {
        $(".main2_left2 .title span").each(function () {
            InitSlider($(this).index());
        })

        $('.main2_left2 .title span').click(function () {
            $(this).addClass('active').siblings().removeClass('active');
            $('.main2_left2_con .itemCon').eq($(this).index()).show().siblings().hide();
