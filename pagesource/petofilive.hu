	<script type="text/javascript">
	    var hms_agree_cookies_initWindowLoaded = false;
		jQuery(function(){
			if (navigator.cookieEnabled === true)
			{
				if (document.cookie.indexOf("visited") == -1)
				{
					jQuery(".resultCookie").load('//www.petofilive.hu/wp-content/plugins/hms-agree-cookies/12/cookies.html',  function( response, status, xhr ) {
					    if (typeof(hms_agree_cookies_init) === typeof(Function))
					    {
                            jQuery(window).resize(function() {
                                if (!hasAgreeCookiesClosed)
                                {
                                    hms_agree_cookies_init();
                                }
                            });
                            jQuery(window).load(function() {
                                hms_agree_cookies_initWindowLoaded = true;
                                hms_agree_cookies_init();
                            });
                        }

						if ( status == "error" ) {
							var msg = "Hiba az átvitelkor: ";
							console.log( msg + xhr.status + " " + xhr.statusText );
							return false;
						}
		  				jQuery('#closecookie').click(function() {jQuery('#cookie').hide("fast");
													document.cookie="visited=yes; expires=Thu, 31 Dec 2020 23:59:59 UTC; path=/";
												});
					});
				}
			}
		});

        // bbc
        jQuery(document).on('click', '.hms_cookeBbc_activate', function() {
            hasAgreeCookiesClosed = true;
            var cookieHeight = jQuery('#cookie').height();
            jQuery('#cookie').hide("fast", function() {
                if (cookieToTop)
                {
                    jQuery('body').css('margin-top', -cookieHeight);
                }
                jQuery('#hms_agree_cookies_wrapper').height('auto').css({overflow: 'hidden'});
                jQuery("body").css({'overflow': 'auto'});
                jQuery('#hms_agree_cookies_wrapper #site-header').css({width: 'calc(100%)'});
            });
            if (cookieToTop)
            {
                // az összes fixed pozíciójú elemet vissza kell tolni....
                jQuery('*').each(function() {
                    var $this = jQuery(this);
                    if ($this.css('position') === 'fixed')
                    {
                        $this.css('margin-top', (parseInt($this.css('margin-top')) - cookieHeight)+'px');
                    }
                });
            }
            else
            {
                hms_agree_cookies_init();
            }
            switch(parseInt(jQuery(this).attr('data-func')))
            {
                case 1 :
                    document.cookie="visited=1; expires=Thu, 31 Dec 2060 23:59:59 UTC; path=/";                    break;
                case 2 :
                    break;
                case 3 :
                    document.cookie="visited=2; expires=Thu, 31 Dec 2060 23:59:59 UTC; path=/";                    break;
            }
        });

        hasBodyWrappered = false;
    var hasAgreeCookiesClosed = false;
    var cookieToTop = false;

    function calculateHeightForOverleapingBanner()
    {
        return jQuery(window).height() - jQuery('#cookie').height();
    }

    function hms_agree_cookies_init()
    {
        if (!hms_agree_cookies_initWindowLoaded)
        {
            return;
        }
        jQuery('.resultCookie').show();
        if (hasBodyWrappered)
        {
            jQuery('#hms_agree_cookies_wrapper').height(calculateHeightForOverleapingBanner());
        }
        else
        {
            hasBodyWrappered = true;
            if (cookieToTop)
            {
                // az összes fixed pozíciójú elemet le kell tolni....
                jQuery('*').each(function() {
                    var $this = jQuery(this);
                    if ($this.attr('id') != 'cookie')
                    {
                        if ($this.css('position') === 'fixed')
                        {
                            $this.css('margin-top', parseInt($this.css('margin-top')) + jQuery('#cookie').height()+'px');
                        }
                    }
                });
                document.addEventListener('scroll', function(e) { jQuery(this).scroll(); jQuery(window).scroll(); }, true);
                jQuery("body").wrapInner('<div onscroll="if (typeof(lazyLoadImagesFunction) !== \'undefined\') { lazyLoadImagesFunction.forceLazyLoad(); }" id="hms_agree_cookies_wrapper" style="position: relative; margin-top: '+jQuery('#cookie').height()+'px; height: '+(calculateHeightForOverleapingBanner())+'px; overflow-x: hidden; overflow-y: scroll; z-index: 1103 !important"></div>');
                jQuery("body").css({overflow: 'hidden'});
            }
            else
            {
                document.addEventListener('scroll', function(e) { jQuery(this).scroll(); jQuery(window).scroll(); }, true);
                jQuery("body").wrapInner('<div onscroll="if (typeof(lazyLoadImagesFunction) !== \'undefined\') { lazyLoadImagesFunction.forceLazyLoad(); }" id="hms_agree_cookies_wrapper" style="height: '+calculateHeightForOverleapingBanner()+'px; overflow-x: hidden; overflow-y: scroll;"></div>');
            }
        }
    }

    
	</script>

