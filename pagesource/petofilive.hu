		<span id="npHelper" style="display:none"></span>
		<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jquery-ajaxtransport-xdomainrequest/1.0.3/jquery.xdomainrequest.min.js"></script>
		
		<script type="text/javascript">
		
			var flow = "";
			jQuery(".playNow .nowContent .nowPlaying").mouseenter(function(){
				var npTitleWidth = getTitleWidth("#npHelper", ".nowPlaying", ".title");
				var npContainerWidth = jQuery(".nowPlaying").width();
				
				if(npTitleWidth > npContainerWidth){
				jQuery(".nowPlaying .title").css("min-width", "350px");
				jQuery(".nowPlaying .title").css("text-align", "left");
					flow = setInterval(function(){
						var margin = parseInt(jQuery(".nowPlaying .title").css("margin-left"));
						
						if(margin <= npTitleWidth && margin > (0 - npTitleWidth) ){
							jQuery(".nowPlaying .title").css("margin-left", margin - 1);
						}else{
							jQuery(".nowPlaying .title").css("margin-left", npContainerWidth);
						}
					}, 50);
				}		
			});
			
			jQuery(".playNow .nowContent .nowPlaying").mouseleave(function(){
				clearInterval(flow);
				jQuery(".nowPlaying .title").css("margin-left", 0);
			});
			
			var programflow = "";
			jQuery(".playNow .nowTitle .programTitle").mouseenter(function(){
				var npTitleWidth = getTitleWidth("#npHelper", ".nowTitle", ".programTitle");
				var npContainerWidth = jQuery(".programTitle").width();
				
				if(npTitleWidth > npContainerWidth){
					jQuery(this).css("text-align", "left");
					
					programflow = setInterval(function(){
						var margin = parseInt(jQuery(".programTitle").css("margin-left"));
						
						if(margin <= npTitleWidth && margin > (0 - npTitleWidth) ){
							jQuery(".programTitle").css("margin-left", margin - 1);
						}else{
							jQuery(".programTitle").css("margin-left", npContainerWidth);
						}
					}, 50);
				}		
			});
			
			jQuery(".playNow .nowTitle .programTitle").mouseleave(function(){
				clearInterval(programflow);
				jQuery(this).css("margin-left", 0);
			});
			
			
			function getTitleWidth(helperId, contentClass, textClass){
				var font = jQuery(contentClass).css("font-size");
				var text = jQuery(contentClass).css("text-transform");
				jQuery(helperId).css("font-size", font);
				jQuery(helperId).css("text-transform", text);
					
				var t = jQuery(textClass).text();
				jQuery(helperId).text(t)
				return jQuery(helperId).width();
			}
			
				
			
			function refreshNowPlay(){
				var xmlUrl = "//stats.mtv.hu/iface/now_2.xml";
					 
				jQuery.ajax({
					type: "GET",
					url: xmlUrl,
					dataType: "xml",
					success: function(xml){
						changeNowPlayContent(xml);	
					},
					error: function(XMLHttpRequest, textStatus, errorThrown){
						//console.log("error "+XMLHttpRequest+' - '+textStatus+' - '+errorThrown);
					}
				});
			}
		
			function changeNowPlayContent(xml){
				var Items = jQuery(xml).find('Items');
				if(Items.length > 0){
					jQuery(Items).find('Item').each(function(){
						var str = jQuery(this).find('Name').text();
						var pos = str.indexOf("-"); 
						var name = str.slice(0, pos);
