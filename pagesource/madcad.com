	function validateUploadForm() {
		j("#uploadErrors").text('');
		var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
    	var hasError = false;
    	
    	if (j.trim(j("#uploadName").val()) == "") {
            j("#uploadErrors").append('Please provide your name.<br/>');
            hasError = true;
    	}
    	
    	if (!emailReg.test(j("#uploadEmail").val()) || j("#uploadEmail").val() == "") {
        	j("#uploadErrors").append('Please enter a valid email address.<br/>');
        	hasError = true;
    	}
    	
    	if (j.trim(j("#uploadPhone").val()) == "") {
            j("#uploadErrors").append('Please provide a phone number.<br/>');
            hasError = true;
   		}
   		
   		if (j("#uploadState").val() == "") {
            j("#uploadErrors").append('Please select a state.<br/>');
            hasError = true;
   		}
   		
   		if (j("#uploadJurisdiction").val() == "") {
            j("#uploadErrors").append('Please select a jurisdiction.<br/>');
            hasError = true;
   		}
   		
   		if (j.trim(j("#uploadNotes").val()) == "") {
            j("#uploadErrors").append('Please provide an upload description.<br/>');
            hasError = true;
   		}
   		
   		if(hasError == true) {
        	return false;
    	} else {
			if (j('#uploadJurisdiction').val() != null) return confirm('Are you sure you want to submit the changes for '+j('#uploadJurisdiction').val()+' - '+j('#uploadState').val()+'?'); else return confirm('Are you sure you want to upload the attached files for '+j('#uploadState').val()+'?');	
