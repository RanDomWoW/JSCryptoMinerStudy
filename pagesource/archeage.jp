			$('#cboxClose').show();
		}});
		
		$('#yt02').colorbox({iframe:true, current:'', innerHeight:'480px', innerWidth:'850px', onLoad:function(){
			$('#cboxClose').hide();
		},onComplete:function(){
			$('#cboxClose').show();
		}});
		
		$('#yt_top').colorbox({iframe:true, current:'', innerHeight:'480px', innerWidth:'850px', onLoad:function(){
			$('#cboxClose').hide();
		},onComplete:function(){
			$('#cboxClose').show();
		}});
		
		//定義
