	var $bg_movie = $("#bg_movie");
	var marginLeftRight = 50;//マージン
	var marginTopBottom = 80;//マージン
	var pvbox = [
								'<iframe width="" height="" src="https://www.youtube.com/embed/B3pMCGksf7Q?rel=0&amp;wmode=transparent&amp;autoplay=1&amp;vq=hd1080" frameborder="0" allowfullscreen></iframe>',
