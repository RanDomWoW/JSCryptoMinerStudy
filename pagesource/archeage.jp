		//動画背景非表示
		$bg_movie.fadeOut();
		$movie_navi.fadeOut();
		//動画iframe削除
		$("#pv iframe").remove();
		
		return false;
	});
		
	$("#movie_navi li a").click(function(){
		var num = $(this).parent("li").index();
		
		$("#movie_navi li a").removeClass("active");
		$(this).addClass("active");
		
		//動画格納
		$pv.empty().append(pvbox[num]);
		
		sizeSet();
		
		return false;
	});
	
	//自動動画再生
	$(".header_bn02 a").click();
});
	
	
	
