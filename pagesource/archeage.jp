							];
	var $windowWidth;
	var $windowHeight;
	var $btn_close = $("#btn_close a");
	var $movie_navi = $("#movie_navi");
	
	//サイズ設定
	sizeSet();
	
	$(window).resize(function(){
		sizeSet();
	});
	
	function sizeSet(){
		
		//ウィンドウ幅・高さ取得
		$windowWidth = $(window).width();
		$windowHeight = $(window).height();
		
		//動画表示領域
		$pv.css({
			"width" : $windowWidth - marginLeftRight*2,
			"height" : $windowHeight - marginTopBottom*2,
			"margin-top" : marginTopBottom,
			"margin-right" : marginLeftRight,
			"margin-bottom" : marginTopBottom,
			"margin-left" : marginLeftRight
		});
		
		//動画サイズ設定
		$("#pv iframe").css({
			"width" : $windowWidth - marginLeftRight*2,
			"height" : $windowHeight - marginTopBottom*2,
		});
	}
	//UPDATE PVボタン
	$(".header_bn02 a").click(function(){

		//ウィンドウ幅・高さ取得
		$windowWidth = $(window).width();
		$windowHeight = $(window).height();

		//動画背景表示
		$bg_movie.fadeIn();
		$movie_navi.fadeIn();

		//動画格納
		$pv.append(pvbox[0]);

		// ナビゲーション
		$("#movie_navi li a").removeClass("active");
		$("#movie_navi li").eq(0).find("a").addClass("active");

		//動画サイズ設定
		$("#pv iframe").css({
			"width" : $windowWidth - marginLeftRight*2,
			"height" : $windowHeight - marginTopBottom*2,
		});

		return false;
	});
		
