  .x-column.x-1-3.col-product:nth-child(odd){
    margin-right:2%;
    margin-left:0;
  }
  
  .x-column.x-1-3.col-product:nth-child(even){
    margin-left:2%;
    margin-right:0;
  }
  
  .rtl .x-column.x-1-3.col-product:nth-child(odd){
    margin-left:0%;
    margin-right:2%;
  }
  
  .rtl .x-column.x-1-3.col-product:nth-child(even){
    margin-right:0%;
    margin-left:2%;
  }
}

@media (max-width:767px){
  
