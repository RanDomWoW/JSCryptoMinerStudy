body, p, h1, h2, h3, h4, h5, h6,
.h1, .h2, .h3, .h4, .h5, .h6 {
  font-family: 'BoutrosAsma';
}
h1, .h1 {
  font-weight: 900;
  font-size: 45px;
  letter-spacing: 0.025em;
}
h2, .h2 {
  font-weight: 400;
  font-size: 36px;
  letter-spacing: 0.025em;
}
h3, .h3, h4, .h4, h5, .h5, h6, .h6, p {
  font-weight: 300;
  letter-spacing: 0.025em;
}
h3, .h3 {
  font-size: 30px;
  letter-spacing: 0.025em;
}
h4, .h4 {
  font-size: 25px;
  font-weight: 900;
  letter-spacing: 0.025em;
}
h5, .h5 {
  font-size: 16px;
  letter-spacing: 0.025em;
}
body, p {
  font-size: 16px;
  line-height: 1.6;
}

.rtl h1, .rtl .h1,
.rtl h2, .rtl .h2,
.rtl h3, .rtl .h3, .rtl h4, .rtl .h4, .rtl h5, .rtl .h5, .rtl h6, .rtl .h6, .rtl p {
  letter-spacing: -0.05px;
}



.font-boutros{
  font-family: 'BoutrosAsma' !important;
}

.has-shadow{
  text-shadow: 0px 2px 1px rgba(0, 0, 0, 0.35);
}

/* BUTTON STYLES */
.x-btn {
  font-family: 'BoutrosAsma';
  font-weight: 400;
  font-size: 18px;
  text-shadow: none;
}
.btn-1 {
  background-color: #fff;
  padding: 1.2em 3em;
  color: #1b477b;
  border: none;
  font-weight: 300;
  font-family: 'BoutrosAsma';
  letter-spacing: 0.1em;
  font-size: 14px;
  text-shadow: none;
}
.btn-1:hover {
  background-color: #1b477b;
  color: #fff;
}

/* COLOR HELPERS */
.white {
  color: #fff;
}
.black {
  color: #000;
}

/* PADDING HELPERS */
.col-xs {
  padding-left: 2%;
  padding-right: 2%;
}
.col-sm {
  padding-left: 3%;
  padding-right: 3%;
}
.col-md {
  padding-left: 4%;
  padding-right: 4%;
}
.col-lg {
  padding-left: 6%;
  padding-right: 6%;
}
.col-xl {
  padding-left: 10%;
  padding-right: 10%;
}

/* RTL Columns */
body.rtl .x-column:not(.col-product){
  margin-left: 4%;
  margin-right:0;
}
/* breadcrumb delimeter */
body.rtl .x-crumbs-delimiter{
  margin-right:10px;
  margin-left:0;
}

@media (max-width:979px){
  body.rtl .x-column:not(.col-product).x-1-4:nth-child(even){
   margin-left:0; 
  }
}

@media (max-width:767px){
  body.rtl .x-column:not(.col-product){
   margin-left:0; 
  }
}

body.rtl .x-column:not(.col-product).last, 
body.rtl .x-column:not(.col-product):last-of-type{
  margin-left: 0;
  margin-right: 0;
}

/* MAX WIDTH HELPERS */
.max-450 {
  max-width: 450px;
}

/* CARD FLEX STYLES */
.cards {
  display: flex;
  flex-flow: row wrap;
}

/* QUOTE MARK */
.quote {
  position: relative;
}
.quote:before {
  position: absolute;
  content: "";
  background-image: url("/wp-content/uploads/2017/10/Quote-L.png");
  background-repeat: no-repeat;
  background-size: 100%;
  width: 40px;
  height: 40px;
  left: -50px;
}

/* FOOTER OPT-IN STYLES */
input[type="email"] {
  border-radius: 0;
  -webkit-border-radius: 0;
  height: 50px;
  margin-top: 10px;
  border: 0;
  min-width: 100%;
  font-size: 1.125em;
  font-weight: 300;
  padding: 0.2em 1.5em;
}
.submit-btn {
  text-shadow: none;
  height: 50px;
  padding: 0.7em 2.25em;
  font-size: 1.125em;
  min-width: 100%;
}
@media(min-width:768px) {
  input[type="email"] {
    min-width: 430px;
  }
  .submit-btn {
    min-width: 0; 
  }
}

/* FOOTER CORRECTIVE STYLES */
.x-colophon {
  border-top: 0;
  box-shadow: none;
}

/* WHITE BUTTON USING CS V2 BUTTON */
.btn-white.x-anchor {
    border-width: 2px;
    border-style: solid;
    border-color: #333333;
    font-size: 1em;
    background-color: hsla(0, 0%, 100%, 0);
    border-radius:0;
    box-shadow:none;
}

.btn-white.x-anchor .x-anchor-content {
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-align-items: center;
    align-items: center;
    padding: .8em 2em .8em 2em;
}
.btn-white.x-anchor[class*="active"] {
    border-color: #333333;
    background-color: rgba(255, 255, 255, 1);
}
.btn-white.x-anchor .x-anchor-text {
    margin: 5px;
}
.btn-white.x-anchor .x-anchor-text-primary {
    font-family: inherit;
    font-size: 18px;
    font-style: normal;
    font-weight: 400;
    line-height: 1;
    color: rgba(0, 0, 0, 1);
}
.btn-white.x-anchor[class*="active"] .x-anchor-text-primary {
    color: hsl(0, 0%, 0%);
}

/* BOX PADDING */
/* Boxes (columns) have a standard padding
   adding the class "col-standard-padding" will
   apply these */
.x-column.col-standard-padding{
  padding:60px 85px;
}

/* Padding only for left and right 
   Works for Columns, Rows and Sections
*/
.x-column.side-padding,
.x-container.side-padding,
.x-section.side-padding{
  padding-left:85px;
  padding-right:85px;
}

/* There are instances when an image or another element
   inside the box span through the entire box with zero padding.
   In order to override the container's padding, we add the 
   "no-padding" class to that element.
   
   What it does is it adds negative values for side margins 
   equivalent to the value of the side paddings of the box. 
   This, in effect, negates the padding 
*/
.x-column.col-standard-padding .no-padding,
.x-column.side-padding .no-padding,
.x-container.side-padding .no-padding,
.x-section.side-padding .no-padding{
  margin-left:-85px;
  margin-right:-85px;
}

/* Most boxes has colored backgrounds and white text.
  Adding the class, "text-white" to the box (column)
  makes its texts, those using h3, h4, and p WHITE  
*/
.x-column.text-white h3,
.x-column.text-white h4,
.x-column.text-white p{
  color:white !important;
}
  
.x-column.text-white h3{
  font-size:30px !important;
  line-height:36px !important;
}

@media (max-width:767px){
  /* lessen the box padding on small devices */
  .x-column.col-standard-padding,
  .x-column.side-padding,
  .x-container.side-padding,
  .x-section.side-padding{
    padding:40px 45px;
  }
  
  .x-column.col-standard-padding .no-padding,
  .x-column.side-padding .no-padding,
  .x-container.side-padding .no-padding,
  .x-section.side-padding .no-padding{
    margin-left:-45px;
    margin-right:-45px;
  }
}

/* PRODUCTS */
.products-section{
  margin:0;
  padding:0px 0px 45px 0px;
}

