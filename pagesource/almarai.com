// function for getting cookies by name
function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}

/*  Keep accordion open even after reload */
jQuery(document).ready(function($){
    
    var open_accordions = new Array();
		var open_accords = getCookie('open_accordions');
  
    if( open_accords != '' )
    {
      $.each( open_accords.split(','), function(index, value){
				$('.button-' + value ).addClass('is-open').val("Show Less -");
        $('.rtl .button-' + value ).addClass('is-open').val("عرض أقل -");
      	$('.button-' + value).parent().prev().show();
      });  
    }
  
  
    $('.button-accordion').click(function() {
        open_accordions = new Array();
        list_open();
    });
    
    function list_open()
    {
        var x = 0;

        $('.button-accordion').each(function(index, obj){
            //console.log( $(obj).css('display') );

            if( $(obj).hasClass('is-open') )
            {
                open_accordions[x] = $(obj).attr('id');
                x++;
            }
        });
    
        console.log( open_accordions.join(',') );
        // add to cookie
        document.cookie = 'open_accordions=' + open_accordions.join(',');
    }
});
