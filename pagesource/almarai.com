      // hide the minus icon
      $(this).find('.x-icon-minus').hide();
      
      // show the plus icon
      $(this).find('.x-icon-plus').show();
    }
  });
    /* =======================================
  	 * Hide and display products when a type 
   	 * filter button is clicked 
   	 * =======================================
   	*/
    $('.btn-product-type-filter').click(function(){
      
      // remove "active" class from the active button
      $('.btn-product-type-filter').removeClass('active');
      // add it to the newly clicked button
      $(this).addClass('active');
      
      // hide all col-product boxes
      $('.col-product').css({'opacity': 0, 'position' : 'fixed'})
      // display only those have the product filter class of the clicked button
      // first get the product filter class of the clicked button
      var product_type = $(this).data('product-type');
      $('.col-product.product-type-' + product_type).css({'opacity': 1, 'position' : 'static'});
      
      // reload slider
      //$('.col-product.product-type-' + product_type).find(".x-flexslider").removeData('flexslider');
      //$('.col-product.product-type-' + product_type).find(".x-flexslider").flexslider();
      
      // Set margins
      set_product_list_margins();
      
      
      
    });
  
    /* Set Auto-height for Euroland Iframe */
    $(document).ready(function(){	
      $('.x-iframe').EurolandIFrameAutoHeight();
    }); 

    // Detect when a nav button is clicked.
    // Each time a button is clicked, that and only 
    // that button should have the "active" class.
    // The button that had that class previously should 
    // have it removed.
    $('.nav-button').click(function(){
      $('.nav-button').removeClass('active');
      $('.x-section').removeClass('active');
      
      $(this).addClass('active');
      
      // get index
      var idx = $('.nav-button').index(this);
      $('.section-content:eq(' + idx + ')').addClass('active');
      
    });
  	
  
	$(document).ready(function(){
    
    if( $('.btn-product-type-filter').length > 0 )
    {
    	set_product_list_margins();
    }
    
    // product list page
    if( $('.col-product').length > 0 ){
      $('.col-product').each(function(index, obj){
        if( $(obj).css('opacity') == 0 ){
          $(obj).css('position', 'fixed');
        }
      });
    }
  });
  
  // set the margins
  function set_product_list_margins()
  {
    //alert('sdf');
    var counter = 1;
    //$('.product-list').find('.col-product:visible').each(function( index, obj ){
    $('.product-list').find('.col-product').each(function( index, obj ){
      
      // check if col product is visible
      if( $(obj).css('opacity') == 1 ){
        // remove no-margin-right
        $(obj).removeClass('no-margin-right');

        // if arabic
        if( $('body').hasClass('rtl') )
        {
          if( counter == 1 )
          {
            $(obj).addClass('no-margin-right');
          }

          if( counter == 3 )
          {
            counter = 0;
          }
        }
        else
        {
          // if english
          if( counter % 3 == 0 )
          {
            $(obj).addClass('no-margin-right');
          }
        }

        counter++;
      }
      
    });
  }
  
  
});

