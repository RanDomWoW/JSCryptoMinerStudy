              <div class="desc">Meet other creators and chat about making videos, YouTube, or anything else!</div>
              <div class="link text-teal text-bold text-small">Explore <i class="fm fm-more"></i></div>
            </div>
          </a>
        </div>
      </div>
    </div>
  </section>

  <!-- users -->
  <section class="home-users-1704 bg-pattern bg-pattern-teal text-center">
    <div class="container">
      <h2 class="heading">Member Spotlight</h2>
      <!-- user list -->
      <div class="list-user">
        <div class="row">
          <div class="col col-xs-12 col-lg-6" v-for="item in more.users">
            <div class="user-panel-spot user-panel-spot-1704 bg-white card-shadow" v-if="item.id">
              <div class="row row-compact">
                <div class="col col-xs-5 hide-mobile hide-tablet">
                  <div class="avatar-box-user">
                    <a class="avatar-square bg-cover" :href="item.url" :style="shoBgim(item.avatar_file)"></a>
                  </div>
                </div>
                <div class="col col-xs-12 col-lg-7 bg-smoke">
                  <!-- basic -->
                  <div class="user-spot-basic">
                    <div class="row row-compact">
                      <div class="col col-xs-5 hide-desktop">
                        <a class="avatar-square bg-cover" :href="item.url" :style="shoBgim(item.avatar_file)"></a>
                      </div>
                      <div class="col col-xs-7 col-lg-12">
                        <h4 class="user-channel hide-overflow">{{ item.youtube_channel || item.username }}</h4>
                        <div class="user-subscribe" v-if="item.youtube_channel">
                          <a class="inline-block" :href="item.youtube_url" target="_blank">
                            <span class="text inline-block"><i class="fm fm-youtube"></i> Subscribe</span>
                            <span class="num inline-block">{{ item.youtube_subscribe }}</span>
                          </a>
                        </div>
                      </div>
                    </div>
                  </div>
                  <!-- advanced -->
                  <div class="user-spot-adv">
                    <ul class="user-spot-adv-top clearfix">
                      <li class="with-border">
                        <div class="text">Name</div>
                        <div class="data">{{ item.username }}</div>
                      </li>
                      <li class="with-border">
                        <div class="text">From</div>
                        <div class="data">{{ item.location }}</div>
                      </li>
                      <li class="with-border">
                        <div class="text">Since</div>
                        <div class="data">{{ item.created_at }}</div>
                      </li>
                      <li class="with-border">
                        <div class="text">Points</div>
                        <div class="data">{{ item.point }}</div>
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- blogs -->
  <section class="home-blogs-1704 bg-pattern bg-pattern-smoke">
    <div class="container">
      <h2 class="heading">New in the blog!</h2>
      <div class="list-blog">
        <div class="row">
          <div class="col col-xs-12" :class="index === 0 ? '' : 'col-lg-4'" v-for="(item, index) in more.blogs">
            <div class="item-blog" :class="index === 0 ? 'first-blog' : ''" v-if="item.id">
              <a class="item-cover bg-cover bg-responsive" :class="index === 0 ? 'bg-4x1' : 'bg-16x9'" :href="item.url" :style="shoResc(item.img)"></a>
              <a class="item-title" :href="item.url">{{ item.title }}</a>
              <div class="item-cate" v-html="conCate(item.category_id)"></div>
              <div class="item-info text-gray">
                <span class="author">By {{ item.user.username }}, </span>
                <span class="time">{{ item.add_time }}</span>
                <span class="count">
                  <span class="count-like"><i class="fm fm-like"></i> {{ item.agree_count }}</span>
                  <span class="count-comment"><i class="fm fm-comment"></i> {{ item.comment_count }}</span>
                </span>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="btm-normal text-right text-bold">
        <a class="show-all" :href="conf.base + conf.navi.blog.list">Keep reading <i class="fm fm-more"></i></a>
      </div>
