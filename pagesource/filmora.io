<!-- body -->
<main id="body" class="a-block transition fast unload" data-pro="filmora" data-cat="community" data-nav="site" data-sub="home" data-sys="auto" data-lan="en">
  <!-- top banner -->
  <section class="top-banner top-banner-home-1704 bg-cover text-center text-white" :style="shoBgim(conf.path.imgs + 'home/top-banner-bg.jpg')">
    <div class="container">
      <div class="icon">
        <span class="icon-module icon-module-community" :style="shoBgim(conf.path.imgs + 'home/sprites-module-icon.png')"></span>
      </div>
      <h1 class="heading">Your YouTube Community</h1>
      <div class="desc text-large">A community for creators! All the resources you need to grow your YouTube channel in one place.</div>
      <div class="action">
        <button class="button button-teal" data-toggle="login" data-login="register" data-ga-type="event" data-ga-category="register" data-ga-action="signup-topbanner-join">Join Now</button>
