      <div class="list-forum-home">
        <div class="col col-xs-12 col-lg-4" v-for="item in more.forums">
          <div class="item-forum-home card-shadow text-left" v-if="item.id">
            <div class="item-top"></div>
            <div class="item-mid">
              <a :href="item.url" class="item-title">{{ item.title }}</a>
              <div class="item-info">
                <div class="row">
                  <div class="col col-xs-6">
                    <div class="count text-gray">
                      <span class="inline-block count-view"><i class="fm fm-view"></i> {{ item.view_count }}</span>
                      <span class="inline-block count-like"><i class="fm fm-thumbs-up"></i> {{ item.agree_count }}</span>
                      <span class="inline-block count-comment"><i class="fm fm-comment"></i> {{ item.comment_count }}</span>
                    </div>
                  </div>
                  <div class="col col-xs-6">
                    <div class="item-cate text-right hide-overflow" v-html="conCate(item.category_id)"></div>
                  </div>
                </div>
              </div>
            </div>
            <div class="item-btm card-shadow">
              <a :href="item.url" class="text-center text-large">Comments <i class="fm fm-more"></i></a>
