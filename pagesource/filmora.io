                  <div class="item-title hide-overflow">{{ item.title }}</div>
                  <div class="colm-title">{{ col.title }}</div>
                  <div class="item-desc text-small">{{ item.description }}</div>
                  <div class="action text-small">See Full Series <i class="fm fm-more"></i></div>
                </div>
              </div>
              <div class="item-title hide-overflow" v-else>{{ item.title }}</div>
            </a>
          </div>
        </div>
      </div>
      <div class="action">
        <a class="button button-steel" :href="conf.base + conf.navi.video.list">Start Watching <i class="fm fm-more"></i></a>
      </div>
    </div>
  </section>

