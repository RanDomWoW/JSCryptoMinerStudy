<script>
	var addThisTimer;

	$(document).ready(function() {
		$("#sticker").sticky({topSpacing:0, zIndex: '6'});

		$('.btn-newsletter').click(function() {
			var email = $(this).parents().eq(2).find('input').val();
			var msg;
			var type;

			if(check(email) && email) {
				$.post('../files/ajax/newsletter_subscribe.php', {
					email: email
				}, function(result) {
					if(result == 'success') {
						type = 'success';
						msg = "Votre e-mail a bien été ajouté. Vous recevrez prochainement notre newsletter !";
					} else if(result == 'already-exists') {
						type = 'error';
						msg = "L'adresse indiquée est déjà enregistrée.";
					} else {
						type = 'error';
						msg = "Une erreur est survenue.";
					}

					popup('newsletter-popup', type, msg);
				});
			} else {
				type = 'error';
				msg = "Veuillez entrer une adresse au format email.";
				popup('newsletter-popup', type, msg);
			}
		});

		$('.input-newsletter').keypress(function(e) {
			if(e.which == 13) {
				$('.btn-newsletter').click();
			}
		});

		if(addthis != undefined) {
			addthis.addEventListener('addthis.ready', addthisReady);
		}

		function addthisReady(evt) {
			addThisTimer = setInterval(endLoad, 500);
		} 

		function endLoad() {
			if($(".at-share-btn-elements .at-icon-wrapper svg.at-icon").length >= 1) {
				$(".at-share-btn-elements .at-icon-wrapper.at-share-btn").css({
					"background-color":"#ffffff",
					"border":"solid 1px #202224"
				});

				$(".at-share-btn-elements .at-icon-wrapper svg.at-icon").css({
					"fill": "rgb(32, 34, 36)"
				});

				clearInterval(addThisTimer);
			}
		};
	});
</script> 
