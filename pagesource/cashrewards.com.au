<script>
    (function () {
        var up = $("a#back-to-top"), win = $(window), h = Math.floor(win.height() / 2);
        win.scroll(function () {
            if (win.scrollTop() > h) {
                if (!up.hasClass('visible')) {
                    up.addClass('visible').stop(true, true).fadeIn();
                }
            } else {
                if (up.hasClass('visible')) {
                    up.removeClass('visible').stop(true, true).fadeOut();
                }
            }
        });
        up.click(function () {
            $("html, body").animate({ scrollTop: 0 }, "slow");
            return false;
        });
    })();
</script>
    
