  <script>
  var mapZoom = 14;
  var mapCenter = new google.maps.LatLng(26.097609, 127.676867);
  function mapCreate(){
      var mapOption={
        zoom:mapZoom,
        center:mapCenter,
        scrollwheel: false,
        draggable: false
    };
    var map=new google.maps.Map(document.getElementById('map-canvas'),mapOption);
    markerCreate(map);
  }
  function markerCreate(map){
    new google.maps.Marker({
      position:mapCenter,
      map:map
    });
  }google.maps.event.addDomListener(window, 'load', mapCreate);

   $(function(){

      var setImg = '#viewer';
      var fadeSpeed = 1500;
      var switchDelay = 5000;

      $(setImg).children('.viewer-img').css({opacity:'0'});
      $(setImg + '.viewer-img:first').stop().animate({opacity:'1',zIndex:'20'},fadeSpeed);

      setInterval(function(){
          $(setImg + ' :first-child').animate({opacity:'0'},fadeSpeed).next('.viewer-img').animate({opacity:'1'},fadeSpeed).end().appendTo(setImg);
      },switchDelay);

  });
  $(window).on("load resize",function(){

        var $_windowWidth = $(window).width();

        if($_windowWidth < 992) {
            $(".covervid-video").css({display: "none"})
            $("#viewer").css({display: "block"})
        }else {
            $(".covervid-video").css({display: "block"})
            $("#viewer").css({display: "none"})
        }

    });
</script>

