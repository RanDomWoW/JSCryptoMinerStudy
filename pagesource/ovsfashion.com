<script type="text/javascript">
function validationNewsletterHeader(){
	var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
	if( $(".top-banner-newsletter .checkme").prop("checked") && $(".top-banner-newsletter .newsletteremail").val()!="" && re.test($(".top-banner-newsletter .newsletteremail").val())){
		$(".top-banner-newsletter .sendNews").removeAttr("disabled");
		console.log("VALID");
	} else{
		$(".top-banner-newsletter .sendNews").attr("disabled","disabled");
		console.log("INVALID");
	}
}
$(validationNewsletterHeader);

$(".top-banner-newsletter .checkme").change(validationNewsletterHeader);
$(".top-banner-newsletter .newsletteremail").keyup(validationNewsletterHeader);

$(".top-banner-newsletter .sendNews").on("click",function(){
	console.log("window.commonFunction",window.commonFunction);
	window.commonFunction.subscribe($(".top-banner-newsletter .newsletteremail").val())
	.done(function(response){
		if(response === 'success'){
			$(".top-banner-newsletter .newsletteremail").val(null);
			$(".top-banner-newsletter .checkme").removeAttr('checked');
			$(".top-banner-newsletter .signin").removeClass("active");
			$(".top-banner-newsletter .signed").addClass("active");
		}else if(response === 'lead_registered'){
			$(".top-banner-newsletter .signin").removeClass("active");
			$(".top-banner-newsletter .signed").removeClass("active");
			$(".top-banner-newsletter .error").addClass("active");				
		}
		//else nothing
	});
});
 </script>


