blz.currentUser = (function currentUser() {
  'use strict';

  var headerAccount = document.querySelector('.header-account');
  var me = blz.me || {};

  if (!document.querySelector('.current-username')) {
    return false;
  }

  var currentUsernameProfile = document.querySelectorAll('.profile-greeting-text');
  var headerAccountAvatar = document.querySelector('.header-account-avatar');
  var headerAccountAvatarProfile = document.querySelector('.profile-greeting-pic');
  var currentUser = document.querySelector('.current-user');
  var currentFullName = document.querySelectorAll('.current-fullname');
  var icoAvatar = document.querySelector('.spr-avatar-woman');
  var mobileMenuTablet = document.querySelector('.is-tablet .menu-mobile-item-account');
  var profileLink = document.querySelector('.header-account-login');
  var isLogged = me.familyName || me.gravatar;
  var slicedMail = (me.email || '').slice(0,10) + '...';

  if (isLogged) {
    if (icoAvatar) icoAvatar.className = icoAvatar.className.replace('is-hide', '');
    if (currentUser) currentUser.className += ' is-logged';
    if (currentUsernameProfile) currentUsernameProfile.forEach(function(user) { user.textContent = 'Olá, ' + (me.nickName || me.givenName || me.email || ''); });
    if (me.facebookId) {
      me.gravatar = 'https://graph.facebook.com/' + me.facebookId + '/picture?type=large&width=150&height=150';
    }
    if (headerAccountAvatar) headerAccountAvatar.setAttribute('src', me.gravatar);
    if (headerAccountAvatarProfile) headerAccountAvatarProfile.setAttribute('src', me.gravatar);
    if (profileLink) profileLink.href = '/minha-conta#/meus-pedidos';
    if (currentFullName) currentFullName.forEach(function(user) { user.textContent = (me.givenName || '') + ' ' + (me.familyName || '') || me.nickName || me.email || ''; });

    if (mobileMenuTablet) {
      if (icoAvatar) icoAvatar.className += ' is-hide';
    }

  } else {
    if (icoAvatar) icoAvatar.className = icoAvatar.className.replace('is-hide', '');
    if (currentUser) currentUser.className += ' isnt-logged';
  }

  var currentUsername = document.querySelectorAll('.current-username');
  setCurrentUserName(currentUsername, isLogged);

  function setCurrentUserName(currentUsername, isLogged) {
    if (currentUsername.length) {
      currentUsername.forEach(function(user) {
        if (isLogged) {
          user.textContent = (user.className.indexOf('only-first-name') > -1 ? '' : 'Olá, ') + (me.nickName || me.givenName || slicedMail || '');
        } else {
          user.textContent = (user.className.indexOf('only-first-name') > -1 ? '' : 'Olá! Entrar na');
        }
      });
    }
  }

});

reqAnimFrame(blz.currentUser);

blz.eventListener('load', function() {
  function blzy() {
  var lazy = document.getElementsByClassName('blzy') || [];

  if (!lazy.length) return;

  for (var i = 0; i < lazy.length; i++) {
    var image = lazy[i];
    if (image.getAttribute('data-src')) {
      image.src = image.getAttribute('data-src');
      image.removeAttribute('data-src');
    }
    if (image.getAttribute('data-background')) {
      image.style.backgroundImage = 'url(' + image.getAttribute('data-background') + ')';
      image.removeAttribute('data-background');
    }
  }
}

