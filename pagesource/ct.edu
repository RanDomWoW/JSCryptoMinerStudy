<script type="text/javascript">

$.fn.setAllToMaxHeight = function(){
return this.height( Math.max.apply(this, $.map( this , function(e){ return $(e).height() }) ) );}

$.fn.setAllToMaxHeightPadding = function(){
return this.innerHeight( Math.max.apply(this, $.map( this , function(e){ return $(e).height() }) ) );}
	
$(document).ready(function(){

	$('.footable').footable();

	$(window).on("load resize",function(e){
		if ($(window).width() < 767) {
			$("#bar-links").roller({autoAdvance:true, pagination:false, infinite:true, paged: true, autoWidth: true, autoTime:3000, maxWidth: '767px'});
			}
		else {$("#bar-links").roller("destroy");}
	});

	$(window).load(function() {
		$('#homepage-bar-links').setAllToMaxHeight();
		$('.homepage-spotlight').setAllToMaxHeightPadding();
	});

	$(window).resize(function() {
		$('#homepage-bar-links li').css('height','auto');
		$('#homepage-bar-links li').setAllToMaxHeight();
		$('.homepage-spotlight').css('height','auto');
		$('.homepage-spotlight').setAllToMaxHeight();
	});

	$('#homepage-bar-links li').css('cursor','pointer');
	$("#homepage-bar-links li").click(function(){
		 window.location=$(this).find("a").attr("href"); 
		 return false;
	});
	
	
	$('a.wrapper').each(function() {
	  var bgURL = $(this).find('img').attr("src");
	  $(this).find('img').css('opacity', '0');
	  $(this).css('background-image', 'url(' + bgURL + ')');   
	});

	//$(window).scroll(function() {
		//var scrolled = $(window).scrollTop();
		//$('#header-image').css('background-position-y', (scrolled) + 'px');
	//});

	if($(window).width() > 990){

		var $items = $('#bar-links > div'),
		delay = 3000;
		//$items.first().addClass('grow');

		setInterval(function(){
		  var $current = $items.filter('.grow'),
			  $next = $current.next().length ? $current.next() : $items.first();

		  //$current.removeClass('grow');
		  //$next.addClass('grow');
		}, delay);

	};  

});
</script>
