 var maxValue = this.barsArray[index].maxValue;
 if ( stopScroll == true ) return;

 switch(direction)

 {
  case "up":
    preValue++;
    if ( preValue >= maxValue )
    {
     preValue = 0;
    }
    barID.scrollTop = preValue;
    break;
  case "down":
    preValue--;
    if ( preValue <= 0 )
    {
     preValue = maxValue;
    }
    barID.scrollTop = preValue;
    break;
  case "left":
    preValue++;
    if ( preValue >= maxValue )
    {
     preValue = 0;
    }
    barID.scrollLeft = preValue;
    break;
  case "right":
    preValue--;
    if ( preValue <=0 )
    {
     preValue = maxValue;
    }
    barID.scrollLeft = preValue;
    break;
 }
 this.barsArray[index].preValue = preValue;
}
