/**** Eloqua IP Info Integration ****/
var updateIPInfo = function (fieldName, elqVisitorFieldName) {
	// Check that the function exists
	if (typeof GetElqContentPersonalizationValue == 'function') {
		var elqData = GetElqContentPersonalizationValue(elqVisitorFieldName);
		// cache the hidden form fields since they will be used more than once
		var hiddenFields = jQuery("input[id*=" + fieldName + "]");
		// check that both have some value and store the value to the hidden field so it will be posted on submit
		if (elqData.length > 0 && hiddenFields.length > 0) {
			hiddenFields.attr("value", elqData);
		}
	}
}

// Update the hidden IP info fields with the data from Eloqua.
// Do not change the name of SetElqContent. Eloqua's scripts call it when they finish loading.
function SetElqContent() {
	// Fire off event that Eloqua is done loading.
	var event = document.createEvent('Event');
	event.initEvent('elqReady', true, true);
	document.dispatchEvent(event);

	if (this.GetElqContentPersonalizationValue) {
		updateIPInfo("IPState", "V_ProvinceFromIP");
		updateIPInfo("IPCountry", "V_CountryFromIP");
		updateIPInfo("IPCity", "V_CityFromIP");
		updateIPInfo("IPZip", "V_ZipCodeFromIP");
	}

}

