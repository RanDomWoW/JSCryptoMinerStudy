        jQuery(function () {
            jQuery("#clubModal").appendTo("body");

            jQuery("#clubModal .clubs a").click(function () {
                var invisibleClubInput = jQuery("input[data-id='75967d19-3f60-4852-963f-36be65c7e94a']");
                invisibleClubInput.val(jQuery(this).data("language-invariant-name"))
                    .keyup(); //this triggers the client validation
                jQuery("#selected-club").text(jQuery(this).data("name"));
                // We need to explicitly add 'modal-open' to 'body' if there is any modal open. Otherwise, if there
                // are two open modals and only one is closed, 'modal-open' will be removed from 'body', making the
                // open modal not scrollable. Bootstrap explicitly states that 'Multiple open modals not supported'.
                // Therefore, a workaround from https://stackoverflow.com/questions/19305821/multiple-modals-overlay
                // is needed.
                jQuery('.modal').on('hidden.bs.modal', function (e) {
                    if (jQuery('.modal').hasClass('in')) {
                        jQuery('body').addClass('modal-open');
                    }    
                });
            });

            // Client-side club name validation
            if (jQuery.validator && jQuery.validator.unobtrusive) {
                var validationFunctionName = 'isvalidclubname';

                jQuery.validator.unobtrusive.adapters.addSingleVal(validationFunctionName, 'clubnamesstring');

                jQuery.validator.addMethod(validationFunctionName,
                    function (value, element, clubnamesstring) {
                        var clubNames = clubnamesstring.split(",");
                        return clubNames.indexOf(value) !== -1;
                    });
                jQuery.validator.addClassRules('require-valid-club-name', { 'isvalidclubname': true });
            }
        });
    </script>
