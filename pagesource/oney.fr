	<script>
    var resizeTimer;




    $(document).ready(function() {

        var optionsDesktop = {
            separation: 150,
            forcedImageWidth: 260,
            forcedImageHeight: 165,
            flankingItems: 1,
            clickedCenter: function($clickedItem) {
		      // $clickedItem is a jQuery wrapped object describing the image that was clicked.
		      var imageUrl = $clickedItem.attr('data-url');
		     window.open(imageUrl,"_top");
		    }
        };

        var optionsMobile = jQuery.extend({}, optionsDesktop);
        optionsMobile.forcedImageWidth = 180;
        optionsMobile.forcedImageHeight = 114;
        optionsMobile.separation = 50;
        

        if ($(window).width() < 768) {


            var carousel = $("#carrousel-cartes").waterwheelCarousel(optionsMobile);
        } else {
            var carousel = $("#carrousel-cartes").waterwheelCarousel(optionsDesktop);
        }

        $(window).on('resize', function(e) {

            clearTimeout(resizeTimer);
            resizeTimer = setTimeout(function() {

                if ($(window).width() < 768) {

                    carousel.reload(optionsMobile);

                } else {

                    carousel.reload(optionsDesktop);
                }

            }, 500);

        });




        $('#prev').bind('click', function() {
            carousel.prev();
            return false
        });

        $('#next').bind('click', function() {
            carousel.next();
            return false;
        });

    });
</script>
