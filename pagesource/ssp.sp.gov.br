	<script type="text/javascript">
		//Coloca mascara para o campo
		//campo - id do campo
		//tipo_mascara - tipo da mascara a ser aplicada (cpf, data, ano, idade);
		function mascara(campo, tipo_mascara) {
			var mascara = "";
			switch (tipo_mascara) {
				case "cpf":
					mascara = "999.999.999-99";
					break;
				case "data":
					mascara = "99/99/9999";
					break;
				case "hora":
					mascara = "99:99";
					break;
				case "ano":
					mascara = "9999";
					break;
				case "cnpj":
					mascara = "99.999.999/9999-99";
					break;
				case "telefone":
					mascara = "(99)9999-9999";
					break;
				case "celular":
					mascara = "(99)99999999?9";
					break;
				case "cep":
					mascara = "99999-999";
					break;
			}

			$(campo).mask(mascara,
				{ placeholder: " " }
			);
		}

	</script>

