	<section ng-controller="ctrlDefault">
		<div class="container-fluid noPadding" ng-if="ListaFinalizada">
			<div class="col-lg-12" ng-repeat="noticia in noticias">
				<!--Even Hidden-sm e Hidden-xs-->
				<div style="padding-bottom: 25px; padding-top: 25px;" ng-if="$even" class="hidden-sm hidden-xs">
					<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
						<div class="not_destaque">
							<div class="not_destaque">
								<a ng-click="AbrirNoticia(noticia.ID)">
									<img ng-if="noticia.Album != null" class="img-responsive img-center img-responsive not_chp1_img" ng-src="{{noticia.Album.ImagemMachete.Link}}" style="width: 95%;" />
									<img ng-if="noticia.Album == null" class="img-responsive img-center img-responsive not_chp1_img" ng-src="img/padrao1.png" style="width: 95%;" />
								</a>
							</div>
						</div>
					</div>


					<a ng-click="AbrirNoticia(noticia.ID)">
						<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
							<div><span class="chapeu2 leftPad rightPad" ng-bind="noticia.Chapeu.Nome"></span></div>
							<div class="leftPad rightPad"><span class="tituloazul" ng-bind="noticia.Titulo"></span></div>
							<br />
							<div class="leftPad rightPad"><span style="font-weight: bold; color:black;" ng-bind="noticia.Subtitulo"></span></div>
