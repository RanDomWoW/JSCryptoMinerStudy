<!-- flipclock PST converter -->
<script type="text/javascript">
  //<![CDATA[ 
  var clock;
  var clockf;
  function calcTime(){
    var d = new Date();
    var localTime = d.getTime();
    var localOffset = d.getTimezoneOffset() * 60000;
    var utc = localTime + localOffset;
    var offset = -7;   
    var la = utc + (3600000*offset);
    var nd = new Date(la); 


    var h = nd.getHours();
    var m = nd.getMinutes();
    var s = nd.getSeconds();
    var d = nd.getDay();
    var ch=0;
    var cm=59;
    var cs=59;

    if (h >= 15 ){ // At or After 3PM - Counts to 8AM next day
      ch=8+(23-h);
      if(d==5){ch=ch+24+24;} // If Friday 
      else if(d==6){ch=ch+24;} // If Saturday 
    }
    else if (h >= 12 && h < 15){ // After 12AM before 3PM - counts down to 3PM
      ch=14-h
      if(d==0){ch=ch+24-4;}  //If Sunday - counts down to 8AM Monday
      else if(d==6){ch=ch+24+24-4;} // If Saturday - counts down to 8AM Monday
    }
    else if (h >= 8 && h <12){ // After 8AM before 12PM - counts down to 12pm
      ch=11-h
      if(d==0){ch=ch+24-2;} //If Sunday - counts down to 8AM Monday
      else if(d==6){ch=ch+24+24-2;} // If Saturday - counts down to 8AM Monday
    }
    else{ // Before 8AM - counts down to 8AM 
      ch=7-h;
      if(d==0){ch=ch+24;}  //If Sunday
      else if(d==6){ch=ch+24+24;} //If Saturday
    }
    ch=ch*60*60;
    cm=(cm-m)*60;
    cs=cs-s;
    var ret = ch+cm+cs;
    return ret;
  }
  $(document).ready(function() {	
    clock = $('.clock').FlipClock(calcTime(), {
      clockFace: 'HourlyCounter',
      countdown: true,
      callbacks: {
        stop: function() {
          setTimeout(function(){
            clock.setTime(calcTime());
            clock.start();
          },5000);
        }
      }
    });
    clockf = $('.clockf').FlipClock(calcTime(), {
      clockFace: 'HourlyCounter',
      countdown: true,
      callbacks: {
        stop: function() {
          setTimeout(function(){
            clock.setTime(calcTime());
            clock.start();
          },5000);
        }
      }
    });
  });
  $(window).scroll(function() {
    if ($(this).scrollTop()>50)
     { $('.hp-clock-wrapper').fadeOut(); }
    else
     { $('.hp-clock-wrapper').fadeIn(); }
 });
  //]]>
</script>

