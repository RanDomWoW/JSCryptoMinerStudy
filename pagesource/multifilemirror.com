<script type='text/javascript'>
	var upload_id = '152149278470911622';
	var ext_allowed='';
	var ext_not_allowed='';
	var max_upload_files='1';
	var max_upload_filesize='';
	var descr_mode='1';
	var public_on = '';
	var lang_description='Description:';
	var lang_published='';

	jQuery.support.cors = true;
	function check_sites_limit(id) {
		
		var selected_sites = 0;
		$('[name="site"]').each(function() {
			if($(this).prop('checked'))selected_sites++;
		});
		if(selected_sites > 20) {
			document.getElementById(id).checked=false;
                        Materialize.toast('Please select a maximum of 20 sites', 4000);
			return false;
		}
		
		return false;
	}
	var go_t = 1;
	function StartUpload(form, c) {
		
		var selected_sites = 0;
		$('[name="site"]').each(function() {if($(this).prop('checked'))selected_sites++;});
		if(selected_sites > 20) {
			alert('Selected too many sites for upload. Please uncheck some sites.');
			return false;
		}
		
		if(c) {
			var cf = checkForm(form);
			if(!cf) return cf;
			
		} else {
			if(countLines($('#links').get()[0]) > 1) {
				alert('Please enter not more 1 links!'+countLines($('#links').get()[0]));
				return false;
			}
			if(countLines($('#links').get()[0]) <=0) {
				alert('Please enter at least one link!');
				return false;
			}

		}
		if($('#remote').val()==1 ) {
			$('#sdownload').show();
		}

		$('#uploadbtn').prop("disabled", true);
		$('#filebtn').prop("disabled", true);
		$('#remotebtn').prop("disabled", true);
		$('#remoteupload').hide();
		$('#fileupload').hide();
		$('#bar1').show();
		setTimeout(UpdateStat, 2500);
	}
	function update_stat(json) {
		var a = json
					if(a.error) {
						$('#error_msg').html(a.error);
						$('#uploadbtn').prop("disabled", false);
						$('#filebtn').prop("disabled", false);
						if($('#remote').val()==1 ) {
							$('#remoteupload').show();
							$('#sdownload').hide();
						} else {
							$('#fileupload').show();
						}
						$('#bar1').hide();
						$('#speed').html("");
						$('#eta').html("");
						go_t = 0;
						return ;
					}
					if(a.code && a.percent>=99) {document.location='https://multifilemirror.com/result/'+a.code;}
					$('#bar2').width(a.percent+'%');
					$('#bar3').html(a.percent+'%');
					if(a.speed) {
						$('#speed').html(a.speed);
						$('#eta').html(a.eta+' left');
					}
					if(a.percent>0) $('#sdownload').hide();
	}
	function UpdateStat() {
		var jqxhr = $.ajax(($('#remote').val()==1?'https://multifilemirror.com/rstat/'+upload_id:'https://multifilemirror.com/tmp/'+upload_id+'.json')+'?callback=?&rand='+Math.random()*100, function(json) {
			var js = eval(json);
			update_stat(js);
			if(go_t == 1) setTimeout(UpdateStat, 1000);
		}).error(function(e,jx) {
			setTimeout(UpdateStat, 1500);
		}).done(function(data) {
			var js = eval(data);
			update_stat(js);
			if(go_t == 1) setTimeout(UpdateStat, 1000);
		});
	}
	var files = 0;
	function changeUpload(utype) {
		if(utype == 'remote') {
			$('#fileupload').hide();
			$('#remoteupload').show();
			$('#remotebtn').addClass('active');
			$('#filebtn').removeClass('active');
			$('#remote').val(1);
		}
		if(utype == 'file') {
				$('#fileupload').show();
			$('#remoteupload').hide();
			$('#filebtn').addClass('active');
			$('#remotebtn').removeClass('active');
			$('#remote').val(0);
		}
	}
	function StartUpload0(el) {
		var ids = new Array()
		var sites = $('input:checked');
		$(sites).each(function(){ids.push($(this).val())});
		setCookie('sites',ids.toString(), 100000, '.');
		return StartUpload(el, 1-$('#remote').val());

	}
</script>

