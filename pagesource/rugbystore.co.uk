<script>
    function bootstrapMenuHelper() {
        var menuContainer = $("#navbar");
        var menu = menuContainer.children("ul").first();

        if (menu) {
            menu.removeClass();
            menu.addClass("nav navbar-nav");

            menu.children("li").each(function(item, value) {
                if ($(value).has("ul").length > 0) {
                    addDropDownLink(value,0);
                }
            });

            

        }
        $("#navbar").removeClass("fallback");

    }

    function addDropDownLink(item, level) {


        var li = $(item);
        var a = li.children("a").first();
        var el = $('<a />', { "class": 'dropdown-toggle expandButton' })
            .attr({
                "role": "button",
                "aria-haspopup": "true",
                "aria-expanded": "false"
            });

        if (level === 0) {
            el.attr("data-toggle", "dropdown");
        }
        a.after(el);

        var menu = li.children("ul");
        menu.children("li").each(function(item, value) {
            if ($(value).has("ul").length > 0) {
                addDropDownLink(value, 1);
            }
        });
        menu.addClass("dropdown-menu");
        menu.children('span').remove();
        if (level === 0){
            a.next(".expandButton").dropdown();
        }

    }

    
    $(document).ready(function() {
        bootstrapMenuHelper();
        

        /* ensure opening a menu closes the other */
        var mainId = "navbar", searchId = "navbar-search";
        var mainMenu = $("#mobile-main-menu");
        var searchMenu = $("#mobile-search-menu");
        var mainContainer = $("#"+mainId);
        var searchContainer = $("#"+searchId);

        mainMenu.click(function() {
            searchContainer.collapse('hide');
        });
        
        searchMenu.click(function () {
            mainContainer.collapse('hide');
        });

        
        $('.dropdown-menu').on("click", function(e) {
            e.stopPropagation();
        });
        $('.dropdown-menu a.expandButton').on("click", function (e) {
            var el = $(this);
            var li = el.parent("li");
            
            if (li.hasClass("open")) {
                li.removeClass("open");
                el.attr("aria-expanded", false); // used for screen readers
            } else {
                li.addClass("open");
                el.attr("aria-expanded", true); // used for screen readers
            }
            //close others
            li.siblings("li").removeClass("open");
            li.siblings("li").children("a.expandButton").attr("aria-expanded", false);
            e.stopPropagation();
            e.preventDefault();
        });

        /* close menu when click outside*/
        var docMoved = false;
        $(document)
            .on('touchstart', function () {
                docMoved = false;
            })
            .on('touchmove', function () {
                docMoved = true
            })
            .on('touchend', function (e) {
                if (!docMoved) {
                    closeMenuCheck(e)
                }
            });
        $(document).on('click', function (e) {
            closeMenuCheck(e);
        });


        function closeMenuCheck(event) {
            var clickover = $(event.target);
            var mainOpen = mainContainer.hasClass("in");
            var searchOpen = searchContainer.hasClass("in");
            var insideMain = clickover.parents("#" + mainId).length || event.target.id == mainId;
            var insideSearch = clickover.parents("#" + searchId).length || event.target.id == searchId;
            var outside = !(clickover.hasClass("navbar-toggle") || (insideMain || insideSearch));
            if ((mainOpen || searchOpen) && outside) {
                event.preventDefault();
                mainContainer.collapse('hide');
                searchContainer.collapse('hide');
            }
        }

        /*
        $(document).click(function (event) {
            
            var clickover = $(event.target);
            var mainOpen = mainContainer.hasClass("in");
            var searchOpen = searchContainer.hasClass("in");
            var insideMain = clickover.parents("#"+mainId).length || event.target.id == mainId;
            var insideSearch =  clickover.parents("#"+searchId).length || event.target.id == searchId;
            var outside = !(clickover.hasClass("navbar-toggle") ||  (insideMain || insideSearch));
            if ((mainOpen || searchOpen) && outside) {
                event.preventDefault();
                mainContainer.collapse('hide');
                searchContainer.collapse('hide');
            }
        });
        */

    });
</script>  

