var bannerkids = $(".bannerslider").children("[data-banneritem]");
var bannerlength = (bannerkids.length);
var bannerlengthrandom = (bannerkids.length) - 2;
var randomnumer = Math.floor(Math.random() * bannerlengthrandom) + 1;

bannerkids.each(function(index, value){ 
    $(this).attr("id", "banner-" + (index+1));
    var titlebtn  = $(this).find(".j-infotext").html();
    $("#btn-" + (index+1)).find(".titlebtn").text(titlebtn);
    $("#btn-" + (index+1)).addClass("show").removeClass("hide");
})

$("#banner-" + randomnumer).addClass("show").removeClass("hide");
$("#btn-" + randomnumer).removeClass("btn-deselect").addClass("btn-select");

$(document).ready(function(){
    for(var i = 1; i<=bannerlength; i++){
        $("#btn-" + i).addClass("btnumber-" + bannerlength);
        $("#btn-" + i).on("click", function() {
            var id = $(this).attr("id").split("btn-")[1];
            $(this).removeClass("btn-deselect").addClass("btn-select");
            $(this).siblings().removeClass("btn-select").addClass("btn-deselect");
            $("#banner-" + id).addClass("show").removeClass("hide");
            $("#banner-" + id).siblings().addClass("hide").removeClass("show");
        })
    }
})
</script>

