    <div class="sta-flight-deals" ng-controller="homepageDealsListCtrl" ng-init="currency='AUD'" >
        <header>
            <p class="sta-heading-m">Our best flight deals</p> 
            <ul class="sta-tabs">
                <li class="active first-tab">All Flights</li>
                <li class="second-tab">Student &amp; youth flights</li>
                <div class="sta-switch"></div>
            </ul>
        </header>
        <div class="sta-slider-wrapper sta-slider-wrapper_first" ng-init="init(deals_list_tooltips)" ng-cloak>
            <div class="swiper-container swiper-container_desktop">
                <div class="swiper-wrapper">
                    <div class="swiper-slide" ng-repeat="(i, slider) in faresAll.content" >
                        <div class="sta-flight-ticket" ng-repeat="(j, deal) in slider" ng-class="deal.isLoading?'flight-ticket_loading':(deal.multiflex==1?'sta-flight-ticket_blue-ticket':'')">
                            <div class="sta-flight-ticket__shadow-wrapp"></div>
                            <span class="sta-flight-ticket__rounds"></span>
                            <div class="sta-flight-ticket__shadow-wrapp"></div>
                            <div class="sta-flight-ticket__preloader" ng-if="deal.isLoading">
                                <div class="sta-flight-ticket__arc"></div>
                            </div>
                            <div class="sta-flight-content" data-fare-id="{{deal.id}}" data-top-offers="{{deal.topOfferLink}}" ng-if="!deal.isLoading">
                                <div ng-if="deal.multiflex==1" class="sta-flight-content__blue-header">STA Travel's BlueTickets</div>
                                <img ng-if="deal.logoSrc" ng-src="{{deal.logoSrc}}" alt="{{deal.airline}}" class="sta-flight-content__img">
                                <div class="sta-flight-content__data">
                                    <div class="sta-flight-content__row">
                                        <div class="sta-flight-content__title">
                                            <p> <strong ng-bind="deal.destinationFrom"></strong> (<span ng-bind="deal.destinationFromIATACode"></span>)</p>
                                            <p> <strong ng-bind="deal.destinationTo"></strong> (<span ng-bind="deal.destinationToIATACode"></span>)</p>
                                        </div>
                                        <div class="sta-flight-content__price">
                                            <span class="sta-flight-content__hint">{{deal.priceType == "return" ? 'Return': 'One way' }} from</span>
                                            <p class="sta-flight-content__price__value">
                                                <span>AUD</span>
                                                <strong ng-bind="deal.price"></strong>
                                            </p>
                                        </div>
                                    </div>
                                    <div class="sta-flight-content__tooltips">
                                      <div class="sta-flight-content__tooltip-wrapper" ng-if="deal.callToAction == 'phone'">
                                          <img ng-if="deals_list_tooltips.hp_tooltips[0].image" alt="{{deals_list_tooltips.hp_tooltips[0].popUpTitle}}" ng-src="{{deals_list_tooltips.hp_tooltips[0].image}}">
                                          <p class="sta-flight-content__tooltip sta-{{deals_list_tooltips.hp_tooltips[0].code}}">{{deals_list_tooltips.hp_tooltips[0].popUpTitle}}</p>
                                      </div>
                                      <div class="sta-flight-content__tooltip-wrapper sta-flight-content__tooltip-wrapper_blue" ng-if="deal.multiflex == 1" data-type="BLT" data-lightBoxTitle="BLUE TICKET">
                                          <img src="/static/au_division_web_live/assets/BT2.png">
                                          <p class="sta-flight-content__tooltip sta-BLT">BlueTicket</p>
                                      </div>
                                      <div class="sta-flight-content__tooltip-wrapper" ng-repeat="tooltip in deals_list_tooltips.hp_tooltips" ng-if="deal.tooltips.indexOf(tooltip.code) >= 0">
                                        <img ng-if="tooltip.image" alt="{{tooltip.popUpTitle}}" ng-src="{{tooltip.image}}" />
                                        <p class="sta-flight-content__tooltip sta-{{tooltip.code}}">{{tooltip.popUpTitle}}</p>
                                      </div>
                                    </div>
                                    <div class="sta-flight-content__row">
                                        <div class="sta-flight-content__dates">
                                            <span ng-if="deal.importType != 2">Travel between <span ng-bind="deal.travelFrom"> </span> - <span ng-bind="deal.travelTo"> </span></span>
                                            <strong ng-if="deal.importType != 2">Book by <span ng-bind="deal.bookBy"> </span></strong>
                                            <span ng-if="deal.importType == 2">Recently found</span>
                                        </div>
                                        <div class="sta-flight-content__hint sta-flight-content__hint_tax">
                                            <span>Per person incl.</span>
                                            <span>taxes & fees</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="swiper-container swiper-container_mobile">
                <div class="swiper-wrapper">
                    <div class="swiper-slide" ng-repeat="(i, slider) in faresAllMobile.content" >
