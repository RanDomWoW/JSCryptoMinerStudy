<script type="text/javascript" src="https://www.armor-vacances.com/form/jquery.validate.min.js"></script>

<script type="text/javascript">
$(document).ready(function() {
        var cgre = "Champ requis"; //define alternate error message for required fields

    $("#requestform").validate({
			ignore: ".ignore",
	        debug: false,
	        focusInvalid: false,
	        onfocusout: false,
	        onkeyup: false,
	        onclick: false,
            rules: {


				author: {
					required: true
				},
				email: {
					required: true
				},
				desc: {
					required: true
				}

			},
			messages: {
				author: {
					required: cgre
				},
				email: {
					required: cgre
				},

				desc: {
					required: cgre //display cgre error message is 'required' validation fails
				}

            },
            highlight: function(element) {
                $(element).css({
                    "border": "2px solid #CC0000"});
            },
            unhighlight: function(element){
                    $(element).css({
                        "border": "2px solid #390"});
             },
            submitHandler: function() {
                //submit the form
                $.post("https://www.armor-vacances.com/holiday/properties/send_request", //post
                    $("#requestform").serialize(),
                    function(data){


                        if (data.message == 'success') {
								$('#alertBoxSuccess2').show();
								$('#alertBoxSuccess2').html("<p>Votre demande a été envoyée avec succès ! N'oubliez de répondre auxpropriétaires.</p>");
							 	setTimeout(function(){ $('#alertBoxSuccess').fadeOut('slow'); 
								}, 3000);
                          return false;
                        }else{
							$('#alertBoxError2').show();
							$('#alertBoxError2').html("<p>Votre demande n'a pas été envoyé  ! Veuillez essayer ultérieurement.</p>");
						 	setTimeout(function(){ $('#alertBoxError').fadeOut('slow'); 
							}, 3000);
                            return  false;
                		}



                    },"json");
                    return false; //don't let the page refresh on submit.*/
                }
    }); //validate the form

	$('#submitButton').click(function() {
		$('#requestform').submit();
	});

});
</script>
