		function response() {
			var name = $("[name='name']").val();
			var phone = $("[name='phone']").val();
			var email = $("[name='email']").val();
			var title = $("[name='title']").val();
			var message = $("[name='message']").val();

			if (name == '') {
				alert('请输入您的姓名，以便于联系，谢谢！');
				return;
			}

			if (phone == '' && email == '') {
				alert('请您至少输入一种联系方式(邮件或者电话)，谢谢！');
				return;
			}

			if (title == '' && message == '') {
				alert('请您输入信息内容再提交，谢谢！');
				return;
			}
			$.ajax({
				url : '../sboxmanage/sendContact',
				dataType : 'json',
				type : 'post',
				data : {
					'name' : name,
					'phone' : phone,
					'email' : email,
					'title' : title,
					'message' : message

				},
				success : function() {
