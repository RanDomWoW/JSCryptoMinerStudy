				$('button').button();
				
				// context path ok?
				$.cookie('oneOnOne.rid', '', {path: '/', expires: new Date('January 01, 1970 00:00:00')});
				$.cookie('oneOnOne.sid', '', {path: '/', expires: new Date('January 01, 1970 00:00:00')});
