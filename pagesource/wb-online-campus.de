					// When the appcache is populated with the offline fallback site, then we add an according link to the footer.
					$('#appCacheCheck')
						.load(function() {
							
							var $iframe = $('#appCacheCheck').contents();
							
							//alert('Iframe loaded');
							if ($iframe.find('#offlineAccordion').size()) {
								//alert('Offline page is available. Will add Offline-Resources link to footer.');
								$('.wl-page-footer')
									.append('<ul class="third"><li><a title="Reisemodus" href="/offline">Reisemodus</a></li></ul>')
									.find('ul')
										.css('width', '33%')
										.end()
									.find('.third')
										.css('text-align', 'center');
							} else {
								//alert('Offline page is NOT available!');
							}
							
							// Remove the iframe after the test.
							// Seems to make problems in Safari since Version 10. ksr/20.10.2016
							//$('#appCacheCheck').remove();
						});

					/*
					 * Because of Safari crash problems with iOS 9.3 and Safari, we have to set the src-Attribute
					 * of the offline-Page after the page was loaded. Unfortunately we cannot access the offline-page
					 * over an AJAX-Request. So we have to use the iframe-Element. ksr/24.03.2016
					 */
					// Seems to be unnecessary for Safari since Version 10. Maybe the remove above was also the problem in iOS 9.3. ksr/20.10.2016
					//setTimeout(function() {
					//	$('#appCacheCheck').attr('src', '/offline');
					//}, 0);

				
				
				
				
				
			});
