  <script type="text/javascript">
  
    //---- Send Distributor Request From Mobile
    function DistributorRequestCall()
    {
        var name = $("#DistReqUserName").val();
        var email = $("#DistReqUserEmail").val();
        var contact = $("#DistReqUserPhone").val();
        
        if(name=='' || email=='' || contact=='')
        {
            $('#DistReqError2').html('All fields are required.');
            return false;
        }
        
        var datatransaction =  { name:name,email:email,contact:contact};    
        $.ajax({
            type: "POST",
            url: "MobileDistRequest",
            data: datatransaction,
            success: function(data)
            {
                $('#DistributorForm1')[0].reset();
                $('#DistReqError2').hide();
                $('#DistMsg2').html('Your request submitted successfully. We will get back to you soon.!');
            }
        });
    }
    
