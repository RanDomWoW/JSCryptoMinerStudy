    function submitdata()
    {
        var name = $("#Name").val();
        var email = $("#Email").val();
        var contact = $("#Phone").val();
        var subject = '';
        var message = $("#Message").val();
        
        if(name=='' || email=='' || contact=='' || message=='')
        {
            $('#UserError').html('All fields are required.');
            return false;
        }
        
        var datatransaction =  { name:name,email:email,contact:contact,message:message,subject:subject};    
    
        $.ajax({
          type: "POST",
          url: "indexcontactusform",
          data: datatransaction,
          success: function(data){
              $('#UserError').hide();
              $('#contact-form')[0].reset();
