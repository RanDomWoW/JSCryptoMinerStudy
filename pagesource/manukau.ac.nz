<script>
function validateImageURL(url,callback){
    var img=new Image();
    img.onload=function(){callback(true);}
    img.onerror=function(){callback(false);}
    img.src=url;
}
if($('.carousel>figure').length<2)$('.carousel>button').remove();
$('#home-banner .carousel>figure img').each(function(){
    var $this=$(this);
    validateImageURL($this.attr('src'), function(valid){
        if(valid)console.log($this.attr('data-pos')+' loaded.');
        else{
            console.log($this.attr('data-pos')+' not loaded.');
            var $slide=$this.closest('figure');
            if($slide.hasClass('carousel-active')){$slide.removeClass('carousel-active');$slide.siblings().first().addClass('carousel-active');}
            $slide.remove();
            if($('.carousel>figure').length<2)$('.carousel>button').remove();
        }
    });
});
</script>
