function M(o){
  for(var i=0;i<o.length;i++){
    o[i].onmouseover=function(){
	  var p=this.parentNode.children,pp=this.parentNode.parentNode.children;
	  for(var j=0;j<p.length;j++){
		if(p[j]==this){
		   p[j].className="s"+(j+1).toString()+" now";
		   pp[j+1].style.display="block";
		}else{
	    	p[j].className="s"+(j+1).toString();
			pp[j+1].style.display="none";
		}
	  }
	}
  }
}


