<style>
body{overflow-x:hidden}
.srx-section-2{padding-bottom:18px;width:100%;background:#f4f4f4;float:left;text-align:center}
.srx-section-inner{width:100%;margin:0 auto;text-align:center;display:flex}
.srx-section-4 h1{text-align:center;margin:0 auto;font-size:42px;color:#3B51A3;padding:20px;font-weight:500}
.srx-img img{
    width: 100%;
}
.srx-img{
    height: 197px;
    display: flex;
    align-items: center;
    width: 16.8%;
    float: left;
    padding: 1%;
}
.srx-section-3{
    background: #fff !important;    
    margin: 0 auto;
    width: 100%;
    max-width: 1180px;
}
.srx-section-3 .srx-section-3-inner {
    float: left;
    padding: 30px 0 !important;
}
.srx-content{
    width: 30%;
    float: left;
    margin: 0 15px;
}
.srx-section-4{background:#F4F4F4;width:100%;float:left;padding:0 0 25px 0; text-align:center;}
.srx-four-img img{width:100%}
.srx-four-img{text-align:center;width:16%;padding:2%;display:flex;align-items:center;float:left;height:190px}
.srx-section-5 {
    float: left;
    margin: 0 auto;
    width: 100%;    
}
.srx-section-5 .srx-section-5-inner {    
    margin: 0 auto;
    max-width: 1180px;
    padding: 0px 0px 36px;
}
.srx-section-5 p{
    width: 100%;
    max-width: 600px;
    margin: 0 auto;
}
.srx-section-5 h1{
    text-align: center;
    margin: 0 auto;
    font-size: 42px;
    color: #000;
    padding: 25px 0 37px 0;
    width: 100%;
}
.srx-section-6 {
    background: #F4F4F4;
    float: left;
    margin: 0 auto;
    width: 100%;    
}
.srx-section-6 .srx-section-6-inner {    
    margin: 0 auto;
    max-width: 1180px;
}
.srx-right{padding:0 10px;width:50%
}
.srx-left{padding:0 10px;width:50%}
.srx-right img,
.srx-left img{width:100%}
.srx-section-7 { 
    float: left;
    margin: 0 auto;
    text-align: center;
    width: 100%;
}
.srx-section-7 .srx-section-7-inner {    
    margin: 0 auto;
    padding: 30px 0 0 0;
    max-width: 1180px;
}
.srx-section-7 h1{
    font-size:34px;
}
.srx-section-inner-6{width:95%;margin:0 auto;display:flex;padding:35px 0}
.srx-section-inner-6 h1{font-size:2.1vw;color:#3b51a3;padding:0 0 20px 0}
.srx-section-inner-6 p{font-size:22px}
.srx-section-inner-6 ul{font-size:22px;margin-top:10px}
.srx-section-inner-6 ul li{margin-top:5px}
.srx-content div img{width:100%;max-width:300px}
.srx-content div{height:115px;max-height:300px !important;vertical-align:middle;text-align:center;align-items:center;display:flex}
.srx-section-2 h1{font-size:42px;text-align:center;padding:16px 0 14px 0;color:#3b51a3}
@media(max-width:930px){.srx-content{width:28%}}
@media(max-width:767px){
    .srx-section-outer-image{display:inline-block;width:31%;text-align:center !important}
    .srx-img img{width:84%;max-height:100%;margin:0 auto}
    .srx-four-img{width:82%;padding:3%}
