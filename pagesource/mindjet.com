				jQuery('.popup-link-parallel').hide();
			});
			/* popup jquery */
			var b_i = l_i = 1;
			/*parallel button first */
            jQuery('.popup-btn-parallel').each(function(){
                jQuery(this).addClass('popup-btn-parallel_'+b_i);
                b_i++;
           	});
           	/*parallel link first */
            jQuery('.popup-link-parallel').each(function(){
                jQuery(this).addClass('popup-link-parallel_'+l_i);
                l_i++;
           	});
		});
	</script>






