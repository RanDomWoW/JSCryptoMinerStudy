    <script type="text/javascript">
        jQuery(document).ready(function() {
            get_max_content_height();
            jQuery(window).resize(function() {
                get_max_content_height();
            });

            function get_max_content_height() {

                var arr1 = []; //array declaration
                var arr2 = []; //array declaration
                var arr3 = []; //array declaration

                jQuery(".ru-homepage-h").each(function() {
                    if (parseInt(jQuery(this).height()) > 0) {
                        arr1.push(jQuery(this).height()); // array push
                    }
                });

                jQuery(".max_height_c p").each(function() {
                    if (parseInt(jQuery(this).height()) > 0) {
                        arr2.push(jQuery(this).height()); // array push
                    }
                });

                jQuery(".max_height_c1 p").each(function() {
                    if (parseInt(jQuery(this).height()) > 0) {
                        arr3.push(jQuery(this).height()); // array push
                    }
                });

                jQuery(".ru-homepage-h").height(Math.max(...arr1)); //spread operator
                jQuery(".max_height_c p").height(Math.max(...arr2)); //spread operator
                jQuery(".max_height_c1 p").height(Math.max(...arr3)); //spread operator

            }
        });

</script>
