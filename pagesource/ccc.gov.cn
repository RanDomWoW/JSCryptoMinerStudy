        document.getElementById("ctl00_themestyle").href = "/Widgets/Themes/theme/Style_blue.css";
        setInterval("timeer()", 1000);
        //setInterval("changeBg()", 30000);
        $('#btn_search').click(function () {
            var searchValue = $.trim($('#text_search').val());
            var regu = "[`~!@#$^&*()=|{}':;',\\[\\].<>/?~%！@#￥……&*（）——|{}【】‘；：”“'。，、？]";
            var re = new RegExp(regu);
            if (re.test(searchValue)===false) {
                searchValue = escape(searchValue);
                window.open("Search.aspx?title=" + searchValue + "&content=" + searchValue);//
            } else {
                alert("不允许输入特殊字符");
                return;
            }
        });

if (IsiPad() === false)
        {
            $("#objBanna").css("display", "none")
        }
    });

function IsiPad() {
        var userAgentInfo = navigator.userAgent;
        var Agents = ["iPad"];
        var flag = true;
        for (var v = 0; v < Agents.length; v++) {
            if (userAgentInfo.indexOf(Agents[v]) > 0) {
                flag = false;
                break;
            }
        }
        return flag;
    }

    function IsPC() {
        var userAgentInfo = navigator.userAgent;
        var Agents = ["Android", "iPhone",
                    "SymbianOS", "Windows Phone",
                    "iPad", "iPod"];
        var flag = true;
        for (var v = 0; v < Agents.length; v++) {
            if (userAgentInfo.indexOf(Agents[v]) > 0) {
                flag = false;
                break;
            }
        }
        return flag;
    }

    function timeer() {
        var date = new Date();
        var datastring = date.getFullYear() + "年" + (date.getMonth() + 1) + "月" + date.getDate() + "日" + date.getHours() + ":" + date.getMinutes() + ":" + date.getSeconds();
        jQuery("#timeshow").text(datastring + '   星期' + '日一二三四五六'.charAt(date.getDay()));
    }

    var currentIndex = 0;
    function changeBg() {
        //定义要切换的背景图片，双引号里面，可以放任意多个
        var bgImgs = ["/Widgets/Themes/theme/images/banner_1.png", "/Widgets/Themes/theme/images/banner_1_red.png", "/Widgets/Themes/theme/images/banner_1_write.png"];
        if (currentIndex >= bgImgs.length)
            currentIndex = 0;
        var obj = document.getElementById["td_Title"];
        obj.style.backgroundImage = "url(" + bgImgs[currentIndex] + ")";
        currentIndex += 1;
    }
