	<script type="text/template" id="template_colors">
		<% _.each(colors,function(color,key,arr) { %>
			<%if(isMainPalette && color.name === 'panda'){%>
				<a href="http://panda.network" target="_blank" class="panda" data-clipboard-text="#000" style="background:#000">
                	<span class="label">Made by Panda Network</span>
                	<img src="img/panda.svg" alt="" />
                	<span class="more" data-palette-id="muic_black">MORE</span>
            	</a>
				<%}else{%>
					<%if(color.name !== "back"){%>
						<div class="color <%=(color.light)?'light':''%>" style="background:<%=color.color%>">
							<div class="copy-area" data-clipboard-text="<%=color.color%>">
								<span class="label"><%=color.name%></span>
								<span class="copy">COPY</span>
							</div>
							<%if(isMainPalette){%>
								<span class="more" data-palette-id="<%=color.palette_id%>">MORE</span>
								<%}%>
						</div>
						<%}else{%>
							<div class="back" style="background:#000">
								<span class="go-back">BACK</span>
							</div>
							<%}%>
								<%}%>
									<% }); %>
	</script>

