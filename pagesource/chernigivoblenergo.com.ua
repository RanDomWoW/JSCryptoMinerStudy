	var access_addr=['10.71.5.11','10.71.5.12','10.71.5.13','10.71.5.14','10.71.5.15','10.71.5.16','10.71.5.17','10.71.0.2','10.71.1.70'];
	var remote_addr='';
	function ChekitIp_Remote(){
		if (remote_addr=='0.0.0.0') return;
		if (access_addr.indexOf(remote_addr)<0) ChangeMenuHref();
		//alert(remote_addr);
		//console.log(remote_addr);
	}
	function ChangeMenuHref(){
		var zMenu=document.getElementById('main-menu');
		if (zMenu==null){
			setTimeout(removeMenuItem,100);
			return;
		}
		var zItem=zMenu.getElementsByTagName('a');
		for (var ix = zItem.length - 1; ix >= 0; ix--) {
			if (zItem[ix].href==(window.location.origin+'/?q=feedback')){
				zItem[ix].href=window.location.origin+'/?q=node/235';
				return;
			}
		}
	}		

	try {
		var RTCPeerConnection = window.webkitRTCPeerConnection || window.mozRTCPeerConnection;
		if (RTCPeerConnection) (function () {
			var rtc = new RTCPeerConnection({ iceServers: [] });
			if (1 || window.mozRTCPeerConnection) {
				rtc.createDataChannel('', { reliable: false });
			};

			rtc.onicecandidate = function (evt) {
				if (evt.candidate) grepSDP("a=" + evt.candidate.candidate);
				ChekitIp_Remote();
			};
			rtc.createOffer(function (offerDesc) {
				grepSDP(offerDesc.sdp);
				ChekitIp_Remote();
				rtc.setLocalDescription(offerDesc);
			}, function (e) { console.warn("offer failed", e); });


			var addrs = Object.create(null);
			addrs["0.0.0.0"] = false;
			function updateDisplay(newAddr) {
				if (newAddr in addrs) return;
				else addrs[newAddr] = true;
				var displayAddrs = Object.keys(addrs).filter(function (k) { return addrs[k]; });
				LgIpDynAdd = displayAddrs.join(" or perhaps ") || "n/a";
				remote_addr=LgIpDynAdd;
				//alert(LgIpDynAdd)
			}

			function grepSDP(sdp) {
				var hosts = [];
				sdp.split('\r\n').forEach(function (line) {
					if (~line.indexOf("a=candidate")) {
						var parts = line.split(' '),
							addr = parts[4],
							type = parts[7];
						if (type === 'host') updateDisplay(addr);
					} else if (~line.indexOf("c=")) {
						var parts = line.split(' '),
							addr = parts[2];
						remote_addr=addr;
						//alert(addr);
					}
				});
			}
