							<ul class="itemGroupList groupList" ng-show="productGroups.length" ng-repeat="productGroup in productGroups">
								<li class="itemRow">
									<a class="link itemRowColumnImg" ng-click="gotoProductCategory(productGroup, $event)">
										<img ng-src="{{productGroup.PictureUrl}}" alt="{{productGroup.GroupName}}" />
									</a>
									<div class="itemRowColumnText">
										<p>
											<a class="link" ng-click="gotoProductCategory(productGroup, $event)">
												<span class="crumbName" ng-repeat="crumbName in productGroup.CrumbNames track by $index">{{crumbName}}</span>
												<span>{{productGroup.GroupName}}</span>
											</a>
										</p>
									</div>
								</li>
							</ul>
						</div>
						<div class="itemGroupRight">
							<h2 id="suggestedItemsTitle">
								<span ng-show="!quickSearchInput && isAuthenticated">Senest s&#248;gte produkter</span>
