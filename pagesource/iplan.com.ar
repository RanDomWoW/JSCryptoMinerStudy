// Abrir las opciones del footer cuando está en tamaño "Mobile" (menos de 900px).
$(".hrefmobile").click(function(event){
    if(esMobile()){
           event.preventDefault();        
    }
});

$(".footer_columna").click(function (event) {
	if ( esMobile() ) {
		var element = $(event.target);

		if (element.hasClass('footer_columna')) {
			element.find('>ul').slideToggle();
			element.toggleClass('active');
		}else if (element.is('.footer_columna>a') || element.is('.footer_columna>a>img') ) {
			element.parents('.footer_columna').toggleClass('active').find('>ul').slideToggle();
			
		}

		if (element.hasClass('footer_columna_subcolumna')) {
			element.find('>ul').slideToggle();
			element.toggleClass('active');
		}else if (element.is('.footer_columna_subcolumna>a')) {
			element.next().slideToggle();
			element.parents('.footer_columna_subcolumna').toggleClass('active');
		};
	};

});

var topNav = {};
$('.header_menu_1er_nivel>li').each(function(){
	var self = $(this);
	var text = self.children('a').text();
	var obj = {};
	obj.element = self;
	obj.enableTransition = true;
	topNav[text] = obj;
});

$('.header_menu_1er_nivel>li').click(function(){
	var key = $(this).children('a').text();
	var section = $(this).data('section');
	if(section && location.pathname != section){
		location.href = section;
	}else{
		return false;
	}
	onclickMenuLevel2($(this), key);
}).hover(function(){
	if($(this).hasClass('active')){
		return false;
	}
	var key = $(this).children('a').text();
	onclickMenuLevel2($(this), key);
	
	var _this = $(this);
	setTimeout(function(){
		$('.header_menu_2do_nivel>ul>li', _this).first().click();
	}, 300);
}, function(){
        setTimeout(function(){
		sliderFull.removeClass('no_clickeable');
	}, 300);
});

$('.header_menu_1er_nivel>li').mouseleave(function(){
	hideActiveLevel3();
	hideActiveLevel2();
        sliderFull.removeClass('no_clickeable');
});

$('.header_menu_2do_nivel>ul>li').click(function(e){
	onclickMenuLevel3($(this), e);
});

$('.header_menu_opciones').click(function(e){
	e.stopPropagation();
});

var menuLevel3Enabled = true;

function onclickMenuLevel2(elem, key){
	if(!topNav[key].enableTransition){
		return false;
	}
	if(elem.hasClass('active')){
		hideMenuLevel2(elem, key);
	}else{
		showMenuLevel2(elem, key);
	}
}
function onclickMenuLevel3(elem, event){
	event.stopPropagation();
	if(!menuLevel3Enabled){
		return false;
	}
	if(elem.hasClass('active')){
		hideMenuLevel3(elem);
	}else{
		showMenuLevel3(elem);
	}
}

function showMenuLevel2(elem, key){
	topNav[key].enableTransition = false;
	hideActiveLevel2();
	elem.addClass('active');
	$('.header_menu_2do_nivel', elem).slideDown(function(){
		topNav[key].enableTransition = true;
	});
}
function showMenuLevel3(elem){
	menuLevel3Enabled = false;
	hideActiveLevel3();
	elem.addClass('active');
	sliderFull.addClass('no_clickeable');
	$('.header_menu_opciones', elem).slideDown(function(){
		menuLevel3Enabled = true;
	});
}
function hideMenuLevel2(elem, key){
	topNav[key].enableTransition = false;
	elem.removeClass('active');
	sliderFull.removeClass('no_clickeable');
	$('.header_menu_2do_nivel', elem).slideUp(function(){
		topNav[key].enableTransition = true;
	});
}
function hideMenuLevel3(elem){
	menuLevel3Enabled = false;
	elem.removeClass('active');
	$('.header_menu_opciones', elem).slideUp(function(){
		menuLevel3Enabled = true;
	});
}

function hideActiveLevel2(){
	var active = $('.header_menu_1er_nivel .active').removeClass('active');
	$('.header_menu_2do_nivel', active).hide();
	$('.header_menu_opciones', active).hide();
}
function hideActiveLevel3(){
	var active = $('.header_menu_2do_nivel .active').removeClass('active');
	$('.header_menu_opciones', active).hide();
}

botonContactos.click(function () {
	contactoBox.toggleClass('active');
});

botonMenuMobile.click( function () {
    headerMenuMobile.addClass('active');
    headerMenu2doNivel.slideUp();
    headerMenu1erNivel.removeClass('active');
});

headerMenuMobile.click( function (event) {
    var element = $(event.target);
    
    if ( element.is('li') ){
        var selectorMenu = '#' + element.attr('data-menu');
        
        
        headerMenu1erNivel.removeClass('mobile_active');        
        
        $(selectorMenu).addClass('mobile_active').click();
        headerMenuMobile.removeClass('active');
    }else if ( element.is('a') ){
        var selectorMenu = '#' + element.parent('li').attr('data-menu');
        
        headerMenu1erNivel.removeClass('mobile_active');
        
        $(selectorMenu).addClass('mobile_active').click();
        headerMenuMobile.removeClass('active');
    }
});


