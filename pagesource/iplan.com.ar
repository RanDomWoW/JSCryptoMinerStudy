iplan_utils.getMarketURL = function(){
	var url = '';
	
	if(location.hostname.indexOf(iplan_utils.portal_dev_url)!=-1){
		url = iplan_utils.market_dev_url;
	}else if(location.hostname.indexOf(iplan_utils.portal_stg_url)!=-1){
		url = iplan_utils.market_stg_url;
	}else{
		url = iplan_utils.market_prd_url;
	}
	return location.protocol + '//' + url + ( location.port ? ':' + location.port : '') + iplan_utils.market_context;
}

