<script type="text/javascript">
$('img').bind('contextmenu', function(e) {
    return false;
});

$(document).ready(function() {
    var hours = 168;
    var now = new Date().getTime();
    var setupTime = localStorage.getItem('calloutStateBook');

	if(localStorage.getItem('calloutStateBook') == null) {
		$('#callout').show();
        $('body').addClass('callout-body');
    } else {
    	$('#callout').hide();
        if(now-setupTime > hours*60*60*1000) {
            localStorage.clear();
        }
	}

    $("#callout .callout-close").click(function(){
        localStorage.setItem('calloutStateBook', now);
        $('#callout').hide();
        $('body').removeClass('callout-body');
    });
});

</script>


