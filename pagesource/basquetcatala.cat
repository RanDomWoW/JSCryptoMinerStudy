<script>
var maxSlides = "6"; 
var slide = 0; 
var slideInterval;
var slideIntervalTime = 5000;

var selectedTabId = "1";

/* Slides */ 
function selectSlide( index ) {
	slide = index; 

	$( ".slide-wrapper" ).removeClass( "show" );
	$( "#slide-" + index ).addClass( "show" ); 

	stopInterval();
	autoSlide();
} 
 
function previousSlide() { 
	slide--; 
	if( slide < 0 ) slide = maxSlides - 1; 
	
	selectSlide( slide ); 
} 
 
function nextSlide() { 
	slide++; 
	if( slide >= maxSlides ) slide = 0; 

	selectSlide( slide ); 
} 

function stopInterval() {
	clearInterval( slideInterval );
}

function autoSlide() {
	slideInterval = setInterval( function() { 
		nextSlide();
	}, slideIntervalTime);
}

