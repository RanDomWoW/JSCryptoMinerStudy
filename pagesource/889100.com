<script type="text/javascript">
$(function coursePopup() {
    $("#TOP .js-popupArea .course-menu li a").click(function(){
        var cls = $(this).parent("li").attr("class");
        var clsArray = cls.split(" ");
        var str="";
        var reg = /^act$|^lt[0-9]+$/;

        for(var i = 0; i < clsArray.length; i++) {
            if(clsArray[i].search(reg) != -1)　{
                str = str + clsArray[i] + " ";
            }
        }

        str = str.replace(/^\s+|\s+$/g,'');

        $("#TOP .js-popupArea ul li").removeClass("act");
        $("#TOP .js-popupArea").removeClass("act");
        $("#TOP .js-popupArea .fld").hide();
        //$("#TOP .js-popupArea .fld").slideUp();

        $("#TOP .js-popupArea ul li").removeClass("inact");

        if($("#TOP .js-popupArea .fld."+str).is(":hidden")){
            $("#TOP .js-popupArea .fld."+str).slideDown(300);
            $(this).parent("li").addClass("act");
            $(this).parents(".js-popupArea").addClass("act");
            $("#TOP .js-popupArea ul.course-menu > li").addClass("inact");
            $(this).parent("li").removeClass("inact");
        }

        return false;
    });
});

</script>
