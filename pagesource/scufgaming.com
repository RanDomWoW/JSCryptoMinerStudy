<script>
require(["jquery"], function($){

	jQuery(window).click(function() {
		close_all_menu();
	});

	jQuery(document).ready(function($) {
		var headerContainer = $('#header-container-wrapper');
		var searchIcon = $('#search-icon');

		// Nav icon transformation
		$(document).ready(function(){
			$('#nav-icon3').click(function(){
				$(this).toggleClass('open');
				if( $(this).hasClass('open') ){
					$('body,html').addClass('noscroll');
				} else {
					$('body,html').removeClass('noscroll');
				}
			});
		});

		var cookie_name = 'scuf_cart_count';

		// set a timeout in order to waiting for the cookie update before displaying the red bubble
		setTimeout(function(){
			cartItemsCount = getCookie(cookie_name);
			if( cartItemsCount != null )
				update_cart_count(cartItemsCount);
		}, 1000);

		// Search
		$('.toggle-search').click(function(e){
			headerContainer.toggleClass('showsearch');
			searchIcon.toggleClass('ion-ios-search-strong').toggleClass('ion-android-close');
			if( headerContainer.hasClass('showsearch') ){
				forceFocusSearchBar();
			}
			e.preventDefault();
		});

		// Menu
		$('#mega-menu a').click(function(e){
			e.stopPropagation();
		});

		$('.level-top.arr').click(function(e){

			if($(this).hasClass('mega-menu-item')){
				if($(this).hasClass('active')) {
					$(this).removeClass('active');
					$('#mega-menu').hide()
				} else {
					close_all_menu();
					$(this).toggleClass('active');
					$('#mega-menu').show()
				};
			} else {
				if($(this).hasClass('active')) {
					$(this).removeClass('active');
					$(this).next().hide();
				} else {
					close_all_menu();
					$(this).toggleClass('active');
					$(this).next().show();
				};
			};

			e.preventDefault();
			e.stopPropagation();
		});

		var obj = $("span:contains('Design Your Own')").parent().parent();
		$.each(obj, function( index, value ) {
			$(this).prependTo($(this).parent());
		});


		// Mobile menu
		$('.mobile-menu-toggle').click(function(e){
			toggle_mob_menu();
			e.preventDefault();
		});

		$('.menu-overlay').click(function(e){
			toggle_mob_menu();
		});

		// Submit the research form when button enter is pressed
		$('#search_mini_form').keyup(function(e){
			if(e.keyCode == 13)
				$(this).submit();
		});

		/*** Custom Navbar ***/
			/*** Hide submenu and search when main content is clicked ***/
			$('#maincontent').click(function(){
				if( headerContainer.hasClass('showsearch') ){
					$('.toggle-search').click();
				}
				if( $('#nav-row-2 .display-overlay.active-field').length > 0 ){
					$('#nav-row-2 .display-overlay').each(function(){
						$(this).removeClass('active-field');
					});
					$('#nav-row-3').addClass('hidden');
				}
			});
			/*** Handle submenu display on hover ***/
			$('#nav-row-2 .display-overlay').hover(function(){
				if( $(this).closest('li').find('.link-overlay').length == 0 ) return;

				var overlayContent = $(this).closest('li').find('.link-overlay').html();
				var $this = $(this);
				$('#nav-row-2 .display-overlay').each(function(){
					$(this).removeClass('active-field');
				});
				$this.addClass('active-field');
				$('#nav-row-3').html(overlayContent);
				$('#nav-row-3').removeClass('hidden');

			});
			$('#nav-row-3').mouseleave(function(){
				setTimeout(function(){
					closeSubmenuDesktop();
				}, 1000);
			});

			$('#navsearch').focus(function(){
				$('#mobile-nav-side').scrollTop($('#mobile-nav-side')[0].scrollHeight);
			});

			/*** Handle mobile display ***/
			var menuMainLinks = $('#menu-main-links ul.main-ul').clone();
			if( $('#mobile-nav-side nav.mobile-nav-items #search-mobile').length == 1 )
				$('#mobile-nav-side nav.mobile-nav-items #search-mobile').before(menuMainLinks);
			else	
				$('#mobile-nav-side nav.mobile-nav-items').append(menuMainLinks);

			var dropdownLink = $('.mobile-nav-items .parent > a.level-top');

			dropdownLink.click(function(e){
				$(this).toggleClass('active');
				$(this).next().slideToggle('fast');
				e.preventDefault()
			});

			/*** Mobile display v2 ***/
			$('.mobile-nav-items .level0.parent').click(function(){
				$(this).addClass('active');
				hideMainMenu();
				showSubmenu();
			});
			$('#back_main_menu').click(function(){
				showMainMenu();
				hideSubmenu();
			});

			/*** Store mobile dropdown ***/
			$('.custom_dropdown').click(function(){	
				if( $(this).closest('.mob_dropdown').hasClass('show') )
					$('.mob_dropdown.show').removeClass('show');
				else {
					$('.mob_dropdown.show').removeClass('show');
					$(this).closest('.mob_dropdown').addClass('show');
			    }
			});

			/*** Set store class on body ***/
			var storeCode = getStoreCode();
			//$('body').addClass(storeCode);

			/*** Search Bar ***/
			$('#nav-row-2 .toggle-search').click(function(){
				$('#nav-row-2 .main-ul').toggleClass('fadeout');
			});

		/*** end ***/

		/*** mobile footer ***/

		$('.colname').click(function() {
			col = $(this).closest('.col');
			if(col.hasClass('active')) col.removeClass('active');
			else col.addClass('active');
		});
		/*** end mobile footer ***/

	});

	/*** Functions ***/
	function getStoreCode(){
		var url = window.location.href;
		if( url.indexOf('/eu/') != -1 )
			return 'eu';
		if( url.indexOf('/uk/') != -1 )
			return 'uk';
		if( url.indexOf('/fr/') != -1 )
			return 'fr';
		if( url.indexOf('/dist/') != -1 )
			return 'dist';
		return 'us';
	}
	function forceFocusSearchBar(){
		if( !$('#navsearch1').is(':focus') ){
			setTimeout(function() {
				$('#navsearch1').focus(); 
				forceFocusSearchBar();
			}, 500);
		} else {
			return true;
		}
	}
	function hideMainMenu(){
		$('.mobile-nav-items .level0.parent').each(function(){
			if( !$(this).hasClass('active') )
				$(this).addClass('hidden');
		});
		$('.mobile-nav-items .level0.parent.active a.level-top').addClass('hidden');
		$('.mobile-nav-items .simple-link').addClass('hidden');
		$('#search-mobile').addClass('hidden');
	}
	function showMainMenu(){
		$('.mobile-nav-items .level0.parent').each(function(){
			if( $(this).hasClass('active') ){
				$(this).removeClass('active');
				$(this).find('a.level-top').removeClass('hidden');
				$(this).find('a.level-top').removeClass('active');
				$(this).find('.level0.submenu').css('display', 'none');
			} else {
				$(this).removeClass('hidden');
			}
		});
		$('.mobile-nav-items .level0.parent.active a.level-top').addClass('hidden');
		$('.mobile-nav-items .simple-link').removeClass('hidden');
		$('#search-mobile').removeClass('hidden');
	}
	function showSubmenu(){
		$('#back_main_menu').removeClass('hidden');
	}
	function hideSubmenu(){
		$('#back_main_menu').addClass('hidden');
	}

	function update_cart_count(cartItemsCount){
		if(cartItemsCount > 0 && $('#cartItemCount').length > 0 ){
			$('#cartItemCount').html(cartItemsCount);
			$('#cartItemCount').removeClass('hidden');
		}
	}

	function closeSubmenuDesktop(){
		if( $('#nav-row-3').is(':hover') === true || $('#nav-row-2').is(':hover') === true )
			return;
		if( $('#nav-row-2 .display-overlay.active-field').length > 0 ){
			$('#nav-row-2 .display-overlay').each(function(){
				$(this).removeClass('active-field');
			});
			$('#nav-row-3').addClass('hidden');
		}
	}

	function getCookie(name) {
		var nameEQ = name + "=";
		var ca = document.cookie.split(';');
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		}
		return null;
	}

	function close_all_menu() {
		jQuery('.sub-menu').hide();
		jQuery('.level-top.arr').removeClass('active');
	};

	function toggle_mob_menu() {
		var body = jQuery('body');
		var overlay = jQuery('.menu-overlay');

		if(body.hasClass('menu-active')) {
			body.removeClass('menu-active');
			overlay.addClass('fade-out');

			setTimeout(function(){
				overlay.removeClass('show fade-out');
			}, 250);

		} else {
			body.addClass('menu-active');
			overlay.addClass('show');
		};
	};

});
