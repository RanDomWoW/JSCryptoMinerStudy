        }
        ];
        var circlesDiv = '#customize-section .slide-circles';
        var nextTriggerClass = '#customize-section .slides-nav .slide-nav-right';
        var previousTriggerClass = '#customize-section .slides-nav .slide-nav-left';    
        var visibleItems = getVisibleItems();
        $(window).resize(function(){
            var visibleItems = getVisibleItems();
            cleanDisplay();
            createDisplay(0, controllers, visibleItems);        
        });
        createDisplay(0, controllers, visibleItems);
        $(nextTriggerClass).click(function(){
            getNextSlide(controllers);
        });
        $(previousTriggerClass).click(function(){
            getPreviousSlide(controllers);
        });     
        /*** functions ***/
        function getVisibleItems(){
            var documentWidth = $(document).width();
            if( documentWidth > 992 )
                return 3;
            else if( documentWidth > 768 )
                return 2;
            else
                return 1;
        }
        function getNextSlide(controllers){
            var visibleItems = getVisibleItems();
            var currentFirstElm = parseInt($(controllersRowElm + ' .controller').first().data('position'));
            var nextFirstElm = currentFirstElm + 1;
            cleanDisplay();
            createDisplay(nextFirstElm, controllers, visibleItems);
        }
        function getPreviousSlide(controllers){
            var visibleItems = getVisibleItems();
            var currentFirstElm = parseInt($(controllersRowElm + ' .controller').first().data('position'));
            var nextFirstElm = currentFirstElm - 1;
            cleanDisplay();
            createDisplay(nextFirstElm, controllers, visibleItems);
        }
        function cleanDisplay(){
            $(controllersRowElm + ' .controller').remove();
            $(circlesDiv + ' span:not(.model)').remove();
        }   
        function createDisplay(firstElm, controllers, visibleItems){
            var pos = firstElm;
            for (var i = 0; i < controllers.length; i++) {
                if( pos >= controllers.length )
                    pos = 0;
                if( pos < 0 )
                    pos = controllers.length - 1;
                var controller = controllers[pos];
                var newItem = $(controllersRowElm + ' .model').clone().removeClass('model').addClass('controller');
                if( i > visibleItems - 1 )
                    newItem.addClass('hidden');
                newItem.attr('data-position', pos);
                newItem.find('.img_wrapper img').attr('src', controller.img);
                newItem.find('.img_wrapper a').attr('href', controller.link);
                newItem.find('.controller_name').html(controller.name);
                newItem.find('.controller_copy').html(controller.copy);
                $(controllersRowElm).append(newItem);
                pos++;
            }
            generateCircles(controllers, firstElm);
        }
        function generateCircles(controllers, circlePos){
            var circles = controllers.length;
            var circleModel = $(circlesDiv + ' .model');
            if( circlePos == circles )
                circlePos = 0;      
            if( circlePos < 0 )
                circlePos = circles - 1;
            if( circles > 0 ){
                for (var i = 0; i < circles; i++) {
                    var circleClone = circleModel.clone().removeClass('model');
                    circleClone.attr('data-pos', i - 1);
                    if( i == circlePos )
                        circleClone.addClass('active');
                    $(circlesDiv).append(circleClone);
                }           
            }
        }
    });
</script>
<script>
    require([
        'jquery'
    ], function($){
        /*** Homepage anchor script ***/
        var slideClass = '#homepage-anchor .slide';
        var nextTriggerClass = '#homepage-anchor .slides-nav .slide-nav-right';
        var previousTriggerClass = '#homepage-anchor .slides-nav .slide-nav-left';
        var circlesDiv = '#homepage-anchor .slide-circles';
        var cursorOnHeader = $('#homepage-anchor').is(':hover');
        // var cursorOnHeader = false;
    
        createCircles();
    
        $(document).on({
                mouseenter:function(){ cursorOnHeader = true; },
                mouseleave:function(){ cursorOnHeader = false; },
            }, 
           '#homepage-anchor'
        );  
        setInterval(function(){
            if(!cursorOnHeader)
                getNextSlide();
        }, 5000);
        $(nextTriggerClass).click(function(){
            getNextSlide();
        });
        $(previousTriggerClass).click(function(){
            getPreviousSlide();
        }); 
    
        /*** functions ***/
        function createCircles(){
            var slidesNb = getSlidesNb();
            var circleModel = $(circlesDiv + ' .model');
            for (var i = 0; i < slidesNb; i++) {
                var circleClone = circleModel.clone().removeClass('model');
                circleClone.attr('data-pos', i + 1);
                if( i == 0 )
                    circleClone.addClass('active');
                $(circlesDiv).append(circleClone);
            }
        }
        function getSlidesNb(){
            return $(slideClass).length;
        }
        function getCurrentSlide(){
            return $(slideClass + ':not(.hidden)').data('position');
        }
        function getNextSlide(){
            var slidesNb = getSlidesNb();
            var currentSlidePos = parseInt(getCurrentSlide());
            var slideTargetPos = currentSlidePos + 1;
            if( slideTargetPos > slidesNb ){
                displaySlide(1);
            } else {
                displaySlide(slideTargetPos);
            }
        }
        function getPreviousSlide(){
            var slidesNb = getSlidesNb();
            var currentSlidePos = parseInt(getCurrentSlide());
            var slideTargetPos = currentSlidePos - 1;
            if( slideTargetPos < 1 ){
                displaySlide(slidesNb);
            } else {
                displaySlide(slideTargetPos);
            }
        }
        function displaySlide(pos){
            $(slideClass).addClass('hidden');
            if( $(slideClass + '[data-position="'+ pos +'"]').length == 1 )
                $(slideClass + '[data-position="'+ pos +'"]').removeClass('hidden');
            $(circlesDiv + ' span').removeClass('active');
            if( $(circlesDiv + ' span[data-pos="'+ pos +'"]').length == 1 )
                $(circlesDiv + ' span[data-pos="'+ pos +'"]').addClass('active');       
        }
    });
</script>
