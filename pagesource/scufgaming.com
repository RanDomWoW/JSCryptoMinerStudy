		var spOverlay = $('#hp-spotlight-container-wrapper .content-wrap');
		var contentPoint = $('#hp-adlets-container-wrapper').offset().top - header.height();

		var headerOverlay = function(){
		    var st = $(this).scrollTop();

		    if(st > contentPoint){
				header.removeClass('overlayed');
			} else {
				header.addClass('overlayed');
		    }
		}

		var scrollParallax = function(){
			var windowWidth = $(window).width();
		    var st = $(this).scrollTop();
	        var bgPos = st / 1.2;
	        var txtPos = st / 5;

			if (windowWidth > 991) {
		        spotlight.css({"transform": "translate3d(0px, " + bgPos + "px, 0px)"});
		        spOverlay.css({"transform": "translateY(-" + txtPos + "px)"});

				if (st <= spotlight.height()) {
					spOverlay.css({
						'opacity' : (1 - st / spotlight.height())
					});
				}
			}

		    //headerOverlay();
		};

		//headerOverlay();

	    $(window).scroll(function() {
		    scrollParallax();
	    });

	    $(window).resize(function() {
		    scrollParallax();
		});

		$('#pro-test .block').matchHeight();

	});

});
