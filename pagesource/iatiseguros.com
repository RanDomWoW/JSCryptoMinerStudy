jq132(document).ready(function(){

	jq132("form.validable").bind("submit", function(e){
		if (typeof filters == 'undefined') return;
		
		if(jq132("#tipo_aventura").is(':checked') && jq132("#tipo_crucero").is(':checked')) {
			alert("No es posible seleccionar Aventura/Esquí y Crucero a la vez");
			return false;  
        } 
        
	    jq132(this).find("input, textarea, select").each(function(x,el){
	        if (jq132(el).attr("className") != 'undefined') { 
				jq132(el).removeClass("error");
	        	jq132.each(new String(jq132(el).attr("className")).split(" "), function(x, klass){
		            if (jq132.isFunction(filters[klass]))
	                	if (!filters[klass](el))  jq132(el).addClass("error");
	        	});
	        }
	    });
		if (jq132(this).find(".error").size() > 0) {
			jq132.stop(e || window.event);
			alert("Los campos marcados en naranja no se han informado o están en un formato incorrecto");
			return false;
		}
	    return true;
	});
});


var filters = {
	    requerido: function(el) {var cadena = jq132(el).val(); cadena = jq132.trim(cadena); return (cadena != '' && cadena != -1);},
	    email: function(el) {if (jq132(el).val() == '') {return true;} else{return /^[a-zA-Z0-9][a-zA-Z0-9_.-]*@[a-zA-Z0-9.-]+\.[a-zA-Z]{2,4}$/.test(jq132(el).val());}},
	    numerico: function(el){return /^[0-9\s]*$/.test(jq132(el).val());},
	    decimal: function(el){return /^(?:\+|-)?\d+\.\d*$/.test(jq132(el).val());},	    	    
	    caracter: function(el){return /^[A-Za-z àáéèíìóòúùÁÀÉÈÍÌÓÒÚÙÑñÜüçÇ]*$/.test(jq132(el).val());},										
	    nif : function(el){return /^(X(-|\.)?0?\d{7}(-|\.)?[A-Za-z]|[A-Za-z](-|\.)?\d{7}(-|\.)?[0-9A-Za-z]|\d{8}(-|\.)?[A-Za-z])$/.test(jq132(el).val());},	
	    cp: function(el){return /^([1-9]{2}|[0-9][1-9]|[1-9][0-9])[0-9]{3}$/.test(jq132(el).val());},
	    solo_imagen: function(el){return /^(.+(gif|jpg|png))\b$/.test(jq132(el).val());},
	    direccion: function(el){return /^[A-Za-z àáéèíìóòúùÁÀÉÈÍÌÓÒÚÙÑññÜüçÇ0-9\\\ª\º\/]*$/.test(jq132(el).val());},
	    captcha: function(el){return /^[A-Za-z0-9]{5}$/.test(jq132(el).val());}	    
	    };
</script>
