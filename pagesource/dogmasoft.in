    <script type="text/javascript">
        function gopre() {
            document.getElementById('rdprepaid').checked = true;
            document.getElementById('rdpostpaid').checked = false;

            document.getElementById('prep').style.display = "block";
            document.getElementById('prop').style.display = "none";
        }

        function gopost() {
            document.getElementById('rdprepaid').checked = false;
            document.getElementById('rdpostpaid').checked = true;

            document.getElementById('prep').style.display = "none";
            document.getElementById('prop').style.display = "block";
        }

    </script>
