				error     = true;
				if( is_page_login )
					$('.message_error').html('Thông tin đăng nhập không đúng.');
			}
			if( is_page_login )
				data['return_url'] = $('#redirect').val();
			if( !error )
			{
				if(!run_ajax)
					return false;
				run_ajax 	  = false;
				$('.message_error').html('');
				$( class_form + ' .btn_login').addClass('disabled').html('<i class="fa fa-spinner fa-pulse fa-fw"></i> Đang đăng nhập');
					
				data['LoginForm']['rememberMe'] = 1;
				
				process_ajax_login = $.post('/site/login', data , function(res)
				{	
					process_verify 	= true;
					process_resent 	= true;
					run_ajax 		= true;
					if(res.indexOf("sended_otp") >= 0 || res.indexOf("verify_otp_failed") >= 0)
					{
						if( res.indexOf("sended_otp") >= 0)
						{
							count_down_verify();
							count_down_time_click_resend();
							if($('.fa_resend_otp').hasClass('active'))
							{
								$('.fa_resend_otp').attr('class','fa_resend_otp fa fa-check');
							}
							else
							{
								$('.frm_lg').hide();
								$('.forgot_password').show();
							}
							var data_response = res.toString().split('#_#');
							if (data_response[1].indexOf('@') == -1)
								var title = 'Mã đăng nhập OTP đã được gửi vào số điện thoại <strong>' + data_response[1] + '</strong>';
							else
								var title = 'Mã đăng nhập OTP đã được gửi vào địa chỉ email <strong>' + data_response[1] + '</strong>';
							if(is_page_login)
								var data_btn = 'btnSignIn';
							else
								var data_btn = 'btn_login';
							$('.box_info_verify').html(title + '<span class="expired_otp">. Mã OTP sẽ hết hạn sau <span class="time_count_down"></span> giây</span> hoặc <a style="color: #0096d7;font-size:13px;" href="javascript:;" data-btn="' + data_btn + '" data-type="' + (( data['otp'] == 'email') ? 'phone' : 'email') + '" class="not_has_code">yêu cầu gửi mã OTP qua ' + (( data['otp'] == 'email') ? 'số điện thoại' : 'email') + '<i class="fa_resend_otp hide"></i></a>');
						}
						else if( res.indexOf("verify_otp_failed") >= 0 )
						{
							$('.btn_verify_otp').removeClass('disabled');							$('.loading_check_otp').addClass('hide');
							$('input[name="LoginForm_OTP"][data-toggle="tooltip"].input_active').attr('title','Mã OTP không đúng hoặc đã hết hạn').attr('data-original-title','Mã OTP không đúng hoặc đã hết hạn').tooltip('show').data('bs.tooltip').tip().addClass('tooltip-login');
							setTimeout(function(){
								$('input[name="LoginForm_OTP"][data-toggle="tooltip"].input_active').attr('title','').attr('data-original-title','');
							},1500);
						}
						return;
					}
					
					var arr = $.parseJSON(res);
					if( typeof arr['error'] != 'undefined' && !arr['error'])
					{
						$('.loading_check_otp').attr('class','fa fa-check');
						
						$('.sp_view_phone').remove();
						if( $('#redirect').val() == 'qa' )
							location.reload();
						else if($('#redirect').val() == '' && arr['user']['type'] == 2 && $('.page_login').hasClass('popup_login')){
							$('.close_popup_tb').trigger('click');
							$('.index_login_register .index_register,.li_guest').remove();
							$('.index_login_register .index_login').attr('href', arr['user']['link']).html('<i class="sprite_membership"></i>Hi, ' + arr['user']['name']);
							$('.index_btn_login').removeAttr('onclick');
							$('.avatar.avt_user.current,.div_u_need .body_request > img').attr('src', arr['user']['avatar'] );
							$('.avt_buyer .buyer_name').html( arr['user']['name'] );
							$('.check_login,.favourite_supplier,.favourite_product,.btn-chat').removeClass('nlg');
							$('.notification .notifi_child').addClass('load');
							$('.bread_crumbs span.home:first-child a,.link_home').attr('href', arr['user']['link']);
							$('.btn_ct_supp').addClass('btn-contact');
							if( $('.QAcenter_top_right').length <= 0 )
								location.reload();
							else{
								$('.QAcenter_top_right').attr('style');
								$('.popup_tb').removeClass('fade in');
							}
						}
						else if( typeof arr['redirect'] != 'undefined' && arr['redirect'] != '')
							window.location.href = arr['redirect'];
					}
					else
					{
						run_ajax = true;
						if( typeof data['verify_otp'] != 'undefined' )
						{
							$('.btn_verify_otp .loading_check_otp,.fa_resend_otp').addClass('hide');
							$('input[name="LoginForm_OTP"][data-toggle="tooltip"].input_active').attr('title',arr['LoginForm_password']).attr('data-original-title',arr['LoginForm_password']).tooltip('show').data('bs.tooltip').tip().addClass('tooltip-login');
						}else{
							$( class_form + ' .btn_login').removeClass('disabled').html('Đăng nhập');
						}
						$('.message_error').html(arr['LoginForm_password']);
						user_name_old = user_name;
						password_old  = password;
					}
					
				});
			}
		}
	}
	var interval;
	var count_down_verify = function()
	{
		clearInterval(interval);
		second  = time_count_down_verify_otp/1000;
		$('.time_count_down').parent().removeClass('hide');
		$('.sp_expired_otp').remove();
		interval = setInterval(function(){
			if( second > 0 )
				second -= 1;
			else
			{
				clearInterval(interval);
				$('.time_count_down').parent().addClass('hide');
				if($('.sp_expired_otp').length <= 0)
					$('.not_has_code').after('<span class="sp_expired_otp" style="color:red">. Đã hết thời gian nhập mã OTP. Vui lòng bấm yêu cầu gửi lại mã OTP.</span>');
			}
			$('.time_count_down').html(second);
		},1000);
	}
	var interval_time_click_resend_otp;
	var count_down_time_click_resend = function()
	{
		clearInterval(interval_time_click_resend_otp);
		time_continues_click_resend_otp 	= 30;
		interval_time_click_resend_otp = setInterval(function(){
			if( time_continues_click_resend_otp > 0 )
				time_continues_click_resend_otp -= 1;
			else
			{
				clearInterval(interval_time_click_resend_otp);
			}
		},1000);
	}
	jQuery(document).ready(function($){
		$('#SignInForm_username').focus();
		var type = 'phone';
		$('.btn_login').click(function()
		{
			ajax_login($(this).data('form'),parseInt($(this).data('ispage')),type);
		});
		
		$('input[type="text"].username,input[type="password"].password').keypress(function(e)
		{
			if( e.which == 13)
				$(this).parent().parent().parent().find('.btn_login').trigger('click');
		});
		
		$('input[name="LoginForm_OTP"]').keypress(function(e)
		{
			if( e.which == 13)
				$(this).parent().parent().find('.btn_verify_otp').trigger('click');
		});
		
		$('.btn_verify_otp').click('click', function()
		{
			if( process_verify && run_ajax )
			{
				var _parent 	= $(this).parent().parent();
				_parent.find('input[name="LoginForm_OTP"]').addClass('input_active');
				var code_otp 	= $.trim(_parent.find('input[name="LoginForm_OTP"]').val());
				if(code_otp == '' || code_otp.length < 6)
				{
					_parent.find('input[name="LoginForm_OTP"]').focus().addClass('input_active');
					if(code_otp == '')
						var msg 	= 'Vui lòng nhập mã OTP';
					else if(code_otp.length < 6)
						var msg 	= 'Mã OTP = 6 số';
					_parent.find('input[name="LoginForm_OTP"][data-toggle="tooltip"]').attr('title',msg).attr('data-original-title',msg).tooltip('show').data('bs.tooltip').tip().addClass('tooltip-login');
					setTimeout(function(){
						_parent.find('input[name="LoginForm_OTP"][data-toggle="tooltip"]').attr('title','').attr('data-original-title','');
					},1500);
					return false;
				}
				else
				{
					$('.error_verify_otp').html('');
					process_verify = false;
					_parent.find('.loading_check_otp').removeClass('hide');
					$(this).addClass('disabled');
					ajax_login($(this).attr('classform'),false,code_otp);
				}
			}
		});
		
		$('.not_has_code').live('click',function()
		{
			if( process_resent )
			{
				if( time_continues_click_resend_otp > 0 )
				{
					$.simplyToast('warning', 'Bạn vừa yêu cầu gửi lại mã OTP. Vui lòng thử lại sau ' + time_continues_click_resend_otp + ' giây' );
				}
				else
				{
					process_resent  = false;
					type 			= $(this).data('type');
					$(this).find('.fa_resend_otp').attr('class','fa fa-spinner fa_resend_otp active fa-pulse fa-fw');
					$('.error_verify_otp').html('');
					$('#' + $(this).data('btn')).trigger('click');
				}
			}
		});
	});
</script>
