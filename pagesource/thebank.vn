		function startTime() {
			var today 	= new Date();
			var h 		= today.getHours();
			var m 		= today.getMinutes();
			var s 		= today.getSeconds();
			
			m = checkTime(m);
			s = checkTime(s);
			
			$('.sp_hour_minutes').html(h + ":" + m + ":" + s);
			if( h >= 23 && m >= 50 ){
				var month 	= checkTime(today.getMonth() + 1);
				var date 	= checkTime(today.getDate());
				var year 	= checkTime(today.getYear());
				
				day_name 	= weekday[today.getDay()];
				
				$('.sp_name_date').html(day_name + ', ' + date + '/' + month + '/' + year);
			}
			var t = setTimeout(startTime, 1000);
		}
