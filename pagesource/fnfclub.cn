  <script type="text/javascript">
$(document).ready(function(){
	$('#play_button').css('opacity', 0.4);  
	$('#play_button').hover(
		function(){  
      $(this).stop().fadeTo( 300, 1);  
	   },  
	   function(){  
		  $(this).stop().fadeTo( 300, 0.4);  
	   });  

	 $('#video_box_small').click(function(){
		var video = $('#video_box_small video');
		var button = $('#play_button');
		if(video.get(0).paused){
			button.attr("src", "videos/button_play_small.png");
			video.get(0).play();	
		} else {
			button.attr('src', 'videos/button_pause_small.png');
			video.get(0).pause();	
		}
	 });
	 $('#close').click(function(){
		$('#video_box').fadeOut( 300 );
		$('#video_box video').get(0).pause();
	 });	 
});
  </script>
