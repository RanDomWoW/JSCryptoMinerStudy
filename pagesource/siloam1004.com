		if (pamphletcount < pamphletMaxLoops) {
			pamphletcount++;
		}
		else {
			pamphletcount = 1;
		}
		setTimeout("displayPamphletImage()", bInterval*500+1500);
	}

	function displayBrassImage() {

		brassObj.filters.blendTrans.apply();
		document.images.brassObj.src = eval("brassImage"+brasscount+".src");
		goToBrassUrl =eval("brassUrl"+brasscount);
		brassObj.filters.blendTrans.play();
		if (brasscount < brassMaxLoops) {
			brasscount++;
		}
		else {
			brasscount = 1;
		}
		setTimeout("displayBrassImage()", bInterval*500+1500);
	}


