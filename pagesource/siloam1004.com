	function displayPamphletImage() {

		pamphletObj.filters.blendTrans.apply();
		document.images.pamphletObj.src = eval("pamphletImage"+pamphletcount+".src");
		goToPamphletUrl =eval("pamphletUrl"+pamphletcount);
