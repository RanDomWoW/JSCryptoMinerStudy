	<script>

	// ----------カルーセル----------
	$(function(){
		var li_move = 1;
		var carousel_height = $(".carouselWrap").height();
		var li_height = $(".carousel li").outerHeight();
		var li_num = $(".carousel li").size();
		var ul_height = li_height*li_num;
		if(carousel_height > (ul_height)) {
		$(".carouselWrap").append('<div id="prev" class="hide"></div><div id="next" class="hide"></div>');
		}else{
		$(".carouselWrap").append('<div id="prev" class="hide"></div><div id="next" class="show"></div>');
		}
		$('.carousel ul').css({
			height: ul_height+'px'
		});
		$('#prev').click(function(){
			if($(this).attr("class") != "hide") {
				$('.carousel ul:not(:animated)').animate(
					{top:'+='+li_height*li_move},
					300,
					function(){
						var ul_pos = boxPosition(".carousel ul","top");
						$('#next').attr("class","show");
						if(ul_pos === 0) {
							$('#prev').attr("class","hide");
						}
					}
				);
			}
		});
		$('#next').click(function(){
			if($(this).attr("class") != "hide") {
				$('.carousel ul:not(:animated)').animate(
					{top:'-='+li_height*li_move},
					300,
					function(){
						var ul_pos = boxPosition(".carousel ul","top");
						$('#prev').attr("class","show");
						if(carousel_height > (ul_height+ul_pos)) {
							$('#next').attr("class","hide");
						}
					}
				);
			}
		});
		function boxPosition(ele,pos) {
			var position = $(ele).position();
			return position[pos];
		}
	});

	// ----------タブメニュー----------
	// トップニュース
	$(document).ready(function() {
		$('.activeBox').show();
		// $('.topNewsTab li a:first').addClass('active');
		$('.topNewsTab li a').click(function() {
				$('.topNewsTab li a').removeClass('active');
				$(this).addClass('active');
				$('.topNewsBox').hide();
				$($(this).attr('href')).fadeIn(200);
				return false;
		});
	});
	// プレイレポート・インタビュー
	$(document).ready(function() {
		$('.rightContentsBox:first').show();
		$('.rightContentsTab li a').click(function() {
				$('.rightContentsTab li a').removeClass('active');
				$(this).addClass('active');
				$('.rightContentsBox').hide();
				$($(this).attr('href')).fadeIn(200);
				return false;
		});
	});

		//注目記事 スライダー
		$(document).ready(function() {
