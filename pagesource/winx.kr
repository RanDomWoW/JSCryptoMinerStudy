    function copyToClipboard(elem) {
      // create hidden text element, if it doesn't already exist
      var targetId = "_hiddenCopyText_";
      var isInput = elem.tagName === "INPUT" || elem.tagName === "TEXTAREA";
      var origSelectionStart, origSelectionEnd;
      if (isInput) {
          // can just use the original source element for the selection and copy
          target = elem;
          origSelectionStart = elem.selectionStart;
          origSelectionEnd = elem.selectionEnd;
      } else {
          // must use a temporary form element for the selection and copy
          target = document.getElementById(targetId);
          if (!target) {
              var target = document.createElement("textarea");
              target.style.position = "absolute";
              target.style.left = "-9999px";
              target.style.top = "0";
              target.id = targetId;
              document.body.appendChild(target);
          }
          target.textContent = elem.textContent;
      }
      // select the content
      var currentFocus = document.activeElement;
      target.focus();
      target.setSelectionRange(0, target.value.length);

      // copy the selection
      var succeed;
      try {
      	  succeed = document.execCommand("copy");
      } catch(e) {
          succeed = false;
      }
      // restore original focus
      if (currentFocus && typeof currentFocus.focus === "function") {
          currentFocus.focus();
      }

      if (isInput) {
          // restore prior selection
          elem.setSelectionRange(origSelectionStart, origSelectionEnd);
      } else {
          // clear temporary content
          target.textContent = "";
      }
      return succeed;
    }

    function linkkey() {
             if (document.getElementById('copy').value == "Copy Link") {
              document.getElementById('copy').value = "Shrink Link";
              document.getElementById('copy').style.color = "inherit";
              document.getElementById('msg').innerHTML = "";
             }
         }

     $(function(){
      $("#surl").submit(function(){
       var link=$("input[name=link]").val();
       if (document.getElementById('copy').value == "Copy Link") {
         copyToClipboard(document.getElementById('link'));
         document.getElementById('link').value = "";
         document.getElementById('copy').value = "Shrink Link";
         document.getElementById('copy').style.color = "inherit";
         document.getElementById('msg').innerHTML = "";
         return false;
       }

       $.post("https://winx.kr/lib/create_shortlink.php",{"owner": '', "link":link, "ad":"true"},function(data){
				 if ("fail_url" == data) {
					 document.getElementById('link').value = "";
	         document.getElementById('copy').value = "Shrink Link";
					 document.getElementById('msg').style.color = "#F05050";
					 document.getElementById('msg').innerHTML = "<i class=\"fa fa-times\"></i> Link format invalid.";
				 }
				 else if ("fail_host" == data) {
					 document.getElementById('link').value = "";
	         document.getElementById('copy').value = "Shrink Link";
					 document.getElementById('msg').style.color = "#F05050";
					 document.getElementById('msg').innerHTML = "<i class=\"fa fa-times\"></i> You can't shrink Winx link.";
				 }
				 else {
					 document.getElementById('link').value = data;
	         document.getElementById('copy').value = "Copy Link";
	         document.getElementById('copy').style.color = "#50F200";
					 document.getElementById('msg').style.color = "white";
	         document.getElementById('msg').innerHTML = "<i class=\"fa fa-copy\"></i> Link shrinked!";
				 }
       });

       return false;
      });
     });
    </script>
