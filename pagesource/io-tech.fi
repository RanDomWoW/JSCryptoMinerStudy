<script type="text/javascript">
//-----------------------------------------
// View: Uutiset &#8211; etusivu - start
//-----------------------------------------
'use strict';

document.addEventListener('DOMContentLoaded', function () {
  // if (window.location.search !== '?demo') {
    // return false;
  // }

  var STORE = 'iotech';
  var store = JSON.parse(localStorage.getItem(STORE) || '{}');
  var set = function set(key, value) {
    try {
      store[key] = value;
      localStorage.setItem(STORE, JSON.stringify(store));

      return true;
    } catch (e) {
      console.error(e); // Must be Safari.
      return false;
    }
  };

  var get = function get(key) {
    return store[key] || false;
  };

  var el = function el(tagName, options) {
    var element = document.createElement(tagName);
    var propMap = ['textContent', 'onclick'];

    Object.keys(options).forEach(function (key) {
      if (propMap.indexOf(key) > -1) {
        element[key] = options[key];
      } else {
        element.setAttribute(key, options[key]);
      }
    });

    return element;
  };

  var previewCSS = '.view-toggle-element .view-preview {\n    display: flex !important; /* Override inline */\n  }\n\n  .view-toggle-element .view-full {\n    display: none;\n  }';

  var fullCSS = '';
  var styleEl = document.createElement('style');
  var setCSS = function setCSS(css) {
    return styleEl.textContent = css;
  };
  document.body.appendChild(styleEl);

  var changeViewTo = function changeViewTo(target) {
    switch (target) {
      case 'Esikatselu':
        {
          setCSS(previewCSS);
          break;
        }

      case 'Täysi':
        {
          setCSS(fullCSS);
          break;
        }
    }
  };

  var viewToggleHandler = function viewToggleHandler(e) {
    var choice = get('view');

    if (choice === e.target.title) {
      return; // Don't do anything.
    }

    viewBtns.filter(function (btn) {
      return btn !== e.target;
    }).forEach(function (btn) {
      return btn.classList.remove('active');
    });
    e.target.classList.add('active');

    changeViewTo(e.target.title);
    set('view', e.target.title);
  };

  var viewToggle = el('div', { 'class': 'view-toggle' });
  var fullBtn = el('a', {
    role: 'button',
    'class': 'fa fa-bars',
    title: 'Täysi',
    onclick: viewToggleHandler
  });
  var previewBtn = el('a', {
    role: 'button',
    'class': 'fa fa-list-ul',
    title: 'Esikatselu',
    onclick: viewToggleHandler
  });
  var viewBtns = [fullBtn, previewBtn];

  viewToggle.appendChild(fullBtn);
  viewToggle.appendChild(previewBtn);

  var saveFilterChoice = function saveFilterChoice(e) {
    var filter = e.target;
    var choice = filter.textContent;
    var view = get('view');

    if (get('filter') !== choice) {
      set('filter', choice);

      changeViewTo(view);
    }
  };

  var filterContainer = document.querySelector('.post-body .filtteri');
  var filters = filterContainer.querySelectorAll('.radios-taxonomies-title');
  // var attachFilterListeners = function attachFilterListeners() {
    // filters and filtersContainer are stale after selecting a filter
  //  [].forEach.call(document.querySelectorAll('.post-body .filtteri .radios-taxonomies-title'), function (filter) {
      // filter.addEventListener('click', saveFilterChoice);
  //  });
  // };

  // attachFilterListeners();
  // setInterval(attachFilterListeners, 100); // The "filters" may get replaced at any time.
  
  // Improvement!! No need to do expensive ops 10 times a second with this. 
  window.addEventListener('click', function(e) {
	if (e.target.classList.contains('radios-taxonomies-title')) {
  	  saveFilterChoice(e);
	}
  })

  var filterChoice = get('filter');
  [].forEach.call(filters, function (filter) {
    if (filterChoice) {
      if (filter.textContent === filterChoice) {
        // filter.previousElementSibling.setAttribute('checked', true); // We need to trigger the event :(
        filter.click();
      } else {
        filter.previousElementSibling.removeAttribute('checked');
      }
    }
  });

  var viewChoice = get('view');
  var defaultChoice = 'Täysi';

  if (viewChoice) {
    changeViewTo(viewChoice);
  }

  [].forEach.call(viewBtns, function (btn) {
    var choice = viewChoice || defaultChoice;
    if (btn.title === choice) {
      btn.classList.add('active');
    }
  });

  document.querySelector('#post-65 .col-lg-8').insertAdjacentElement('afterbegin', viewToggle);
});


//-----------------------------------------
// View: Uutiset &#8211; etusivu - end
//-----------------------------------------
</script>
