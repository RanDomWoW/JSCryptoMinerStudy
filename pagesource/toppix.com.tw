                <a class="fast-slick-left arrow-left" href="#">
                    <i class="icon-svg icon-arrow-left-white icon-arrow-left-white-dims"></i>
                </a>
                <a class="fast-slick-right arrow-right" href="#">
                    <i class="icon-svg icon-arrow-right-white icon-arrow-right-white-dims"></i>
                </a>
            </div>
        </div>
        <!-- ./ item slide -->
        <div class="item-content padding-top-40">
            {% if (o.Label.length !== 0){ %}
            <div class="overflow-hidden margin-bottom-6">
                <div class="pull-left">
                    {% for (var i=0; i<o.Label.length; i++) { %}
                        <span class="tag-sm tag-{%=o.Label[i].Style%} uppercase">{%=o.Label[i].Description%}</span>
                    {% } %}
                </div>
            </div>
            {% } %}
            <div class="item-name font-24">{%=o.Name%}</div>

            {% if (o.Events.length !== 0){ %}
                {% for (var i=0; i<o.Events.length; i++) { %}
                    <div class="red margin-bottom-10 font-12 bold inline-block margin-right-5">{%=o.Events[i].Name%}</div>
                {% } %}
            {% } %}
            
            <div class="item-price font-24 margin-bottom-20">
                {% if (o.OriginPrice==o.SellPrice){ %}
                <div class="origin-price inline-block font-19">NT${%=o.OriginPrice%}</div>
                {% }else{ %}
                <div class="origin-price line-through inline-block font-19">NT${%=o.OriginPrice%}</div>
                <div class="discount-price inline-block font-24">NT${%=o.SellPrice%}</div>
                {% } %}
            </div><!-- /.item price -->

            <div class="item-color margin-bottom-20">
                <div class="color-txt font-12 margin-bottom-5">COLOR:{%=o.ActiveColor%}</div>
                {% for (var i=0; i<o.Colors.length; i++) { %}
      <div class="color-chose" data-id="{%=o.Colors[i].ID%}">
        <div class="color-pattern">
          <div class="pattern">
            <img src="{%=o.Colors[i].PintSizePictureUrl%}?h=12&w=12&mode=crop" alt="">
          </div>
        </div>
      </div>
      {% } %}
            </div><!-- /.item color -->

            <div class="item-size margin-bottom-20">
                <div class="font-12 margin-bottom-5">尺寸:</div>
                <div class="size-select">
                    {% for (var i=0; i<o.ActiveSpecs.length; i++) { %}
        <div class="size-opt-block">
          {% if (o.ActiveSpecs[i].Stock <= 0){ %}
            <div class="size-chose sold-out" data-stock="{%=o.ActiveSpecs[i].Stock%}" data-value="{%=o.ActiveSpecs[i].Key%}">{%=o.ActiveSpecs[i].Name%}</div>
          {% }else{ %}
            <div class="size-chose" data-stock="{%=o.ActiveSpecs[i].Stock%}" data-value="{%=o.ActiveSpecs[i].Key%}">{%=o.ActiveSpecs[i].Name%}</div>
          {% } %}
          {% if (o.ActiveSpecs[i].Description !== ''){ %}
          <div>{%=o.ActiveSpecs[i].Description%}</div>
          {% } %}
        </div>
      {% } %}
                </div>
            </div>

            <div class="margin-bottom-10">
                <div class="row">
                    <div class="col-xs-5">
                        <div class="count-chose width-full clearfix">
                            <a class="minus-num" href="#">－</a>
                            <span class="count-num">1</span>
                            <a class="add-num" href="#">＋</a>
                        </div>
                    </div>
                    <div class="col-xs-7">
                        {% if (o.IsPreShow){ %}
                        <a class="btn-add-bag disabled btn btn-gray btn-block uppercase not-allow" href="#" data-custommarketid="{%=o.custommarketid%}" data-sellprice="{%=o.SellPrice%}">coming soon</a>
                        {% } else if (o.noStock) { %}
                        <a class="btn-add-bag disabled btn btn-gray btn-block uppercase" href="#" data-custommarketid="{%=o.custommarketid%}" data-sellprice="{%=o.SellPrice%}">此商品各尺寸暫無庫存</a>
                        {% }else{ %}
                        <a class="btn-add-bag btn btn-gray disabled btn-block uppercase" href="#" data-custommarketid="{%=o.custommarketid%}" data-sellprice="{%=o.SellPrice%}">請選擇尺寸</a>
                        {% } %}
                    </div>
                </div>
            </div>
        </div><!-- ./ item content -->
    </script>
<script type="text/x-tmpl" id="tmpl-item-grid">
        <div class="item-grid">
            <div class="grid-inner">
                <div class="item-img">
                    <a href="{%=o.Url%}">
                        <picture>
                            <source type="image/webp" srcset="{%=o.Cover%}&f.sharpen=50&quality=78&format=webp">
                            <img class="img-responsive" src="{%=o.Cover%}&f.sharpen=50" alt="{%=o.Name%}">
                        </picture>
                    </a>
                    <div class="item-mask">
                        <a class="mask-link" href="{%=o.Url%}"></a>
                        <div class="item-social">
                            <div class="item-social-inner">
                                <div class="share-btns">
                                    {% if (o.IsFavorite == true){ %}
                                    <a class="btn-like item-like liked" href="#" data-custommarketid="{%=o.custommarketid%}">
                                        {% }else{ %}
                                        <a class="btn-like item-like" href="#" data-custommarketid="{%=o.custommarketid%}">
                                            {% } %}
                                            <i class="icon-svg icon-grid-heart icon-grid-heart-dims like-icon"></i>
                                        </a>
                                        <a class="btn-fast-buy btn-buy" href="#" data-toggle="modal" data-target="#lb-item-cart" data-custommarketid="{%=o.custommarketid%}">
                                            <i class="icon-svg icon-grid-bag icon-grid-bag-dims"></i>
                                        </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="item-content">
                    <div class="item-description">
                        <div class="tag-block overflow-hidden margin-bottom-6">
                            <div class="pull-left">
                                {% if (o.Label.length !== 0){ %}
                                    {% for (var i=0; i<o.Label.length; i++) { %}
                                        <span class="tag-sm tag-{%=o.Label[i].Style%} uppercase">{%=o.Label[i].Description%}</span>
                                    {% } %}
                                {% } %}
                            </div>
                        </div>
                        <div class="item-name">
                            <a class="text-gray" href="{%=o.Url%}">
                                {%=o.Name%}
                            </a>
                        </div>
                        <div class="item-price">
                            {% if (o.OriginPrice==o.SellPrice){ %}
                            <div class="origin-price inline-block">
                                <span>NT${%=o.OriginPrice%}</span>
                            </div>
                            {% }else{ %}
                            <div class="origin-price line-through inline-block margin-right-10">
                                <span>NT${%=o.OriginPrice%}</span>
                            </div>
                            <div class="discount-price inline-block">
                                <span>NT${%=o.SellPrice%}</span>
                            </div>
                            {% } %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </script>

