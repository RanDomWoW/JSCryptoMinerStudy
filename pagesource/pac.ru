<script type="text/javascript">
(function(){
	var buttons=window.buttons = $('#header_banner SPAN');
	var interval;
	var type_banner = 4;

	var obj_css_show = {'display' : '', 'z-index' : '1'};
	var obj_css_hide = {'display' : 'none', 'z-index' : '1'};
  
	if(buttons.length < 2)
	{
		$('#header_banner A').css(obj_css_show);
		return;
	}

	function stopEvent(e)
	{
		if(!e) var e = window.event;

		e.cancelBubble = true;
		e.returnValue = false;

		if ( e.stopPropagation ) e.stopPropagation();
		if ( e.preventDefault ) e.preventDefault();

		return false;
	}

	var start = function()
	{
		if(interval)
			return;

		interval = setInterval(function(){
			var span = buttons.filter('.selected').next();
			if(!span.length)
				span = buttons.eq(0);
			span.trigger('click');
		}, 10000);
	};

	var stop = function()
	{
		if(!interval)
			return;

		clearInterval(interval);
		interval = null;
	};

	$('#header_banner').hover(stop, start);

	buttons.bind('click', function(e)
	{
		stopEvent(e);
		var current = buttons.filter('.selected');
		var span    = $(this),
			num     = span.prevAll().length,
			prevNum = current.length ? current.prevAll().length : (num == 0 ? buttons.length - 1 : num - 1),
			curImg  = $('#header_banner A').eq(num),
			prevImg = $('#header_banner A').eq(prevNum);

		if(current.length)
		{
			current.removeClass('selected');
			span.addClass('selected');
									 
			prevImg.css(obj_css_hide);
			curImg.css(obj_css_show);
		}
		else
		{
			span.addClass('selected');
			curImg.css(obj_css_show);
		}
	});

	buttons.eq(random(0, buttons.length-1)).trigger('click');
	var cur = buttons.filter('.selected').prevAll().length;
	$('#header_banner A').css(obj_css_hide);
	$('#header_banner A').eq(cur).css(obj_css_show);
	start();
})();
</script>

		
