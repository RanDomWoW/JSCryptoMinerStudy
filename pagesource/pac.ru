<script type="text/javascript">
	function check_selects(index)
	{
		if (!index) index = 0;
		var count = active_obj.length - 1;
		for (var i = index; i < count; ++i)
		{
			if (is_selected(i))
				make_select(i+1);
			else
				deactivate(i+1);
		}
	}

	function is_selected(index)
	{
		var selected = jQuery('#' + active_obj[index] + ' OPTION:selected');
		if (selected.val() == -1)
			return false;
		return true;
	}

	function get_data(index)
	{
		var str_values = '';
		for (var i = 0; i < index; ++i)
		{
			if (str_values != '')
				str_values += '&';
			var key   = active_obj[i];
			var value = jQuery('#' + key + ' OPTION:selected').val();
			str_values += key + '=' + value;
		}

		jQuery.ajax({
			url: '/modules/mod_quick_search/server.php',
			data: str_values,
			dataType:'json',
			async: true,
			success: function(data){
				fill_selects(data);
			},
			error: function () { console.log('error ajax');}
		});
	}

	function fill_select(id_select, obj)
	{
		var selected_prev = jQuery('#' + id_select + ' OPTION:selected').val();
		var selected_prev_text = jQuery('#' + id_select + ' OPTION:selected').text();
		var first_option = '<option value="-1">'+jQuery('#'+id_select + ' OPTION:first').text() + '</option>';
		var options = '';
		var isset = false;
		for (var k in obj)
		{
			if(selected_prev && selected_prev!=-1 && selected_prev==k)
				isset = true;

			options += '<option value="'+k+'"'+ ((selected_prev && selected_prev!=-1 && selected_prev==k)  ? ' selected="selected"' : '') +'>'+obj[k]+'</option>';
		}

		var list = jQuery('#'+id_select).empty();
		list.append(jQuery(options)
			.toArray()
			.sort(function(a, b) {
				var at = jQuery(a).text(), bt = jQuery(b).text();
				return at < bt ? -1 : at > bt ? 1 : 0;
		}));
		jQuery('#'+id_select).prepend(first_option)

		if (isset)
			jQuery('#'+id_select).next().find('SPAN').text(selected_prev_text);
		else
			clear_select(0, id_select);
	}

	function fill_selects(data)
	{
		for (var j in data['options'])
		{
			var id_select = j;
			var obj  = data['options'][j];

			fill_select(id_select, obj);
		}
	}

	function make_select(index)
	{
		activate(index);
		get_data(index);
	}

	function activate(index)
	{
		jQuery("#" + active_obj[index]).removeAttr('disabled').removeClass('disabled').next('DIV').removeClass('disabled');
	}

	function empty_select(id)
	{
		var text  = jQuery("#" + id + ' OPTION:first').attr('selected', 'selected').text();
		jQuery('#' + id).empty().append('<option value="-1">'+text+'</option>');
		jQuery('#' + id).next().find('span').text(text);
	}

	function clear_select(index, id)
	{
		var id = index ? active_obj[index] : id;
		jQuery('#' + id + ' OPTION:selected').removeAttr('selected');
		var text  = jQuery("#" + id + ' OPTION:first').attr('selected', 'selected').text();
		jQuery('#' + id).next().find('span').text(text);
	}

	function deactivate(index)
	{
		clear_select(index);
		jQuery("#" + active_obj[index]).attr('disabled', 'disabled').addClass('disabled').next('DIV').addClass('disabled');
	}

	function show_error_window(msg)
	{
		var msg = '<p id="quick_search_error_msg">'+msg+'</p>';
		jQuery('#quick_search_error').fancybox({content: msg}).trigger('click');
	}

	function doTrackEvent (obj) {
		var country_city = [];
		var country_name = jQuery('#countries_selector H3').text();
		var city_name = jQuery('#dpt_city_select SELECT option:selected').text();
		country_city.push(country_name ? country_name : 'not set');
		country_city.push(city_name);

		var params = [];
		jQuery('select, input', obj).each(function () {
			var name = jQuery(this).attr('name');

			var value_obj = jQuery('option:selected', this);
			var value = value_obj.text();
			if (jQuery(value_obj).val() == -1)
				value = 'not set';

			if (value_obj.length == 0)
				value = jQuery(this).val();
			params.push(name+'='+value);
		});

		trackEvent('TourSearchSmall', country_city.join(', '), params.join(', '));
	}

	function activate_default (index)
	{
		var selected = jQuery('#' + active_obj[index] + ' OPTION:selected');
		if (selected.val() == -1) // не выбран город вылета
			$('#'+active_obj[index]).val("0").change();
	}
	
	
	var active_obj = ['qs-departFrom', 'qs-country', 'qs-tourtype'];
	var error_text = {'qs-departFrom' : 'Укажите город вылета!', 'qs-country':'Укажите страну!', 'qs-tourtype':'Укажите тип тура'};

	jQuery('#quick_search SELECT').each(function()
	{
		var text = this.options[this.selectedIndex].text;
		var clas = $(this).attr('class');
		jQuery(this)
			.after('<div class="qs-select '+clas+'"><span>'+text+'</span><div class="arrow-down"></div></div>')
			.change(function(){
				var text = this.options[this.selectedIndex].text;
				jQuery(this).next().find('SPAN').text(text);
			});
	});
	
	activate_default(0);
	if (is_selected(0))
		activate(0);
	check_selects();

	jQuery.each(active_obj, function(i, val) {
		jQuery('#' + val).change(function () {
			check_selects(i);
		});
	});
	jQuery("#quick_search FORM").submit(function(){
		for(var i in error_text)
		{
			if(jQuery('#'+ i).is('.disabled') || jQuery('#'+ i + ' option:selected').val() == -1)
			{
				show_error_window(error_text[i]);
				return false;
			}
		}
		doTrackEvent(this);
	});


</script>
