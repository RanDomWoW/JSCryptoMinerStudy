		var span    = $(this),
			num     = span.prevAll().length,
			prevNum = current.length ? current.prevAll().length : (num == 0 ? buttons.length - 1 : num - 1),
			curImg  = $('#top_banner A').eq(num),
			prevImg = $('#top_banner A').eq(prevNum);

		if(current.length)
		{
			current.removeClass('selected');
			span.addClass('selected');
									 
			prevImg.css(obj_css_hide);
			curImg.css(obj_css_show);
		}
		else
		{
			span.addClass('selected');
			curImg.css(obj_css_show);
		}
	});

	buttons.eq(random(0, buttons.length-1)).trigger('click');
	var cur = buttons.filter('.selected').prevAll().length;
	$('#top_banner A').css(obj_css_hide);
	$('#top_banner A').eq(cur).css(obj_css_show);
	start();
})();
</script>

