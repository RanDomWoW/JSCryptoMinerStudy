                if($('html').hasClass('mobile')){
                    inst.dpDiv.css({left: 0, "z-index": 2000});
                }else{
                    inst.dpDiv.css({"z-index":2000});
                }
            }, 0);
        }
    });

    $('#datePickerBookTabCheckOut').datepicker({
        showOn: 'button',
        buttonImage: '/css/images/ico-calendar-booktab.png',
        buttonImageOnly: true,
        buttonText: 'Select date',
        minDate: 0,
        firstDay: 0,
        dateFormat: 'dd/mm/yy',
        onClose: function(selectedDate) {
            if(selectedDate != '') {
                selectedDate = datepickerStrChangeDate(selectedDate, -1);
                $('#datePickerBookTabCheckIn').datepicker('option', 'maxDate', selectedDate);
            }
        },
        beforeShow: function (input, inst) {
            var rect = input.getBoundingClientRect();
            setTimeout(function () {
                if($('html').hasClass('mobile')){
                    inst.dpDiv.css({left: 0, "z-index": 2000});
                }else{
                    inst.dpDiv.css({"z-index":2000});
                }
            }, 0);
        }
    });

    $('#btnHotelBook').on('click', function(){
        gtmTracking({'event': 'bookNow','productName': 'HOTEL','pageCategory': 'Hotel'});

        var noOfRooms = $('#bookRooms').val();
        var noOfAdults = $('#bookAdults').val();
        var noOfChildren = $('#bookChildren').val();
        var checkInDate = $('#datePickerBookTabCheckIn').val();
        var checkOutDate = $('#datePickerBookTabCheckOut').val();
        var today = new Date();

        if(checkInDate != '') {
            checkInDate = checkInDate.split('/')
        }
        else {
            checkInDate = [today.getDate(), today.getMonth() + 1, today.getFullYear()];
        }

        if(checkOutDate != '') {
            checkOutDate = checkOutDate.split('/')
        }
        else {
            checkOutDate = new Date(checkInDate[2], checkInDate[1] - 1, checkInDate[0]);
            checkOutDate.setDate(checkOutDate.getDate() + 1);
            checkOutDate = [checkOutDate.getDate(), checkOutDate.getMonth() + 1, checkOutDate.getFullYear()];
        }

        var param = {
            checkinYear: parseInt(checkInDate[2]), checkinMonth: parseInt(checkInDate[1]), checkinDay: parseInt(checkInDate[0]),
            checkoutYear: parseInt(checkOutDate[2]), checkoutMonth: parseInt(checkOutDate[1]), checkoutDay: parseInt(checkOutDate[0]),
            numberOfNights: 1, numberOfRooms: parseInt(noOfRooms), numberOfAdults: parseInt(noOfAdults), numberOfChildren: parseInt(noOfChildren),
            owsLanguageCode: 'EN'
        };

        var url = 'http://mfstciwhr.webhotel.microsapdc.com/bp/search_rooms.cmd?' + $.param(param);
        window.open(url);
    });

});
