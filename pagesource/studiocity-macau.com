    <script type="text/javascript">
/*<![CDATA[*/
jQuery(function($) {
jQuery('#yw0').after("<input type=\"button\" class=\"btnRefrash\" value=\"\" id=\"yw0_button\" name=\"yt0\" \/>");
jQuery(document).on('click', '#yw0_button', function(){
	jQuery.ajax({
		url: "\/en\/enews\/captcha?refresh=1",
		dataType: 'json',
		cache: false,
		success: function(data) {
			jQuery('#yw0').attr('src', data['url']);
			jQuery('body').data('enews/captcha.hash', [data['hash1'], data['hash2']]);
		}
	});
	return false;
});


    //enews form
    var loadingEnewsForm = false;
    
    $('#enews_country').on('change', function() {
        if($(this).val() == 3) {
            $('.tableCol.region').removeClass('disable');
        }
        else {
            $('.tableCol.region').addClass('disable');
        }        
    });
    
    $('#submitEnews').on('click', function() {
//         initEnewsVersaTag();
        

        if (loadingEnewsForm)
            return;

        setEnewsMessage('', true);
        var hasError = false;
        var container = $('.enewsContainer');
        container.find('.error').removeClass('error');
        
        //validation
        var requiredIds = ['enews_first_name', 'enews_last_name', 'enews_email', 'enews_captcha'];
        var errorMsg = '';
        
        for(var i = 0; i < requiredIds.length; i++) {
            var requiredId = requiredIds[i];            

            if ($('#' + requiredId).val().trim() == '') {
                hasError = true;
                $('#' + requiredId).addClass('error');
            }
        }
		
        var day_birth = $('#dob_d').val(),
            month_birth = $('#dob_m').val(),
            year_birth = $('#dob_y').val();
			
        if( day_birth == '') {
            hasError = true;
            container.find('.dobD').addClass('error');
        }
        if( month_birth == '') {
            hasError = true;
            container.find('.dobM').addClass('error');
        }
        if( year_birth == '') {
            hasError = true;
            container.find('.dobY').addClass('error');
        }
        
        if (!isEmail($('#enews_email').val())) {
            hasError = true;
            $('#enews_email').addClass('error');
        }
        
        var country = $('#enews_country').val();
        if (country == '') {
            hasError = true;
            $('.tableCol.region .transformSelect:eq(0)').find('span').first().addClass('error');
        }
        else if(country == 3) {
            var province = $('#enews_province').val();
            if (province == '') {
                hasError = true;
                $('.tableCol.region .transformSelect:eq(1)').find('span').first().addClass('error');
            }
        }

        var mobileAreaCode = $('#enews_mobile_area_code').val(),
            mobile = $('#enews_mobile').val();

        if (mobileAreaCode == '' || !isDigit(mobileAreaCode)) {
            hasError = true;
            $('#enews_mobile_area_code').addClass('error');
        }
        if (mobile == '' || !isDigit(mobile)) {
            hasError = true;
            $('#enews_mobile').addClass('error');
        }

        if (hasError) {
            errorMsg += 'Please fill in all the mandatory fields<br />';            
        }
        
        if (!$('#enews_tnc').is(':checked')) {
            errorMsg += 'Please accept Privacy Statement and Terms and Conditions';
            hasError = true;
            container.find('.inputTnC').addClass('error');
        }
        
        enewsGTMTracking(!hasError);
        
        if(hasError) {
            setEnewsMessage(errorMsg, false);
            return;
        }

        loadingEnewsForm = true;

        $.ajax({
            url: '/en/enews/submit',
            type: 'POST',
            data: $('#eNewsForm').serialize(),
            dataType: 'json'
        }).done(function(data) {
            enewsRateGTMTracking(data.success);
            if (data.success) {                
                resetEnewsForm();
                $('#submitEnews').hide();
                container.find('.btnRefrash').trigger('click');
                enewsTracking('http://studiocity-macau.com/en/enews/success');
            }
            setEnewsMessage(data.message, data.success);
        }).fail(function() {
            setEnewsMessage('Fail to submit, please try again later', false);
        }).always(function() {
            loadingEnewsForm = false;            
        });
    });

    function setEnewsMessage(message, isSuccess) {
        if (isSuccess) {
            $('#enews_msg').hide().html('');
            $('#enews_thankyou_msg').html(message);            
            $('#enews_thankyou_msg').show();
        }
        else {
            $('#enews_msg').css('color', 'red').html(message).show();
        }
    }

    function resetEnewsForm() {
        var form = $('#eNewsForm');
        form[0].reset();
    }
    
    function enewsGTMTracking(status) {
        gtmTracking({'event': 'eNewsSubmission','pageCategory': 'Homepage','status': status});
    }
    
    function enewsRateGTMTracking(status) {
        gtmTracking({'event': 'newsletterSubscription','pageCategory': 'Homepage','status': status});
    }


    $('#datePickerBookTabCheckIn').datepicker({
        showOn: 'button',
        buttonImage: '/css/images/ico-calendar-booktab.png',
        buttonImageOnly: true,
        buttonText: 'Select date',
        minDate: 0,
        firstDay: 0,
        dateFormat: 'dd/mm/yy',
        onClose: function(selectedDate) {
            if(selectedDate != '') {
                selectedDate = datepickerStrChangeDate(selectedDate, 1);
                $('#datePickerBookTabCheckOut').datepicker('option', 'minDate', selectedDate);
                setTimeout(function(){ $('#datePickerBookTabCheckOut').parent().find('img').click(); }, 100);
            }
        },
        beforeShow: function (input, inst) {
            var rect = input.getBoundingClientRect();
