            <script>
        $.fn.placeholder = function() {
            if(typeof document.createElement("input").placeholder == 'undefined') {
                $('[placeholder]').focus(function() {
                    var input = $(this);
                    if (input.val() == input.attr('placeholder')) {
                        input.val('');
                        input.removeClass('placeholder');
                    }
                }).blur(function() {
                    var input = $(this);
                    if (input.val() == '' || input.val() == input.attr('placeholder')) {
                        input.addClass('placeholder');
                        input.val(input.attr('placeholder'));
                    }
                }).blur().parents('form').submit(function() {
                    $(this).find('[placeholder]').each(function() {
                        var input = $(this);
                        if (input.val() == input.attr('placeholder')) {
                            input.val('');
                        }
                    })
                });
            }
        }

        $.fn.required = function() {
            if(typeof document.createElement("input").required == 'undefined') {
                $('form').submit(function() {
                    var faults = $(this).children('input').filter(function(){
                        if($(this).attr('required') && $(this).val() === "") {
                            $(this).css("border-color","red");
                            $(this).blur();
                            return true;
                        } else {
                            $(this).css("border-color","");
                            $(this).blur();
                            return false;
                        }
                    });

                    if(faults.length) return false; // if any required items are empty, do not submit
                });
            }
        }

        function updateSearchBarText(inputValue) {

            // display advertising for certain items
            if( $.inArray(inputValue.toLowerCase(), ['marriage_divorce','birth_death','employment'] ) >= 0 ) {
                $('.searchBar .advertising').html("Advertising");
            }
            else {
                $('.searchBar .advertising, #mobileAdvertising').html('<img src="/assets/ads/powered-by-intelius-c132935c3792fb443070d73a9f60b421.png" alt="Powered by intelius" /> ');
            }

            switch(inputValue.toLowerCase()) {
                case "employment":
                    $('#typeOfSearch').html("For Hiring");
                    break;

                case "marriage_divorce":
                    $('#typeOfSearch').html("Marriage & Divorce Records");
                    break;

                case "birth_death":
                    $('#typeOfSearch').html("Birth & Death Records");
                    break;

                default:
                    $('#typeOfSearch').html(inputValue.toLowerCase() + " Records");
            }
        }

        function appendOnce(string, appendage) {
            return (string.indexOf(appendage) == -1 ? string + appendage : string);
        }

        ////////////////////////////////////////////////////////////////////////

        $(function() {
            continueAfterSplit();

            $.fn.placeholder();
            $.fn.required();

            recordTypeSetupOnLoad(window.location.href);
            
            // add click event to every record type radio button
            $('input:radio[name=record_type]').click(function(){
                // Sync selected option across other elements
                onRadioButtonClickSyncRecordTypes($(this).val());
            });

            // add change event to every record type dropdown
            $('.record_type_dropdown').on('change', function() {
                // Sync selected option across other elements
                onDropdownChangeSyncRecordTypes($(this).val());
            });

            // add change event on state dropdown
            $('.state_dropdown').on('change', function() {
                var dropdownValue = $(this).val();

                // Update other instances of the same dropdown
                $('.state_dropdown').each(function() {
                    if($(this).val() != dropdownValue)
                        $(this).val(dropdownValue).change();
                });
            });

            collapseSearchBar();
            

            var $advertisementId = $('#advertisementId');


            function continueAfterSplit() {
                // running once immediately to make sure that the search box displays
                // correctly if the user gets to this page via the back button
                updateSearchBarText( $('input:radio[name=record_type]:checked').val() );

                $('.searchBar form').submit(function(e) {
                    // Validate city state field
                    var cityStateElement = $(e.target).find("#cityState");
                    if(cityStateElement.prop("required")) {
                        var stateIsValid = validateState(cityStateElement);
                        if(!stateIsValid) {
                            $(e.target)[0].reportValidity();
                            return false;
                        }
                    }

                    var $totallyNotARobot = $('.self-search input[type="checkbox"]');
                    if($totallyNotARobot.prop('checked')) {
                        return;
                    }

                    var $recordTypes = $('#recordTypes input');
                    var $selectedRecordType = $recordTypes.filter(':checked');

                    var recordType = $selectedRecordType.val();
                    var eventString = recordType[0].toUpperCase() + recordType.slice(1);

                    // Add adword value to the form
                    var adword = "desktop.searchbox";
                    if($(document).width() <= 768) {
                        adword = "mobile.searchbox";
                    }

                    $(this).append('<input type="hidden" name="adword" value="' + adword + '" />');


                    if(recordType == 'employment') {
                        // Send GA event to different category when recordType is 'employment'
                        var eventCategory = "GoodHire Links";
                        var eventString = "GoodHire_Employment_Search";

                        ga('send', 'event', eventCategory, eventString);
                    } else {
                        if( $.inArray( recordType, ['marriage_divorce','birth_death'] ) >= 0 ) {
                            eventString = 'Archives_' + eventString;
                        } else {
                            eventString = 'Intelius_' + eventString;
                        }

                            //console.log( "ga('send', 'event', 'PremiumSearch', '" + eventString + "', '');" );
                            ga('send', 'event', 'PremiumSearch', eventString, '');
                    }
                });
            }


            ////////////////////////////////////////////////////////
            // Utility functions

            function updateRequiredSearchBarFields(recordType) {
                var publicInputs = ["input[id='firstName']", "input[id='lastName']"];
                var propertyInputs = ["input[id='propertyAddress']", "input[id='cityState']"];

                var removeRequired;
                var addRequired;

                if(recordType == 'property') {
                    removeRequired = publicInputs;
                    addRequired = propertyInputs;
                } else {
                    removeRequired = propertyInputs;
                    addRequired = publicInputs;
                }

                // Remove required attribute from fields
                for(var i = 0; i < removeRequired.length; i++) {
                    $(removeRequired[i]).each(function() {
                        if($(this).prop('required'))
                            $(this).removeAttr('required');
                    });
                }

                // Add required attribute to fields
                for(var i = 0; i < addRequired.length; i++) {
                    $(addRequired[i]).each(function() {
                        if(!$(this).prop('required'))
                            $(this).attr('required', '');
                    });
                }
            }

            function updateSearchBarTitle(inputValue) {
                var record_types = {
                    "criminal":         "Search Criminal Records by Name",
                    "marriage_divorce": "Search Marriage & Divorce Records",
                    "birth_death":      "Search Birth & Death Records",
                    "property":         "Search Property Records",
                    "employment":       "Search for Employment Screening",
                    "public":           "Search Public Records by Name"
                }

                if($.inArray(inputValue.toLowerCase(), Object.keys(record_types)) > -1) {
                    $('.searchBar').each(function() {
                        $(this).find('h2').text(record_types[inputValue.toLowerCase()]);
                    });
                }
            }

            function onRadioButtonClickSyncRecordTypes(inputValue) {
                var record_types = ["criminal", "marriage_divorce", "birth_death", "property", "employment", "public"];

                if($.inArray(inputValue.toLowerCase(), record_types) > -1) {
                    // Update dropdown
                    $(".record_type_dropdown").val(inputValue.toLowerCase()).change();

                    // Update other radiobuttons
                    $('.record_type').each(function() {
                        if($(this).val() == inputValue.toLowerCase() && !$(this).is(':checked')) {
                            $(this).prop("checked", true);
                        }
                    });

                    updateSearchBarText(inputValue);
                    updateRequiredSearchBarFields(inputValue.toLowerCase());
                    updateSearchBarTitle(inputValue);
                    flipPropertyAndPublicInputs();
                }
            }

            function onDropdownChangeSyncRecordTypes(inputValue) {
                var record_types = {
                    "criminal":         "recordTypeCriminal",
                    "marriage_divorce": "recordTypeMarriageDivorce",
                    "birth_death":      "recordTypeBirthDeath",
                    "property":         "recordTypeProperty",
                    "employment":       "recordTypeEmployment",
                    "public":           "recordTypePublic"
                }

                if($.inArray(inputValue.toLowerCase(), Object.keys(record_types)) > -1) {                 
                    // Update record type radio buttons
                    $('.record_type').each(function() {
                        if($(this).attr('id') == record_types[inputValue.toLowerCase()])
                            $(this).prop("checked", true);
                    });

                    // Update other instances of the same dropdown
                    $('.record_type_dropdown').each(function() {
                        if($(this).val() != inputValue.toLowerCase())
                            $(this).val(inputValue.toLowerCase()).change();
                    });

                    updateSearchBarText(inputValue);
                    updateRequiredSearchBarFields(inputValue.toLowerCase());
                    updateSearchBarTitle(inputValue);
                    flipPropertyAndPublicInputs();
                }
            }

            function recordTypeSetupOnLoad(currentLocation) {
                var record_types = {
                    "Marriage-Records":         "marriage_divorce",
                    "Divorce-Records":          "marriage_divorce",
                    "Death-Records":            "birth_death",
                    "Birth-Records":            "birth_death",
                    "Land-Records-and-Deeds":   "property",
                    "Foreclosures-and-Tax-Lien-Sales": "property",
                    "Assessor-and-Property-Tax-Records": "property",
                    "GIS-and-Mapping":          "property",
                    "Warrants":                 "criminal",
                    "Jail-and-Inmate-Records":  "criminal",
                    "Sex-Offender-Registration": "criminal",
                    "Courts":                   "criminal"
                }

                // Keep default value
                onRadioButtonClickSyncRecordTypes("public");

                var indexValue;
                var indexes = Object.keys(record_types);
                for(var i = 0; i < indexes.length; i++) {
                    indexValue = indexes[i];
                    if(currentLocation.indexOf(indexValue) > -1) {
                        onRadioButtonClickSyncRecordTypes(record_types[indexValue]);
                        break;
                    }
                }
            }

            // Limit number of function event calls
            function debouncer( func , timeout ) {
               var timeoutID , timeout = timeout || 5;
               return function () {
                  var scope = this , args = arguments;
                  clearTimeout( timeoutID );
                  timeoutID = setTimeout( function () {
                      func.apply( scope , Array.prototype.slice.call( args ) );
                  } , timeout );
               }
            }

            function validateState(inputElement) {
                var inputValue = inputElement.val();
                var errorMessage = "State is required";
                
                if(inputValue == null) {
                    inputElement.get(0).setCustomValidity(errorMessage);
                    return false;
                }
              
                var words = inputValue.toUpperCase().match(/\b(\w+)\b/g);
                var stateAbbr = ["AL", "AK", "AZ", "AR", "CA", "CO", "CT", "DE", "FL", "GA", "HI", "ID", "IL", "IN", "IA", "KS", "KY", "LA", "ME", "MD", "MA", "MI", "MN", "MS", "MO", "MT", "NE", "NV", "NH", "NJ", "NM", "NY", "NC", "ND", "OH", "OK", "OR", "PA", "RI", "SC", "SD", "TN", "TX", "UT", "VT", "VA", "WA", "WV", "WI", "WY"];
                var stateName = ["ALABAMA","ALASKA","ARIZONA","ARKANSAS","CALIFORNIA","COLORADO","CONNECTICUT","DELAWARE","FLORIDA","GEORGIA","HAWAII","IDAHO","ILLINOIS","INDIANA","IOWA","KANSAS","KENTUCKY","LOUISIANA","MAINE","MARYLAND","MASSACHUSETTS","MICHIGAN","MINNESOTA","MISSISSIPPI","MISSOURI","MONTANA","NEBRASKA","NEVADA","NEW HAMPSHIRE","NEW JERSEY","NEW MEXICO","NEW YORK","NORTH CAROLINA","NORTH DAKOTA","OHIO","OKLAHOMA","OREGON","PENNSYLVANIA","RHODE ISLAND","SOUTH CAROLINA","SOUTH DAKOTA","TENNESSEE","TEXAS","UTAH","VERMONT","VIRGINIA","WASHINGTON","WEST VIRGINIA","WISCONSIN","WYOMING"];
              
                if(words != null) {
                    var wordToEvaluate;
                    switch(words.length) {
                        case 1:
                            wordToEvaluate = words[0];
                            break;
                        case 2:
                            wordToEvaluate = words[1];
                            break;
                        default:
                            inputElement.get(0).setCustomValidity(errorMessage);
                            return false;
                    }
              
                    if($.inArray(wordToEvaluate, stateAbbr) > -1 ||
                        $.inArray(wordToEvaluate, stateName) > -1) {
                        return true;
                    }
                }
              
                inputElement.get(0).setCustomValidity(errorMessage);
                return false;
            }

