            ////////////////////////////////////////////////////////
            // Collapse/Expand functions

            function flipPropertyAndPublicInputs() {
                var property = $("#recordTypeProperty").is(":checked");

                if(property) {
                    $(".property-main-input").css('display', 'inline-block');
                    $(".non-property-main-input").css('display', 'none');

                    // Top searchbar
                    if($(".searchBarTop").find(".button-container").css('display') != "none") {
                        $(".searchBarTop").find(".property-input").each(function() { $(this).css('display', 'inline-block'); });
                        $(".searchBarTop").find(".non-property-input").each(function() { $(this).css('display', 'none'); });
                    }

                    $(".searchBarBottom").find(".property-input").each(function() { $(this).css('display', 'inline-block'); });
                    $(".searchBarBottom").find(".non-property-input").each(function() { $(this).css('display', 'none'); });

                } else {
                    $(".property-main-input").css('display', 'none');
                    $(".non-property-main-input").css('display', 'inline-block');

                    // Top searchbar
                    if($(".searchBarTop").find(".button-container").css('display') != "none") {
                        $(".searchBarTop").find(".property-input").each(function() { $(this).css('display', 'none'); });
                        $(".searchBarTop").find(".non-property-input").each(function() { $(this).css('display', 'inline-block'); });
                    }

                    $(".searchBarBottom").find(".property-input").each(function() { $(this).css('display', 'none'); });
                    $(".searchBarBottom").find(".non-property-input").each(function() { $(this).css('display', 'inline-block'); });

                }
            }

            function collapseSearchBar() {
                var collapsed = false;

                // Collapse search bar on load
                if($(document).width() <= 768) {
                    $(".searchBar").find("h2").each(function() { $(this).css('display', 'block') });
                    $(".mobile-expandable").each(function(){ $(this).css('display', 'none') });
                    $(".mobile-non-property-expandable").each(function(){ $(this).css('display', 'none') });
                    $(".mobile-property-expandable").each(function(){ $(this).css('display', 'none') });

                    collapsed = true;
                }

                // Collapse search bar on resize
                $(window).on('resize', function() {
                    var win = $(this);
                    if(win.width() <= 768) {
                        if(!collapsed) {
                            $(".searchBar").find("h2").each(function() { $(this).css('display', 'block') });
                            $(".mobile-expandable").each(function(){ $(this).css('display', 'none') });
                            $(".mobile-non-property-expandable").each(function(){ $(this).css('display', 'none') });
                            $(".mobile-property-expandable").each(function(){ $(this).css('display', 'none') });

                            if(!$("#firstName").hasClass("mobile-search-on"))
                                $("#firstName").addClass("mobile-search-on");

                            if(!$("#propertyAddress").hasClass("mobile-search-on"))
                                $("#propertyAddress").addClass("mobile-search-on");

                            collapsed = true;
                        }
                    } else {
                        if(collapsed) {
                            var property = $("#recordTypeProperty").is(":checked");

                            if(property) {
                                $(".searchBarTop").find(".property-input").each(function() { $(this).css('display', 'inline-block'); });
                                $(".searchBarTop").find(".non-property-input").each(function() { $(this).css('display', 'none'); });
                            } else {
                                $(".searchBarTop").find(".property-input").each(function() { $(this).css('display', 'none'); });
                                $(".searchBarTop").find(".non-property-input").each(function() { $(this).css('display', 'inline-block'); });
                            }

                            $(".searchBar").find("h2").each(function() { $(this).css('display', 'none') });
                            $(".mobile-expandable").each(function(){ $(this).css('display', 'inline-block') });

                            collapsed = false;
                        }
                    }
                });

                // Expand search bar if user clicks in the search bar area
                $(".searchBarTop, .searchBarTop>*").click(function() {
                    if($(document).width() <= 768) {
                        if($("#recordTypeProperty").is(":checked")) {
                            $(".mobile-property-expandable").show();
                        } else {
                            $(".mobile-non-property-expandable").show();
                        }

                        $(".mobile-expandable").show();

                        if($("#firstName").hasClass("mobile-search-on"))
                            $("#firstName").removeClass("mobile-search-on");

                        if($("#propertyAddress").hasClass("mobile-search-on"))
                            $("#propertyAddress").removeClass("mobile-search-on");
                    }
                });

                // Collapse if user clicks outside the search bar area
                /*$(document).click(function(e) {
                    if($(document).width() <= 768 &&
                      !$(e.target).is(".searchBarTop") &&
                      $(e.target).parents(".searchBarTop").length == 0) {
                        $(".mobile-expandable").each(function(){ $(this).css('display', 'none') });
                        $(".mobile-non-property-expandable").each(function(){ $(this).css('display', 'none') });
                        $(".mobile-property-expandable").each(function(){ $(this).css('display', 'none') });

                        if(!$("#firstName").hasClass("mobile-search-on"))
                            $("#firstName").addClass("mobile-search-on");

                        if(!$("#propertyAddress").hasClass("mobile-search-on"))
                            $("#propertyAddress").addClass("mobile-search-on");
                    }
                });*/
            }

            ////////////////////////////////////////////////////////  

        });
    </script>

