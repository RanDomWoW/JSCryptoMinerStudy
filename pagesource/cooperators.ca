<script type="text/javascript">
    $(document).ready(function () {
        his();
$( "#Go" ).click(function() {
  validateReq();
});

$('#postalCode').bind("enterKey",function(e){
validateReq();
});
$('#postalCode').keydown(function(e){
if(e.keyCode == 13)
{
e.preventDefault();
  $(this).trigger("enterKey");
}
});
    

    function Check(a) {
        if (a.value == "Enter your postal code") {
            a.value = ''; his();
        }
    }

    function UnCheck(a) {
        if (a.value == '') {
            a.value = 'Enter your postal code'; his();
        }
    }

    function validateReq() {
        var input = $('#postalCode');
        var re = "([a-zA-Z]\d[a-zA-Z]( )?\d[a-zA-Z]\d)|(\d{5}(-\d{4})?)";
        var is_postal = input.val().replace(/\s/g, '').match(re);
        if (!is_postal && (input.val().replace(/\s/g, '').length != 6) && input.val() != 'Enter your postal code'){ $('#postalCode').css('border', '1px solid red'); 
//alert('failed');
$('#PostalCodeRegularExpressionValidator').css('display', 'inline-block');
} else {
 $('#postalCode').css('border', '1px solid #ccc');
$('#PostalCodeRegularExpressionValidator').css('display', 'none');
//alert("passed");
window.location="/en/PublicPages/AgentLocator.aspx?postal="+input.val().replace(/\s/g, '');
}
    }

    function his() {
        $('#PostalCodeValidator').css('display', 'none');
        $('#PostalCodeRegularExpressionValidator').css('display', 'none');
        $('#postalCode').css('border', '1px solid #ccc');
    }

    function val() {
        Page_ClientValidate("vg");
        if (!Page_IsValid) $('#postalCode').css('border', '1px solid red');
        else $('#postalCode').css('border', '1px solid #ccc');
        return Page_IsValid;
    }
});
</script>

