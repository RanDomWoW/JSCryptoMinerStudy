<script type="text/javascript">
function bind10(){
	$('.comment .c-d').click(function(){
		var th = $(this);
		var c = th.closest('.comment');
		var id = th.attr('data-id');
		var name = c.find('.c-name').text();
		$('.comment-form input[name="answer"]').val(id);
		$('.comment-form textarea[name="text"]').val(name + ', ');
		$('#c12').click();
		$('body').animate({ scrollTop: $('.comment-form').offset().top - 100 }, 100);
		return false;
	});
	$('.comment .open11').click(function(){
		var th = $(this);
		var li = th.closest('li');
		var c = li.find('.collapse');
		if(c.is(':visible')){
			c.slideUp('fast');
		}
		else{
			c.slideDown('fast');
		}
		return false;
	});
}
$(document).ready(function(){
	$('nav a[href="'+location.pathname+'"]').parent().addClass('active');
	$('nav a').click(function(){
		$('nav li').removeClass('active');
		$(this).parent().addClass('active');
	});
	$('.comment-form button').click(function(){
		if(!$(this).hasClass('disabled')){
			var f = $(this).closest('form');
			f.find('.err').removeClass('err');
			var send = true;
			f.find('.required').each(function(){
				if($.trim($(this).val()) == ''){
					$(this).addClass('err');
					send = false;
				}
			});
			if(send){
				$('.comment-form button').addClass('disabled');
				$.post('/' + '?action=comment', f.serialize(), function(data){
					if(data == 1){
						$('#success-message').slideDown('slow', function(){
							setTimeout(function(){
								location.reload();
							}, 3000);
						});
						
					}
					else if(data == 0){
						$('.comment-form button').removeClass('disabled');
						$('#error-message').slideDown('slow', function(){
							setTimeout(function(){
								$('#error-message').slideUp('slow');
							}, 3000);
						});
					}
				});
			}
		}
		return false;
	});
		window.sh5 = 10;
	window.scr = false;
	$('#loadmore').click(function(){
		if(!window.scr){
			$(window).scroll(function(){
				if (($(document).scrollTop() + $(window).height() > $('#loadmore').offset().top && $(document).scrollTop() - $('#loadmore').offset().top < $('#loadmore').height()) && $('#loadmore').is(':visible')){
					$('#loadmore').click();
				}
			});
			window.scr = true;
		}
		$(this).hide();
		var i;
		for(i = 0; i < window.sh5; i++){
			$('#comments-logout .media-list > .hidden1:first').removeClass('hidden1');
		}
		if($('#comments-logout .media-list > .hidden1').length > 0){
			$('#loadmore').show().css('display', 'block');
		}
		return false;
	});
	
	$.post('/', {action: 'comments_view'}, function(data){
		$('#comments-logout .media-list').html(data);
		var i;
		for(i = 0; i < window.sh5; i++){
			$('#comments-logout .media-list > .hidden1:first').removeClass('hidden1');
		}
		if($('#comments-logout .media-list > .hidden1').length > 0){
			$('#loadmore').show().css('display', 'block');
		}
		bind10();
	});
	});
</script>
