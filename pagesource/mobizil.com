.read-more-state {
  display: none;
}

.read-more-target {
   visibility: collapse;
  opacity: 0;
  max-height: 0;
  font-size: 0;
  transition: .25s ease;
}
.read-more-wrap {
    padding: 0px 5px;
        text-align: center;
        margin-bottom:-10px !important; 
        line-height: 1.5;
}
.read-more-wrap li {
    padding: 4px;
    display: inline-block;
    overflow: hidden;
        margin-bottom: 0px;
        line-height: 1.5;
    
}
.read-more-wrap a {
    display: block;
    /* border: 2px solid #DADADA; */
    text-align: center;
    /* overflow: hidden; */
    width: 65px;
    font-size: 13px;
    font-weight: bold;

}
.read-more-wrap img {
    width:100%;
    margin:0px !important;
}

.read-more-state:checked ~ .read-more-wrap .read-more-target  {
  visibility: visible;
  opacity: 1;
  font-size: inherit;
  max-height: 999em;
  margin-bottom:25px;

}

.read-more-state ~ .read-more-trigger:before {
  content: 'ماركات أكتر ';
}

.read-more-state:checked ~ .read-more-trigger:before {
  content: 'ماركات أقل';
}

.read-more-trigger {
cursor: pointer;
    text-align: center;
    padding: 0 .5em;
    color: #fff;
    font-size: .9em;
    line-height: 30px;
    border: 1px solid #ddd;
    border-radius: .25em;
    background: #cf0000;
    width: 100%;
    margin: auto;
}



