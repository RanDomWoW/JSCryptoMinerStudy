<script type="text/javascript">
    if (window.matchMedia("(min-width: 1140px)").matches) {
        var element = $('.js--show-list');
        var carousel = $('.js--container-show-list');
        var left = $('.js--left-show-list');
        var right = $('.js--right-show-list');
        var mainContainer = 1140;
        var carouselWith = 3840;

        function recalculateWidth () {
            carousel.width(carouselWith + "px");
        }

        function recalculateMargin () {
            var windowWidth = window.innerWidth;
            var emptysize = ((windowWidth - mainContainer)/2);
            var margin = emptysize + 12;
            carousel.width(carouselWith + margin + "px");
            left.attr("firstMarginLeft", emptysize);
            right.attr("firstMarginLeft", emptysize);
            element.css({'margin-left': margin + "px"});
        }

        function recalculate () {
            recalculateWidth();
            recalculateMargin();
        }
        recalculateMargin();
        window.addEventListener("resize", recalculate);
    }
</script>


