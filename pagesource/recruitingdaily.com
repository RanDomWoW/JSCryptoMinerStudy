</div>
<script type="text/javascript">
			jQuery(function($) {
				$(".close-footer").click(function() {
					$(".fixedBar").hide();
				    var d = new Date();
				    d.setTime(d.getTime() + (7*24*60*60*1000));
				    var expires = "expires="+d.toUTCString();
				    document.cookie = "nlbar" + "=" + "clicked" + "; " + expires +"; path=/";					
				});
			});
			</script>
</body>
