#subscribe .hidden-fields { display: none; visibility: hidden; }

#subscribe { padding: 20px; background: #1a9bfc; text-align: center;}

#subscribe h4.title1 { font-weight: 900; font-size: 44px; color: #FFFFFF; line-height: 1; text-transform: uppercase; margin-bottom: 4px;} 

#subscribe h4.title2 { font-weight: 900; font-size: 29px; color: #FFFFFF; line-height: 1; text-transform: uppercase; margin-bottom: 15px; } 

#subscribe h4.title1 span { display: inline-block; font-weight: bold; font-size: 28px; margin-top: 15px; }

#subscribe h4.title2 span { display: inline-block; font-weight: bold; font-size: 28px; margin-top: 15px; margin-top: -15px;}

#subscribe p { color: #FFFFFF; margin: 0; }

#subscribe p.text { margin: 15px 0; opacity: 0.8; }

#subscribe input { border: none; width: 100%; box-sizing: border-box; padding: 10px 0; margin: 0; box-shadow: none; border-radius: 0; height: 45px; text-indent: 10px; text-align: center;}

#subscribe .email-field { margin-top: 10px }

#subscribe input.email-field, #subscribe input.name-field { color: #FFFFFF; background: #0762ad; }

#subscribe input::-webkit-input-placeholder { color: inherit; opacity: 0.8; }

#subscribe input:-moz-input-placeholder { color: inherit; opacity: 0.8; }

#subscribe input::-moz-input-placeholder { color: inherit; opacity: 0.8; }

#subscribe input::-ms-input-placeholder { color: inherit; opacity: 0.8; }

#subscribe input:focus::-webkit-input-placeholder {color: transparent !important;}

#subscribe input:focus::-moz-input-placeholder {color: transparent !important;}

#subscribe input:focus:-moz-input-placeholder {color: transparent !important;}

#subscribe input:focus::input-placeholder {color: transparent !important;}

#subscribe input.submit { background: #FFFFFF; color: #1a9bfc; margin-top: 20px; font-size: 18px; text-transform: uppercase; font-weight: 500; box-shadow: 0px 1px 2px 0px rgba(0, 0, 0, 0.05); cursor: pointer;}

#subscribe p.footer-text { margin-top: 10px; font-size: 12px; }

#subscribe_popup #subscribe h4.title { margin-top: 0 }

.subscribe.loading { position: relative }

.subscribe.loading:after { content: ""; display: block; position: absolute; top: 0; left: 0; right: 0; bottom: 0; background: #1a9bfc; }

.subscribe-single #subscribe {text-align: left; margin: 10px 0; clear: both;}

.subscribe-single #subscribe input {text-align: left;}

#subscribe .error, #subscribe .thanks {margin-top: 10px;}

.infusion-field-left { -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box; width: 50%; padding-right: 10px; float: left; } .infusion-field-right { -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box; width: 50%; float: left; } .infusion-field-lg { width: 100%; margin-float: left; } .infusion-field-sm { -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box; width: 33%; padding-right: 10px; float: left; } .infusion-submit { text-align: center; }

.large-form { width: 80%; margin: 0 auto; }

.large-form input, .large-form select { background-color: #f4f9fa; border: 1px solid #dce1e5 !important; border-radius: 4px; }

.large-form label { display: inline-block; }

.large-form input[type="submit"] { color: #ffffff; background-color: #1a9bfc; margin-top: 15px;}

