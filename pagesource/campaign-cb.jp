
<!DOCTYPE html>
<html lang="ja">
<head>
	<meta charset="utf-8">
  <title>申込</title>
  <link rel="stylesheet" href="css/style.css" />
</head>
<body>
<div id="container">
<h1>お客様の情報を入力してください</h1>
<p class="err-msg"></p>
  <form action="" method="post">
  <dl class="input-style">
    <dt>申込ID</dt>
    <dd><input type='text' name='i_entry_id' maxlength='50' value='' placeholder='' class='' /><span class="err-msg"></span></dd>
  </dl>
  <dl class="input-style">
    <dt>電話番号</dt>
    <dd><input type='tel' name='i_tel' maxlength='15' value='' placeholder='例:09012345678' class='' /><span class="err-msg"></span>
    <br>（※ハイフン「-」は付けない）</dd>
  </dl>
  <dl class="input-style">
    <dt>生年月日</dt>
    <dd><input type='tel' name='i_birthday' maxlength='8' value='' placeholder='例:19800101' class='' /><span class="err-msg"></span><br>例：1980年1月1日生まれの場合「19800101」</dd>
  </dl>
	<p class='submit' id='button'></p>
	<input type="hidden" name="i_syozai_id" value=""/>
  </form>
</div>
<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jqueryui/1.8.23/jquery-ui.min.js"></script>
<script type="text/javascript" charset="UTF-8">
  /* 必須項目の<input>タグのnameプロパティ指定 */
  var REQUIRED_ITEMS = new Array("i_entry_id","i_tel","i_birthday");
  /* 必須項目のcheckboxの上位class指定 */
  var REQUIRED_CHECKBOX = new Array();
  /* カナ変換TRUEかFALSEか */
  var KANA_CONVERSION = "TRUE";
  // 吹き出し表示TRUEかFALSEか //
  var TOOL_TIP = "FALSE";
  /* カナ変換の入出力フォームnameプロパティ指定 */
//  var KANA_CONVERSION_NAME = new Array();
  // 住所補間で値を挿入する要素のnameプロパティ
//  var ZIP_INTERPOLATION_ARRAY = new Array();
  // 吹き出しの色を指定
  var SIDE_TOOLTIP_COLOR = "#000";
  // textareaに初期状態でコメントを入れておく為のnameプロパティとコメント指定
/*
  var TEXTAREA_COMMENT_NAME = "i_contents";
  var TEXTAREA_COMMENT = "その他ご意見 ご要望などございましたら、こちらにご記入ください。";
  // 郵便番号用の吹き出しを出すnameプロパティ
  var SIDE_TOOLTIP_ZIP_NAME = "i_postnum";
*/
  /* 入力完了前のツールチップ文言 */
  var TOOLTIP_NOTICE_TEXT = "<span id='efo_box' style='font-weight:bold; color:#ffb700; cursor:pointer; text-decoration:underline;'>必須項目</span>を入力して下さい。";
  /* 入力完了後のツールチップ文言 */
  var TOOLTIP_COMPLETE_TEXT = "<span style='font-weight:bold; color:#ffbb99;'>入力が完了しました。</span><br />確認画面にお進みできます。";
  /* 入力完了前のツールチップ背景色 */
  //var TOOLTIP_NOTICE_COLOR = "#ff2000";
  var TOOLTIP_NOTICE_COLOR = "#ff2000";
  /* 入力完了後のツールチップ背景色 */
  //var TOOLTIP_COMPLETE_COLOR = "#44bbcc";
  var TOOLTIP_COMPLETE_COLOR = "#44bbcc";
  /* ツールチップ透明度 */
  var TOOLTIP_OPACITY_RATE = "0.7";
  /* 未入力時のボタンの文字 */
  var BTNTXT_NOTICE   = "入力内容をご確認ください";
  /* 完了時のボタンの文字 */
  var BTNTXT_COMPLETE = "申　　込";
  /* 必須項目の背景色 */
  var FORM_BG_COLOR = "#ffeaea";
  /* フォーム点滅色 */
  var FORM_BG_FLASH_COLOR = "#FFD700";

</script>
<script type="text/javascript" src="js/efo.js"></script>
</body>
</html>