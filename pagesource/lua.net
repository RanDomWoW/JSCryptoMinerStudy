        <script>
                    // Initialize Intercom
            window.Intercom('boot', {
                app_id: 'g9bv2g4t',
            });

            // Stylesheet to inject
            const image = 'data:image/png;base64,';
            const css = "#intercom-container .intercom-launcher-open-icon {background-image: url('" + image + "')!important;background-size: 40px 35px!important;background-repeat: no-repeat!important;background-color: #493C8E!important;border-radius: 50%!important;position: fixed!important;z-index: 2147483000!important;cursor: pointer!important;background-position: 50%!important;}";
            const stylesheet = document.createElement('style');

            stylesheet.type = "text/css";
            stylesheet.appendChild(document.createTextNode(css));

            // Wait for the iframe to become ready (max 30 seconds)
            const timeout = setTimeout(() => clearInterval(interval), 30000);
            const interval = setInterval(() => {
                const iframe = document.querySelector('.intercom-launcher-frame');
                const iframeMobile = document.querySelector('.intercom-launcher-frame-mobile');

                if (iframe || iframeMobile) {
                    // Append the stylesheet to the iframe head
                    iframe.contentDocument.head.appendChild(stylesheet);

                    clearInterval(interval);
                    clearTimeout(timeout);
                }
            }, 100);
                        </script>
