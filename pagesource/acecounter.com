<script>	
 //document창에 aaa 함수호출
 
 function aaa(){
  drag_dropie();
  if(event.keyCode==37){
   layers.style.left=document.all.layers.style.pixelLeft-1;
  }
  if(event.keyCode==39){
   layers.style.left=document.all.layers.style.pixelLeft+1;
  }
  if(event.keyCode==38){
   layers.style.top=document.all.layers.style.pixelTop-1;
  }
  if(event.keyCode==40){
   layers.style.top=document.all.layers.style.pixelTop+1;
  }
 }
 //dragapproved 처음값을 false값을 가짐
 var dragapproved=false;
 //mouseDrag에서 함수호출
 function drag_dropie(){
  if(dragapproved==true){
   document.all.layers.style.pixelLeft=tempx+event.clientX-iex;
   document.all.layers.style.pixelTop=tempy+event.clientY-iey;
   return false;
  }
 }
 function mouseDrag(){
  iex=event.clientX;
  iey=event.clientY;
  tempx=layers.style.pixelLeft;
  tempy=layers.style.pixelTop;
  dragapproved=true;//마우스를 누른상태에 dragapproved값은  true값을 가짐
  document.onmousemove=drag_dropie;//drag_dropie를 onmousemove이벤트 핸들러로 가짐
 }
 if(document.all){
  document.onmouseup=new Function("dragapproved=false");
  //document창에서 마우스를 놨을 때 멈추는 이벤트핸들러 acecounter
 }
 //document.onkeyup=aaa;
 //document.onkeydown=aaa;
</script>




