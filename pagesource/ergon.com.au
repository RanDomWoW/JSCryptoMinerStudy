  <script type="text/javascript">
    var demandGauge = null;

    google.setOnLoadCallback(function() {

      // set to an initial value to pre-load the arrow
      initGauge($('.demand-meter').width(),$('.demand-meter').height());

      // Get JSON data file containing network demand data 
      $.getJSON( "/static/Ergon/Network%20Demand/currentdemand.json", function(data) {
    
        var items = [];
        $.each( data, function( key, val ) {
          items.push(key,val);
        });
        window.result = items[5][0].currentdemand;

        // Set value for demand gauge:
        // Parameters are demand value, peak demand start time & peak demand end time 
        // Peak demand times are expressed in 24 hr time (eg. 16 = 4pm, 20 = 8pm)
        setDemandValue(parseInt(window.result.data),16,20);

        // Draw gauge:
        // Parameters are demand value, minimum demand value and maximum demand value
        drawGauge(parseInt(window.result.data),1000,2500);
      });
    });
  </script>
