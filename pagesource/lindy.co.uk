<script type="text/javascript">

  function checkPostcode()
  {
    run_again = false;

    if($('#delivery_postcode_textbox').length > 0 )//&& $('#courier_id').val() == 6)
    {
      blnPostcodeChecked = false;

      strPostcode = $('#delivery_postcode_textbox').val();

      if(strPostcode == '')
      {
        $('#courier_id').prop("disabled", true);
        return;
        strPostcode = 'notentered';
      }

      $.getJSON("/ajax/getPostcodeDeliveryPrice/"+ strPostcode + "/true", function(data)
      {
        strDeliveryPrice = data.delivery_price;


        // update the Estimated Delivery information
        if($('.courier_information').length > 0 && data.delivery_timestamp != undefined) {

          $.getJSON("/ajax/getEstimatedDeliveryDate/"+ data.delivery_timestamp + '/' + data.delivery_timestamp_max, function(converted_date) {

            new_estimate = 'Estimated Delivery to ' + strPostcode + ': ' + converted_date.strEstimatedDelivery + '-' + converted_date.strEstimatedDeliveryMax;

            if( $('#courier_information_' + $('#delivery_country_id').val() + '_' + data.courier_id).length == 0) {
              missing_preview = '<ul class="courier_information courier_information_new" id="courier_information_' + $('#delivery_country_id').val() + '_' + data.courier_id + '">';
              missing_preview+= '<li>' + data.courier_name + ' to ' + data.country_name + ':</li>';
              missing_preview+= '<li>' + new_estimate + '</li></ul>';
              $('#basket_delivery_est').append(missing_preview);
            }

            $('#courier_information_' + $('#delivery_country_id').val() + '_' + data.courier_id + ' li:eq(1)').text(new_estimate);
            $('#courier_information_' + $('#delivery_country_id').val() + '_' + data.courier_id).show();

          });
        }

        if(data.couriers_for_postcode != undefined) {
//
          matched_rate = false;

          if(data.couriers_for_postcode.length > 0) {
            new_couriers_html = '<option value="0">Choose Delivery Method</option>';
            $(data.couriers_for_postcode).each(function(i,e) {
              new_couriers_html+= '<option value="' + e.courier_id + '"' + (data.courier_id == e.courier_id ? ' selected="selected"':'') + '>' + e.name + '</option>';
              if(data.courier_id == e.courier_id) {
                matched_rate = true;
              }
            });
          } else {
            new_couriers_html = '<option value="0">No Delivery Method Available</option>';
          }

          $('#courier_id').html(new_couriers_html); // drop the new options in

          if(matched_rate == false) {
            $('#courier_id option:eq(1)').prop('selected',true); // select the first option
            change_courier( $('#courier_id') ); // trigger the full change process
          }
          $('#courier_id').prop("disabled", false);
        }

        strBasketTotal = data.basket_total;
        strBasketVatTotal = data.basket_vat_total;
        if(typeof(strDeliveryPrice) != 'undefined' && $('#delivery_postcode_textbox').val() != '')
        {
          // need to change the display price and calculate this every time a postcode is typed in
          $('#shipping_cost_container').html(strDeliveryPrice).removeClass('enter_postcode_note');

          //set postcode session
          $.getJSON("/ajax/setPostcode/"+ strPostcode);
          blnPostcodeChecked = true;
        }

        $('#basket_total').html(strBasketTotal);
        $('#basket_total').show();

        //recalculate the vat
        $('#vat_total').html(strBasketVatTotal);

        if($('#delivery_postcode_textbox').val() == '')
        {
          $('#shipping_cost_container').html('Enter Postcode').addClass('enter_postcode_note');
          blnPostcodeChecked = false;
        }

        $('#delivery_row td').show();

        // no courier data passed back, then postcode has no couriers so we'll reload the entire page to be sure
        if(data.raw_courier_info.length == 0) {
          $.getJSON("/ajax/setPostcode/"+ strPostcode, function() {
            //location.reload(true);
          });

        }

      });
    }
    else
    {
      $('#shipping_cost_container').removeClass('enter_postcode_note');
    }
  }


