<script type="text/javascript">
jQuery( document ).ready(function() {
    var hash = getHashFromUrl(window.location.href );
    if(hash=="inline1"){
        document.getElementById("loginbox").click();
    }
});

function getHashFromUrl(url){
    var a = document.createElement("a");
    a.href = url;
    return a.hash.replace(/^#/, "");
};


function updateLogin(email,password)
{
$MW_Onestepcheckout('.buttons-set .button').css('display','none');
$MW_Onestepcheckout('#login-please-wait').css('display','block');	//note: echo $this->getChildHtml('login_before') cuar .col2-set
	$MW_Onestepcheckout.ajax({
	type: "POST",
	url: "https://qloudea.com/unifylogin/index/updatelogin",
    data: "email="+email+"&"+"password="+password+"&"+"urlcallback=https://qloudea.com/pro/",
	success: function(msg) {
        //alert(msg);
        if (msg == 0) {
            $MW_Onestepcheckout('.buttons-set .button').css('display', 'block');
            $MW_Onestepcheckout('#login-please-wait').css('display', 'none');
            $MW_Onestepcheckout('#errorlogin').css('display', 'block');
            $MW_Onestepcheckout('#login-email').addClass('validation-failed');
            $MW_Onestepcheckout('#login-password').addClass('validation-failed');
        } else {
            if (msg == 1) {
                location.reload();
            } else {
                $MW_Onestepcheckout(location).attr('href', msg);
            }
        }
    }
    });
}

</script>

<script type="text/javascript">
function updateForgotpassword(email)
{
$MW_Onestepcheckout('#btforgotpass').css('display','none');
$MW_Onestepcheckout('#forgotpass-please-wait').css('display','block');	//note: echo $this->getChildHtml('login_before') cuar .col2-set
	$MW_Onestepcheckout.ajax({
	type: "POST",
    url: "https://qloudea.com/pro/onestepcheckout/index/forgotpass",
	data: "email="+email,
	success: function(msg){
			if(msg==1){
				$MW_Onestepcheckout('#inline3').html('<p>Hemos enviado una nueva contraseña a su dirección de correo electrónico. Haga clic en el enlace para volver a iniciar sesión.</p><p class="back-link"><a id="backlogin" href="#inline1"><small>&laquo; </small>Volver al formulario de acceso</a></p>');
				$MW_Onestepcheckout('#backlogin').magnificPopup({
					type: 'inline',
					midclick: true
				});				
			}
			else{
				$MW_Onestepcheckout('#btforgotpass').css('display','block');
				$MW_Onestepcheckout('#forgotpass-please-wait').css('display','none');
				$MW_Onestepcheckout('#errorforgotpass').css('display','block');	
				$MW_Onestepcheckout('#email_address').addClass('validation-failed')
				}
		}
	});
}
</script>
