        var searchForm = new Varien.searchForm('search_mini_form', 'search', 'Buscar');
        searchForm.initAutocomplete('https://qloudea.com/autocomplete.php?store=default&fallback_url=https://qloudea.com/catalogsearch/ajax/suggest/', 'search_autocomplete');
        //]]>

        function calc_left(offset_left, autocomplete_width, search_width) {
            var left = offset_left - autocomplete_width + search_width;

            if (left < 10) {
                left = 10;
            }

            return left;
        }

        function calc_height(window_height, offset, margin_bottom, form_search) {
            return window_height - offset.top - margin_bottom - form_search.height();
        }

        function set_search() {
            var margin_bottom = 36;
            var offset = form_search.offset();
            var window_height = jQuery(window).height();

            search_autocomplete.css({
                'max-height': calc_height(window_height, offset, margin_bottom, form_search) + 'px',
                'left': calc_left(offset.left, search_autocomplete.width(), form_search.width()),
                'top': offset.top + form_search.height()
            });
        }

        var search_autocomplete, search_bg, form_search;

        jQuery(document).ready(function () {
            search_autocomplete = jQuery("#search_autocomplete");
            search_bg = jQuery("#search-bg");
            form_search = jQuery("#search_mini_form");
            var target = search_autocomplete[0];
            var previousValue = target.style.display;

            jQuery("body").append(search_autocomplete);
            jQuery("body").append(search_bg);

            var observer = new MutationObserver(function (mutations) {
                mutations.forEach(function (mutation) {
                    var newNodes = mutation.addedNodes; // DOM NodeList

                    var $nodes = jQuery(newNodes); // jQuery set
                    $nodes.each(function () {
                        var $node = jQuery(this);
                        if(jQuery(".bubble-elasticsearch").attr('class') == $node.attr('class')){
                            set_search();
                        }
                    });

                    var currentValue = mutation.target.style.display;
                    if (currentValue !== previousValue) {
                        if (previousValue === "none" && currentValue !== "none") {
                            form_search.addClass("open");
                        } else {
                            form_search.removeClass("open");
                        }

                        previousValue = mutation.target.style.display;
                    }
                });
            });

            var config = {
                attributes: true,
                childList: true,
                characterData: true
            };

            observer.observe(target, config);
            //observer.disconnect();

            jQuery(window).resize(function () {
                set_search();
            });

            jQuery(window).scroll(function () {
                set_search();
            });
        });
    </script>
