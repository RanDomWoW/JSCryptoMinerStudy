<script>
$(document).ready(function(){
	$(".flight-booking select, .flight-booking input[type=checkbox]").uniform();

	$('.child1').hide();
	$('.child2').hide();
	$('.child3').hide();
	$('.child4').hide();

	var isVolHotel = false;

	$(".button").on("click", function() {
		var $button = $(this);
		var oldValue = $button.parent().find("input").val();
		var isChildButton = $button.parent().hasClass("child");
		var isParentButton = $button.parent().hasClass("adult");


			if (oldValue < 1 && isChildButton){
				$('.child1').hide();
			}
			if (oldValue <= 2 && isChildButton){
				$('.child2').hide();
			}
			if (oldValue <= 3 && isChildButton){
				$('.child3').hide();
			}
			if (oldValue <= 4 && isChildButton){
				$('.child4').hide();
			}

		if ($(".booking-menu-vol-hotel").hasClass("active") || $("#flight-booking-engine").hasClass("vol-hotel-form")){
			isVolHotel = true;
		}
		else{
			isVolHotel = false;
		}

		if ($button.text() == "+") {
			if (oldValue >= 4 && isChildButton){
				var newVal = parseFloat(oldValue) + 0;
			}
			else{
				if (oldValue >= 9){
					var newVal = parseFloat(oldValue) + 0;
				}
				else {
					var newVal = parseFloat(oldValue) + 1;
				}
			}

			if (newVal >= 1 && isVolHotel && isChildButton){
				$('.child1').css('display', 'inline');
			}
			if (newVal >= 2 && isVolHotel && isChildButton){
				$('.child2').show();
			}
			if (newVal >= 3 && isVolHotel && isChildButton){
				$('.child3').show();
			}
			if (newVal >= 4 && isVolHotel && isChildButton){
				$('.child4').show();
			}
		}
		else {
			// Don't allow decrementing below zero
			if (oldValue > 0) {
				var newVal = parseFloat(oldValue) - 1;

				if (newVal < 1){
					$('.child1').hide();
				}
			}
			else {
				newVal = 0;
			}
		}
			
		$button.parent().find("input").val(newVal);
	});
});
</script> 
