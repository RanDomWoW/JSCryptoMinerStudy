<script type="text/javascript">
	$(document).ready(function(){
		$('#source-aeroport').empty();
		function searchAirport(){
			var search = $('#source-aeroport').prop('value');
			$.ajax({
				url: searchAction({search: search}),
				type: 'POST',
				datatype: 'HTML',
				async: false,
				cache: false,
				success: function(data){
					$('#target').show();
					$('#target').html(data);
				},
				error: function(){
					$('#target').html('');
				}
			})
		}
		function searchAirportFirst(){
			$search = $('#source-aeroport').prop('value');
			if($search != ''){
				window.location = $('#airport-list a:first').prop('href');
			}
		}
		
		$("#source-aeroport").click(function(e){
			e.stopPropagation();
			$('#target').show();
		});
		$('#target').click(function(e){
			e.stopPropagation();
		});
		$('body').click(function(e){
			$('#target').hide();
		});
	
		$('#source-aeroport').prop('value', '');

		
				var searchAction = function(options) {var pattern = '/search-airport?search=:search'; for(var key in options) { pattern = pattern.replace(':'+key, options[key] || ''); } return pattern }
;
		
		
		
		
		
				
		$("#source-aeroport").keyup(function(e){
			var search = $('#source-aeroport').prop('value');
		    if(e.which == 13) {
		    	searchAirportFirst();
		    } else if(search.length > 0){
				searchAirport();
		    } else {
	    		$('#target').html('');
	  	  }
		});

									$("#point-NCE").tipTip({
					maxWidth: "auto",
					edgeOffset: 1,
					activation: 'click',
					activation: 'hover',
