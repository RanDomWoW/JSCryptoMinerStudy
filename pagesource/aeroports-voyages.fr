			if( el.disabled ) { return; }

			var tag = el.tagName.toLowerCase();

			if( tag === 'option' ) {
				options += createOptionHTML(el);
			}
			else if( tag === 'optgroup' ) {
				options += '<li class="cs-optgroup"><span>' + el.label + '</span><ul>';
				[].slice.call( el.children ).forEach( function(opt) {
					options += createOptionHTML(opt);
				} );
				options += '</ul></li>';
			}
		} );

		var opts_el = '<div class="cs-options"><ul>' + options + '</ul></div>';
		this.selEl = document.createElement( 'div' );
		this.selEl.className = this.el.className;
		this.selEl.tabIndex = this.el.tabIndex;

		var temp = window.location.href ;
		var query = window.location.search.substring(1);

		var param = query.split("=");

		/*var langParam = param[1];*/

		var langParam = "fr";

		if (langParam === "it"){
			this.selectedOpt.textContent="Italiano";
		}
		else if (langParam === "de" ){
			this.selectedOpt.textContent="Deutsch";
		}
		else if (langParam === "en" ){
			this.selectedOpt.textContent="English";
		}
		else if (langParam === "es" ){
			this.selectedOpt.textContent="Español";
		}
		else if (langParam === "fr" ){
			this.selectedOpt.textContent="Français";
		}
