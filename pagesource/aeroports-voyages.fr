<script type="text/javascript">
	var searchActionNewsletter = function(options) {var pattern = '/newsletter?email=:email'; for(var key in options) { pattern = pattern.replace(':'+key, options[key] || ''); } return pattern }
;
	$("#emailAjax").keypress(function(e) {
	    if(e.which == 13) {
	    	newsletter();
	    }
	});
	function newsletter(){
		$email = $('#emailAjax').prop('value');
		if($email == "") {
			$('#targetNewsletter').html('<span class="news-error">Email non valide</span>');
		} else {
			$.ajax({
				url: searchActionNewsletter({email: $email}),
				type: 'POST',
				datatype: 'HTML',
				success: function(data){ $('#targetNewsletter').html(data); },
				error: function(){
					$('#targetNewsletter').append('');
				}
			})
		}
	}
	function newsletterEnter(){
		newsletter();
	}
