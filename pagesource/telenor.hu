<script>
window.APPModules = window.APPModules || {};
window.APPModules.NEWSLETTER_FORM=(function() {
	var $myform;

	function beforeSend(arr, $form, options) {
		$form.find('button').attr('disabled','disabled');
		$form.find('.fld').removeClass('showerror');
		APP.Form.hideErrorHint();
	}

	function onResult(response, status, xhr, $form) {
		var success = response.success,
			errors = response.errors,
			$input;
		//var $form = $form || $myform;

		if (!success) {
			for (var i in errors) {
				if (errors[i]+'') {
					$input = $form.find('*[name="' + i + '"]');
					$input.closest(".fld").addClass("showerror");
					APP.Form.showErrorHint($input, errors[i]);
				}
			}
			$form.find('button').removeAttr('disabled');

		} else {
			$form.parent().find('.success').removeClass('hide').addClass('show');
			$form.parent().find('.hideonsuccess').removeClass('show').addClass('hide');

			//ga('send', 'event', 'newsletter', 'Subscribe', window.location.toString() + (($form.attr('id') === 'newsletterblock') ? ' - subscribe box' : ''));
			APP.GTMHelpers.gtmPush('eventTracker', 'newsletter', 'Subscribe', window.location.toString() + (($form.attr('id') === 'newsletterblock') ? ' - subscribe box' : ''));
		}
	}

	function init() {
		$myform = $('form.newsletter');

		$myform.ajaxForm({
			dataType: 'json',
			beforeSubmit: beforeSend,
			success: onResult,
			error: function(a,b,c) {
				console.log('ajaxForm error: ', a, b, c);
			}
		});

	}

	return {init: init};
})();
</script>


