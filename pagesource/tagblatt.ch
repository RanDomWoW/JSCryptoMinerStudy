<script type="text/javascript">

	/**************************************************************************************
	// Autor: 		Alberto D'Angelo 
	// Funktion: 	Keywords für Google News werden gesetzt. Falls die Keywords
	// 				im Artikel-Editor leer sind, werden bei den Detailseiten 
	//				folgende Keywords eingesetzt: "Unterzeile, Nachrichten + Ressort, Titel".
	//				Bei den Uebersichtseiten wird "Nachrichten + Ressort" eingesetzt.
	// Version: 	1.0
	**************************************************************************************/
    
	fcmsJs.load('extlib_jquery').onLoad(function () { 


		(function($) {
	
			
          	var valueMetaTag = $('meta[name=news_keywords]').attr("content");
			
			// Falls das Keyword im fcms-Editor vom Redaktor leer gelassen wurde.
			if(document.querySelector("meta[name=news_keywords]") && valueMetaTag.length > 1) {

				console.log('nichts machen, news_keywords sind vorhanden!');
				
			} else {

				var daa_uebersichtsseite = '';
				var daa_titel 			 = 'St. Galler Tagblatt';
				
				// Ueberpruefe, ob es sich um eine Uebersichts- oder Detailseite handelt.
				if(daa_uebersichtsseite == "") {
					// Meta-Tag fuer die Uebersichtsseite wird generiert
					$('head').append('<meta content="Nachrichten Home" name="news_keywords">');
				} else {
					// Meta-Tag fuer die Detailseite wird generiert. 
					// Falls die Stichzeile + Nachrichten (11 Zeichen) + die Kategorie grösser sind als 50 Zeichen, wird der
					// Titel nicht angezeigt
					var daa_stichzeile 	= '';
					var daa_kategorie 	= 'Home';
					var laenge_kategorie = daa_stichzeile.length + daa_kategorie.length + 12;
						
					if(laenge_kategorie > 50) {
						daa_titel_str = "";
					} else {
						daa_titel_str = ', ' + daa_titel;
					}
					$('head').append('<meta content=", Nachrichten Home' + daa_titel_str + '" name="news_keywords">');
				}                   

			}
		
		})(jQuery);
	});
    
    
</script>  


