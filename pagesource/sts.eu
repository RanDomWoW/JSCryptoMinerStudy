                        <input type="text" id="last-name-brochure" name="customer.lastName" data-validate="required">
                        <label for="last-name-brochure">Last name *</label>
                        <div class="alert">required</div>
                    </div>
                </div>
            </div>

            
            <div class="row" id="pdf-delivery-brochure">
                <div class="col-md-6">
                    <div class="input m-b">
                        <input type="text" id="email-brochure" name="customer.email" data-validate="email">
                        <label for="email-brochure">E-mail *</label>
                        <div class="alert">Wrong email format</div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="input m-b">
                        <input type="text" id="confirm-email-brochure" name="customer.confirmEmail" data-validate="match" data-validate-match-with="customer.email">
                        <label for="confirm-email-brochure">Confirm E-mail *</label>
                        <div class="alert">E-mail and confirm E-mail doesn&#39;t match</div>
                    </div>
                </div>
            </div>

            
                <div class="row hidden" id="mail-delivery-brochure">
                    <div class="col-md-6">
                        <div class="input m-b">
                            <input type="text" id="street-brochure" name="customer.address.street" data-validate="required" data-validate-condition="checkbox:#mail">
                            <label for="street-brochure">Street *</label>
                            <div class="alert">This field is required</div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="input m-b">
                            <input type="text" id="zipCode-brochure" name="customer.address.zipCode" data-validate="required" data-validate-condition="checkbox:#mail">
                            <label for="zipCode-brochure">Zip code *</label>
                            <div class="alert">This field is required</div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="input m-b">
                            <input type="text" id="city-brochure" name="customer.address.city" data-validate="required" data-validate-condition="checkbox:#mail">
                            <label for="city-brochure">City *</label>
                            <div class="alert">This field is required</div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="input m-b">
                            <input type="text" id="country-brochure" name="customer.address.country" data-validate="required" data-validate-condition="checkbox:#mail">
                            <label for="country-brochure">Country *</label>
                            <div class="alert">This field is required</div>
                        </div>
                    </div>
                </div>

            <div class="row">
                <div class="col-md-6">
                    <div class="input m-b">
                        <input type="text" id="phone-brochure" name="customer.phone" data-validate="required, maxLength" data-maxLength="21">
                        <label for="phone-brochure">Telephone number *</label>
                        <div class="alert" data-alert="required">This field is required</div>
                        <div class="alert" data-alert="maxLength">Maximum number of carachters is 21</div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="m-b">
                        <label for="interest.comment-brochure">Questions for STS</label>
                        <textarea id="interest.comment-brochure" name="interest.comment"></textarea>
                    </div>
                </div>
            </div>



            <hr class="m-b" />
            <div data-am-alert="error icon" id="terms-error-brochure" style="display: none" class="m-b">
                <p>Please accept terms to submit the form</p>
            </div>
            <div data-am-alert="error icon" id="server-error-brochure" style="display: none" class="m-b">
                <p>Something went wrong trying to submit your brochure order, please try again later</p>
            </div>
            <div data-am-alert="positive icon" id="server-success-brochure" style="display: none" class="m-b">
                <p>Thank you for your order, happy reading!</p>
            </div>
            <div class="text-center">
                <button type="submit" name="btnSubmit" data-am-button="primary rounded-50 padding text-medium">Order</button>
            </div>
        </div>
    </form>
</div>

