<script type="text/javascript">
function updateAsync_eval(target,url) {
	new Ajax.Updater(target, url, {asynchronous:true,evalScripts:true});
}

function ajax_getshopname(target) {
	var e = document.getElementById('chome_sel');
	var param = (e != null) ? '?chomecd=' + e.value : '' ;

	if(e != null && e.value == '') {
		alert('住所を選択してください');
		return false;
	}

	url = '$!{iyns}${apcontext}/common/ajax_getshopname.do' + param;
	new Ajax.Updater(target, url, {asynchronous:true,evalScripts:true});
}

function updateAsync_chomechange(target,url) {
	new Ajax.Updater(
		target,
		url,
    	{
    		asynchronous:true,
    		evalScripts:true
    	}
	);
}


function  checkpostno(){
	var e = document.getElementById('zipcode');
	if (e != null ) {
		if (!e.value.match(/^[0-9]{7}$/)) {
			alert('ハイフンを除く７桁を半角数字でご入力ください');
			return false;
		}
	}
	return true;
}

function  checkHttpReady(xmlhttp){
	var msg = document.getElementById("msg");
	if (xmlhttp.readyState == 4) {
		if (xmlhttp.status == 200) {
			return true;
		} else {
//			msg.innerHTML = "担当店舗検索は一時的にご利用いただけません。しばらくしてからもう一度お試しください。";
			return false;
		}
	}
}
</script>
