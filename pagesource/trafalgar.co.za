<script type="text/javascript">
function Custom_WPPFL_getBaseUrl(flag){
        var res = "";
        res = 'https://www.trafalgar.co.za/';
        if(flag!=""){
            res = res+"?"+flag;
        }
        return res;
    }

function Custom_WPPFL_sendNotifyToServer(flag, values) {
    $.ajax({
        url: saprizo_getJsonUrl(flag),
        dataType: 'json',
        data: values,
        success: function() {
            return 1;
        }
    });
}

function Custom_WPPFL_increaseNumberOfFavorites(){
    var current_number = parseInt($('.number_of_favorites').html());
    $('.number_of_favorites').html(current_number+1);
}

function Custom_WPPFL_reduceNumberOfFavorites(){
    var current_number = parseInt($('.number_of_favorites').html());
    $('.number_of_favorites').html(current_number-1);
}

function Custom_WPPFL_changeAddToDeleteButton(property_id){
    var element = $('#favorite_link_'+property_id);
    element.html('Delete from Favourite');
    element.removeAttr('onclick');
	element.addClass('dele_link');
    element.unbind('click');
    element.click(function(){
        Custom_WPPFL_deletePropertyFromFavorite(property_id);
    });
    //element.onclick = Custom_WPPFL_deletePropertyFromFavorite(property_id);
}

function Custom_WPPFL_changeDeleteToAddButton(property_id){
    var element = $('#favorite_link_'+property_id);
    element.html('Add to Favourite');
    element.removeAttr('onclick');
	element.removeClass('dele_link');
    element.unbind('click');
    element.click(function(){
        Custom_WPPFL_addPropertyToFavorite(property_id);
    });
    //element.onclick = Custom_WPPFL_addPropertyToFavorite(property_id);
}

function Custom_WPPFL_addPropertyToFavorite(property_id){
    $.get(Custom_WPPFL_getBaseUrl(""),
        {favorite_property: property_id},
        function(data){
            data = $.trim(data);
            if(data=="1"){
                Custom_WPPFL_increaseNumberOfFavorites();
                alert("Property successfully added");
            }else{
                alert("Property was already added");
            }
        }
    );
    Custom_WPPFL_changeAddToDeleteButton(property_id);
}

function Custom_WPPFL_deletePropertyFromFavorite(property_id){
    $.get(Custom_WPPFL_getBaseUrl("delete=1"),
        {favorite_property: property_id},
        function(data){
            data = $.trim(data);
            if(data=="1"){
                Custom_WPPFL_reduceNumberOfFavorites();
                alert("Property successfully deleted form favourite list");
            }else{
                alert("Property was already deleted form favourite list");
            }
        }
    );
    Custom_WPPFL_changeDeleteToAddButton(property_id);
}

