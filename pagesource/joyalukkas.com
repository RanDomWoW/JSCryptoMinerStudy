          // menu on hover
          function menuhover() {
              if ($(".media-query-trigger").css("font-size") > "12px") {
                  $('ul.nav > li a').hover(function () {
                      if (!$(this).parent('li').siblings().find('a').hasClass('active')) {
                          $(this).addClass('active').parent('li.dropdown').find('.dropdown-menu').stop(true, true).delay(300).fadeIn();
                      }
                      else {
                          $(this).addClass('active').parent('li.dropdown').find('.dropdown-menu').stop(true, true).show();
                          $(this).parent('li').siblings().find('.dropdown-menu').stop(true, true).hide();
                          $(this).parent('li').siblings().find('a').removeClass('active');
                      }
                  });
                  $('ul.nav').mouseleave(function () {
                      $(this).find('.dropdown-menu').stop(true, true).delay(300).fadeOut();
                      $(this).find('li a.active').removeClass('active');
                  });
              }
          }
