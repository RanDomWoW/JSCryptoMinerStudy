                      }
                  }
              });
              $('.owl-dots').css({
                  'width': $('.main-slider').find('.owl-dot').size() * 26
              });
              $('.main-slider').find('.owl-prev').html('<span class="thin-left"></span>');
              $('.main-slider').find('.owl-next').html('<span class="thin-right"></span>');
          }


          function PlayVideo(url) {
              //youtube script
              var tag = document.createElement('script');
              tag.src = "https://www.youtube.com/iframe_api";
              var firstScriptTag = document.getElementsByTagName('script')[0];
              firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

              var player;

              onYouTubeIframeAPIReady = function () {
                  player = new YT.Player('player', {
                      height: '100%',
                      width: '100%',
                      videoId: url,  // youtube video id
                      playerVars: {
                          'autoplay': 0,
                          'rel': 0,
                          'showinfo': 0,
                          // 'controls': 0,
                      },
                      events: {
                          'onStateChange': onPlayerStateChange
                      }
                  });
              }
              onPlayerStateChange = function (event) {
                  if (event.data == YT.PlayerState.ENDED) {
                      $('.start-video').fadeIn('normal');
                  }
              }
              $(document).on('click', '.start-video', function () {
                  $(this).fadeOut('normal');
                  player.playVideo();
              });
              $(window).scroll(function () {

                  var wsp = $(window).scrollTop();
                  var topfixedht = 0;
                  if ($(".media-query-trigger").css("font-size") == "12px") {
                      topfixedht = 52;
                  }
                  else {
                      topfixedht = 142;
                  }
                  $("#player").each(function () {
                      if (((wsp + $(window).height() * .8) > $(this).offset().top) && ((wsp + topfixedht) < $(this).offset().top)) {
                          $('.start-video').fadeOut('normal');
                          player.playVideo();
                      }
                      else {
                          player.pauseVideo();
                      }
                  });
              });

          }
          function getMobileOperatingSystem(data) {

              var userAgent = navigator.userAgent || navigator.vendor || window.opera;

              var src = 'https://www.youtube.com/embed/' + data + '?autoplay=0&amp;rel=0&amp;showinfo=0&amp;enablejsapi=1&amp;widgetid=1&quot;';
              if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
                  $('.video-home-latest').html('<iframe id="player" frameborder="0" allowfullscreen="1" title="YouTube video player" width="100%" height="100%" src=' + src + ' enablejavascript="false"></iframe>');

              }
              else if (navigator.appVersion.indexOf("Mac") != -1) {
                  $('.video-home-latest').html('<iframe id="player" frameborder="0" allowfullscreen="1" title="YouTube video player" width="100%" height="100%" src=' + src + ' enablejavascript="false"></iframe>');
              }
              else
                  PlayVideo(data);


          }
          //youtube script end
      </script>
         
