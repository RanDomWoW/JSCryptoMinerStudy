              if ($(".media-query-trigger").css("font-size") == "12px")
                  socialwidth = socialcount * 44 - 12;
              else
                  socialwidth = socialcount * 56 - 20;
              $('.footer-social-media-bar').children('ul').css({
                  'width': socialwidth
              });


          }


  </script>
    
