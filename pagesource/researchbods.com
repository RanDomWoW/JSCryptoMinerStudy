    <script type="text/javascript">

          var deleteLog = false;

          $(document).ready(function() {
              $('#fullpage').fullpage({
                  sectionsColor: ['', '#3A3939'],
                  anchors: ['firstPage', 'secondPage', 'thirdPage'],
                  menu: '#menu',
                  responsive: 1100,

                  onLeave: function(index, nextIndex, direction) {

                  if ($(window).width() > 1100) {
                      if(index == 1,2 && direction == 'down') {
                          $('#slide-loader').show('slide',{direction:'down'},500);
                          $('.slide-loader-gif').delay(300).fadeIn('800');
                          $('.slide-loader-gif').delay(300).fadeOut('300');
                          $("#slide-loader").delay(700).hide('slide',{direction:'up'},500);
                  }
              } else if ($(window).width() < 1100) {
                  $('#slide-loader').hide();
                  $('.slide-loader-gif').hide();
                  }

              if ($(window).width() > 1100) {
                      if(index == 2,3 && direction == 'up') {
                          $('#slide-loader').show('slide',{direction:'up'},500);
                          $('.slide-loader-gif').delay(300).fadeIn('800');
                          $('.slide-loader-gif').delay(300).fadeOut('300');
                          $("#slide-loader").delay(700).hide('slide',{direction:'down'},500);
                  }
              } else if ($(window).width() < 1100) {
                  $('#slide-loader').hide();
                  $('.slide-loader-gif').hide();
                  }


                  },
                  onSlideLeave: function(anchorLink, index, slideIndex, direction, nextSlideIndex){
                      if(deleteLog){
                          $('#callbacksDiv').html('');
                      }

                  if ($(window).width() < 1100) {
                  $('#slide-loader').hide();
                  $('.slide-loader-gif').hide();
                  }

                  },
                  afterRender: function(){
                  },
                  afterResize: function(){
                  },
                  afterSlideLoad: function(anchorLink, index, slideAnchor, slideIndex){
                  },
                  afterLoad: function(anchorLink, index){
                   if (!(document.documentMode) && window.StyleMedia || ((/*@cc_on!@*/false) || (document.documentMode))) {
                      $('.main-transition').show();
                      $('.first-transition').show();
                      $('.second-transition').show();
                      $('.third-transition').show();
                      $('.fourth-transition').show();
                      $('.fifth-transition').show();
                      $('.sixth-transition').show();
                   } else {
                      if(index == 3) {
                          $('.main-transition').delay(500).fadeIn('slow');

                          $('.first-transition')
                              .delay(1000)
                              .queue( function(next){
                              $(this).fadeIn().addClass('animated fadeInDown');
                              next();
                          });

                          $('.second-transition')
                              .delay(1500)
                              .queue( function(next){
                              $(this).fadeIn().addClass('animated fadeInDown');
                              next();
                          });

                          $('.third-transition')
                              .delay(2000)
                              .queue( function(next){
                              $(this).fadeIn().addClass('animated fadeInDown');
                              next();
                          });
                          $('.fourth-transition')
                              .delay(2500)
                              .queue( function(next){
                              $(this).fadeIn().addClass('animated fadeInDown');
                              next();
                          });
                          $('.fifth-transition')
                              .delay(3000)
                              .queue( function(next){
                              $(this).fadeIn().addClass('animated fadeInDown');
                              next();
                          });
                          $('.sixth-transition')
                              .delay(3500)
                              .queue( function(next){
                              $(this).fadeIn().addClass('animated fadeInDown');
                              next();
                          });

                      }
                  }
                  }
              });

          });

  </script>
<script>

// $(window).resize(function(){location.reload();});

/* window preloader and cookie bar */

if ($(window).width() < 1024) {
        $(window).on("scroll", function() {
    var scrollPos = $(window).scrollTop();
    if (scrollPos <= 0) {
        $(".header-backdrop").fadeOut();
        console.log("header")
    } else {
        $(".header-backdrop").fadeIn();
    }
});
}

if ($(window).width() > 1024) {
    $(window).on("load", function() {
    $(".preloader-gif").delay(10).fadeOut();
    $("#preloader").delay(50).fadeOut("slow");
    $("#cookie-bar").delay(900).slideDown("slow");

});

} else {
    $(window).on("load", function() {
    $(".preloader-gif").delay(300).fadeOut();
    $("#preloader").delay(400).fadeOut("slow");
    $("#cookie-bar").delay(900).slideDown("slow");
});

}


/* hiding clip-path videos on edge and internet explorer */

    // Internet Explorer 6-11
    if ((/*@cc_on!@*/false) || (document.documentMode)) {
        $('.diamond-videos').hide();
        $('#second-home img').show();
    }

    if ((window.opr && opr.addons) || window.opera || (navigator.userAgent.indexOf(' OPR/') >= 0)) {
        $('.diamond-videos').hide();
        $('#second-home img').show();
    }

    // Edge 20+
    if (!(document.documentMode) && window.StyleMedia) {
    $('.diamond-videos').hide();
    $('#second-home img').show();
}

</script>



