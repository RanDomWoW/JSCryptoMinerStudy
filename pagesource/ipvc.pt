	  function mudaTudo() {
		//alert("mudaTudo vai ser executado");
		$(".mini-day-on a").removeAttr('href');
              $(".date-heading a").removeAttr('href');      
		
		$(".mini-day-on a").mouseover(function () {
			$(this).css('cursor', 'pointer');
			});	
		
		$(".has-events").click(function(){ // submete a data
				var datadia = this.id; 
				var datac  = datadia.substring(11,21); 
				$('#edit-field-adata-value-value-date').val(datac); 
				$('#edit-field-adata-value2-value-date').val(datac);
				$('#edit-submit-calendario').trigger('click'); // mudar id do bloco
			});
		$(".mini-day-on").click(function(){ 
			$(".calendario td").removeClass('today'); 
			$(".calendario td div").removeClass('seleccionado'); 
			$(this).addClass('seleccionado');
			});
		}

	$(document).ready(function(){
			mudaTudo();				
		});
		
		
	$('#block-views-calendario-block_1 .block-inner').ajaxSuccess(function() {
			mudaTudo();				
	});
	
		
	
	$(".link").mouseover(function() {
		$(this).addClass("mpover");
    	})
	$(".link").mouseout(function() {
		$(this).removeClass("mpover");
    	})

	$(".dptabela").mouseover(function() {
		$(this).addClass("dpover");
    	})
	$(".dptabela").mouseout(function() {
		$(this).removeClass("dpover");
    	})
	
	$(".escola").mouseover(function() {
		var esco = $(this).attr("class");
		var escola ="."+esco.substring(7)+"1";
		$(escola).removeClass("esconde");
    	})
	$(".escola").mouseout(function() {
		var esco = $(this).attr("class");
		var escola ="."+esco.substring(7)+"1";
		$(escola).addClass("esconde");
    	})


	
}(jQuery));




