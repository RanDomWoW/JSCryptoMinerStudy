 <script type="text/javascript">
    
    
        var pollRequestUrl = "/Poll/DisplayTnpsPoll";
        setTimeout(function() {
            $.ajax({
                type: 'GET',
                url: pollRequestUrl,
                data: { pollTriggerName: 'bob web', onlineEvent: 'bob web' },
                async: false,
                success: function(data) {
                    if (data['DisplayPoll'] == true) {
                        $('#tnpsFeedId').val(data['FeedId']);
                        TriggerTnpsPollLightBox();
                        $('#tnpsLightBoxMain').show();
                    }
                }
            });
        }, 220000);
    

        function TriggerTnpsPollLightBox() {
            // We need to use a timeout so the lightbox that fired the event can destroy itself before the new lightbox is shown
            window.setTimeout(function () {
                $('#lightboxTnpsPollTrigger').trigger('click');
            }, 300);
        }

        function TriggerTnpsPoll() {
            $('#tnpsLightBox').height(630);
            $('#tnpsLightBox').width(900);
            $("#tnpsLightBox").css("margin-top", "-210px");
            $("#tnpsLightBox").css("margin-left", "-450px");
            $("#tnpsPollIframe").height($('#tnpsLightBox').height() - 85);
            $('#tnpsLightBoxContent').hide();
            var feedId = $('#tnpsFeedId').val();
            var url = "https://tagfeedback.com" + "/webSurvey?feed_id=" + feedId;
            $("#tnpsPollIframe").attr('src', url);
            $("#tnpsPollIframe").show();
            $("#tnpsIframeContent").show();
        }

        function TnpsPollUnsubscribe() {
            $('#tnpsLightBox').height(630);
            $('#tnpsLightBox').width(900);
            $("#tnpsLightBox").css("margin-top", "-210px");
            $("#tnpsLightBox").css("margin-left", "-450px");
            $("#tnpsPollIframe").height($('#tnpsLightBox').height() - 85);
            $('#tnpsLightBoxContent').hide();
            var feedId = $('#tnpsFeedId').val();
            var url = "https://tagfeedback.com" + "/backend/web/unsubscribe?feedId=" + feedId + "&targetId=25";
            $("#tnpsPollIframe").attr('src', url);
            $("#tnpsPollIframe").show();
            $("#tnpsIframeContent").show();
        }

        function TnpsPollHidden() {
            $('#tnpsLightBoxMain').hide();
        }

    </script>
   
