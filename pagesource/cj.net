<script type="text/javascript">
	var pathname = location.pathname;
	var search  = location.search;

	function goMlngSitePc(wlngUrl) {
		var url = "";
		if (pathname.indexOf("/cj_now/view__.asp") >= 0) {
			url = wlngUrl + "/cj_now/index.asp";
		} else if (pathname.indexOf("/footer/privacy") >= 0
						|| pathname.indexOf("/footer/useinfo") >= 0
							|| pathname.indexOf("/footer/legal") >= 0
								|| pathname.indexOf("/footer/pemailgathering") >= 0
									|| pathname.indexOf("/footer/winwin") >= 0
										|| pathname.indexOf("/footer/cyber") >= 0) {
			url = wlngUrl;
		} else if (pathname.indexOf("/ir/") >= 0) {
			if (pathname.indexOf("/ir/notice/") >= 0)
			{
				if (wlngUrl.indexOf("8081") >= 0
						|| wlngUrl.indexOf("english") >= 0
							|| wlngUrl.indexOf("www") >= 0
								|| wlngUrl.indexOf("dev") >= 0
						)
				{
					url = wlngUrl+ pathname + search;		
				} 
			}else{
				url = wlngUrl;
			}
		} 
		 else {
			url = wlngUrl + pathname + search;
		}
		
		location.href = url;
	}

</script>
