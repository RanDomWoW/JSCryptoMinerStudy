                    $('#open-sideMenu').on('click', sideMenuHandler);

                    function sideMenuHandler(e) {
                        $('.mobile-login').removeClass('menu-open');

                        if ($("#mobile-nav-container").css("display") == "none") {
                            addClassWithDelay("in", "#mobile-nav-list");
                            $('body').addClass("side-open");
                        } else {
                            addClassWithDelay("out", "#mobile-nav-list");
                            $('body').removeClass("side-open");
                        }

                        /*MOBILE LANG CAROUSEL*/
                        if ($('#flags').length !== 0) {
                            $('.mobile-flags .owl-carousel').owlCarousel({
                                loop: false,
                                nav: true,
                                items: 1,
                                dots: false
                            })
                        }
                    }

                    function addClassWithDelay(direction, target) {

                        $('#open-sideMenu').off('click', sideMenuHandler);

                        $(target).find('>li').each(function (i) {
                            $(this).delay(20 * i).queue(function () {
                                $(this).addClass("fade" + direction).dequeue();
                            });
                        });

                        var itemsNum = $(target).find('>li').children().length;

                        if (direction === "out") {
                            $("#mobile-nav-container").fadeOut(20 * itemsNum, function () {
                                $('#open-sideMenu').on('click', sideMenuHandler);
                                clearClass();
                            });
                        } else {
                            $("#mobile-nav-container").fadeIn("fast", function () {
                                $('#open-sideMenu').on('click', sideMenuHandler);
                            });
                        }
                    }

                    function clearClass() {
                        $("#mobile-nav-list").find('>li').each(function (i) {
                            $(this).removeClass("fadein fadeout");
                        });
                    }

                    function doneResizing() {
                        if ($("#mobile-nav").css("display") == "none") {
                            clearClass();
                            $('.top-row-menu-container').css('display', '');
                            $('.mobile-login').removeClass('menu-open');

                            if ($('body').hasClass("side-open")) {
                                $('#open-sideMenu').trigger("click");
                            }
                        }
                    }

                    $(".fl-dropdown-custom").click(function () {
                        if ($(this).parent().children('.fl-dropdown-menu').is(':visible')) {
                            $(this).parent().children('.fl-dropdown-menu').slideUp();
                            $(this).children('.fl-plus').show();
                            $(this).children('.fl-minus').hide();
                        }
                        else {
                            $(this).parent().children('.fl-dropdown-menu').slideDown();
                            $(this).children('.fl-plus').hide();
                            $(this).children('.fl-minus').show();
                        }
                    });

                    
