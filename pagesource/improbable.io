    <script type="text/javascript">
      document.addEventListener('DOMContentLoaded', function() {
    
        var $ioHeader = document.getElementById('ioHeader');
        var $ioBurger = document.getElementById('ioBurger');
        var $ioSubheader = document.getElementById('ioSubheader');
        var $ioHeaderTitle = document.getElementById('ioHeaderTitle');
        var $ioSwitcher = document.getElementById('ioSwitcher');
        var activeClass = 'is-active';
        var subheaderOpen = false;
        var hovering = false;
    
        if ($ioBurger) {
          $ioBurger.addEventListener('click', function() {
            if (subheaderOpen) {
              closeSubheader();
            } else {
              openSubheader();
            }
          });
        }
    
        if ($ioHeaderTitle) {
          $ioHeaderTitle.addEventListener('mouseenter', openMenu);
          $ioHeaderTitle.addEventListener('mouseleave', closeMenu);
        }
    
        if ($ioSwitcher) {
          $ioSwitcher.addEventListener('mouseenter', openMenu);
          $ioSwitcher.addEventListener('mouseleave', closeMenu);
        }
    
        function openSubheader() {
          subheaderOpen = true;
          $ioBurger.classList.add(activeClass);
    
          if ($ioSubheader) {
            $ioSubheader.classList.add(activeClass);
            $ioSubheader.scrollTop = 0;
          }
        }
    
        function closeSubheader() {
          subheaderOpen = false;
          $ioBurger.classList.remove(activeClass);
    
          if ($ioSubheader) {
            $ioSubheader.classList.remove(activeClass);
          }
        }
    
        function openMenu() {
          hovering = true;
          $ioHeader.classList.add(activeClass);
          $ioSwitcher.classList.add(activeClass);
        }
    
        function closeMenu() {
          hovering = false;
          setTimeout(function() {
            if (!hovering) {
              $ioHeader.classList.remove(activeClass);
              $ioSwitcher.classList.remove(activeClass);
              hovering = true;
            } else {
              hovering = false;
            }
          }, 200);
        }
    
      });
    </script>
    
