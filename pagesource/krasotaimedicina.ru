		if ($('.smallLogoGeo').css('display') == 'block') 
		{
			/*hideSearchBlock();
			$('.smallLogoGeo').hide();*/
			//showSearchBlock();
		}
		hideSearchBlock();
		if ($('input.searchIndexMenuInput').val().length > 0 || $(document.activeElement).hasClass('searchIndexMenuInput')) 
		{
			showSearchBlock();
			$('.smallLogoConteiner').css('display','none');
			$('.smallLogoGeo').css('display','block');
		}
		
			}
	else if (pageWidth > 580 && pageWidth <= 800) 
	{
				if ($('input.searchIndexMenuInput').val().length > 0 || $(document.activeElement).hasClass('searchIndexMenuInput')) showSearchBlock();
		else 
		{
			hideSearchBlock();
			$('table.headIndexTbl td.menuTd').css('display','table-cell');
			reLoadTopMenu();
		}
		$('.smallLogoGeo').hide();
		$('.smallLogoConteiner').css('display','block');
				
		
	}
	else if (pageWidth > 800)
	{
				showSearchBlock();
		$('.smallLogoConteiner').css('display','block');
		$('table.headIndexTbl td.menuTd').css('display','table-cell');
		reLoadTopMenu();
		$('.smallLogoGeo').hide();
			}
	$('.title-search-result').css('display','none');

}
$('.indexMenuSearchIcon').click(function(){
	var pageWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
	showSearchBlock();
	
		
	if (pageWidth < 580)
	{
		$('.smallLogoConteiner').css('display','none');
		$('.smallLogoGeo').css('display','block');
	}	
	
		
	$('input.searchIndexMenuInput').focus();
});


$('.smallLogoGeo').click(function(){
	$(this).hide();
	hideSearchBlock();
	$('.hiddenDiv').hide();
	$('#changeGeo').addClass('selectedLink');
	$('#geoList').show();
});

$(function(){
	correctSearchBlock();
});

$(window).resize(function(){
	correctSearchBlock();
});


