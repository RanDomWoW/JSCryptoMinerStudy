    <style type="text/css">
        body
        {
            margin: 0px !important;
            direction: ltr;
        }

        .foxpush_subdomains
        {
            width: 300px;
            margin:15% auto;
            overflow: hidden;
            padding: 20px;
        }

        .foxpush_subdomains .cover
        {
            width: 250px;
            margin: auto;
        }

        .foxpush_subdomains h1,.foxpush_subdomains h2
        {
            padding: 0px;
            margin:0px;
            font-size: 15px;
            text-align: center;
            font-family: tahoma, arial;
        }

        .foxpush_subdomains h1
        {
            font-weight: bold;
            font-family: arial, Georgia, serif;

        }

        .foxpush_subdomains h2
        {
            font-weight: normal;
            font-size: 12px;
        }


        .foxpush_copyright
        {
            display: block;
            text-align: center;
            padding: 5px;
            font-size: 11px;
            margin-bottom: 3px;
            margin-left: 11px;
            margin-top: 50px;
            font-family: verdana;
        }

        .foxpush_copyright a
        {
            text-decoration: none;
            color: #000;
            padding-left: 16px;
            background: url('https://cdn.foxpush.net/images/logo_icon.svg');
            background-size: 11px;
            background-repeat: no-repeat;
        }
    </style>
</head>
<body>



<script id="foxpush-tpl" type="text/x-handlebars-template">
    <div class="foxpush_subdomains">
        <div class="cover">
            <img src="{{icon}}" width="250" height="150" alt="" />
        </div>
        <div class="body">
            <h1> {{title}} </h1>
            <h2> {{desc}} </h2>
        </div>

        {{#if copyright}}
        <span class="foxpush_copyright">
            <a href="https://www.foxpush.com?ref={{foxsubdomain}}" target="_blank"> Powered by FoxPush </a>
        </span>
        {{/if}}

    </div>
</script>


<script id="foxpush-error" type="text/x-handlebars-template">
    <div class="foxpush_subdomains">
        <div class="cover">
            <img src="{{icon}}" width="250" height="150" alt="" />
        </div>
        <div class="body">
            <h1 style="color: red;">{{try_again_later_msg}}</h1>

        </div>

    </div>
</script>


<script id="foxpush-success" type="text/x-handlebars-template">
    <div class="foxpush_subdomains">
        <div class="cover">
            <img src="{{icon}}" width="250" height="150" alt="" />
        </div>
        <div class="body">
            <h1>  {{thanks_for_subscription_msg}}   </h1>
        </div>

    </div>
</script>



<script id="foxpush-waiting" type="text/x-handlebars-template">
    <div class="foxpush_subdomains">
        <div class="cover">
            <img src="{{icon}}" width="250" height="150" alt="" />
        </div>
        <div class="body">
            <h1>  Please wait .. .  </h1>
        </div>

    </div>
</script>


<script id="foxpush-blocked" type="text/x-handlebars-template">
    <div class="foxpush_subdomains">
        <div class="cover">
            <img src="{{icon}}" width="250" height="150" alt="" />
        </div>
        <div class="body">
            <h1> {{blocked_website_msg}} </h1>
        </div>
    </div>
</script>



<script type="text/javascript">
    
            var foxpush_localstorage_config =
            {
                foxpush_localstorage:0,
                foxpush_status:'none',
                foxpush_user_token:'',
                foxpush_segmentation:''
            };

            var foxpush_config =
            {
              domain:'fox.push',
              foxsubdomain:'foxpush',
              icon:'https://cdn.foxpush.net/images/default_icon.jpg',
              title:'Subscribe to our push notifications',
              desc:'Keep up to date with our latest news by clicking Allow',
              short_title:'Notification',
              allow_text:'Allow',
              deny_text:'Maybe Later',
              allow_mobile:1,
              allow_firefox:0,
              allow_safari:0,
              popup_style:'native',
              is_cache:'',
              referrer:document.referrer,
              hostname:'https://foxpush.foxpush.net',
              conversion_tracker:'',
              try_again_later_msg:' We are sorry, please try again later!',
              thanks_for_subscription_msg:'Thanks for subscribing.',
              blocked_website_msg:'Sorry you have blocked our website. Please allow notifications first in your browser settings.',
              copyright:'1',};var welcome_object = {};
</script>


<script type="text/javascript" data-cfasync="true" src="https://cdn.foxpush.net/publishers/assets/foxpush.js"></script>


</body>
</html>