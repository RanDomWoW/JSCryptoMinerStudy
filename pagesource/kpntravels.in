<script type="text/javascript">
    $(document).ready(function () {
           
        $("#WebContent_ddlFrom").append($("<option></option>").val('0'));
        $("#WebContent_ddlTo").append($("<option></option>").val('0').html('Select a city'));
        $("#WebContent_ddlFrom").change(function () {

            var ImageLoader = document.getElementById("WebContent_ImageLoader");
            ImageLoader.style.display = "inline";
            var fromPlaceId = $("#WebContent_ddlFrom").val();
            if (fromPlaceId != 0 && fromPlaceId != null && fromPlaceId != '') {
                $.ajax({
                    type: "POST",
                    url: "PlanaService.asmx/FillPlacesByFromPlaceId",
                    data: '{FromPlaceId:"' + fromPlaceId + '"}',
                    contentType: "application/json; charset=utf-8",
                    async: false,
                    dataType: "json",
                    success: function (data) {
                        var ddlToPlace = $('#WebContent_ddlTo');
                        ddlToPlace.empty();
//                            ddlToPlace.append($("<option></option>").val('0'));
                        ddlToPlace.append($("<option></option>").val('0').html('Select a city'));
                        $.each(data.d, function (key, value) { ddlToPlace.append($("<option></option>").val(value.Id).html(value.Name)); });
                    },
                    error: function (result) {
                        alert('Error Occured view log file');
                    }
                });
            }
            else {
                $('#WebContent_ddlTo').empty();
                $('#WebContent_ddlTo').append($("<option></option>").val('0').html('Select a city'));
            }
            ImageLoader.style.display = "none";
        });

        $("#WebContent_ddlTo").change(function () {
            $("#WebContent_hdnToPlaceID").val($("#WebContent_ddlTo").val());
        });
        $("#WebContent_txtTravelDate").change(function () {
            $("#WebContent_txtReturnDate").val("");
            return false;
        });
        $("#WebContent_txtReturnDate").change(function () {

            var retDate = new Date($("#WebContent_txtReturnDate").val());
            var trDate = new Date($("#WebContent_txtTravelDate").val());
            if (Date.parse(retDate) <= Date.parse(trDate)) {
                alert("Return Date should be greater than Travel date");
                $("#WebContent_txtReturnDate").val("");
                return false;
            }
        });
    });

</script>
