        $(document).ready(function(){


            $('#tab-additional_information table>tbody>tr>td').each(function(){
                if($(this).find('p').length){
                    var charatd = $(this).find('p').html();
                    var addresschar = $(this).find('p');
                    $.each(js_array, function( index, value ) {
                        var index = charatd.indexOf(value);
                        if(index != -1 && charatd[index-1] != '>'){
                            var res = charatd.replace(value, "<span class='wikipost' data-id='"+value+"'>"+value+"</span>");
                            addresschar.html(res);
                        }
                    });

                }
            });
            $('#tab-additional_information table>tbody>tr>th').each(function(){
                if($(this).length){
                    var charatd = $(this).html();
                    var addresschar = $(this);
                    $.each(js_array, function( index, value ) {
                        var index = charatd.indexOf(value);
                        if(index != -1 && charatd[index-1] != '>'){
                            var res = charatd.replace(value, "<span class='wikipost' data-id='"+value+"'>"+value+"</span>");
                            addresschar.html(res);
                        }
                    });

                }
            });

            $('#tab-description p').each(function(){
                if($(this).length){
                    var charatd = $(this).html();
                    var addresschar = $(this);
                    $.each(js_array, function( index, value ) {
                        var index = charatd.indexOf(value);
                        if(index != -1 && charatd[index-1] != '>'){
                            var res = charatd.replace(value, "<span class='wikipost' data-id='"+value+"'>"+value+"</span>");
                            addresschar.html(res);
                        }
                    });

                }
            });

            function reset_wiipost_data(){
                if($('#wikibox').hasClass('active') && !$('.wikipost.loading').length && !$('#wikibox:hover').length) {
                    $('#wikibox').html('');
                    $('#wikibox').css('display', 'none');
                    $('#wikibox').removeClass('active');
                    $('.wikipost.active').removeClass('active');
                }
            }

            function set_wikipost_data(_mywikipost){
                if(!_mywikipost.hasClass('active') && !_mywikipost.hasClass('loading')){
                    reset_wiipost_data();
                    $('.wikipost').removeClass('loading').removeClass('active');
                    _mywikipost.addClass('loading');
                    _mywikipost.addClass('reserved');
                    var offtop = _mywikipost.offset().top;
                    var offleft = _mywikipost.offset().left-483;
                    var dataid = _mywikipost.attr('data-id');

                    $.ajax({
                        url: 'http://www.gbook.ir/wiki_redirect_url',
                        type: 'POST',
                        data: {wikiid : dataid},
                        success: function(items) {
                            if(_mywikipost.hasClass('loading')){
                                if(!$('#wikibox').hasClass('active')) {
                                    $('#wikibox').css('top', offtop + 'px');
                                    $('#wikibox').css('left', offleft + 'px');
                                    $('#wikibox').css('display', 'block');
                                    $('#wikibox').html(items);
                                    $('#wikibox').addClass('active');
                                }
                                _mywikipost.addClass('active');
                                _mywikipost.removeClass('loading');
                            }
                        }
                    });
                }
            }

            $('body').append("<div id='wikibox'></div>");
            var _zm_wikipost_load_timeout;
            var _zm_wikipost_unload_timeout;
            $(document).delegate('.wikipost','mouseover',function(){
                var _target = $(this);
                _zm_wikipost_load_timeout = setTimeout(function(){
                    set_wikipost_data(_target);
                },500);
                clearTimeout(_zm_wikipost_unload_timeout);
            });
            $(document).delegate('.wikipost','mouseleave',function(){
                clearTimeout(_zm_wikipost_load_timeout);
                _zm_wikipost_unload_timeout = setTimeout(function(){
                    reset_wiipost_data();
                },1500);
            });

            $(document).delegate('#wikibox','mouseleave',function(){
                _zm_wikipost_unload_timeout = setTimeout(function(){
                    reset_wiipost_data();
                },1500);
            });
            $(document).delegate('#wikibox','mouseover',function(){
                clearTimeout(_zm_wikipost_unload_timeout);
            });

            $('body').click(function(){
                reset_wiipost_data();
            });
        });
    </script>
