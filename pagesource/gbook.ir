        var ajax_add_to_cart_url = 'http://www.gbook.ir/ajax_add_to_cart_custom_url/';
        function zm_ajax_add_to_cart(product_id,quantity,modal_id){
            $('#'+modal_id+' .modal-content').addClass('loading');
            $.ajax({
                url: ajax_add_to_cart_url,
                method: 'get',
                data: {
                    'product_id': product_id,
                    'quantity': quantity,
                }
            }).done(function(result){
                $('#'+modal_id+' .loading').removeClass('loading');
                translated_result = 'نتیجه_نامشخص';

                if(result.indexOf('success')!='-1')result = 'success';

                switch (result){
                    case 'invalid_product_id': translated_result = 'پارامتر ورودی شناسه محصول صحیح نمی باشد.'; break;
                    case 'success': translated_result = 'محصول مورد نظر با موفقیت به سبد خرید اضافه شد.'; break;
                    case 'not_enough_stock': translated_result = 'تعداد درخواستی از موجودی بیشتر است.'; break;
                }
                if(result=='success'){
                    $('#'+modal_id+' .modal-content').addClass('finished');
                    setTimeout(function(){$('#'+modal_id).removeClass('zm_active').modal('hide');},500);
                    update_ajax_cart();
                }else{
                    $('#'+modal_id+' .modal-content .modal-body').append('<div class="clearfix"></div><br><div class="alert alert-danger">'+translated_result+'</div>');
                }
            });
        }
    </script>
