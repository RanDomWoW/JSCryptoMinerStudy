            <script type="text/javascript" LANGUAGE="JavaScript">
                function efeLogHead()
                {
                    var dados = $('#frm_lgn_head').serialize();
                    $.ajax({
                        url: "http://www.cineart.com.br/ajax/ajax.doLogin.php",
                        type: "POST",
                        cache: false,
                        dataType: 'json',
                        data: dados
                    }).done(function (data)
                    {
                        if (data.sts == 1)
                        {
                            $.fancybox('<h2 class="modalpro" style="color:#0e5e98">Login efetuado!</h2>');
                            location.reload();
                            return false;
                        } else if (data.sts == 9)
                        {
                            $('#mascara').hide();
                            $('#logar').hide();
                            $.fancybox('<h2 class="modalpro">Login e/ou Senha errados</h2>');
                            return false;
                        }
                    });
                }

                $(function ()
                {
                    $("#frm_lgn_head").validar({classe: "input_texterro", div_sel: '.inputVl1H', onclick_salv: efeLogHead});
