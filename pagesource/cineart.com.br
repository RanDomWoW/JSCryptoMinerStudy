                                $('#filme').addClass("input_texterro");
                                $('#cinema').addClass("input_texterro");
                                $.fancybox('<h2 class="modalpro">Escolha um Cinema ou Filme!</h2>');
                                return false;
                            } else if ($('#cinema').val() == 0)
                            {
                                window.location = "http://www.cineart.com.br/filme/" + $('#filme option:selected').attr('data-code') + "/" + $('#filme').val() + ".html";
                                muda_filme_unload($('#filme').val());
                                return false;
                            } else if ($('#filme').val() == 0 && $('#cinema').val() != 0)
                            {
                                window.location = "http://www.cineart.com.br/programacao/" + $('#cidade option:selected').attr('data-url') + "/" + $('#cinema').val() + "/" + $('#cinema option:selected').attr('data-url') + ".html";
                                return false;
                            } else
                            {
                                window.location = "http://www.cineart.com.br/programacao/" + $('#cidade option:selected').attr('data-url') + "/" + $('#cinema').val() + "/" + $('#filme option:selected').attr('data-code') + "/" + $('#filme').val() + "/" + $('#cinema option:selected').attr('data-url') + ".html";
                                muda_filme_unload($('#filme').val());
                                return false;
                            }
                        });
