//김혁민 달력함수
function fx(btn, documentReady) {
	var listWidth = "";//테이블 달력 날짜 div 저장하는변수
	if(btn=='plus') {
		day += 1;
	} else if(btn=='minus') {
		day -= 1;
	}
	$.ajax({
		url:"/test111.bs",//url호출
		data:{"paramType": "month","paramInt": ""+day},//날짜 불러옴
		success:function(result){
			var listComment ="";//테이블에들어갈내용
			listWidth+="<tr>";
			for(var i=0;i<result.length;i++) {
				if (i == 0) {
					$("#yearTitle").html(result[i].date.substring(0,4));//년div도에들어갈것
					$("#monthTitle").html(result[i].date.substring(4,6)+"월");//월div에들어갈것
				}
				listWidth +="<td class='date'><div class='rauder ";//날짜클래스
				listComment += "<div  class='hideClass' id=\"comment" + result[i].date + "\">";//hideclass에  내용들을 넣어두고 hover 했을때 불러온다.hideclass는 안보인다.
				if(result[i].restTime == "YES" ) {
					listWidth += "close";//휴관색깔
					listComment +="<div style='float:left'>휴관일입니다.</div>";//휴관이면 나타낼 문구.
					var count = 0;
					for(var j = 0; j < result[i].dateList.length; j++) {
							var objectType = result[i].dateList[j].OBJECTTYPE;//이미지
							var title=result[i].dateList[j].TITLE.replace(/\s/gi,'');//타이틀은 띄어쓰기 쓰면 안되기 때문에 정규식처리로 띄어쓰기 다 없앰
							if (objectType == "edu") {
								listComment += "<div class='widBar'> | </div>";
								listComment += "<div class='wid' title=" + title + "><a href=\""+result[i].dateList[j].URL+"\">";
								var titleLength = Math.floor(Math.floor(($(".dday").width() - (47 * result[i].dateList.length + $(".widBar").innerWidth()* (result[i].dateList.length -1)))/parseInt($(".wid").css("font-size")))/result[i].dateList.length);//타이틀 길이 구하기
								
								if (result[i].dateList[j].TITLE.length > titleLength) {//타이틀 길이...붙일지 말지 정하는것
									listComment += result[i].dateList[j].TITLE.substring(0, titleLength) + "...";
								} else {
									listComment += result[i].dateList[j].TITLE;
								}
							
							//항목에따른 이미지 구분
								listComment += "<img src='https://www.science.go.kr/images/main/calendar_icon_edu.gif' alt='교육' />" + "</a></div>";
								
							}
							
					}
				} else if(result[i].dateList.length > 0) {
					listWidth += "blueC";//일정있으면색깔
					for(var j = 0; j < result[i].dateList.length; j++) {
							var title=result[i].dateList[j].TITLE.replace(/\s/gi,'');//타이틀은 띄어쓰기 쓰면 안되기 때문에 정규식처리로 띄어쓰기 다 없앰
							listComment += "<div class='wid' title=" + title + "><a href=\""+result[i].dateList[j].URL+"\">";
							var titleLength = Math.floor(Math.floor(($(".dday").width() - (47 * result[i].dateList.length + $(".widBar").innerWidth()* (result[i].dateList.length -1)))/parseInt($(".wid").css("font-size")))/result[i].dateList.length);//타이틀 길이 구하기
							
							if (result[i].dateList[j].TITLE.length > titleLength) {//타이틀 길이...붙일지 말지 정하는것
								listComment += result[i].dateList[j].TITLE.substring(0, titleLength) + "...";
							} else {
								listComment += result[i].dateList[j].TITLE;
							}
// 							listComment += result[i].dateList[j].TITLE;
							var objectType = result[i].dateList[j].OBJECTTYPE;//이미지
							//항목에따른 이미지 구분
							if (objectType == "junci") {
								listComment += "<img src='https://www.science.go.kr/images/main/calendar_icon_exbition.gif' alt='전시' />" + "</a></div>";
							} else if (objectType == "edu") {
								listComment += "<img src='https://www.science.go.kr/images/main/calendar_icon_edu.gif' alt='교육' />" + "</a></div>";
							} else if (objectType == "hengsa") {
								listComment += "<img src='https://www.science.go.kr/images/main/calendar_icon_event.gif' alt='행사' />" + "</a></div>";
							}
							if (result[i].dateList.length - 1 > j) listComment += "<div class='widBar'> | </div>";
					}
				} else {
					listWidth += "black";//둘다 없으면
					listComment +="일정이없습니다.";//휴관일이 아니고 일정이 없으면 나타낼 문구
				}
				listWidth += "'>" + result[i].date.substring(6,8)+"<input type='hidden' class='showClass' value=" + result[i].date +" /></div></td>";//날짜
				listComment +="</div>";
			}
			
			listWidth+="</tr>";
			listWidth+="<tr><td colspan="+result.length+" id ='calendarComment' class='nline Tline'></td></tr>";
			$("#a1").html(listWidth);
			$(".on2").html(listComment);
			$(".rauder").hover(function() {
				$(".gray").removeClass("gray");
				$(this).addClass("gray");
// 				console.log($(this).html());
// 				console.log($("#comment"+$(this).html()).html());
				$("#calendarComment").html($("#comment" + $(this).children().val()).html());
			});
			if(documentReady =='documentReady' && result.length > 0) {
				$("#calendarComment").html($("#comment"+result[0].date).html());
			}
			
		},error:function(request, status, error){
			alert("code:" + request.status + "%n message:" + request.responseText + "%n error:" + error);
		}
	});
}


