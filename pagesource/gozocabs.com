    function changeDestination(value, obj) {
        if (!value.length)
            return;
        var existingValue = obj.getValue();
        if (existingValue == '')
        {
            existingValue = '';
        }
        obj.disable();
        obj.clearOptions();
        obj.load(function (callback) {
            //  xhr && xhr.abort();
            xhr = $.ajax({
                url: '/lookup/nearestcitylist/source/' + value,
                dataType: 'json',
                success: function (results)
                {
                    obj.enable();
                    callback(results);
                    obj.setValue(existingValue);
                },
                error: function () {
                    callback();
