        }
        if ($('#BookingTemp_bkg_transfer_type_1').is(':checked')) {
            $('#bkg_to_city_id_tr').val($('#BookingTemp_bkgAirport').selectize().val()).change();
            $('#bkg_from_city_id_tr').val($('#BookingTemp_bkgTransferLoc').selectize().val()).change();
            trType = 2;
        }
        var strCityErr = '';
        if (trType == 0) {
            alert('error');
        }

        if (trType == 1) {
            if ($('#bkg_from_city_id_tr').val() == '') {
                strCityErr += "Pickup Airport must be selected before proceed.";
            }
            if ($('#bkg_to_city_id_tr').val() == '') {
                strCityErr += "\nDestination location must be selected before proceed.";
            }

        }
        if (trType == 2) {
            if ($('#bkg_from_city_id_tr').val() == '') {
                strCityErr += "Pickup location must be selected before proceed.";
            }
            if ($('#bkg_to_city_id_tr').val() == '') {
                strCityErr += "\nDrop off Airport must be selected before proceed.";
            }
        }
        if (strCityErr != '') {
            alert(strCityErr);
            return false;
        } else {
            return true;
        }
    }


