        (function(w, d){

            var loading = false,
                ids = [],
                offset = 0,
                limit = 20,
                feedAlias = '',
                feedBox = d.getElementById('feed'),
                feedLoadingBox = d.getElementById('feed_loading');


            function loadNext() {
   
                if (loading) {
                    return;
                }

                loading = true;

                offset += limit;

                Tochka.rmClass(feedLoadingBox, 'hidden');

                $.get('//tochka.net/_load/' + (feedAlias ? feedAlias + '/' : '') + offset + '/', function(data) {

                    Tochka.addClass(feedLoadingBox, 'hidden');

                    $(feedBox).append(data);

                    checkDuplicates();

                    $(feedBox.querySelectorAll('img.lazy')).lazyload({
                        skip_invisible: true
                    });
                    
                    loading = false;
                });

            }
            
            w.addEventListener('scroll', function() {
              
                if ( (d.body.clientHeight - Tochka.winHeight()) <= ( w.pageYOffset + 500 ) ) {

                    loadNext();

                }
            });
            
            
            function checkDuplicates() {
                
                var dublicates = [];
                
                // Find dublicates
                [].forEach.call(feedBox.getElementsByClassName('media'), function(item) {
                 
                    if (item.checked) {
                        return;
                    }
                    
                    var id = item.getAttribute('data-id');

                    if (ids.indexOf( id ) >= 0) {
                        dublicates.push(item);
                    } else {
                        ids.push(id);
                        item.checked = true;
                    }
                });
                
                // Remove dublicates
                [].forEach.call(dublicates, function(item) {
                    $(item).remove();
                });
            }
            
            
            //$(function(){
                $(feedBox.getElementsByClassName('lazy')).lazyload({
                    skip_invisible: true
                });
