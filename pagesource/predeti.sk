    require(['jquery', 'eshop'], function($, eshop) {
        $(document).on('eshop.command', function(e, command, data) {
            if (data && typeof data['minicart'] != 'undefined') {
                var minicart = data['minicart'];
                $('#minicart-dropdown').html(minicart.html);
                eshop.bindCommands($('#minicart-dropdown'));
                if (minicart.amount > 0) {
                    $('.cartText').show();
                    $('.cartTextEmpty').hide();
                } else {
                    $('.cartText').hide();
                    $('.cartTextEmpty').show();
                }
                if (minicart.missingAmountToFreeDelivery != null) {
                    $('.cartDeliveryInformation').show();
                    $('.cartDeliveryMissingAmount').html(minicart.missingAmountToFreeDelivery);
                } else {
                    $('.cartDeliveryInformation').hide();
                }
                $('.cartAmount').html(minicart.amount);
                $('.cartPrice').html(minicart.price);
            }
        });

    });
</script>

