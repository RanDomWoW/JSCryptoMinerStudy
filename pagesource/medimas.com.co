<!DOCTYPE html>
<html lang="es">
  <head>
    <title>Medimás</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>
  <body>
    <h3>
      Redireccionando a Medimás EPS en 
      <span id="countdown">5</span> segundos
    </h3>
    <script type="text/javascript">
      var seconds = 5;

      function countdown() {
        seconds = seconds - 1;
        if (seconds < 1) {
          document.getElementById("countdown").innerHTML = seconds;
          window.location = "http://www.medimas.com.co";
        } else {
          document.getElementById("countdown").innerHTML = seconds;
          window.setTimeout("countdown()", 1000);
        }
      }
      countdown();
    </script>
  </body>
</html>