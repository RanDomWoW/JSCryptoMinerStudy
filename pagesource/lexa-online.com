function comprobarLogin() {
    if ($("#usuario").val() === '' || $("#passw").val() === '') {
        $("#textErrorModal").text("* Por favor, introduce tu nombre de usuario y contraseña");
    } else {
        //comprobacion asíncrona de la contraseña
$.ajax({
            type: 'POST',
            url: 'chk_user.php',
            data: "usuario="+document.getElementById("usuario").value+"&clave="+document.getElementById("passw").value,
            success: function (data) {
                if(data==='1'){
                    $("#textErrorModal").text("");
                    document.form1.submit();                    
                }else{
                     $("#textErrorModal").text("* Usuario y/o contraseña incorrecto.");
                }
            }
        });
    }
}
$("input").keypress(function(event) {
    if (event.which === 13) {
        event.preventDefault();
        comprobarLogin();
        //$("#form1").submit();
    }
});
