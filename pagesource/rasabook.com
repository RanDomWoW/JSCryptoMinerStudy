                            function setSliderTimer() {
                                fullwidthSliderTimer = setInterval(function () {

                                    var $selected = $('.full-width-slider .bullets li.selected');

                                    if ($selected.index() + 1 < slideCount) {

                                        $selected.next().trigger('click');

                                    } else {

                                        $selected.parent().find('li:first-child').trigger('click');
                                    }

                                }, 5000);
                            }

                            function clearSliderTimer() {
