<script>

jQuery(function(){

  function carbon_position_homepage_utility_bar () {

    if ( jQuery( 'body.page-homepage' ).length && jQuery( '#orange-alert' ).length && jQuery( '#nav-local' ).length ) {

      var height = parseInt( jQuery( '#orange-alert' ).outerHeight() ) + parseInt( jQuery( '#header' ).outerHeight() );

      if ( jQuery( 'body.admin-bar' ).length ) {

        height += parseInt( jQuery( '#wpadminbar' ).outerHeight() );

      }

      height = height - 2;

      jQuery( '#nav-local' ).css( { 'top' : height + 'px' } );

    }

  }//carbon_resize_homepage_utility_bar()

  function carbon_position_nav_gradient () {

    var height = parseInt( jQuery( '#orange-alert' ).outerHeight() ) + parseInt( jQuery( '#header' ).outerHeight() );

    if ( jQuery( 'body.admin-bar' ).length ) {

      height += parseInt( jQuery( '#wpadminbar' ).outerHeight() );

    }

    jQuery( '#nav-local .hoverRight' ).css( { 'top' : height + 'px' } );

  }//carbon_position_nav_gradient()

  function carbon_position_search () {

    var height = parseInt( jQuery( '#orange-alert' ).outerHeight() ) + parseInt( jQuery( '#header' ).outerHeight() );

    if ( jQuery( 'body.admin-bar' ).length ) {

      height += parseInt( jQuery( '#wpadminbar' ).outerHeight() );

    }

    if ( jQuery( 'body.page-homepage' ).length ) {

      height = height - 2;

    } else {

      height = height - 1;

    }

    jQuery( '#g6-uni-header-search-drop' ).css( { 'top' : height + 'px' } );

  }//carbon_position_search()

  //Get the time stamp to prevent the browser from chaching the RSS feed
  var time = new Date().getTime();

  jQuery.get( document.location.protocol + '//www.getrave.com/rss/syr/channel1?' + time, function( data ) {

    var $xml = jQuery( data );

    //Inspect the feed
    $xml.find( "item" ).each( function() {

      //Load the feed item
      var $this = jQuery( this ),
          item = {
              title: $this.find("title").text(),
              link: $this.find("link").text(),
              description: $this.find("description").text(),
              pubDate: $this.find("pubDate").text(),
              author: $this.find("author").text()
      }

      //Check to see if there is an active orange alert
      if ( item.description != 'ALL CLEAR' ) {

        jQuery('#orange-alert').addClass( 'visible' );

        //Check to see if the alert text has changed since the last load
        if ( item.description != jQuery.cookie('orange-alert-text') ) {
          jQuery.cookie( 'orange-alert-text', item.description );
          jQuery.cookie( 'orange-alert-close', '0' );
        }

        jQuery.cookie( 'orange-alert-close', '0' );

        //Show the alert if the user did not dismiss it yet
        if ( jQuery.cookie( 'orange-alert-close') == '0' ) {

          jQuery('#orange-alert').html('<div class="alert"><div class="container"><div class="cell cell-left"><img src="/wp-content/themes/g6-carbon/img/alert.svg" alt="Alert" style="width: 28px;" /></div><div class="cell cell-center">' + item.description + '</div><div class="cell cell-right"><button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div></div></div>');

          carbon_position_homepage_utility_bar();
          carbon_position_search();
          carbon_position_nav_gradient();

          jQuery( window ).resize( function() {

            carbon_position_homepage_utility_bar();
            carbon_position_search();
            carbon_position_nav_gradient();

          });

          //Set the close button behaviour
          jQuery('#orange-alert .close').click(function () {
            jQuery('#orange-alert').removeClass( 'visible' );
      			jQuery('#orange-alert .alert').hide();
      			jQuery.cookie( 'orange-alert-close', '1' );

            carbon_position_homepage_utility_bar();
            carbon_position_search();
            carbon_position_nav_gradient();

       		});

        }

        //Exit jQuery loop
        return false;

      } else {

        //Set empty cookies
        jQuery.cookie( 'orange-alert-text', '' );
        jQuery.cookie( 'orange-alert-close', '0' );

      }

    });

  });

});

</script>

