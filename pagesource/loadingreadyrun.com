  $(document).ready(function () {
	$('#carousel_tabs a').on("mouseenter click", function(event) {
	 	 event.preventDefault();			
	 	 target_id = $(this).attr('href');
		 target_id = target_id.substr(6,1);
		 curr_id = $('#video_carousel .active').attr('id');
		 curr_id  = curr_id.substr(5,1);
	 	 changeCarousel(curr_id, target_id);
	 	 window.clearInterval(carouselTimer)
	});
	var carouselTimer = window.setInterval(function() {
			max_items = 5;
			curr_id = $('#video_carousel .active').attr('id');
			curr_id = +curr_id.substr(5,1);
			target_id = curr_id+1;
			if(target_id >5){
				target_id = 1;
			}
			changeCarousel(curr_id, target_id);
		}, 3000);
	});
	function changeCarousel(curr_id, target_id) {
		if(!$("#item-"+target_id).hasClass('active')){
			$("#item-"+curr_id).stop(true, true).fadeOut('300');
			$("#item-"+target_id).stop(true, true).fadeIn('300');
			$("#item-"+curr_id).removeClass('active');
			$("#item-"+target_id).addClass('active');
			$("#tab-"+target_id+" .overlay").hide();
			$("#tab-"+curr_id+" .overlay").show();
  		}
	}
</script>
