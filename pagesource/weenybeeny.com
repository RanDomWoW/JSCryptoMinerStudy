(function($){
	$(function(){

		//텝 온오버

		$('.mobile_tab > li').hide();
		$('.mobile_tab > li').eq(0).show();
		$('.tapface > li').on('mouseover',function(){
			$('.tapface > li').removeClass('on_tab');
			$(this).addClass('on_tab');

			$('.mobile_tab > li').hide();
			var _idxz = $(this).index();
			$('.mobile_tab > li').eq(_idxz).show();


		});

		// 중앙 엠디픽 배너
		var _mselect3 = $('.prod_cricle a');
		_mselect3.on({
			mouseenter : function(){
				$(this).parent().css('z-index','3');
				var _curImg = $(this).find('.rpl_img').attr('src');
				_curImg = _curImg.replace('_s','_la');
				$(this).find('.rpl_img').attr('src' ,_curImg);
				
				$(this).stop().animate({
					'width' : 315,
					'height' : 315,
					'top' : '-78px',
					'left' : '-78px'
				});


				$(this).find('.spc_img').stop().animate({
					'top' : 0
				});

			},mouseleave : function(){
				$(this).parent().css('z-index','2');
				var _curImg = $(this).find('.rpl_img').attr('src');
				_curImg = _curImg.replace('_la','_s');
				$(this).find('.rpl_img').attr('src' ,_curImg);

				$(this).stop().animate({
					'width' : 150,
					'height' : 150,
					'top' : '0px',
					'left' : '0px'
				});
				$(this).find('.spc_img').stop().animate({
					'top' : 315
				});


			}
		});
		
		// 상단 로그인메뉴들 오버효과
		$('.visual > a').on({		
			mouseenter : function(){
				$(this).find('img').stop().animate({				
				 'opacity' : '0.4'
				})

			},mouseleave : function(){
				$(this).find('img').stop().animate({				
				 'opacity' : '1'
				})
			}
		});

		
		// 상품 오버시 추가상품명노출
		$('.item_wrap .grid_box').on({
			mouseenter : function(){
				$(this).find('.subname_wrap').css('display','table');
			},mouseleave : function(){
				$(this).find('.subname_wrap').css('display','none');
			}
		});
		

		$('.selection01 .pro_cont a').on({
			mouseenter : function(){
				var _mainBan = $(this).find('img').attr('src');
				_mainBan = _mainBan.replace('_off','_over');
				$(this).find('img').attr('src',_mainBan);
			},mouseleave : function(){
				var _mainBan = $(this).find('img').attr('src');
				_mainBan = _mainBan.replace('_over','_off');
				$(this).find('img').attr('src',_mainBan);
			}
		});


	});



})(jQuery);
</script>

