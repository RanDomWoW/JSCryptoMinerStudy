<script type="text/javascript" src="/design/cmix5044/js/jquery.cycle.all.js"></script>
<script type="text/javascript">
(function($){
	$(function(){

		var _subcate_ban = $('.sub_cate > ul > li > a');		
		_subcate_ban.on({
			mouseenter : function(){	
				var _cate_src = $(this).attr('data-src');
				$('.sub_cate > a > img').attr('src',_cate_src);	
			},mouseleave : function(){
				//var _cate_src_ori = $('.sub_cate > a > img').attr('src')
				//$('.sub_cate > a > img').attr('src',__cate_src_ori);
			}
		});

		$('.head_gnb > ul > li > div.t_cate > a').on({
			mouseenter : function(){
				var _cate_src = $(this).attr('data-src');
				$('.sub_cate > a > img').attr('src',_cate_src);
			}
		});

	

		// 상단 중분류 카테고리
		var _headGnb = $('.head_gnb_wrap .head_gnb > ul > li');
		_headGnb.on({
			mouseenter : function(){
				var _tCate = $(this).children().find('img').attr('src');
				_tCate = _tCate.replace('_off','_ov');
				$(this).find('.t_cate_img img').attr('src',_tCate);				
				$(this).find('.sub_cate').stop(true,true).slideDown();


			},mouseleave: function(){
				var _tCate = $(this).children().find('img').attr('src');
				_tCate = _tCate.replace('_ov','_off');
				$(this).find('.t_cate_img img').attr('src',_tCate);
				$(this).find('.sub_cate').stop(true,true).slideUp();

			}
		});

		//검색박스
		var _search_from = $('.search_form .input_box .search_in');
		$('.search_form .input_box .search_btn').on('mouseover',function(){
			_search_from.show();
			_search_from.animate({
				'width' : 160	
			});		
			$(this).find('input').attr('src','/design/cmix5044/wib/common/search_icon_ov.jpg');
		});

		
		// 상단메뉴 고정
		var _t_cate = $('.head_gnb .t_cate');
		$(window).on('scroll',function(){
			var _headTop = $('.head_gnb_wrapper').offset().top;
			var _screenScroll = $(window).scrollTop();
			if(_headTop < _screenScroll){


				$('.head_gnb_wrap .head_gnb > ul > li > div > a > span').css('padding-top','15px');
			        $('.head_gnb_wrap .head_gnb > ul > li').css({
					'height' : '50px',
					'width' : '11.5%'   // (상단에 붙었을때)  메뉴수정시 가로사이즈  
				});
				$('.head_gnb_wrap .head_gnb > ul > li > .sub_cate').css('top' , '-32px');
				$('.t_cate_img').css('display','none');
				$('.head_gnb_wrap').css({
					'position' : 'fixed',
					'top' : '0',
					'z-index' : '10',
					'height' : '50px'
				});

				$('.head_gnb_wrap .head_gnb .sub_logo').css('z-index','10');


			}else{

				$('.head_gnb_wrap .head_gnb > ul > li > div > a > span').css('padding-top','4px');
			        $('.head_gnb_wrap .head_gnb > ul > li').css({
					'height' : '82px',
					'width' : '11.5%'   // (기본메뉴)  메뉴수정시 가로사이즈 
				});
				$('.head_gnb_wrap .head_gnb > ul > li > .sub_cate').css('top','0px');
				$('.t_cate_img').css('display','block');
				$('.head_gnb_wrap').css({
					'position' : 'static',
					'top' : '0',
					'z-index' : '9999',
					'height' : '82px'
				});
				$('.head_gnb_wrap .head_gnb .sub_logo').css('z-index','2');
				$('#todayWrap').css('top','350');

			}


			var _neScroll	= $('.todayWrap_wrap').offset().top;
			if(_neScroll < _screenScroll){
				$('#todayWrap').css({
					'position' : 'fixed',
					'top' : 100
				});
			}else{
				$('#todayWrap').css({
					'position' : 'static',
					'top' : 0
				});
			}
		});

		// 로그인단 마우오버효과
		$('.header_mid > div > a').on({		
			mouseenter : function(){
				$(this).animate({				
				 'opacity' : '0.5'
				})

			},mouseleave : function(){
				$(this).animate({				
				 'opacity' : '1'
				})
			}
		});


		// sns  마우오버효과
		$('.sns_icon > a').on({
			mouseenter : function(){
				var _snsMenu = $(this).find('img').attr('src');
				_snsMenu = _snsMenu.replace('_off','_on');
				$(this).find('img').attr('src',_snsMenu);
			},mouseleave : function(){
				var _snsMenu = $(this).find('img').attr('src');
				_snsMenu = _snsMenu.replace('_on','_off');
				$(this).find('img').attr('src',_snsMenu);
			}
		});


		// 상단띠배너
		$('.top_btn').toggle(function(){
			$('.header_top').animate({
				'height' : 0
			});
			
			$('.top_btn > img').animate({
				'top' : '-38'
			});
			$('.top_btn').animate({
				'bottom' : '-38'
			});
		},function(){
			$('.header_top').animate({
				'height' : '80'
			});	
			$('.top_btn > img').animate({
				'top' : '0'
			});
			$('.top_btn').animate({
				'bottom' : '-20'
			});

		});

	});


	$(function(){
		$('.scroll_slides_side').appendTo('.scroll_slides');
		$('.scroll_slides_side').cycle({
			fx : 'fade',
			timeout : 3000,
			speed : 700,
			next : '.sNext',
			prev : '.sPrev',
		});


	});
})(jQuery);
  
</script>


