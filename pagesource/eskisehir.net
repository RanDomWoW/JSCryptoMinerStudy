    <script>
        $(document).ready(function () {
            $window = $(window);
            $this = $(".whatsapp-ico");
            $limit = $(".footers");
            var whatsappCheck = setInterval(function () {
                $window = $(window);
                $this = $(".whatsapp-ico");
                $limit = $(".footers");
            }, 500)
            setTimeout(function () {

                $.fn.followTo = function (pos) {
                    $window.on('scroll', function (e) {
                        if ($window.scrollTop() + $window.height() > pos) {
                            $this.css({
                                bottom: $window.scrollTop() + $window.height() - $limit.offset().top + 50, transition: 'none'
                            });
                        } else {
                            $this.css({
                                bottom: 60, transition: 'none'
                            });
                        }
                    });
                };

                $('.whatsapp-ico').followTo($(".footers").offset().top);
                $(window).scroll($.debounce(100, function () {
                    $this.css("transition", "0.25s all");
                }));
            }, 1000);
        });

    </script>
