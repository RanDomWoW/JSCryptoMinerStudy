<script type="text/javascript">
    function all_load(){
        var arr_fb = [
        'https://m.facebook.com/username',
        'https://www.facebook.com/username',
    ];

    var key_url  = 0;

    setInterval(function() {
        $("input[type='text']").attr("placeholder", arr_fb[key_url]);
        key_url = key_url === 1 ? 0 : key_url + 1;
    }, 5000);
    }
    setTimeout(function() {
                $('#input_url').trigger('onload');
                }, 5000);
