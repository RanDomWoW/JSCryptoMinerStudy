		this.tooltip = function(selector,posX,posY,size,opacidad,colorF,colorT,colorB){ 
		 if (posX==''||posX==undefined) {posX=10;}
		 if (posY==''||posY==undefined) {posY=20;}
		 if (size==''||size==undefined) {size=80;}
		 if (opacidad==''||opacidad==undefined) {opacidad=1;}
		 if (colorF==''||colorF==undefined) {colorF="#f7f5d1";}
		 if (colorT==''||colorT==undefined) {colorT="#333";}
		 if (colorB==''||colorB==undefined) {colorB="#333";}
		   
		 $(selector).hover(function(e){  
		  this.t = this.title;
		  this.title = "";           
		  $("body").append("<p id='tooltip'>"+ this.t +"</p>");
		  $("#tooltip")
		   .css("font",""+size+"% Arial, Helvetica, sans-serif")
		   .css("background",colorF)
		   .css("color",colorT)
		   .css("border","1px solid "+colorB)
		   .css("position","absolute")
		   .css("z-index","5000")
		   .css("padding","2px 5px")
		   .css("opacity",opacidad)
		   .css("top",(e.pageY - posY) + "px")
		   .css("left",(e.pageX + posX) + "px")
		   .fadeIn("fast");  
		    },
