		$("#giftSearcher").keyup(function(){

			var finalSearch = "";

			var kwd = $(this).val();

			// return false;
			if( !giftSearching ){
				giftSearching = true;
				App.send_request({
					url: "gift-searcher?desktop=1",
					data: {
						name:  kwd
					},
					callback: function(response){
						if( response.status && response.result ){
							console.log( response.result );
							var finalGiftsHtml = "";
							$.each(response.result,function(i){
								var target = response.result[i];
								finalGiftsHtml += App.build_template("#template-giftresult",{
									icon: BaseUrl + "img/games/icon/"+ target.game_id + ".png",
									name: target.game_name,
									url: BaseUrl + "game/"+ target.game_id + "/bonus-collector"
								});
							});
							$("#giftSearchResults").html( finalGiftsHtml );
						}
						giftSearching = false;
					}
				});
			}
		});
	</script>

