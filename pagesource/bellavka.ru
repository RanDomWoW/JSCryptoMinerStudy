// отслеживание баннеров
function banner_look() {
	var page = "home",
		pos = 1;
	$('.banner_center, .banner_left, .banner_right, .banner_right_bottom, .content_banner, .banner_cat, .stocks_item').each(function(index, element) {
        var slot = $(this).attr('class'),
			id = $(this).find('img').eq(0).attr('src');
		id = id.replace(/^.*\/(.+)\..*?$/, "$1");
		id = page + '_' + id;
		$(this).attr('data-id',id);
		$(this).attr('data-position',pos + "_" + slot);
		if (ga != undefined) ga('ec:addPromo', { 
		  'id': id,
		  'name': id,
		  'position': pos + "_" + slot
		});
		pos++;
    });
}

function banner_click($el) {
        var pos = $el.attr('data-position'),
			id = $el.attr('data-id');
		if (ga != undefined) ga('ec:addPromo', { 
		  'id': id,
		  'name': id,
		  'position': pos
		});
		if (ga != undefined) ga('ec:setAction', 'promo_click');
  		if (ga != undefined) ga('send', 'event', 'Ecommerce', 'click', 'banner_click');
}

