<script type="text/javascript" defer>
    $(document).ready(function () {
        // inits the bootstrap's popover
        // for the header customer care number
        $('[data-toggle="popover"]').popover();
        $(window).on("scroll", function () {
            $(".jsCookieBanner").hide();
        });

        var frmInputTooltip = function () {
            var msgEmail = "Lâindirizzo email ci serve per <b>eventuali comunicazioni</b> circa lâesito dei tuoi preventivi e delle tue richieste";
            var msgPhone = "Senza un numero di telefono valido non potremo prendere in carico la tua richiesta";
            var $email = $("#frmEmail, #email");
            var $phone = $("#frmPhone, #phone, #frmCellulare");

            $email.focus(function () {
                $(this).parent().append("<div class='email-tooltip'><strong>Email</strong><p></i>" + msgEmail + "</p></div>");
            });
            $email.blur(function () {
                $(this).parent().find(".email-tooltip").remove();
            });
            $phone.focus(function () {
                $(this).parent().append("<div class='phone-tooltip'><strong>Cellulare</strong><p></i>" + msgPhone + "</p></div>");
            });
            $phone.blur(function () {
                $(this).parent().find(".phone-tooltip").remove();
            });
        };
        frmInputTooltip();

        var menu = {
            //it manages the $(".jsMenuBlock") and the $(".jsHamburger")
            //those components are located into 'navbar-fixed-top.php' and 'navbar-menu-index.php'
            onScroll: function () {
                if ($(window).scrollTop() > 125) {
                    $(".jsHamburger").show().parent().addClass("navbar-header--hamburger");
                } else {
                    $(".jsHamburger").hide().removeClass("open").parent().removeClass("navbar-header--hamburger");
                    $(".jsMenuBlock").removeClass("container").removeClass("menu-block--fixed");
                }
            },

            hamburger: function () {
                $('.jsHamburger').click(function () {
                    $(this).toggleClass('open');
                    $(".jsMenuBlock").toggleClass("container").toggleClass("menu-block--fixed");
                });
            },

            lazyload: function () {
                $(".jsMenuBlock").children().children().on("mouseenter", function () {
                    if (!$(this).hasClass("logo-loaded")) {
                        $(this).addClass("logo-loaded");
                        $(this).find("img").each(function () {
                            var src = $(this).data("src");
                            if (src !== "") {
                                $(this).prop("src", src);
                                $(this).next("span").remove();
                            }
                        })
                    }
                })
            },

            init: function () {
                this.hamburger();
                this.onScroll();
                this.lazyload();
            }
        };

        $("nav.jsMenuBlock").is(":visible") ? menu.init() : null;
        window.addEventListener("scroll", function () {
            $("nav.jsMenuBlock").is(":visible") ? menu.onScroll() : null;
        });


        !function () {
            function createOnClickBehaviour(id, i) {
                var wrapperId = id;
                var index = i;
                return function (e) {
                    e = e || window.event;
                    var targ = e.target || e.srcElement;
                    if (targ.nodeType === 3) targ = targ.parentNode;

                    var rating_element_id = wrapperId;
                    var rating = index;
                    this.parentNode.classList.add('voted');
                    $.ajax({
                        method: "POST",
                        url: "/rating",
                        data: {rating_element_id: rating_element_id, rating: rating}
                    }).done(function (msg) {
                        var old_element = targ.parentNode.parentNode;
                        var new_element = old_element.cloneNode(true);
                        old_element.parentNode.replaceChild(new_element, old_element);
                        Array.prototype.slice.call(targ.querySelectorAll('input')).map(function (el) {
                            el.disabled = true;
                        });
                    });
                }
            }


            function createStar(wrapperId, index) {

                var fullStarInputElement = document.createElement('input');
                fullStarInputElement.type = 'radio';
                fullStarInputElement.classList.add('sm-rating-input');
                fullStarInputElement.id = wrapperId + 'star' + index;
                fullStarInputElement.value = index;
                fullStarInputElement.name = 'sm-rating-' + wrapperId;
                fullStarInputElement.onclick = createOnClickBehaviour(wrapperId, index);

                return fullStarInputElement
            }

            function createLabel(id) {
                var fullStarLabelElement = document.createElement('label');
                fullStarLabelElement.classList.add('full');
                fullStarLabelElement.classList.add('sm-rating-label');
                fullStarLabelElement.htmlFor = id;
                return fullStarLabelElement
            }

            Array.prototype.slice.call(document.querySelectorAll('.sm-rating')).map(function (el) {
                var alreadyFilled = !!el.querySelector('.sm-rating-wrapper');
                var useSchema = !el.classList.contains('no-schema');


                var wrapperId = el.id;
                if (!alreadyFilled) {
                    $.ajax({
                        method: "GET",
                        url: "/rating?rating_element_id=" + encodeURIComponent(el.id)
                    }).done(function (msg) {
                        var starWrapperElement = document.createElement('fieldset');
                        starWrapperElement.classList.add('sm-rating-wrapper');

                        var titleElement = document.createElement('div');
                        titleElement.classList.add('sm-rating-title');

                        titleElement.innerText = 'Lascia la tua recensione';

                        var previousRatingElement = document.createElement('div');
                        previousRatingElement.classList.add('sm-rating-subtext');
                        previousRatingElement.classList.add('clearfix');
                        if(useSchema) {
                            previousRatingElement.setAttribute('itemprop', 'aggregateRating');
                            previousRatingElement.setAttribute('itemscope', '');
                            previousRatingElement.setAttribute('itemtype', 'http://schema.org/AggregateRating');
                            previousRatingElement.innerHTML = 'Valutazione media: <span itemprop="ratingValue">' + msg.value + '</span> su <span itemprop="bestRating">5</span>. Basata su <span itemprop="ratingCount">' + msg.votes + '</span> recensioni';
                        }
                        else{
                            previousRatingElement.innerHTML = 'Valutazione media: <span>' + msg.value + '</span> su <span>5</span>. Basata su <span>' + msg.votes + '</span> recensioni';
                        }

                        var fullStarInputElement;
                        var fullStarLabelElement;

                        for (var i = 1; i <= 4; i++) {
                            fullStarInputElement = createStar(wrapperId, i);
                            fullStarLabelElement = createLabel(fullStarInputElement.id);
                            if (i === Math.round(msg.value || 3)) {
                                fullStarInputElement.checked = true;
                            }
                            starWrapperElement.insertBefore(fullStarLabelElement, starWrapperElement.firstChild);
                            starWrapperElement.insertBefore(fullStarInputElement, starWrapperElement.firstChild);
                        }
                        fullStarInputElement = createStar(wrapperId, 5);
                        fullStarLabelElement = createLabel(fullStarInputElement.id);
                        if (i === Math.round(msg.value || 3)) {
                            fullStarInputElement.checked = true;
                        }
                        starWrapperElement.insertBefore(fullStarLabelElement, starWrapperElement.firstChild);
                        starWrapperElement.insertBefore(fullStarInputElement, starWrapperElement.firstChild);
                        if (el.classList.contains('review-box')) {
                            var rightArrowElement = document.createElement('i');
                            rightArrowElement.classList.add('fa');
                            rightArrowElement.classList.add('fa-caret-right');

                            var linkReviewBoxElement = document.createElement('a');
                            linkReviewBoxElement.setAttribute('href', "#review-box");
                            linkReviewBoxElement.classList.add('sm-link-review-box');
                            titleElement.appendChild(rightArrowElement);
                            linkReviewBoxElement.appendChild(titleElement);
                            el.appendChild(linkReviewBoxElement);
                        }
                        else {
                            el.appendChild(titleElement);
                        }
                        el.appendChild(starWrapperElement);
                        el.appendChild(previousRatingElement);
                    });
                }
                else{
                    Array.prototype.slice.call(el.querySelectorAll('.sm-rating-input')).map(function (fullStarInputElement, index) {
                        fullStarInputElement.onclick = createOnClickBehaviour(wrapperId, 5-index);
                    });
                }
            });
        }();
    });
</script>




  
