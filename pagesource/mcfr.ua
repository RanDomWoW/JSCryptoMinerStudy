	  			$('.loading').hide();
	  			$('.maincont').fadeIn('slow');
	  			$('#animate').addClass('animated fadeInDown');
	  			$('#starter').addClass('animated fadeInUp');
	  			$('#map').addClass('animated fadeInUp');
	  			$( ".prodglimp" ).hover(function() {
		  			$(this).addClass('animated flipInX');
		  		});
		  		$('.container').hover(function() {
		  			$( ".prodglimp" ).removeClass('animated flipInX');
		  		});

		  		$(window).scroll( function(){
			  		$('.hideme').each( function(i){
				  		var bottom_of_object = $(this).position().top + $(this).outerHeight() - 50;
				  		var bottom_of_window = $(window).scrollTop() + $(window).height();
				  		if( bottom_of_window > bottom_of_object ){
					  		$(this).animate({'opacity':'1'},500);
					  	}
					});
					$('.fromleft').each( function(i){
				  		var bottom_of_object = $(this).position().top + $(this).outerHeight();
				  		var bottom_of_window = $(window).scrollTop() + $(window).height();
				  		if( bottom_of_window > bottom_of_object ){
					  		$(this).addClass('animated fadeInLeftBig');
					  	}
					});
					$('.fromright').each( function(i){
				  		var bottom_of_object = $(this).position().top + $(this).outerHeight();
				  		var bottom_of_window = $(window).scrollTop() + $(window).height();
				  		if( bottom_of_window > bottom_of_object ){
					  		$(this).addClass('animated fadeInRightBig');
					  	}
					});
					$('.product_tit').each( function(i){
				  		var bottom_of_object = $(this).position().top + $(this).outerHeight();
				  		var bottom_of_window = $(window).scrollTop() + $(window).height();
				  		if( bottom_of_window > bottom_of_object ){
					  		$(this).addClass('animated flipInX');
					  	}
					});
					$('.fromtop').each( function(i){
				  		var bottom_of_object = $(this).position().top + $(this).outerHeight();
				  		var bottom_of_window = $(window).scrollTop() + $(window).height();
				  		if( bottom_of_window > bottom_of_object ){
					  		$(this).addClass('animated fadeInDown');
					  	}
					});
				});
	$('.sist_link,.gourn_link').on('click', function(e){
	   e.preventDefault();
    });
  	$(".sist_link").click(function() {
		$('html, body').animate({
		    scrollTop: $("#sist_link").offset().top-25
		}, 1000);
  	});
  	$(".gourn_link").click(function() {
	   $('html, body').animate({
    		scrollTop: $("#gourn_link").offset().top-25
	   }, 1000);
	});
			});
		</script>


  	</body>
</html>