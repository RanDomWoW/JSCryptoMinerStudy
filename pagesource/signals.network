            <!-- Referrer Code -->
            <script src="/js/crypto-js/3.1.9-1/crypto-js.min.js"></script>
            <script src="/js/crypto-js/3.1.9-1/hmac-sha256.min.js"></script>
            <script src="/js/crypto-js/3.1.9-1/enc-base64.min.js"></script>
            <script>
            (function(){
              function createCookie(name, value, days){
                if(days){
                  var date = new Date();
                  date.setTime(date.getTime() + (days*24*60*60*1000));
                  var expires = "; expires=" + date.toGMTString();
                }
                else var expires = "";
                document.cookie = name + "=" + value + expires + "; path=/; domain=signals.network";
              }

              function readCookie(name){
                var nameEQ = name + "=";
                var ca = document.cookie.split(';');
                for(var i=0; i < ca.length; i++){
                  var c = ca[i];
                  while(c.charAt(0)==' ') c = c.substring(1,c.length);
                  if(c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
                }
                return null;
              }

              function eraseCookie(name){
                createCookie(name, "", -1);
              }

              function hasReferrerPath(pathname) {
                return pathname.indexOf('/join/') === 0;
              }

              function getReferrerFromPath(pathname) {
                return pathname.split('/')[2];
              }

              function createHash(referrerId, count, timestamp) {
                var message = referrerId+count.toString()+timestamp;
                var hash = CryptoJS.HmacSHA256(message, "38487747703936657550252c29244f307c77535f72435d6a2355597348");
                return CryptoJS.enc.Base64.stringify(hash);
              }

              function sendPost(url, params, callback) {
                var ajax = new XMLHttpRequest();
                ajax.open("POST", url, true);
                ajax.setRequestHeader("Content-Type", "application/json");
                ajax.onreadystatechange = function() {
                  if(ajax.readyState === 4) {
                    if('undefined' !== typeof(callback)){
                      // We don't care what is the response
                      // Just execute the post AJAX callback
                      callback();
                    }
                  }
                }
                ajax.send(JSON.stringify(params));
              }

              function sendToBounty(referrerId, callback) {
                var count = 1;
                var timestamp = new Date().toISOString();
                var params = {
                  ReferralId:referrerId,
                  Count:count,
                  Timestamp:timestamp,
                  Signature:createHash(referrerId,count,timestamp)
                };
                sendPost("https://bounty.signals.network/api/referral/add", params, callback);
              }

              function trackReferrerId(referrerId, callback) {
                if('undefined' !== typeof(ga)) {
                  ga('send', 'event', {
                    'eventCategory': 'Referrals',
                    'eventAction': 'visitPage',
                    'eventLabel': referrerId,
                  });
                }
                sendToBounty(referrerId, callback);
              }

              /* Referrer Agency Logic */

              function hasReferrerAgencyPath(pathname) {
                return pathname.indexOf('/join-token-sale/') === 0;
              }

              function getReferrerAgencyFromPath(pathname) {
                return pathname.split('/')[2];
              }

              function handleReferrerAgency() {
                var daysToBeAlive = 10 * 365;
                var cookie = readCookie('referrerAgency');
                var currentLocation = document.location.pathname;
                var isOnJoinUrl = hasReferrerAgencyPath(currentLocation);

                if (isOnJoinUrl && (!cookie || cookie === 'none')) {
                  var referrerAgency = getReferrerAgencyFromPath(currentLocation);
                  createCookie('referrerAgency', referrerAgency, daysToBeAlive);
                } else if (!cookie) {
                  createCookie('referrerAgency', 'none', daysToBeAlive);
                }

                if (isOnJoinUrl) {
                  document.location = '/token-sale-registration';
                }

              }

              handleReferrerAgency();

              var cookieVal = readCookie('referrerId');

              if(document.location.pathname && hasReferrerPath(document.location.pathname)) {
                if(null === cookieVal){
                  var referrerId = getReferrerFromPath(document.location.pathname);
                  createCookie('referrerId', referrerId, 10*365);
                  trackReferrerId(referrerId);
                } else {
                  document.location = '/';
                }
                return;
              } else if(document.location.search && ('' !== document.location.search.substr(1))){
                var urlVars = document.location.search.substr(1).split('&');
                for(var i in urlVars){
                  var pair = urlVars[i].split('=');
                  if('ref' == pair[0]){
                    if('' !== pair[1].substr(1)){
                      if(null === cookieVal){
                        var referrerId = pair[1];
                        createCookie('referrerId', referrerId, 10*365);
