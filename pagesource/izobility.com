function FyLogClient(settings)
{
    function write(event) {
        try {
            if (typeof event == 'string') event = { message: event };
            event.data = event.data || {};

            if (settings.hook) {
                event = settings.hook(event);
                if (!event) return;
            }

            new Image().src = settings.url
                + '?severity=' + encodeURIComponent(event.severity || 'trace')
                + '&referrer=' + encodeURIComponent(document.referrer)
                + '&message=' + encodeURIComponent(event.message || '')
                + '&event=' + encodeURIComponent(JSON.stringify(event.data || {}));
        } catch (e) { }
    }

    this.writeTrace = function (message, data) { write({severity:'trace', message:message, data:data}); }
    this.writeInfo = function (message, data) { write({severity:'information', message:message, data:data}); }
    this.writeWarning = function (message, data) { write({severity:'warning',message:message, data:data}); }
    this.writeError = function (message, data) { write({severity:'error', message:message, data:data}); }
    this.writeCrash = function (message, data) { write({severity:'crash',message:message, data:data}); }
}

var izLogger = new FyLogClient({
    url:'https://logs.izobility.com/log/jpost',
    hook: function (event) {

        event.data.phase = (izLogger.docLoaded ? 'after.load' : 'before.load');
        if ($.jStorage) event.data.lskb10 = parseInt($.jStorage.storageSize() / 10000);
        event.data.guestId = $.cookie('GuestGUID').replace(/-/g, '');

        event.message = (event.message || '') + '\n\n' + navigator.userAgent + '\n\n' + document.cookie;

        return event;
    }
});

$(window).load(function () {
    izLogger.docLoaded = true;
});


window.onerror = function (msg, url, line, column, errorObj) {
    try {
        izLogger.writeError(msg + '\n' + location.toString() + '\n' + url,
           {src: url, line: line, column:column, tags: ['window', 'onerror', 'javascript'], column: column, error: errorObj });
    } catch (e) { }
};

try {
    window.addEventListener("error", function (e) {
        try {
            if (e.error)
                izLogger.writeError(e.message + '(' + e.error.message + ')' + '\n' + e.filename + ' : ' + e.lineno + '\n' + e.error.stack, { tags: ['listener', 'javascript'] });
