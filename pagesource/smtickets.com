	function results (form) {
	
		var answer = confirm("You will be redirected to Skyjet Airlines website. Do you want to continue?")
		if (answer){
		 
			var depdate = form.dep_date2.value;
			var retdate = form.ret_date2.value;

			var adult= form.pax_adult.value;
			var child= form.pax_child.value;
			var infant= form.pax_infant.value;

			var origin=form.org.value;


			var today = new Date();
			var dd = today.getDate();
			var mm = today.getMonth()+1; //January is 0!

			var yyyy = today.getFullYear();
			if(dd<10){dd='0'+dd} if(mm<10){mm='0'+mm} var today = yyyy+'/'+mm+'/'+dd;


			var desireddate=depdate.replace(/[&\/\\#,+()$~%.'":*?<>{}]/g,"");
			var curdate=today.replace(/[&\/\\#,+()$~%.'":*?<>{}]/g,"");
			var returndate=retdate.replace(/[&\/\\#,+()$~%.'":*?<>{}]/g,"");

			var ret_num=Number(returndate);
			var des_num=Number(desireddate);
			var cur_num=Number(curdate);

			var adult2= Number(adult);
			var child2= Number(child);
			var infant2=Number(infant);

			var total_pax=adult2+child2+infant2;


			var radio1=form.radio1[0].value;
			var radio2=form.radio1[1].value;

			if(origin!=" "){
				if(total_pax<=9){
					if(adult>=infant){ 
					
						//current date is greater than departure date
						if(cur_num>des_num){
						alert('Your departure date has past already.');

						}
						//range departure to return
						else if(des_num<ret_num){ 
							depdate = depdate.replace(/\//g,"");
							retdate = retdate.replace(/\//g,"");
							
							document.getElementById('departureDate2').value = depdate;
							document.getElementById('returnDate2').value = retdate; 
 							document.forms["form"].submit();
	 
						}
						//return date greater than current date
						else if(ret_num<cur_num){
							alert('Your departure date is higher than return.');
						}else{
							depdate = depdate.replace(/\//g,"");
							retdate = retdate.replace(/\//g,"");
							
							document.getElementById('departureDate2').value = depdate;
							document.getElementById('returnDate2').value = retdate;
							document.forms["form"].submit();	  
						}
					 
						if(document.getElementById('radio1').checked) {
							if(des_num>ret_num){
							// $("#myModal6").reveal();
							alert('Your departure date is higher than return.');
							}
						}
	 
						//for one way

						else if(radio2==0){
							if(des_num>ret_num){
								depdate = depdate.replace(/\//g,"");
								retdate = retdate.replace(/\//g,"");
								
								document.getElementById('departureDate2').value = depdate;
								document.getElementById('returnDate2').value = retdate;
								document.forms["form"].submit();	  
							}
						} 
				 
					}
					else{
					// $("#myModal2").reveal();
					alert('Adult Passengers should be more than infant passengers.');
					}
					
				}
				else{
					alert('You cannot use internet book for more than the group limit of 9 passengers.');

				}
			}
			else{
			alert('Please select origin airport.');

			}	 
			
		}
		else{
			return false;
		}
		
  	}
 
</script>
		
