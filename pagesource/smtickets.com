	<script>
	$(document).ready(function(){
		var $calendar = $('#calendar');
		var SelectedDates = [];
		var SelectedText = [];
		var events = [];
			obj = jQuery.parseJSON(localStorage.getItem("allEvents"));
			$.each( obj, function( key, value ) {
				date = new Date(value.event_date_time);
				fulldate = (date.getMonth() + 1) + '/' + date.getDate() + '/' +  date.getFullYear();
				
				if(typeof events[fulldate] === 'undefined'){
					events[fulldate] = '';
				}
				events[fulldate] = events[fulldate]+'<li>'+value.event_long_title+'</li>';
				
				SelectedDates[new Date(fulldate)] = new Date(fulldate);
				SelectedText[new Date(fulldate)] = '<ul>'+events[fulldate]+'</ul>'	
			});
			
			$calendar.datepicker({
				minDate: 0,
				beforeShowDay : function(date){
					var Highlight = SelectedDates[date];
					var HighlighText = SelectedText[date];
					if(Highlight){
						return [true, "ui-highlight", HighlighText];
					}else{
						return [false];
					}					
				},
				onSelect: function (fd) {
					window.location="https://www.smtickets.com/events/calendar/"+fd;
				}
			}).on("mouseenter", ".ui-datepicker-calendar", function() {
				$.each($(".ui-tooltip"), function (index, element) {
					$(this).remove();
				});
				$(this).tooltip({
					track: true,
					content: function(){
						var event = $(this).attr('title');
						return event
					},
					position: {
						my: "center bottom-10",
						at: "center top"
					}
				});
			}).on("mouseleave", ".ui-datepicker-calender", function(){
				$.each($(".ui-tooltip"), function (index, element) {
					$(this).remove();
				});
			});
	});
	</script>
