function fetch_unix_timestamp()
{
 return parseInt(new Date().getTime().toString().substring(0, 15))
}
var timestamp = fetch_unix_timestamp();
var page = "../xml/tygiavang.xml?t="+timestamp;
function ajax(url,target)
 {
    // native XMLHttpRequest object
   document.getElementById(target).innerHTML = '<img src="http://www.sjc.com.vn/images/loading.gif" border="0" width="16" height="16" vspace="3" align="absmiddle" /> <font color="#000">Loading data...</font>';
   if (window.XMLHttpRequest) {
       req = new XMLHttpRequest();
       req.onreadystatechange = function() {ajaxDone(target);};
       if (navigator.userAgent.search("MSIE") >= 0){
			req.open("POST", url, true);
		}
		else
		{
			req.open("GET", url, true);
		}
       req.send();
   // IE/Windows ActiveX version
   } else if (window.ActiveXObject) {
       req = new ActiveXObject("Microsoft.XMLHTTP");
       if (req) {
           req.onreadystatechange = function() {ajaxDone(target);};
           if (navigator.userAgent.search("MSIE") >= 0){
			req.open("POST", url, true);
		}
		else
		{
			req.open("GET", url, true);
		}
           req.send();
       }
   }
		   setTimeout("ajax(page,'price1')", 30000);
}

function ajaxDone(target) {
// only if req is "loaded"
if (req.readyState == 4) {
// only if "OK"
if (req.status == 200 || req.status == 304) {

xmlDoc=req.responseXML;
city=xmlDoc.getElementsByTagName("city")[0].getAttribute("name");
update=xmlDoc.getElementsByTagName("ratelist")[0].getAttribute("updated");

var s = new StringBuilder();

s.append('   <table class="table scroll" ><tbody> ');
s.append('<tr><td colspan="3"  bgcolor="#A78D61" align="center" class="text_tgvang_mota" style="color:#fff">  '+update+' <br/>(Đơn vị: ngàn đồng/lượng)</td> </tr>');
//s.append('<tr><td colspan="3"  bgcolor="A78D61" align="center" class="text_tgvang_mota" style="color:#fff"> &#272;&#417;n v&#7883; t&#237;nh: ng&#224;n &#273;&#7891;ng/l&#432;&#7907;ng</td></tr>');
s.append('<tr style="background:#E6E9E9; font-weight:bold; color:#000; vertical-align:top; text-align:center; "><td  align="center" valign="top" style="font-size:11px;width:220px; ">LOẠI VÀNG</td><td style="font-size:11px">MUA VÀO</td><td style="font-size:11px">BÁN RA</td></tr>');
var items = xmlDoc.getElementsByTagName("city");
for (var i = 0 ; i < items.length ; i++) {
	//for (var i = 0 ; i < items.length ; i++) 
	{
	// get one item after another
	var item = items[i];
	var length=item.getElementsByTagName("item").length
	s.append(' <tr style="color:#003468; font-weight:bold; text-transform:uppercase;background:#D9CB99" style="height:5px"><th colspan="4" style="height:5px;text-align:left">'+items[i].getAttribute("name")+'</th></tr>');
	for(var j=0;j<length;j++)
	{
	// now we have the item object, time to get the contents
		var type = item.getElementsByTagName("item")[j].getAttribute("type");
		var buy = item.getElementsByTagName("item")[j].getAttribute("buy");
		var sell = item.getElementsByTagName("item")[j].getAttribute("sell");

		s.append('<tr><td align="left" class="text_tgvang_chitiet" width="220" style="padding-left:5px;background:#18406f; color:#FFF">'+ type +'</td><td align="left" class="text_tgvang_chitiet"  style="padding-right:5px;background:#18406f; color:#FFF">'+ buy+'</td><td align="center" class="text_tgvang_chitiet"  style="padding-right:5px;background:#18406f; color:#E2D682">'+ sell+'</td></tr>');		
		if(i==0 && j==0)
		{
			s.append('<tr><td align="left" class="text_tgvang_chitiet" width="220" style="padding-left:5px;background:#18406f; color:#FFF">V&#224;ng SJC 5 chỉ</td><td align="left" class="text_tgvang_chitiet"  style="padding-right:5px;background:#18406f; color:#FFF">'+buy+'</td><td align="center" class="text_tgvang_chitiet"  style="padding-right:5px;background:#18406f; color:#E2D682">'+numberFormat(parseFloat(sell)*1000+20)+'</td></tr>');		
			s.append('<tr><td align="left" class="text_tgvang_chitiet" width="220" style="padding-left:5px;background:#18406f; color:#FFF">V&#224;ng SJC 0.5 chỉ, 1 chỉ, 2 chỉ</td><td align="left" class="text_tgvang_chitiet" style="padding-right:5px;background:#18406f; color:#FFF">'+ buy+'</td><td align="center" class="text_tgvang_chitiet"  style="padding-right:5px;background:#18406f; color:#E2D682">'+numberFormat(parseFloat(sell)*1000+30)+'</td></tr>');	
		}
	}
	}
}
s.append('</table>');
document.getElementById(target).innerHTML= s.toString();  
} else {
document.getElementById(target).innerHTML="ajax error:\n" +
req.statusText;
}
}
}
ajax(page,'price1');
function numberFormat(nStr,prefix){
    var prefix = prefix || '';
    nStr += '';
    x = nStr.split('.');
    x1 = x[0];
    x2 = x.length > 1 ? '.' + x[1] : '';
    var rgx = /(\d+)(\d{3})/;
    while (rgx.test(x1))
        x1 = x1.replace(rgx, '$1' + '.' + '$2');
    return prefix + x1 + x2;
}
