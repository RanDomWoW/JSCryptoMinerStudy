function validateSearchTunes(){
        var searchTxt = $("#search_text_tunes").val();
        if(searchTxt=="" || searchTxt=="Enter your name here"){
                alert("Please enter search keyword");
                $("#search_text_tunes").focus();
                return false;
        }
        var iChars = "!@#$%^*()+=[]\\\;,./{}|\:<>?";
        for (var i = 0; i < searchTxt.length; i++) {
        if (iChars.indexOf(searchTxt.charAt(i)) != -1) {
            alert ("Please enter valid search keyword");
            return false;
        }
        }
        var type = $("#pageAccessTunes").val();
        switch(type){
                case "HT":
                        var url = 'https://www.airtelhellotunes.in/tone/search-result-tunes/'+$('input:radio[name="search_type_tunes"]:checked').val()+'/'+replaceAll(searchTxt," ","+");
                        break;
        }
        var radioButtonValue = $('input:radio[name="search_type_tunes"]:checked').val();
                location.href = url;
        return true;
}
function validateSearch(){
        var searchTxt = $("#search_text").val();
        if(searchTxt=="" ){
                alert("Please enter search keyword");
                $("#search_text").focus();
                return false;
        }
        var iChars = "!@#$%^*()+=[]\\\;,./{}|\:<>?";
        for (var i = 0; i < searchTxt.length; i++) {
        if (iChars.indexOf(searchTxt.charAt(i)) != -1) {
            alert ("Please enter valid search keyword");
            return false;
        }
        }
        var radioButtonValue = $('input:radio[name="search_type"]:checked').val();
        var type = $("#pageAccess").val();
        if(radioButtonValue == "sunny_leone" || radioButtonValue == "name"){
                switch(type){
                        case "HT":
                                var url = 'https://www.airtelhellotunes.in/tone/search-result-tunes/'+$('input:radio[name="search_type"]:checked').val()+'/'+replaceAll(searchTxt," ","+");
                                break;
                }
        }else{
                switch(type){
                        case "HT":
                                var url = 'https://www.airtelhellotunes.in/tone/search-result/'+$('input:radio[name="search_type"]:checked').val()+'/'+replaceAll(searchTxt," ","+");
                                                                break;
                        case "AL":
                                var url = 'https://www.airtelhellotunes.in/album/search-result/'+replaceAll(searchTxt," ","+")+'/album_name';
