                            var suggestionMessage = suggestionsSpans.join(' | ');
                            $("[data-js=chatbox_suggestions_nick]").removeClass('hide');
                            $("[data-js=suggestions_no_valid]").html(suggestionMessage);

                        }
                    }
                });
            };


            return;
        }
