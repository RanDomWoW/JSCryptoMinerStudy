                .replace(/^-+/, '')             // Trim - from start of text
                .replace(/-+$/, '');            // Trim - from end of text

            var url_source = "/internalapi/users/username-available";

            $("[data-js=nick_not_valid]").removeClass("hide");
            //if after clean nick, nick is empty, dont call to api
            if (nick != ''){
                $.ajax({
