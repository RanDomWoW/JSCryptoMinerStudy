    function isValidNick(nick)
    {
        if(nick.length > 32 ){
            return false;
        }

        for (i = 0; i < nick.length; i++) {
            if(nick[i] >= 'A' && nick[i] <= '}'){
                continue;
            }
            if ((((nick[i] >= '0') && (nick[i] <= '9')) || (nick[i] == '-')) && i != 0 ){
                continue;
            }
            return false;
        }
        return true;
    }

