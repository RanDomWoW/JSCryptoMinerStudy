    $(document).on('submit', 'form[data-pjaxchatform]', function(event) {
        var nick = $('input[name=nick]').val();

        if(!isValidNick(nick)) {
            $("i", this).removeClass("icon-spinner icon-spin");
            $(".ajaxbtn").attr("disabled", true);
            event.preventDefault();
            event.stopPropagation();
            nick =  nick.replace(/\s+/g, '-')           // Replace spaces with -
                .replace(/[^\w\-\[\]\_\{\}\^]+/g, '')       // Remove all non-word chars
