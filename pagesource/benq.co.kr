        },
        next: function () {
            var newsCount = $(".news_ul li").length;
            if (move_index < newsCount) {
                $('.Content ul').stop(false, true).animate({'margin-left': '-=803px'});
                move_index++;
            }
        },
        prev: function () {
            if (move_index > 1) {
                $('.Content ul').stop(false, true).animate({'margin-left': '+=803px'});
                move_index--;
            }
        },
        showCurse: function (newsCount) {
            var newsCount = $(".news_ul li").length;
            if (move_index > 1 && move_index <= newsCount) {
                $('.ArrowLeft').css("visibility", "visible");
            }
            else {
                $('.ArrowLeft').css("visibility", "hidden");
            }
            if (move_index >= 1 && move_index < newsCount) {
                $('.ArrowRight').css("visibility", "visible");
            }
            else {
                $('.ArrowRight').css("visibility", "hidden");
            }
        }
    }
    $(function () {
        new_curse.init();
    })
