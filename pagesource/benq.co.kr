		if($("#home_video_top").html().trim()=="" && $("#home_video_mask .micro_film_tit").html().trim()==""){
			$(".micro_film").hide();
			top_video = false;
		}

		$(".video_list .video_list_c").each(function(index,element){
			if($(this).find("img").count > 0 || $(this).find(".video_list_c_txt").html()!=""){
				list_video = true;
			}
			if(index>2 && ($(this).find("img").count > 0 || $(this).find(".video_list_c_txt").html()!="")){
				prev_next_btn = true;
			}
		});
		if(!list_video){
			$("#ca-container").hide();
		}
		else if(!prev_next_btn){
			$(".ca-nav-prev").css("visibility","hidden");
			$(".ca-nav-next").css("visibility","hidden");
		}
		if(!top_video && !list_video){
			$(".benq_club ").hide();
		}
	}
}
