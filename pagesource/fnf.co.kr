<script>

var state = 1;
var bg_image_width;
var bg_image_height;

// 메인 메뉴 현제 위치
var this_value;

$(window).ready(function(){
	$('.bg_slides').jBanner({width:1000, height:750});

//	bg_image_width = parseInt($(".bg_image").width());
//	bg_image_height = parseInt($(".bg_image").height());
	bg_image_width = 1000;
	bg_image_height = 750;

	win_width = parseInt($(document).width());
	win_height = parseInt($(document).height());

	if(win_width>1200) win_width = parseInt($(window).width());
	if(win_height>768) win_height = parseInt($(window).height());

	winBgResize();
	footerLayer();

	$(".footer_family").click(function(){
		if(state==1){
			$('#brand_footer_link').show(); 
			$('#family_site_arrow').css('background-image','url(../img/icon/icon_arrow02.gif)')
			state=0;
		}else{
			$('#brand_footer_link').hide(); 
			$('#family_site_arrow').css('background-image','url(../img/icon/icon_arrow01.gif)')
			state=1;
		}
	})

	$('.item').click(function(){
		if(this_value!=$(this).attr("value")){

			$('.item .subitem').hide(500);		
			$('.subitem', this).show(500);
			$(".item .left_item_a").css("color","#D0D0D0");
			$(".left_item_a", this).css("color","#FFFFFF");
			$("#first_subitem", this).css("color","#FFFFFF");
			document.ifrm_contents.focus();

		}
		this_value = $(this).attr("value");
	}); 			   
	$('.left_subitem_a').click(function(){
		$(".left_subitem_a").css("color","#888");
		$(this).css("color","#FFFFFF");

	});
})

$(window).resize(function(){
	win_width = parseInt($(document).width());
	win_height = parseInt($(document).height());

	if(win_width>1200) win_width = parseInt($(window).width());
	if(win_height>768) win_height = parseInt($(window).height());

	winBgResize();
	footerLayer();
})

function winBgResize(){
	

	$("#main_bg").width(win_width+'px');
	$("#main_bg").height(win_height+'px');
	$("#auto_width").attr("width",win_width+'px');
	
	if( ( win_width/win_height ) > bg_image_width/bg_image_height ) {
		$(".bg_image").width(win_width+'px');
		$(".bg_image").height((  bg_image_height  / bg_image_width ) * win_width);
	}else{
		$(".bg_image").height(win_height+'px');
		$(".bg_image").width((  bg_image_width  / bg_image_height ) * win_height);
	}
	
}

function footerLayer(){
	var top;
	 top = win_height - 100 ;
	if(top < 580) top = 580;
	$("#footer_wrap").css("top",top+"px");
}
</script>
