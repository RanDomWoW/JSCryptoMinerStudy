        ser_l.click(function () {
            if (ser == 0) {
                ss--;
            } else {
                xx--;
            }
            ser_next();
        });
        ser_r.click(function () {
            if (ser == 0) {
                ss++;
            } else {
                xx++;
            }
            ser_next();
        });
        $('.service_btn a').click(function () {
            $(this).addClass('cur').siblings().removeClass('cur');
            if ($(this).index() == 0) {
                ss = 0;
            } else if ($(this).index() == 1) {
                ss = 4;
            } else {
                ss = 8;
            }
            ser_next();
        });
        $('.service_btn a').click(function () {
            $(this).addClass('cur').siblings().removeClass('cur');
            if (ser == 0) {
                if ($(this).index() == 0) {
                    ss = 0;
                } else if ($(this).index() == 1) {
                    ss = 4;
                } else {
                    ss = 8;
                }
            } else {
                if ($(this).index() == 0) {
                    xx = 0;
                } else if ($(this).index() == 1) {
                    xx = 4;
                } else {
                    xx = 8;
                }
            }
            ser_next();
        });
        if (ww > 1320) {
            $('.ser_list').css('width', '1200px');
            wh = 4;
        } else if (ww > 1020) {
            $('.ser_list').css('width', '900px');
            wh = 3;
        } else if (ww > 720) {
            $('.ser_list').css('width', '600px');
            wh = 2;
        } else {
            $('.ser_list').css('width', '300px');
            wh = 1;
        }
        function ser_next() {
            if (ww > 1320) {
                $('.ser_list').css('width', '1200px');
                wh = 4;
            } else if (ww > 1020) {
                $('.ser_list').css('width', '900px');
                wh = 3;
            } else if (ww > 720) {
                $('.ser_list').css('width', '600px');
                wh = 2;
            } else {
                $('.ser_list').css('width', '300px');
                wh = 1;
            }
            if (wh < ser_con.children('ul').eq(0).children('li').length) {
                if (ss <= ser_con.children('ul').eq(0).children('li').length - wh && 0 < ss) {
                    ser_con.children('ul').eq(0).animate({ left: ss * -300 + 'px' });
                } else if (ss == -1) {
                    ss = ser_con.children('ul').eq(0).children('li').length - wh;
                    ser_con.children('ul').eq(0).animate({ left: ss * -300 + 'px' });
                } else {
                    ss = 0;
                    ser_con.children('ul').eq(0).animate({ left: ss * -300 + 'px' });
                }
            }
            if (wh < ser_con.children('ul').eq(1).children('li').length) {
                if (xx <= ser_con.children('ul').eq(1).children('li').length - wh && 0 < xx) {
                    ser_con.children('ul').eq(1).animate({ left: xx * -300 + 'px' });
                } else if (xx == -1) {
                    xx = ser_con.children('ul').eq(1).children('li').length - wh;
                    ser_con.children('ul').eq(1).animate({ left: xx * -300 + 'px' });
                } else {
                    xx = 0;
                    ser_con.children('ul').eq(1).animate({ left: xx * -300 + 'px' });
                }
            }
            if (ss < 4 && xx < 4) {
                $('.service_btn a').eq(0).addClass('cur').siblings().removeClass('cur');
            } else if (ss < 8 && xx < 8) {
                $('.service_btn a').eq(1).addClass('cur').siblings().removeClass('cur');
            } else {
                $('.service_btn a').eq(2).addClass('cur').siblings().removeClass('cur');
            }
        };

        //品牌客户的背景
        $('.customer_list ul li:even').addClass('cus');

        //联系我们左右切换
        var pan_con = $('.con_rq');
        var cons = $('.con_rq .contact_nr');
        var let = $('.contact_l');
        var rit = $('.contact_r');
        var b = 0;
        var lenc = cons.length;
        pan_con.css('width', lenc * 100 + '%');
        cons.css('width', (1 / lenc) * 100 + '%');
        let.click(function () {
            b--;
            shows()
        });
        $(".tjxq").click(function () {
            b--;
            shows()
        });
        $(".phone_message").click(function () {
            $("html,body").animate({ scrollTop: $("#contact").offset().top }, 1000);
            b--;
            shows()
        });
        rit.click(function () {
            b++;
            shows()
        });
        function shows() {
            if (b < lenc && b >= 0) {
                pan_con.animate({ left: -b + '00%' });
            } else if (b < 0) {
                b = lenc - 1;
                pan_con.animate({ left: -b + '00%' });
            } else {
                b = 0;
                pan_con.animate({ left: -b + '00%' });
            }
        }


    });
