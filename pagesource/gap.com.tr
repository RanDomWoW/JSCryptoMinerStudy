                            jQuery("div.cambridgeDisrupter").insertBefore(jQuery("div.nav-shift.fixed-header_at-sm"));
                            jQuery("div.universal-modal--backdrop").insertBefore(jQuery("div.nav-shift.fixed-header_at-sm"));
                            jQuery("div.cambridgeDisrupter").css("display", "block");
                            jQuery("div.cambridgeDisrupter").animate({
                                height: targetHeight + "px"
                            }, 550, function () {
                                // animate
                                var tOffset = jQuery("div.cambridgeDisrupter").offset();
                                var tHeight = jQuery("div.cambridgeDisrupter").height();
                                // console.info(tOffset);
                                // console.info(tHeight);
                                var addedSpace = 0;
                                if (tOffset.top > 10) {
                                    addedSpace = 50;
                                }
                                var modalOffset = tOffset.top + targetHeight + addedSpace;
                                // console.info(modalOffset);
                                jQuery("div.universal-modal--backdrop").css({
                                    "display": "block",
                                    "position": "absolute",
                                    "top": modalOffset
                                }); // need to set "top" to height of message
                            });
