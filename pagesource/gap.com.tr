                        var WCD_CambridgeGetInfo = function () {
                            personalizationService.api.onPersonalization(function (event) {
                                if (typeof personalizationService.model.personalizationData.personalizationInfoV1 != null) {
                                    cambridgePasswordStatus = personalizationService.model.personalizationData.personalizationInfoV1.custCambridgeStatus;
                                    if (cambridgePasswordStatus == "true") {
                                        gidLib.setCookieVar("mktUniversalPersist", "Cambridge", "never");
                                    } else {
                                        if (typeof personalizationService.model.personalizationData.personalizationInfoV1.customerAccount != "undefined") {
                                            WCD_CambridgeName = personalizationService.model.personalizationData.personalizationInfoV1.customerAccount.firstName;
                                            //console.info("PaS name:" + WCD_CambridgeName);
                                            gidLib.setCookieVar("mktUniversalPersist", "CambridgeUname", WCD_CambridgeName);
                                            //console.info("set name:" + gidLib.getCookieVar("mktUniversalPersist","CambridgeUname"));
                                            if (gidLib.getCookieVar("mktUniversalPersist", "CambridgeUname").length > 1) { // on first click, set cookie so disrupter fires on next screen
