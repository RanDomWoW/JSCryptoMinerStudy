 var DR = new google.maps.DirectionsRenderer();

 var latlng = new google.maps.LatLng(35.6500351, 139.7126344);
 var opts = {
 zoom: 17,
 center: latlng,
 mapTypeControl: true,
 mapTypeId: google.maps.MapTypeId.ROADMAP
 };
 /* 表示エリアのID名を指定。この場合id="map"のところに出力されます */
