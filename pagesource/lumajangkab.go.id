            {src:"https://www.lumajangkab.go.id/portal-background/background-2017-07-20_083653.jpg"},{src:"https://www.lumajangkab.go.id/portal-background/background-2017-07-20_083812.jpg"},{src:"https://www.lumajangkab.go.id/portal-background/background-2017-07-20_083859.jpg"},{src:"https://www.lumajangkab.go.id/portal-background/background-2017-07-20_083939.jpg"}            ],
            timer: false,
            loop:true,
            delay:7000
        });
        $('.vegas-transition-fade2').addClass('lazy');
        $('.vegas-transition-fade2-in').addClass('lazy');
        $('.vegas-transition-fade2-out').addClass('lazy');
        $('div.lazy').lazyload();
        $('img.lazy').lazyload();
        $('.nopreview').click(function () {
            Grid.hidePreview();
        });

    });
