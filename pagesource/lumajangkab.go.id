<script>
    document.addEventListener("DOMContentLoaded", function (event) {
        var element = document.getElementsByClassName('ini-content');
        var height = element.offsetHeight;
        if (height < screen.height) {
            document.getElementsByClassName("footer").classList.add('stikybottom');
        }
    }, false);
    function cariDesa(e){
        var kecamatan = $(e).val();
        var desa = $('#desa');

        if(kecamatan != ''){
            $.ajax({
                url:'ajax_desa.php',
                type:'post',
                data:'kecamatan='+kecamatan,
                success:function(d){
                    desa.html(d);
                }
            })
        }
    }
    function cariLink(e){
        var link = $('#desa').val();
        if(link != '') window.location=link;
    }
    function cariBerita(){
        var bulan = $('#bulan_berita option:selected').val();
        var tahun = $('#tahun_berita').val();
        var kata = $('#kata_berita').val();
        var where = [];
        var where_kon = '';
        var url_berita = 'https://portalberita.lumajangkab.go.id';
        i=0;

        if(bulan != ''){
            where[i] = 'bulan='+bulan;
        }else{

        }
        if(tahun != ''){
            i += 1;
            where[i] = 'tahun='+tahun;
        }
        if(kata != ''){
            i += 1;
            where[i] = 'keycari='+kata;
        }
        if(where.length > 0){
            where_kon = where.join('&');
        }
        if(where_kon != '') url_berita += '?'+where_kon;
        window.location=url_berita;
    }
    $(function () {
        Grid.init();
        $("body").vegas({
            transition: 'fade2',
            transitionDuration: '5000',
