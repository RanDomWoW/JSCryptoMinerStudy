    <script src="http://scsport.ba/wp-content/themes/sportcentar/assets/bs/js/ie10-viewport-bug-workaround.js"></script>





    <script type="text/javascript">
      $(document).ready(function() {
        var menuOn = false;
          var top = 0;
        $(".show-mobile-menu").on("click", function(){
            if(menuOn){
                document.getElementById("main").style.overflow = "auto";
                document.getElementById("mobile-menu").style.width = "0px";
                $(".mobile-menu").show();
                $("body").css("top");


                menuOn = false;
            }else {
                document.getElementById("mobile-menu").style.width = "350px";
                document.getElementById("main").style.overflow = "hidden";
                top = $("body").css("top");
                $(".mobile-menu").hide();

                menuOn = true;
            }



          $(".mobile-menu").toggle();

        });
        $(".tabs-item").on("click", function(){
          var tab = $(this).attr("data-name");
          $(".tabs-item").removeClass("active-tab");
          $(this).addClass("active-tab");
          $(".tab-3-content").removeClass("tab-active");
          $("."+tab).addClass("tab-active");
        });
        $(window).scroll(function(){
          if ($(this).scrollTop() > 100) {
            $('#scroll').fadeIn();
          } else {
            $('#scroll').fadeOut();
          }
        });
        $('#scroll').click(function(){
          $("html, body").animate({ scrollTop: 0 }, 600);
          return false;
        });


        $(".main-menu-top li").on("mouseenter", function(){
          if($(this).hasClass("dropdown")) {
            var id = $(this).attr("data-id");
            $(".navbar-menu-content").html("");
            $(".navbar-menu-content").html("");
            jQuery.ajax({
              type: "POST",
              url: 'http://scsport.ba/wp-content/themes/sportcentar/php/ajax/front.php',
              data: {catId: id, catMenu: id},
              beforeSend: function () {

                //jQuery(".loading-spinner"+postId).show();
              },
              success: function (data) {
                //jQuery(".loading-spinner"+postId).hide();
                $(".navbar-menu-content").html(data);
              }
            });
            //$(".hover-menu-block").html(" " + id);
            $(".main-menu-top li").removeClass("menu-hover-active");
            $(this).addClass("menu-hover-active");
            if ($(".navbar-menu-content").css("display") == "block") {
              $(".navbar-menu-content").show();
            } else {

              $(".navbar-menu-content").fadeIn(180);
            }
          }
        });

        $(".main-menu-top li").on("mouseleave", function(){
            if ($(".navbar-menu-content").css("display") == "block") {
            } else {
              $(".navbar-menu-content").hide();
              $(".main-menu-top li").removeClass("menu-hover-active");

            }
        });
        $(".navbar-menu-content").on("mouseleave", function(){
            $(".navbar-menu-content").hide();
            $(".main-menu-top li").removeClass("menu-hover-active");


        });

        $(".navbar-menu").on("mouseleave", function(){
            $(".navbar-menu-content").hide();
            $(".main-menu-top li").removeClass("menu-hover-active");



        });



      });

    </script>
	
  </body>
</html>