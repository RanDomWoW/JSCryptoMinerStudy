<script type="text/javascript">
    $(document).ready(function () {
        var errorRegisterNum = $("#errorRegisterNum");
        var errorRegisterNom = $("#errorRegisterNom")
        var numcarte = $("#numcarte");
        var nom = $("#nom");

        function validatenom() {
            //if it's NOT valid
            if (nom.val().length == 0 || nom.val() == 'Mon nom de famille') {
                errorRegisterNom.html('Le nom est obligatoire');
                return false;
            } else {
                return true;
            }
        }

        function validatenumcarte() {
            //if it's NOT valid
            var filter = /^[a-zA-Z0-9]+$/;
            if (numcarte.val().length == 0 || numcarte.val() == 'Mon numéro de carte') {
                errorRegisterNum.html('Le numéro de carte est obligatoire');
                return false;
            } else {
                if (filter.test(numcarte.val())) {
                    errorRegisterNum.html('');
                    return true;
                }
                else {
                    errorRegisterNum.html('Numero de la carte invalide');
                    return false;
                }
            }
        }

        $("#registerWithFidCard").click(function (e) {
            if (validatenumcarte() & validatenom()) {
                return true;
            }
            else {
                return false;
            }
        });

    });

