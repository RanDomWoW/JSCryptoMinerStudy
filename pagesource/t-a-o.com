			$.data(this, 'click', false);
		}).data('click', false);

		$('#rollover_cart_popup').click(function() {
			$.data(this, 'click', true);
		}, function() {
			$.data(this, 'click', false);
		}).data('click', false);
		$('#miniCart span').mouseenter(function() {
			$.ajax({
				url: '/cached/cart/rollover/MiniCart',
				success: function(result) {
					$('#rollover_cart_popup').html(result);
					$('#rollover_cart_popup').show();
					$('#cart_header').fadeIn();
				}
			});
		});
		$('#cart_header').mouseleave(function() {
			$('#cart_header').fadeOut();
		});
		$('body').click(function(e) {
		    if (!$(e.target).parents('#cart_header').length) {
				$('#cart_header').fadeOut();
		    }
		})
	});
