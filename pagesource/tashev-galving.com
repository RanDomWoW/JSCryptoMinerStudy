<script type="text/javascript">
	var impromptuSubmitCBR  = function(e,v,m,f){
		if(v === -1) {
			$.prompt.prevState();
			return false;
		} else if(v === 1) {
			$.prompt.nextState();
			return false;
		}
	};

	function doBounce(element, times, distance, speed) {
		for(i = 0; i < times; i++) {
			element.animate({marginTop: '-='+distance},speed)
				.animate({marginTop: '+='+distance},speed);
		}
	}

	function bounce_main_help() {
		doBounce($('#mainhelp'), 1, 12, 200);
		doBounce($('#mainhelp'), 1, 8, 200);
		doBounce($('#mainhelp'), 1, 6, 200);
		var next = Math.floor((Math.random()*10)+6);
		if($.browser.webkit)
			setTimeout( function(){ bounce_main_help(); }, next * 1000);
	}

	$(document).ready(function() {
		$(window).resize();
		$('#mainhelp').click(function(e){
			e.preventDefault();
			e.stopPropagation();

			$.prompt.setStateDefaults({
				submit: function(e,v,m,f){
					if(v === -1) {
						$.prompt.prevState();
						return false;
					} else if(v === 1) {
						$.prompt.nextState();
						return false;
					}
				}
			});


			var pn = pagename();
			try { res = srpc.help(pn); } catch (e) { server_problem(e); return false; };
			if (res === undefined || !res || res.success === undefined || !res.success) {
				var errmsg;
				if (!res || res.msg === undefined || !res.msg.length) {
					errmsg = 'Системна грешка.';
				} else {
					errmsg = "Грешка: "+res.msg; // means failure
				}
				console.log(errmsg);
				return false;
			} else {
				$.prompt(res.data);
			}

			return false;
		});

		bounce_main_help();
	});

	$('#request-modal, #forgotten-input-modal').overlay({
		mask: {
			color: '#777',
			loadSpeed: 200,
			opacity: 0.5
		},
		left: 50,
		closeOnClick: false
	});

	$('#forgotten-input-modal-close-btn').click(function(e){
		e.stopPropagation();
		e.preventDefault();
		$('#forgotten-input-modal').overlay().close();
		return false;
	});

	$('#request-modal-close-btn').click(function(e){
		e.stopPropagation();
		e.preventDefault();
		$('#request-modal').overlay().close();
		return false;
	});

	$('#request-modal-send-btn').click(function(e){
		e.stopPropagation();
		e.preventDefault();

		if ($('#request-subject').val().length <= 0) {
			element_highlight($('#request-subject').focus(), '#fee');
			return false;
		}

		if ($('#request-message').val().length <= 0) {
			element_highlight($('#request-message').focus(), '#fee');
			return false;
		}

		var obj = {};
		obj.sbj = $('#request-subject').val();
		obj.msg = $('#request-message').val();
		$('#request-modal-close-btn, #request-modal-send-btn').attr('disabled', 'disabled');
		$('#request-result').html('<span style="float: left;"><img src="/images/loading1.gif" /></span> изпращане...');

		setTimeout(function(){
			try { res = srpc.request_send(obj); } catch (e) { server_problem(e); return false; };
			if (res === undefined || !res || res.success === undefined || !res.success) {
				var errmsg;
				if (!res || res.msg === undefined || !res.msg.length) {
					errmsg = 'Системна грешка.';
				} else {
					errmsg = "Грешка: "+res.msg; // means failure
				}
				console.log(errmsg);
				$('#request-result').html(errmsg);
				$('#request-modal-close-btn, #request-modal-send-btn').removeAttr('disabled');
				return false;
			} else {
				$('#request-modal').overlay().close();
			}
		}, 100);
	});


	function request_form(subj) {
		rm = $('#request-modal');
		rm.find('#request-message').val('');
		rm.find('#request-result').html('');
		$('#request-modal-close-btn, #request-modal-send-btn').removeAttr('disabled');
		if (subj && subj.length > 0) {
			rm.find('#request-subject').val(subj).attr('disabled', 'disabled');
			setTimeout(function() {
				$('#request-message').focus();
			}, 300);
		} else {
			rm.find('#request-subject').val('').removeAttr('disabled');
			setTimeout(function() {
				$('#request-subject').focus();
			}, 300);
		}
		rm.overlay().load();
	}


	$('#contact-btn').live('click', function(e) {
		e.preventDefault();
		e.stopPropagation();
		request_form();
	});

</script>

