	<script type="text/javascript">
		function remove_compare(id) {
			var compare = $.cookies.get('compare');
			if (!compare) return;

			try { ga_track_event('Compare', 'Remove', id); } catch (e) { };

			var new_compare = new Array();

			$(compare).each(function(i, e){
					if (this.id == id) {
						$('.compareboxproducts').find('.compareproduct[rel="'+this.id+'"]').remove();
						$('.addtocompare[data-pid="'+this.id+'"]').removeAttr('checked');
					} else {
						new_compare.push(this);
					}
			});
			$.cookies.set('compare', new_compare);
			update_compare();
		}

		function update_compare() {
			var compare = $.cookies.get('compare');
			if (!compare) {
				$('.addtocompare').removeAttr('checked');
				$('.compareboxdiv').fadeOut();
				return;
			}

			$(compare).each(function(i, e){
				$('.addtocompare[data-pid="'+this.id+'"]').attr('checked', 'checked');
				if ($('.compareboxproducts').find('.compareproduct[rel="'+this.id+'"]').length) {
				} else {
					var html=' \
					<div class="compareproduct" rel="'+this.id+'"> \
						<div class="compareproduct-img"> \
							<img src="'+this.img+'" /> \
						</div> \
						<div class="compareproduct-title"> \
							<b>'+this.name+'</b> \
						</div> \
						<div class="compareremoveproduct" onclick="remove_compare(\''+this.id+'\');"> \
							&nbsp; \
						</div> \
					</div>';
					$('.compareboxproducts').append(html);
				}
			});

			var len = $('.compareboxproducts').find('.compareproduct').length;
			if (len) {
				if (len > 1) {
					$('.compareselected').removeClass('gray').addClass('blue').removeAttr('disabled');
				} else {
					$('.compareselected').removeClass('blue').addClass('gray').attr('disabled', 'disabled');
				}
				$('.compareboxdiv').fadeIn();
			} else {
				$('.compareboxdiv').fadeOut();
			}
		}

		$(document).ready(function(){
			update_compare();
		});

		$('.addtocompare').click(function(e){
			if (!$(this).is(':checked')) {
				remove_compare($(this).attr('data-pid'));
			} else {

				var compare = $.cookies.get('compare');
				if (!compare)
					compare = new Array();

				var p = { };
				p.id = $(this).attr('data-pid');
				p.name = $(this).attr('data-name');
				p.img = '//tashev-galving.com/images/items/'+p.id+'.jpg';

				var found = 0;
				$(compare).each(function(i, e){
					if (this.id == p.id) {
						found = 1;
					}
				});

				if (!found) {
					compare.push(p);
					$.cookies.set('compare', compare);
				}
			}
			update_compare();
		});

		$('.orderbtn').click(function(e){
			e.preventDefault();
			e.stopPropagation();

			//if (!$.userdata || !$.userdata.id) {
			//	// there is no $.userdata focus the login and return
			//	$('.tg-login-username').focus();
			//	element_highlight($('.tg-login-username'), '#fcc');
			//	return false;
			//}

			var master = $('.tg-product[rel="'+$(this).attr('rel')+'"]');
			var orgimg = $(master).find('img[rel="productimg"]');
			orgimg = $(orgimg[0]); // use the first element only
			if (!orgimg || !orgimg.length) {
				//console.log('cant find img');
				//console.dir(orgimg);
				return false;
			}

			// animate image-to-basket
			scroll_to_top();
			$('body').animate({ scrollTop: 0 }, 400, function() {
				// create object
				var pobj = {};
				pobj.id  = $(master).attr('data-id');
				pobj.mid = $(master).attr('data-catalog');
				pobj.bc  = $(master).attr('data-barcode');
				pobj.qty = 1;
				pobj.prc = parseFloat($(master).attr('data-price'));
				pobj.prc_disc = parseFloat($(master).attr('data-price-disc'));
				pobj.discount = parseFloat($(master).attr('data-discount'));
				pobj.name = $(master).attr('data-name');
				pobj.producer = $(master).attr('data-producer');
				pobj.unit = $(master).attr('data-unit');
				//console.dir(pobj);

				var bimg = $('.tg-login-profile-cart-img'); // basket img
				var pimg = orgimg.clone()  // cloned product (original) img
					.appendTo($(orgimg).parent())
					.css({
						position: 'fixed',
						width: orgimg.width(),
						height: orgimg.height()
					})
					.offset($(orgimg).offset());

				var boff = bimg.offset(); // basket img offset

				pimg.css('border', '2px dotted black');
				// the animation
				pimg.delay(500).animate({
					top: boff.top,
					left: boff.left,
					width: bimg.width(),
					height: bimg.height(),
					opacity: 0.6,
				}, 400, function() {
					// when animation is done, fadeout and remove
					element_highlight(bimg, '#dfd');
					$(this).fadeOut(function(){
						$(this).remove();

						if (send_to_basket(pobj)) {
							//console.log('shaking');
							//bimg.effect("shake", {times: 1, distance: 7}, 50);
							bimg.shake_af(5, 7, 500);
						} else {
							element_highlight(bimg, '#f00');
						}
					});
				});
			});
		});




