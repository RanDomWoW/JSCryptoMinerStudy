				if ( $('#search-current-cat').val() != 0 ) {
					$('.tg-search-extended').stop().css({height: 20}).slideDown(600, 'easeOutBack');
				}
			}).blur(function(){
				var t = setTimeout(function() {
					if ($('#search').is(':focus')) {
						return;
					}
					$('.tg-search-extended').stop().css({height: 20}).slideUp(600, 'easeOutBack');
					if ($('#search').val().length == 0) {
						$('#search').addClass('first-run');
						//$('#search').val('търсене в сайта');
						$('#search').data('focused', false);
					}
