			<script type="text/javascript">
				$(document).ready(function() {
					$('.tg-login-button').click();
				});
													$.userdata = undefined;
					$.basket = $.cookies.get('tgbasket');
					//$('.tg-login-profile-cart').html(($.basket && $.basket.total && $.basket.total > 0) ? $.basket.total.toFixed(2) : '0.00');
					update_cart_price(($.basket && $.basket.total && $.basket.total > 0) ? $.basket.total.toFixed(2) : '0.00');
					//console.log('no user');
				
				$(window).bind('beforeunload', function(e){
					return undefined;
					if ($.intended) {
						$.intended = undefined;
						return undefined;
					}
					var btot = $('.tg-login-profile-cart').html();
					if (typeof $.userdata !== 'undefined' && typeof $.userdata.id !== 'undefined' && typeof btot !== 'undefined' && parseFloat(btot) > 0) {
						var msg = "Имате неприключена кошница. Наистина ли искате да напуснете магазина ?";
						$('#unsupported').html('<center><div>'+msg+'</div></center>').slideDown('slow');
						(e || window.event).returnValue = msg;
						setTimeout(function(){
							$('#unsupported').html('<center><div></div></center>').slideUp('slow');
						}, 5000);
						return msg;
					}
					return undefined;
				});


				$('.tg-login-profile-cart-img').click(function(e){
					e.preventDefault();
					e.stopPropagation();
					if ( !$.userdata || !$.userdata.id ) {
						load_content('/dashboard-basket-guest');
					} else {
						load_content('/dashboard/basket');
					}
					return false;
				});

				$('.tg-login-forgotten').click(function(e){
					e.stopPropagation();

					$('.tg-login-data, .tg-login-profile-left').fadeOut(200, function() {
						$('.tg-login-forgotten-div').fadeIn(200);
					});
					return false;
				});

				$('.tg-login-forgotten-cancel').click(function(e){
					e.stopPropagation();

					$('.tg-login-forgotten-div').fadeOut(200, function() {
						$('.tg-login-data, .tg-login-profile-left').fadeIn(200);
					});
					return false;
				});

				function send_forgotten_password_request() {
					var res;
					var data = {};
					data.email = $('.tg-login-forgotten-input').val();

					if (!data.email || data.email.length < 6 || data.email == $('.tg-login-forgotten-input').attr('rel')) {
						element_highlight($('.tg-login-forgotten-input').focus(), '#f99');
						return false;
					}

					try { res = srpc.forgotten_password(data); } catch (e) { server_problem(e); return false; };
					if (res === undefined || !res || res.success === undefined || !res.success) {
						var errmsg;
						if (!res || res.msg === undefined || !res.msg.length) {
							errmsg = 'Системна грешка.';
						} else {
							errmsg = "Грешка: "+res.msg; // means failure
						}
						alert(errmsg);
						return false;
					}

					$('#forgotten-input-modal').overlay().load();

					$('.tg-login-forgotten-div').fadeOut(200, function() {
						$('.tg-login-data, .tg-login-profile-left').fadeIn(200);
					});

				}

				$('.tg-login-forgotten-submit').click(function(e){
					e.stopPropagation();
					send_forgotten_password_request();
					return false;
				});

				$('.tg-login-button').click(function(event){
					event.stopPropagation();
					event.preventDefault();

					$(this).stop().fadeOut(100);

					$('.tg-loginbox').css({
						border: '1px dashed #F48423',
						borderRadius: 7,
						backgroundColor: '#FDFCE2',
						boxShadow: '8px 8px 15px -10px #000'
					}).animate({
						width: 330,
						height: 80,
					}, 800, 'easeOutBack', function(){
						$('.tg-login-data').fadeIn(600);
						// AF - disable X // $('.tg-login-cancel').fadeIn(600);
						if ($.userdata !== undefined) {
							//console.log('doing loginbox');
							show_user_logged_in_box();
						}
					});

				}).mouseenter(function(){
					$(this).css({boxShadow: 'none'}).animate({
						//boxShadow: "0px 8px 15px -5px #f48423"
						boxShadow: "0px 0px 15px 2px #f48423"
					});
				}).mouseleave(function() {
					$(this).animate({
						boxShadow: "0px 8px 15px -10px #000"
					});
				});

				$('.tg-login-cancel').click(function(){
					// AF: disable the X
					return false;
					$('.tg-loginbox').fadeOut(600, function(){
						$('.tg-login-cancel, .tg-login-data').hide();
						$(this).css({
							width: 'auto',
							height: 'auto',
							border: '0px',
							backgroundColor: '#ffffff',
							boxShadow: 'none'
						}).show();
						$('.tg-login-button').fadeIn(600);
					});
				});

				//$('.tg-login-username, .tg-login-password').afInputs();

				$('.tg-login-username, .tg-login-password, .tg-login-forgotten-input').focus(function(){
					if ($(this).hasClass('first-run')) {
						$(this).removeClass('first-run').val('');
					}
				}).blur(function(){
					if (! $(this).val().length) {
						$('.'+$(this).attr('class')+'-x').fadeOut('slow');
						$(this).addClass('first-run').val($(this).attr('rel'));
					}
				}).keyup(function(e){
					if (e.keyCode == 13) {
						if ($(this).hasClass('tg-login-username') || $(this).hasClass('tg-login-password')) {
							$('.tg-login-dologin').click();
						} else if ($(this).hasClass('tg-login-forgotten-input')) {
							$('.tg-login-forgotten-submit').click();
						}
					} else {
						if ($(this).val().length) {
							$('.'+$(this).attr('class')+'-x').fadeIn('slow');
						} else {
							$('.'+$(this).attr('class')+'-x').fadeOut('slow');
						}
					}
				});

				function show_user_logged_in_box() {
					if ($.userdata !== undefined) {
						$('.tg-login-data').fadeOut(200, function(){
							$('.tg-login-cancel').fadeOut(200);
							$('.tg-login-profile-name').html($.userdata.name + " " + $.userdata.family);
														$('.tg-login-profile').fadeIn(200);
						});
					} else {
						//$('.tg-login-profile-cart').html($.basket.total.toFixed(2));
						update_cart_price($.basket.total.toFixed(2));
					}
				}

				function update_basket_price() {
					var res;
					var data = {};
					data.id = 0;

					try { res = srpc.get_basket_info(data); } catch (e) { server_problem(e); return false; };
					if (res === undefined || !res || res.success === undefined || !res.success) {
						var errmsg;
						if (!res || res.msg === undefined || !res.msg.length) {
							errmsg = 'Системна грешка.';
						} else {
							errmsg = "Грешка: "+res.msg; // means failure
						}

						return false;
					}
					var sum = res.sum;
					//$('.tg-login-profile-cart').html(sum);
					update_cart_price(sum);
				}

				$('.tg-login-dologin, .tg-login-profile-logout').click(function(event){
					event.preventDefault();
					event.stopPropagation();
					var res = submit_login();
					if (res == true) { // login ok
						show_user_logged_in_box();
						update_basket_price();
						$('#homebutton').click();
					} else { // login incorrect or logout
						if ($.userdata !== undefined) { // logout
							$('.tg-login-profile').fadeOut(200, function(){
								// AF - disable X // $('.tg-login-cancel').fadeIn(200);
								$('.tg-login-data').fadeIn(200);
							});
							$.userdata = undefined;
							//$('.tg-login-profile-cart').html('0.00');
							update_cart_price('0.00');
							$('#homebutton').click();
						}
						if (res.msg !== undefined && res.msg.length) {
							show_login_error(res.msg);
						} else {
							show_login_error(res);
						}
					}
					return false;
				});

				$('.tg-login-username-x, .tg-login-password-x, .tg-login-forgotten-input-x').click(function(){
					$('.'+$(this).attr('rel')).val('').focus();
					$(this).fadeOut('slow');
				});

				function show_login_error(res) {
					console.log("err: "+res);
					$('.tg-login-links').fadeOut(200, function(){
						$('.tg-login-result')
							.html(res)
							.fadeIn(200)
							.animate({backgroundColor: '#f77', color: '#3ff'}, 200)
							.animate({backgroundColor: '#fcc', color: '#f00'}, 400)
							.delay(2000)
							.fadeOut(200, function(){
								$('.tg-login-links').fadeIn(200);
							});
					});
				}

			</script>

