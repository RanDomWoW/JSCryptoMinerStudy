    $(window).scroll(function () {
        if ($(this).scrollTop() >= 1000) { // this refers to window
            if ($(this).width() < 995) {
                $('.backtotop').fadeIn("slow");
            }
        }
        else { $('.backtotop').fadeOut("slow"); }
    });
    $(".backtotop a").on('click', function () {
        $("html, body,document").animate({ scrollTop: 0 }, "slow");
        return false;
