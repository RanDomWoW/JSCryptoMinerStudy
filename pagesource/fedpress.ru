        var top, st_top, m_l = "-550";
        
        if($(".main .s_links2").length > 0){
        $(window).scroll(function(){
          top = $("body").scrollTop();
          st_top = $(".main .s_links2").last().offset().top;
          if (top > $(".news-page").offset().top - 50) {
            if (top < st_top - 500
              || top > st_top + 200)
            {
              if (window.innerWidth > 1199) { m_l = "-550"; }
              if (window.innerWidth > 991 && window.innerWidth < 1200) { m_l = "-540"; }
              if (window.innerWidth > 767 && window.innerWidth < 992) { m_l = "-440"; }
              if (window.innerWidth < 768) {
                $(".s_links2.fixed").css({
                  "position":"fixed",
                  "top":"60px",
                  "left":"10px",
                  "margin-left": "0px",
                  "display":"block",
                  "z-index":"1"
                }); 
              } else {
                $(".s_links2.fixed").css({
                  "position":"fixed",
                  "top":"60px",
                  "left":"50%",
                  "margin-left": m_l + "px",
                  "display":"block"
                }); 
              }
            } else {
              $(".s_links2.fixed").css({
                "display":"none"
              }); 
            }
          } else {
            $(".s_links2.fixed").removeAttr("style");
          }
        });
        }
        
