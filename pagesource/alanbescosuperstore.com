<script>

	function cycleImages(){
		var $slides = $('#mobile_sliders').children();
		var els = $slides.get();
		if (els.length < 2) {
			return this;
		}
		var $active = $('#mobile_sliders .active');
		var $next = ($active.next().length > 0) ? $active.next() : $('#mobile_sliders img:first');
		$next.fadeIn(2800);//fade in the next image
		$active.fadeOut(2800,function(){//fade out the top image
		$active.removeClass('active');
		  $next.addClass('active');
		});
	}

	function imgHeight() {
		var W = $(window).width();

		$('#mobile_sliders').height(234);

		if (W < 480) {
			$('#mobile_sliders').height(146);
		}

	}

	function formSubmit(form) {

		var el = $(form);

		var fields = {
			'name': $('#name').val(),
			'email': $('#email').val(),
			'phone': $('#phone').val(),
			'message': $('#message').val()
		};

		$.ajax({
			url: 'process.php',
			type: "POST",
			data: fields,
			dataType: 'json',
			beforeSend: function() {
				//$('#ind').show()
			},
			complete: function(){
				//$('#ind').hide()
			},
			success: function(data) {
				if (data.error) {
					el.hide(1000, function(){
						success.fadeIn(1500);
						el.each (function(){
						  this.reset();
						});
					});

					var success = $('<div>Please fill in all fields.</div>');
					success.hide();
					el.after(success);
				}
				else {

					el.hide(1000, function(){
						success.fadeIn(1500);
						el.each (function(){
						  this.reset();
						});
					});
					var success = $('<div>Thank you for your submission. We will contact you soon!</div>');
					success.hide();
					el.after(success);

				}

		  }
		});
	}

	$(document).ready(function() {
		$('#menu').superfish({
			autoArrows:  false,                           // disable generation of arrow mark-up
			dropShadows: false                            // disable drop shadows
		});

		$("#contact-form").validate({
			submitHandler: formSubmit
		});

		$("#rental-info-form").validate({
			submitHandler: formSubmit
		});


		window.setTimeout(function(){
			$('#sliders #slide1').cycle({
				fx: 'fade', // choose your transition type, ex: fade, scrollUp, shuffle, etc...
				timeout:       6000
			});
		}, 2000);


		$('#sliders #slide2').cycle({
			fx: 'fade', // choose your transition type, ex: fade, scrollUp, shuffle, etc...
			timeout:       4500
		});

		window.setTimeout(function(){
			$('#sliders #slide3').cycle({
				fx: 'fade', // choose your transition type, ex: fade, scrollUp, shuffle, etc...
				timeout:       6000
			});
		}, 4000);

		$('#sliders #slide4').cycle({
			fx: 'fade', // choose your transition type, ex: fade, scrollUp, shuffle, etc...
			timeout:       5500
		});

		cycleImages();
		setInterval('cycleImages()', 5000);
		imgHeight();
		$(window).bind('resize', function() { imgHeight(); });

	});

</script>

