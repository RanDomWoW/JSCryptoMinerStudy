;(function($, window, document, undefined) {
	var $win = $(window);
	var $doc = $(document);

	$.fn.swipeEvents = function() {
		return this.each(function() {

			var startX;
			var startY;
			var $this = $(this);

			$this.bind('touchstart', touchstart);

			function touchstart(event) {
				var touches = event.originalEvent.touches;
				if (touches && touches.length) {
					startX = touches[0].pageX;
					startY = touches[0].pageY;
					$this.bind('touchmove', touchmove);
				}
			}

			function touchmove(event) {
				var touches = event.originalEvent.touches;

				if (touches && touches.length) {
					var deltaX = startX - touches[0].pageX;
					var deltaY = startY - touches[0].pageY;

					if (deltaX >= 50) {
						$this.trigger('swipeLeft');
					}
					if (deltaX <= -50) {
						$this.trigger('swipeRight');
					}
					if (deltaY >= 50) {
						$this.trigger('swipeUp');
					}
					if (deltaY <= -50) {
						$this.trigger('swipeDown');
					}
					if (Math.abs(deltaX) >= 50 || Math.abs(deltaY) >= 50) {
						$this.unbind('touchmove', touchmove);
					}
				}
			}
		});
	};

	$doc.ready(function() {
		var sliderIntro = $('.slider--intro');
		var sliderIntroWrapper = $('.slider--intro .owl-carousel');

		sliderIntroWrapper.owlCarousel({
			loop: true,
			nav: true,
			dots: false,
			mouseDrag: false,
			touchDrag: false,
			animateOut: 'fadeOut',
			items: 1,
			navText: ['&#xf053;', '&#xf054;'],
			responsive : {
				0 : {
					nav: false,
					autoplay: true,
					autoplayTimeout: 7000
				},
				// breakpoint from 761 up
				761 : {
					nav: true,
					autoplay: true,
					autoplayTimeout: 7000
				}
			}
		});

		sliderIntro.swipeEvents();

		sliderIntro.on('swipeLeft', function() {
			$(this).find('.slider__slides').trigger('next.owl.carousel');
		}).on('swipeRight', function() {
			$(this).find('.slider__slides').trigger('prev.owl.carousel');
		});

		sliderIntroWrapper.on('translate.owl.carousel', function(){
			$('.slider--intro .owl-item video').each(function(){
				$(this).get(0).pause();
			});

			setTimeout(function() {
				if ( $('.slider--intro .owl-item.active video').length > 0 ) {

					$('.slider--intro .owl-item.active video').get(0).play();
				}
			}, 200);
		});


	});
})(jQuery, window, document);
