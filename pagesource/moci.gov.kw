    function checkLogin()
    {
        var txt = document.getElementById("EmployeeLogin1_txtUser");
        var pass= document.getElementById("EmployeeLogin1_txtPass");
        if(txt.value=="")
        {
            alert("عفواً, يجب أن تدخل اسم المستخدم");
