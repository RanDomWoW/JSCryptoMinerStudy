            alert("عفواً, يجب أن تدخل كلمة المرور");
            pass.focus();
            return false;
        }
    }
    function ControlLoginKey(evt)
    {
        var charCode = (evt.which) ? evt.which : event.keyCode
        
        if (charCode ==13)
        {
            //alert("ENTER");
            document.getElementById("EmployeeLogin1_lnkLogin").click();
            return false;
        }
        
        return true;
    }
    function serverError()
    {
        alert("عفواً, يوجد خطأ في اسم الموظف أو كلمة المرور");
    }
