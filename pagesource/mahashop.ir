<script type="text/JavaScript">
function confirmIt(msg) {
	if (confirm(msg)) {
		return true;
	}else{
		return false;
	}
}

function jsPopup(data,w,onClk) {
	// Remove old popup
	if ($('.boxCorners').length > 0) {
		$('.boxCorners').remove();
	}

	$('body').prepend('<div class="boxCorners"><div class="bc_top"><div class="bc_tl"></div><div class="bc_tm"></div><div class="bc_tr"></div></div><div class="bc_middle"><div class="bc_ml"><div class="bc_mr"><div class="bc_cnt">'+data+'</div></div></div></div><div class="bc_bottom"><div class="bc_bl"></div><div class="bc_bm"></div><div class="bc_br"></div></div></div><div id="bgLayer"></div>');
	var pW = $(window).width();
	var pH = $(window).height();
	var boxH = $('.boxCorners').height();
	$('.boxCorners').css({
		width: w,
		left: (pW/2)-(w/2),
		top: (pH/2)-(boxH/2),
		display: 'block'
	});
	$('#bgLayer').css({
		width: pW,
		height: $(document).height(),
		opacity: '0.8',
		display: 'block'
	});
	
	if (onClk == true) {
		$('#bgLayer').click(function() {
			$(this).css({
				display: 'none'
			});
			$('.boxCorners').css({
				display: 'none'
			});
	
		});
	}
}

$(document).ready(function() {
	var bskCnt = false;
	$('form[name=basket]').submit(function() {
		if (!$('input[name=price]:checked').val()) {
			alert ('نوع قیمت را مشخص کنید');
			$('input[name=price]:first-child').focus();
		}else{
			jsPopup('لطفا صبر کنید ...<br /><br /><img src="http://www.mahashop.ir//media/images/loading-h.gif" alt="در حال بارگذاری" />',250,false);
			$.ajax ({
				url : $(this).attr('action')+'add='+$('input[name=price]:checked').val(),
				type : 'GET',
				success: function(data) {
					$("#bskCnt").html(data);
					bskCnt = true;
				}
			});
		}
		
		return false;

	});

	$('a[href=#login]').click(function() {
		var $formContent = $('#loginForm').html();
		jsPopup($formContent,400, true);
		return false;
		
	});
	
	$('a.moreBasket').click(function() {
		if (bskCnt) {
			var el = $(this).parent().parent();
			if (el.css('top') == '-140px') {
				el.animate({top: 0}, 'slow', function() {
					$(this).find('img').attr('src','media/images/arrow_up.png');
				});
			}else{
				el.animate({top: '-140'}, 'slow', function() {
					$(this).find('img').attr('src','media/images/arrow_down-w.png');
				});
			}
		}else{
			alert('سبد خرید شما خالی است.');	
		}
		return false;
	});
	
	$("#sidebar .box span.cnt ul li").hover(function(){
		$(this).animate({paddingRight:5},100);
	},function() {
		$(this).animate({paddingRight:0},100);
	});
	
	$("ul#topMenu li").hover(function() {
		$(this).animate({paddingTop:5},80);
	},function() {
		$(this).animate({paddingTop:3},80);
	});

	$('#slideshow').slides({
		preload: true,
		preloadImage: 'images/loading.gif',
		play: 5000,
		pause: 2500,
		hoverPause: true,
		generatePagination: false,
		fadeSpeed: 500,
		effect: 'fade'
	});

});
function popup(page,w,h,scrl){
	scrl = (scrl ? scrl : "yes");
	var LeftPosition=(screen.width)?(screen.width-w)/2:100;
	var TopPosition=(screen.height)?(screen.height-h)/2:100;
	window.open(page,'sd','width='+w+',height='+h+',left='+LeftPosition+',top='+TopPosition+',toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars='+scrl+',copyhistory=no,resizable=no');
}
