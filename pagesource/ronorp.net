function EcCart() {
    this.orders = {};
}
EcCart.prototype.add = function (order_id, product, tax) {
    if (!this.orders[order_id]) {
        this.orders[order_id] = {
            revenue: 0.0,
            taxes: 0.0,
            products: [],
        };
    }
    if (typeof tax == "string") {
        tax = parseFloat(tax.replace(',', '.'));
    } else {
        tax = 0.0;
    }
    if (tax) {
        this.orders[order_id].taxes += tax;
    }
    if (typeof product.price == "string") {
        product.price = parseFloat(product.price.replace(',', '.'));
    }
    if (typeof product.quantity == "string") {
        product.quantity = parseInt(product.quantity);
    }
    this.orders[order_id].revenue += product.price * product.quantity + tax;
    this.orders[order_id].products.push(product);
};
EcCart.prototype.commit = function () {
