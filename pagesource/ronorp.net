                kucheWidth: 300,
                fixWrapper: true,
                adaptive: true,
                scrollInterval: 4
            });
            if (typeof lightwindow !== "undefined") {
                myLightWindow = new lightwindow({
                    hideGalleryTab:true,
                    showGalleryCount: false,
                    hideFlash:true,
                    animationHandler : (Prototype.Browser.IE) ? function() {
                        if (myLightWindow.windowType == 'video' || myLightWindow.windowType == 'media')
                        {
                            myLightWindow._finishWindow(); return function(){}
                        }
                        else
                        {
                            myLightWindow._defaultAnimationHandler();  return function(){}
                        }
                    } : false,
                    deactivateHandler: function() {
                        $('lightwindow_contents').update(video_dummy);
                        if (scroller)
                            scroller.pause();
                    }
                });
                $$('.video-open').invoke(
                    "observe",
                    "click",
                    function(){
                        if (scroller)
                            scroller.pause(1);
                        if (myLightWindow)
                            myLightWindow.activate(null, $('video_content_link'));
                    }
                )
            }
            scroller.onResize();
        });
    
    </script>
