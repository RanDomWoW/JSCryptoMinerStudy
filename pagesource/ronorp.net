                _ec_list_positions[list_id] = 0;
            }
            document.cookie = "ec_list=" + encodeURIComponent(list_id) + '; path=/';
            var params = {
                id: this.dataset.ecProductId,
                name: this.dataset.ecProduct,
                category: this.dataset.ecCategory,
                brand: 'RonOrp',
                price: this.dataset.ecPrice.replace(',', '.'),
                list: list_id,
