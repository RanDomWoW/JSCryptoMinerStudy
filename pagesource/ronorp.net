function register_ec_tracked_elements() {
    var list = $$("*[data-ec-trace=yes]");
    list.each(function (item) {
        item.dataset.ecTrace = 'processed';
        switch (item.dataset.ecEvent) {
            case 'load':
                __ec_tracker.call(item);
                break;
            case 'click':
                item.addEventListener('click', function (e) {
                    __ec_tracker.call(this);
                    ecEvents.send();
                });
                break;
            case 'manual':
                ecManualEventMediator.add(item);
                break;
            default:
                console.log('Error: Undefined ec-event '+item.dataset.ecEvent);
                break;
        }
    });
    if (list.length) {
        if (ecManualEventMediator.isTriggered) {
            ecManualEventMediator.trigger(true);
        }
        ecCart.commit();
        ecEvents.send();
    }
