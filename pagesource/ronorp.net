			window.location = data.back_url;
        }
	}
	function fb_login_handler(response) {
		var city = window.location.pathname.split("/")[1];
		if(!city)
			city = 'zuerich';
		var fb_uid = response.authResponse.userID;
		var fb_accessToken = response.authResponse.accessToken;
		var frame = new Element('iframe', {
			id: 'facebook_'+fb_uid,
			width: '1px',
			height: '1px',
			visibility: 'hidden',
			src: fblogin_siteUrl + city + "/social-data?oauth=facebook&login=1&uid=" + fb_uid + "&token=" + fb_accessToken + "&back_url=" + document.location.href,
		});
		$(document.body).insert(frame);
	}
	function fb_response_handler(response) {
		if(response.status === 'connected') {
			$$(".fb-login-custom-button").invoke("addClassName", "hidden");
			fb_login_handler(response);
		} else {
			RonOrpModal.hide();
		}
	}
	function rordz_FBLoginInit(response)
	{
		$$(".fb-login-custom-button").each(function(item) {
			if(item.hasClassName("initiated")) {
				return;
			}
			item.addClassName("initiated");
			item.observe('click', function(e) {
				RonOrpModal.show(null, ' ');
				FB.login(fb_response_handler, {scope: 'email'});
			});
		});
	}
	function cmf_fbAsyncInit()
	{
		if (!fblogin_Checked) {
			if (typeof FB != "undefined" && cmf_fbInitCalled) {
				FB.getLoginStatus(rordz_FBLoginInit);
				fblogin_Checked = true;
			} else {
				setTimeout(cmf_fbAsyncInit, 200);
			}
		}
	}
	initRonorpFunction.push(cmf_fbAsyncInit);
