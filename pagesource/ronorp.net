<script>
//
    
    function mobile_user_login_check() {
        wFORMS.errorPosition = "after";
        var check = wFORMS.behaviors.validation.run($('redesign_login_by_users'));
        if (check) {
            $('redesign_login_by_users').submit();
        }
        
    }
    
    function mobile_scroll(to) {
        $(to).scrollTo();
        var s = document.viewport.getScrollOffsets();
        window.scrollTo(0, s.top-50);
        
    }

    function registerFormSubmit() {
        wFORMS.errorPosition = "after";
        if (wFORMS.behaviors.validation.run($('register_form'))) {
            if (typeof fbq != "undefined") {
                fbq('track', 'CompleteRegistration');
            }
        }
    }
    function switch_register_block(a) {
        rordz_FBLoginInit();
        $$('.top_nav_box .btn').invoke('removeClassName', 'off');
        
        if (a==1) {
            $$('.top_nav_box .btn')[1].addClassName('off');
            $('signup_box').show();
            $('subscribe_box').hide();
        } else {
            $$('.top_nav_box .btn')[0].addClassName('off');
            $('signup_box').hide();
            $('subscribe_box').show();
        }
    }
   
    //

	var show_rdz_form	= '';
	var send_confirm_email	= '';
	var send_registration_confirm = 'Möchtest du die Bestätigungsmail erneut anfordern?';
	var message_after_complete = "Wow du bist Grossartig! Du solltest nun ein Mail von mir erhalten haben (falls nicht, schaue im Spam). Klicke den Link, um deine Registrierung abzuschliessen.";
	var redirect_url = '';

	var nlHeight 		= 0;
	var scrollDirection = 0;
	var nlScrollStep	= 3;
	var nlScrollSpeed	= 5;
	var nlCurrPos		= 0;
	var on_mousedown	= (Prototype.Browser.MobileSafari) ? 'touchstart' : 'mousedown';
	var on_mouseup		= (Prototype.Browser.MobileSafari) ? 'touchend' : 'mouseup';
    
	document.observe('dom:loaded', function(){
	    if($('register_in_ron_mail')) {
	        $('register_in_ron_mail').checked = true;
	    }
		if (redirect_url)
		{
			$$('input[type=hidden].redirect_url').each(function(el){
				el.value	= redirect_url;
			})
		}
		//wFORMS.debugValidate=1;
		wFORMS.behaviors['validation'].afterValidate.push(function(element, nbErrors) {
                
          
            var elmt = $('register_in_ron_mail');
		    if(element.id == 'register_form') {
                if(elmt && elmt.checked) {
                    hideError(elmt);
        		    var checkbox_checked = true;
                } else {
                    wFORMS.behaviors.validation.showError(elmt, 'Keine Angst, wenn dir mein Newsletter nicht gefällt, kannst du den auch gleich wieder abbestellen.');
        		    var checkbox_checked = false;
                }
               
                /*if (!$$('input[name="data[city_list][]"]:checked')[0] || !$$('input[name="data[city_list][]"]:checked')[0].value) {
                    var checkbox_checked = false;
                    var elmtcl = $('data_city_list___');
                    if (elmtcl) {
                        wFORMS.behaviors.validation.showError(elmtcl, wFORMS.behaviors.validation.errMsg_required);
                    }
                }**/
                 
            }
            var eid = element.readAttribute('id');
			if (eid == 'register_form' && (!nbErrors || (nbErrors == 1))  && checkbox_checked)
			{
                checkFormRegistration();
			}
		});
	});
	
	function displayFinalMessage()
	{
		//ronorp_showFox(message_after_complete);
		DialogWindow.Alert("", message_after_complete);
	}
	function scrollNewsletter()
	{
		if (scrollDirection)
		{
			nlHeight	= getNLPreviewHeight();
			var is_scroll	= false;
			if (scrollDirection < 0)
			{
				if (Math.abs(nlCurrPos+scrollDirection) < nlHeight)
					is_scroll	= true;
			}
			else
			{
				if (nlCurrPos+scrollDirection < 0)
					is_scroll	= true;
			}
			if (is_scroll)
			{
				nlCurrPos	+= scrollDirection;
				$('nl_preview_content').setStyle({top: nlCurrPos+'px'});
				setTimeout(scrollNewsletter, nlScrollSpeed);
			}
			else
			{
				scrollDirection	= 0;
			}
		}
	}
	function changeNewsletter(city_id)
	{
		var ajax = new Ajax.Request(ajaxURL, {
				parameters:	{
					cmf_action: 'users_get_current_newsletter',
					city_id: city_id
				},
				onCreate: function(){
					$('nl_preview_content').down('#archive_content').update('');
					nlCurrPos	= 0;
					nlHeight	= 0;
					$('nl_preview_content').setStyle({top: '0px'});
				},
				onComplete:	function(response) {
					if(response.responseText)
					{
						$('nl_preview_content').down('#archive_content').update(response.responseText);
						nlCurrPos	= 0;
						nlHeight	= getNLPreviewHeight();
					}
				}
		});
	}
    function checkFormRegistration()
    {
  
        var url	= ajaxURL + '?cmf_action=users_send_registration_form&'+$('register_form').serialize();
        var ajax = new Ajax.Request(url, {
                parameters:	{},
                onCreate: function(){
                    //$('rdz_register_form').setStyle({zIndex:900});
                },
                onComplete:	function(response) {
                   // $('rdz_register_form').setStyle({zIndex:999});
                    if(response.responseText.isJSON())
                    {
                        var response = response.responseText.evalJSON();
                        if (response)
                        {
                            if (response.result == 'ok')
                            {
                                send_confirm_email	= response.send_confirm_email;
                                //$('footer_message').update(response.footer_message);
                                //$$('#data_cities_-E label.data_cities__id_'+$('data_city_list___').value).invoke('hide');
                                //hideRDZForm();
                                //showRDZFrom('rdz_cities_form');
                                $('register_form').reset();
                                $('reactivate-E').addClassName('hidden');
                                $('reg_button_2-E').removeClassName('hidden');
                                if (RonOrpModal)
                                RonOrpModal.hide();
                                displayFinalMessage();
                                //
                               
                            }
                            else if (response.errors)
                            {
                                for (var fld in response.errors)
                                {
                                    var errFld	= $('register_form').down('[name="'+fld+'"]');
                                    
                                    if(!errFld){
                                        errFld = $('data_'+fld+"_");
                                    }
                                    if (errFld)
                                        wFORMS.behaviors['validation'].showError(errFld,response.errors[fld])
                                }
                                if (response.reactivate) {
                                    $('reactivate-E').removeClassName('hidden');
                                    $('reg_button_2-E').addClassName('hidden');
                                }
                                else {
                                    $('reactivate-E').addClassName('hidden');
                                    $('reg_button_2-E').removeClassName('hidden');
                                }
                            }
                        }
                    }
                }
        });
    }

</script>            
