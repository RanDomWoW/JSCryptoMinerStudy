                    })();            function slider_handler() {
                
                var slide = $$('.modal-slider .swiper-slide-active')[0];
                var slide_width = slide.getBoundingClientRect().width;
                var img = slide.down('img.slide-img');
                if (img && $$('.swiper-button-prev')[0] && $$('.swiper-button-next')[0]) {
                    var padding = parseInt(10 + (slide_width - img.width)/2) + 'px';
                    if (img.width>slide_width || img.width == 0) {
                        var padding = parseInt(10) + 'px';
                    } else {
                        var padding = parseInt(10 + (slide_width - img.width)/2) + 'px';
                    }
                    if (img.width == 0) {
                        setTimeout(function(){slider_handler()}, 500);
                    }
                    $$('.swiper-button-prev')[0].setStyle({left: padding});
                    $$('.swiper-button-next')[0].setStyle({right: padding});

                }
                if (img) {
                    processSlideLoad(img);
                }
                
            }
