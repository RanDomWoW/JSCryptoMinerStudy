            }
            if (this.dataset.ecQuantity) {
                params['quantity'] = this.dataset.ecQuantity;
            }
            if (this.dataset.ecVariant) {
                params['variant'] = this.dataset.ecVariant;
            }
            var action = 'ec:addProduct';
            var list_cookie = document.cookie.split(';').filter(function (item) {
                return item.trim().indexOf('ec_list=') == 0;
            })[0];
            if (list_cookie.length) {
                var list_id = decodeURIComponent(list_cookie.trim().split('=')[1]);
            } else {
                var list_id = '';
            }
            
                            
                ga(action, params);
                switch (this.dataset.ecAction) {
                    case 'detail':
