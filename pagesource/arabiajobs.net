<script type="text/javascript">
	function callRemarketing(pagetype, kw, loc) {
		var iframe = document.createElement('iframe');
		iframe.setAttribute('style', 'width: 1px; height: 1px; visibility: hidden;');
		iframe.setAttribute('src', '/files/remarketing.php?kw=' + encodeURIComponent(kw) + '&pt=' + encodeURIComponent(pagetype) + (loc ? '&loc=' + encodeURIComponent(loc) : ''));
		document.body.appendChild(iframe)
	}
	
	function adjustSearchButtons() {
		var kw = $('input[name="kw"]').val();
		var loc = $('input[name="loc"]').val();
		
		var url = '/';
		
		if (loc == '' && kw == '') {
			url = '/-/';
		} else {
			if (loc == '') {
				loc = '-';
			}
			url = '/' + encodeURI(loc) + '/' + encodeURI(kw);
		}
		
		
		$('a.do-search').attr('href', url);
	}
	
	function subscribeAdditionalSearch(kw, loc) {
		$.post('/ajax/subscribe', {kw: kw, loc: loc}, function() {});
	}
	
	function validateEmail(email) {
		var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
		return (reg.test(email) !== false);
	}

	function showAppInstallModal() {
		$('#app-modal-bg').show();
		$('#app-modal').show();
	}
	
	function hideAppInstallModal() {
		$('#app-modal-bg').hide();
		$('#app-modal').hide();
		$('#adwords-modal').hide();
		$.cookie('app_install_modal_fired', '1', {path: '/', expires: 3});
	}

	function isTouchScreen() {
		return !!(('ontouchstart' in window) || navigator.msMaxTouchPoints);
	}
	
	function getSearchParameters() {
	      var prmstr = window.location.search.substr(1);
	      return prmstr != null && prmstr != "" ? transformToAssocArray(prmstr) : {};
	}

	function transformToAssocArray( prmstr ) {
	    var params = {};
	    var prmarr = prmstr.split("&");
	    for ( var i = 0; i < prmarr.length; i++) {
	        var tmparr = prmarr[i].split("=");
	        params[tmparr[0]] = tmparr[1];
	    }
	    return params;
	}
	
	function hideModal() {
		_hide('embg');
		_hide('em');
	}
	
doOnJSReady('jquery.cookie', function() {
	$(document).ready(function() {

		$('a.job').click(function() {
			var adId = $(this).attr('data-adid');

			if (adId) {
				var adsClicked = [];
				
				if ($.cookie('ads_clicked')) {
					adsClicked = $.cookie('ads_clicked').split(',');
				}
				
				adsClicked.push(adId);
				$.cookie('ads_clicked', adsClicked.join(','), {path: '/', expires: 10});
			}
		});
		
		$('.input[name="kw"]').change(function() {
			adjustSearchButtons();
		});

		$('input[name="kw"], input[name="loc"]').keyup(function() {
			adjustSearchButtons();
		});

		$('input[name="kw"], input[name="loc"]').keypress(function (e) {
			if (e.which == 13) {
				window.location = $('a.do-search').attr('href');
				return false;
			}
		});
		
		adjustSearchButtons();
		
		var email = ['arabiajobs.net', '@', 'info'].reverse().join('');
		$('a.email').html(email);
		$('a.email').attr('href', 'mailto:' + email);

		$(document).keydown(function(e) {
		    if (e.keyCode == 27) {
		    	hideAppInstallModal();
		    	hideModal();
		    }
		});
	});
});
	

	function PushManager() {
		this.endpoint	= null;
		this.kw			= null;
		this.loc		= null;
		
		function getChromeVersion() {     
		    var raw = navigator.userAgent.match(/Chrom(e|ium)\/([0-9]+)\./);
		    return raw ? parseInt(raw[2], Math.pow(10, raw[2].length - 1)) : false;
		}
		
		function getFFVersion() {     
		    var raw = navigator.userAgent.match(/Firefox\/([0-9]+)\./);
		    return raw ? parseInt(raw[1], Math.pow(10, raw[1].length - 1)) : false;
		}
		
		this.ifBrowserSupportsPush = function () {
			var chromeVersion = getChromeVersion();
			var ffVersion = getFFVersion();
			return ((chromeVersion && chromeVersion >= 42) || (ffVersion && ffVersion >= 44));
		}
	};

	PushManager.prototype.subscribeBrowser = function (kw, loc) {
		this.initPushServiceWorker(function(endpoint, reg) {
			console.log('s1');
			$.post('/ajax/subscribe_browser', {endpoint: endpoint, kw: kw, loc: loc}, function(ret) {
				console.log('s2');
				if (window.yaCounter46150224 && !$.cookie('push_conversion_fired')) {
					window.yaCounter46150224.reachGoal('SUBSCRIBE_BROWSER_PUSH');
					$.cookie('push_conversion_fired', 1, {path: '/', expires: 1000});
				}
			});
		});
	};

	PushManager.prototype.initPushServiceWorker = function (callback) {
		if (this.ifBrowserSupportsPush() && 'serviceWorker' in navigator) {
			console.log('Service Worker is supported');
			navigator.serviceWorker.register('/shared/js/sw_push.js').then(function(reg) {
				console.log('SW_Push Registered', reg);

				if (callback) {
					reg.pushManager.subscribe({userVisibleOnly: true}).then(function(sub) {
			        	console.log('endpoint:', sub.endpoint);
			        	callback.call(null, sub.endpoint, reg);
			        });
				}
			}).catch(function(err) {
				console.log('SW Registration error', err);
			});
		}
	};
