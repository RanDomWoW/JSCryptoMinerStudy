window.onJSReady = {};

function doOnJSReady(js, callback) {
	if (!window.onJSReady[js]) {
		window.onJSReady[js] = [];
	}
	window.onJSReady[js].push(callback);
}

function handleOnJSReady(js) {
	if (window.onJSReady[js]) {
		while((callback = window.onJSReady[js].pop())) {
			callback.apply(window);
		}
	}
}

function downloadJS(arr) {
	var elementHead = document.getElementsByTagName("head")[0];
	for(var i in arr) {
		var item = arr[i];
		var element = document.createElement("script");
		element.src = item['src'];
		element.type = "text/javascript";
		element.onreadystatechange = function(item, js) {
			return function() {
				if (this.readyState == 'complete') {
					if (item.children) {
						downloadJS(item.children);
					}
				}
				handleOnJSReady(js);
			}
		}(item, i);
		element.onload = function(item, js) {
			return function() {
				if (item.children) {
					downloadJS(item.children);
				}
				handleOnJSReady(js);
			}
		}(item, i);
		elementHead.appendChild(element);
	}
}

