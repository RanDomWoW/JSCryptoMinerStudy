<script type="text/javascript">


  function renderRating(value) {
    try {
      console.log(value)
      var n = (value).toFixed(1).split(".")

      var ratingId = "r-588-star" + n[0]

      if(n[0] === "0" && n[1] === "0"){
        return 
      } else {
        ratingId += ([3,4,5,6,7].indexOf(parseInt(n[1])) != -1)?  "half" : ""
      }
      var el = document.getElementById(ratingId)
      el.checked = true

    } catch (err) {
      console.error(err)
    }
  }

  $("#rating-588 label").on("click", (e) => {
    const value = Math.ceil($("#" +  $(e.target).attr("for")).val())
    const url = "/classificar/588/value"
    $.get( url.replace("value", value) , data => {
      const { count, average } = data
      renderRating(average)
    })
  })

  renderRating(0)


</script>
