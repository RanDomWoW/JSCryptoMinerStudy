    const productImageUrl = "http://images.exerciciosresolvidos.net/imagem/figura/:id/slug/:size"
    const productBaseUrl = "/produto/:slug"

    function productClick(id) {

      const product = products.find(prd => prd.id === id)
      product.imageUrl = productImageUrl.replace(":id",product.picture_id)
                                  .replace(":slug",product.slug)
                                  .replace(":size","220x320")
      
      product.url = productBaseUrl.replace(":slug",product.slug)
      $("#products-list .product").removeClass("active")
      $(`#products-list .product-${id}`).addClass("active")


      $("#selected-product #book-details h3").text(product.name)
      $("#selected-product #book-details p").text(product.description)
      $("#selected-product #book-details .btn-go").attr("href",product.url)
      $("#selected-product .book #book-front").attr("src",product.imageUrl)

      console.log(product)
    }

    $("#products-list .product-link").on("click", e => {
      let id = $(e.currentTarget).data("id")
      e.preventDefault()      

      productClick(id)
    })

    productClick(products[0].id)
  </script>
