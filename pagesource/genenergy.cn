 <script type="text/javascript">
function MoveBox(obj, index2) {
    var divTop = 0;
    var divLeft = 0;
    if(index2==null || typeof(index2)=="undefined"){
        index2="1";
    }
    Math.random()*15;
    var deg = Math.random()*15;
    deg = parseInt(8-deg, 10);

    $(obj).css({
        "position": "absolute",
        "z-index": "500",
        "left": divLeft + "px",
        "top": divTop + "px",
        "transform":"rotate(0deg)"
    });
    $(obj).animate({
        "left": "410px",
        "top": "-30px",
        "width": "40px",
        "height": "25px",
        "z-index": index2
    },
    500,
    function() {
        $(obj).css({"transform":"rotate("+deg+"deg)"}).animate({
            "left": "0px",
            "top": "0px",
            "width": "400px",
            "height": "266px"
        },500);
    });
}

var tid;
var bClicked = false;
 
function clear(){
    if(tid!=null){clearTimeout(tid);tid=null;}
    bClicked = false;
}

$(function(){
    $(".img>li").click(function(){
        if(parseInt($(this).css('z-index'))!=$(".img>li").length){
            return;
        }
        if(bClicked){return;}
        bClicked = true;
        tid = setTimeout("clear()", 1000);
        
        var current = this;
        for(var i=1; i<=$(".img>li").length;i++){
            var o = $(".img>li:eq("+(i-1)+")");
            if(o==this){
                $(o).css('z-index',1);
                current = $(".img>li:eq("+(i+1)+")");
            }else{
                 var index = parseInt($(o).css('z-index'))+1;
                $(o).css('z-index',index);
                if(index==$(".img>li").length){
                    current = o;
                }
            }
        }
        //$(current).css("z-index",$(".img>li").length);
        $(current).css("transform","rotate(0deg)");
        MoveBox(this);
        
        var obj = $(".ry_lb>li").eq($(current).attr("id"));
        $(obj).addClass("on").siblings().removeClass("on")
        
    });    
    
    $(".ry_lb>li").click(function(){
        $(this).addClass("on").siblings().removeClass("on");
        var index = $(".ry_lb>li").index(this);
        
        var current = $(".img>li").eq(index);
        if(parseInt($(current).css('z-index')) == $(".img>li").length){
            return;
        }
        var prev = current;
        for(var i=0; i<$(".img>li").length;i++){
            var o = $(".img>li:eq("+(i)+")");
            var index = parseInt($(o).css('z-index'));
            if(index==$(".img>li").length){
                prev = o;
                break;
            }
        }
        
        MoveBox(prev, $(current).css('z-index'));
        $(current).css('z-index',$(".img>li").length);
        $(current).css("transform","rotate(0deg)");
        
    });
})
</script>
