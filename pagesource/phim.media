<script type="text/javascript">
var jscroll = jQuery.noConflict();

 jscroll(document)
.on('click', '#toTop', function() {
	$('html, body').animate({
		scrollTop: 0
	}, 700);
});
jscroll(window).scroll(function () {
	if(jscroll(window).scrollTop() === 0)
	{
		jscroll('#toTop').hide();	
	}
	else
	{
		jscroll('#toTop').show();	
	}
	if (jscroll(window).scrollTop() === 0 && jscroll('html').scrollTop() === 0) {
	  jscroll('#toTop').fadeOut(400);
	} else {
	  jscroll('#toTop').fadeIn(400);
	}
});
</script>
