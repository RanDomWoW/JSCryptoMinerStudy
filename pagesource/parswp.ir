function downSubscribe(form) {	
var email = jQuery('[name="email"]',form).val();
if(!isEmail(email)){
swal({
title: 'خطا !',
text: 'لطفا ایمیل خود را وارد نمائید',
type: 'warning',
confirmButtonText: 'بستن'
}).then(function () {					
});
return false;
}
jQuery("button",form).prop('disabled', true);
var form_data = jQuery(form).serialize();
form_data = form_data + '&down_subscribe=true';
jQuery.ajax({
data: form_data,
type: "POST",
dataType: 'json',
success: function(data){
jQuery("button",form).prop('disabled', false);
if(data['status'] == '1'){
return downSubscribe2(email);
}
else if(data['status'] == '2'){
swal({
type: 'success',
text: 'ثبت نام در خبرنامه با موفقیت انجام شد',
confirmButtonText: 'بستن'
})
}
else if(data['status'] == '-1'){
swal({
type: 'warning',
text: 'لطفا از ایمیل معتبر استفاده نمائید',
confirmButtonText: 'بستن'
})
}
else if(data['status'] == '-2'){
swal({
type: 'warning',
text: 'این ایمیل قبلا ثبت نام شده است',
confirmButtonText: 'بستن'
})
}
}
});
return false;
}
function downSubscribe2(email) {
swal({
type: 'info',
confirmButtonText: 'ثبت نهایی',
html:
'<input id="swal-name" class="swal2-input" placeholder="نام شما">' +
'<input id="swal-mobile" class="swal2-input" placeholder="شماره همراه (اختیاری)">',
showLoaderOnConfirm: true,
preConfirm: function (inputValue) { 
return new Promise(function (resolve, reject) {
var name = $('#swal-name').val();
if( name == ''){
swal.showValidationError('لطفا نام خود را وارد نمائید')
reject()
return false;
}
var mobile = $('#swal-mobile').val();
if( mobile != ''){
if( mobile.length != '11'){
swal.showValidationError('شماره همراه وارد شده نامعتبر می باشد')
reject()
return false;
}
var valid_mobile = mobile.charAt(0)+mobile.charAt(1);
if(valid_mobile != '09'){
swal.showValidationError('شماره همراه وارد شده نامعتبر می باشد')
reject()
return false;
}
}
var form_data = 'down_subscribe=true&email='+email+'&name='+name+'&mobile='+mobile+'';
jQuery.ajax({
data: form_data,
type: "POST",
dataType: 'json',
success: function(data){
if(data['status'] == '1'){
return downSubscribe2(email);
}
else if(data['status'] == '2'){
swal({
type: 'success',
text: 'ثبت نام در خبرنامه با موفقیت انجام شد',
})
}
else if(data['status'] == '-1'){
swal({
type: 'warning',
text: 'لطفا از ایمیل معتبر استفاده نمائید',
})
}
else if(data['status'] == '-2'){
swal({
type: 'warning',
text: 'این ایمیل قبلا ثبت نام شده است',
})
}
}
});
return false;		
})
},
//allowOutsideClick: false
}).then(function (result) {
})
return false;
}
</script>
