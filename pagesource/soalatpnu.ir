			$(function(){
				$.ajax({
					url: "service/service.php?module=Exam&request=getAllFields",
					success: function(data){
						var fields = [];
						for(var i=0;i<data.result.length;i++){
							var title = data.result[i].title;
							if(data.result[i].grade == 'کارشناسی ارشد') title = 'ارشد ' + title;
							fields.push({label: title, value: data.result[i].id, desc: ''});
						}
						$("#search_fields").autocomplete({
							minLength: 0,
							source: fields,
							focus: function( event, ui ) {
								$( "#search_fields" ).val( ui.item.label );
								$( "#search_fields_id" ).val( ui.item.value );
								return false;
							},
							select: function( event, ui ) {
								$( "#search_fields" ).val( ui.item.label );
								$( "#search_fields_id" ).val( ui.item.value );
								$( "#submit_field_btn" ).click();
								return false;
							}
						})
						.autocomplete( "instance" )._renderItem = function( ul, item ) {
							return $( "<li>" )
								.append( "<a>" + item.label + "<br>" + item.desc + "</a>" )
								.appendTo( ul );
						};
					},
					error: function(data){
						
					},
					dataType: "json"
				});
			});
			</script><input id="submit_field_btn" type="button" class="responsive_hidden_reverse" value="جستجو" onclick="window.location='?get_questions&field='+$('#search_fields_id').val();" />
