<script>
	function ticketReplyNotification(){
		$.ajax({
			url: "service/service.php?module=Ticket&request=getUserNewTickets", 
			success: function(result){
				if(result.status == "success"){
                    var last_cnt = window.last_ticket_cnt || 0;
					var cnt = result.count;
					$("#new_tickets_count").html(cnt);
					if(cnt > last_cnt)
						$("#ticket_notification_holder").fadeIn('fast');
					else if(cnt < last_cnt)
						$("#ticket_notification_holder").fadeOut('fast');
                    window.last_ticket_cnt = cnt;
				}
				setTimeout(ticketReplyNotification, 10000);
			},
			error: function(){
				setTimeout(ticketReplyNotification, 10000);
			},
			dataType: "json"
		});
	}
	setTimeout(ticketReplyNotification, 5000);
</script>
