<script>
$j(document).ready(function(){
    $j(".togglesearch").click(function(){

        $j("#tagline").css("display", "none");
        $j("#searchDiv").css("display", "inline-block");
        $j("#search").focus();
        $j('.search_icon').hide();
        $j('.close_icon').css("display","inline-block");
        $j('#logo-div').addClass("tagline-show");
        $j('#icon-div').addClass("tagline-show"); 
        
    });

    $j(".toggleclose").click(function(){
        $j("#tagline").css("display", "inline-block");
        $j("#searchDiv").css("display", "none");
        $j('.search_icon').css("display","inline-block");
        $j('.close_icon').hide();
        $j('#logo-div').removeClass("tagline-show");
        $j('#icon-div').removeClass("tagline-show");     
    });

    //menu width
    menuwidth();

});

$j(window).resize(function(){
    menuwidth();
});

function menuwidth()
{

    var fullwidth = $j(".megamenu-pc").width();
    var menuwidth = $j("#ms-topmenu").width();

    if($j(window).width() >= 1024){
        if(menuwidth == 0)
        {
            menuwidth = 1008;
        }
    }

    /*var menuwidth = width;*/
    var left = (fullwidth - menuwidth) / 2;
    
    $j(".ms-submenu").css('width',menuwidth);
    $j(".ms-submenu").css('left',left);
}


$j(".dropbtn").click(function(){
   $j(".dropdown-content").toggle(); 
   if($j("#header-cart").hasClass("skip-active"))
   {
        $j("#header-cart").removeClass("skip-active");
   }
});

// Close the dropdown menu if the user clicks outside of it
$j(document).click(function(){
    if($j(".dropdown-content").css('display') == "block")
    {
        $j(".dropdown-content").css("display","none");
    }
});

/* Clicks within the dropdown won't make
   it past the dropdown itself */
$j(".dropdown-content").click(function(e){
  e.stopPropagation();
});
$j(".dropdown").click(function(e){
  e.stopPropagation();
});

</script>

