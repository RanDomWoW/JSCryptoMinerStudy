    function countTimer(seconds, type, display, dealtype, dealclass, endtime) {
        var t = setInterval(function () {
            var hours = Math.floor(seconds / 3600);
            var amountOfSeconds = seconds - (hours * 3600);
            var minutes = Math.floor(amountOfSeconds / 60);
            var remainingSeconds = amountOfSeconds - ( minutes * 60 );
            hours = (hours < 10) ? "0" + hours : hours;
            minutes = minutes < 10 ? "0" + minutes : minutes;
            remainingSeconds = remainingSeconds < 10 ? "0" + remainingSeconds : remainingSeconds;
            dealtype.text("Deal " + type);
            //console.log(hours + ":" + minutes + ":" + remainingSeconds + " HRS");
            display.text(hours + ":" + minutes + ":" + remainingSeconds + " HRS");
            dealclass.addClass('deals');
            if (seconds == 0) {
                clearInterval(t);
                if (type == 'starts') {
                    if (endtime) {
                        countTimer(endtime, 'ends', display, dealtype, dealclass, null);
                    }
                } else {
                    dealclass.text("This Deal has Ended");
                }
            } else {
                seconds--;
            }
        }, 1000);
    }
