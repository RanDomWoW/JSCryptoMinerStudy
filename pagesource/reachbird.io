<script>
  (function() {
    var language_select_modal = document.getElementById('language-select-modal');
    var language_select_modal_inner = document.getElementById('language-select-modal-inner');
    var language_select_icon = document.getElementById('language-select-icon');
    var language_select_icon_text = document.getElementById('language-select-icon__text');

    var language_select_english_btn = document.getElementById('language-select-english-btn');
    var language_select_german_btn = document.getElementById('language-select-german-btn');

    language_select_english_btn.onclick = function() {
      App.setLanguageSetting('EN');
      App.redirectToLocale('EN');
    };

    language_select_german_btn.onclick = function() {
      App.setLanguageSetting('DE');
      App.redirectToLocale('DE');
    };

    language_select_icon_text.innerHTML = App.currentLocale();

    language_select_icon.onclick = function() {
      language_select_modal.classList.remove('hide');

      setTimeout(function() {
        var removeEventListener = App.onClickOutside([language_select_modal_inner], function() {
          language_select_modal.classList.add('hide');
          removeEventListener();
        });
      }, 100);

      if ($('.btn-nav').hasClass('active')) {
        $('.menu').slideUp();
        $('.btn-nav').removeClass('active');
      }
    };

  }())
</script>


