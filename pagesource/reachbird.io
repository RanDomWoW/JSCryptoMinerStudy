<script>
  window.App = {
    currentLocale: function() {
      if (window.location.href.includes('/de')) {
        return 'DE';
      } else {
        return 'EN';
      }
    },

    setLanguageSetting: function(language) {
      localStorage.setItem('language', language);

      return language;
    },

    redirectToLocale: function(locale) {
      if (locale == 'DE') {
        if (window.location.href.includes('influencer')) {
          window.location.href = '/de/influencer';
        } else {
          window.location.href = '/de/';
        }
      }

      if (locale == 'EN') {
        if (window.location.href.includes('influencer')) {
          window.location.href = '/influencers';
        } else {
          window.location.href = '/';
        }
      }
    },

    onClickOutside: function(elems, fn) {
      function handleClick(e) {
        var isOutside = !elems.map(function(elem) { return elem.contains(e.target) })
          .filter(function(bool) { return bool === true })
          .length;
        return isOutside && fn(isOutside);
      }

      document.addEventListener('click', handleClick);

      var removeEventListener = function() { document.removeEventListener('click', handleClick) };

      window.addEventListener('beforeunload', removeEventListener);

      return removeEventListener;
    },
  }
</script>

<script>
  (function() {
    var country_locales = {
      'AT': 'DE',
      'DE': 'DE',
    };

    var country_modal = document.getElementById('country-modal');
    var country_modal_inner = document.getElementById('country-modal-inner');
    var country_modal_country_name = document.getElementById('country-modal__country_name');

    var localize_btn = document.getElementById('localize-btn');
    var nolocalize_btn = document.getElementById('nolocalize-btn');

    function getLanguageSetting() {
      return localStorage.getItem('language');
    }

    function getCountry(success) {
      $.getJSON('https://app.reachbird.io/country', success)
    }

    localize_btn.onclick = function(e) {
      e.preventDefault();

      App.setLanguageSetting('DE');

      App.redirectToLocale('DE');
    };

    nolocalize_btn.onclick = function(e) {
      e.preventDefault();

      App.setLanguageSetting('EN');

      country_modal.classList.add('hide');
    };

    (function run() {
      if (getLanguageSetting()) {
        if (App.currentLocale() !== getLanguageSetting()){
          return App.redirectToLocale(getLanguageSetting());
        }
      }

      if (getLanguageSetting() == null) {
        getCountry(function(obj) {
          var detected_country_code = obj.country_code;

          if (App.currentLocale() == country_locales[detected_country_code]) {
            return App.setLanguageSetting(App.currentLocale());
          } else if (country_locales[detected_country_code]) {
            console.log(obj);
            country_modal_country_name.innerHTML =
              obj.country_name_translations[country_locales[detected_country_code].toLowerCase()] || obj.country_name;

            country_modal.classList.remove('hide');

            setTimeout(function() {
              var removeEventListener = App.onClickOutside([country_modal_inner], function() {
                country_modal.classList.add('hide');
                App.setLanguageSetting(App.currentLocale());
                removeEventListener();
              });
            }, 100);
          }
        });
      }
    }())

  }())
</script>

