<script type="text/javascript">
    function RUrl(url)
    {
        if (url == "") {
            return void(0);
        } else {
            if (url.indexOf("http://") == -1) {
                url = "http://" + url;
            } 
        }
        window.open(url);
    }
    function Kurl(val) {
        var url = "http://www.baidu.com/s?wd=云搜宝";
        window.open(url.replace('云搜宝', encodeURIComponent(val)));
    }
    function KeyPrice() {
        var val=0;
        if($.trim(val)<=0)
        {
            alert("请先登录");
            window.open("/Member/Login.aspx");
            return false;
        }
        val = $.trim($(".seo-input").val());
        if (val != "" && val != "每天查询关键词价格") {
            $(".seo-keyword").html('查询结果：' + val);
            $(".seo-result span").html("<img src='/Images/loading.gif'/>");
            $.post("/Ajax/Keyword.ashx?T" + Math.random() + "=" + Math.random(), { Act: "GetNewKeyWordIndex", Value: escape(val) },
                function (data) {
                    data = eval('(' + data + ')');
                    switch (data.tag) {
                        case '-5':
                        case '0':
                            alert("系统忙，请稍后重试");
                            $(".seo-result span").html("暂无");
                            break;
                        case '-4':
                            alert("请输入关键词");
                            $(".seo-result span").html("暂无");
                            break;
                        case '-3':
                            alert("对不起，免费客户只允许查5次");
                            $(".seo-result span").html("暂无");
                            break;
                        case '-2':
                            alert("查询出错，请稍后重试");
                            $(".seo-result span").html("暂无");
                            break;
                        case '-1':
                            alert("云搜宝不提供关键词【" + val + "】的优化服务，请选择类似长尾关键词！");
                            $(".seo-result span").html("暂无");
                            break;
                        case '0':
                            alert("查询失败");
                            break;
                        case '1':
                            $(".seo-result").html("<li>百度第一页<span>" + data.bd1 + "</span></li>"
                                + "<li>百度第二页<span>" + data.bd2 + "</span></li>"
                                + "<li>谷歌第一页<span>" + data.gg1 + "</span></li>"
                                + "<li>谷歌第二页<span>" + data.gg2 + "</span></li>");
                            break;
                    }
                });
        } else {
            alert('请输入关键词');
        }
    }
    $(function () {
        $(".seo-btn").click(function () {
            KeyPrice();
        });
        $(".kUrl").click(function () {
            Kurl($(this).attr('ref'));
        });
        $(".seo-input").keypress(function (e) {
            if (e.keyCode == 13) {
                KeyPrice();
            }
        });
    })
</script>  
