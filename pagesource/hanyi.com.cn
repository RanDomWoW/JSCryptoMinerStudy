<script type="text/javascript">
    $('#activateModal').on('hidden.bs.modal', function () {
        // 执行一些动作...
        if ($.cookie("hanyiUser")) {
            window.location.reload();
        }
    })


    var findActive = function () {
        var urls = window.location.href.split("?");
        var url = urls[0];

        console.log(url);
        $("ul.navbar-nav li a").each(function () {
            var that = $(this);
            var hrefVal = that.attr("href");
            var d = url.length - hrefVal.length;
            if (url.lastIndexOf(hrefVal) == d && d >= 0) {
                console.log(url);
                console.log(hrefVal);
                if (that.parent().parent().attr("class") == 'dropdownInner') {
                    that.parent().parent().parent().prev().addClass("active");
                } else {
                    that.addClass("active");
                }
            }
        });
    }
    findActive();

    //add icon
    $("#loginModal").on("shown.bs.modal", function () {
        var $captcha_img = $(".captcha").parents('.row').find('img'),
            captcha_init = function() {
                $captcha_img.attr("src", 'data/captcha/image?tmp=' + (new Date()).getTime())
            };
        captcha_init();
        $captcha_img.click(captcha_init);
        //增加enter确定
        $("#login .psw").keypress(function (e) {
            if (e.which == 13) {
                $("#login .loginSubmit").click();
            }
        });
        $("#register .password").keypress(function (e) {
            if (e.which == 13) {
                $("#register .registerBtn").click();
            }
        });
    });
    //点击弹出注册框、登录框
    $(".loginBox .loginBtn").click(function () {
        $("#myTab li:eq(0) a").tab('show');

    });
    $(".loginBox .registerBtn").click(function () {
        $("#myTab li:eq(1) a").tab('show');
    });

    //验证手机号,暂时没有添加正则表达式验证
    var checkTel = function (tel) {
        if (tel == null || tel.length < 6) {
            return false;
        } else {
            return true;
        }
    }

    var checkName = function (name) {
        if (name == null || name.length < 1) {
            return false;
        } else {
            return true;
        }
    }
    //点击登录ajax验证
    $("#loginModal #login .loginSubmit").click(function () {
        $("#loginModal #login .help-block").text("");
        var errorMsg = ["手机号或者密码输入不正确", "账号密码不匹配"];
        var tel = $("#loginModal #login .telphone").val();
        var psw = $("#loginModal #login .psw").val();
        if (!checkTel(tel) || psw == null || psw.length < 6) {
            $("#loginModal #login .help-block").text(errorMsg[0]);
        } else {
            //发动ajax来判断用户账号密码是否匹配，如果匹配，则存储cookie并刷新页面
            $.ajax({
                url: "data/login/login.php",
                type: "POST",
                data: {
                    "tel": tel,
                    "psw": psw
                },
                success: function (data) {
                    console.log(data);
                    if (data == "111") {
                        location.reload();
                    } else {
                        $("#loginModal #login .help-block").text(errorMsg[1]);
                    }
                }
            });
        }
    });
    //注册部分
    //验证码验证
    var initSeconds = 60;
    var second = initSeconds;
    var hyRootUrl = "http://www.hanyi.studio";
    var showSecond = function () {
        if (second > 0) {
            $("#loginModal #register .codeBtn").text("还剩" + second + "秒");
            second--;
        }
        if (second == 0) {
            $("#loginModal #register .help-block").html("<font style='color:red;'>获取验证码超时</font>");
            $("#loginModal #register .codeBtn").text("获取验证码");
        }
        return second;
    }
    $("#loginModal #register .codeBtn").click(function () {
        //首先检查手机号输入是否正确
        var tel = $("#loginModal #register .telphone").val();
        var captcha = $("#loginModal #register .captcha").val();
        var $help_block = $("#loginModal #register .help-block");
        if (captcha.length != 4) {
            $help_block.html("<font style='color:red;'>验证码输入错误</font>");
            return;
        }
        if (!checkTel(tel)) {
            $help_block.html("<font style='color:red;'>手机号输入不正确</font>");
            return;
        }
        $help_block.html("");
        $.ajax({
            url: "data/captcha/sms",
            type: "post",
            dataType: 'json',
            data: {
                "tel": tel,
                "captcha": captcha,
                "opera": "register"
            },
            success: function (data) {
                console.log(data);
                if (data.Exception == true) {
                    $("#loginModal #register .help-block").html("<font style='color:red;'>" + data.ExceptionMessage + "</font>");
                    return;
                }
                second = initSeconds;
                if (typeof(t) == "undefined") {
                    showSecond();
                    t = setInterval("showSecond()", 1000);
