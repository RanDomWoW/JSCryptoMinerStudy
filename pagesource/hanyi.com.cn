<script>
    // 1024获取页面来源
    window.record_refer = function (event) {
        var refer = $.cookie('site_refer')||'',
            site = refer.replace(/https*:\/\//,'').match(/[^\/]*/,'')[0];
        _czc && _czc.push(["_trackEvent", event, site]);
        $.ajax({
            url: "data/event/put",
            type: 'post',
            data: {
                "category": event,
                "action": site,
                "label": document.referrer,
                "value": refer,
                "page": location.href
            }
        });
    };
    if ($.cookie("hanyiUser")) {
        $("#activateModal .wxMarkBtn").unbind("click").click(function () {
            //此处进行激活
            var activatecode = $("#activateModal .wxMarkInput input").val();
            if (activatecode != "") {
                console.log($.cookie("hanyiUser"));
                users = getUsersByCookie($.cookie("hanyiUser"));
                $.ajax({
                    url: hyRootUrl + "/hanyiwebsite/usermanagement/usermanagerhandler.ashx",
                    data: {
                        "opera": "activate",
                        "id": $.cookie("hanyiUser"),
                        "activatecode": activatecode
                    },
                    success: function (data) {
                        console.log(data);
                        if (data.Exception) {
                            alert(data.ExceptionMessage);
                        } else {
                            alert("激活成功");
                            record_refer("用户激活");
                            window.location.reload();
                        }

                    }
                });
            } else {
                alert("验证码不能为空");
            }
        });
    }

    if (getUserName() != null) {
        $(".loginBox .on .dropdown-toggle .username").text(getUserName());
        $(".loginBox .on").removeClass("hide");
        $(".loginBox .off").hide();
    }
    $(document).ready(function () {
        //返回头部
        $(".arrowUp").click(function () {
            $('body').animate({scrollTop: 0}, 200);
        });
    });
</script>
