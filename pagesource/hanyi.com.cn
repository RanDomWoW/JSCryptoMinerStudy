    //找回密码部分监听
    var showSecondFP = function () {
        if (second > 0) {
            $("#forgetPwdModal #actiTel .codeBtn").text("还剩" + second + "秒");
            second--;
        }
        if (second == 0) {
            $("#forgetPwdModal #actiTel .help-block").html("<font style='color:red;'>获取验证码超时</font>");
            $("#forgetPwdModal #actiTel .codeBtn").text("获取验证码");
        }
        return second;
    }
    $("#forgetPwdModal #actiTel .codeBtn").click(function () {
        //首先检查手机号输入是否正确
        var tel = $("#forgetPwdModal #actiTel .telphone").val();
        var captcha = $("#forgetPwdModal #actiTel .captcha").val();
        var $help_block = $("#forgetPwdModal #actiTel .help-block");
        if (captcha.length != 4) {
            $help_block.html("<font style='color:red;'>验证码输入错误</font>");
            return;
        }
        if (!checkTel(tel)) {
            $help_block.html("<font style='color:red;'>手机号输入不正确</font>");
            return;
        }
        $help_block.html("");

        $.ajax({
            url: "data/captcha/sms",
            type: "post",
            dataType: 'json',
            data: {
                'captcha': captcha,
                "tel": tel,
                "opera": "reset"
            },
            success: function (data) {
                if (data.Exception == true) {
                    $("#forgetPwdModal #actiTel .help-block").html("<font style='color:red;'>" + data.ExceptionMessage + "</font>");
                    return;
                }
                second = initSeconds;
                if (typeof(t) == "undefined") {
                    showSecondFP();
                    t = setInterval("showSecondFP()", 1000);
                }
                if ($("#forgetPwdModal #actiTel .codeBtn").text() != "获取验证码") {
                    //说明此时还在时间范围内
                    $("#forgetPwdModal #actiTel .idcode").change(function () {
                        if ($(this).val() == data.Code) {
                            clearInterval(t);
                            $("#forgetPwdModal #actiTel .codeBtn").hide();
                            $("#forgetPwdModal #actiTel .codeSuccessBtn").removeClass("hide");
                            //获取新密码、新的重复密码，验证后注册
                            $("#forgetPwdModal #actiTel .registerBtn").click(function () {
                                //处理重置逻辑
                                //如果密码不小于6位，则进入下一步否则提示
                                //如果俩次密码相同，则修改成功、否则提示
                                var fpwd = $("#forgetPwdModal #actiTel .fpwd").val();
                                var fpwdConfirm = $("#forgetPwdModal #actiTel .fpwdConfirm").val();
                                if (fpwd.length >= 6) {
                                    if (fpwdConfirm == fpwd) {
                                        //执行修改密码操作
                                        $.ajax({
                                            url: 'http://www.hanyi.studio/hanyiwebsite/usermanagement/usermanagerhandler.ashx',
                                            type: 'GET',
                                            data: {
                                                "opera": "reset",
                                                "tel": tel,
                                                "pwd": fpwd
                                            },
                                            success: function (data) {
                                                console.log(data);
                                                alert("修改成功,请重新登陆！");
                                                window.location.reload();
                                            }
                                        });
                                    } else {
                                        $("#forgetPwdModal #actiTel .help-block").html("<font style='color:red;'>俩次输入密码不一致</font>");
                                    }
                                } else {
                                    $("#forgetPwdModal #actiTel .help-block").html("<font style='color:red;'>密码长度不能小于6位</font>");
                                }
                            });
                        }
                    });
                }
            }
        });
    });
</script>
