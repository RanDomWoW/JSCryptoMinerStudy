<html>
<head>
<script language="JavaScript">


  switch((navigator.language || navigator.browserLanguage).toLowerCase())
	{
		case 'en':
		case 'en-us':
		case 'en-gb':
		case 'en-ca':
		case 'en-au':
		case 'en-nz':
		case 'en-ie':
		case 'en-za':
		case 'en-bz':
		case 'en-tt':
		window.location.replace("tsa/en/home.aspx");
		break;
		case 'ja':
		case 'jp':
		window.location.replace("tsa/ja/home.aspx");
		break;
		case 'ko':
		case 'ko-kr':
		case 'ko-kp':
		window.location.replace("tsa/ko/home.aspx");
		break;
		default:
		window.location.replace("tsa/zh/home.aspx");
	}
</script>
</head>
</html>