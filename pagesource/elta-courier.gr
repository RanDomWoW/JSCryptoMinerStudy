				 .selectAll("path")
				 .transition()
				 .style("fill","#aaa")
				 .duration(100);
			map.select('#' + nomos)
				 .selectAll("polygon")
				 .transition()
				 .style("fill","#aaa")
				 .duration(100);
  });
}

function search_tk() {
	if( $('#tk-search-input').val().length < 5)
		return;
	$.ajax({
		url: "/search_tk.php",
		type: "POST",
		dataType: 'json',
		data: { 'tk': $('#tk-search-input').val() },
	})
	.done( function( res ) {
		if(res['result'] == 'ok') {
			showStores(res['prefecture_id'],res['prefecture_name'], function() {
				showInfos(res['store_id']);
				$('#stores-list .list').animate({
																		scrollTop: $("#" + res['store_id']).offset().top - 500
																}, 1000);
			});
		} else {
			$('#stores').html('<p style="padding: 10px;margin: 10px;">Δεν βρέθηκε κατάστημα για αυτό τον ταχυδρομικό κώδικα.</p>');
		}
	});
}

