function track_table( tracking_data) {
	var res = '<div class="table-responsive">' +
						'<table class="table table-hover">' +
							'<tr>' +
								'<th>Κατάσταση</th>' +
								'<th>Ημερομηνία</th>' +
								'<th>Ώρα</th>' +
								'<th>Περιοχή</th>' +
							'</tr>';
	for ( var i in tracking_data ) {
		res += 	'<tr>' +
							'<td>' + tracking_data[i]['status'] + '</td>' +
							'<td>' + tracking_data[i]['date'] + '</td>' +
							'<td>' + tracking_data[i]['time'] + '</td>' +
							'<td>' + tracking_data[i]['place'] + '</td>' +
						'</tr>';
	}
	res += 	'</table>' +
					'</div>';
	return res;
}

