function calculate_cost() {
	$('.has-error').removeClass('has-error');
	$('.input_error').html('');
	$('#calc_response').html('');
	
	var has_error = false;
	
	if( $('#aweight').val() == '' && $('#vweight').val() == '' ) {
		showError('aweight')
		showError('vweight', 'Εισάγετε τo βάρος της αποστολής');
		has_error = true;
	}
	
	if( $('#aweight').val() != '' && !$.isNumeric($('#aweight').val()) )	{
		showError('aweight', 'Το βάρος που έχετε εισαγάγει δεν είναι έγκυρο.  Για δεκαδικές τιμές χρησιμοποιήστε τελεία και όχι κόμμα.');
		has_error = true;
	} else if( $('#vweight').val() != '' && !$.isNumeric($('#vweight').val()) )	{
		showError('vweight', 'Το βάρος που έχετε εισαγάγει δεν είναι έγκυρο.  Για δεκαδικές τιμές χρησιμοποιήστε τελεία και όχι κόμμα.');
		has_error = true;
	}
	
	var tk_from = $('#tk_from').val();
	var tk_to = $('#tk_to').val();
	
	if( tk_from == '' ) {
		showError('tk_from', 'Eισάγετε ταχυδρομικό κωδικό της περιοχής παραλαβής της αποστολής');
		has_error = true;
	}
	
	if(!Math.floor(tk_from) == tk_from || !$.isNumeric(tk_from)) {
		showError('tk_from', 'O ταχυδρομικός κωδικός δεν είναι έγκυρος.');
		has_error = true;
	}	

	if( tk_to == '' ) {
		showError('tk_to', 'Eισάγετε ταχυδρομικό κωδικό της περιοχής προορισμού της αποστολής');
		has_error = true;
	}
	
	if(!Math.floor(tk_to) == tk_to || !$.isNumeric(tk_to)) {
		showError('tk_to', 'O ταχυδρομικός κωδικός δεν είναι έγκυρος.');
		has_error = true;
	}
	
	if( $('#da_poso').val().length > 0 )
		$('#da_flag').prop( "checked", true );
	else if( $('#da_flag').prop( "checked" ) == true ) {
		showError('da_poso', 'Εισάγετε ποσό δηλωμένης αξίας.');
		has_error = true;
	}
	
	if( $('#da_poso').val() != '' && !$.isNumeric($('#da_poso').val()) )	{
		showError('da_poso', 'Το ποσό που έχετε εισαγάγει δεν είναι έγκυρο.  Για δεκαδικές τιμές χρησιμοποιήστε τελεία και όχι κόμμα.');
		has_error = true;
	}
	
	if(!has_error) {
		var ant_flag = 0;
		var da_flag = 0;
		var ed_flag = 0;
		var po_flag = 0;
		var es_flag = 0;
		var ps_flag = 0;
		if($('#ant_flag').is(':checked'))
			ant_flag = 1;
		if($('#da_flag').is(':checked'))
			da_flag = 1;
		if($('#ed_flag').is(':checked'))
			ed_flag = 1;
		if($('#po_flag').is(':checked'))
			po_flag = 1;
		if($('#es_flag').is(':checked'))
			es_flag = 1;
		if($('#ps_flag').is(':checked'))
			ps_flag = 1;
		
		$('#calc_response').html('<img style="margin-top:20px;" src="/img/loading.gif" />');
		
		$.ajax({
			url: "price.php",
			type: "POST",
			dataType: 'json',
			data: { 'aweight': $('#aweight').val(), 
							'vweight': $('#vweight').val(), 
							'tk_from': $('#tk_from').val(), 
							'tk_to': $('#tk_to').val(), 
							'ant_flag': ant_flag, 
							'da_flag': da_flag, 
							'da_poso': $('#da_poso').val(), 
							'ed_flag': ed_flag, 
							'po_flag': po_flag, 
							'es_flag': es_flag, 
							'ps_flag': ps_flag },
		})
		.done( function( res ) {
			if( res['status'] == 0 ) {
				$('#calc_response').html(res['result'])
			} else {
				var cost =  ( (	Number(res['result']['apostoli']) + 
												Number(res['result']['ant']) + 
												Number(res['result']['da']) + 
												Number(res['result']['ed']) + 
												Number(res['result']['po']) + 
												Number(res['result']['es']) +
												Number(res['result']['ps']) ) * 
												(1 + Number(res['result']['vat'])/100 ) ).toFixed(2);
				var html = 	'<table class="table table-condensed">' +
										'<tr><td>Κατάστημα παραλαβής: </td><td>' + res['result']['from'] + '</td></tr>' +
										'<tr><td>Κατάστημα προορισμού: </td><td>' + res['result']['to'] + '</td></tr>' + 
										'<tr><td>Κόστος αποστολής (χωρίς Φ.Π.Α): </td><td>' + res['result']['apostoli'] + '€</td></tr>';
				if(res['result']['ant'] > 0)
					html += '<tr><td>Αντικαταβολή: </td><td>' + res['result']['ant'] + '€</td></tr>';
				if(res['result']['da'] > 0)
					html += '<tr><td>Δηλωμένη Αξία: </td><td>' + res['result']['da'] + '€</td></tr>';
				if(res['result']['ed'] > 0)
					html += '<tr><td>Ειδική Διαχείριση: </td><td>' + res['result']['ed'] + '€</td></tr>';
				if(res['result']['po'] > 0)
					html += '<tr><td>Προκαθορισμένη Ώρα: </td><td>' + res['result']['po'] + '€</td></tr>';
				if(res['result']['es'] > 0)
					html += '<tr><td>Επίδοση Σαββάτου: </td><td>' + res['result']['es'] + '€</td></tr>';
				if(res['result']['ps'] > 0)
					html += '<tr><td>Παραλαβή Σαββάτου: </td><td>' + res['result']['ps'] + '€</td></tr>';
				html += '<tr><td>Φ.Π.Α: </td><td>' + res['result']['vat'] + '%</td></tr>' +
								'<tr class="success"><td>Συνολικό κόστος: </td><td>' + cost + '€</td></tr>';
				$('#calc_response').html(	html );
			}
		});
	}
}

function calculate_ogko() {
	$('.has-error').removeClass('has-error');
	$('.input_error').html('');
	$('#ogko_response').html('');
	
	var has_error = false;
	
	if( $('#mikos').val() == '' )	{
		showError('mikos', 'Εισάγετε το μήκος.');
		has_error = true;
	} else if( !$.isNumeric($('#mikos').val()) )	{
		showError('mikos', 'Το μήκος που έχετε εισαγάγει δεν είναι έγκυρο.  Για δεκαδικές τιμές χρησιμοποιήστε τελεία και όχι κόμμα.');
		has_error = true;
	}
	
	if( $('#platos').val() == '' )	{
		showError('platos', 'Εισάγετε το πλάτος.');
		has_error = true;
	} else if( !$.isNumeric($('#platos').val()) )	{
		showError('platos', 'Το πλάτος που έχετε εισαγάγει δεν είναι έγκυρο.  Για δεκαδικές τιμές χρησιμοποιήστε τελεία και όχι κόμμα.');
		has_error = true;
	}
	
	if( $('#ypsos').val() == '' )	{
		showError('ypsos', 'Εισάγετε το ύψος.');
		has_error = true;
	} else if( !$.isNumeric($('#ypsos').val()) )	{
		showError('ypsos', 'Το ύψος που έχετε εισαγάγει δεν είναι έγκυρο.  Για δεκαδικές τιμές χρησιμοποιήστε τελεία και όχι κόμμα.');
		has_error = true;
	}
	
	if(!has_error) {
		var ogkos = (( Number($('#mikos').val()) * Number($('#platos').val()) * Number($('#ypsos').val()) ) / 5000).toFixed(5).replace(".", ",");
		$('#ogko_response').html( '<table class="table table-condensed">' +
															'<tr class="success"><td>Ογκομετρικό Βάρος: </td><td>' + ogkos + ' κιλά/kg</td></tr>' +
															'</table>');
	}
}

function showError( field_id, message ) {
