function track() {
	if( $('#voucher').val() == '')
		return;
	$('#search_response').html('<img style="margin-top:20px;" src="/img/loading.gif" />');
	$.ajax({
		url: "/track.php",
		type: "POST",
		dataType: 'json',
		data: { 'number': $('#voucher').val() },
	})
	.done( function( res ) {
		$('#search_response').html('');
		if( res['status'] == 1 ) {
			for( var i in res['result'] ) {
				$('#search_response').append('<h4>ΑΠΟΣΤΟΛΗ ' + escapeHtml(i) + '</h4>');
				if( res['result'][i]['status'] == 1 )
					$('#search_response').append( track_table( res['result'][i]['result'] ) );
				else if ( res['result'][i]['status'] == 2 )
					$('#search_response').append( '<p>' + res['result'][i]['result'] + '</p>' );
				else
					$('#search_response').append( '<p>Δεν υπάρχει καταχώριση για τον αριθμό ' + escapeHtml(i) + '</p>' );
			}
