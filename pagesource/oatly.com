  <script type="text/javascript">
      $(function() {

          function initCountryPopUp() {
            $("#countryPopup, #popupOverlay").addClass('isactive');

      			$("#countrySelect").countrySelect({
      				defaultCountry: "us",
      				onlyCountries: ['us', 'fi', 'fr', 'de', 'nl', 'no', 'es', 'se', 'gb'],
      				preferredCountries: ['us'],
              responsiveDropdown: true
      			});

            $(window).on('resize', function () {
              $('.selected-flag, .country-list').width( $('.country-select').width() + 15 );
            });
            $(window).resize();

            $(document).on('click', '#countryPopup .continue', function(e) {
              var countryData = $("#countrySelect").countrySelect("getSelectedCountryData"),
                  countryCode = countryData.iso2.toLowerCase(),
                  countryName = countryData.name.toLowerCase(),
                  countryName = countryName.replace(/\(.*/,'').replace(' ', '').replace(' ', '-');
                  //console.log(countryName);

              Cookies.set('Oatley_country_code', countryCode);

              if (countryCode == 'us'){
                window.location.href = 'https://us.oatly.com';
              }else{
                window.location.href = 'http://www.oatly.com/?oat_lang='+countryName+'&url=http://www.oatly.com/';
              }
            });
          }

          $.ajax({
            url: 'http://freegeoip.net/json/',
            type: 'GET',
            success: function(data) {
              var countryCode = data.country_code,
                  popupStatusCookie = Cookies.get('Oatley_country_popup'),
                  countryCookie = Cookies.get('Oatley_country_code');

              // redirect on site load automatically
              //if(countryCookie == 'us' && countryCode == 'US'){
                //window.location.href = 'https://us.oatly.com';
              //}

              if(popupStatusCookie == undefined && countryCode == 'US'){
                setTimeout(function(){
                  initCountryPopUp();
                }, 2000);
              }

              $(document).on('click', '#countryPopup #closeBtn', function(e) {
                Cookies.set('Oatley_country_popup', 1);
                $("#countryPopup, #popupOverlay").removeClass('isactive');

                // Redirecto when clicking on popup close button if country is US
                if(countryCode == 'US'){
                  Cookies.set('Oatley_country_code', 'us');
                  window.location.href = 'https://us.oatly.com';
                }
              });

            },
            error: function(data) {
              //console.log(data);
            }
          })

      });
  </script>


