    <script>
        window.app = angular.module('InstitutionalApp', ['inst_ajax']);
        app.config(function($interpolateProvider){
            $interpolateProvider.startSymbol('{[{');
            $interpolateProvider.endSymbol('}]}');
        });
        window.test_version = "default"; // Setar aqui com o optimizely um indentificador para este teste caso exista

        // Criar um usuário visitante ou atualizar o que já existe
        app.run(function(Ajax, $interval) {
            var successCallback = function(result) {
                if (!localStorage.getItem("visitorId") && jQuery.isNumeric(result.data)) {
                    localStorage.setItem("visitorId", result.data);
                }
            };
            Ajax.post('/rest_api/visitor_rest_api/api/salvar_perfil', {
                perfil_dict: angular.toJson({
                    visitor_id: localStorage.getItem("visitorId") || "",
                    test_version: window.test_version,
                    user_agent: navigator.userAgent})
            }).then(successCallback);

            // Ficar atualizando permanência do usuário de 15 em 15 segundos
            $interval(function(){
                Ajax.post('/rest_api/visitor_rest_api/api/salvar_perfil', {
                    perfil_dict: angular.toJson({
                        visitor_id: localStorage.getItem("visitorId") || "",
                        update_duration: true,
                        test_user_id: localStorage.getItem("testUserId") || "",
                        user_agent: navigator.userAgent})
                }).then(successCallback);
            }, 15000);
        });
    </script>

