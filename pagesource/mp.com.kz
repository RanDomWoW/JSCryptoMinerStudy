        <script type="text/javascript">
            var additionalElementsLoading = false;
            var page = 1;
            var maxPage = parseInt(21.1);
            var lotIndex = 0;
            var lotsCount = 202;
            var countPerPage = 10;

            jQuery(document).ready(function () {
                jQuery(window).scroll(function () {
                    additionalElementsLoad();
                });
            });

            function additionalElementsLoad() {
                if (!additionalElementsLoading && page < maxPage && (jQuery(window).scrollTop() + jQuery(window).height() * 1.5) > jQuery('#addtional_elements_line').offset().top) {
                    additionalElementsLoading = true;
                    page++;
                    jQuery("body").addClass("ajax-wait");
                    jQuery.ajax({
                        url: "/?loadAdditional=",
                        type: "post",
                        dataType: "json",
                        data: "searchPage=" + page,
                        success: function (data) {
                            jQuery(data.result).each(function (i, v) {
                                v.index = lotIndex++;
                            });
                            jQuery('#lotRowTmpl').tmpl(data.result).insertBefore('#insertbefore');
                            if (page == maxPage) {
                                jQuery("#addtional_elements_line").hide();
                            }
                        },
                        error: function () {
                        },
                        complete: function () {
                            additionalElementsLoading = false;
                            jQuery("body").removeClass("ajax-wait");
                        }
                    })
                }
            }
        </script>
        
        
        
