<script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.min.js"></script>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyDcPj_jrcT6t_SEov8kRXkiVj11_e94yjg&region=GB" type="text/javascript"></script>
  <script>

    //init Autocomplete on input
    $(document).ready(function(){
      $('#service_type_dropdown').select2();
        $("#service_type_dropdown").val('').trigger('change')

    });

    function initialize() {
      var options = {
          componentRestrictions: {country: "gb"}
      };
        var input = document.getElementById('lookupField');
        var autocomplete = new google.maps.places.Autocomplete(input,options);
        google.maps.event.addListener(autocomplete, 'place_changed', function () {
            var place = autocomplete.getPlace();
            document.getElementById('latField').value = place.geometry.location.lat();
            document.getElementById('lngField').value = place.geometry.location.lng();
        });
    }
    google.maps.event.addDomListener(window, 'load', initialize);



    //Load in the service types

    $.ajax({
    url:'https://findservices.brook.org.uk/service_types.json',
    type:'GET',
    dataType: 'json',
    success: function( json ) {
            $.each(json.service_types, function(i, value) {
                $('#service_type_dropdown').append($('<option>').text(value.service_type.name).attr('value', value.service_type.id.$oid));
            });
        }
    });

    $('#service_type_dropdown').on('change',function(){

        $('#providingField').val($('#service_type_dropdown option:selected').text());
    });

    //Now setup validation
    $("#searchForm").submit(function(event){


      //Validate the form
      $("#searchForm").validate({ignore: [],rules: {post: 'required',lat: 'required',lng: 'required'},messages: {lng:'Please select an entry from the location dropdown.'}});


      if(!$("#searchForm").valid()){event.preventDefault();}



    });


    $('#find-location-link,#find-location-link-mob').on('click touchstart',function(event){

      event.preventDefault();
      $(this).text('Retrieving location...')
      if (navigator.geolocation) {navigator.geolocation.getCurrentPosition(setAddress);} else {
            $('#find-location-link,#find-location-link-mob').text('Geocode not supported')
       }



    });

    function setAddress(position){

      var geocoder = new google.maps.Geocoder();

          //now set the lat lng fields

          $('#latField').val(position.coords.latitude);
          $('#lngField').val(position.coords.longitude);

          var latlng = {lat: parseFloat(position.coords.latitude), lng: parseFloat(position.coords.longitude)};
          geocoder.geocode({'location': latlng}, function(results, status) {
            if (status === 'OK') {
              if (results[0]) {

                var addrComp = results[0].address_components
                var l = addrComp.length

                var addressResult = []

                for(var i =0; i < l; i++)
                {
                    if(addrComp[i].types[0] == 'route' || addrComp[i].types[0] == 'locality')
                    {addressResult.push(addrComp[i].long_name)}


                }

                var formattedAddress = addressResult.join(', ')

                $('#lookupField').val(formattedAddress);
                $('#lookupField').removeAttr('placeholder');
                $('#find-location-link,#find-location-link-mob').text('Find my current location')
              }
            } else {
              window.alert('Geocoder failed due to: ' + status);
            }
          });


    }

    </script>



  





