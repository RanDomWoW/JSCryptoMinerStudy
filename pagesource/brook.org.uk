            // Internet Explorer and use "x.y" to determine the
            // version.

            var re  = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
            if (re.exec(ua) != null)
                rv = parseFloat( RegExp.$1 );
            return rv;

