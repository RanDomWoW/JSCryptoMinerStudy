	padding-bottom:100px !important;
}

@media (max-width: 480px){
  .imp-details .x-block-grid-item:last-of-type{
  	margin-left:5%;
	}

	.sales-page .x-section.bg-image .x-container{
		padding:0px !important;
		background-color:transparent !important;
		width:90% !important;
	}

	.sales-page .x-section.bg-image .x-btn.x-btn-global{
		width:100% !important;
	}

	.sales-page .x-section:nth-of-type(n+2) .x-btn.x-btn-large{
		width:100% !important;
	}

	.sales-page .x-section.card-layout {
    	margin-left: -35px !important;
	    margin-right: -35px !important;
	}

	#cta .x-container.narrow
	{
		width:90% !important;
	}

	.sales-page .order-option .x-text ul{
		margin-left:0px !important;
	}	

	

}

@media (max-width:1200px){
	.sales-page .x-section.bg-image .x-btn{
		width:60% !important;
	}
}

@media (max-width: 979px){
	.lesson-section .x-container:nth-of-type(n+2){
		flex-wrap:wrap;
	}
	
	.sales-page .x-section.card-layout{
		padding-left:45px !important;
		padding-right:45px !important;
	}
	
	.sales-page .x-section.bg-image .x-btn{
	width:70% !important;
}

	.card-layout .x-container:nth-of-type(n+1){
		width:49% !important;
	}

	.card-layout .x-container:first-of-type{
		width: 80% !important;
	}

	.lesson-section .x-container:nth-of-type(n+2) .x-column{
   	width:45%;
    margin-bottom:30px;
  }

	
	.lesson-section .x-container:nth-of-type(n+2) .x-column.x-1-3{
		width:49% !important;
	}

	.sales-page .order-option{
		
	}
	
	.x-container.narrow,
	.x-container.narrow-width{
		width:80% !important;
	}
}

@media (max-width: 767px){
	.card-layout .x-container:nth-of-type(n+1){
		width:100% !important;
		height:auto !important;
		margin-bottom:30px !important;
	}

	.sales-page .x-section.bg-image:before{
		content: " ";
    	display: block;
    	background-color: #ffffff;
    	height: 100%;
    	width: 100%;
    	position: absolute;
    	top: 0;
    	opacity: .8;
	}

	.sales-page .x-section.bg-image{
		padding-top:50px !important;
		padding-bottom:50px !important;
	}

	.sales-page .x-section.bg-image .x-container{
		width:80% !important;
	}

	.sales-page .x-section.bg-image .x-container .x-column.x-sm{
		padding:0px !important;
		background-color:transparent !important;
	}

	.sales-page .x-section.bg-image .x-btn{
		width:85% !important;
	}

	.sales-page .x-section.bg-image .x-container .x-column .h1{
		font-size:200%;
	}
	
	.lesson-section .x-container:nth-of-type(n+2) .x-column.x-1-3,
	.lesson-section .x-container:nth-of-type(n+2) .x-column.x-1-2{
		width:90% !important;
		margin-left:5% !important;
		margin-right:5% !important;
	}

	.imp-details.move-to-top{
		/*margin-top:-20px !important;*/
	}

	.narrow,
	.x-container.narrow-width{
		width:90% !important;
	}
	
	#section-instructors .x-container{
		margin-bottom:60px !important;
	}

	#section-instructors .h5{
		text-align:center;
		margin-top:40px;
	}

	#section-instructors img{
		margin:0 auto;
		display:block;
	}

	.sales-page .x-section:nth-of-type(n+2) .x-btn{
		width:auto !important;
	}

	.sales-page .order-option .x-text{
		width:100% !important;
	}
}

@media (min-width:1201px){
	.sales-page .x-section.card-layout{
		padding-left:calc( (100% - 1200px) / 2 ) !important;
		padding-right:calc( (100% - 1200px) / 2 ) !important;
	}
}
