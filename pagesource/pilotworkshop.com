}

.x-btn.ebu-btn{
  text-transform:uppercase;
}

.alt-link a {
  color: #fff;
}

.alt-link a:hover {
  color: #F4B804;
}

.text-center{
  text-align:center;
}

/*Callout Box*/

.callout {
  max-width: 800px;
  margin: 0 auto;
  padding: 5% 8%;
  background-color: #005BC0;
  border: 1px solid #005BC0;
  border-radius: 5px;
}

/*Form Styles*/

input[type="text"],
