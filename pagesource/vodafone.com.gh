<script>
var currentimage = 0;
var totalimages = 0;
$('.galthumbnails').click(function(){
	var imageid = $(this).attr('id');
	var catid = $(this).attr('name');
	
	currentimage = imageid;
	//alert(imageid);
	$.ajax({
		type:'POST',
		url:'http://www.vodafone.com.gh/getlightboximages.php',
		cache:false,
		data:'imageid='+imageid+'&catid='+catid,
		success:function(data,status){
			//alert(data);
			var arraydata =  $.parseJSON(data);
			$.each(arraydata, function(i, object) {
				var imageparameters = object['params']; // get image parameters(imageurl,alt,height,width)
				var imageidfromdb = object['id'];
				var imagename = object['name'];
				
				var imageparamsarray = $.parseJSON(imageparameters);//convert parameters into a json array
				var imagesrc = imageparamsarray['imageurl'];// get the imageurl parameter value
				
				$('#lightbox ul').append('<li id="gallery_'+imageidfromdb+'" class="bigimages" name="'+i+'"><p class="flowertitle">'+imagename+'</p><hr class="divider"><img src="http://www.vodafone.com.gh//'+imagesrc+'" width="100%" name="'+imageidfromdb+'" /></li>');//insert images into the lightbox div
			});
			console.log(arraydata);
			
			$('#gallery_'+imageid).show();
			$('#overlay').fadeIn();
			$('#positioner').fadeIn();
			totalimages = $('.bigimages').length;
		},
		error:function(data,status){
			alert('error');
		}
	});
});





$('#nextlightboxitem').click(function(){
	//alert(totalimages);
	$('.bigimages').hide();
	
	
	var nextitemnumber = $('#gallery_'+currentimage).next('.bigimages').attr('name');
	if(nextitemnumber==null){
		$('.bigimages:first').fadeIn(1000);
		currentimage = $('.bigimages:first').find('img').attr('name');
	}else{
		$('#gallery_'+currentimage).next('.bigimages').fadeIn(1000);
		currentimage = $('#gallery_'+currentimage).next('.bigimages').find('img').attr('name');
	}
	
});

$('#prevlightboxitem').click(function(){
	//alert(currentimage);
	$('.bigimages').hide();
	var previtemnumber = $('#gallery_'+currentimage).prev('.bigimages').attr('name');
	if(previtemnumber==null){
		$('.bigimages:last').fadeIn(1000);
		currentimage = $('.bigimages:last').find('img').attr('name');
	}else{
		$('#gallery_'+currentimage).prev('.bigimages').fadeIn(1000);
		currentimage = $('#gallery_'+currentimage).prev('.bigimages').find('img').attr('name');
	}
});

$('#closelightbox').click(function(){
	$('#overlay').fadeOut();
	$('#positioner').fadeOut();
	$('#lightbox ul').html('');
});
</script>             
