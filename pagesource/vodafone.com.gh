<script>
  var $ = jQuery; 
	$(function(){
		var totWidth=0;
		var positions = new Array();
	
		$('.banner_passion li ').each(function(i){
			/* Loop through all the slides and store their accumulative widths in totWidth */
			positions[i]= totWidth;
			totWidth += $(this).width();
	
			/* The positions array contains each slide's commulutative offset from the left part of the container */
	
			if(!$(this).width())
			{
				alert("Please, fill in width & height for all your images!");
				return false;
			}
		});
	
		$('.banner_passion').width(totWidth);
	
		/* Change the cotnainer div's width to the exact width of all the slides combined */
	
		$('.slid_passion li a').click(function(){
			/* On a thumbnail click */
			$('.slid_passion li').removeClass('current').addClass('not_current');
			$(this).parent().addClass('current');
	
			var pos = $(this).parent().prevAll('.slid_passion li').length;
	
			$('.banner_passion').stop().animate({marginLeft:-positions[pos]+'px'},312);
			/* Start the sliding animation */
			
			/* Prevent the default action of the link */
			
			return false;		
			
		});
		
		$('#slid_passionprev').click(function(){
	
			/* On a next click */
			
			var pos = $('.slid_passion li.current').prevAll('.slid_passion li').length;
			$('.slid_passion li').removeClass('current').addClass('not_current');
			
			var totalSlides = $('.slid_passion li').prevAll().length;
			
			var nextPos = pos+1;
			
			if(nextPos > totalSlides) { nextPos = 0; }
			
			var activeSlide = nextPos+1;
			
			$('.slid_passion li:nth-child('+activeSlide+')').addClass('current').removeClass('not_current');
			
			$('.banner_passion').stop().animate({marginLeft:-positions[nextPos]+'px'},312);
			/* Start the sliding animation */
			
			/* Prevent the default action of the link */
			
			return false;
			
		});
		
		$('#slid_passionnext').click(function(){
	
			/* On a prev click */
			
			var pos = $('.slid_passion  li.current').prevAll('.slid_passion  li').length;
			$('.slid_passion  li').removeClass('current').addClass('not_current');
			
			var totalSlides = $('.slid_passion  li').prevAll().length;
			
			var prevPos = pos-1;
			
			if(prevPos < 0) { prevPos = totalSlides; }
			
			var activeSlide = prevPos+1;
			
			$('.slid_passion  li:nth-child('+activeSlide+')').addClass('current').removeClass('not_current');
			
			$('.banner_passion').stop().animate({marginLeft:-positions[prevPos]+'px'},312);
			/* Start the sliding animation */
			
			/* Prevent the default action of the link */
			
			return false;
			
		});
		$('.slid_passion li:first').addClass('current').siblings().addClass('not_current');
			// Remove scrollbar in JS
		/* On page load, mark the first thumbnail as active */
	});
    
    

</script> 
