    //Ranges edited
    var d = new Date();
    var curr_date = d.getDate();
    var curr_month = d.getMonth();
    var curr_year = d.getFullYear();

    var ld = new Date().add(-1).month();
    var last_date = ld.getDate();
    var last_month = ld.getMonth();
    var last_year = ld.getFullYear();

    //console.log(d+','+ld);

    $('#datepicker').glDatePicker(
    {

        showAlways: false,

        cssName: 'darkneon',
        allowMonthSelect: false,
        allowYearSelect: false,
        prevArrow: '<',
        nextArrow: '>',
        selectedDate: new Date(curr_year, curr_month, curr_date),
        selectableDateRange: [
            {
                /*
                from: new Date(last_year, last_month, last_date),
                to: new Date(curr_year, curr_month, curr_date)
                */
                from: new Date(curr_year-1, 01, 01),
                to: new Date(curr_year+6, curr_month, curr_date)
            }
        ],
        onClick: function(target, cell, date, data) {
            var thu = convertNameDay(date.getDayName());
            //console.log(thu);

            target.val(thu +' - '+formatDate(date.getDate())+' / ' +formatDate((date.getMonth()+1))+ ' / ' +date.getFullYear());
