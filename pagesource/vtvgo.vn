                    settings: {
                        slidesToShow: 4,
                        slidesToScroll: 4
                    }
                }
            ]
        });

		$("#reload_stream").on('click', function(){
			var id = $(this).data("id");
			var url = "http://vtvgo.vn/"+"ajax-get-stream";
			$.ajax({
				url: url,
				type: 'GET',
				data: {id:id},
				beforeSend: function() {
				},
				success: function(result){
					//console.log(result);
					var res = JSON.parse(result);
					//console.log(res);
					addPlayer(res.stream_url[0], id, '', '', '',2,"{}");
				}
			});
		});


        $(".btn-popupcontent").click(function(){
            check_popup = '0';
            if(check_popup == 1){
                $(".popup-content").css({"top": 0});
            }else{
                alert('Hiện tại chưa có nội dung tương tác');
            }

        });

        check_popup = '0';
        check_prepare_popup = '0';


        function check_show_popup(){
            console.log(check_popup + "__"+ check_prepare_popup);
            if(check_popup == 0 && check_prepare_popup == 1) {
                var url = "http://vtvgo.vn/" + "ajax-get-check-show-popup";
                $.ajax({
                    url: url,
                    type: 'GET',
                    data: {id: '-1'},
                    beforeSend: function () {
                    },
                    success: function (result) {
                        var res = JSON.parse(result);

                        if (res.is_popup == 1) {
                            $(".show-popup").css({"display":"block"})
                            $(".popup-content").css({"top": 0});
                            check_popup = 1;
                        }
                    }
                });
            }
        }

        if(check_popup == 0 && check_prepare_popup == 1) {
            setInterval(check_show_popup, 10000);
        }

        $(".shareFacebookClick").click(function(){
            var id = $("li.active").data('id');
            var url = 'http://vtvgo.vn/ts/'+id;
            window.open('http://www.facebook.com/sharer/sharer.php?u=' + url, 'facebook_share', 'height=320, width=640, toolbar=no, menubar=no, scrollbars=no, resizable=no, location=no, directories=no, status=no')
