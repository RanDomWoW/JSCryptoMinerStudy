<script>
    $(document).on('ready', function(){
		$(".open-link").click(function(){
			window.location.href = $(this).data("link");
		});
        $(".slide-post-news").mouseover(function(){
            $(".slide-news .swiper-button-next").css("opacity", "1");
        });
        $(".slide-post-news").mouseout(function(){
            $(".slide-news .swiper-button-next").css("opacity", "0");
        });


        var BaseUrl = $("#base_url").val();
        var slidesPerView = 4;
        var slidesPerGroup = 4;
        if (ww>=1000)
        {
            slidesPerView = 4;
            slidesPerGroup = 4;
        }

        if (ww>468 && ww<=1000)
        {
            slidesPerView = 3;
            slidesPerGroup = 3;
        }
        if (ww<=468)
        {
            slidesPerView = 1;
            slidesPerGroup = 1;
        }

        // Scroll and load more multiple slider
        $('div').find('.general-slider').each(function(){
            var channel_id = this.id;
            //console.log("-------",channel_id);
            //Tam thoi copy 3 function
            var swiper = new Swiper('.slider-'+channel_id, {
                slidesPerView: slidesPerView,
                spaceBetween: 2,
                slidesPerGroup: slidesPerGroup,
                loop: false,
                nextButton: '.swiper-button-next'+channel_id,
                prevButton: '.swiper-button-prev'+channel_id,

                onReachEnd: function(){
                    var current_page = $("#current_page_of_slider_"+channel_id).val();
                    $.ajax({
                        //url: url,
                        url: BaseUrl+'ajax-get-more-item-playlist',
                        data: ({next_page: parseInt(current_page) + 1, channel_id: channel_id }),
                        type: 'GET',
                        beforeSend: function() {
                            $( ".slider-"+channel_id ).append( '<div class="black-bg"><div class="txtloading-sl">Loading...</div></div>' );
                        },
                        success: function(result){
                            //console.log(result);
                            $(".swiper-button-next").css("opacity", 1);
                            swiper.appendSlide(result);

                            $("#current_page_of_slider_" + channel_id).val(parseInt(current_page) + 1);
                            $(".black-bg").remove();

                            // Not link (not click items) when end items
                            if (result === undefined || result === null){
                                $("#block_slider_"+channel_id).find(".swiper-button-disabled").removeClass("swiper-button-disabled");
                            }
                            $(".swiper-button-next").removeClass("swiper-button-disabled");

                        }
                    });
                }
            });
        });
    });
</script>
