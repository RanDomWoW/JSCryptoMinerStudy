            var dl =  date.getFullYear()+ '-' +(date.getMonth()+1)+ '-' +date.getDate();
            loadEpg(dl);
        }
    });

    //Scroll to pos
    var toPos = $('.isBroadCast').offset().top;
    $('.box-epg').animate({scrollTop:toPos-350},1500);

