<script>
    $('aside .sidebar').ready(function() {
        var expires = new Date(new Date().getTime() + 365 * 86400000); // 31 days

        if ($.getCookie('xbSidebarCookie') == "1") {
            $("html").addClass("xbHideSidebar");
        }

        $(".sidebarCollapse").click(function() {
            if ($.getCookie('xbSidebarCookie') == "0") {
                // Set cookie value to yes
                $.setCookie('xbSidebarCookie', '1', expires);
                // Add the class to the body
                $("html").addClass("xbHideSidebar");
            }
            // If the cookie was already set to yes then remove it
            else {
                $.setCookie('xbSidebarCookie', '0', expires);
                $("html").removeClass("xbHideSidebar");
            }
        });
    });
</script>


