    <script>
        (function () {
            var banner = document.getElementById('banner');
            var ban_img = document.getElementById('ban_img');

            var bannerData  = [
                {
                    imageUrl: "/static/images/2_new.gif",
                    domain: ''
                },
                {
                    imageUrl: "/static/images/5.gif",
                    domain: 'https://www.youtube.com/channel/UCEd7HM3OpxRWim5gTkS9t4A'
                },
                {
                    imageUrl: "/static/images/4.gif",
                    domain: 'https://www.youtube.com/watch?v=H8DCW8dw2do'
                }
            ];

            var n = getCookie("n");

            var date = new Date(new Date().getTime() +7 * 60 * 60 * 1000);

            if (!n) {
                document.cookie = "n=0; path=/; expires=" + date.toUTCString();
                n = getCookie("n");
            }

            if (n == 0) {
                document.cookie = "n=1; path=/; expires=" + date.toUTCString();
            } else if (n == 1){
                document.cookie = "n=2; path=/; expires=" + date.toUTCString();
            } else if (n == 2){
                document.cookie = "n=0; path=/; expires=" + date.toUTCString();
            }

            function getCookie(name) {
                var cookie = " " + document.cookie;
                var search = " " + name + "=";
                var setStr = null;
                var offset = 0;
                var end = 0;
                if (cookie.length > 0) {
                    offset = cookie.indexOf(search);
                    if (offset != -1) {
                        offset += search.length;
                        end = cookie.indexOf(";", offset)
                        if (end == -1) {
                            end = cookie.length;
                        }
                        setStr = unescape(cookie.substring(offset, end));
                    }
                }
                return(setStr);
            }

            ban_img.style.backgroundImage = 'url(' + bannerData[n].imageUrl + ')';
            ban_img.setAttribute('href', bannerData[n].domain);

            if(parseInt(n) === 0){
                ban_img.removeAttribute('target');
                ban_img.removeAttribute('href');
            }

            window.onresize = function () {
                if (window.innerWidth < 1024){
                    banner.style.display = 'none'
                } else {
                    banner.style.display = 'block'
                }
            }
        })();
    </script>
