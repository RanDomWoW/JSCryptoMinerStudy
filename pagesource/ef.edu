<script type="text/javascript">
    var leadinfo = true;
    window.Minerva = {};
    window.Minerva.events = (function () {
        var topics = {};
        var hOP = topics.hasOwnProperty;

        return {
            subscribe: function (topic, listener) {
                console.log('in subscribe topic:', topic);
                // Create the topic's object if not yet created
                if (!hOP.call(topics, topic)) topics[topic] = [];

                // Add the listener to queue
                var index = topics[topic].push(listener) - 1;

                // Provide handle back for removal of topic
                return {
                    remove: function () {
                        delete topics[topic][index];
                    }
                };
            },
            publish: function (topic, info) {
                console.log('in publish', topic);
                // If the topic doesn't exist, or there's no listeners in queue, just leave
                if (!hOP.call(topics, topic)) return;

                // Cycle through topics queue, fire!
                topics[topic].forEach(function (item) {
                    item(info !== undefined ? info : {});
                });
            }
        };
    })();


    var  GetCookieValue = function(cookieName) {
        var cookieArray = document.cookie.split(';');
        for (var index = 0; index < cookieArray.length ; index++) {
            var cookie = cookieArray[index];
            while (cookie.charAt(0) === ' ') {
                cookie = cookie.substring(1, cookie.length);
                if (cookie.indexOf(cookieName) === 0) {
                    return cookie.substring(cookieName.length + 1, cookie.length);
                }
            }
        }
        return '';
    }
    function SetSessionCookie(c_name, value) {
        var session_expiry_date = new Date();
        session_expiry_date.setTime(session_expiry_date.getTime() + (30 * 60 * 1000));
        var c_value = value + ";path=/; expires=" + session_expiry_date.toUTCString();
        document.cookie = c_name + "=" + c_value;
    }

   

    var originalEntryUrlstr = 'OriginalEntryUrl';
    var originalReferringUtlStr = 'OriginalReferringURl';
    var errorPageName = 'errorPageName';
    var originalEntryUrl = GetCookieValue(originalEntryUrlstr);
