      			<div id="enviarAlias" class="button_verde">Confirmar</div>
      			
      			<label id="response" style="display: block; margin-top: 20px;"></label>
      		</div>
      	</div>
      
      	<div id="setearCodigo" style="display:none">
      
      		<img id="login_cruz" src="/img/cruz.png">
      		<img id="sus_loading" src="/img/loading.gif">
      		
      		<label id="login_texto">Confirmación</label>
      
      		<div class="login_title">
      			<label>Ingrese a continuacion el código recibido por SMS.</label>
      			<label class="setearCodigo_celular">
      			</label>
      		</div>
      
      		<div id="login_pad">
      
      			<form id="formConfirmarCodigo">
      				<div>
      					<input type="hidden" id="token" value="">
      					<input type="hidden" id="celular" value="">
      					<input type="text" id="codigo" placeholder="Código">
      				</div>
      			</form>
      
      			<div id="enviarCodigo" class="button_verde">Confirmar</div>
      			<div id="reenviarSms_codigo">Reenviar SMS</div>
      			
      			<label id="response" style="display: block; margin-top: 20px;"></label>
      		</div>
      	</div>
      
      	<script type="text/javascript">
      		$(document).ready(function() {
      			$("#modalLogin form").submit(function(e) {
      				e.preventDefault();
      				e.stopPropagation();
      			})
      
      			$("#modalLogin input").keydown(function(event){
      			    if(event.keyCode == 13) {
      			      event.preventDefault();
      			      return false;
      			    }
      			  });
      
      			$("#suscribir").click(function(event) {
      				$("#sus_loading,#opacityBody,#modalLogin").hide();
      	           	window.location.replace('/suscripcion');
      				/*render('/suscripcion');*/
      			});
      
      			$("#login_recuperar").click(function(event) {
      				if ($("#name").val() == '')
      					return $("#response").text("Debe ingresar un e-mail válido").css('color','red');
      				$("#sus_loading").show();
      				$("#login #response").text(" ");
      			    $.ajax({
      			        url: "/suscripcion/recuperar/",
      			        type: 'POST',
      			        data: { mail: $("#name").val() }
      			    }).done(function(res) {
      			    	$("#login #response").text("Se ha enviado correctamente un mail a " + $("#name").val() + " para cambio de password.").css('color','green')
      			    	$("#login_recuperar").hide();
      			    	$("#sus_loading").hide();
      			    }).error(function(res) {
      			    	if (res.status == 409)
      			    		$("#login #response").html("El mail " + $("#name").val() + " no esta registrado.");
      			    	else
      			    		$("#login #response").text("Ha ocurrido un error al enviar el mail a " + $("#name").val() + ". Intentelo nuevamente más tarde.")
      			    	$("#login #response").css('color','red')
      			    	$("#sus_loading").hide();
      			    });
      			});
      
      			$("#name").keypress(function(event) {
      		        if (event.which == 13)
      		          $("#pass").focus();
      	    	});
      
      			$("#pass").keypress(function(event) {
      		        if (event.which == 13)
      		          $("#ingresar").click();
      	    	});
      
      	    	var doingAction = false;
      
      			$("#ingresar").click(function(event) {
      				if (doingAction)
      					return;
      				doingAction = true;
      				if ($("#name").val() == '')
      					return $("#login #response").text("Debe ingresar un e-mail válido").css('color','red');
      				if ($("#pass").val() == '')
      					return $("#login #response").text("Debe ingresar un password válido").css('color','red');
      				$("#suscribir").attr('disabled','disabled');
      				$("#recuperar").attr('disabled','disabled');
      				$("#ingresar").attr('disabled','disabled');
      				$("#token").attr('disabled','disabled');
      				$("#sus_loading").show();
      				//$(".loginIniciar").hide();
      				$("#login #response").text(" ");
      
      			    $.ajax({
      			        url: "/suscripcion/login",
      			        data: { mail: $("#name").val().toLowerCase(), pass: $("#pass").val() },
      			        type: 'POST',
      			    }).done(function(res) {
      			    	doingAction = false;
      			    	if (!res.userId || !res.userToken || !res.userName || !res.userLastName)
      			    		$("#login #response").text("Ha ocurrido un error, intentelo nuevamente más tarde.")
      			    	else
      						doLogin(res.userId, res.userToken, res.userName, res.userLastName, res.userAvatar);
      					$("#login #response").css('color','red')
      			    	$("#sus_loading").hide();
      			    }).error(function(res) {
      
      			    	doingAction = false;
      			    	$("#login #response").css('color','red')
      			    	$("#sus_loading").hide();
      
      			    	if (res.status == 412) {
      			    		var token = res.responseJSON.token;
      						$("#formConfirmarAlias #token").val(token);
      			    		$("#login").hide();
      			    		$("#setearAlias").show();
              				$("#modalLogin").height(410);
      			    		return;
      			    	}
      
      		    		if (res.status == 410) {
      		    			$("#login #response").text("Usuario o contraseña inválida.")
      		    			return;
      		    		}
      
      			    	if (res.status == 409){
      			    		$("#response").html("Estamos esperando que valide su correo, revise su casilla de email "+res.responseJSON.mail+". Si aún no lo recibió verifique su correo no deseado, o haga <a class='validarMail'><b>click aquí para reenviarlo</b></a>");
      			    		return;
      			    	}
      
      	    			if (res.status == 413){
      	    				$("#recibirSms #celular").val(res.responseJSON.celular);
      			    		$("#recibirSms #token, #setearCodigo #token").val(res.responseJSON.token);
      
      			    		if (res.responseJSON.celular)
      			    			$(".setearCodigo_celular").text("El código fue enviado al número: " + res.responseJSON.celular);
      			    		else
      			    			$(".setearCodigo_celular").text("");
      
      			    		$("#login").hide();
      			    		$("#setearCodigo").show();
              				$("#modalLogin").height(350);
      			    		return;
      	    			}
      			    	
      			    	$("#login #response").text("Ha ocurrido un error, intentelo nuevamente más tarde.")
      			    });
      
      			});
      
      			$("#ingresar_codigo").click(function(event) {
      				$("#login, #recibirSms, #setearAlias").hide();
      				$("#setearCodigo").show();
      				$("#modalLogin").height(350);
      				$("#formConfirmarCodigo #token").val($("#recibirSms #token").val());
      			});
      
      			$("#reenviarSms_codigo").click(function(event) {
      				$("#login, #setearCodigo, #setearAlias").hide();
      				$("#recibirSms").show();
      				$("#modalLogin").height(410);
      				$("#recibirSms #token").val($("#formConfirmarCodigo #token").val());
      			});
      
      			$("#enviarCodigo").click(function(event) {
      				var data = { userToken: $("#formConfirmarCodigo #token").val(), code: $("#formConfirmarCodigo #codigo").val() }
      
      				if (!data.code)
      					return $("#setearCodigo #response").text("Debe ingresar un código.").css('color','red');
      
      				if (!data.userToken)
      					return $("#setearCodigo #response").text("Ocurrió un error.").css('color','red');
      
      				$("#setearCodigo #response").text("");
      				$("#sus_loading").show();
      				$.ajax({
      		            type: "POST",
      		            url: "/suscripcion/validar-codigo",
      		            data: data,
      		        }).done(function(res){
      			    	if (!res.userId || !res.userToken || !res.userName || !res.userLastName) {
      			    		$("#setearCodigo #response").text("Ha ocurrido un error, intentelo nuevamente más tarde.").css("color", "red")
      			    	} else {
      						doLogin(res.userId, res.userToken, res.userName, res.userLastName, res.userAvatar);
      			        	$("#setearCodigo #response").text("Código correcto.").css('color','green');
      			        	setTimeout(function(){$("#setearCodigo").hide();}, 3000);
      			        	$("#enviarCodigo").remove();
      			    	}
      		        }).error(function(err) {
      		        	$("#setearCodigo #response").text("Código incorrecto.").css("color", "red");
      		        }).always(function() {
      		            $("#sus_loading").hide();
      		        });
      			});
      
      			$("#reenviarSms").click(function(event) {
      				if ($("#formConfirmarCelular #celular").val() == '')
      					return $("#recibirSms #response").text("Debe ingresar un numero válido").css('color','red');
      				
      				$("#recibirSms #sus_loading").show();
      				$(".loginIniciar").hide();
      				$("#reenviarSms, #ingresar_codigo").hide();
      
      				var data = { celular: $("#formConfirmarCelular #celular").val(), token: $("#recibirSms #token").val() };
      				
      				$("#recibirSms #response").text(" ");
      			    $.ajax({
      			        url: "/suscripcion/reenviar-sms/",
      			        data: data,
      			        type: 'POST',
      			    }).done(function(res) {
      					$("#recibirSms #sus_loading").hide();
      					$("#recibirSms").hide();
      					$("#setearCodigo, #ingresar_codigo").show();
      					$("#formConfirmarCodigo #token").val(data.token);
      					$("#formConfirmarCodigo #celular").val(data.celular);
      					$("#reenviarSms").remove();
      				}).error(function(err){
      					$("#reenviarSms, #ingresar_codigo").show();
      					$("#recibirSms #sus_loading").hide();
      					if(err.status == 406)
      						$("#recibirSms #response").text("Usted ha agotado la cantidad de envios SMS permitidos, lo invitamos a registrase nuevamente, gracias.").css('color','red');
      					else if (err.status == 412)
      		    			$("#recibirSms #response").text("El teléfono celular ya se encuentra registrado.").css('color','red');
      		    		else if (err.status == 440)
      		    			$("#recibirSms #response").text("El teléfono celular ingresado no es válido u ocurrió un error al enviar el SMS.").css('color','red');
      		    		else
      						$("#recibirSms #response").text("Ha ocurrido un error, intentelo nuevamente más tarde.").css('color','red');
      				});
      			});
      
      			$("#enviarAlias").click(function(event) {
      				if ($("#formConfirmarAlias #alias").val() == '')
      					return $("#setearAlias #response").text("Debe ingresar un numero válido").css('color','red');
      				
      				$("#sus_loading").show();
      				$(".loginIniciar").hide();
      				$("#setearAlias #response").text(" ");
      
      				var datos = {
      					token: $("#formConfirmarAlias #token").val(),
      					client : {
      						username: $("#formConfirmarAlias #alias").val()
      					}
      				};
      
      			    $.ajax({
      			        url: "/suscripcion/actualizar-usr",
      			        data: datos,
      			        type: 'POST',
      		        }).done(function(res){
      		        	$("#sus_loading").hide();
      		        	$("#setearAlias").hide();
      			    	if (!res.userId || !res.userToken || !res.userName || !res.userLastName)
      			    		$("#setearAlias #response").text("Ha ocurrido un error, intentelo nuevamente más tarde.")
      			    	else
      						doLogin(res.userId, res.userToken, res.userName, res.userLastName, res.userAvatar);
      					$("#enviarAlias").remove();
      		        }).error(function(err) {
      		        	$("#sus_loading").hide();
      		        	if(err.status == 413) {
      			    		$("#setearCodigo #token").val(datos.token);
      			    		$("#setearAlias").hide();
      
      			    		if (datos.celular)
      			    			$(".setearCodigo_celular").text("El código fue enviado al número: " + datos.celular);
      			    		else
      			    			$(".setearCodigo_celular").text("");
      
      			    		$("#setearCodigo").show();
              				$("#modalLogin").height(350);
      		        	} else if (err.status == 411)
      		    			$("#setearAlias #response").text("El alias ingresado ya está registrado.").css("color","red");
      		    		else
      		        		$("#setearAlias #response").text("Hubo un error, por favor intente mas tarde.").css("color","red");
      		        });
      			});
      
      			var reenviado = false;
      
      			$(document).on('click', '.validarMail', function(event) {
      				if (reenviado)
      					return;
      
      				$("#sus_loading").show();
      				$(".loginIniciar").hide();
      				
      				$("#login #response").text("");
      			    $.ajax({
      			        url: "/suscripcion/reenviar-mail",
      			        data: { mail: $("#formLogin #name").val() },
      			        type: 'POST',
      			    }).done(function(res) {
      			    	$("#login #response").text("Se ha enviado un mail de confirmación a " + $("#name").val() + ". Por favor, revise su casilla de correo y siga las instrucciones").css('color','green');
      			    	$("#sus_loading").hide();
      					reenviado = true;
      			    }).error(function(res) {
      			    	if (res.status == 409)
      			    		$("#login #response").html("El mail " + $("#name").val() + " no esta registrado.");
      			    	else
      			    		$("#login #response").text("Ha ocurrido un error al enviar el mail a " + $("#name").val() + ". Intentelo nuevamente más tarde.")
      			    	$("#login #response").css('color','red')
      			    	$("#sus_loading").hide();
      			    });
      			});
      
      		});
      	
      	</script>
      
      </div>      <div id="modalCropImage">
      
      	<img id="login_cruz" src="/img/cruz.png">
      	<img id="sus_loading" src="/img/loading.gif">
      
      	<div class="image-editor">
            <input type="file" class="cropit-image-input">
            <input type="file" class="crop" style="display:none">
            <div class="cropit-preview"></div>
            <div class="image-size-label">
              Redimensionar
            </div>
            <input type="range" class="cropit-image-zoom-input">
            <button class="rotate-ccw">Girar a la izquierda</button>
            <button class="rotate-cw">Girar a la derecha</button>
      
            <button class="export saveProfilePic">Guardar</button>
      
          </div>
      
          <script>
      
         $(document).ready(function() {
      
          $(function() {
              $('.image-editor').cropit({
                type: 'image/jpeg',
                quality: .9,
                smallImage: 'allow',
                originalSize: false
              });
      
              $('.rotate-cw').click(function() {
                $('.image-editor').cropit('rotateCW');
              });
              $('.rotate-ccw').click(function() {
                $('.image-editor').cropit('rotateCCW');
              });
      
              $('.export').click(function() {
      
                //oculto modal crop
                $("#opacityBody").finish().fadeOut('400');
                $("#modalCropImage").finish().fadeOut('400', function() {
                  $("#opacityBody").finish().hide();
                });
      
                //show cargando
                $("#opFadeIn").finish().fadeIn('400', function() {
                  $("#opacityBody").finish().show();
                });
                $("#opacityBody").finish().fadeIn('400');
      
                var imageData = $('.image-editor').cropit('export');
      
                uploadImageProfile(imageData, function(err, url) {
                  console.log(err, "Error")
                  
                  $("#opacityBody").finish().fadeOut('400');
                  $("#modalCropImage").finish().fadeOut('400', function() {
                    $("#opacityBody").finish().hide();
                  });
      
                  $('#imgInput').attr('src', 'https://px.fotos.cdn.0223.com.ar/f/'+url);
                  $('#profile_pic').val('/'+url);
      
                });
                
              });
            });
          });
          </script>	
      
      </div>

    </div>
    <link href='https://fonts.googleapis.com/css?family=Lato|Roboto+Slab:400,700|Roboto:300,400,700|Open+Sans' rel='stylesheet' type='text/css'>
