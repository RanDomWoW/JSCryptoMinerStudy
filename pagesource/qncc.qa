  transition: all 5s ease-in-out;
}

.popup .close {
  position: static;
  top: 20px;
  right: 30px;
  transition: all 200ms;
  font-size: 30px;
  font-weight: bold;
  text-decoration: none;
  color: #00757b;
}
.popup .close:hover {
  color: black;
}
.popup .content {
  max-height: 90%;
  overflow: auto;
}
    
</style>
                    	
                    	
