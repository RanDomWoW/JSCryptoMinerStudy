function getgeo(){
	if(navigator.geolocation) {
		var options = {
			// 位置情報を毎回再取得する
			'maximumAge': 0,
			// 12秒以内に取得できなかったらエラー
			'timeout': 12000,
			// GPSを利用(Android)
			'enableHighAccuracy': true
		};

		// Androidの一部で正常に取得出来ないため、オプションを消去
		if(lowerAndroid(4.0)) {
			options = {};
		}

		navigator.geolocation.getCurrentPosition(function(position) {
			var now = new Date();
			now.setTime( now.getTime() - 1000 * 3600 * 5 );
			var expire = new Date();
			expire.setTime( expire.getTime() + 1000 * 3600 * 24 );
			document.cookie = 'today_reserve='+now.getTime().toString()+'; expires=' + expire.toUTCString() + '; path=/';

			location.href="/hotel/near/?lat=" + position.coords.latitude + "&lng=" + position.coords.longitude;
		}
		, function(error) {
			// 位置情報取得に失敗した場合の処理
			switch(error.code){
				case 1:alert("位置情報の取得が許可されていません。\n\n端末の設定で位置情報の取得を許可してご利用下さい。");break;
				case 2:alert("位置情報が取得できませんでした。\n\n端末の設定で位置情報の取得を許可してご利用下さい。");break;
				case 3:alert("時間内に位置情報が取得できませんでした。\n場所を変えてお試しいただくか、お手数ですが、他の検索メニューからご利用をお願いいたします。\n\n(※何度もこのエラーが起こる場合、端末の再起動で改善することがあります。)");break;
				default:alert("予期せぬエラーが発生しました。");break;
			}
		}, options);
	} else {
		alert("ご利用のブラウザでは、位置情報を利用できません。\n\nこの機能は標準のブラウザでご利用下さい。");
	}
}
var isAndroid = false;
function lowerAndroid(n) {
    var bo = false;
    var ua = navigator.userAgent.toLowerCase();
    var version = ua.substr(ua.indexOf('android')+8, 3);
    if(ua.indexOf("android")) if(parseFloat(version) < n) bo = true;
    return bo;
}

function is_sp() {
	var ua = navigator.userAgent;
	if(ua.indexOf('iPhone') != -1 || ua.indexOf('iPod') != -1 || (ua.indexOf('Android') != -1 && ua.indexOf('Mobile') != -1)){
		return true;
	}
		return false;
}

function today_reserve(){
	var now = new Date();
	now.setTime( now.getTime() - 1000 * 3600 * 5 );
	var expire = new Date();
	expire.setTime( expire.getTime() + 1000 * 3600 / 2 );

	document.cookie = 'today_reserve='+now.getTime().toString()+'; expires=' + expire.toUTCString() + '; path=/';

	var date = new Date();
	if (date.getHours() < 5) {
		location.href = '/late/';
	} else {
		if (is_sp()) {
			location.href = '/list.html';
		}
	}

}
