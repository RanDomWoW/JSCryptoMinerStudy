		$('.text_helper').css('top', ($('#form_reserve input[name=search_text]').offset().top + $('#form_reserve input[name=search_text]').outerHeight()) + 'px');
		$('.text_helper').css('left', $('#form_reserve input[name=search_text]').offset().left - $('#wrap').offset().left + 'px');
		$('.text_helper').show();
	});
	$('.text_helper, #form_reserve input[name=search_text]').on('click', function(e) {
		e.stopPropagation();
	});

	$('body, .text_helper .close').on('click', function() {
		$('.text_helper').hide();
	});

	$('.text_helper .inner a').on('click', function() {
		$('#form_reserve input[name=search_text]').val($('input[name=search_text]').val() + $(this).html() + ' ');
	});
});

function set_date_ui(no_select) {
	no_select = no_select || $('input[name=date_no_select]');
	if (no_select.prop('checked')) {
		$('input[name=select_date]').attr('disabled', true);
		$('select[name=select_stay]').val('1').attr('disabled', true);
		$('span.disable_detector_clicker').show();
	} else {
		$('input[name=select_date]').attr('disabled', false);
		$('select[name=select_stay]').attr('disabled', false);
		$('span.disable_detector_clicker').hide();
	}

}
