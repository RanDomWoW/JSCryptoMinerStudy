		$('#form_reserve_area_sp .calendar_box').hide();
		$('.hotel_criteria').hide();
		$('#form_reserve_area_sp').attr('action', '/jal_dp/');
		$('#form_reserve_area_sp').attr('method', 'get');
		$('#form_reserve_area_sp').attr('target', '_blank');
	});

	$('#search_pref_area_sp').change(function(){
		if ($('#areaholder').length == 0) {
			$('body').append('<div id="areaholder" style="display:none;"></div>');
			$('#hotelno option').clone().appendTo('#areaholder');
		}
		$('#hotelno_area_sp option').remove();
		$('#areaholder option').each(function() {
			if ($(this).val() == '') return;

			var search_pref = $('#search_pref_area_sp').val();
			if (hotels[$(this).val()][3] == search_pref || ! search_pref) {
				$(this).clone().appendTo('#hotelno_area_sp');
			}
		});
		set_hotel_area_sp();
	});

	$("#form_reserve_area_sp input[type='radio']").change(function(){
		if($(this).is(":checked")){
			$('#form_reserve_area_sp .crobd01').removeClass("c_on");
			$('#form_reserve_area_sp .crobd02').removeClass("c_on");
			$('#form_reserve_area_sp .crobd03').removeClass("c_on");
			$(this).parent().addClass("c_on");
		}else{
			$(this).parent().removeClass("c_on");
		}
	});

	$('#hotelno_area_sp').change(function(){
		set_hotel_area_sp();
	})

	function set_hotel_area_sp() {
		hotelno_area_sp = $('#hotelno_area_sp').val();
		if (hotelno_area_sp && hotels[hotelno_area_sp][1])
		{
			$('#hc_area_sp').val(hotels[hotelno_area_sp][1]);
			$('#reserve_type_airticket_area_sp').attr('disabled', false);
		}
		else
		{
			$('#hc_area_sp').val(null);
			$('#reserve_type_airticket_area_sp').attr('disabled', true);
		}
	}

	$('#form_reserve_area_sp span.disable_detector_clicker').on('click', function(e) {
		if ($('#date_no_select_area_sp').prop('checked')) {
			$('input[name=select_date]').attr('disabled', false);
			$('input[name=date_no_select]').removeAttr('checked');
			$('select[name=select_stay]').val('1').attr('disabled', false);
			$(this).hide();
			$("#select_date_side_area_sp").datepicker('show', $("#select_date_side_area_sp"));
		}
	});

});
