<script type="text/javascript">

    if ("geolocation" in navigator) {
        window.SuggestedStoresPostback = window.SuggestedStoresPostback || function () { }; // no-op

        navigator.geolocation.getCurrentPosition(
            function success(results) {
                // Create location object
                jQuery('#hdLatitude').val(results.coords.latitude);
                jQuery('#hdLongitude').val(results.coords.longitude);

                // Post back update panel
                window.SuggestedStoresPostback();
            },
            function failure(err) {
                // Could not detect location
            },
            {
                timeout: 15000,
                enableHighAccuracy: false,
                maximumAge: 1200000
            }
        );

    } else {
        // Location API is not available
    };


    window.StartOrder = function () {
        // Update text and disable button after click
		jQuery('#btn-start-order').html('Loading...');
		jQuery('#btn-start-order').prop('disabled',true);
		
		var province = jQuery('#select-province option').filter(':selected').val();
        var address = jQuery('#street-address').val();
        address = address == "" ? "null" : address;
        var orderType = jQuery('#order-type input').filter(':checked').val();
        var url = 'https://order.debonairspizza.co.za';
        url += '/restaurants/address/' + encodeURIComponent(address.trim()) + "/" + encodeURIComponent(orderType.trim()) + '/' + encodeURIComponent(province.trim());
        window.location = url;
    };

    window.InitialiseRestaurantSelector = function () {
        $('#order-type label').click(function () {
            var value = $(this).find('input').val();
            $('.restaurant-link').each(function() {
                var link = $(this);
                link.attr('href', link.attr('href').replace(/ordertype=[^\&\?]+/gi, "orderType=" + value));
            });
        });
    }

</script>


