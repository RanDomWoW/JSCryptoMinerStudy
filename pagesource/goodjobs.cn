        } else {
            $("#youcity").removeClass('hide').addClass('show');
        }
        return false;
    });
    $(".cityJob").bind('mouseover', function(){
        $("#youcity").removeClass('middleCitySelect').addClass('topCitySelect');
        $("#youcity").removeClass('hide').addClass('show');
        
    });
    
