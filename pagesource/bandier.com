<script>
    $j('.bag-item-qty-action').on('click', function (event) {
        event.preventDefault();
        var $qtyButton = $j(this);
        var $qtySelect = $qtyButton.closest('.bag-item-qty');
        var $qtyField = $qtySelect.find('.bag-item-qty-field');
        var qtyValue = parseInt($qtyField.val());
        var qtyAction = $qtyButton.data('action');

        if (qtyAction === 'increase') {
            if (qtyValue == 20) {
                qtyValue += 0
            } else {
                qtyValue += 1;
            }

            if (isNaN(qtyValue)) {
                qtyValue = 1;
            }
        }

        if (qtyAction === 'decrease') {
            if (qtyValue == 1) {
                qtyValue = 1
            } else {
                qtyValue -= 1;
            }

            if (isNaN(qtyValue)) {
                return false
            }

        }

        $qtyField.val(qtyValue);
        updateQty();
        bindCartShow();
    });
    $j('#cart_mini_popup .bag-toggle-mini').on('click', function (event) {
        alert("toggle");
        event.preventDefault();
        $j('body').toggleClass('page-bag-open');
    });
</script>

