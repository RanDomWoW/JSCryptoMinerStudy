<script type="text/javascript">
    var newsletterSubscriberFormFooter = new VarienForm('newsletter-subscribe-footer');

    Validation.add('validate-email','Please enter a valid email address', function (v) {
        //return Validation.get('IsEmpty').test(v) || /\w{1,}[@][\w\-]{1,}([.]([\w\-]{1,})){1,3}$/.test(v)
        //return Validation.get('IsEmpty').test(v) || /^[\!\#$%\*/?|\^\{\}`~&\'\+\-=_a-z0-9][\!\#$%\*/?|\^\{\}`~&\'\+\-=_a-z0-9\.]{1,30}[\!\#$%\*/?|\^\{\}`~&\'\+\-=_a-z0-9]@([a-z0-9_-]{1,30}\.){1,5}[a-z]{2,4}$/i.test(v)
        return Validation.get('IsEmpty').test(v) || /^([a-z0-9,!\#\$%&'\*\+\/=\?\^_`\{\|\}~-]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z0-9,!\#\$%&'\*\+\/=\?\^_`\{\|\}~-]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*@([a-z0-9-]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z0-9-]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*\.(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]){2,})$/i.test(v)
    });

    function sendSubscriberFooter() {
        if (newsletterSubscriberFormFooter.validator.validate()) {
            jQuery.ajax({
                type: 'post',
                url: jQuery('#newsletter-subscribe-footer').attr('action'),
                data: jQuery('#newsletter-subscribe-footer').serialize(),
                dataType: 'json',
                success: function(data){
                    if (!data['success'] && data['message']) {
                        $j('.footer-inner .subscribe').addClass('subscribe-error');
                        $j('#email-subscribe-footer').val(data['message']);
                        $j('#email-subscribe-footer').focus(function(){
                            $j('#email-subscribe-footer').val('');
                        });
                    } else {
                        $j('#email-subscribe-footer').val('Thank you for your subscription');
                        if (typeof fbq == 'function') {
                            fbq('track', 'CompleteRegistration');
                        }
                        if (typeof ga == 'function') {
                            ga('send','event','email sign up','click','Footer');
                        }
                    }
                }
            });
        }
        if($j('#advice-validate-email-email-subscribe-footer').length > 0 ){
            $j('#email-subscribe-footer').val('');
            $j('#email-subscribe-footer').attr('placeholder',$j('#advice-validate-email-email-subscribe-footer').text());
            $j('.footer-inner .subscribe').addClass('subscribe-error');
        }
        if($j('#advice-required-entry-email-subscribe-footer').length > 0 ){
            $j('#email-subscribe-footer').val('');
            $j('#email-subscribe-footer').attr('placeholder','Please enter a valid email address');
            $j('.footer-inner .subscribe').addClass('subscribe-error');
        }
    }
    $j(document).ready(function(){

        if (store.enabled) {
            if (store.get('redeem') != 0) {
                $j('#ntf_ribbon_container').show();
            }
            else {
                $j('#ntf_ribbon_container').hide();
            }
        }
        else {
            $j('#ntf_ribbon_container').hide();
        }

        if(!isMobile) {
            CoverPop.start();
            $j('.CoverPop-close').on('click', function(event) {
                event.preventDefault();
                //$j('#CoverPop-cover').addClass('hide');
                CoverPop.close();
                if (store.enabled) {
                    if(store.get('redeem') != 0){
                        $j('#ntf_ribbon_container').show();
                    }
                }
            });
            $j('#ntf_ribbon_container .callout-btn').on('click', function(event) {
                event.preventDefault();
                CoverPop.open();
                //$j('#CoverPop-cover').removeClass('hide');
            });

            $j('#ntf_ribbon_container .callout-btn-close').on('click',function(event){
                event.preventDefault();
                $j('#ntf_ribbon_container').hide();
            });
        }
        if ($j(window).width() > 800) {
            $j('.line-through').show();
            $j('.line-mobile').hide();
        }
        else {
            $j('.line-through').hide();
            $j('.line-mobile').show();
        }
    });
    $j(window).resize(function() {
        if ($j(window).width() > 800) {
            $j('.line-through').show();
            $j('.line-mobile').hide();
        }
        else {
            $j('.line-through').hide();
            $j('.line-mobile').show();
        }
    });
</script>
        

