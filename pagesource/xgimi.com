<script type="text/javascript">
    var stimer=0, xx = 0, uTimer=0;
    function showProductList(t, isOnProduct){
        xx = t;
        if(t==1 || t==2 || t==3 || t==4 || t==5){
            $(".nav-menu").addClass('bg-menu');
            $(".nav-menu-list").hide();
            $(".nav-menu-list"+t).show();

            if(stimer){
                window.clearTimeout(stimer);
                stimer=null;
            }
            $("#product-list").slideDown(300);
            if ( isOnProduct ) {
                if (!$(".nav-menu-list"+t).hasClass('nav-menu-action')) {
                    $(".nav-menu-list"+t).addClass('nav-menu-action');
                }
            } else {
                setTimeout(function () {
                    if (!$(".nav-menu-list"+t).hasClass('nav-menu-action')) {
                        $(".nav-menu-list"+t).addClass('nav-menu-action');
                    }
                }, 10)
            }
        }
        if(t==0){
            $("#product-list").slideUp(300);
            setTimeout('$(".nav-menu").removeClass("bg-menu");',300);
        }
    }
    function hideProductList(){
        stimer = setTimeout('showProductList(0)',200);
    }
    $(window).scroll(function(){
        // $('.nav-menu-main').hide();
    });
    
    function showMyInfo(){
        $('#info-list').show();
        clearTimeout(uTimer);
    }
    function hideMyInfo(){
        uTimer = setTimeout(function(){
            $('#info-list').hide();
        },200);
    }
    
    //获取购物车清单
    function cartNumber(){
        $.post('https://www.xgimi.com/api/cartNumber', {
        data: 'cartnumber'
        }, function(data) {   
            if(data.code == 200){
                $('#cartNumber').empty().append(data.data); 
            } 
        }, 'json');
    }

    var isGetNavCart = true;

    function navCart(){
        $.post('https://www.xgimi.com/api/navCart', {
        data: 'navcart'
        }, function(html) {
            $('#cartList').empty().append(html);
            cartNumber();
            setTimeout(function () {
                isGetNavCart = true
            }, 2000)
        }, 'html');
    }

    //页头-展开购物车
    function cartShow(){
        if (isGetNavCart) {
            navCart();
            isGetNavCart = false
        }
        $('.member_details').addClass('active');
    }   

    function cartHide(){
        $('.member_details').removeClass('active');
    }
    
    $('#cartList').mouseover(function(event) {
        $('.member_details').addClass('active');
    });
    $('#cartList').mouseout(function(event) {
        $('.member_details').removeClass('active');
    });
    //删除数据
    function drop_goods_do(shoppingcartId, parentId) {
        $.post('https://www.xgimi.com/api/dropgoods', {
            'shoppingCartId': shoppingcartId,
            'parentId': parentId
        }, function(data) {
            if(data.code == 200){
                navCart();
                cartNumber();
            }else{
                gm_alert(data.msg);            
            }
        }, 'json');
    }
</script>
    
