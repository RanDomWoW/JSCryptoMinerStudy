<script>
  (function () {
    var wrapper, clearID, count, _isPlaying, _isFade, start, stop, isPlaying, setBackgroundFade;

    var wrapperId = 'now_loading';

    /**
     * Loading画像の再生
     * @param fps
     */
    start = function (fps) {
      var self = arguments.callee;

      // fpsがundefinedならデフォルト値代入
      if (fps === undefined) {
        fps = 12;
      }

      // 要素取得
      wrapper = document.getElementById(wrapperId);

      // 最初に実行
      if (count === undefined) {
        count = 0;
        _isPlaying = true;

        // 要素表示
        wrapper.style.display = 'block';
      }

      if (count < fps) {
        ++count;
      }
      else {
        count = 1;
      }

      var imgCount = 1;
      if(fps/2 < count){
        imgCount = 2;
      }

      $('#loadImgLeft,#loadImgRight').removeClass().addClass('img_'+imgCount);
      $('#loadText').removeClass().addClass('text_'+count);

      clearID = setTimeout(self, 1000 / fps, fps, wrapperId);
    };

    /**
     * Loading画像の停止
     */
    stop = function () {
      // 要素非表示
      wrapper.style.display = 'none';
      clearTimeout(clearID);
      count = undefined;
      _isPlaying = false;
    };

    /**
     * 再生中かどうか
     * @return {*}
     */
    isPlaying = function () {
      return _isPlaying;
    };

    window.Loading = {
      start: start,
      stop: stop,
      isPlaying: isPlaying
    };
  })();

    /* 9FPSでアニメーション*/
    Loading.start(9);

    $(function(){
        if(window.isContinueLoading != 1){
            Loading.stop();
        }
    });
</script>


                
                

                
