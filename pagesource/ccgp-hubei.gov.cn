           return false;
	   }
	   if(password.length<=0){
	        alert("请输入密码！");
	   	   //document.getElementById("td").innerHTML="请输入密码！";   
           return false;
	   }
       if(inputCode.length <=0)   
       {   
           alert("请输入验证码！");
           //document.getElementById("td").innerHTML="请输入验证码！";   
           return false;
       }   
       else if(inputCode.toUpperCase() != code )   
       {   
       	  alert("验证码输入错误！");
       	 // document.getElementById("td").innerHTML="验证码输入错误！";  
          createCode();//刷新验证码   
