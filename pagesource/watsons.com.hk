		function cdoGet(form_data, url,myCallback,methodType,dataType,errorCallBack, isSubscriptionFreeItem) {
            //$(".child").hide();

			showLoadingDiv();
			responseData = null;
			if(methodType==undefined||methodType==null)methodType="get";
			if(dataType==undefined||dataType==null)dataType="html";
			$.ajax({
				type: methodType,
				url: url,
				data: form_data,
				cache: false,
				dataType : dataType,
				success: function(responseData) {
				 // $(".child").hide();
					var hasError = isErrorPopup(responseData);
					if (hasError) {
						originOpenContentPopup(responseData);
						closeLoadingDiv();
					}else {
						if(errorCallBack!=undefined&&errorCallBack!=null&&typeof(errorCallBack)=='function'
							&&$(responseData).find("input[name='errors']").length>0){
							errorCallBack.call(this,responseData);
						}else{
							if(myCallback!=undefined&&myCallback!=null&&typeof(myCallback)== 'function' ){
								myCallback.call(this,responseData);
								callback(responseData, 0);
							}else{
								callback(responseData, 0);
							}
							
							if($('<div>').html(responseData).find("#myBasket").html() != null) {
								$("#myBasket").empty().html($('<div>').html(responseData).find("#myBasket").html());
							}
						}
						if(!isSubscriptionFreeItem) {
							closeLoadingDiv();
						}
					}
					$(".branchBankCodeSelectId-menu").show().css({"z-index":"-1"});
					$(".branchBankCodeSelectId-menu").mCustomScrollbar("update");
				},
				error:function(responseData){
					//alert("error:"+responseData.status);
				}
			});
			return false;
		}
		function doGet(form_data, url,myCallback,methodType,dataType,errorCallBack) {
