		function isErrorPopup(responseData) {

			var openErrorPopup = $(responseData).andSelf().find("#serviceUnavailable");
			var minicartShippableToPRCErrorPopup = $(responseData).find("#minicartShippableToPRCErrorPopup");

			var hasError = $(openErrorPopup).size() > 0;
			var hasMinicartError = $(minicartShippableToPRCErrorPopup).size() > 0;
			
			var errorFlag=false;
			if(hasError || hasMinicartError){
				errorFlag=true;
			}
			return errorFlag;
		}
		
