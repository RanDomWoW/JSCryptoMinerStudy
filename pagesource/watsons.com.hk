		function doErrors(obj,div){
			switch (obj.status) {
				case 403:
					if(div!=undefined&&div!=null&&$(div).attr("id")!=undefined)
						ajaxRequestCommon(div,globlContextUrl+"accessDenied");
					else alert("have no right!");
					break;
				case 404:
					if(div!=undefined&&div!=null&&$(div).attr("id")!=undefined)
						ajaxRequestCommon(div,globlContextUrl+"resourceNotFound");
					else alert("can not find the resource!");
					break;
				case 408:
					alert("request time out!");
					break;
				case 503:
					alert("the server is not available!");
					break;
				case 500:
					alert("request error!errorMessage:\n"+$(obj.responseText).find("#_exception").text());
					break;
			}	
		}
		
