					//alert("error:"+responseData.status);
				}
			});
			return false;
		}
		
		function changeRegion(code,subSelectId,url,autoOpen,selectValue){
			var id="#"+subSelectId;
			var success=function(list){
				$(id).selectmenu("disable");
				$(id).html("");
				$("<option value=''>請選擇</option>").appendTo($(id));
				for(var i=0;i<list.length;i++){
					if(selectValue!=undefined&&selectValue==list[i]['code']){
						$("<option>").val(list[i]['code']).text(list[i]['name']+" "+list[i]['postcode']).prop("selected",true).appendTo($(id));
					}else{
						$("<option>").val(list[i]['code']).text(list[i]['name']+" "+list[i]['postcode']).appendTo($(id));
					}
				}
				$(id).selectmenu("destory").selectmenu();
				if(autoOpen==undefined||autoOpen)
					$(id).selectmenu('open');
			}
			if($.trim(code)==""){
				success.call(this,new Array());
				return;
			}
			doGet(null,url,success,"get","json");		
		}		
		
