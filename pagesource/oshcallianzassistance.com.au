				function purchaseNow(nValue) {
					document.Main.hiddenLengthOfCover.value = nValue;
					document.Main.submit();
					return;
				}

				function StringToDate(strDate) {
					var dtCh = "/";

					var pos1 = strDate.indexOf(dtCh);
					var pos2 = strDate.indexOf(dtCh, pos1 + 1);
					var strDay = strDate.substring(0, pos1);
					var strMonth = strDate.substring(pos1 + 1, pos2);
					var strYear = strDate.substring(pos2 + 1);
					if (strYear.length == 2)
						strYear = "20" + strYear;

					var dte = new Date();
					dte.setMonth(strMonth - 1);
					dte.setDate(strDay);
					dte.setYear(strYear);

					return dte;
				}

				function validateForm() {

					//Check that the proposed start date is within 12months of today
					if (!validateStartEndDate(document.Main.tbStartDate.value, '17/03/2019', "You cannot purchase a policy more than 12 months in advance.")) return false;


				//if( !checkField( "ddOshcPlanType", "the Plan Type", 1, 'T', 1, 50, 'Main' ) ) return false;

				if (!checkField("tbStartDate", "the Start Date", 1, 'D', 1, 10, 'Main')) return false;

				var now = new Date();
				var yearsFromNow = 5;

				var comDate = StringToDate(document.getElementById("tbStartDate").value);

				if (comDate < now) {
					alert('Commencement Date cannot be before today');
					return false;
				}

				var sYear = now.getFullYear();
				var latestDate = new Date();
				latestDate.setFullYear((sYear * 1) + yearsFromNow);

				if (comDate > latestDate) {
					alert('Commencement Date cannot be more than ' + yearsFromNow + ' years from today');
					return false;
				}

				if (!checkField("ddOshcCoverPeriod", "the Period of Cover", 1, 'T', 1, 50, 'Main')) return false;
				return true;
			}
			
			</script>	
		
