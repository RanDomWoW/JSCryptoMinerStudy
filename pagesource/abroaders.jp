    <script type="text/javascript">
      function callback (info) {
        /* レコメンドアイテムをHTMLに整形する処理を記述 */
        var target = document.getElementById(info.div_id), item = target.innerHTML;
        if (info.item_list.length > 0) {
            var itemInfo = localStorage['itemInfoBdash'];
             localStorage.removeItem('info');
            localStorage['info'] =  JSON.stringify(info);
            if (!itemInfo) {
              item += "<div class=\"owl-carouse12\">";
              for (var i = 0; i < info.item_list.length; i++) {
                var id = info.item_list[i].id;
                var article_id = info.item_list[i].article_id;
                var author_avatar = info.item_list[i].author_avatar;
                var author = info.item_list[i].author;
                var title = info.item_list[i].title;
                var description = info.item_list[i].description;
                var image = info.item_list[i].image;
                var sliderID = i+1;
                var country_name = '日本';
                var div = document.createElement("div");
                div.innerHTML = title;
                var text = div.textContent || div.innerText || "";
                item += "<div class=\"item item-cus bor-"+sliderID+"\">";
                item += '<img src="' +image.replace("http:", "https:") + '" />';
                item += '<div class="slider-caption">';
                item += '<a href="https://www.abroaders.jp/client/article-detail/'+article_id+'">'+text+'</a>';
                if(info.item_list[i].ambassador_id > 0 ){
                    item += '<div class="country-cus"><a href="https://www.abroaders.jp/ambassador/detail/'+info.item_list[i].ambassador_id+'"><img src="'+author_avatar.replace("http:", "https:")+'" alt="" class="cus-img"><span>'+author+'</span></a></div>';
                }else{
                    item += '<div class="country-cus"><img src="'+author_avatar.replace("http:", "https:")+'" alt="" class="cus-img"><span>'+author+'</span></div>';
                }
                item += '<div class="country-cus"><a href="https://www.abroaders.jp/client/country-article-list/'+getCountry(article_id, 'id')+'"><img src="'+getCountry(article_id, 'image')+'" alt="" class="cus-img"><span>'+getCountry(article_id, 'name')+'</span></a></div>';
                item += '</div>';
                item += '</div>';
              }
                item += "</div>";
               localStorage['itemInfoBdash'] = item;
               localStorage['lastupdated_bdash'] = new Date().getTime();
              }
            } else {
              item = "<p>現在おすすめアイテムはありません。</p>";
            }
            item = localStorage['itemInfoBdash'];
            target.innerHTML = item;
      }

      function errorCallback (info) {
        /* レコメンドアイテムの取得に失敗した場合の処理を記述 */
        var target = document.getElementById(info.div_id);
        target.innerHTML = '<p>現在おすすめアイテムはありません。</p>';
      }

      function requestRecommend () {
        // localStorage.clear();
        $.getJSON("assets/ambassador-files/lastupdated_bdash.text", function(data) {
            if (Number(localStorage['lastupdated_bdash']) < Number(data.lastupdated)) {
                localStorage.clear();
            }
        });
        var infoCache = localStorage['itemInfoBdash'];
        if (infoCache) {
            var info = localStorage['info'];
            callback(JSON.parse(info));
        } else {
            console.log("B Dash");
            var bdr_74 = new BDR();
            bdr_74.rid = 74;
            bdr_74.div_id = 'recommendArea'; // レコメンドを表示するdiv要素のid
            bdr_74.cb = 'callback'; // 上記で定義したコールバック関数名
            bdr_74.cb_err = 'errorCallback'; // 上記で定義したコールバック関数名(エラーの時)
            bdr_74.alt_html = '<p>現在おすすめアイテムはありません。</p>';
            bdr_74.item_id = '';
            bdr_74.cust_cd = '';
            bdr_74.request();
        }
      }
      function getCountry($article_id, $column){
        var response = $.ajax({type: "GET", url: 'https://www.abroaders.jp/ajax/get-country/'+$article_id+'/'+$column, async: false}).responseText;
        return response;
      }
    </script>
