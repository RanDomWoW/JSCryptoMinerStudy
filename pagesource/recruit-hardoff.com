	/*
	 * # マップの動き
	 *
	 */

	// 地図部分を変数にしとく
	var _map = $("#MapSVG");
	// リスト上の地方・都道府県も変数にしとく
	var _target = $("#MapList").find("a");
	// 生きてる地方（aタグ持ち）を[data-select="active"]にする -> 色が変わる
	_map.find("[data-area^='region']:has(a)").attr("data-status", "active");

	// --- リスト上の地方・都道府県（エリア）をhoverした時、地図部分も連動して色が変わるようにする
	_target.on({
		// hoverしたとき
		'mouseenter': function(){
			// hoverしたエリアを取得して
			var _area = $(this).attr("data-area");
			// console.log(_area);
			// 同じdata-area値をもつ地図部分を取得する
			var _thisArea = _map.find("[data-area="+ _area +"]");
			// その場所に[data-select="on"]を与える
			_thisArea.attr("data-select", "on");
		},
		// hoverが外れたときは逆に
		'mouseleave': function(){
			var _area = $(this).attr("data-area");
			var _thisArea = _map.find("[data-area="+ _area +"]");
			// [data-select="on"]をなかったことにする
			_thisArea.attr("data-select", "");
		}
	});
});
