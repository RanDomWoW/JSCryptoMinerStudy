      var path_base_auth = "http://dev.api.weschool.com/oauth/v2/token";
      var path_request_access = "http://dev.api.weschool.com/v1/platforminvites";        
    }else if(env === "stage"){
      var path_base_auth = "http://stage.api.weschool.com/oauth/v2/token";
      var path_request_access = "http://stage.api.weschool.com/v1/platforminvites";
    }

    var client_id = "2_63vhapa1xosos88o004cgs4wkg444gcos8sc4gg0gggksc8k0g";
    var client_secret = "11o29phvsgb488gg4gsso488w480wk484oowcsk4o4cg808048";

    var access_token = "";

    var clearFeedbacks = function(){
      jQuery(".frm_error_style, .frm_success_style").remove();
    };

    var onRequestError = function(ret){
      console.log(ret.responseJSON);
      var add="";
      var errorId=ret.responseJSON.message_id;
      if(errorId=="error_u_invite_already_send")
{add="red";};
      var message=errorMessages["en"][errorId];
      console.log(message);
      var markup = "<div class='frm_error_style "+add+"' id='form_error'>"+message+"</div>";
      clearFeedbacks();
      jQuery("#request-invite-form").before(markup);
      jQuery('#request-invite-form input').prop('disabled', false);
      location.hash = "#form_error";
    }

    var onRequestSuccess = function(){

      var responseMsg = {
        it: "Controlla la tua casella email per completare la registrazione.",
        en: "Check your inbox to complete the registration",
        es: "Comprueba tu correo electrónico para completar la registración",
      };

      var markup = "<div class='frm_success_style' id='form_success'>" + responseMsg.en + "</div>";
	ga('send', 'event', 'Invitation', 'Requested');
      clearFeedbacks();
      jQuery("#request-invite-form").before(markup);
      jQuery("#request-invite-form").hide();
      location.hash = "#form_success";
    }

    var tokenRequest = function(){
      return jQuery.ajax({
        type: "POST",
        dataType: "json",
        url: path_base_auth,
        data: {
          client_id: client_id,
          client_secret: client_secret,
          grant_type: "client_credentials",
          scope: "client"
        },
        success: function(ret){
          access_token = ret.access_token;
        },
        error: function(ret){
          //onRequestError();
        }
      });
    };

    var accessRequest = function(){
      clearFeedbacks();
      var inputs = {};
      inputs.email = jQuery("#request-invite-form input[name='email']").val();
      inputs.language = "en";

      jQuery('#request-invite-form input').prop('disabled', true);

      jQuery.ajax({
        type: "POST",
        dataType: "json",
        contentType : "application/json",
        url: path_request_access,
        beforeSend: function (xhr){
          xhr.setRequestHeader("Authorization", "Bearer " + access_token);
        },
        data: JSON.stringify(inputs),
        success: function(ret){
          onRequestSuccess();
        },
        error: function(ret){
          onRequestError(ret);
        }
      });
    } 

    var form = jQuery("#request-invite-form"); /** access request */
    if(form.length > 0){
      form.on("submit", function(e){
        e.preventDefault();

        jQuery.when(tokenRequest())
          .then(accessRequest, onRequestError)

        return false;
      });
    }

   var joinRedirectButton = jQuery("#join-redirect");
        if (joinRedirectButton.length > 0) {
            joinRedirectButton.on("click", function (e) {
                joinRedirect();
            });
        }

        function  joinRedirect () {
            var joinGroup=jQuery("#join-group");
            if (joinGroup.length > 0) {
                var code = joinGroup.val();
                if(code!=""){
                    window.location = " http://app.weschool.com/#join/"+code;
                }else{
                    var message = errorMessages["en"]["error_void_element"];
                    var markup = "<div class='frm_error_style' id='form_error'>" + message + "</div>";
                    jQuery("#join-group").before(markup);
                }
            }
        }
	

    /*function debounce(func, wait, immediate) {
      var timeout;
      return function() {
        var context = this, args = arguments;
        var later = function() {
          timeout = null;
          if (!immediate) func.apply(context, args);
        };
        var callNow = immediate && !timeout;
        clearTimeout(timeout);
        timeout = setTimeout(later, wait);
        if (callNow) func.apply(context, args);
      };
    };*/

	/*
	 * Custom page scrolling
	 */
	 
	// Disable nicescroll
	$("html").getNiceScroll().remove();
	$("div[id^='ascrail']").remove();
	
	if (applyScrollScriptToPage())
		scrollScript();
	
	function applyScrollScriptToPage() {
		return $('body').hasClass('home') || $('body').hasClass('page-template-page-schools')  || $('body').hasClass('page-template-page-companies');
	}
    
	function scrollScript() {
		/** Setup variables used for scrolling and auto height */
		var
			scrollDebounceDelay = 500 // 375
			,delta // Number of pixels to count around section to mae page scroll to it
			,header = $('.navbar')
			,footer = $('.footer')
			,adminBar = $('#wpadminbar')
			,adminBarHeight = adminBar.length > 0 ? adminBar.outerHeight() : 0
			,$sections = $(".js-section")
			,offsets = $sections.map(function(index, element) { return $(element).offset().top; })
		;
		
		function updateSectionsInfo() { // Add classes to divide page into sections with custom classes
			var isDesktop = $(window).width() >= 768;
		  
			var 
				headerHeight = header.outerHeight()
				,footerHeight = footer.outerHeight()
			;
		  
			var 
				vpHeight = $(window).height()
				,scrollHeight = vpHeight - adminBarHeight
				,firstScrollHeight = scrollHeight - headerHeight
        ,lastScrollHeight = scrollHeight - footerHeight
			;

      var realScrollHeight = "calc(100vh - " + adminBarHeight + "px)";
      var realFirstScrollHeight = "calc(100vh - " + adminBarHeight + "px - "+headerHeight+"px)";
      var realLastScrollHeight = "calc(100vh - " + adminBarHeight + "px - "+footerHeight+"px)";
      var realHalfScrollHeight = "calc((100vh - " + adminBarHeight + "px) / 2)";
			
			delta = window.delta || Math.floor(vpHeight / 2) // 200 

			$("body").addClass("js-section section");
			//$(".themo_slider_0").css("height", firstScrollHeight);
			//$("#themo_html_2").addClass("js-section section").css("height", scrollHeight); //video section
			//$("#themo_tour_1").addClass("js-section section").css("height", scrollHeight); //tour home
			//$("#themo_service_block_1").addClass("js-section section").css("height", scrollHeight);
			/*if (isDesktop) {
				$("#themo_tour_1_0, #themo_tour_1_1").removeClass("js-section section").css("height", Math.ceil(scrollHeight / 2)); //tour home
			} else {
				//su smartphone due blocchi in home a tutta pagina, con scroll autommatico
				$("#themo_tour_1_0, #themo_tour_1_1").addClass("js-section section").css("height", scrollHeight); //tour home
			}*/
			//$('.feature-integration').addClass("js-section").outerHeight(scrollHeight);
			//$(".page-id-883 #themo_tour_1 section").addClass("js-section").css({"height": scrollHeight}); //schools sections
     
      //home
      $('.masthead-home').css({'height': realFirstScrollHeight, 'min-height': realFirstScrollHeight}).removeClass('js-section'); //body already has it
      $('.feature.integration, .feature.intro, .feature.unleash').css({'height': realScrollHeight,'min-height': realScrollHeight}).addClass('js-section');
      $('.feature.unleash').css({'height': realLastScrollHeight, 'min-height': realLastScrollHeight}); //last section
      if(isDesktop){
        $(".feature.no-limits").addClass('js-section');
        $(".feature.no-limits, .feature.lab").css("height", realHalfScrollHeight); //half blocks home
      }
			
			// New Schools page sections
			$('.masthead').css({'height': realFirstScrollHeight, 'min-height': realFirstScrollHeight});
			$('.superpower').css({'height': realScrollHeight,'min-height': realScrollHeight}).addClass('js-section');
			$('.superpower:last-of-type').css('height', realLastScrollHeight);

      //companies
      $('.scroll').css({'height': realScrollHeight, 'min-height': realScrollHeight}).addClass('js-section');
      $('.scroll:first-of-type').css({'height': realFirstScrollHeight,'min-height': realFirstScrollHeight}).removeClass('js-section'); //body already has it
      $('.scroll:last-of-type').css({'height': realLastScrollHeight, 'min-height': realLastScrollHeight});
			
			$sections = $(".js-section");
      offsets = [];
			$sections.each(function(index, element){
        //do not scroll to sections higher than view (otherwise they cannot be fully viewed)
        var elemType = $(element).prop('nodeName').toLowerCase();
        //console.info($(element).attr("class"), $(element).height(), scrollHeight)
        //add offset is section is shorter than scroll height OR is body (aka first scroll of the page)
        if($(element).innerHeight() <= scrollHeight || elemType === 'body'){
          //check valid height of double scroll (two sections in one view), when needed
          if($(element).hasClass("no-limits") === false || $(element).innerHeight()*2 <= scrollHeight){
            //console.info($(element).attr("class"), $(element).hasClass("no-limits"), $(element).innerHeight(),$(element).innerHeight()*2,scrollHeight);
				    offsets.push($(element).offset().top);
          }
        }
			});
		}

		function handleScroll(){
			var doAnimation = true;
		  
			function scrollToClosestSection(){
        if($(window).width() >= 768 && !agentManager.isIPhone && !agentManager.isWindowsPhone){

    			var st = $(this).scrollTop();
    			
    			$.each(offsets, function(index, offset) {
    				var realOffset = offset - adminBarHeight;
    				//console.info("now:", st, st+delta, st-delta, realOffset);
    				if(realOffset <= st + delta && realOffset >= st - delta && doAnimation) {
    					doAnimation = false;
    					$('html, body').stop(true, true).animate({ 
    						scrollTop: realOffset
    					}, 200, function() {
    						doAnimation = true;
    					});
    				}
    			});

        }
			}

			$(window).scroll(_.debounce(function(event) {
				if ('snapping' in window && window.snapping === false)
					return;
				$('html, body').stop(true, false);
				updateSectionsInfo(); // update offset (may be useful if page has been resized)
				scrollToClosestSection();
			}, scrollDebounceDelay, false));

			scrollToClosestSection();
		}
		
		updateSectionsInfo();
		$(window).resize(_.debounce(updateSectionsInfo, 500, false));
		window.setTimeout(handleScroll, 300);
	}
	
	if (isHome()){
		setupIntroVideo();
  }
	
	function setupIntroVideo() {
		var 
			introScroll = document.querySelector('.feature.intro'),
			$introScroll = $(introScroll),
			playButton = introScroll.querySelector('.ws-button')
		;

    if(agentManager.isIPad || agentManager.isIPhone || agentManager.isAndroid || agentManager.isWindowsPhone){
      if(jQuery(".integration video").length > 0){
        jQuery(".integration video").replaceWith("<div class='text-center'><img src='https://s3-eu-west-1.amazonaws.com/www.weschool.com/video/lessons.gif' /></div>");
      }
    }
		
		playButton.addEventListener('click', function(e) {
			e.stopPropagation;
			e.preventDefault();
			
      /*if(jQuery(window).innerWidth() >= 990 && !agentManager.isIPad && !agentManager.isIPhone && !agentManager.isAndroid && !agentManager.isWindowsPhone ){
  			playIntro();
      }else{
        
      }*/

            var vid = "https://www.youtube.com/embed/P1CeWmov1As?rel=0&amp;showinfo=0&amp;autoplay=1";
      jQuery("#yt").attr("src", vid);
      $introScroll.removeClass('loading').addClass('visible');
		})
		
		function playIntro() {
      $introScroll.addClass('loading');
			
			var tag = document.createElement('script');
			tag.src = "https://www.youtube.com/iframe_api";
			var firstScriptTag = document.getElementsByTagName('script')[0];
			firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
		
			var player;
  
			window.onYouTubeIframeAPIReady = function () {
				player = new YT.Player('yt', {
					events: {
						'onReady': onPlayerReady,
						'onStateChange': onPlayerStateChange
					}
				});
			}
			
			function onPlayerReady(event) {
				player.playVideo();
			}
			
			function onPlayerStateChange(event) {
				var started = false;
				
				if (event.data == YT.PlayerState.PLAYING && !started) {
					$introScroll.removeClass('loading').addClass('visible');
					started = true;
				}
			}
			
		}
	}
	
	function isHome() {
		return $('body').hasClass('page-template-page-ws-home');
	}

  });
</script>


