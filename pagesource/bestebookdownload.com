        $('#sub').click(function(e){
            e.preventDefault();

            var o_f = $(this).parents('form');
            var o_author = o_f.find('input[name=author]');
            var i_author = o_author.val();
            if(o_author.val().length == 0) {
                o_author.addClass('error');
                return false;
            }

            var o_email = o_f.find('input[name=email]');
            var i_email = o_email.val();
            var email_reg =  /^([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/; 
            if(i_email.length == 0) {
                o_email.addClass('error');
                return false;
            } else if(!email_reg.test(i_email)) {
                o_email.attr('placeholder', '格式不正确').addClass('error');
                return false;
            }

            var o_comment = o_f.find('textarea[name=comment]');
            var i_comment = o_comment.val();
            if(i_comment.length == 0) {
