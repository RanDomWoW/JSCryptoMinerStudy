<script type="text/javascript">

/*------ 下固定バナー ------*/

/*-------------------------------
・下固定バナー全体を囲うclass名は統一する
・各数値の部分は変数にして、PHPからJSONでjsに数値を渡す。
　→管理画面上で数値を設定できるようにする
-------------------------------*/

(function($){
  $(function(){

    var floatHigh = $('.common_cta').height();//下固定バナーの高さを取得

    //最初はバナーを非表示にしておく
    $('.common_cta').hide();

    //550は表示させるウインドウ幅。ここを変数にする。
    if($(window).width() <= 550) {

      //下固定バナーの高さ+設定した高さ。ここを変数にする。
      $('.pagetop').css({
        'bottom' : floatHigh + 20
      });

      //footer下のpaddingは下固定バナーの高さと一緒
      $('#footer').css({
        'padding-bottom' : floatHigh
      });

      //10ピクセルスクロールしたらフェイドインで表示
      $(window).scroll(function(){
        if($(this).scrollTop() > 10) {
          $('.common_cta').fadeIn('fast');
        }
      });

    //下固定バナー非表示のとき
    }else{

      //設定した高さ（20を好きな数値に書き換える）ここを変数にする。
      $('.pagetop').css({
        'bottom' : 20
      });

      //footer下のpaddingは0
      $('#footer').css({
        'padding-bottom' : 0
      });
    }

    return false;
  });

})(jQuery);

</script>
