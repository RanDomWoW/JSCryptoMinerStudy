        }

        $("input#words").autocomplete({
            search: function( event, ui ) { $(".psrc .loading").show(); },
            source: function( request, response ) {
                $.ajax({
                    type: "get",
                    url: ssl_ajaxdir + "get=products&action=getProductsForPreSearch",
                    dataType: "json",
                    data: {
                        term: request.term
                    },
                    success: function( data ) {
                        $(".psrc .loading").hide();
                        response( $.map( data, function( item ) {
                            return {
                                label: htmlDecode(item.title),
                                value: htmlDecode(item.title),
                                id: item.id,
                                seoLink: item.seoLink

                            }
                        }));
                    }
                });
            },
            minLength: 2,
            select: function( event, ui ) {
                $("input#searchedProductId").val(ui.item.id);
                window.location.href = ui.item.seoLink;

            }
        });

        $("form#search").submit(function(e){
            e.preventDefault();
            var $form = $(this);
            var $submitObj = $("input:submit", $form);
            var $url = $form.attr("action");
            var $data = $form.serialize();
            var $proId = $("#searchedProductId", $form).val();
            var $catId = $("#catselect", $form).val();
            var $words = $("#words", $form).val();
            var $defVal = $("#words", $form).attr("defval");
            if($proId != 0 && $proId.length > 0){
                if($catId != 0){
                    $url = $url + "&id=" + $proId + "&cat_id=" + $catId + "&words=" + $words;
                }else{
                    $url = $url + "&id=" + $proId + "&words=" + $words;
                }
                $.ajax({
                    type: "get",
                    url: $url,
                    dataType: "json",
                    async: false,
                    beforeSend: function(){
                        var ret = checkAnyForms($form);
                        if(ret === false){
                            return ret;
                        }
                    },
                    success: function(content){
                        if($.isPlainObject(content)){
                            if(content.type == "Success"){
                                hideError();
                                window.location.href = content.message;
                            }else{
                                showError($submitObj,true,content.message);
                            }
                        }
                    }
                });
            }else if($catId != 0 && ($words.length <= 0 || $words == $defVal)){
                $url = $url + "&cat_id=" + $catId;
                $.ajax({
                    type: "get",
                    url: $url,
                    dataType: "json",
                    async: false,
                    beforeSend: function(){
                    },
                    success: function(content){
                        if($.isPlainObject(content)){
                            if(content.type == "Success"){
                                hideError();
                                window.location.href = content.message;
                            }else{
                                showError($submitObj,true,content.message);
                            }
                        }
                    }
                });
            }else{
                if(($words.length <= 0 || $words == $defVal)){
                    showError($submitObj,true,"lütfen aranacak en az bir kriter belirleyin");
                }else{
                    $(this).unbind('submit');
                    this.submit();
                }

            }
        });
