<script type="text/javascript">
	$(document).ready(function(){
	var storage = window.localStorage;
        var winHeight = jQuery(window).height();
        var navPosition = $('#tipimg').position();

        $('.slide-arrow').css('top',winHeight*0.32);
        $('#nav').css('top',winHeight*0.32);
        $('#nav').css('left',navPosition.left-36);
        
        jQuery(window).bind("resize", changeSAposition);
 
        function changeSAposition( e ) {
            var winHeight = jQuery(window).height();
            var navPosition = $('#tipimg').position();
            
            $('.slide-arrow').css('top',winHeight*0.32);
            $('#nav').css('left',navPosition.left-36);
        };
        
        //  Initialize Backgound Stretcher	   	
        $('BODY').bgStretcher({
        images: ['/wp-content/themes/rucweb/images/ruc_bg_290.jpg','/wp-content/themes/rucweb/images/ruc_bg_289.jpg','/wp-content/themes/rucweb/images/ruc_bg_288.jpg','/wp-content/themes/rucweb/images/ruc_bg_287.jpg','/wp-content/themes/rucweb/images/ruc_bg_285.jpg','/wp-content/themes/rucweb/images/ruc_bg_284.jpg','/wp-content/themes/rucweb/images/ruc_bg_276.jpg'],
        imageWidth: 1680, 
        imageHeight: 1050,
        transitionEffect: 'fade',
        sequenceMode: 'normal',
        buttonPrev: '#cclb-btn-prev',
        buttonNext: '#cclb-btn-next',
        nextSlideDelay: '45000',
        preloadImg: true 
  //pagination: '#nav'
});                
                $('#cclb-btn-pause').click(function(){
                        if ($(this).html() == "Pause"){
                                $(this).html("Resume");
                                $(this).attr('class','cclb-btn-start');
                                $('BODY').bgStretcher.pause();
                        } else {
                                $(this).html("Pause");
                                $(this).attr('class','cclb-btn-pause');
                                $('BODY').bgStretcher.play();
                        }
		})
		
		$('.logo').hover(function(){
			$('.lang').slideDown(200);
            $('.animate-area div').stop(true, true);
			$('.sum-content').slideUp(200);
			$('.animate').animate({
				width: '200'
			},200, function(){
				$('.sum').removeClass('no-bg-color');
				$('.sum-content span').removeClass('changebg');
			});
		},function(){
			$('.lang').slideUp(200);
		});

		$('.animate span').mouseenter(function(){
            $('#searchbox').slideUp(200);
			$('.animate-area div').stop(true, true);
			$('.animate').removeClass('no-bg-color');
			$('.animate span').removeClass('changebg');
			$(this).addClass('changebg');
			$('.aa-content').hide();
			var box = $(this).parent();
			box.removeClass('animate');
			box.animate({
				width: '300'
			}, 200, function(){
				box.addClass('no-bg-color');
				box.children('.aa-content').slideDown(200);
				box.addClass('animate');
			});
			$('.animate').animate({
				width: '150'
			}, 200);
		});
		
		$('.aa-content').mouseleave(function(){
			$('.animate-area div').stop(true, true);
			$(this).slideUp(200);
			$('.animate').animate({
				width: '200'
			},200, function(){
				$(this).removeClass('no-bg-color');
				$('.animate span').removeClass('changebg');
			});
		});
		
		$('#search').click(function(){
			$('#searchbox').toggle();
			$(this).css('font-weight','700');
		});
		
		$('#s').focusin(function(){
			var val = $(this).val();
			if(val == '请输入搜索内容...'){
				$(this).val('');
			}
		});
		
		$('#s').focusout(function(){
			var val = $(this).val();
			if(val == ''){
				$(this).val('请输入搜索内容...');
			}
		});
		
		$(document).keypress(function(e) {
			if (e.shiftKey && e.which == 70) $('#searchbox').toggle();
			$('#search').css('font-weight','700');
		});
		
		
		if (storage.getItem("lastvisitURL") && storage.getItem("lastvisitSite")){ 
			$('#lastvisitLink').html(storage.getItem("lastvisitSite"));
			$('#lastvisitLink').attr('href',storage.getItem("lastvisitURL"));
			$('.lastvisit').show();
		};

		$('.newsbox').hover(function(){
                        $('#togobox').attr('class','mb-left');
                        $('.infolist').hide();
                        $('#newslist').show();
                });
                $('.infobox').hover(function(){
                	$('#togobox').attr('class','mb-left-info');
                });
                $('.infobox a').hover(function(){
                        var target=$(this).attr('box');
                        $('.infolist').hide();
                        $('#'+target).show();
                });
                
            	$('.links').hover(function(){
          	$('#searchbox').slideDown();
		$('.serv-content').slideUp(200);
                $('.serv').removeClass('no-bg-color');;
         	 });
                
	});
	
</script>


