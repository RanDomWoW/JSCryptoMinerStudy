<!-- KNOW YOUR OCEAN -->

<section id="knowyourocean">
    <div class="row">
        <h2><a href="http://www.whoi.edu/know-your-ocean/">#KnowYourOcean</a></h2>
        <h3>No matter where you live, the ocean affects your life.</h3>
        <div class="fifth columns"><img src="/cms/responsive/img/icon-oxygen.png" alt=""/>
            <span class="percent">50%</span>
            <h4>of the <span>oxygen</span> from photosynthesis is produced in the ocean</h4>
        </div>

        <div class="fifth columns"><img src="/cms/responsive/img/icon-population.png" alt=""/>
            <span class="percent">44%</span>
            <h4>of Earth's <span>population</span> lives within 150km (100 miles) of a coast</h4>
        </div>

        <div class="fifth columns"><img src="/cms/responsive/img/icon-transport.png" alt=""/>
            <span class="percent">90%</span>
            <h4>of international <span>trade</span> travels by ship</h4>
        </div>

        <div class="fifth columns"><img src="/cms/responsive/img/icon-earth.png" alt=""/>
            <span class="percent">95%</span>
            <h4>of the ocean is <span>unexplored</span> by humans</h4>
        </div>

        <div class="fifth columns"><img src="/cms/responsive/img/icon-climate.png" alt=""/>
            <span class="percent">90%</span>
            <h4>of <span>heat</span> from global warming has been absorbed by the ocean</h4>
        </div>

    </div>

</section>

<!-- /KNOW YOUR OCEAN -->

