<div id="form-login-content" class="wrap">
	<p class="title login"><span>ACCEDER CON EMAIL</span></p>
	<p class="title password-recovery" style="display: none;"><span>RECUPERAR CONTRASEÑA</span></p>
	
	<form id="login-form" onsubmit="return submitFunc();">
		<div id="login-section">
			<p class="error" style="display:none;"></p>
			<div>
				<input type="text" id="input_email" name="login" size="50" value="" placeholder="Nombre de usuario o e-mail">
			</div>
			<div>
				<input type="password" id="input_password" name="password" size="50" value="" placeholder="Tu Contraseña">
			</div>
			<div id="password-recovery-link">
				<a onClick="showPassRecovery();" href="#">¿Has olvidado tu contraseña?</a>
			</div>
			<button id="input_send" type="submit" class="btn btn-big btn-blue">Acceder</button>
		</div>

		<div id="login-recovery-section" style="display:none;">
			<div class="msgs">
				<p class="error" style="display:none;"></p>
				<p class="success" style="display:none;"></p>
			</div>
			<div id="ask-for-password-section">
				<div id="password-recovery-field">
					<input type="text" id="pass_recover_input_email" size="50" value="" placeholder="Tu e-mail">
				</div>
			</div>
			<div class="back-to-login">
				<a onClick="backToLogin();" href="#">Atrás</a>
			</div>

			<button id="send-pass-recovery" type="submit" onClick="sendPassRecovery(event);" class="btn btn-big btn-blue">Enviar</button>
		</div>
	</form>
</div>
<script>
var that=this;

document.addEventListener("DOMContentLoaded", function(event) {
	
	$("#form-login-content input").on("click", function(){
		return !($("#login-section").hasClass("disabled") || $("#login-recovery-section").hasClass("disabled"))
	})

	function transition(type){
		if (type==="in"){
			$("#login-section").addClass("disabled")
			$("#login-recovery-section").addClass("disabled")
			$("#login-form").css("opacity",0.5)
		} else {
			$("#login-section").removeClass("disabled")
			$("#login-recovery-section").removeClass("disabled")
			$("#login-form").css("opacity",1)
		}
	}

	/* Start login */
	that.submitFunc = function () {
		
		if ($("#login-section").hasClass("disabled")){
			return false;
		}

		var wrong_password_msg = 'El nombre de usuario/email o la contraseña son incorrectos';
		var session_limit_msg = 'Ha superado el número de sesiones simultáneas contratadas.';
		var server_error_msg = 'Error del servidor. Vuelve a intentarlo pasados unos minutos.'

		transition("in")
		$('.error').hide();

		$.ajax({
			type: 'POST',
			url: "https://vlex.com/session?t=" +(new Date().getTime()),
			data: {
				model: {
					login: $('#input_email').val(),
					password: $('#input_password').val()
				}
			},
			xhrFields: {
				withCredentials: true
			},
			success: function () {
				window.location = "https://app.vlex.com";
				transition("out")
			},
			error: function (jqXHR) {
				var message;
				if (jqXHR.status === 403 || jqXHR.status === 401 ) {
					if (JSON.parse(jqXHR.responseText).error === "Authentication failed") {
						message = wrong_password_msg;
					} else {
						message = session_limit_msg;
					}
				} else {
					message = server_error_msg;
				}

				$('#login-section .error').text(message).show();
				transition("out")
			}
		});

		return false;

	}
	/* End login */

	/*Start password recovery */
	

	that.showPassRecovery = function(){
		transition("in")
		$(".title.login").hide()
		$(".title.password-recovery").show()
		$("#login-section").hide()
		
		$("#input_send").attr("type","button")
		$("#send-pass-recovery").attr("type","submit")

		$("#login-recovery-section").show()
		$("#ask-for-password-section").show()
		$("#send-pass-recovery").show()
		transition("out")
		return false;
	}

	that.sendPassRecovery = function(e){
		e.preventDefault()
		e.stopPropagation()

		if ($("#login-recovery-section").hasClass("disabled")){
			return false;
		}

		if (!/@/.test($("#pass_recover_input_email").val())){
			$("#login-recovery-section").addClass("error-block")
			$(".error").text("Email incorrecto. Por favor introduce uno válido").show()
			return false;
		}

		$("#login-recovery-section").removeClass("error-block")
		$(".msgs p").hide()
		transition("in")
		
		$.ajax({
			'type': 'POST',
			'url':  'https://app.vlex.com/account/password_reset',
			'data': {'login' : $("#pass_recover_input_email").val() },
			success: function(resp){
				$("#ask-for-password-section").hide()
				$("#send-pass-recovery").hide()
				$(".msgs").show()
				$(".success").text("En breve le llegará un correo a " + $("#pass_recover_input_email").val() + " para restablecer su contraseña").show()
				$("#pass_recover_input_email").val("")
				transition("out")
			},
	  		error: function(resp) {
				$(".msgs").show()
				$("#login-recovery-section").addClass("error-block")
				$(".error").text("Ha ocurrido un error. Por favor, comprueba que el email es correcto o inténtalo más tarde").show()
				transition("out")
	  		}
	  	})
	}

	that.backToLogin = function(){
		transition("in")
		$('.error').text("")
		$("#login-recovery-section").removeClass("error-block")
		$(".msgs p").hide()
		$(".title.password-recovery").hide()
		$(".title.login").show()
		
		$("#send-pass-recovery").attr("type","button")
		$("#input_send").attr("type","submit")

		$("#login-recovery-section").hide()
		$("#login-section").show()
		transition("out")

		return false;
	}
	/*End password recovery */
})
