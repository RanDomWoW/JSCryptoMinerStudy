<script>
     var that=this;
     function validaSignup(form, field) {
          var handleError = function(field, message) {
               var dropdownField = $(field).hasClass("tmp-custom-dropdown")

               if (dropdownField){
                    $(field).closest("div.field-group-block").find(".validationError").remove();
                    $(field).closest("div.field-group-block").addClass("error");
               } else {
                    $(field).closest("div").find(".validationError").remove();
                    $(field).closest("div").addClass("error");
               }
               
               if (message) {
                    if (dropdownField){
                         $(field).closest(".field-group").parent().append("<p class='error validationError'>" + message +"</p>");
                    } else {
                         $(field).closest("div").append("<p class='validationError'>" + message +"</p>");
                    }
               }
          }

          function validateSpainPhone(field){
               var esRegExPattern = /^(34|0034)?[9|6|8|7]\d{8}$/
               return field.value && field.value.replace(/\D+/g, "").match(esRegExPattern)
          }

          function validateGeneralPhone(field){
               var phoneRegex = /^[0-9]{7,25}$/
               return field.value && field.value.replace(/\D+/g, "").match(phoneRegex)
          }

          that.sanitizePhoneNumber = function(field) {
               field = $(field);
               field.val(field.val().replace(/\D+/g, ""))
               return true;
          }

          that.mapTitle = function(val) {
               var title = { 'default' : 'default', '1' : "Abogado", '17' : "Administrador de Empresas", '15' : "Asesor/Gestor", '2' : "Bibliotecario / Documentalista", '12' : "Consulta Particular", '13' : "Contador / Contable", '18' : "Contador Público", '19' : "Contador Público/Revisor Fiscal", '20' : "Docente", '7' : "Estudiante", '16' : "Estudiante de Derecho", '21' : "Financiero", '9' : "Investigador", '22' : "Investigador Académico", '3' : "Notario", '23' : "Otra profesión distinta de las anteriores", '8' : "Otra Profesión Legal", '14' : "Otra profesión relacionada con el derecho", '6' : "Otro", '10' : "Personal Administración Pública", '4' : "Procurador", '11' : "Profesional no relacionado con el derecho", '5' : "Profesor" }

               return title[val]
          }

          that.mapIndustry = function(val) {
               var industry = { 'default' : 'default', '1' : 'Inst. Académica con facultad derecho', '2' : 'Despacho profesional > 10 abogados', '3' : 'Despacho profesional < 10 abogados', '4' : 'Asociación de Abogados', '5' : 'Otros', '6' : 'PYME / Autónomo', '7' : 'CT- Autor - Doctrina', '8' : 'CT- Autor - Contratos', '9' : 'CT- Autor - Varios', '10' : 'CT- Vpublishing', '11' : 'Abogado independiente', '12' : 'Inst. Académica sin facultad derecho', '13' : 'Agencia Gubernamental', '14' : 'Gran Empresa', '15' : 'Administración Pública', '16' : 'Despacho', '17' : 'Firma', '18' : 'Fundaciones, ONGs', '19' : 'Notaría', '20' : 'Profesional Independiente', '21' : 'PYME', '22' : 'Sector Académico' }

               return industry[val]
          }

          that.isFieldDisplayed= function(field){
               return $(field) && $(field).parent() && $(field).parent().css("display") !== "none";
          }

          var emptyError = function(field) {
               return !field.value || field.value == field.placeholder
          }
          
          //we use this to detect failed inputs at any moment
          var someFieldFailed = false;
          var emailRegex = /^[a-zA-Z0-9_\-\.]+([.][a-zA-Z0-9_\-]+)*[@][a-zA-Z0-9_\-]+([.][a-zA-Z0-9_]+)*[.][a-zA-Z]{2,4}$/i;
          
          // Remove all errors
          if (!field) {
               $("form .validationError").remove();
               $("form input.error").removeClass("error");
          }

          /***************************/
          /* FIELDS ERRORS DETECTION */
          /***************************/


          // Email
          if (!field || field.id === "input_email") {
               if (emptyError(form.input_email)) {
                    handleError(form.input_email, "El Email es obligatorio");
                    someFieldFailed = true;
               } else if (emailRegex.test(form.input_email.value) == false) {
                    handleError(form.input_email, "El Email no es valido");
                    someFieldFailed = true;
               }
               
               if (someFieldFailed){
                    return false;
               }
          }

          //Name
          if (form.input_name && (!field || field.id === "input_name")) {
               if (emptyError(form.input_name)) {
                    handleError(form.input_name, "El nombre es obligatorio");
                    someFieldFailed = true;
               }
               
               if (someFieldFailed){
                    return false;
               }
          }

          //Lastname
          if (form.input_ape && (!field || field.id === "input_ape")) {
               if (emptyError(form.input_ape)) {
                    handleError(form.input_ape, "Los apellidos son obligatorios");
                    someFieldFailed = true;
               }
               
               if (someFieldFailed){
                    return false;
               }
          }

          //Password
          if (!field || field.id === "input_password") {
              if (emptyError(form.input_password)) {
                  handleError(form.input_password, "La contraseña es obligatoria");
                  someFieldFailed = true;
              } else if (form.input_password.value.length < 4) {
                  handleError(form.input_password, "La contraseña es demasiado corta");
                  someFieldFailed = true;
              }

              if (someFieldFailed){
                  return false;
              }
          }

          //Password repeat  
          if (!field || field.id === "input_password_repeat") {
              if (form.input_password.value != form.input_password_repeat.value) {
                  handleError(form.input_password_repeat, "La contraseña y su confirmación no coinciden");
                  someFieldFailed = true;
              }

              if (someFieldFailed){
                  return false;
              }
          }

          //Phone
          if (!field || field.id === "phone"){
              if (emptyError(form.phone)) {
                   handleError(form.phone, "El teléfono es obligatorio");
                   someFieldFailed = true;
              } else if (!validateGeneralPhone(form.phone)){       
                   handleError(form.phone, "El teléfono es incorrecto");
                   someFieldFailed = true;
              } else if (!field){
                   that.sanitizePhoneNumber(form.phone)
              }
          
              if (someFieldFailed){
                   return false;
              }
          }

          //Job title
          /*if (!field || field.id === "title"){
               if (form.title.value === "default"){
                    handleError(form.title, "La profesión es obligatoria");
                    someFieldFailed = true;
               }

               if (someFieldFailed){
                    return false;
               }
          }*/

          //Sub Job Title (industry)
          /*if (!field || (field.id === "abogados" || field.id === "instituciones" || field.id === "profesionales" || field.id === "administracion") ){
               if (!field){
                    //submit
                    $.each(["#abogados","#instituciones","#profesionales","#administracion"],function(i,el){
                         if (that.isFieldDisplayed($(el)) && $(el).val()==="default"){
                              handleError(el, "El tipo de organización es obligatorio");
                              someFieldFailed = true;
                              return false;
                         }
                    })
               } else {
                    //changing field
                    if (that.isFieldDisplayed(field) && field.value === "default"){
                         handleError(field, "El tipo de organización es obligatorio");
                         someFieldFailed = true;
                    }
                    
               }

               if (someFieldFailed){
                    return false;
               }
          }*/

          //Main jurisdiction content selection
          if (!field || field.id === "input_pais"){
               if (form.input_pais.value === "default"){
                    handleError(form.input_pais, "La jurisdicción principal de interés es obligatoria");
                    someFieldFailed = true;
               }
          }

          return !someFieldFailed;
          /**********************************/
          /* END OF FIELDS ERRORS DETECTION */
          /**********************************/
     }


     //After DOM Content Load
     document.addEventListener("DOMContentLoaded", function(event) {

          that.show_industry= function(valor){
               if(valor == '1'){
                    clean_industry();
                    $('#abogados_div').css("display", "block");
                    //$("#abogados option[value='default']").attr("selected", "selected")
                    update_hidden1();

               } else if (valor == '5' || valor == '9' || valor == '2'){
                    clean_industry();
                    $('#instituciones_div').css("display", "block");
                    //$("#instituciones option[value='default']").attr("selected", "selected")
                    update_hidden1();

               //}else if (valor == 'Profesional no relacionado con el derecho'){
               //     clean_industry();
               //     $('#profesionales_div').css("display", "block");
               //     $("#profesionales option[value='default']").attr("selected", "selected")
               //     update_hidden1();
               //
               } else if (valor == '10'){
                    clean_industry();
                    $('#hidden1').val("15");
                    //$("#hidden1 option[value='default']").attr("selected", "selected")

               } else {
                    clean_industry();  
                    $('#hidden1').val("5");
               }
          }

          function clean_industry(){
               $(".field-group-block.industry").find(".validationError").remove()
               $(".field-group-block.industry").removeClass("error")

               $('#abogados_div').css("display", "none");
               $('#instituciones_div').css("display", "none");
               //$('#profesionales_div').css("display", "none");
               //$('#administracion_div').css("display", "none");

               $('#abogados').val("default");
               $('#instituciones').val("default");
               //$('#profesionales').val("default");
               //$('#administracion').val("default");
               
               $('#hidden1').val("default");
          }

          function update_hidden1(){
               $('#abogados').off("change").on("change", function(){
                    $("#hidden1").val($(this).val());
                    $(this).closest("div.field-group-block").find(".validationError").remove();
                    $(this).closest("div.field-group-block").removeClass("error");
                    that.validaSignup($("#signup-form")[0], this);
               })
               $('#instituciones').off("change").on("change", function(){
                    $("#hidden1").val($(this).val());
                    $(this).closest("div.field-group-block").find(".validationError").remove();
                    $(this).closest("div.field-group-block").removeClass("error");
                    that.validaSignup($("#signup-form")[0], this);
               })
               //$('#profesionales').off("change").on("change", function(){
               //     $("#hidden1").val($(this).val());
               //})
               //$('#administracion').off("change").on("change", function(){
               //     $("#hidden1").val($(this).val());
               //})
          }

     
          $("#signup-form").submit(function(){ 
               if (this && this.phone){
