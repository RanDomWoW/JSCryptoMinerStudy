    <script>
        jQuery(document).ready(function () {
            WalkThroughPopUp();
            //adds DIV ids to DIVs without ids. For UI testing
            jQuery.ajaxSetup({ cache: false });

            AFMS.GenerateControlIds.init()

            if (cartData != "" && cartData.FareMedias != null & cartData.FareMedias.length === 0) {
                jQuery('#minicartimage').hide();jQuery('#minicartimage-mobile').hide();
            }
            setSignInSignOut()

            function setSignInSignOut ()
            {
                if (jQuery('.miniCartMenu').attr('style') == 'display:none') {

                    jQuery('.navbar-brand').addClass('MobileLogo');
                    jQuery('.signInSignOut').addClass('signInright');

                } else {

                    jQuery('.navbar-brand').removeClass('MobileLogo');
                    jQuery('.signInSignOut').removeClass('signInright');
                }
            }
            //if order passed - hide the mini cart
            if(AFMS.hasOwnProperty("isOrderSuccess") && AFMS.isOrderSuccess)
            {
                jQuery("#minicartimage").hide();
                jQuery("#minicartimage-mobile").hide();
            }

        })

        var eligibleProducts= AFMS.Util.toJS(null);
        var customerLO = AFMS.Util.toJS(null);


        /*********WalkThrough Start******************/
        //Check if walkthrough cookie exist then do not show popup
        function WalkThroughPopUp()
        {
            var cname = "walkthrough";
            if(getWalkTroughCookie(cname).length >0)
            {
                jQuery(".Home_skipWalkThrough").click();
            }
        }

        //Deleting walkthrough cookie when sign out
        jQuery(".signInSignOut").click(function (){
            document.cookie = 'walkthrough=; expires=Thu, 01 Jan 1970 00:00:01 GMT;';
        });

        //Get walkthrough cookie
        function getWalkTroughCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1);
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

        /*********WalkThrough End******************/
    </script>

    <!--Accordion anchor tag solution -->
    <script type="text/javascript">
        jQuery(document).ready(function () {
            if(location.hash != null && location.hash != ""){
                jQuery('.collapse').removeClass('in');
                //jQuery(location.hash + '.collapse').collapse('show');
                var divtag = location.hash;
                if(typeof divtag != 'undefined' && divtag)
                {
                    var accodiondivIndex = divtag.indexOf("+");

                    var res = divtag.substring(1, accodiondivIndex);
                    if(res == 'ByE-mail')
                    {
                        res = 'EmailcollapseSecond'
                    }
                    if(typeof res != 'undefined' && res && accodiondivIndex > 0)
                    {
                        jQuery('div#' + res + '.collapse').collapse('show');
                        //alert('local' + res + 'hash ' + divtag + '++' + accodiondivIndex );
                    }

                }


            }

        });
    </script>

