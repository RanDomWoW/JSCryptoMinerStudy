<script type="text/javascript">
    function myfunc(msg) {
        var newresult;
        
        var afmsHostName = window.location.hostname;     //if ((msg == "English") || (msg == "français"))
        if ((msg == "english") || (msg == "français") || (msg == "English") || (msg == "Français") || (msg.toLowerCase() == "english") || (msg.toLowerCase() == "français") || (msg == "English [fr]") || (msg.toLowerCase() == "français")) {


            if (window.location.href.toLocaleLowerCase().indexOf(afmsHostName + "/en/") > -1) {
                newresult = noHomeUrl.replace(afmsHostName + "/en/", afmsHostName + "/fr-ca/");
            }
            else if (window.location.href.toLocaleLowerCase().indexOf(afmsHostName + "/fr-ca/") > -1) {
                newresult = noHomeUrl.replace(afmsHostName + "/fr-ca/", afmsHostName + "/en/");
            }
            else if (window.location.href.toLocaleLowerCase().split(afmsHostName + "/en")[1] === "") {
                newresult = noHomeUrl.replace(afmsHostName + "/en", afmsHostName + "/fr-ca/");
            }
            else if (window.location.href.toLocaleLowerCase().toLocaleLowerCase().split(afmsHostName + "/fr-ca")[1] === "") {
                newresult = noHomeUrl.replace(afmsHostName + "/fr-ca", afmsHostName + "/en/");
            }
            else {
                newresult = noHomeUrl.replace(afmsHostName, afmsHostName + "/" + (msg == "English" ? "en/" : "fr-ca/"));
            }
            window.location.replace(newresult);
        }
    }

    jQuery(document).ready(function () {        
        
        var afmsHostName = window.location.hostname;
        if (window.location.href.toLowerCase().indexOf(afmsHostName + "/en") > -1) {
            jQuery(".langClick").text("français");
            jQuery(".langClick").prop('lang','fr-ca');
        }
        if (window.location.href.toLowerCase().indexOf(afmsHostName + "/fr-ca") > -1) {
            jQuery(".langClick").text("english");
            jQuery(".langClick").prop('lang', 'en');
        }
    });

</script>

