<script>
jQuery(document).ready(function() {
	$.get('https://fb.wpxhosting.com/reviews.json', function(data) {
		if (Object.keys(data).length > 0) {
			var carouselHTML = '<div class="item active">';
			var old_counter;
			var i = 1;

			for (old_counter = 1; old_counter <= Object.keys(data).length; old_counter++) {
				var id = old_counter - 1;
				var datetime = new Date( data[id]['created_time'] );
				carouselHTML += '<div class="span6 slide-item">' +
					'<div class="quote">' +
						'<div class="quote-header">' +
							'<div class="picture">' +
								'<a href="https://facebook.com/' + data[id]['reviewer']['id'] + '" target="_blank">' +
									'<img src="https://graph.facebook.com/' + data[id]['reviewer']['id'] + '/picture?type=normal" class="img-responsive img-circle " alt="' + data[id]['reviewer']['name'] + '">' +
								'</a>' +
							'</div>' +
							'<div class="name">' + data[id]['reviewer']['name'] + '</div>' +
							'<div class="rating">' + data[id]['rating'] + ' &nbsp;<i class="fa fa-star"></i></div>' +
							'<div class="date">' + datetime.toDateString() + '</div>' +
						'</div>' +
						'<p>' + (data[id]['review_text'] ? readmore(data[id]['review_text']) : '') + '</p>' +
					'</div>' +
				'</div>';
				if (old_counter % 2 === 0 && old_counter < Object.keys(data).length) {
					carouselHTML += '</div><div class="item">';
					$(".carousel-indicators").append('<li data-target="#fbCarousel" data-slide-to="'+ i +'" ></li>');
					i++;
				}
			}

			carouselHTML += '</div>';
			$('#block-fb-reviews .carousel-inner').append(carouselHTML);

			$('.carousel').carousel();
		}
	});

	function readmore(str) {
