jQuery(window).load(function() {
	var registration_popups = [];
	
	for (var i = 0; i < registration_popups.length; i++)
		registration_popups[i]["shown"] = jQuery.cookie("registration_popup_shown_" + registration_popups[i]["nid"]);
	
	if (registration_popups.length) {
		var nid = parseInt(jQuery.cookie("registration_popup"));
		
		if (!nid || (nid < 0)) {
			if (!nid)
				nid = registration_popups[Math.floor(Math.random() * registration_popups.length)]["nid"];
			else {
				for (var i = 0; i < registration_popups.length; i++) {
					if (registration_popups[i]["nid"] == Math.abs(nid)) {
						nid = null;
						
						var j = i;
						
						do {
							j += 1;
							
							if (j >= registration_popups.length)
								j = 0;
								
							if (!registration_popups[j]["shown"]) {
								nid = registration_popups[j]["nid"];
								
								break;
							}
						}
						while (j != i);
						
						break;
					}
				}
			}
			
			if (nid)
				jQuery.cookie("registration_popup", nid, {path: "/"});
		}
		
		if (nid) {
			var rp = null;
			
			for (var i = 0; i < registration_popups.length; i++) {
				if (registration_popups[i]["nid"] == nid) {
					rp = registration_popups[i];
					
					break;
				}
			}
			
			if (rp) {
				var img = new Image();
				img.onload = function() {}
				img.src = rp["image"];

				var timeout = parseInt(jQuery.cookie("registration_popup_timeout"));
				
				if (!timeout)
					timeout = rp["show_timeout"];

				var timer_id = null;
				
				jQuery(".registration_div").find(".body_div").empty().append(rp["body_html"]);
				jQuery(".registration_div").find(".registration_body_div").css("background-color", rp["background_colour"]);
				jQuery(".registration_div").find(".registration_popup_thanks_container input").val(rp["nid"] + "|" + rp["thanks_page"]);
				jQuery(".registration_div").addClass("registration_div_" + rp["nid"]);

				var ouibounce_instance = ouibounce(document.getElementById("registration_div"), {
					sitewide:true,
					cookieExpire:-1, 
					sensitivity:(Math.floor(jQuery(window).height() / 4)),
					callback:function() {
						clearInterval(timer_id);
						timer_id = null;
						ouibounce_instance.disable({cookieExpire:-1});
						
						jQuery("body, #page").addClass("overflow_hidden");
						
						jQuery(".registration_div").scrollLeft(jQuery(".registration_div").offset().left - (jQuery(window).width() - jQuery(".registration_div").outerWidth(true)) / 2);
						
						var data = "nid=" + rp["nid"];	

						jQuery.ajax({
							url:Drupal.settings.basePath + "registration_popups_shown",  
							async:true,
							type:"GET",
							data:data,
							cache:false
						});
						
						var date = new Date();
						date.setTime(date.getTime() + rp["repeat_timeout"]);
						jQuery.cookie("registration_popup_shown_" + rp["nid"], 1, {expires: date, path: "/"});
						jQuery.cookie("registration_popup_timeout", 0, {path: "/"});
						jQuery.cookie("registration_popup", -rp["nid"], {path: "/"});
					}
				});
				
				if (isMobile()) {
					timer_id = setInterval(function() {
						timeout -= 1000;
						
						if (timeout <= 0) {
							clearInterval(timer_id);
							timer_id = null;
							ouibounce_instance.disable({cookieExpire:-1});
							
							jQuery(".registration_div").css("display", "block");
							
							jQuery("body, #page").addClass("overflow_hidden");
							
							jQuery(".registration_div").scrollLeft(jQuery(".registration_div").offset().left - (jQuery(window).width() - jQuery(".registration_div").outerWidth(true)) / 2);
							
							var data = "nid=" + rp["nid"];	

							jQuery.ajax({
								url:Drupal.settings.basePath + "registration_popups_shown",  
								async:true,
								type:"GET",
								data:data,
								cache:false
							});
							
							var date = new Date();
							date.setTime(date.getTime() + rp["repeat_timeout"]);
							jQuery.cookie("registration_popup_shown_" + rp["nid"], 1, {expires: date, path: "/"});
							jQuery.cookie("registration_popup_timeout", 0, {path: "/"});
							jQuery.cookie("registration_popup", -rp["nid"], {path: "/"});
						}	
						else
							jQuery.cookie("registration_popup_timeout", timeout, {path: "/"});
					}, 1000);
				}
				
				jQuery(".registration_div").find(".close_button").click(function() {
					jQuery("body, #page").removeClass("overflow_hidden");
					
					jQuery(".registration_div").css("display", "none");
				});	
			}
		}
	}
});</script>
