<script>
    $(document).ready(function () {
        $(document).on('click', 'a', function(event){
            if ($.attr(this, 'href')[0] === "#" && $.attr(this, 'href').length > 1) {
                event.preventDefault();

                $('html, body').animate({
                    scrollTop: $( $.attr(this, 'href') ).offset().top
                }, 500);
            }
        });
        var $nl = $("#news-listing");
        if ($nl.length) {
            var t = $nl.offset().top - $(window).scrollTop();
            $(window).scroll(function () {
                var d = t - $(window).scrollTop();
                console.log(t - $(window).scrollTop());
                if (d <= 60) {
                    $nl.addClass("yl-listing-fixed");
                }
                if (d > 60) {
                    $nl.removeClass("yl-listing-fixed");
                }
            });
        }
        var $imps = $("#impressions");
        var oneDay = 24*60*60*1000; // hours*minutes*seconds*milliseconds
        var firstDate = new Date(2017, 9, 27);
        var secondDate = new Date();
        var diffDays = Math.abs((firstDate.getTime() - secondDate.getTime())/(oneDay));
        var imps = Math.round(diffDays * 150000000 + 60321450472);
        $imps.text(imps);
        function incimp () {
            imps += Math.round(150 + Math.random() * 100);
            $imps.text(imps);
            window.setTimeout(incimp, 10);
        }
        incimp();
    });
</script>
