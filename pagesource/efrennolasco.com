article ol{
    list-style-type: none !important;
    margin-bottom: 10px;
    margin-left: 1em;
    padding: 0;
    counter-reset: li-counter;
}
article ol > li{
    position: relative;
    margin-bottom: 10px;
    padding-left: 0.5em;
    min-height: 3em;
}
article ol > li:before {
    position: absolute;
    top: 0;
    left: -1em;
    width: 0.8em;
     font-style: italic;
    font-size: 3em;
    font-weight: normal;
    text-align: right;
    color: #ddd;
 
    content: counter(li-counter);
    counter-increment: li-counter;
}
