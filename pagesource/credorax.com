<script language="javascript">
$(document).ready(function(){ 
  if (document.getElementById('splash')) {
    $('#splash').click(function(){
      $('#splash').hide();
      $('#header,#banner,#content,#footer').show();
      initHomePage();
    });
  }
  else {
    initHomePage();
  }
  return false;
});
		  
var initHomePage = function() {
  var bannerCurrent = 0;
  var bannerCount = $('#banner-slides').children().length;
  var bannerLock = false;
  var bannerTimer = null;
  var bannerTimeout = 8000;

  var bannerDots = document.getElementById('banner-dots');
  for ( var i = 0; i < bannerCount; i++ ) {
    bannerDots.innerHTML += '<a href="#"' + (i ? '' : ' class="active"') + '></a>';
  }
  
  var slideBanner = function(bannerNew){
    if (bannerLock) return;
    bannerLock = true;
    $('#banner-dots a:eq('+bannerCurrent+')').removeClass('active');
    $('#banner-slides div.banner-slide:eq('+bannerCurrent+'),#banner-slides div.banner-slide:eq('+bannerNew+')').toggle('fade', function(){
      $('#banner-dots a:eq('+bannerNew+')').addClass('active');
      bannerCurrent = bannerNew;
      bannerLock = false;
    });
  };
  
  var slideNextBanner = function(){
    slideBanner((bannerCurrent + 1) % bannerCount);
  };
  
  bannerTimer = setInterval(slideNextBanner, bannerTimeout);
  
  $('#banner-slides div:first-child').show();
  $('#banner-dots').on('click', 'a', function(){
    if (!this.classList.contains('active')){
      var bannerNew = $(this).index();
      slideBanner(bannerNew);
    }
    return false;
  });

  $('#banner-tab-left').click(function(){
    slideBanner((bannerCurrent + bannerCount - 1 ) % bannerCount);
    return false;
  });
  $('#banner-tab-right').click(function(){
    slideNextBanner();
    return false;
  });
  $('#banner-navigation').mouseenter(function(){
    if (bannerTimer) {
      clearInterval(bannerTimer);
      bannerTimer = null;
    }
  });
  $('#banner-navigation').mouseleave(function(){
    bannerTimer = setInterval(slideNextBanner, bannerTimeout);
  });


  var newsCurrent = 0;
  var newsCount = $('#homepage-news').children().length;
  var newsLock = false;
  var newsTimer = null;
  var newsTimeout = 5000;

  var updateNewsPager = function() {
    document.getElementById('homepage-news-pager-text').innerHTML =
      '<span class="active">' + (newsCurrent + 1) +'</span> of ' + newsCount;
  }
  var slideNews = function(n){
    if (newsLock) return;
    newsLock = true;
    $('#homepage-news div:eq('+newsCurrent+')').toggle('fade', function(){
      $('#homepage-news div:eq('+n+')').toggle('fade',function() {
        newsCurrent = n;
        updateNewsPager();
        newsLock = false;
      });
    });
  };
 
  var slideNextNews = function() {
	slideNews((newsCurrent + 1 ) % newsCount);
  };

  $('#homepage-news div:first-child').show();

  if (newsCount > 1) {
    newsTimer = setInterval(slideNextNews, newsTimeout);
  
    updateNewsPager();
    $('#homepage-news-left').click(function(){
      slideNews((newsCurrent + newsCount - 1 ) % newsCount);
      return false;
    });
    $('#homepage-news-right').click(function(){
          slideNextNews();
      return false;
    });
    $('#homepage-right-bottom').mouseenter(function(){
      if (newsTimer) {
        clearInterval(newsTimer);
        newsTimer = null;
      }
    });
    $('#homepage-right-bottom').mouseleave(function(){
      newsTimer = setInterval(slideNextNews, newsTimeout);
    });
  }
  else {
    $('.pager').hide();
  }

  
  var quotesCurrent = 0;
  var quotesCount = $('#quotes-list').children().length;
  var quotesLock = false;
  var quotesTimer = null;
  var quotesTimeout = 8000;

  var slideQuotes = function(n){
    if (quotesLock) return;
    quotesLock = true;
    $('#quotes-list div.testimonial:eq('+quotesCurrent+')').toggle('fade', function(){
      $('#quotes-list div.testimonial:eq('+n+')').toggle('fade',function() {
        quotesCurrent = n;
        quotesLock = false;
      });
    });
  };
 
  var slideNextQuotes = function() {
	slideQuotes((quotesCurrent + 1 ) % quotesCount);
  };

  $('#quotes-list div.testimonial:first-child').show();

  if (quotesCount > 1) {
    quotesTimer = setInterval(slideNextQuotes, quotesTimeout);
  
    $('#quotes-content div.left a').click(function(){
      slideQuotes((quotesCurrent + quotesCount - 1 ) % quotesCount);
      return false;
    });
    $('#quotes-content div.right a').click(function(){
      slideNextQuotes();
      return false;
    });
    $('#quotes-content').mouseenter(function(){
      if (quotesTimer) {
        clearInterval(quotesTimer);
        quotesTimer = null;
      }
    });
    $('#quotes-content').mouseleave(function(){
      quotesTimer = setInterval(slideNextQuotes, quotesTimeout);
    });
  }
};

</script>
