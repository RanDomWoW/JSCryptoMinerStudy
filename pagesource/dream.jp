<script type="text/javascript">

$(window).load(function(){

	// MyDTI、MyMailタブ切り替え
	$('ul.firstmenu li.mymail').click(function() {
		$('span',this).css('display','block');
		$('ul.firstmenu li.mydti span').css('display','none');
		$('ul.firstmenu li.mydti span').removeClass('active');
		$('ul.secondmenu li.mymail').css('display','block');
	});
	$('ul.firstmenu li.mydti').click(function() {
		$('span',this).css('display','block');
		$('ul.firstmenu li.mymail span').css('display','none');
		$('ul.firstmenu li.mymail span').removeClass('active');
		$('ul.secondmenu li.mymail').css('display','none');
	});

	$('li.mydti form input.id,li.mymail form input.id').focusout(function() {
		idtext = $(this).val();
		if (idtext != '') {
			$(this).addClass('id_focusout');
		} else {
			$(this).removeClass('id_focusout');
		}
    });

	$('li.mydti form input.password,li.mymail form input.password').focusout(function() {
		passwordtext = $(this).val();
		console.log(passwordtext);
		if (passwordtext != '') {
			$(this).addClass('password_focusout');
		} else {
			$(this).removeClass('password_focusout');
		}
    });

	//各要素の高さ合わせ
	$('.login_height').tile();
	$('.dtiinfo_height').tile();
	$('.servicemenu_height').tile();

	// 左右要素の高さ合わせ
	right_h = $('#toppage .toppage_servicemenu .left').outerHeight(true);
	service_h = $('#toppage .toppage_servicemenu .service').outerHeight(true);
	phonenumber_h = $('#toppage .toppage_servicemenu .inquiry_phonenumber_wrapper').outerHeight(true);
	left_pt = parseInt($('#toppage .toppage_servicemenu .left').css('padding-top'));
	calc_h = right_h - service_h - phonenumber_h - left_pt;
	$('#toppage .toppage_servicemenu .inquiry_phonenumber_wrapper').css('margin-top',calc_h+'px');

	//各サービスアイコンホバー時に白くする
	$('#toppage .toppage_servicemenu .left ul li').hover(function() {
		$('img',this).css('opacity','0.8');
		$('h2',this).css('text-decoration','underline');
		$('p',this).css('text-decoration','underline');
		$(this).css('cursor','pointer');
	},function() {
		$('img',this).css('opacity','1');
		$('h2',this).css('text-decoration','none');
		$('p',this).css('text-decoration','none');
	});

	//各サービス枠全体をクリックでリンクする
	$('#toppage .toppage_servicemenu .left ul li').click(function() {
		url = $('a:last',this).attr('href');
		document.location = url;
	});

	// スライドバナー開始
	var huga = $('.bxslider').bxSlider({
        controls: false,
		auto: true,
		useCSS: false,
		pause: 6000,
		speed: 1200,
        onSlideAfter: function(){
            huga.startAuto();
        }
    });

});
