function initSlider(){
	var slider = $("#search-form .getSlider");
	var output = $("#search-form .output");
	var theValue = slider.val();
	//무관 check
	$("#search-form .nomatt").click(function(){
		if(this.checked){
			slider.attr('disabled','disabled');
			output.attr('disabled','disabled');
			output.css('color','#ebebe4;');
			slider.addClass("noneslider");
		}else{
			slider.removeAttr('disabled');	
			output.removeAttr('disabled');
			output.css('color','#545454;');
			slider.removeClass("noneslider");
		}
	})
	var ms_ie = false;
    var ua = window.navigator.userAgent;
    var old_ie = ua.indexOf('MSIE ');
    var new_ie = ua.indexOf('Trident/');

    if ((old_ie > -1) || (new_ie > -1)) {
        ms_ie = true;
    }

    if ( ms_ie ) {
		slider.removeClass(slider);
    }

    if(theValue == 0) $("#search-form .nomatt").trigger('click');
	
}

function initSliderMobile(){
	var slider = $("#search-form-mobile .getSlider");
