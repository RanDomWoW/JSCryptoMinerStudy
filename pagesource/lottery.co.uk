	<script type="text/javascript">
	<!--
		//SMOOTH SCROLLING WITHOUT JQUERY
		
		function yPos() {
			if (self.pageYOffset) return self.pageYOffset;
			if (document.documentElement && document.documentElement.scrollTop) return document.documentElement.scrollTop;
			if (document.body.scrollTop) return document.body.scrollTop;
			return 0;
		};
		
		function elYPos(eID) {
        	var elm = document.getElementById(eID), y = elm.offsetTop, node = elm;
        	while (node.offsetParent && node.offsetParent != document.body) {
            	node = node.offsetParent;
            	y += node.offsetTop;
        	} return y;
    	};

		function smoothScroll(eID) {
			var startY = yPos();
			var stopY = elYPos(eID);
			var distance = stopY > startY ? stopY - startY : startY - stopY;
			if (distance < 100) {
				scrollTo(0, stopY); return;
			}
			var speed = Math.round(distance / 100);
			if (speed >= 20) speed = 20;
			var step = Math.round(distance / 25);
			var leapY = stopY > startY ? startY + step : startY - step;
			var timer = 0;
			if (stopY > startY) {
				for ( var i=startY; i<stopY; i+=step ) {
					setTimeout("window.scrollTo(0, "+leapY+")", timer * speed);
					leapY += step; if (leapY > stopY) leapY = stopY; timer++;
				} return;
			}
			for ( var i=startY; i>stopY; i-=step ) {
				setTimeout("window.scrollTo(0, "+leapY+")", timer * speed);
				leapY -= step; if (leapY < stopY) leapY = stopY; timer++;
			}
		}


		//Powerball Mini Banner Slide Down
		function slideDown(){
			var close = document.getElementById("close-promo");
			var open = document.querySelector(".promoStrap");
			open.classList.add("active");
			close.addEventListener("click", function(){
	    		open.classList.remove("active");
	    		setTimeout("document.querySelector('.promoStrap').style.display = 'none'",500);
	    		var cookieExpire = new Date();
		  		cookieExpire.setDate(cookieExpire.getDate() + 1);
		  		var cookieValue	= escape("promoStrap") + ( (1 == null) ? "" : "; expires="+cookieExpire.toUTCString()+"; path=/" );
		  		document.cookie = "promoStrap" + "=" + cookieValue;
	    	});
		}
		addLoadEvent(function(){slideDown()});

		
		
