    setCarouselHeight('#comment-carousel');

    function setCarouselHeight(id) {
        var slideHeight = [];
        $(id + ' .item').each(function () {
            // add all slide heights to an array
            slideHeight.push($(this).height());
        });

        // find the tallest item
        max = Math.max.apply(null, slideHeight);

        // set the slide's height
        $(id + ' .carousel-content').each(function () {
            $(this).css('height', max + 'px');
        });
    }

    if ($('#tabrizGooglemap').length) {
        var tabrizlatitude = $('#tabrizGooglemap').data('latitude');
        var tabrizlongitude = $('#tabrizGooglemap').data('longitude');

        var tehranlatitude = $('#tehranGooglemap').data('latitude');
        var tehranlongitude = $('#tehranGooglemap').data('longitude');

        function initialize_map() {
            var tabrizLatlng = new google.maps.LatLng(tabrizlatitude, tabrizlongitude);
            var tabrizmapOptions = {
                zoom: 15,
                scrollwheel: false,
                center: tabrizLatlng
            };

            var tehranLatlng = new google.maps.LatLng(tehranlatitude, tehranlongitude);
            var tehranmapOptions = {
                zoom: 15,
                scrollwheel: false,
                center: tehranLatlng
            };
            var tabrizmap = new google.maps.Map(document.getElementById('tabrizGooglemap'), tabrizmapOptions);
            var tehranmap = new google.maps.Map(document.getElementById('tehranGooglemap'), tehranmapOptions);

            var tabrizinfowindow = new google.maps.InfoWindow({
                content: '<div class="map-content"><ul class="address">' + $('.contact-us .address.tabriz').html() + '</ul></div>'
            });
            var tehraninfowindow = new google.maps.InfoWindow({
                content: '<div class="map-content"><ul class="address">' + $('.contact-us .address.tehran').html() + '</ul></div>'
            });
            var tabrizmarker = new google.maps.Marker({
                position: tabrizLatlng,
                map: tabrizmap
            });
            var tehranmarker = new google.maps.Marker({
                position: tehranLatlng,
                map: tehranmap
            });
            google.maps.event.addListener(tabrizmarker, 'click', function () {
                tabrizinfowindow.open(tabrizmap, tabrizmarker);
            });

            google.maps.event.addListener(tehranmarker, 'click', function () {
                tehraninfowindow.open(tehranmap, tehranmarker);
            });
        }
        google.maps.event.addDomListener(window, 'load', initialize_map);
    }

