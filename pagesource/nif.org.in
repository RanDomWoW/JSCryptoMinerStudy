       		<script>
		    $(function () {

		    	var errors=[];

		    	// allow only letters 
                $('.stronlytext').keydown(function (e) {
                  if (e.altKey) {
                      e.preventDefault();
                  } else {
                      var key = e.keyCode;
                      if (!((key == 9) || (key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90))) {
                          e.preventDefault();
                      }
                  }
                });

                // on select parent and guardian
                $('.sel_parent').change(function(){
                	var selectedRadio = $(this).val();
                	if(selectedRadio==='parent'){
                	    
                		$('.sel_tmp_input').hide();
                		$('.addrelationship').hide();    
                		
                		$('.sel_parent_rel').show();
                		$('.sel_parent_rel').prop("required",true);
                		
                		$('.addrelationship').prop('required',false);
                		$('.addrelationship').hide();

                	} else if (selectedRadio==='guardian'){
                		$('.sel_tmp_input').hide();
                		$('.sel_parent_rel').prop('required',false);
                		$('.sel_parent_rel').hide();
                		
                		$('.addrelationship').show();                		
                		$('.addrelationship').prop('required',true);                		
                	}
                });

                $('.str_aadhar').on('keydown keyup blur change',function(){

					var $this = $(this);
				    var val = $this.val();
				    var valLength = val.length;
				    var maxCount = 16;
				    if(valLength>maxCount){
				        $this.val($this.val().substring(0,maxCount));
				    }

                	var adharno = $('.str_aadhar').val();
                	if(adharno.length < 12){
                		$('.adharerror').html('Not a valid adhar no.');
                		errors['adhar']='not a valid adhar no';                		
                	} else if(adharno.length > 16){
                		$('.adharerror').html('Not a valid adhar no.');
                		errors['adhar']='not a valid adhar no';                		
                	} else if(adharno.length > 11 && adharno.length < 17 ) {
                		$('.adharerror').html('');
                		return true;
                	}	
                });

                $('.str_pincode1').on('keydown keyup blur change',function(){
                    
					var $this = $(this);
				    var val = $this.val();
				    var str_pincode1 = $this.val();
				    
				    //if(val!=""){
				      //  var action="verifypincode";
				        //$.ajax({
				          //  url:'hiren_ajax.php',
				            //type:'POST',
				            //data:'action='+action+'&pincode='+val,
				            //success:function(data){
				                    
				            //}
				            
				        //});        
				    //}
				    
				    
				    var valLength = val.length;
				    var maxCount = 6;
				    if(valLength>maxCount){
				        $this.val($this.val().substring(0,maxCount));
				    }
                	
                	if(str_pincode1.length < 6){
                		$('.pincoderror1').html('Not a valid pincode');
                		str_pincode1.slice(0, 6);
                		errors['pincode1']='not a valid pincode';                		
                	} else if(str_pincode1.length > 6){
                		$('.pincoderror1').html('Not a valid pincode');
                		errors['pincode1']='not a valid pincode';                		
                		str_pincode1.slice(0, 6);
                	} else if(str_pincode1.length == 6) {
                		$('.pincoderror1').html('');
                		return true;
                	}	
                });

                $('.str_pincode2').on('keydown keyup blur change',function(){

					var $this = $(this);
				    var val = $this.val();
				    var valLength = val.length;
				    var maxCount = 6;
				    if(valLength>maxCount){
				        $this.val($this.val().substring(0,maxCount));
				    }

                	var str_pincode2 = $('.str_pincode2').val();
                	if(str_pincode2.length < 6){
                		$('.pincoderror2').html('Not a valid pincode');
                		errors['pincode2']='not a valid pincode';                		
