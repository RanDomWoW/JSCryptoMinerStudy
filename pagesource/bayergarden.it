            site_tree_lvl2: '',  //site_tree_lvl2
            site_tree_lvl3: '',  //site_tree_lvl3
            site_tree_lvl4: '',
            site_tree_lvl5: '',

            site_tree: 'Frontpage::',

            segment: 'Frontpage',

            content_type: 'Frontpage',  //content_type << We don't know that!
            tags: '',  //tags

            filter_category: '',  //filter_category
            filter_brand: '',  //filter_brand
            form_city: '',  //form_city
            form_postal_code: '',  //Form_Postal_Code
            form_country: '',  //Form_Country

            error_label: '',  //error_label << if we hit a 404?

            clickzone_model: '1',  //clickzone_model
            clickzone_trigger: '1', //clickzone_trigger

            search_page: 0,
            search_keyword: '',
        };
        
        if (tc_vars.search_keyword.length > 0) {
            tc_vars.np = $('.pagination .active a').text();

            if (tc_vars.np.length === 0) {
                tc_vars.np = 1;
            }
        }

        tc_vars.xt_page = tc_vars.site_tree + tc_vars.page_name;

        window.tc_vars = tc_vars;
    </script>
    <script type="text/javascript">
        // tagcommander tracking
        (function () {
            // virtual page event for store seaarch
            $('#storeSearch').on('change', function () {
                var that = $(this);
                setTimeout(function () {
                    tc_vars["location"] = $(that).val();
                    // timeout to get autocompleted value
                    logger('click', tc_vars.site_tree + $(that).val(), 'N');
                }, 400);
            });

            // virtual page event for clicking left store item
            $('.store-locator').on('click', '.item', function () {
                //logger('Click left menu store', tc_vars.xt_page + "::" + window.tc_vars["location"] + "::" +  $(this).find('h2').text(), 'A');
                logger('click', tc_vars.xt_page + "::" + window.tc_vars["location"] + "::" + $(this).find('h2').text(), 'A');
            });
            // virtual page event if store pin was clicked on map
            $('#map-canvas').on('click', function () { 
                if (!window.storeisClicked) {
                    //logger('Click store pin on the map', tc_vars.xt_page + "::" + window.tc_vars["location"] + "::" + window.storeName, 'A');
                    logger('click', tc_vars.xt_page + "::" + window.tc_vars["location"] + "::" + window.storeName, 'A');
                }
            });

            var doClick = function () {
                var $this = $(this),
                    eventLabel = getEventLabel($this),
                    eventType = getEventType($this),
                    generalType = 'click';

                if ($this.parents('.product-filters').length > 0 /*|| $this.parents('').length > 0*/) {
                    generalType = 'page';
                    eventLabel = $this.text().trim();

                    if (eventLabel.length === 0) {
                        eventLabel = $this.siblings('.product-filter-label').text().trim();
                    }

                    eventLabel = tc_vars.site_tree + eventLabel;

                    eventType = 'N';
                }

                logger(generalType, eventLabel, eventType);
            };

            $('.typeahead, .search-field').on('change', doClick);

            // Attach click functions
            // Dynamic click event for tab slider items
            /**
             * Pinterest: a with data-pin-href or data-pin-log
             */
            $('body').on('click', '.scfSubmitButtonBorder input, ' +
                                  '.product-filter,' +
                                  '.main-image .play,' +
                                  '[data-pin-href],' +
                                  '#print-page,' +
                                  '#mail-page,' +
                                  '.yass-nav-next,' +
                                  '.yass-nav-prev,' +
                                  '.yass-paging li,' +
                                  '.flex-next,' +
                                  '.flex-prev,' +
                                  '.flex-control-nav li,' +
                                  '.tab-slider .item', doClick);
            // General a click event see logger function
            $('a').on('click', doClick);

            var getEventLabel = function (that) {
                var a = window.location.href,
                    b = 'content',
                    c = that.text().trim();

                /* Determine b value */
                if (that.parents('footer').length > 0) {
                    b = 'footer';
                }
                if (that.parents('header').length > 0) {
                    b = 'header';
                }

                /* Determine c value */

                // if search button (no text)
                if (that.hasClass('search-button') ||
                    that.hasClass('typeahead') ||
                    that.hasClass('search-field')) {
                    c = 'search';
                }

                // Product pages with related products
                if (that.hasClass('yass-nav-next') ||
                    that.hasClass('flex-next')) {
                    c = 'next';
                }
                if (that.hasClass('yass-nav-prev') ||
                    that.hasClass('flex-prev')) {
                    c = 'previous';
                }
                if (that.parent().hasClass('yass-paging') ||
                    that.parent().hasClass('flex-control-nav')) {
                    c = 'more-products';
                }

                // Social interactions
                if (that.attr('data-pin-href') != undefined) {
                    c = 'pinterest';
                }

                // Print and mail
                if (that.is('#print-page')) {
                    c = 'print';
                }
                if (that.is('#mail-page')) {
                    c = 'mail';
                }

                // Video
                if (that.is('.play')) {
                    c = 'video-play';
                }

                // Search result and related product with a lot of content wrapped inside the a tag
                if (that.parents('.search-result, .yass-content').length > 0) {
                    c = that.find('.header, .headline, h3').text().trim();
                }

                // Isotope items have 3 a tags - we need the h3 value for all 3 events
                if (that.parents('.isotope-item').length > 0) {
                    c = that.parents('.isotope-item').find('h3 a, h2 a').text().trim();
                }

                if (that.parent('.scfSubmitButtonBorder').length > 0) {
                    c = 'submit';
                }

                // Last way out if no value is present
                if (!c) {
                    c = that.attr('href');
                }

                return a + '::' + b + '::' + c;
            };

            var getEventType = function (that) {
                var eventType = 'N',
                    href = that.attr('href');

                // If not an a tag - return N
                if (!that.is('a')) {
                    return eventType;
                }

                // S : Other website (target blank)
                // Important to do this before A event type, since A type doesn't have the same host
                if (that.prop('host') !== location.host) {
                    eventType = 'S';
                }
                
                if (href != undefined && href.length > 0) {
                    // T : Download
                    if (href.lastIndexOf('/~/media/', 0) === 0) {
                        eventType = 'T';
                    }

                    // A : Not linking to new page - action on same page
                    if (href.indexOf('#', 0) === 0 || href.indexOf('javascript:', 0) === 0) {
                        eventType = 'A';
                    }
                }

                return eventType;
            };

            var logger = function (generalType, eventLabel, eventType) {
                var tc_vars = window.tc_vars;
                console.log(' generalType:' + generalType + ' eventLabel: ', eventLabel, ' eventType: ', eventType);

                var event = {};
                /*
                In order to determine values of “event_label”, the following rules apply : 
                  To be able to uniquely identify the clicks and as it’s apparently not possible to specifically define the CTA / INTEREST / ACTION 
                values we required, we suggest the following format: [A]::[B]::[C], which should be set following those rules :
                o   [A]: Current page path (page url without the domain) 
                o   [B]: ’footer’ or ‘header’ or ‘content, following were the click happened 
                o   [C]: text link and / or targetUrl following what is available
                */
                event.event_label = eventLabel;
                /*
                The id_site2_at value                    
                */
                event.event_lvl2 = tc_vars.id_site2_at;
                /*
                Fixed value, C for click event, P for page event
                */
                event.event = generalType.split('')[0].toUpperCase();
                /*
                In order to determine values of “event_type”, the following rules apply : 
                  ‘T’: if the target url contains a file name (example : “xxxxxxx.pdf”)
                  ‘S’: if the target url contains a domain name that is not the current domain 
                  ‘A’: the click is not leading the visitor to another page but is more like an action on the page
                  ‘N’: All other links (supposed to lead the user to another page of the current website)
                How do we differentiate between A and N: IF it’s an <A href/> tag, then it’s N, if not, then it’s an A.
                */

                event.event_type = eventType;
                
                window.tc_events_1(this, generalType, event);
            };

            // virtual page event for form submit
            var form = $('body').find('form .scfForm');
            if (form.length > 0) {

                // when the form is submitted no submitButton exists
                if (form.find('.scfSubmitButton').length === 0) {
                    logger('page', tc_vars.site_tree + tc_vars.page_name + '::validated', 'A');
                }

                // when the form is loaded and the scfValidationSummary exists, an error has occured
                if (form.find('.scfValidationSummary').length > 0) {
                    logger('page', tc_vars.site_tree + tc_vars.page_name + '::validate', 'A');
                }
            };

            var searchResults = $('.search-result').children('div').text().replace(/\D/g, '');
            if (searchResults.length > 0) {
                tc_vars.search_page = searchResults;
            }
        })();
    </script>
