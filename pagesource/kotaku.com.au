<!-- Header template -->
<script id="template-article-header" type="text/x-handlebars-template">
    
	<div class="posts__post-item-header">

		{{#if canonical_url}}
	        <div class="posts__post-item-shared-from">
	            <span class="dashicons dashicons-update"></span>Shared from <a href="{{canonical_url}}">{{canonical_publication}}</a>
	        </div>
		{{/if}}

		<div class="posts__post-item-header-left">
			<h1 class="headline" itemprop="headline">
				<a href="{{url}}" itemprop="url"><span itemprop="headline">{{ this.title}}</span></a>
			</h1>
			<div class="meta">
				<div class="meta__avatar">
					{{#if avatar}}
						{{{avatar}}}
					{{/if}}
				</div>
				<div class="meta__byline" itemprop="author">
					{{#if one_author}}
						<a href="{{author_url}}">{{author_name}}</a>
					{{else}}
						{{author_name}}
					{{/if}}
				</div>
				<time class="meta__post-time" itemprop="datePublished">
					{{date}}
				</time>
			</div>
		</div>

		<div class="posts__post-item-header-right">
			{{#if display_comments}}
				{{#if comment_status}}
			<span class="comment-count-wrapper">
				<a href="{{url}}#article-comments">
					<i class="fa fa-comment-o" aria-hidden="true"></i> {{comment_count}}
				</a>
			</span>
				{{/if}}
			{{/if}}
		</div>

	</div>
</script>
<!-- END: Header template -->

