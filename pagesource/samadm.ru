    <script>

        ymaps.ready(init);

        function init() {
        var myMap = new ymaps.Map('trafficMap', {
            center: [53.195, 50.102],
            zoom: 10,
            controls: []
        });

        var ap = new ymaps.traffic.provider.Actual();
        ap.setMap(myMap);

        var stateMonitor = new ymaps.Monitor(ap.state);
        stateMonitor.add('level', function (newValue) {
            if ((typeof newValue != 'undefined') && (newValue != null)) {
            //                    console.log(newValue);
            if (newValue == 0) {
                $('.road .hidden-xs').html('<img src="/local/templates/samara/img/svet-green.png">' + newValue + ' баллов');
            } else if (newValue == 1) {
                $('.road .hidden-xs').html('<img src="/local/templates/samara/img/svet-green.png">' + newValue + ' балл');
            } else if ((newValue > 1) && (newValue < 4)) {
                $('.road .hidden-xs').html('<img src="/local/templates/samara/img/svet-green.png">' + newValue + ' балла');
            } else if (newValue == 4) {
                $('.road .hidden-xs').html('<img src="/local/templates/samara/img/svet-yellow.png">' + newValue + ' балла');
            } else if ((newValue > 4) && (newValue <= 6)) {
                $('.road .hidden-xs').html('<img src="/local/templates/samara/img/svet-yellow.png">' + newValue + ' баллов');
            } else {
                $('.road .hidden-xs').html('<img src="/local/templates/samara/img/svet-red.png">' + newValue + ' баллов');
            }
            ;
            }
        });

        } 
    </script>

