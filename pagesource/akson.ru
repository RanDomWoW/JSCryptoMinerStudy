						$(".sticky_menu_main_div").addClass("sticky_display");


						$('.menuLineWrap').addClass('sticky_catalog_menu sticky_box_shadow').removeClass('new_ng_header_menu_holder');

						/*if($('.sticky_search').width() <= 120) {
							$('.sticky_search').animate({'width' : '420px'}, 200, function() {

								if($('.sticky_menu_main_div').hasClass('sticky_display')) {
									searchFix();
								}

							}).css({"opacity" : "0"});
						}*/

						searchFix();
						$('.sticky_search').css({"opacity" : "0"})

					} else {

						searchFixRemove();

	//					$('.sticky_search').css({"width" : "90px"}).css({"opacity" : "1"});
						$('.sticky_search').css({"opacity" : "1"});

						$(".sticky_menu_main_div").removeClass("sticky_display nobox_shadow");

						$('.menuLineWrap').removeClass('sticky_catalog_menu sticky_box_shadow').attr('style', '').addClass('new_ng_header_menu_holder');

						$('.stikcy_arrow_up').attr('style', '');


					}


				} else {
					searchFixRemove();
					$('.sticky_search').css({"opacity" : "1"});

					$(".sticky_menu_main_div").removeClass("sticky_display nobox_shadow");

					$('.menuLineWrap').removeClass('sticky_catalog_menu sticky_box_shadow').attr('style', '');

					$('.stikcy_arrow_up').attr('style', '');

				}
			});
		
		$(document).on('click', '.sticky_burger', function () {

			if($('.menuLineWrap').hasClass('sticky_catalog_menu')) {

				if( $('.menuLineWrap').is(':visible')) {

					$('.stikcy_arrow_up').fadeOut();

					$('.menuLineWrap').slideUp(300, function () {
						$(".sticky_menu_main_div").removeClass("nobox_shadow");
					});
				} else {
					$(".sticky_menu_main_div").addClass("nobox_shadow");
					$('.menuLineWrap').slideDown(300, function(){
						$('.stikcy_arrow_up').fadeIn(100);
					});
				}
			}

		});

		function searchFix() {

			$('.boxSearch').hide();

			$('.boxSearch').addClass('boxSearch_fix');
			$('.formSearch').addClass('formSearch_fix');
			$('.buttomSearch').addClass('buttomSearch_fix');
			$('.ac_results').addClass("ac_results_fix");
			$('.top-basket-line').addClass('top-basket-line_fix');

			var cssValue = (($(document).width() - 1180) / 2) + $('.sticky_search').position().left;

			var cssBaskect = (($(window).width() - 1180) / 2) + 15;

			$('.boxSearch').css({"left" : cssValue});
			$('.top-basket-line').css({"right" : cssBaskect});

			$('.boxSearch').show();
		}

		function searchFixRemove() {

			$('.boxSearch').hide();
			$('.boxSearch').removeClass('boxSearch_fix');
			$('.formSearch').removeClass('formSearch_fix');
			$('.buttomSearch').removeClass('buttomSearch_fix');
			$('.top-basket-line').removeClass('top-basket-line_fix');

			$('.boxSearch').css({"left" : "auto"});
			$('.top-basket-line').css({"right" : "auto"});

			if($('.ac_results').hasClass("ac_results_fix")) {
				$('.ac_results').removeClass("ac_results_fix").hide();
			}

			$('.boxSearch').show();
		}

		function updateFavsCount() {
			if($('.sticky_favs_counts').length > 0) {
				$.ajax({
					url: '/ajax/favorites/getCountFavorites.php',
					type: 'POST',
					dataType: 'json',
					cache: false,
					success: function(data){
						if (data.count == 0) {
							$('.sticky_favs_counts').hide();
						}
						$('.sticky_favs_counts').text(data.count);
					}
				});
			}
		}

	});
</script>


