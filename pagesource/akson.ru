<script>
	$(document).ready(function() {
		$('#errorFeedbackPageURL').val(document.location.href);

		$(document).on('click', '.errorFeedbackOpener, .errorFeedbackFooterButton', function() {
			$('.errorFeedbackTitle').text('Сообщить об ошибке');
			$('#errorFeedbackForm').show();
			$('#errorFeedbackAfterSendText').hide();
			$('.errorFeedbackInput').val('');
			$('#errorFeedbackUploaderImgPath').val('');
			$('.errorFeedbackUploader').text('Приложить файл');
			$.fancybox.open({
				'type' : 'html',
				'content' : $('.errorFeedbackContainer'),
				'wrapCSS' : 'sq-style gray-cross'
			}, {
				helpers: {
					overlay: {
						locked: false
					}
				}
			});
		});

		$(document).on('click', '.errorFeedbackSendButton', function() {
			if ($('#errorFeedbackComment').val().length !== 0) {
				$.ajax({
					type: "POST",
					url: '/ajax/new_error_feedback.php',
					data: $('#errorFeedbackForm').serialize(),
					success: function(data) {
						if (data === 'ok') {
							$('.errorFeedbackTitle').text('Спасибо за содействие!');
							$('#errorFeedbackForm').hide();
							$('#errorFeedbackAfterSendText').show();
						}
					}
				});
				/*$('.errorFeedbackTitle').text('Спасибо за содействие!');
				$('#errorFeedbackForm').hide();
				$('#errorFeedbackAfterSendText').show();*/
			} else {
				alert('Заполните поле "Комментарий"!');
			}
		});

		$(document).on('click', '.errorFeedbackUploader', function () {
			if (!$('.errorFeedbackUploader').hasClass('disabledUploader')) {
				$('#errorFeedbackUploaderInput').click();
			}
		});

		var fileName = '';

		$(document).on('change', '#errorFeedbackUploaderInput', function() {
			$('.errorFeedbackUploader').addClass('disabledUploader');

			event.stopPropagation();
			event.preventDefault();

			if (this.files && this.files[0]) {
				$('.errorFeedbackUploader').addClass('errorFeedbackLoader');
				console.log(this.files[0].name);
				fileName = this.files[0].name;
				if (fileName.length > 25)
					fileName = fileName.substr(0, 23)+'...';
				if (this.files[0].size < 6291456) {
					uploadReviewImages(this.files);
				}
			} else {
				$('.errorFeedbackUploader').text('Приложить файл');
			}
			setTimeout(function() {
				$('.errorFeedbackUploader').removeClass('disabledUploader');
			}, 3000)
		});

		function uploadReviewImages (object) {

			var files = object;

			var fileNameSufix = "suffix";

			if(files.length === 0)
				return false;

			var formData = new FormData();

			$.each( files, function( key, value ){
				formData.append( 'file['+key+']', value );
			});

			var imgMunber = '1';

			formData.append('file_id', imgMunber);
			formData.append('product_id', "1");
			formData.append('fileNameSuffix', fileNameSufix);

			$.ajax({
				url: "/ajax/imageUploaderErrorsCallback.php",
				type: "POST",
				contentType: false,
				processData: false,
				cache: false,
				data: formData,
				dataType: 'json',
				xhr: function(){
					var xhr = $.ajaxSettings.xhr();
					xhr.upload.addEventListener('progress', function(evt){
						if(evt.lengthComputable) {

						}
					}, false);
					return xhr;
				},
				success: function(respond, textStatus, jqXHR) {
					if( typeof respond.error === 'undefined' ) {
						$('#errorFeedbackUploaderImgPath').val(respond.files[0]);
						if (fileName !== '') {
							$('.errorFeedbackUploader').text(fileName);
						}
						$('.errorFeedbackUploader').removeClass('errorFeedbackLoader');
					}
					else{
						$('.errorFeedbackUploader').text(respond.error);
						$('.errorFeedbackUploader').removeClass('errorFeedbackLoader');
						$('.errorFeedbackUploader').removeClass('disabledUploader');
						console.log('ОШИБКИ ОТВЕТА сервера: ' + respond.error );
					}
				},
				error: function( jqXHR, textStatus, errorThrown ){
					console.log('ОШИБКИ AJAX запроса: ' + textStatus );
				}
			});
		}
	});
</script>			

			
