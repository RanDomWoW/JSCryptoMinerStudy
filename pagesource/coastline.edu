        <script type="text/javascript">
            $(document).ready(function()
            {
                var $logoContainer = $(".logo-container");
                // setTimeout(function(){$logoContainer.addClass("scale");},4000);
                CCCUtil.WindowWatcher.watchScroll(function(top)
                {
                    if(top < 10){if($logoContainer.hasClass("scale")){$logoContainer.removeClass("scale");}}
                    else{if(!$logoContainer.hasClass("scale")){$logoContainer.addClass("scale");}}
                });

                (function(w)
                {
                    function Toggle()
                    {
                        this.actions = {};
                        this.hookActions = [];
                        this.enabled = true;
                        this.roots = [];
                        this.autohide = true;

                        this.hook = function(root)
                        {
                            this.roots.push(root);

                            root.context = {
                                "enabled":this.enabled,
                                "autohide":this.autohide,
                                "parent":root,
                                "actions":{},
                                "run":function(action, subject)
                                {
                                    if(subject === null){return;}
                                    for(var i = 0; i < root.context.actions[action].length; i++)
                                    {
                                        if(root.context.actions[action][i](root.context, subject) === false){break;}
                                    }
                                }
                            };

                            for(var i in this.actions) // attach actions
                            {
                                root.context["actions"][i] = [];

                                for(var j = 0; j < this.actions[i].length; j++)
                                {
                                    root.context["actions"][i].push(this.actions[i][j]);
                                }
                            }

                            for(var i = 0; i < this.hookActions.length; i++)
                            {
                                if(this.hookActions[i](root.context) === false){break;}
                            }
                        }
                    }
                    w.Toggle = Toggle;

                    // Mega Menu Controller
                    // ______________________________________________________________________________________________________
                    var MegaMenuController = new Toggle();

                    MegaMenuController.hookActions.push(function(context)
                    {
                        context["current"] = null;
                        context["controls"] = {};
                        context["firstchild"] = {};
                        context["panels"] = {};
                        context["mega-panel"] = $(context.parent).find(".mega-panel")[0];
                        context["timestamp"] = 0;
                        context["ttl"] = 250;
                        context["ttl-hover"] = 250;
                        context["timeout"] = function()
                        {
                            var ts = (new Date()).getTime();
                            var diff = ts - context.timestamp;
                            if(diff > context.ttl)
                            {
                                context.timestamp = ts;
                                return false;
                            }
                            return true;
                        };


                        if(context.autohide === true)
                        {
                            $(document).on("click touchstart", function(event)
                            {
                                e = event || window.event;
                                var current = e.target;
                                
                                while(current !== null)
                                {
                                    if(current === context.parent){return;}
                                    current = current.parentNode;
                                }
                                context.run("hide", context.current);
                            });
                            
                        }

                        context.actions["toggle"] = [function(context, name)
                        {
                            if(context.current != name){context.run("show", name);}
                            else{context.run("hide", name);}
                        }];

                        context.actions["show"] = [function(context, name)
                        {
                            if(context.current == name){return;}
                            if(context.current !== null){context.run("hide", context.current);}

                            $(context["mega-panel"]).addClass(name);
                            $(context.controls[name]).addClass("showing");
                            context.controls[name].setAttribute("aria-expanded", "true");
                            context.current = name;
                        }];

                        context.actions["hide"] = [function(context, name)
                        {
                            if(context.current === null){return;}
                            $(context["mega-panel"]).removeClass(context.current);
                            $(context["controls"][context.current]).removeClass("showing");
                            context.controls[name].setAttribute("aria-expanded", "false");
                            context.current = null;
                        }];

                        context.actions["hover"] = [function(context, name)
                        {
                            if(context.current == name){return;}
                            if(context.currrent !== null){context.run("hide", context.current);}
                            context.run("show", name);
                        }];

                        $(context.parent).find("[cccmm-control]").each(function()
                        {
                            var name = this.getAttribute("cccmm-control");

                            context["controls"][name] = this;
                            context["panels"][name] = $(context.parent).find(this.getAttribute("cccmm-control-selector"))[0];
                            
                            this.setAttribute("role","button");
                            this.setAttribute("aria-haspopup","true");
                            this.setAttribute("aria-expanded", "false");
                            this.setAttribute("aria-controls", "cccmm-" + name + "-panel");
                            context.panels[name].setAttribute("id", "cccmm-" + name + "-panel");
                            
                            $(this).click(function(event)
                            {
                                var e = event || window.event;
                                if(context.timeout())
                                {
                                    e.preventDefault();
                                    e.cancelBubble = true;
                                    return false;
                                }

                                if(context.current != name)
                                {
                                    context.run("show", name);
                                    e.cancelBubble = true;
                                    return false;
                                }
                            });

                            window.setTimeout(function()
                            {
                                // console.log("looking at " + name)
                                $(context["controls"][name]).mouseenter(function()
                                {
                                    context.timestamp = (new Date()).getTime();
                                    context.run("hover", name);
                                });
                            }, context["ttl-hover"]);

                            $(this).keydown(function(event)
                            {
                                // console.log("keydown on control " + name);
                                var e = event || window.event;
                                var space = 32;
                                var esc = 27;
                                var tab = 9;
                                var left = 37;
                                var up = 38;
                                var right = 39;
                                var down = 40;
                                // console.log("keyCode: " + e.keyCode)
                                // console.log(e)
                                if(e.keyCode == space)
                                {
                                    context.run("toggle", name);
                                    $(context.firstchild[name]).focus();
                                    e.preventDefault();
                                    e.cancelBubble = true;
                                    return false;
                                }
                                else if(e.keyCode == esc)
                                {
                                    e.preventDefault();
                                    context.run("hide", name);
                                    e.cancelBubble = true;
                                    return false;

                                }
                                else if(e.keyCode == right)
                                {
                                    var next = $(context.panels[name]).next();
                                    // console.log(next)
                                    // $(context.panels[name]).next().focus();
                                }
                                else if(e.keyCode == tab && context.current == name)
                                {
                                    if(e.shiftKey === false)
                                    {
                                        e.preventDefault();
                                        $(context.panels[name]).find("a")[0].focus();
                                        e.cancelBubble = true;
                                        return false;
                                    }
                                    else
                                    {
                                        context.run("hide", name);
                                    }
                                }
                            });

                            $(context.panels[name]).keydown(function(event)
                            {
                                 var e = event || window.event;
                                 var esc = 27;
                                 if(e.keyCode === esc)
                                 {
                                    $(context.controls[name]).focus();
                                    context.run("hide", name);
                                 }
                            });

                            var panel = context.panels[name];
                            var children = $(panel).find("a").toArray();
                            
                            context["firstchild"][name] = children[0];
                            children[0].ToggleTab = children[children.length - 1];
                            children[children.length - 1].ToggleTab = children[0];
                            
                            $(children[0]).keydown(function(event)
                            {
                                var e = event || window.event;
                                if(e.keyCode == 9 && e.shiftKey === true)
                                {
                                    e.preventDefault();
                                    $(this.ToggleTab).focus();
                                    e.cancelBubble = true;
                                    return false;
                                }
                            });

                            $(children[children.length - 1]).keydown(function(event)
                            {
                                var e = event || window.event;
                                if(e.keyCode == 9 && e.shiftKey === false)
                                {
                                    e.preventDefault();
                                    $(this.ToggleTab).focus();
                                    e.cancelBubble = true;
                                    return false;
                                }
                            });

                        });


                        $(".mega-panel").mouseleave(function(event)
                        {
                            var e = event || window.event;
                            if(context.current !== null){context.run("hide", context.current);}
                        });
                    });

                    var MobileMenuController = new Toggle();
                    MobileMenuController.autohide = false;
                    MobileMenuController.hookActions.push(function(context)
                    {
                        context["panels"] = {};
                        context["controls"] = {};
                        context["menu-control"] = $(context.parent).find("[cccmobile-menu-toggle]")[0];
                        context["menu-panel"] = $(context["menu-control"]).next()[0];
                        context["menu-control"].setAttribute("aria-expanded","false");
                        $(context["menu-control"]).click(function()
                        {
                            if(this.getAttribute("aria-expanded") == "true"){context.run("hide-menu", this);}
                            else{context.run("show-menu", this);}
                        });
                        
                        if(context.autohide === true)
                        {
                            $(document).on("click touchstart", function(event)
                            {
                                e = event || window.event;
                                var current = e.target;
                                
                                while(current !== null)
                                {
                                    if(current === context.parent){return;}
                                    current = current.parentNode;
                                }
                                context.run("hide-menu", context["menu-control"]);
                            });
                        }


                        $(context.parent).find("[cccmobile-control]").each(function()
                        {
                            var name = this.getAttribute("cccmobile-control");
                            var link = this.children[0];
                            var panel = $(link.parentNode).next()[0];
                            var control = (new CJTag("a", {"href":"#", "className":"cccmm-mobile-toggle"})).render();
                            control.appendChild((new CJTag("span", {"className":"sr-only"}, name + " submenu")).render());
                            
                            while(this.firstChild !== null){this.removeChild(this.firstChild);}
                            
                            this.appendChild(link);
                            this.appendChild(control);

                            context.controls[name] = control;
                            context.panels[name] = panel;


                            control.setAttribute("role","button");
                            control.setAttribute("aria-expanded", "false");
                            control.setAttribute("aria-controls", "cccmobile-panel-" + name);
                            panel.setAttribute("id", "cccmobile-panel-" + name);

                            $(control).click(function(event)
                            {
                                var e = event || window.event;

                                context.run("toggle", control);
                                e.preventDefault();
                                return false;
                            });
                        });

                        context.actions["toggle"] = [function(context, control)
                        {
                            if(control.getAttribute("aria-expanded") == "false"){context.run("show", control);}
                            else{context.run("hide", control);}
                        }];

                        context.actions["show"] = [function(context, control)
                        {
                            context.run("hide-all", control);
                            if(control.getAttribute("aria-expanded") == "true"){return;}
                            control.setAttribute("aria-expanded", "true");
                            $(control.parentNode).addClass("show");
                            $(context.panels[control.parentNode.getAttribute("cccmobile-control")]).addClass("show");
                        }];

                        context.actions["hide"] = [function(context, control)
                        {
                            if(control.getAttribute("aria-expanded") == "false"){return;}
                            control.setAttribute("aria-expanded", "false");
                            $(control.parentNode).removeClass("show");
                            $(context.panels[control.parentNode.getAttribute("cccmobile-control")]).removeClass("show");
                        }];
                        
                        context.actions["hide-all"] = [function(context, control)
                        {
                            for(var i in context.controls)
                            {
                                context.run("hide", context.controls[i]);
                            }
                        }];

                        context.actions["show-menu"] = [function(context, control)
                        {
                            // console.log("show-menu")
                            if(control.getAttribute("aria-expanded") == "true"){return;}
                            control.setAttribute("aria-expanded", "true");
                            $(context["menu-panel"]).addClass("show");
                        }];

                        context.actions["hide-menu"] = [function(context, control)
                        {
                            // console.log("hide-menu")
                            if(control.getAttribute("aria-expanded") == "false"){return;}
                            control.setAttribute("aria-expanded", "false");
                            $(context["menu-panel"]).removeClass("show");
                        }];

                        // if(context.autohide === true)
                        // {
                        //     $(document).on("click touchstart", function(event)
                        //     {
                        //         e = event || window.event;
                        //         current = e.target;
                        //         
                        //         while(current !== null)
                        //         {
                        //             if(current.Toggle === context){return;}
                        //             current = current.parentNode;
                        //         }
                        //         context.run("hide", context.current)
                        //     });
                        // }
                    });


                    var CCCToggle = new Toggle();
                    CCCToggle.hookActions.push(function(context)
                    {
                        var subject = $(context.parent).next()[0];
                        context["subject"] = subject;

                        context.parent.setAttribute("aria-expanded", "false");
                        
                        context.actions["toggle"] = [function(context, control)
                        {
                            if(control.getAttribute("aria-expanded") == "false"){context.run("show", control);}
                            else{context.run("hide", control);}

                        }];

                        context.actions["show"] = [function(context, control)
                        {
                            if(control.getAttribute("aria-expanded") == "true"){return;}
                            control.setAttribute("aria-expanded", "true");
                            $(control).addClass("show");
                        }];

                        context.actions["hide"] = [function(context, control)
                        {
                            if(control.getAttribute("aria-expanded") == "false"){return;}
                            control.setAttribute("aria-expanded", "false");
                            $(control).removeClass("show");
                        }];
                        
                        $(context.parent).on("click", function(event)
                        {
                            var e = event || window.event;
                            context.run("toggle", this);
                        });

                        if(context.parent.getAttribute("ccc-toggle-autohide") == "true")
                        {
                            $(document).on("click touchstart", function(event)
                            {
                                e = event || window.event;
                                current = e.target;
                                
                                while(current !== null)
                                {
                                    if(current === subject){return;}
                                    current = current.parentNode;
                                }
                                context.run("hide", context.parent)
                            });
                        }
                    });
                    

                    $(document).ready(function()
                    {
                        MegaMenuController.hook($("[cccmm]")[0]);
                        MobileMenuController.hook($("[cccmobile]")[0]);
                        $("[ccc-toggle]").each(function(){CCCToggle.hook(this);}); // takes care of the mobile menu. Others?
                    })
                })(window);

                $(".impact-wrapper .impact a").each(function()
                {
                    var target = $(this).attr("href");
                    if(CCCUtil.isString(target, true))
                    {
                        target = $(target)[0];
                        if(!!target)
                        {
                            $(this).click(function(event)
                            {
                                event = event || window.event;
                                if(event.preventDefault){event.preventDefault();}
                                CCCUtil.moveTo(target,250);
                                if(event.stopPropagation){event.stopPropagation();}
                                return false;
                            });
                        }
                    }
                });

            });
