<script type="text/javascript">
    function loadNews(j)
    {
        var target = $("#news").find(".content-list")[0];
        while(target.firstChild !== null){target.removeChild(target.firstChild);}
        
        for(var i = 0; i < j.data.length; i++)
        {
            var result = document.createElement("a");
            $(result).attr("href", "/spotlight?id=" + j.data[i]["id"]);
            
            result.appendChild(document.createTextNode(j.data[i]["title"]));
            target.appendChild($(result)[0]);
        }
        if(j.data.length < 1)
        {
            var plug = document.createElement("p");
            plug.appendChild(document.createTextNode("No news items"));
            target.appendChild(plug);
        }
    }
    function loadEvents(j)
    {
        var target = $("#events").find(".content-list")[0];
        while(target.firstChild !== null){target.removeChild(target.firstChild);}
        
        for(var i = 0; i < j.data.length; i++)
