        var $topMenuClone = $('.TopMenu ul').clone(),
            $phoneNumber = $('<a />', {
                'text': $('.js-drawermenu-store-phone-container').find('.phoneIcon').text(),
                'href': 'tel:' + $('.js-drawermenu-store-phone-container').find('.js-drawermenu-store-phone').text()
