classifieds.seo_encode = function (str){
    str = str.replace(/&/g, '');
    str = str.replace(/ /g, '-');
    for(var i=0; i<5; i++) {str = str.replace('--', '-'); }
    str = str.toLowerCase(); 
    //str = encodeURI(str);
    return str;
};


