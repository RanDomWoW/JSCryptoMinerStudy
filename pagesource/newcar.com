<script type="text/javascript" language="javascript">
jQuery(document).ready(function() {
    
        var gptSlotsArray = {
            "spot_1_ad": "gpt-ad-slot-spot-1",
            "spot_2_ad": "gpt-ad-slot-spot-2",
            "spot_3_ad": "gpt-ad-slot-spot-3",
            "square_ad": "gpt-ad-slot-square-top-0",
            "square_middle_ad": "gpt-ad-slot-square-middle-1",
            "square_bottom_ad": "gpt-ad-slot-square-bottom-1",
            "banner_top_0_ad": "gpt-ad-slot-banner-top-0",
            "banner_bottom_1_ad": "gpt-ad-slot-banner-bottom-1",
            "banner_bottom_0_ad": "gpt-ad-slot-banner-bottom-0"
        };
        
        setTimeout(function () { 
            var refreshAds = false;
            if(refreshAds) {
                console.log('30 Secs: First Refresh');
                isScrolledIntoView(gptSlotsArray);
                    
                setTimeout(function () {
                    console.log('60 Secs: Second Refresh');
                    isScrolledIntoView(gptSlotsArray);
                        
                    setTimeout(function () {
                        console.log('90 Secs: Third Refresh');
                        isScrolledIntoView(gptSlotsArray);
                            
                        setTimeout(function () {
                            console.log('120 Secs: Fourth Refresh');
                            isScrolledIntoView(gptSlotsArray);
                        }, 30000);
                            
                    }, 30000);
                        
                }, 30000);
                refreshAds = false;
            }
        }, 30000);
        
        
        function refreshAdsBySlot(gptSlotRefresh) {

            if(gptSlotRefresh == "spot_1_ad") {
                googletag.pubads().refresh([spot_1_ad]);
            }
            if(gptSlotRefresh == "spot_2_ad") {
                googletag.pubads().refresh([spot_2_ad]);

                // This ad is hidden in a carousel tab.
                // Once we fetch spot2, we need to refresh it too.
                if( typeof spot_3_ad !== 'undefined') {
                    googletag.pubads().refresh([spot_3_ad]);
                }
            }
            if(gptSlotRefresh == "spot_3_ad") {
                googletag.pubads().refresh([spot_3_ad]);
            }
            if(gptSlotRefresh == "square_ad") {
                googletag.pubads().refresh([square_ad]);
            }
            if(gptSlotRefresh == "square_middle_ad") {
                googletag.pubads().refresh([square_middle_ad]);
            }
            if(gptSlotRefresh == "square_bottom_ad") {
                googletag.pubads().refresh([square_bottom_ad]);
            }
            if(gptSlotRefresh == "banner_bottom_0_ad") {
                googletag.pubads().refresh([banner_bottom_0_ad]);
            }
            if(gptSlotRefresh == "banner_bottom_1_ad") {
                googletag.pubads().refresh([banner_bottom_1_ad]);
            }
            if(gptSlotRefresh == "banner_top_0_ad") {
                googletag.pubads().refresh([banner_top_0_ad]);
            }
        }
        
        function isScrolledIntoView(gptArr) {
            var gptSlotRefresh = false;
            
            for (var key in gptArr) {
                var $elem = $("#"+gptArr[key]); 
                if($elem.length > 0) {
                    var $window = $(window);
                    var topBarHeight = $(".nc-container").height() + 20;

                    var docViewTop = $window.scrollTop();
                    var docViewBottom = docViewTop + $window.height() - topBarHeight;

                    var elemTop = $elem.offset().top - topBarHeight;
                    var elemBottom = elemTop + $elem.height();

                    //if ((elemBottom <= docViewBottom) && (elemTop >= docViewTop)) {
                    if ((elemBottom >= docViewTop) && (elemTop <= docViewBottom)) {   
                        //console.log('TRUE >> elemBottom >> '+elemBottom+' docViewBottom >> '+docViewBottom);
                        //console.log('TRUE >> elemTop >> '+elemTop+' docViewTop >> '+docViewTop);
                        refreshAdsBySlot(key);
                        gptSlotRefresh = true;
                    } else {
                        //console.log('FALSE >> elemBottom >> '+elemBottom+' docViewBottom >> '+docViewBottom);
                        //console.log('FALSE >> elemTop >> '+elemTop+' docViewTop >> '+docViewTop);
                    }
                }     
                    //return (elemBottom >= docViewTop) && (elemTop <= docViewBottom);
            }
            return gptSlotRefresh;
        }
        
});
</script>
