        <script>
                function setRollAll(inId,pId,spanId,speed){
                
                    var $n=1;
                    //轮播开始
                    var myset=setInterval(function(){rollRight(inId,pId,spanId);},speed);
                    //鼠标点击控制器，图片切换
                    $(spanId).find('span').click(function(){
                        clearInterval(myset);
                        $n=$(spanId).find('span').index(this);
                        rollRight(inId,pId,spanId);
                        myset=setInterval(function(){rollRight(inId,pId,spanId);},speed);
                    })
                  
                    function rollLeft(inId,pId,spanId){
                        var imgLength=$(inId).find("img").length;
                        var imgWidth=$(inId).find("img").width();
                        var subImg=($n+imgLength)%imgLength;
                        $(inId).find("a").eq(subImg).css({'display':'block','left':-imgWidth,'zIndex':10});
                        $(inId).find("a").eq(subImg).animate({left:0,zIndex:5},300).siblings('a').animate({zIndex:1},300);
                        $(pId).children("p").eq(subImg).animate({"top":0},300).siblings("p").animate({"top":600},300);
                        $(spanId).children("span").eq(subImg).css("background","#0a59a6").siblings("span").css("background","#ffffff");
                        $n++;
                    }
                    function rollRight(inId,pId,spanId){
                        var imgLength=$(inId).find("img").length;
                        var imgWidth=$(inId).find("img").width();
                        var subImg=($n+imgLength)%imgLength;
                        $(inId).find("a").eq(subImg).css({'display':'block','left':imgWidth,'zIndex':10});
                        $(inId).find("a").eq(subImg).animate({left:0,zIndex:5},300).siblings('a').animate({zIndex:1},300);
                        $(pId).children("p").eq(subImg).animate({"top":0},300).siblings("p").animate({"top":600},300);
                        $(spanId).children("span").eq(subImg).css("background","#0a59a6").siblings("span").css("background","#ffffff");
                        $n++;
                    }
                
                    return $n;
                
                }
                
                setRollAll('.inId2','.inId-p2','.inId-span2',3000);
                </script> 

