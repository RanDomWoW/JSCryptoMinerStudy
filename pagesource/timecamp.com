        $(document).ready(function () {
            var visiblePopup = false;
            $('#close-footer-popup').on('click', function(){
                visiblePopup = 'footer';
                $('.footer-popup').removeClass('animated bounceInLeft');
                $('.footer-popup').addClass('animated bounceOutLeft');
            });
            $('#close-top-popup').on('click', function(){
                visiblePopup = 'top';
                $('.top-popup').removeClass('animated bounceInDown');
                $('.top-popup').addClass('animated bounceOutUp');
            });
            function showPopup(){
                var halfWindowHeight = $('body').height()/2;
                if(visiblePopup == false){
                    if ($(window).scrollTop() > 120 && $(window).scrollTop() < halfWindowHeight) {
                        $('.top-popup').addClass('animated bounceInDown');
                        $('.top-popup').removeClass('invisible');
                        visiblePopup = 'top';
                    }
                    else if($(window).scrollTop() >= halfWindowHeight){
                        $('.footer-popup').addClass('animated bounceInLeft');
                        $('.footer-popup').removeClass('invisible');
                        $('.top-popup').removeClass('animated bounceInDown');
                        $('.top-popup').addClass('animated bounceOutUp');
                        visiblePopup = 'footer';
                    }
                }
                else if(visiblePopup == 'top'){
                    if($(window).scrollTop() >= halfWindowHeight){
                        $('.footer-popup').addClass('animated bounceInLeft');
                        $('.footer-popup').removeClass('invisible');
                        $('.top-popup').removeClass('animated bounceInDown');
                        $('.top-popup').addClass('animated bounceOutUp');
                        visiblePopup = 'footer';
                    }
                }
            }
            if($('.sticky-scroll-box').length){
                var top = $('.sticky-scroll-box').offset().top;
                var boxHeight = $('.sticky-scroll-box').height();
                var bodyHeight = $('body').height();
                $(window).scroll(function (event) {
                    var y = $(this).scrollTop();
                    if (y >= top && y + boxHeight <= bodyHeight - boxHeight - 200)
                        $('.sticky-scroll-box').addClass('fixed margin-top-80');
                    else
                        $('.sticky-scroll-box').removeClass('fixed margin-top-80');
                    $('.sticky-scroll-box').width($('.sticky-scroll-box').parent().width());
                });
                var y = $(this).scrollTop();
                if (y >= top && y + boxHeight <= bodyHeight - boxHeight - 200)
                    $('.sticky-scroll-box').addClass('fixed margin-top-80');
                else
                    $('.sticky-scroll-box').removeClass('fixed margin-top-80');
                $('.sticky-scroll-box').width($('.sticky-scroll-box').parent().width());
            }

            var parallax = document.querySelectorAll(".parallax"),
                speed = 0.2;
            window.onscroll = function(){
                [].slice.call(parallax).forEach(function(el,i){
                    var windowYOffset = window.pageYOffset,
                        elBackgrounPos = -200 + (windowYOffset * speed);
                    //el.style.top = elBackgrounPos + "px";
                });
            };

            var headerWrapState = false;
            function showHideNavBar() {
                if ($(window).scrollTop() > 20) {
                    if(headerWrapState == false) {
                        $('#header-wrap').addClass('visible');
                        $('#header-wrap').removeClass('invisible');
                        $('.new-section').removeClass('d-lg-block');
                        headerWrapState = true;
