								<script type="text/javascript">
									jQuery('.filter-estado').bind('click', function(){
										$estado = jQuery(this).data('filter');
										$estadoNome = jQuery(this).html();
										jQuery.ajax({
											type: 'POST',
											url: 'http://www.cnec.br/wp-admin/admin-ajax.php',
											dataType: 'JSON',
											data: "action=filterunidades&estado="+$estado ,
											success: function(dataRetorno){
												if($estadoNome!='SELECIONE')
													jQuery('#estado-sel-header').html($estadoNome);
												else
													jQuery('#estado-sel-header').html('ESTADO');

												if(dataRetorno.hasUnidades==true){
													//COMBO UNIDADE
													$htmlUnidades = '';
													for (var key in dataRetorno.unidades) {
														var objUnidade = dataRetorno.unidades[key];
														$htmlUnidades += '<li><a class="filter-unidade" href="'+objUnidade.link+'" data-logo="'+objUnidade.logo_preview+'" data-img="'+objUnidade.imagem_direita+'" data-nome="'+objUnidade.nome+'" data-endereco="'+objUnidade.endereco+'" data-telefone="'+objUnidade.telefone+'">'+objUnidade.post_title+'</a></li>';

													}
													jQuery('#dropdown-unidades-header').html($htmlUnidades);

												}else {
													alert('Nenhuma unidade encontra com o filtro selecionado.');	
												}
												
											},
											error: function(jqXHR, textStatus, errorThrown){
												alert('Erro ao filtrar unidades. (COD: '+textStatus+')');
											}
										});
										
									});
								</script>
