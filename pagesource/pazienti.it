<script type="text/javascript" charset="utf-8">
  var fillRateableField = function(input_name, id) {
    var input = document.getElementById('new_rating_form').querySelector('[name="' + input_name + '"]');
    input.value = id;
  };

  var moveHalfStep = function(direction) {
    var score_description = document.getElementById('score_desctiption');
    var checked = document.querySelector('[name="score"]:checked');
    var i = checked? checked.getAttribute('value') : 0 ;
    var n = direction == 'up' ? parseInt(i) + 1 : parseInt(i) - 1;
    var next = document.querySelector('[name="score"][value="' + n + '"]');
    if (!next)
      return;

    document.querySelector('[name="score"]').checked = false;
    next.checked = true;
    if (score_description)
      score_description.innerHTML = getDefaultScoreDescription();
  };
  var getDefaultScoreDescription = function() {
    var description = '';
    var checkboxId = '';
    var checked = document.querySelector('[name="score"]:checked');
    if (checked) {
      checkboxId = checked.getAttribute('id');
      description = document.querySelector('label[for="' + checkboxId + '"]').getAttribute('title');
    }
    return description;
  };
  document.getElementById('rating_interface_stars').onmouseover = function (e) {
    var score_description = document.getElementById('score_desctiption');
    var t = e.target;
    var text = '';
    if (t.tagName == 'LABEL')
    {
      text = t.getAttribute('title');
    }
    else
    {
      text = getDefaultScoreDescription();
    }
    if (score_description)
      score_description.innerHTML = text;
  };
  $('#rating_form input, #rating_form textarea').on('blur', function() {
    $(this).addClass('blur');
  }).on('focus change', function() {
    $(this).removeClass('blur');
    $(this)[0].setCustomValidity('');
  });
</script>

<script type="text/javascript" charset="utf-8">

  var displayRatingErrors = function(response) {
    var errorsText = [];
    var is_browser_desktop = typeof angular != 'undefined';
    var flashMessagesContainer = document.getElementById('flash_message_ctrl');
    if (is_browser_desktop)
      var ang_Flashing = angular.element(document.querySelector('[ng-controller="FlashMessagesCtrl"]')).scope();
    var i = 0;

    for (var key in response) {
      if (response.hasOwnProperty(key)) {
        errorsText.push(response[key]);
        input = document.querySelector('#new_rating_form [name="'+key+'"]');
        input.className = input.className + ' blur';
        i = 0;
        while (i < key.length) {
          if (response[key][i]) {
            input.setCustomValidity(response[key][i]);
            if (is_browser_desktop)
              ang_Flashing.setMessage(response[key][i]);
          }
          i++;
        }
        if (is_browser_desktop) {
          flashMessagesContainer.style['display'] = 'block';
          flashMessagesContainer.style['position'] = 'fixed';
          flashMessagesContainer.style['bottom'] = '0px';
          flashMessagesContainer.style['z-index'] = '99';
        }
        else
        {
          $("body.mobile").append("<div class='rails_message_notice'><p>" + errorsText.join(' - ') + "</p></div>");
          $(".rails_message_notice").append('<a href="javascript: void(0);" class="dismiss" id="closeRailsMessage">&times;</a>');
        }
      }
    }
  };

  $(document).on('submit', '#new_rating_form', function(e) {
    var $myForm = $('#new_rating_form');
    var is_browser_desktop = typeof angular != 'undefined';
    var wait = is_browser_desktop ? 2000 : 3000
    var redirect_url = "";
    console.log(redirect_url);
    if(! $myForm[0].checkValidity()) {
      // If the form is invalid, submit it. The form won't actually submit;
      // this will just cause the browser to display the native HTML5 error messages.
      $myForm.submit();
    }
    else
    {
      e.preventDefault();
      var button = $('#new_rating_submit');
      var formData = $('#new_rating_form').serialize();
      var url = '/ratings';
      button.prop('disabled', true);

      $.ajax({
        dataType: "json",
        url : url,
        method : 'POST',
        data : {
          rating : formData
        },
        success : function (response) {
          console.log('response');
          if (response['result']) {
            $('#new_rating_submit').text('inviato');
            $('#new_rating_submit').addClass('success');
            button.prop('disabled', true);
            setTimeout(function() {
              if (is_browser_desktop) {
                $('#rating_form').modal('hide');
              }
              else
              {
                window.location = redirect_url;
              }
            }, wait)
            return;
          }
          else
          {
            button.prop('disabled', false);
            displayRatingErrors(response);
          }
        }
      });
    }
  });
//$(document).on('click', '#new_rating_submit', function() {
//  $('#new_rating_form').submit();
//});

</script>

