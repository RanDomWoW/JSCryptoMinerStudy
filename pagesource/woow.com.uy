        removeClassABtest(variante1,'active');
        removeClassABtest(variante1,'in');
        removeClassABtest(variante2,'active');
        removeClassABtest(variante2,'in');
      },  
      function() {    /* Variation 1 (Mas vendidas)*/
        // console.log('exp29-variante');
        var orig = document.querySelectorAll('.exp29-orig');
        var variante1 = document.querySelectorAll('.exp29-variante1');
        var variante2 = document.querySelectorAll('.exp29-variante2');
        // console.log('orig='+orig);
        // console.log('variante='+variante);
        addClassABtest(variante1,'active');
        addClassABtest(variante1,'in');
        removeClassABtest(orig,'active');
        removeClassABtest(orig,'in');
        removeClassABtest(variante2,'active');
        removeClassABtest(variante2,'in');
      },
      function() {    /* Variation 2 (Ofertas del mail) */
        // console.log('exp29-variante');
        var orig = document.querySelectorAll('.exp29-orig');
        var variante1 = document.querySelectorAll('.exp29-variante1');
        var variante2 = document.querySelectorAll('.exp29-variante2');
        // console.log('orig='+orig);
        // console.log('variante='+variante);
        addClassABtest(variante2,'active');
        addClassABtest(variante2,'in');
        removeClassABtest(orig,'active');
        removeClassABtest(orig,'in');
        removeClassABtest(variante1,'active');
        removeClassABtest(variante1,'in');
      }
    ];

    //aux functions
    function removeClassABtest(elements, myClass) {
      if (!elements) { return; }
      if (typeof(elements) === 'string') {
        elements = document.querySelectorAll(elements);
      }
      else if (elements.tagName) { elements=[elements]; }
      var reg = new RegExp('(^| )'+myClass+'($| )','g');
      for (var i=0; i<elements.length; i++) {
        elements[i].className = elements[i].className.replace(reg,' ');
      }
    }
    function addClassABtest(elements, myClass) {
      if (!elements) { return; }
      if (typeof(elements) === 'string') {
        elements = document.querySelectorAll(elements);
      }
      else if (elements.tagName) { elements=[elements]; }
      for (var i=0; i<elements.length; i++) {
        if ( (' '+elements[i].className+' ').indexOf(' '+myClass+' ') < 0 ) {
          elements[i].className += ' ' + myClass;
        }
      }
    }

    // Wait for the DOM to load, then execute the view for the chosen variation.
    document.addEventListener("DOMContentLoaded", function(event) { 

        pageVariations[abtestVariation]();

    });
};
