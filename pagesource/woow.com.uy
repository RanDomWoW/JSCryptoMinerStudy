<script type="text/javascript">

var client_id = "000000004811069A",
	redirect_uri = "http://www.woow.com.uy/live_connect/callback_connect",
	live_contacts = [];
	
function loadJS(src,callback) {
    var d=document,
    	h=d.getElementById('js-live-net'),
    	s=d.createElement('script');

    s.type='text/javascript';

    if(h){
	    s.async=true;
	    s.src=src;
	    s.onreadystatechange = s.onload = function() {
	        var state = s.readyState;console.log(state);
	        if (!callback.done && (!state || /loaded|complete/.test(state))) {
	            callback.done = true;
	            callback();
	        };
	    };
        h.parentNode.insertBefore(s,h);
        h.id='js-live-net-loaded';
    } else {
    	callback();
    };
}

function load_live_connect(){
	loadJS( '//js.live.net/v5.0/es/wl.js',
			function() { 
		   		WL.init({ client_id: client_id, redirect_uri: redirect_uri });
		   		live_connect_init();
			}
	);
}


	


function live_connect_init() {

	WL.login({ scope: ["wl.basic", "wl.contacts_emails"] }).then(
	    function(response) {
	        getUserData();
	        getContacts();
	    },
	    function(response) {
	        live_connect_log("Could not connect, status = " + response.status);
	    }
	);
}

function getUserData() {
	WL.api({
		path : "me/picture",
		method : "get"
	}).then(function(response) {
		$('#meImg').html("<img src='" + response.location + "' width='64' height='64' />");
	});

	WL.api({
		path : "me",
		method : "get"
	}).then(function(response) {
		$('#meName').html(response.name);
	});
	
	$('#google-yahoo-contacts').hide();
	$('#live-connect-data').show();
}

function getContacts() {
    WL.api({ path: "/me/contacts", method: "GET" }).then(
        onGetContacts,
        function(response) {
            live_connect_log("Cannot get contacts: " + JSON.stringify(response.error).replace(/,/g, ",\n"));
        }
    );
}

function onGetContacts(response) {
    var items = response.data;
    live_contacts = [];
    for (var i = 0; i < items.length; i++) {
        getContactProperties(items[i].id);
    }
    
}

function getContactProperties(contactId) {
    WL.api({ path: contactId, method: "GET" }).then(onGetContactProperties);
}

function onGetContactProperties(response) {
	live_contacts.push(JSON.stringify(response['emails']['preferred']).replace(/,/g, ",\n"));
}
