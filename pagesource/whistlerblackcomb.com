<script id="header_weather__forecast" type="text/template">
    <div class="row header_weather__forecast__top_row">
        <div class="col-xs-8 c126__title--v1 row header_weather__title header_weather__title--forecast uppercase"><span class="header_weather__title_icon header_weather__title_icon--forecast icon-weather-todays-forecast"></span><%= (weather.HeaderSettings.DailyForecastTitle) %></div> 
        <div class="col-xs-4 header_weather__measurement_toggle">
            <a href="javascript:void(0);" class="header_weather__measurement header_weather__measurement--fahrenheit <%= ((weather.UnitOfMeasure == 1)? 'c126__metricselected--v1' : 'c126__metricunselected--v1' ) %>" data-component-element="hdrWeatherHeader_Units_Link" data-component-units="fahrenheit" data-track="click">F&#176;<span class="sr-only">, Click here for Fahrenheit</span></a>
            <a href="javascript:void(0);" class="header_weather__measurement header_weather__measurement--celsius <%= ((weather.UnitOfMeasure == 1)? 'c126__metricunselected--v1' : 'c126__metricselected--v1') %>" data-component-element="hdrWeatherHeader_Units_Link" data-component-units="celsius" data-track="click">C&#176;<span class="sr-only">, Click here for Celsius</span></a>
        </div>
    </div>
    <div class="row header_weather__forecast__container">
        <div class="col-xs-6 col-sm-7 col-md-8 col-lg-6 header_weather__forecast__weather_container">
            <div class="header_weather__forecast__description_container">
                <div class="c126__temp1--v1 header_weather__forecast__weather">
                    <i class="header_weather__forecast_icon" aria-hidden="true"></i>
                    <span class="tempnumber header_weather__us_only"><%= (weather.CurrentTempStandard) %></span><span class="header_weather__tempunit header_weather__us_only"></span>
                    <span class="tempnumber header_weather__international_only"><%= (weather.CurrentTempMetric) %></span><span class="header_weather__tempunit header_weather__international_only"></span>
                </div>
                <div class="header_weather__forecast__description c126__shortdescription1--v1"><%= (weather.WeatherShortDescription) %></div>
            </div>
        </div>
        <div class="col-xs-6 col-sm-5 col-md-4 col-lg-6 header_weather__forecast__hi_low_container">
            <div class="header_weather__forecast_hi">
                <div class="uppercase header_weather__us_only header_weather__temperature_reading c126__temp2--v1">
                    <%= (weather.HighTempStandard) %><span class="header_weather__tempunit"></span>
                </div>
                <div class="uppercase header_weather__us_only c126__temp2--v1">
                    <span class="header_weather__icon--arrow icon-weather-arrow-hi"></span><span class="c126__hilow1--v1 uppercase">hi</span>
                </div>
                <div class="uppercase header_weather__international_only header_weather__temperature_reading  c126__temp2--v1">
                    <%= (weather.HighTempMetric) %><span class="header_weather__tempunit"></span>
                </div>
                <div class="uppercase header_weather__international_only c126__temp2--v1">
                    <span class="header_weather__icon--arrow icon-weather-arrow-hi"></span><span class="c126__hilow1--v1 uppercase">hi</span>
                </div>
            </div>
            <div class="header_weather__forecast_low">
                <div class="uppercase header_weather__us_only header_weather__temperature_reading  c126__temp2--v1">
                    <%= (weather.LowTempStandard) %><span class="header_weather__tempunit"></span>
                </div>
                <div class="uppercase header_weather__us_only c126__temp2--v1">
                    <span class="header_weather__icon--arrow icon-weather-arrow-low"></span><span class="c126__hilow1--v1 uppercase">low</span>
                </div>
                <div class="uppercase header_weather__international_only header_weather__temperature_reading c126__temp2--v1">
                    <%= (weather.LowTempMetric) %><span class="header_weather__tempunit"></span>
                </div>
                <div class="uppercase header_weather__international_only c126__temp2--v1">
                    <span class="header_weather__icon--arrow icon-weather-arrow-low"></span><span class="c126__hilow1--v1 uppercase">low</span>
                </div>
            </div>
        </div>
        <div class="col-xs-2"></div>
    </div>
    <div class="row header_weather__link header_weather__link--forecast">
        <% if (weather.MountainCamLink && weather.MountainCamLink.Url) { %>
            <a class="col-xs-12 c126__link--v1 linkCTA"
               href="<%= weather.MountainCamLink.Url %>"
               data-component-element="hdrWeatherHeader_MountainCams_Link"
               data-track="click"
               title="<%= weather.MountainCamLink.Title %>">
                <%= (weather.MountainCamLink.Text) %>
            </a>
        <% } %>
    </div>

</script>

<script id="header_weather__snow" type="text/template">
    <div class="row">
        <div class="col-xs-12 c126__title--v1 header_weather__title uppercase"><span class="header_weather__title_icon header_weather__title_icon--snowflake icon-weather-snow-report"></span><%= (headerSettings.SnowReportTitle) %></div> 
    </div>
    <div class="row header_weather__snow_report_container">
        <% _.each(snowReport, function(snowfall, index){ %>
        <div class="col-xs-5 col-lg-6 header_weather__us_only header_weather__snowfall_container">
            <div class="c126__total1--v1 header_weather__snowfall_measurement"><%= snowfall.Depth.Inches %>in</div>
            <div class="c126__label1--v1 header_weather__snowfall_measurement uppercase"><%= snowfall.Description %></div>
        </div>
        <div class="col-xs-5 col-lg-6 header_weather__international_only header_weather__snowfall_container">
            <div class="c126__total1--v1 header_weather__snowfall_measurement"><%= snowfall.Depth.Centimeters %>cm</div>
            <div class="c126__label1--v1 header_weather__snowfall_measurement uppercase"><%= snowfall.Description %></div>
        </div>
        <% }) %>
    </div>
    <div class="row header_weather__link">
        <% if (weather.WeatherReportLink && weather.WeatherReportLink.Url) { %>
            <a class="col-xs-12 c126__link--v1 linkCTA" 
               href="<%= weather.WeatherReportLink.Url %>" 
               data-component-element="hdrWeatherHeader_Conditions_Link" 
               data-track="click"
               title="<%= weather.WeatherReportLink.Title %>">
                <%= weather.WeatherReportLink.Text %>
            </a>
        <% } %>
    </div>
</script>

<script id="header_weather__lift_status" type="text/template">
    <div class="row">
        <div class="col-xs-12 c126__title--v1 header_weather__title uppercase"><span class="header_weather__title_icon header_weather__title_icon--terrain icon-brochure-mountain"></span><%= (weather.HeaderSettings.LiftAndTerrainStatusTitle) %></div> 
    </div>

    <div class="header_weather__lift_status_container row">
        <% if (weather.HeaderSettings.ShowLiftsOpenSection === true) { %>
        <% if (weather.HeaderSettings.ShowLiftsGraph === true) { %>
        <div class="header_weather__circle col-lg-3 hidden-xs hidden-sm hidden-md" data-open="<%= (weather.OpenLifts) %>" data-total="<%= (weather.TotalLifts) %>">
            <div class="header_weather__lift_icon" tabindex="0">
                <span class="icon-lift-gondola"></span>
            </div>
        </div>
        <% } %>
        <div class="header_weather__lifts_open_container c126__link--v1 col-xs-4 <%= (weather.HeaderSettings.ShowLiftsGraph === true)? 'col-lg-3' : 'col-lg-6 header_weather__lifts_open_container--nograph' %>">
            <div class="header_weather__lifts_open c126__number1--v1"><%= (weather.OpenLifts) %><span class="c126__number2--v1">/<%= (weather.TotalLifts) %></span></div>
            <div class="c126__label1--v1 uppercase">Lifts Open</div>
        </div>
        <% } %>
        <div class="header_weather__lift_times_container col-xs-8 <%= (weather.HeaderSettings.ShowLiftsOpenSection === true)? 'col-lg-6' : 'col-lg-12' %>">
            <div class="header_weather__todays_hours c126__time--v1"><%= (weather.CurrentWorkDay.Description) %></div>
            <div class="c126__label1--v1 uppercase"><%= (weather.HoursOfOperationTitle) %></div>
        </div>
    </div>
    <div class="row header_weather__link header_weather__link--terrain">
        <% if (weather.TerrainStatusLink && weather.TerrainStatusLink.Url) { %>
            <a class="col-xs-12 c126__link--v1 linkCTA" 
               href="<%= weather.TerrainStatusLink.Url %>" 
               data-component-element="hdrWeatherHeader_Terrain_Link" 
               data-track="click"
               title="<%= weather.TerrainStatusLink.Title %>">
                <%= weather.TerrainStatusLink.Text %>
            </a>
        <% } %>
    </div>
</script>

