			e.preventDefault();
			if(!$(this).hasClass("active")){
				$(".js-location-tag").removeClass("active");
				$(this).addClass("active");
				e.preventDefault();
				var place = $(this).attr("href").substr(1);
				$(".js-list li").hide();
				$(".js-list li").each(function(){
					var data = $(this).data("pref");
					console.log(place);
					if(($(this).data("place").indexOf("日進") != -1 || $(this).data("place").indexOf("名古屋") != -1 )&& place == "愛知県"){
						$(this).show();
					}
					if(data && place.indexOf(data) != -1){
						$(this).show();
					}
				});
				$(".js-list li:visible:gt(3)").hide();
			}else{
				$(this).removeClass("active");
				$(".js-list li").show();
				$(".js-list li:gt(3)").hide();
			}
		});
		$(".js-list li:gt(3)").hide();
	})(jQuery);
