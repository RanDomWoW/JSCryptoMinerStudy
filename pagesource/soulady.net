<script>
function addNoMode(item, position, options){
	var clone = $('#no-mode').clone().removeAttr('id').css('display','block').addClass('append');
	clone.find('.title-box').find('a').text(item.title).attr('href', item.url);
	clone.find('.tag-style-other').text(item.cname).attr('href', item.clink);
	clone.find('.source').text(item.clicks).attr('href', item.url);
	clone.find('.updatedtime').text(item.updatedtime)
	if (item.fromapi){
		clone.addClass('mediav-newsfeed-listitem-ads');
	}
	if (item.clktk){
		clone.attr('data-imptk', item.clktk);
	}
	if (item.imptk){
		clone.attr('data-clktk', item.imptk);
	}
	if (position){
		clone.insertAfter(position);
	}else{
		$('#infoflow').append(clone);
	}
	if (options){
		$('#infoflow').append($('<li id="mv_ad_dom"></li>'));
	}
}
function addMoreMode(item, position, options){
	var clone = $('#more-mode').clone().removeAttr('id').css('display','block').addClass('append');
	clone.find('.title-box').find('a').attr('href', item.url).text(item.title);
	childs = clone.find('.img-list').children('a');
	$(childs[0]).attr('href',item.url).find('img').attr('src', item.imgs[0]);
	$(childs[1]).attr('href',item.url).find('img').attr('src', item.imgs[1]);
	if (item.imgs[2]) $(childs[2]).attr('href',item.url).find('img').attr('src', item.imgs[2]);else $(childs[2]).remove();
	if (item.imgs[3]) $(childs[3]).attr('href',item.url).find('img').attr('src', item.imgs[3]);else $(childs[3]).remove();
	clone.find('.img-list').find('.pic-tip').find('span').text(item.totalimgs);
	clone.find('.footer-bar').find('.tag-style-other').attr('href', item.clink).text(item.cname);
	clone.find('.footer-bar').find('.source').text(item.clicks);
	clone.find('.updatedtime').text(item.updatedtime)
	if (item.fromapi){
		clone.addClass('mediav-newsfeed-listitem-ads');
	}
	if (item.clktk){
		clone.attr('data-imptk', item.clktk);
	}
	if (item.imptk){
		clone.attr('data-clktk', item.imptk);
	}
	if (position){
		clone.insertAfter(position);
	}else{
		$('#infoflow').append(clone);
	}
	if (options){
		$('#infoflow').append($('<li id="mv_ad_dom"></li>'));
	}
}
function addSingleMode(item, position, options){
	var clone = $('#single-mode').clone().removeAttr('id').css('display','block').addClass('append');
	clone.find('.title-box').find('a').attr('href', item.url).text(item.title);
	clone.find('.single-mode-lbox').find('a').attr('href', item.url);
	clone.find('.single-mode-lbox').find('img').attr('src', item.imgs[0]);
	clone.find('.footer-bar').find('.description').text(item.desc);
	clone.find('.footer-bar').find('.tag-style-society').text(item.cname).attr('href', item.clink);
	if (item.fromapi){
		clone.addClass('mediav-newsfeed-listitem-ads');
	}
	if (item.clktk){
		clone.attr('data-imptk', item.clktk);
	}
	if (item.imptk){
		clone.attr('data-clktk', item.imptk);
	}
	if (position){
		clone.insertAfter(position);
	}else{
		$('#infoflow').append(clone);
	}
	if (options){
		$('#infoflow').append($('<li id="mv_ad_dom"></li>'));
	}
}
//根据图片数量显示不同的样式
function additem(item, position, options){
	item.desc = item.desc.length>60 ? item.desc.substr(0, 60) + '...' : item.desc;
	item.imgs = eval(item.imgs);
	totalimgs = 0;
	for(var i=0; i<item.imgs.length; i++){
		if (item.imgs[i] != "") totalimgs++;
	}	
	item.totalimgs = totalimgs;
	if (item.clicks)
		item.clicks = item.clicks + '次点击';
	else
		item.clicks = '';
	if (!item.clink) item.clink = '/';
	
	if (item.totalimgs>3) addMoreMode(item, position, options);
	else if (item.totalimgs>0) addSingleMode(item, position, options);
	else addNoMode(item, position, options);
}
function showitems(page){
	var cid=0;
	var ishot=0;
	var url = '/api/moreitems.php?cid='+cid+'&page='+page+'&perpage=12&ishot='+ishot;
	$.ajax(url,{
		dataType: 'json',
		async:false,
		success: function(data){
			var index = 1;
			$.each(data, function(k,v){
				if (cid && cid!=0){
					v.clink = 'listing.php?cid='+cid;
				}else{
					v.clink = '/';
				}
				if (index++ % 3 ==0){
					additem(v, false, true);
				}else{
					additem(v);
				}
			});
		}
	});
}

var currentpage = 0;
var timer;
var slideindex = -1;
function showslide(num){
	var childs = $('.slide-list').children('li');
	slideindex++;
	if (slideindex >= childs.length) slideindex = 0;
	childs.removeClass('slide-item-active');
	if (typeof num != 'undefined') slideindex = num;
	$(childs[slideindex]).addClass('slide-item-active');
}
function showslidemanual(num){
	clearInterval(timer);
	showslide(num);
}

$(document).ready(function(){
	showitems(currentpage++);
	showslide();
	timer = setInterval(showslide, 5000);
});

$(window).scroll(function (){
	var bottom = 50;
	if ((bottom + $(window).scrollTop()) >= ($(document).height() - $(window).height())){
		showitems(currentpage++);
	}
	if ($(window).scrollTop()>680){
		$('#fixed-right').addClass('fixed-right');
	}else{
		$('#fixed-right').removeClass('fixed-right');
	}
});

</script>

