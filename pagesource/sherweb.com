			height: '49px'
		}, 'slow');
		$('#woInvite').addClass('minimized');
	}
	function woInviteClose() {
		PushEvent('InviteClosed');
		stopInvite();
	}
	function woStartChat() {
		OpenSalesChat('');
		PushEvent('InviteAccepted');
		stopInvite();
	}
    
    	// COOKIE ADVISOR
	function SettingCookieAdvisorId() {
    if (jQuery.urlParam("aid")) {
        jQuery.cookie("AdvisorId", jQuery.urlParam("aid"), { 
            expires: 90, 
            path: "/", 
            domain: "sherweb.com" });
    }
}

jQuery.urlParam = function(name) {
    var results = new RegExp("[\?&]" + name + "=([^&#]*)").exec(window.location.href);
    if (results == null) {
        return null;
    } else {
        return results[1] || 0;
    }
}

SettingCookieAdvisorId();

</script>
