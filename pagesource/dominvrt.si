/**
 * Switch focus news
 */
global_featurednews_currentid = 0;

function featurednews_change(new_id)
{
	// if there is no element defined, just proceed to the next one
	if (new_id == undefined)
	{
		global_featurednews_currentid = global_featurednews_currentid+1;
		if (global_featurednews_currentid> (global_featurednews_all_elements.length-1))
			global_featurednews_currentid = 0;
	}
	else
	{
		if (new_id == 'prev')
			global_featurednews_currentid--;
		else if (new_id == 'next')
			global_featurednews_currentid++;
		else
			global_featurednews_currentid = new_id-1;

		if (global_featurednews_currentid> (global_featurednews_all_elements.length-1))
			global_featurednews_currentid = 0;
		if (global_featurednews_currentid<0)
			global_featurednews_currentid =  (global_featurednews_all_elements.length-1);
	}
	
	global_featurednews_all_elements.each( function(elt) {
		elt.style.display = "none";
	});

	el = global_featurednews_all_elements[global_featurednews_currentid];
	el.style.display = "";

	$('featurednews_elements').getElements('[rel=top_thumbs]').each( function(old_el) {
		old_el.removeClass('selected');
	});

	switch ( global_featurednews_currentid ) {
		case 0:
			$('featurednews_thumb1').addClass('selected');
			break;
		case 1:
			$('featurednews_thumb2').addClass('selected');
			break;
		case 2:
			$('featurednews_thumb3').addClass('selected');
			break;
	}
}


function featurednews_click(id) {
	url = global_featurednews_all_elements[id-1].getElements("[rel=featurednews_url]")[0].href;
	document.location.href = url;
}


function featurednews_timer_start()
{
	global_featurednews_timer = (function() {	featurednews_change(); }).periodical(5000);
}
function featurednews_timer_end()
{
	if ($defined(global_featurednews_timer))
		$clear(global_featurednews_timer);
}

function featurednews_set_thumbs()
{
	$('featurednews_thumb1').innerHTML = global_featurednews_all_elements[0].getElements("[rel=featurednews_thumb]")[0].innerHTML
	$('featurednews_thumb2').innerHTML = global_featurednews_all_elements[1].getElements("[rel=featurednews_thumb]")[0].innerHTML
	$('featurednews_thumb3').innerHTML = global_featurednews_all_elements[2].getElements("[rel=featurednews_thumb]")[0].innerHTML
}

window.addEvent('domready', function() { 
	global_featurednews_all_elements = $('featurednews_elements').getElements('[rel=featurednews_element]');
	featurednews_set_thumbs();
	featurednews_change(1);
});

</script>




