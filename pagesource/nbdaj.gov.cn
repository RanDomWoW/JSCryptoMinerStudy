<!--
var vmin=1;
var vmax=3;
var vr=0.01;
var timer1;
function Chip(iIndex,width,height){
	this.index=iIndex;
	this.vx=vmin+vmax*Math.random();
	this.vy=vmin+vmax*Math.random();
	this.w=width;
	this.h=height;
	this.xx=0;
	this.yy=0;
	this.timer1=null;
}
function movechip(iIndex){
    chip = chips[iIndex];

    pageX=window.document.documentElement.scrollLeft;
    pageW=window.document.documentElement.offsetWidth-22;
    pageY=window.document.documentElement.scrollTop;
    pageH=window.document.documentElement.offsetHeight-34;
    chip.xx=chip.xx+chip.vx;
    chip.yy=chip.yy+chip.vy;
    chip.vx+=vr*(Math.random()-0.5);
    chip.vy+=vr*(Math.random()-0.5);
    if(chip.vx>(vmax+vmin)) chip.vx=(vmax+vmin)*2-chip.vx;
    if(chip.vx<(-vmax-vmin)) chip.vx=(-vmax-vmin)*2-chip.vx;
    if(chip.vy>(vmax+vmin)) chip.vy=(vmax+vmin)*2-chip.vy;
    if(chip.vy<(-vmax-vmin)) chip.vy=(-vmax-vmin)*2-chip.vy;
    if(chip.xx<=pageX){
        chip.xx=pageX;
        chip.vx=vmin+vmax*Math.random();
    }
    if(chip.xx>=pageX+pageW-chip.w){
        chip.xx=pageX+pageW-chip.w;
        chip.vx=-vmin-vmax*Math.random();
    }
    if(chip.yy<=pageY){
        chip.yy=pageY;
        chip.vy=vmin+vmax*Math.random();
    }
    if(chip.yy>=pageY+pageH-chip.h){
        chip.yy=pageY+pageH-chip.h;
        chip.vy=-vmin-vmax*Math.random();
    }

    eval('document.all.chip'+iIndex+'.style.pixelLeft='+chip.xx);
    eval('document.all.chip'+iIndex+'.style.pixelTop ='+chip.yy);

    chip.timer1 = setTimeout("movechip("+iIndex+")",100);
}
function stopme(iIndex){
    chip = chips[iIndex];
    if(chip.timer1 != null){
        clearTimeout(chip.timer1);
    }
}
var chips = new Array();
function pagestart(){
    var floats = document.getElementById("floatContainer").childNodes;
    if(floats.length == 0){
        return;
    }
    for(var i = 0 ; i < floats.length ; i++){
        var id = "chip" + i;
        floats[i].id = id;
        floats[i].iIndex = i;
        chips[chips.length] = new Chip(i,117,75);
        movechip(i);
        floats[i].onmouseover = function(){
            stopme(this.iIndex);
        }
        floats[i].onmouseout = function(){
            movechip(this.iIndex);
        }
    }
}
window.attachEvent("onload",function(){setTimeout(pagestart,2000)});
//-->
