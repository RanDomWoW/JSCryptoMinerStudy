<script>
    //变量init_pos为层的最初Y坐标值，last_pos为层最后一层移动后的Y坐标值，在下面的函数执行之前它们是相等的	
	
    var init_pos = last_pos = oLayerxLeft.style.posTop;
	var init_pos1 = last_pos1 = oLayerxRight.style.posTop;

function xxx() {
	    var x = document.documentElement.clientWidth;
		if(x>=1004){
			x=(x-1004)/2-135;
			if(x<0){
				x=0;
				}
			}else{
				x=0;
				}
		oLayerxLeft.style.posLeft=x;
		oLayerxRight.style.posRight=x;
        //目标坐标，上两段代码里解释过具体意思了
        var scrollTop = document.body.scrollTop || document.documentElement.scrollTop || 0;

        var target_pos = scrollTop + init_pos;
		var target_pos1 = scrollTop + init_pos1;

        var step = (target_pos - last_pos) / 10 | 0;
		var step1 = (target_pos1 - last_pos1) / 10 | 0;
        //以step为步长移动层
        oLayerxLeft.style.posTop += step;
		oLayerxRight.style.posTop += step1;
        //更新变量last_pos的值，没有这步也不可能产生弹性移动效果
        last_pos += step;
		last_pos1 += step1;

        //alert(scrollTop);

        //这里的1是1毫秒
    }

setInterval("xxx();", 10)

function fnCloseOLayerx(){
    document.getElementById("oLayerxLeft").style.display="none";
	document.getElementById("oLayerxRight").style.display="none";
}


</script>
