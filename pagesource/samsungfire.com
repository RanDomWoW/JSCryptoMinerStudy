{								var tmp = row.replace(/\{\{articleId\}\}/gi,item.articleId).replace(/\{\{subject\}\}/gi,item.subject);  								html[html.length] = tmp;							}						}						$("#notice").children(".notice_list").append(html.join(""));						addParamA("#notice", "?main_area=MIDDLE_04_01");				}			},function(){console.log("error");});		};		var init = function(){						// 메인슬라이드 설정			var $visual = $("#main_visual"),				$slides = $visual.find(".link_item");						for (var i=0,cnt=$slides.length;i<cnt;i++) {				$slides.eq(i).attr("href",getPreUrl($slides.eq(i)));    			}						$visual.slick({				slidesToShow: 1, slidesToScroll: 1,				autoplay: true, autoplaySpeed: 8000,				prevArrow:"",nextArrow:"",				arrows: true, dots: true,				customPaging: function(slider, i) { return '<a href="#">'+(i+1)+'번째 이미지 슬라이더로 이동</a>'; }			});						addParamA("#main_visual", "?main_area=MIDDLE_01");						// 아이콘 메뉴 url 셋팅			var $icons = $("#list_quick").find("a");			for (var i=0,cnt=$icons.length;i<cnt;i++) {				$icons.eq(i).attr("href",getPreUrl($icons.eq(i)));    			}						addParamA("#right_menu", "?main_area=MIDDLE_02");			addParamA(".event", "?main_area=MIDDLE_03_01");			addParamA(".user_service", "?main_area=MIDDLE_03_02");			addParamA(".recommend", "?main_area=MIDDLE_03_03");						// 공지사항			getNoticeList();									addParamA(".tip", "?main_area=MIDDLE_04_02");			addParamA(".customer", "?main_area=MIDDLE_04_03");									// 로그인 탭			if (getLoginYn()) {				$("#login_pn03").show();			} else {				$("#login_pn01,#login_pn02").show();				$(".login_select_wrap","#right_menu").children("li").children("a").click(function(e){					var $this = $(this),						$that = null;										if ($this.hasClass("certificate")) {						$that = $(".phone","#right_menu");												$this.addClass("on").children(".certificate_img_view").attr("src","/images/front_new/2017/text_certificate_login_o.gif");						$that.removeClass("on").children(".phone_img_view").attr("src"