    var domain = "https://boxycharm.com";
    var currUrl = window.location.href;
    var restUrl = "";
    for (i = (domain.length-1); i < currUrl.length; i++) {
      if(currUrl[i] == "?") {
        restUrl = currUrl.substring(i);
