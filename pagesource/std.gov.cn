<script> 
var showAllTypeTimer;
function showAllType(time) {
	$('.dcpsp-search-area .search-body .search-type .type-area .badge:not(.active):first').addClass('active').animate({'backgroundColor':'#789FC1','color':'white','margin-right':'-4px'},time);
	if($('.dcpsp-search-area .search-body .search-type .type-area .badge:not(.active):first').length==0){
		clearInterval(showAllTypeTimer);
	}
}
function removeAllType(time) {
	$('.dcpsp-search-area .search-body .search-type .type-area .badge.active:first').removeClass('active').animate({'backgroundColor':'#EAEAEA','color':'#8A8A8A','margin-right':'10%'},time);
	if($('.dcpsp-search-area .search-body .search-type .type-area .badge.active:first').length==0){
		clearInterval(showAllTypeTimer);
	}
}
$(function(){
	var isTouch=('ontouchstart' in window);
    if(isTouch){
      $(".carousel").on('touchstart', function(e){
          var that=$(this);
          var touch = e.originalEvent.changedTouches[0];
          var startX = touch.pageX;
          var startY = touch.pageY;
          $(document).on('touchmove',function(e){
            touch = e.originalEvent.touches[0] ||e.originalEvent.changedTouches[0];
            var endX=touch.pageX - startX;
            var endY=touch.pageY - startY;
            if(Math.abs(endY)<Math.abs(endX)){
              if(endX > 10){
                $(this).off('touchmove');
                that.carousel('prev');
              }else if (endX < -10){
                $(this).off('touchmove');
                that.carousel('next');
              }
              return false;
            }
          });
      });
      $(document).on('touchend',function(){
        $(this).off('touchmove');
      });
    }
	 $(".search-head li").each(function(index){ 
         $(this).click(function(){
        	 $(".search-head li").removeClass("active");
        	 var key = $(".search-form:visible .input-group input").val();
        	 $(this).addClass("active"); 
        	 $(".search-form").hide();
        	 if(index==0||index==4){
        		$('.dropdown-toggle').html("全部<span class=\"caret\"></span>");
             	$('.dropdown-toggle').attr("tid","");
        	 }
        	 $(".search-body>div:eq("+index+")").find('.input-group input').val(key);
        	 $(".search-body>div:eq("+index+")").show();
         })
     });
	
	 $(document).on('click',".search-form .hotword a",function(){
		 if($(this).parent().parent().attr("rid")=='0')
			 $(this).attr("href",'http://www.std.gov.cn/search/std?q='+encodeURIComponent($(this).text()));
    	 if($(this).parent().parent().attr("rid")=='1')
    		 $(this).attr("href",'http://www.std.gov.cn/search/org?q='+encodeURIComponent($(this).text()));
    	 if($(this).parent().parent().attr("rid")=='2')
    		 $(this).attr("href",'http://www.std.gov.cn/search/person?q='+encodeURIComponent($(this).text()));
    	 if($(this).parent().parent().attr("rid")=='3')
    		 $(this).attr("href",'http://www.std.gov.cn/search/kpi?q='+encodeURIComponent($(this).text()));
    	 if($(this).parent().parent().attr("rid")=='4')
    		 $(this).attr("href",'http://www.std.gov.cn/search/iso?tid=&q='+encodeURIComponent($(this).text()));
	 });

	 $(".nda").each(function(index){
		var clickNum = 2;
		$(this).click(function(){
			if(index<3){
				var ps=new Array("stdKeysPage","orgKeysPage","perKeysPage");
				$.ajax({
					type: "post",
					dataType:'json',
					cache:false,
					url: "/home/"+ps[index],
					data:{"pageNumber":clickNum, "pageSize":8},
					success: function(data){
						var con = "";
						$.each(data, function(index, item){
							con += '<div class="col-xs-4 col-md-3"><a href="#">'+item.NAME+'</a></div>';
						});
						if(con.length>0)$("#"+ps[index]).html(con);
						clickNum++;
					}
	      		});
			}
		 })
	 });
	 
	 
	 $(".btn-primary").each(function(index){
         $(this).click(function(){
      	   var q = $(".form-control:eq("+index+")").val();
      	   var params = ["std","org","person","kpi","iso"];
      	   var uri = 'http://www.std.gov.cn/search/'+params[index]+'?q='+encodeURIComponent(q);
      	   if(index==4){//国际国外标准
      		 if($('.dropdown-toggle:eq(1)').attr("tid"))
        		   uri = uri+'&tid='+$('.dropdown-toggle:eq(1)').attr("tid");
      	   }else{
      		 if($('.dropdown-toggle:eq(0)').attr("tid"))
        		   uri = uri+'&tid='+$('.dropdown-toggle:eq(0)').attr("tid");
      	   }
      	   window.open(uri);
         })
     });
	 $(".dropdown-menu li").each(function(index){
         $(this).click(function(){
        	var idx = $(this).parent('.dropdown-menu').attr("idx")
        	$('.dropdown-toggle:eq('+idx+')').html($(this).text()+"<span class=\"caret\"></span>");
        	$('.dropdown-toggle:eq('+idx+')').attr("tid",$(this).attr("tid"));
         })
     });
	 $(".form-control").each(function(index){
  	   $(this).bind('keypress',function(event){
			    if(event.keyCode == "13"){
			    	$(".btn-primary:eq("+index+")").click();
			    }
			});
     });
	 $(".list-div a").each(function(index){
		  $(this).click(function(){
	      	   var q = $(this).text().replace(/[^\u4e00-\u9fa5]/gi,"");
	      	 	window.open("http://www.std.gov.cn/gb?initnode="+encodeURIComponent(q)+"&rid="+$(this).attr('rid')); 
	      }) 
	 });
	 $(".hb-list li a").each(function(index){
		  $(this).click(function(){
	      	   var q = $(this).text();
	      	   q = q.replace(/[^a-z]+/ig,"")+' '+q.replace(/[^\u4e00-\u9fa5]/gi,"");
	      	   $(this).attr("href","http://www.std.gov.cn/hb/hbQuery?initnode="+encodeURIComponent(q));
	      }) 
	 });
	 $(".gj-list li a").each(function(index){
		  $(this).click(function(){
	      	   var id = $(this).attr("gjid");
	      	   $(this).attr("href","http://www.std.gov.cn/gj/search/gjOrgDetailed?id="+id);
	      }) 
	 });
	 $(".db-list .db-title span").each(function(index){
		  $(this).click(function(){
	      	 var q = $(this).text().replace(/[^\u4e00-\u9fa5]/gi,"");
	      	 $(this).attr("href","http://www.std.gov.cn/db/search/dbQuery?initnode="+encodeURIComponent(q+"地区"));
	      }) 
	 });
	 $(".db-list a").each(function(index){
		  $(this).click(function(){
	      	   var q = $(this).text();
	      	   $(this).attr("href","http://www.std.gov.cn/db/dbQuery?initnode="+encodeURIComponent(q));
	      }) 
	 });
	 $(".tb-list li a").each(function(index){
		  $(this).click(function(){
	      	   var q = $($(this).parent().children()[0]).text();
	      	   $(this).attr("href","http://www.std.gov.cn/tb?initnode="+encodeURIComponent(q));
	      }) 
	 });
	 
	 $(".org-list li a").each(function(index){
		  $(this).click(function(){
	      	   var q = $(this).html();
	      	   $(this).attr("href","http://www.std.gov.cn/search/orgDetailView?tcCode="+encodeURIComponent(q.substring(0,q.indexOf("（"))));
	      }) 
	 });
	$('ul.listtab2 li').click(function(){
		var id = $(this).parent().attr('id');
		id = (id+"_div").substr(3);
        $(this).prevAll().removeClass('active');
        $(this).nextAll().removeClass('active');
        $(this).addClass('active');
        $('.'+id).removeClass('div_show').addClass('div_hide');
        var liNum = $(this).index();
        $('.'+id).each(function(){
            if ((liNum+1)==$(this).index()) {
            	$(this).removeClass('div_hide').addClass('div_show');
			}
        });
   	})
	showAllTypeTimer = setInterval("showAllType(300)",180);
})
</script>
