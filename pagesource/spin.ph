      function loadAd($placeholder, dataID, type, size, keywords, articleTags) {
          if (dataID == adsNum) {
              $placeholder.append('<div id="div-gpt-ad-dynamic-' + adsNum + '" class="dfp_ad"></div>');
              googletag.cmd.push(function() {
                  if (typeof keywords !== "undefined" && keywords.length != 0) {
                      console.log(keywords);
                      gptAdSlots[adsNum - 1] = googletag.defineSlot("/1015428/" + type, size, "div-gpt-ad-dynamic-" + adsNum).addService(googletag.pubads()).setTargeting("Tag", keywords).setTargeting("ArticleTag", articleTags);
                  } else {
                      gptAdSlots[adsNum - 1] = googletag.defineSlot("/1015428/" + type, size, "div-gpt-ad-dynamic-" + adsNum).addService(googletag.pubads()).setTargeting("Tag", "").setTargeting("ArticleTag", "");
                  }
                  googletag.cmd.push(function(){ googletag.display('div-gpt-ad-dynamic-' + adsNum); });
                  googletag.pubads().refresh([gptAdSlots[adsNum - 1]]);
                  googletag.pubads().collapseEmptyDivs();
              });
              adsNum++;
          };
      }

      $(document).ready(function(){
        console.log('dynamic_ads');

        if ('1') {
          console.log('dynamitcAd - 1');
          //var CHANNEL_SLUG = '';
          var WEBSITE = 'spin';

          function isInView(elem){
            var docViewTop = $(window).scrollTop();
            var docViewBottom = docViewTop + $(window).height();

            var elemTop = $(elem).parent().offset().top;
            var elemBottom = elemTop + $(elem).height();

            var viewIn = $(window).height();
            // return ((elemBottom <= docViewBottom) && (elemTop >= docViewTop));
            return (docViewTop >= (elemTop - viewIn));
          }

          var adShowned = '';

          $('.ad-holder > .dynamic-ad').each(function(i,n){
            var adsId = 31;
            i = i + adsId;
            $(n).attr('id','dynamic-ad-' + i);
            $(n).attr('data-id',i);
            $(n).parent().css('height','0px');
            console.log('id-'+i);
          });

          $('blockquote.inset').parent().addClass('clear');

          $(window).scroll(function(){
            $('.ad-holder > .dynamic-ad').map(function(i,n) {
              var item = '#'+$(this).attr('id');
              var dataID = $(this).attr('data-id');
              var channelSlug = 'home';
              itemDisplay = $(item+' > .dfp_ad').css('display');

              if (isInView(item) && !$(item).hasClass('dynamic-ad-expand')) {
                if (i == 0) {
                  loadAd($(item), dataID , WEBSITE.toLowerCase()+'_inview_'+channelSlug, [],'native-in-article-discoverability', ''); // Load ad w/ targeting
                } else {
                  i+=1;
                  loadAd($(item), dataID , WEBSITE.toLowerCase()+'_inview_'+channelSlug, [], 'dynamic-inview-'+i, ''); // Load ads
                }

                console.log(item+' - '+WEBSITE.toLowerCase()+'_inview_'+channelSlug+" - ");

                $(item+' > .dfp_ad iframe').on('load',function() {
                  setTimeout(function(){
                    itemDisplay = $(item+' > .dfp_ad').css('display');

                    if (itemDisplay == 'none' || itemDisplay == undefined) {
                      $(item).parent().hide();
                    }

                  }, 1000);

                  $(item).addClass('dynamic-ad-expand');
                  $(item).parent().css({'padding' : '15px 0px', 'height' : 'auto'});

                  console.log('ads loaded');
                });
              }
            });
          });
        }
      });
    </script>

