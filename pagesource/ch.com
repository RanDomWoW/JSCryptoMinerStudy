<script type="text/javascript">
    var interval;
    var loginLoad = 0;
    function updateUserName(){
    	try {
    		var iframeDocument = document.getElementById('LoginCheckerIframe').contentWindow.document;
  			var userInfo=document.getElementById('LoginCheckerIframe').contentWindow.document.body.innerText;
            if(userInfo == undefined){
                userInfo=document.getElementById('LoginCheckerIframe').contentWindow.document.body.innerHTML;
            }

            var arruserInfo = userInfo.split('|');
            if(arruserInfo[0] == 1 && !!arruserInfo[1]){
        	    window.parent.$.fn.head._updateUser({Name:arruserInfo[1]});
            }
            clearInterval(interval);
    	} catch(e) {
    		
    	}
    }

    function authHelperNotifyStatus() {
        if(!interval){
    	    interval = setInterval(updateUserName, 1000);
        }
    }

    var onload = function(){
        if(loginLoad == 0){
            loginLoad++;
            var frame = document.getElementById('LoginCheckerIframe');
