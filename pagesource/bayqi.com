<script type="text/javascript">
 
    
       $('#formNewLestter1').on('submit', function() {
	   
            var email = $('.inputNew1').val();
            $(".success_inline, .warning_inline, .error").remove();
            if(!isValidEmailAddress(email)) {
                $('.valid1').html("<div class=\"error alert alert-danger\">Enter Valid Email<button type=\"button\" class=\"close\" data-dismiss=\"alert\">×</button></div></div>");
                $('.inputNew1').focus();
                return false;
            }
            var url = "https://www.bayqi.com/pt/common/footer/subscribe.html";
            $.ajax({
                type: "post",
                url: url,
                data: $("#formNewLestter1").serialize(),
                dataType: 'json',
                success: function(json)
                {
                    $(".success_inline, .warning_inline, .error").remove();
                    if (json['error']) {
                        $('.valid1').html("<div class=\"warning_inline alert alert-danger\">"+json['error']+"<button type=\"button\" class=\"close\" data-dismiss=\"alert\">×</button></div>");
                    }
                    if (json['success']) {
                        $('.valid1').html("<div class=\"success_inline alert alert-success\">"+json['success']+"<button type=\"button\" class=\"close\" data-dismiss=\"alert\">×</button></div>");
                    }
                }
            });
            return false;
        });
    
	
	 function isValidEmailAddress(emailAddress) {
        var pattern = new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);
        return pattern.test(emailAddress);
    }

  
