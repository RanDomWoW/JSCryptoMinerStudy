		$('#rvalor_frete').text('0,00');
		$('#rvalor_total').text('0,00');
		$('#rvalor_10x').text('0,00');
		$('#rvalor_cartao').text('0');
		$('#rvalor_cartao_cents').text('00');
		$('#rvalor_dinheiro').text('0');
		$('#rvalor_dinheiro_cents').text('00');
		$('#badgeC').hide();
		$("#btnModalCarrinhoFinalizar").hide();
		$("#btnSalvarPDF").hide();
		
	}
}
