<script>
function statusChangeCallback(response, login) {
	if (response.status === 'connected') {
		$.ajax({
			type: "POST",
			url: '/r.php?r=j&req=login.facebook&requisicao=visualizar',
			data: 'accessToken='+response.authResponse.accessToken,
			dataType: 'json',
			success: function(data) {
				fb.nome = data.facebook.nome;
				fb.email = data.facebook.email;
				fb.registrado = data.facebook.registrado
				if (fb.registrado == 'S')
					logado = true;
				$('#nome').val(fb.nome);
				$('#email').val(fb.email);
				$('#divFacebookCadastro').html('Você está logado via Facebook como<br /><p><b>'+fb.nome+'</b><br /></p>Clique em avançar, caso queira efetuar Logout, <a href="javascript:fbLogout();">clique aqui.</a>');
				$('#modalFacebookLogin').modal('hide');
				localStorage.setItem('querpromo', 'S');
				if (login) {
					$('#MeuCadastro').modal({keyboard: true });
				}
			}
		});
	} else  {
		$('#divFacebookCadastro').html('<div align="center"><img src="/images/facebook-login.png" width="358" height="77" style="cursor:pointer;" onClick="fbLogin();"  alt=""/></div><p align="center">Caso não queira utilizar o facebook basta clicar em avançar.</p>');
		/*if (response.status === 'not_authorized')
			document.getElementById('status_facebook').innerHTML = 'Por Favor, Efetue o Login';
		else
			document.getElementById('status_facebook').innerHTML = 'Mais rápido e mais fácil, ou se preferir, informe abaixo seu email e cep.';
		*/
	}
	console.log(response.status);
}
function checkLoginState() {
	FB.getLoginStatus(function(response) {
		statusChangeCallback(response);
	});
}
window.fbAsyncInit = function() {
	FB.init({
		appId      : '1089955901116499',
		cookie     : true,
		xfbml      : true,
		version    : 'v2.8'
	});
	checkLoginState();
};
var fb = {};
(function(d, s, id) {
var js, fjs = d.getElementsByTagName(s)[0];
if (d.getElementById(id)) return;
js = d.createElement(s); js.id = id;
js.src = "//connect.facebook.net/pt-BR/sdk.js";
fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));

function fbLogin(){
	FB.login(function(response) {
		statusChangeCallback(response, true);
	}, { scope: 'public_profile,email,user_friends' });
}
function fbLogout() {
	FB.logout(function(response) {
		statusChangeCallback(response, true);
	});	
}
