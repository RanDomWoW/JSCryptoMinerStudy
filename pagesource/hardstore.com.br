            output += '<td class="cLabel2" style="text-align: right;">'+number_format(p.subtotal, 2, ',', '.')+'</td>';
            output += '<td style="text-align: right;"><a href="javascript:apagaProduto('+p.produto+');">';
			output += '<span class="glyphicon glyphicon-trash" aria-hidden="true" data-toggle="tooltip" data-placement="top" title="Remover Produto"></span></a></td></tr>';
		}
		output += '</table>';
