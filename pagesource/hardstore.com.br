<script>
	function efetuarLoginModal() {
		hash = '';
		busca_hash = '';
		try {
			hash = window.localStorage.getItem( 'hash' );
			busca_hash = window.localStorage.getItem( 'busca_hash' );
		} catch ( e ) {}
		$.ajax( {
			type: "POST",
			url: '/r.php?r=j&req=login&requisicao=visualizar',
			data: 'usuario=' + $( '#login_modal' ).val() + '&senha=' + $( '#pass_modal' ).val() + '&hash=' + hash + '&busca_hash=' + busca_hash,
			dataType: 'json',
			success: function ( data ) {
				RSEfetuarLoginModal( data );
			}
		} );
	}

	function RSEfetuarLoginModal( dados ) {
		if ( dados.hasOwnProperty( 'transacao' ) )
			if ( dados.transacao[ 0 ].status == 'Logado' ) {
				$( '#mensagem_login_modal' ).html( '' );
				logado = true;
				if ( direcao != '' )
					location.href = direcao;
				else {
					try {
						ultimosPedidos();
					} catch ( e ) {}
					try {
						meusDados();
					} catch ( e ) {}
					try {
						listaRMA();
					} catch ( e ) {}
					if ( direcao_script == 'desejo' ) modalListaDesejos();
				}
				$( '#modalLogin' ).modal( 'hide' );
			} else {
				$( '#mensagem_login_modal' ).html( '<br /><div class="alert alert-danger" role="alert"><span class="glyphicon glyphicon-remove"></span> ' + dados.transacao[ 0 ].status + '</div>' );
			}
	}
	var direcao = '';
	var direcao_script = '';

	function direciona( pagina ) {
		direcao = pagina;
		if ( logado )
			location.href = direcao
		else
			$( '#modalLogin' ).modal( {
				keyboard: true
			} );
	}
