    <script>
    $(function(){

        $(window).bind("hashchange", function () {
            var faviconTag = $('link[type="image/x-icon"]');

            if (faviconTag == undefined || faviconTag.length == 0)
                faviconTag = $('link[rel="shortcut icon"]');

            if(faviconTag != undefined && faviconTag.length == 1) {
                var link = faviconTag.remove().attr("href");
                $('<link href="' + link + '" rel="shortcut icon" type="image/x-icon" />').appendTo('head');
            }
        });

    	$.each($('.truncate'),function(index,truncate){
			$(this).succinct({
		        size: $(this).attr('size')
		    });
		});
        
        $( window ).resize(function() {
        	$.each($('.truncate'),function(index,truncate){
				$(this).succinct({
			        size: $(this).attr('size')
			    });
			});
        });
        
        if(($(window).height() + 100) < $(document).height()) {
            $('#top-link-block').removeClass('hidden').affix({
                offset: {top:100}
            });
        }

		if(navigator.platform.indexOf('Win') != -1){
			$('a,p,h1,h2,h3,h4,h5,dt,dd,th,td,li,span').each(function(index,tag){
				var color = $(tag).css('color');
				var size  = $(tag).css('font-size');

				if(color.search('rgb') == -1)
					return;

				var rgba = color.match(/^rgba?\((\d+),\s*(\d+),\s*(\d+)(?:,\s*(\d+))?\)$/);

				if (rgba[1] == 0 && rgba[2] == 0 && rgba[3] == 0 && rgba[4] == 0)
					return;

				$(tag).css('text-shadow', '0 0 1px rgba('+rgba[1]+','+rgba[2]+','+rgba[3]+',1)');

				// To use calculated shadow of say, 1/15th of the font height
				var fsize = size.match(/(\d+.\d+)px/);
				if(fsize === null)
					fsize = size.match(/(\d+)px/);

				$(tag).css('text-shadow', '0 0 '+(fsize[1]/17)+'px rgba('+rgba[1]+','+rgba[2]+','+rgba[3]+',0.3)');
			});
		}

        $('[data-toggle="tooltip"]').tooltip();
    });
</script>
