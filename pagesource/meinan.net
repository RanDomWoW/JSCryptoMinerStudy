<script>
var loopImgTimer;
//切り替え時間 5秒
var interval = 5000;
//フェードアウト時間 1秒
var fade_time = 1000;

var topimgno=0;

$(document).ready(function() {
	$("#top_mainimg .img li").eq(0).css({ 'z-index': 1, 'opacity':1 });

	//ロールオーバー、アウト設定。
	$("#top_mainimg .navi li").hover(
	function () {
		stopTimer();
		$(this).find("img").fadeTo(250, 0.7);

		topimgno =  $(this).index();

		$("#top_mainimg .img li:not(:eq(topimgno))").stop().animate({ 'z-index': 1, 'opacity':0 });
		$("#top_mainimg .img li").eq( topimgno ).stop().animate({ 'z-index': $("#top_mainimg .img li").size(), 'opacity':1 });


	},
	function () {
		$(this).find("img").fadeTo(250, 1.0);
		startTimer();
	});
	startTimer();
});


function startTimer(){

	loopImgTimer = setInterval(function(){

		$("#top_mainimg .img li:not(:eq(topimgno))").stop().animate({ 'z-index': 1, 'opacity':0 });

		$("#top_mainimg .img li").eq( topimgno ).stop().animate({ 'z-index': $("#top_mainimg .img li").size(), 'opacity':1 });

		topimgno++;
		if( $("#top_mainimg .img li").size()-1 < topimgno){
			topimgno=0;
		}

	} , interval);

}
function stopTimer(){
	clearInterval(loopImgTimer);
}
</script>


