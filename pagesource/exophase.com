<script id="cardTpl" type="text/template">
  <img class="gamercard" src="{{card_url}}" />

  <form class="gamercard" role="form">

    <div class="p-4 additional-options gamedata-selection">

        <div class="pb-3">
          <div class="d-flex flex-column flex-sm-row btn-group btn-group-toggle platform-filter" data-toggle="buttons">
              <label class="btn btn-secondary active">
                  <input type="checkbox" name="filter[psn]" autocomplete="off" checked="checked"> PSN
              </label>
              <label class="col btn btn-secondary active">
                  <input type="checkbox" name="filter[xbox]" autocomplete="off" checked="checked"> Xbox
              </label>
              <label class="col btn btn-secondary active">
                  <input type="checkbox" name="filter[steam]" autocomplete="off" checked="checked"> Steam
              </label>
              <label class="col btn btn-secondary active">
                  <input type="checkbox" name="filter[blizzard]" autocomplete="off" checked="checked"> Blizzard
              </label>
              <label class="col btn btn-secondary active">
                  <input type="checkbox" name="filter[origin]" autocomplete="off" checked="checked"> Origin
              </label>
              <label class="col btn btn-secondary active">
                  <input type="checkbox" name="filter[retro]" autocomplete="off" checked="checked"> Retro
              </label>
          </div>
          <small class="form-text text-muted">
            All highlighted platforms (in blue) will appear on the gamercards. If you wish to hide a platform, deselect it from the above list.
          </small>
        </div>

        <div class="pb-3 form-row">
            <div class="col">
                <select id="userGamercardSkinSelection" class="skin form-control">
                  <option data-skin-id="11" data-animated="0" value="Full">Full Skin</option>
                  <option data-skin-id="11" data-animated="1" value="Full">Animated (Requires at least two services linked)</option>
                  <option data-skin-id="12" data-animated="0" value="Compact">Compact Skin</option>
                </select>
            </div>
        </div>

        <div class="form-check form-check-inline">
            <input type="radio" class="form-check-input" name="top_gamedata" id="userGamercardShowGames" value="games">
            <label class="form-check-label" for="userGamercardShowGames">Show Games</label>
        </div>

        <div class="form-check form-check-inline">
            <input type="radio" class="form-check-input" name="top_gamedata" id="userGamercardShowAwards" value="awards">
            <label class="form-check-label" for="userGamercardShowAwards">Show Achievements</label>
        </div>

        <div class="form-check form-check-inline">
            <input type="radio" class="form-check-input" name="top_gamedata" id="userGamercardShowBoth" value="both">
            <label class="form-check-label" for="userGamercardShowBoth">Show Both</label>
        </div>
    </div>

    <div class="input-group">
        <div class="input-group-prepend">
            <div class="input-group-text">BB Code</div>
        </div>
        <input type="text" class="form-control" id="card_bb" value="[URL={{profile_url}}][IMG]{{card_url}}[/IMG][/URL]">
    </div>

    <div class="input-group">
        <div class="input-group-prepend">
            <div class="input-group-text">HTML Tag</div>
        </div>
        <input type="text" class="form-control" id="card_bb" value='<a href="{{profile_url}}"><img src="{{card_url}}"></a>'>
    </div>

    <div class="input-group">
        <div class="input-group-prepend">
            <div class="input-group-text">Direct</div>
        </div>
        <input type="text" class="form-control" id="card_bb" value='{{card_url}}'>
    </div>

    <input type="hidden" name="card_id" value="{{card_id}}">

  </form>
</script>

<script id="shareTpl" type="text/template">

  <form data-name="{{name}}" data-id="{{id}}" data-endpoint="{{endpoint}}" data-caption="{{name}} {{type}} in {{title}}" data-message="{{player}} earned '{{name}}' {{type}} in {{title}}" class="share-form" role="form">

    <div class="message-container form-group">
        <div class="message"><h3 class="listing"><span>Message (Required)</span></h3></div>
        <textarea id="message" name="message" class="form-control" rows="3"></textarea>
    </div>

    <div class="selected">

        <div class="caption">
            <h3 class="listing"><span>Selected</span></h3>
            <img src="{{icon}}" />
            <p></p>
        </div>

    </div>

    <div class="previous">
        <h3 class="listing"><span>History</span></h3>

        <div class="row">
        </div>
    </div>

  </form>
</script>

<script id="editGameTpl" type="text/template">

    <h3>{{title}}</h3>

    <form class="edit-game" role="form">

      <div class="form-group row form-lastplayed">

          <label class="col-sm-3 col-form-label" for="lastplayed">Last Played</label>

          <div class="col-sm-9">
             <input name="lastplayed" type="text" class="datepicker form-control" disabled>

            <p style="display: none" class="form-text text-muted">The last played time cannot be changed for this game as it is automatically tracked.</p>
          </div>

      </div>

      <div class="form-group row form-playtime">

          <label class="col-sm-3 col-form-label" for="playtime">Hours Played</label>
          <div class="col-sm-9">
            <input name="playtime" type="text" class="form-control">

            <p class="form-text text-muted">For Xbox One and Steam, note that any values entered here will override the automatically tracked time.</p>
          </div>

      </div>

      <div class="form-group row form-completion">

          <label class="col-sm-3 col-form-label" for="completion">Completion Date</label>

          <div class="col-sm-9">
            <input name="completion" type="text" class="datepicker form-control" disabled>

            <p style="display: none" class="form-text text-muted">If you've completed this game, enter a time here.</p>
          </div>

      </div>


      <div class="p-4 col-sm-12">
        <strong class="p-2">Additional Options</strong>

        <label class="checkbox-inline">
          <input type="checkbox" name="privacy_level" id="inlineCheckbox1"> Hide game on profile page
        </label>
      </div>

      <input type="hidden" name="environment" value="{{env}}">
      <input type="hidden" name="gameid" value="{{gameid}}">

      <div style="clear: both"></div>

    </form>
</script>

<script id="twoFactorTpl" type="text/template">

    <form class="two-factor" role="form">

      <div class="form-group">
        <p>Due to security settings on this account, additional authentication is required.</p>
        <p>A verification code has been dispatched to the device of your choosing, either by email or via an authentication app, depending on the settings chosen.</p>
        <p>Enter it below and hit submit.</p>
        <input class="form-control mt-4" name="code" type="text" placeholder="Verification code">
      </div>

      <input type="hidden" name="provider" value="{{provider}}">

      <div style="display: none" class="response pop-alert alert"></div>

    </form>
</script>

<script id="reportTpl" type="text/template">
    <p>
        <strong>This form should be used only for reporting achievement cheaters</strong>. Otherwise, any user reports should be directed to site
        staff either through our contact page or on the forums.</p> <p>To assist our enforcer team, please be specific as to which games(s) this user has
        cheated on.
    </p>

    <textarea id="report_textarea" class="form-control" rows="3"></textarea>

    <div style="display: none" class="response pop-alert alert"></div>
</script>

<script id="banTpl" type="text/template">

    {{#if banned_by}}
        <div class="ban-block yes">User is already banned</div>

        <p><strong>Ban notes:</strong></p>

        <p>Original report thread: <a href="{{reference}}">{{reference}}</a></p>
    {{else}}
        <div class="ban-block no">User is currently not banned</div>
    {{/if}}

    <div style="display: none" class="response pop-alert alert"></div>
</script>

<script id="addGameTpl" type="text/template">

    <form class="add-game" role="form">

        <div class="form-group row">
          <label class="col-sm-3 col-form-label" for="platform">Platform</label>
          <div class="col-sm-9">
            <select class="form-control" name="platform">
              <option value="default">Select a platform</option>

              {{#each platforms}}
                <option value="{{id}}">{{name}}</option>
              {{/each}}
            </select>
          </div>
        </div>

        <div style="display:none" class="form-group row">
            <label class="col-sm-3 col-form-label" for="game-title">Game Title</label>
            <div class="col-sm-9">
              <input class="autocomplete-game form-control" id="name" name="name" class="form-control" type="text" placeholder="Start typing a game title...">
            </div>
            <input type="hidden" name="game" value="0">
        </div>

      <div class="form-group row">

          <label class="col-sm-3 col-form-label" for="lastplayed">Last Played Date</label>

          <div class="col-sm-9">
              <input name="lastplayed" type="text" class="datepicker form-control">
          </div>

      </div>

      <div class="form-group row">

          <label class="col-sm-3 col-form-label" for="playtime">Hours Played</label>
          <div class="col-sm-9">
              <input name="playtime" type="text" class="form-control">
          </div>

      </div>

      <div class="game">
      </div>

    </form>
</script>

<script id="legacyGameTpl" type="text/template">

  <img src="{{icon}}" />

  <span>{{name}}</span>

</script>

<script id="minimalListTpl" type="text/template">

    <div data-name="{{name}}" data-id="{{id}}" data-icon="{{icon}}" data-endpoint="{{endpoint}}" data-caption="{{name}} {{type}} in {{title}}" data-message="{{player}} earned '{{name}}' {{type}} in {{title}}" class="award col-auto">
        <img src="{{icon}}" />
    </div>

</script>

<script id="forgotTpl" type="text/template">
  <form class="forgot-password" role="form">

    <span class="help-block">
        <p>To initiate a password reset, enter the email address corresponding to your Exophase account.</p>
    </span>
    <div class="form-group">
      <input id="email" class="form-control" name="email" type="text" placeholder="Email address">
    </div>

    <div id="captcha" class="g-recaptcha"></div>

    <div style="display: none" class="response pop-alert alert fade"></div>


  </form>
</script>

<script id="editPanelTpl" type="text/template">
  <div class="edit-panel" style="display: none">

    <div style="display:none" class="edit-options">
      <button style="display:none" type="button" class="datepicker btn-blue blue-hover btn btn-primary"><i class="fa fa-calendar"></i> Edit Date</button>
      <button type="button" class="hours btn-blue blue-hover btn btn-primary"><i class="fa fa-clock-o"></i> Edit Playtime Hours</button>
    </div>

    <div style="display:none" class="social-options">
      <a data-service="twitter" class="btn btn-social btn-twitter">
        <i class="fa fa-twitter"></i>
        Share on Twitter
      </a>

      <a data-service="facebook" class="btn btn-social btn-facebook">
        <i class="fa fa-facebook"></i>
        Share on Facebook
      </a>
    </div>

    <div style="display: none" class="edit-generic-overlay hours-overlay">
      <form>
        <div class="form-group">
          <input type="text" class="form-control" id="InputHours" placeholder="Hours played">
          <p class="help-block">Manually update hours played for this game. Please input a number or decimal only.</p>
        </div>
        <button type="submit" class="submit btn-blue blue-hover btn btn-default">Update</button>
        <button type="submit" onclick="closeModalCustom(this); return false;" class=" btn btn-danger">Close</button>
      </form>
    </div>
  </div>
</script>

<script id="gamelistTpl" type="text/template">
  <li data-completion="{{completion_date}}" data-lastplayed="{{lastplayed}}" data-playtime="{{playtime}}" data-env="{{meta.environment_slug}}" data-gameid="{{gameid}}" class="d-flex justify-content-start align-items-center flex-wrap {{stripe}} {{meta.environment_slug}} {{status}} {{privacy}}">

    <div class="p-1 small-game-image">
      {{#if endpoint_awards}}
          <img src="{{resource_standard}}" />
      {{else}}
        <img class="small-game-image" src="{{resource_standard}}" />
      {{/if}}

      <div class="game-controls">
        <i data-toggle="tooltip" data-placement="bottom" title="Edit Game" data-options="edit-options" class="fa fa-pencil-square edit-game tooltip-act"></i>
        <i data-toggle="tooltip" data-placement="bottom" title="Share Game Activity" data-options="social-options" class="fa fa-share-alt share-game tooltip-act"></i>
      </div>
    </div>

    <div class="p-1 game-info">
      <h3 style="padding-bottom: 0px;" class="game-db-title">
        {{#if meta.endpoint_awards}}
          <a href="{{meta.endpoint_awards}}">{{meta.title}}</a>
        {{else}}
          {{meta.title}}
        {{/if}}
      </h3>

      {{#if completion_date}}

        {{#if completion_information}}
          <span class="greentxt2" style="display:block">Completed {{completion_information}}</span>
        {{else}}
            {{#if meta.endpoint_awards}}
                <span data-playtime="{{playtime}}" style="display: block;"><span class="count">{{earned_awards}}</span> out of <span class="count">{{meta.total_awards}}</span> {{meta.award_type}}</span>
            {{/if}}
        {{/if}}

        {{#if playtime}}
          <span class="hours" style="display:block"><i class="fa fa-clock-o"></i> {{playtime}} hours played</span>
        {{/if}}
      {{else}}
        <span class="hours" style="display: block; padding-bottom: 3px;"><i class="fa fa-clock-o"></i> {{playtime}} hours played</span>
      {{/if}}

      {{#each meta.platforms}}
        <div style="display: inline-block; line-height: 1.3 !important;" class="{{@root.meta.environment_slug}}-profile-pf inline-pf">{{alternate_name}}</div>
      {{/each}}
    </div>

    <div class="ml-auto p-1 game-progress">
      {{#if meta.endpoint_awards}}
      <div class="holders">
        <div class="holder-wrapper">
          {{#if earned_bronze}}<span class="bronze" alt="Bronze Trophy Count">{{earned_bronze}}</span>{{/if}}
          {{#if earned_silver}}<span class="silver" alt="Silver Trophy Count">{{earned_silver}}</span>{{/if}}
          {{#if earned_gold}}<span class="gold" alt="Gold Trophy Count">{{earned_gold}}</span>{{/if}}
          {{#if earned_platinum}}<span class="platinum" alt="Platinum Trophy Count">{{earned_platinum}}</span>{{/if}}
          {{#if earned_awards}}<span class="total" alt="Achievement Count">{{earned_awards}}</span>{{/if}}
          {{#if earned_points}}<span class="gamerscore" alt="Gamerscore">{{earned_points}}</span>{{/if}}
        </div>
      </div>
      <div class="progress">
          <div class="progress-bar" role="progressbar" aria-valuenow="{{percent}}" aria-valuemin="0" aria-valuemax="100" style="width: {{percent}}%;">
              <span>{{percent}}%</span>
          </div>
      </div>
      {{else}}
      <div style="padding-top: 18px"></div>
      {{/if}}

      {{#if lastplayed}}
        <div class="lastplayed">{{formatDate lastplayed "short"}}</div>
      {{/if}}

    </div>

  <div style="clear:both"></div>
  </li>
</script>


