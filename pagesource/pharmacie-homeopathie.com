function verif_email(email)
		{
    	var reg = new RegExp('^[a-z0-9]+([_|\.|-]{1}[a-z0-9]+)*@[a-z0-9]+([_|\.|-]{1}[a-z0-9]+)*[\.]{1}[a-z]{2,6}$', 'i');
    	if(reg.test(email))				
        	return (true);
		else
			return (false);
		}
        
