	var dom2=$("div:eq(1)",dom);
	if(action=='big'){
		$("a:eq(1)",dom1).fadeOut(500,function(){
		dom1.slideUp(500,function(){
			dom2.slideDown(1000,function(){
				$("a:eq(1)",dom2).fadeIn(500,function(){
					if(bool==true){
						topadsettimeout = setTimeout("topad('small')",5000);
					}
				});
			});
		});
		});
	}
	else if(action=='small'){
		$("a:eq(1)",dom2).fadeOut(500,function(){
		dom2.slideUp(1000,function(){
			dom1.slideDown(500,function(){
				$("a:eq(1)",dom1).fadeIn(500,function(){
					clearTimeout(topadsettimeout);
				});
			});
		});
		});
	}
}
