<script type="text/javascript">
$(document).ready(function(){
	$(".jq_datepicker").datepicker({dateFormat:"dd/mm/yy"});
	$("#k").focus().autocomplete(kw);
	$("a[rel^='prettyPhoto']").prettyPhoto({theme: 'light_square',horizontal_padding: 5, slideshow:5000, autoplay_slideshow:true});
	
	slidein = false;

	if ($.cookie("design") != "off") {
		// After 5 seconds, auto Slide In
		timer_slideIn(5000);
		
		// After 0.5 seconds, auto Slide Out
		$('#slideout').delay(500).queue( function(next) {
			$('#slideout').css("right", "0px");
			next();
		});
	}
	
	$('#slideout_close').click(function() { // Mouseover the Slide aread
		cancel_t_sIn();
		$.cookie("desgin", "off", { expires: 0.5 }); // Delete cookie after 0.5 day
		$('#slideout').hide();
	});
	
	$('#slideout').mouseover(function() { // Mouseover the Slide area
		if (slidein == true) {
			$('#slideout').css("right", "0px");
		}
	});
	
	$('#slideout').mouseout(function() { // Mouseover the Slide area
		if (slidein == true) {
			$('#slideout').css("right", "-261px");
		}
	});
	
	function slideIn() {
		$('#slideout').css("-webkit-transition-duration", "0.5s");
		$('#slideout').css("-moz-transition-duration", "0.5s");
		$('#slideout').css("-o-transition-duration", "0.5s");
		$('#slideout').css("transition-duration", "0.5s");
		$('#slideout').css("right", "-261px");
		slidein = true;
	}

	function timer_slideIn(mSec) {
		t_sIn = setInterval( function(){ 
			if ($("#slideout:hover").length < 1) { // Not mouseover
				cancel_t_sIn();
				slideIn();
			}
		}, mSec );
	}

	function cancel_t_sIn() {
		clearInterval(t_sIn);
	}

	

	
});

