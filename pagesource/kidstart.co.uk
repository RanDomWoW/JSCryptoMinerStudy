    function RedirectUserForAuth(response)
    {
        var uid = response.authResponse.userID;
        var accessToken = response.authResponse.accessToken;
        var mergeQS = "";
      var urlString = "";
        if (document.URL.toLowerCase().indexOf("mergeaccounts.aspx") > -1)
        {
            mergeQS = "&merge=true";
        }
      if (document.URL.toLowerCase().indexOf("startsaving") > -1) {
        urlString = "&ssURL="+document.location.pathname.toLowerCase().replace("/","_").replace(".","-") + document.location.search.toLowerCase();
      }
        window.location = "https://www.kidstart.co.uk/login.aspx?facebooklogin=true&facebookuid=" + uid + "&accesstoken=" + accessToken + mergeQS + urlString;
    }
    
    function RedirectUserForRegistration(response)
    {
        var uid = response.authResponse.userID;
        var accessToken = response.authResponse.accessToken;
        window.location = "https://www.kidstart.co.uk/register.aspx?facebookregister=true&facebookuid=" + uid + "&accesstoken=" + accessToken;
    }
   
    function checkLoginStatus(response) {
        if(response && response.status == 'connected') {
            if (document.URL.toLowerCase().indexOf("logout=true") > -1)
            {
                window.location = "https://www.facebook.com/logout.php?next=" +
                 'https://www.kidstart.co.uk/' +
                 "&access_token=" + response.authResponse.accessToken;
                
                return;
            }
            
            if (document.URL.toLowerCase().indexOf("login.aspx?returningfromfacebook=true") > -1)
            {
                RedirectUserForAuth(response);
            }
            
            if (document.URL.toLowerCase().indexOf("register.aspx?returningfromfacebook=true") > -1)
            {
                RedirectUserForRegistration(response);
            }
            
            if (document.URL.toLowerCase().indexOf("mergeaccounts.aspx?returningfromfacebook=true") > -1)
            {
                RedirectUserForAuth(response);
            }
            
            if (document.URL.toLowerCase().indexOf("register.aspx") == -1
            && document.URL.toLowerCase().indexOf("mergeaccounts.aspx") == -1
            && document.URL.toLowerCase().indexOf("register-facebook-further-information.aspx") == -1
            && document.URL.toLowerCase().indexOf("passwordrecover.aspx") == -1
            && document.URL.toLowerCase().indexOf("registrationthanks.aspx") == -1)
            {
                if (getCookie() != null) {
                    RedirectUserForAuth(response);
                }
            }
        } else {
            if (document.URL.toLowerCase().indexOf("logout=true") > -1)
            {
                window.location = 'https://www.kidstart.co.uk/';
                return;
            }
        }
    }
    
    function getCookie()
    {
        var c_value = document.cookie;
        var c_start = c_value.indexOf(" KidStartFacebookAutoLogin=");
        if (c_start == -1)
          {
          c_start = c_value.indexOf("KidStartFacebookAutoLogin=");
          }
        if (c_start == -1)
          {
          c_value = null;
          }
        else
          {
          c_start = c_value.indexOf("=", c_start) + 1;
          var c_end = c_value.indexOf(";", c_start);
          if (c_end == -1)
          {
        c_end = c_value.length;
        }
        c_value = unescape(c_value.substring(c_start,c_end));
        }
        return c_value;
    }
    
