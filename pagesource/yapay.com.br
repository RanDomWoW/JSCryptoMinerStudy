                var title = document.querySelector('.popup-content .name');
                title.textContent = 'SUPERPAY';
                addClass(el, 'active');
            }

            var popup = document.querySelector('.popup-content');
            popup.onclick =  function(){
                var popup = document.querySelector('.popup-content');
                removeClass(popup, 'active');
            }
