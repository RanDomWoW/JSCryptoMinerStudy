    <script type="text/javascript">



        var isLoginn = false;

        $(document).ready(function () {

            var links = [];
            
            links.push({u: '/page/About-modern', t: 'درباره مدرن‌'});
           
            links.push({u: '/page/FAQ', t: 'پاسخ به پرسش های متداول'});
           
            links.push({u: '/page/JobOpportunities', t: 'فرصت‌های شغلی'});
           
            links.push({u: '/page/modern', t: 'مدرن'});
           
            links.push({u: '/page/Terms-Conditions', t: 'شرایط استفاده از سرویس ها و خدمات شرکت مدرن'});
           
            links.push({u: '/page/How-To-Order', t: 'ثبت سفارش'});
           
            links.push({u: '/page/Privacy-Policy', t: 'حریم خصوصی'});
           
            links.push({u: '/page/modernbon', t: 'معرفی مدرن بن'});
           
            links.push({u: '/page/ContactUs', t: 'تماس با ما'});
           
            links.push({u: '/page/commingsoon', t: 'به زودی.....'});
           

            if ($('.favoritbs a.order').length) {
                $('.favoritbs .noresult').remove();
            }
            else {                
                $('.favoritbs .more').remove();
            }
            
            for (var i = 0; i < links.length; i++) 
                $('[href="' + links[i].u + '"]').attr('title', links[i].t).text(links[i].t);


            if (isLoginn) {
                $('.nologin').remove();
            }
            else {
                $('.islogin').remove();
            }


            $.ajax({
                url: '/',
                dataType: "text",
                type: 'POST',
                data: { action: 'listCategory' },
                success: function (data) {
                    data = $.parseJSON(data);
                    var result = '';

                    for (var i = 0; i < data.list.length; i++) {

                        var tr = $('#' + data.list[i].Id);
                        if (tr && tr.length) {

                            var allcount = data.list[i].allcount;

                            var v = 2;
                            while (v < 25) {

                                if (allcount < 3 * v + (v / 2))
                                    break;
                                v++;
                            }
                            var lst = data.list[i].CategoryList;
                            if (!lst || !lst.length) continue;
                            var trc = 0;
                            var td = $('<td></td>').appendTo(tr);
                            var tdc = 1;
                            var to = 0;
                            for (var c = 0; c < lst.length; c++) {

                                var ul = $('<ul></ul>').appendTo(td);

                                if (to >= v) {
                                    td = $('<td></td>').appendTo(tr);
                                    tdc++;
                                    ul = $('<ul></ul>').appendTo(td);
                                    to = 0;
                                }

                                ul.append('<li class="title"><a href="/category/' + lst[c].Id + '/' + lst[c].Title + '">' + lst[c].Title + '</a></li>');
                                to++;


                                if (lst[c].CategoryList && lst[c].CategoryList.length) {
                                    var cts = lst[c].CategoryList;
                                    for (var k = 0; k < cts.length; k++) {

                                        if (to >= v) {
                                            td = $('<td></td>').appendTo(tr);
                                            tdc++;
                                            ul = $('<ul></ul>').appendTo(td);
                                            to = 0;
                                        }

                                        //  ul.append('<li class="title"><a href="/category/' + lst[c].Id + '/مشاهده موارد بیشتر"></a></li>');
                                        //     break;
                                        ul.append('<li class="item"><a href="/category/' + cts[k].Id + '/' + cts[k].Title + '">' + cts[k].Title + '</a></li>');
                                        to++;

                                    }
                                }

                            }
                            while (tdc < 4) {
                                td = $('<td></td>').appendTo(tr);
                                tdc++;
                            }


                        }

                    }

                },
                error: function (msg) {
                    //alert('خطاي 404 لطفاً به پشتيباني اطلاع دهيد');
                }
            });


            $('.account').click(function () {

                $(this).addClass('active');
                $('.topprofile').removeClass('hidden');
            });

            $(document).click(function (e) {
                var target = e.target;

                if (!$(target).is('.account') && !$(target).parents().is('.account') && !$(target).parents().is('.topprofile') && !$(target).parents().is('.topprofile')) {
                    $('.account').removeClass('active');
                    $('.topprofile').addClass('hidden');
                }
            });



            $('#signout').click(function () {

                var self = $(this);
                var val = self.html();
                self.html('لطفاً صبر کنيد...');

                $.ajax({
                    url: '/',
                    dataType: "text",
                    type: 'POST',
                    data: { action: 'signout' },
                    success: function (data) {
                        self.html(val);
                        location.reload(false)
                    },
                    error: function (msg) {
                        self.html(val);
                        //alert('خطاي 404 لطفاً به پشتيباني اطلاع دهيد');
                    }
                });
            });
        });
    </script>

