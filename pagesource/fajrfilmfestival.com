    $(function () {
        $('.btn_show_movie').click(function () {
            $.LoadingOverlay("show", {
                image: "http://fajrfilmfestival.com/assets/front/images/loader.gif",
                zIndex: 99999
            });
            var $modal = $('#myModal');
            var $this = $(this);
            var param = {
                id: $this.attr('data-id')
            };
            $.post('http://fajrfilmfestival.com/multimedia/movies/details', param, function (data) {
                $modal.find('.modal-title').text(data.title);
                $modal.find('.modal-description').text(data.description);
                $modal.find('#movie > source').attr('src', data.full_path_movie);
                $modal.find('#movie')[0].load();
                $.LoadingOverlay("hide");
                $modal.modal('show');
            }, 'json').fail(function (jqXhr) {

            }).always(function () {
                $.LoadingOverlay("hide");
            });

        });

        $('#myModal').on('hidden.bs.modal', function () {
            $("video").each(function () {
                this.pause()
            });
        })

        $('.bulletin-download').click(function () {
            $.get($(this).data('ajax-url'));
        });
    });
</script>
