                                                $(function () {
                                                    function show_errors(jqXhr) {
                                                        var alerts = {
                                                            success: 'عملیات با موفقیت انجام شد!',
                                                            failed: 'خطا: لطفا دوباره تلاش نمائید!'
                                                        };
                                                        if (jqXhr.status === 401)
                                                            $(location).prop('pathname', 'auth/login');
                                                        else if (jqXhr.status === 422) {
                                                            $errors = jqXhr.responseJSON;
                                                            errorsHtml = '<div class="alert alert-danger"><ul>';
                                                            $.each($errors, function (key, value) {
                                                                errorsHtml += '<li>' + value[0] + '</li>';
                                                            });
                                                            errorsHtml += '</ul></di>';
                                                            $('#form-errors').html(errorsHtml);
                                                            return false;
                                                        } else {
                                                            errorsHtml = '<div class="alert alert-danger">' + alerts.failed + '</div>';
                                                            $('#form-errors').html(errorsHtml);
                                                            return false;
                                                        }
                                                    }

                                                    $('#btn_submit_newsletter').on('click', function () {
                                                        /*$('.newsletter-div').LoadingOverlay("show", {
                                                            image: "../assets/front/images/loader.gif",
                                                            zIndex: 99999
                                                        });*/
                                                        var $this = $(this);
                                                        $this.prop('disabled', true);
                                                        $.post('http://fajrfilmfestival.com/newsletters', $('#frm-newsletter').serialize(), function (data) {
                                                            if (data.status) {
                                                                $('#form-errors').html('<span class="text-success">' + data.message + '</span>');
                                                                $('.div-newsletter').hide();
                                                            }
                                                            else/* if(data.status == false)*/
                                                            {
                                                                $('#form-errors').html('<div class="alert alert-danger">' + data.message + '</div>');
                                                                $('.newsletter-div').LoadingOverlay("hide");
                                                                return false;
                                                            }
                                                        }, 'json').fail(function (jqXhr) {
                                                            show_errors(jqXhr);
                                                            $('.newsletter-div').LoadingOverlay("hide");
                                                        }).always(function () {
                                                            $('.newsletter-div').LoadingOverlay("hide");
                                                            $this.prop('disabled', false);
                                                        });
                                                    });
                                                });
