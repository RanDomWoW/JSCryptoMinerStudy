    function GetCookie1 (name) {
        if (!navigator.cookieEnabled || !document.cookie) return 'noc';
        var arg = name + "=";
        var alen = arg.length;
        var clen = document.cookie.length;
        var i = 0;
        while (i < clen) {
            var j = i + alen;
            if (document.cookie.substring(i, j) == arg) return getCookieVal1 (j);
            i = document.cookie.indexOf(" ", i) + 1;
            if (i == 0) break;
        }
        return 'noc';
    }

