<script>
    new WOW().init();

    $(function() {
        $("#scroll-to-top").illBeBack();
    });

    $(document).ready(function(e){
        $('.search-panel .dropdown-menu').find('a').click(function(e) {
            e.preventDefault();
            var param = $(this).attr("href").replace("#","");
            var concept = $(this).text();
            $('.search-panel span#search_concept').text(concept);
            $('.input-group #search_param').val(param);
        });
    });

    $(document).ready(function(){

//        $(".slidingDiv").hide();
//        $(".show_hide").show();
//
//        $('.show_hide').click(function(){
//            $(".slidingDiv").slideToggle();
//        });


        $('.slidingDiv').hide();
        $(".show_hide").show();
        $('.show_hide').click(function(e){
            e.stopPropagation();
            $('.slidingDiv').slideToggle();
        });
        $('.slidingDiv').click(function(e){
            e.stopPropagation();
        });

        $(document).click(function(){
            $('.slidingDiv').slideUp();
        });

    });

    $(document).ready(function(){
        //jQuery-Mask-Plugin

        //Form Curso de Gestantes
        $('#data_nascimento').mask('99/99/9999');
        $('#data_parto').mask('99/99/9999');

        //Form de Simulador de Vendas
        $('#dnascimento').mask('99/99/9999');



    });


    $("#search").change(function(){
        var e = $(this).find('option:selected').attr('value');
        if(e == 'CNPJ'){
            $("#pesquisa").unmask().mask("99.999.999/9999-99");
        }else{
            $("#pesquisa").unmask();
        }
    });

    $('#turma').on('change', function () {
        $(".div_info_eventos, .alert").hide();
        if(this.value != ''){
            $("#div_info_eventos_" + this.value).show('fast');
        }
    });

    $(document).on("input", "#mensagem", function() {
        var limite = 200;
        var informativo = "caracteres restantes.";
        var caracteresDigitados = $(this).val().length;
        var caracteresRestantes = limite - caracteresDigitados;

        if (caracteresRestantes <= 0) {
            var comentario = $("textarea[name=mensagem]").val();
            $("textarea[name=mensagem]").val(comentario.substr(0, limite));
            $(".caracteres").text("0 " + informativo);
        } else {
            $(".caracteres").text(caracteresRestantes + " " + informativo);
        }
    });



    /* TABS RAMDOMIC REVENDAS */

    var shuffle = function( el ) {
        var i = el.length, j, tempi, tempj;
        if ( i == 0 ) return el;
        while ( --i ) {
            j       = Math.floor( Math.random() * ( i + 1 ) );
            tempi   = el[i];
            tempj   = el[j];
            el[i] = tempj;
            el[j] = tempi;
        }
    }

    $(document).ready(function() {
        $('#myModal').modal('show');
    });

    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-37758896-1']);
    _gaq.push(['_trackPageview']);

    (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();


    /* RAMDOMIC TABS REVENDAS DESKTOP*/

    var arr = jQuery('#items').find('li');
    shuffle( arr );
    jQuery('#items').html( arr );

    /* RAMDOMIC TABS REVENDAS MOBILE*/

    var arrrr = jQuery('#items_mobile').find('span');
    shuffle( arrrr );
    jQuery('#items_mobile').html( arrrr );


    /* RAMDOMIC VENDEDORES DESKTOP */

    var arrr = jQuery('#ecletica_ramdom').find('li');
    shuffle( arrr );
    jQuery('#ecletica_ramdom').html( arrr );

    var arrr = jQuery('#medeventos_ramdom').find('li');
    shuffle( arrr );
    jQuery('#medeventos_ramdom').html( arrr );

    var arrr = jQuery('#resolve_ramdom').find('li');
    shuffle( arrr );
    jQuery('#resolve_ramdom').html( arrr );

    var arrr = jQuery('#intervendas_ramdom').find('li');
    shuffle( arrr );
    jQuery('#intervendas_ramdom').html( arrr );

    var arrr = jQuery('#univendas_ramdom').find('li');
    shuffle( arrr );
    jQuery('#univendas_ramdom').html( arrr );



    /* RAMDOMIC VENDEDORES MOBILE */

    var arrr = jQuery('#ecletica_ramdom_mobile').find('li');
    shuffle( arrr );
    jQuery('#ecletica_ramdom_mobile').html( arrr );

    var arrr = jQuery('#medeventos_ramdom_mobile').find('li');
    shuffle( arrr );
    jQuery('#medeventos_ramdom_mobile').html( arrr );

    var arrr = jQuery('#resolve_ramdom_mobile').find('li');
    shuffle( arrr );
    jQuery('#resolve_ramdom_mobile').html( arrr );

    var arrr = jQuery('#intervendas_ramdom_mobile').find('li');
    shuffle( arrr );
    jQuery('#intervendas_ramdom_mobile').html( arrr );

    var arrr = jQuery('#univendas_ramdom_mobile').find('li');
    shuffle( arrr );
    jQuery('#univendas_ramdom_mobile').html( arrr );

    $(".msk_date").inputmask("date", {placeholder: "dd/mm/aaaa"});
    $('.telefone').mask('(00) 0000-0000');
    $('.cpf').mask('000.000.000-00');



</script>






