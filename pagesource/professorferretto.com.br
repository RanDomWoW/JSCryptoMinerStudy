	<script type="text/javascript">
		$(document).ready(function() {
			$.ajax("http://www.professorferretto.com.br/apis/banner.php?random=" + Math.random(), {
				success:function(data, textResult, jqXHR) {
					if (data.existe) {

						function irPlanos() {
							var top = document.getElementById("planos").offsetTop;
							window.scrollTo(0, top);
						}

						$(document).on('lity:close', irPlanos);
						var lightbox = lity(data.url, {esc:true});

						$(".lity-content").on("click", function(event) {
							lightbox.close();
						})

					}
				}
			})
		})
	</script>

