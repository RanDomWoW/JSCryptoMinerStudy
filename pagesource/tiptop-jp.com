<!-- インフォバナー -->
<script type="text/javascript">
(function ($) {
  $(function () {
    var display = function () {
      if ($(this).scrollTop() < 600) {
        $(".Bnr").fadeIn();
      } else {
	$(".Bnr").fadeOut();
      }
    };
    $(window).on("scroll", display); 

    $(".Bnr p.close a,.Bnr p.close-2 a").click(function () {
      $(".Bnr").fadeOut();
      $(window).off("scroll", display);

    });
  });
})(jQuery);
</script>

<!-- トップへ -->
<script type="text/javascript">
$(function() {
	var topBtn = $('#page-top');	
	topBtn.hide();
	$(window).scroll(function () {
		if ($(this).scrollTop() > 0) {
			topBtn.fadeIn();
		} else {
			topBtn.fadeOut();
		}
	});

	//スクロールしてトップ
    topBtn.click(function () {
		$('body,html').animate({
			scrollTop: 0
		}, 'slow');
		return false;
    });
});
</script> 

<!-- スクロールダウン -->
