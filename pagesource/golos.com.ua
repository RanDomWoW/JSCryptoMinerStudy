<script>


function init_slider()
{
    if ($(".slide").length)
    {
        check_slider_height();
        
        $(".slide:eq(0)").css("opacity", "1").addClass("active");
        
        $(".slide").css("visibility", "hidden");
        $(".slide").filter(".active").css("visibility", "visible");
        
        if ($(".slide").length>1)
        {
            setInterval(change_slide, 10000);
        }
    }
}


function change_slide()
{
    var cur_slide=$(".slide").filter(".active");
    if (!cur_slide.length)
    {
        cur_slide=$(".slide:eq(0)");    
    }
    
    var next_slide=$(cur_slide).next(".slide");
    if (!next_slide.length)
    {
        next_slide=$(".slide:eq(0)"); 
    }

    $(".slide").removeClass('active');
    $(next_slide).addClass("active");
    
    $(cur_slide).animate(
    {
        "opacity" : "0"
    }, 1500, function(){$(cur_slide).css("visibility", "hidden");});

    $(next_slide).css("visibility", "visible");
    $(next_slide).animate(
    {
        "opacity" : "1"
    }, 1500);
}


function check_slider_height()
{
    var w=$(window).width();
    
    var h=260;
    if (w<=350) { h=540; }
    else if (w<=375) { h=460; }
    else if (w<=380) { h=420; }
    else if (w<=400) { h=400; }
    else if (w<=480) { h=485; }
    else if (w<=535) { h=475; }
    else if (w<=543) { h=450; }
    else if (w<=560) { h=360; }
    else if (w<=570) { h=350; }
    else if (w<=595) { h=335; }
    else if (w<=600) { h=320; }
    else if (w<=620) { h=310; }
    else if (w<=630) { h=300; }
    else if (w<=660) { h=290; }
    else if (w<=685) { h=275; }
    else if (w<=753) { h=260; }
    else if (w<=770) { h=470; }
    else if (w<=780) { h=460; }
    else if (w<=788) { h=450; }
    else if (w<=805) { h=410; }
    else if (w<=820) { h=390; }
    else if (w<=830) { h=380; }
    else if (w<=880) { h=350; }
    else if (w<=905) { h=320; }
    else if (w<=960) { h=300; }
    else if (w<=1000) { h=280; }
    
    $("#slider_container").css("height", h+"px");
    $(".slide").css("height", h+"px");
    
    $(".slide").each(function()
    {
        var idx=$(".slide").index($(this));
        
        $(this).css("top", (-1*idx*(h-3))+"px");
    });
}

init_slider();

$(window).resize(function()
{
    check_slider_height();
})


</script>

