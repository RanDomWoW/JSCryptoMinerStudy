	$(".nav-menu-sub .nav-menu-container").on("click", function() {
		$(".nav-menu-alert").removeClass("active");
		if($(this).parent().hasClass("active")){
			$(this).parent().removeClass("active");
		}else{
			$(this).parent().toggleClass("active");
			if(!$(".nav-menu-sub").hasClass("active")){
				$(".nav-menu-sub").addClass("active");
			}
			redimensionarMenuSub();
		}
	});
	$(".nav-menu-alert .nav-menu-container").on("click", function() {
		$(".nav-menu-sub").removeClass("active");
		if($(this).parent().hasClass("active")){
			$(this).parent().removeClass("active");
		}else{
			$(this).parent().toggleClass("active");
			if(!$(".nav-menu-alert").hasClass("active")){
				$(".nav-menu-alert").addClass("active");
			} 
		}
	});
	$(document).ready(function() {
		redimensionarMenuSub()
		$( window ).resize(function() {
			redimensionarMenuSub()
		});
		$(".body").bind("mouseup", function() {
			$(".nav-menu-alert").removeClass("active");
			$(".nav-menu-sub").removeClass("active");
		});
		$(".footer").bind("mouseup", function() {
			$(".nav-menu-alert").removeClass("active");
			$(".nav-menu-sub").removeClass("active");
		});
		$(".bottom").bind("mouseup", function() {
			$(".nav-menu-alert").removeClass("active");
			$(".nav-menu-sub").removeClass("active");
		});
	});
	function redimensionarMenuSub() {
		var w = $('.nav-menu-sub').width();
		if(w > 220){
			$('#menu-nome').width(w - 1);
		}else{
			$('#menu-nome').width(220);
		}
	}
</script>
