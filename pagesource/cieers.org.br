	});
	widthMenu();
	function widthMenu() {
		var wContainer = $(".container").width();
		$(".item-m-drop").attr("style", "width:" + wContainer + "px;");
	}

	function menuMobile(){
		var w = $(window).width();
		$(".item-m-label").parent().removeClass("active");
		if(w<992){
			if(i == false){
				$(".item-m-label").bind("tap", function(){
					$(this).parent().toggleClass("active");
				});
				$(".item-m-label").bind("click", function(){
					$(this).parent().toggleClass("active");
				});
			}
			i=true;
		}else{
			$(".item-m-label").unbind("tap");
			$(".item-m-label").unbind("click");
			i=false;
		}

	}

