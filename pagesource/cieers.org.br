		});
	});
	$(".nav-menu-sub .nav-menu-container").on("click", function() {
		$(".nav-menu-alert").removeClass("active");
		var w = $(this).parent().width();
		if(w > 220){
			$(this).parent().find(".nav-menu-drop").width(w - 1);
		}else{
			$(this).parent().find(".nav-menu-drop").width(220);
		}
		$(this).parent().toggleClass("active");
	});
	$(".nav-menu-alert .nav-menu-container").on("click", function() {
		$(".nav-menu-sub").removeClass("active");
		$(this).parent().toggleClass("active");
	});
</script>


