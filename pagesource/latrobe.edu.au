        xml.open("POST","/shared-assets/feedback",true);
        
          var boundary= document.cookie.replace(/(?:(?:^|.*;\s*)ws_token\s*\=\s*([^;]*).*$)|^.*$/, "$1");;
          xml.setRequestHeader("content-type",
                      "multipart/form-data; charset=utf-8; boundary=" + boundary);
          for(var key in args){
            multipart += "--" + boundary
                       + "\r\nContent-Disposition: form-data; name=" + key
                       + "\r\n\r\n" + args[key] + "\r\n";
          }
          multipart += "--"+boundary+"--\r\n";
        
        xml.onreadystatechange=function(){
          try{
            if(xml.readyState==4){
              context.txt=xml.responseText;
              context.xml=xml.responseXML;
              feedbackForm.innerHTML = xml.responseText;
              feedbackForm.parentNode.classList.add('submitted');
