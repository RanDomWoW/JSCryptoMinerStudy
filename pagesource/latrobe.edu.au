    for (i = 0; i < toggleFields.length; i++) {
        toggleFields[i].classList.add('hidden');
    }
    var toggleFieldsControl = document.getElementsByName('q816209:q7');
    for (i = 0; i < toggleFieldsControl.length; i++) {
        toggleFieldsControl[i].addEventListener('change',function(){
            if (this.checked == true && this.value == '1') {
