function resetFeedback() {
    var feedbackForm = document.getElementById('form_email_816191');
    var feedbackSubmit = document.getElementById('form_email_816191_submit');
    var httpRequest;
    
    httpRequest = new XMLHttpRequest();
    
    if (!httpRequest) {
        alert('Giving up :( Cannot create an XMLHTTP instance');
        return false;
    }
    httpRequest.onreadystatechange = printContents;
    httpRequest.open('GET', '/shared-assets/feedback');
    httpRequest.send();
    
    function printContents() {
        if (httpRequest.readyState === XMLHttpRequest.DONE) {
          if (httpRequest.status === 200) {
            feedbackForm.innerHTML = httpRequest.responseText;
            conditionalFields();
          } else {
            alert('There was a problem with the request.');
          }
        }
    }
