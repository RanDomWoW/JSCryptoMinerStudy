  var iconLoginButton = $('div.topbar div.right div.btnIcon.login');
  
  function hideTopbarPanels() {
    hideMenuLeft();
    panelSide.addClass("hide");
    btnSearchSpace.addClass("hide");
  }

  function togglePanelSide() {
    //
    if(panelSide.hasClass('hide')) {
      //
      hideTopbarPanels();
      if(userLoggedDiv.is(':visible')) {
        openUserLogged();
      } else {
        openUserLogin();
      }
    } else {
      //
      panelSide.addClass('hide');
    }
  }

  function closePanelSlide() {
    //
    panelSide.addClass('hide');
    $('Body').css('overflow-y', 'auto');
  }

  function openUserLogin() {
    //
    panelSide.removeClass('hide');
    //
    userLogin.removeClass('hide');
    userLogged.addClass('hide');
    // reabilita o scroll
    $('Body').css('overflow-y', 'hidden');
  }

  function openUserLogged() {
    //
    panelSide.removeClass('hide');
    //
    userLogin.addClass('hide');
    userLogged.removeClass('hide');
    // tranca o scroll
    $('Body').css('overflow-y', 'hidden');
  }

  function hideMenuLeft() {
    $('Header').removeClass('menuOpened');
    $('Body').css('overflow-y', 'auto');
  }

  function showMenuLeft() {
    $('Header').addClass('menuOpened');
    $('Body').css('overflow-y', 'hidden');
  }

  function selectSearch () {
    btnSearch.toggleClass('gray');
  }

  function showSearchBar () {
    btnSearchSpace.removeClass("hide");
    SectionEditorial.addClass("hide");
  }

  function hideSearchBar () {
    btnSearchSpace.addClass("hide");
    SectionEditorial.removeClass("hide");
  }

  function menuInit() {
    btnMenu.click(function() {
      if($('Header').hasClass('menuOpened')) {
        hideMenuLeft();
      } else {
        hideTopbarPanels();
        showMenuLeft();
      }  
    });
    // close menu
    $('Header div.leftMenu div.panelRight').click(function() {
      hideMenuLeft();
    });
    // topbar init
    btnSearch.click(function() {
      //
      if(btnSearch.hasClass('gray')) {
        hideSearchBar();
      } else {
        hideTopbarPanels();
        showSearchBar();
      }
      selectSearch();
    });
    userLoggedButton.click(function() {
      togglePanelSide();
    });
    iconLoginButton.click(function() {
      //
      togglePanelSide();
    });
    //
    btnClose.click(function() {
      //
      closePanelSlide();
    });
    //
    leftClose.click(function() {
      //
      closePanelSlide();  
    });
    //
    mainLoginButton.click(function() {
      togglePanelSide();
    });
  }

  function handleScroll() {
    var screen_w = $(window).width();
    var scroll_top_break = 40;
    if(screen_w > 980) {
      scroll_top_break = 120;
    }
    //console.log(screen_w, scroll_top_break);
    if(screen_w >= 728) {
      $(window).scroll(function() {
        if($(window).scrollTop() > scroll_top_break) {
          $('Header').addClass('minimized');
          $('Containers').addClass('minimized');
        } else {
          //
          $('Header').removeClass('minimized');
          $('Containers').removeClass('minimized');
          // verifica se a busca ta exibida
          if(btnSearchSpace.is(':visible')) {
            hideSearchBar();
            //hideAdvancedSearch();
            //hideAdvancedSearchOpen();
            SectionEditorial.show();
            selectSearch();
          }
        }
      });
    }
  }



  $(window).load(function() {
    // manipula o click do btnIcon.menu
    menuInit();
    // manipula o scroll para suprimir a barra
    handleScroll();
  });

</script>
