	<script>
		'use strict';
		(function(){
			// おいしい！ひらめき おすすめコンテンツ
			var jsonObjHirameki = new XMLHttpRequest();
			jsonObjHirameki.open('get', '/ajax/mega_menu/delicious_inspiration.json', true);
			jsonObjHirameki.onload = function(){
				var data = JSON.parse(this.responseText);
				var items = data.mega_menu_delicious_inspiration[0].ng_news;

				for(var i=0; i<6; i++) {
					var appHTML = '';
					if(i === 2 || i ===3){
						appHTML += '<li class="c-header__megamenu__imglist__item is-sp_menu ng_news">';
					} else{
						appHTML += '<li class="c-header__megamenu__imglist__item ng_news">';
					}

					if(items[i].n_url === '') {
						appHTML += '<span class="c-header__megamenu__imglist__link">';
					} else {
						if(items[i].n_url.indexOf('http') == 0){
							appHTML += '<a href="'+ encodeURI(items[i].n_url) +
								'" target="_blank" class="c-header__megamenu__imglist__link">';
						} else {
							appHTML += '<a href="'+ encodeURI(items[i].n_url) +
								'" class="c-header__megamenu__imglist__link">';
						}
					}
					appHTML += '<img src="./news_file/file/'+ encodeURI(items[i].n_image_file_name) +
						'" class="c-header__megamenu__imglist__img">'+
						'<p class="c-header__megamenu__imglist__caption">'+ items[i].n_title +'</p>';
					if(items[i].n_url === '') {
						appHTML += '</span>';
					} else {
						appHTML += '</a>';
					}
					var elem;
					if(i < 4) {
						elem = document.getElementById('megamenu_delicious_inspiration_1');
					} else if(i === 4) {
						elem = document.getElementById('megamenu_delicious_inspiration_2');
					} else if(i === 5) {
						elem = document.getElementById('megamenu_delicious_inspiration_3');
					}
					elem.insertAdjacentHTML('beforeend', appHTML);
				}
			}
			jsonObjHirameki.send(null);

			// 新着キャンペーン
			var jsonObjCampaign = new XMLHttpRequest();
			jsonObjCampaign.open('get', '/ajax/mega_menu/products_campaign.json', true);
			jsonObjCampaign.onload = function(){
				var data = JSON.parse(this.responseText);
				var items = data.mega_menu_products_campaign[0].ng_news;
				var appHTML = '';

				for(var i=0; i<2; i++) {
					appHTML += '<li class="c-header__megamenu__imglist__item ng_news">';

					if(items[i].n_url === '') {
						appHTML += '<span class="c-header__megamenu__imglist__link">';
					} else {
						if(items[i].n_url.indexOf('http') === 0){
							appHTML += '<a href="'+ encodeURI(items[i].n_url) +
								'" target="_blank" class="c-header__megamenu__imglist__link">';
						} else {
							appHTML += '<a href="'+ encodeURI(items[i].n_url) +
								'" class="c-header__megamenu__imglist__link">';
						}
					}
					appHTML += '<img src="./news_file/file/'+ encodeURI(items[i].n_image_file_name) +
						'" class="c-header__megamenu__imglist__img">'+
						'<p class="c-header__megamenu__imglist__caption">'+ items[i].n_title +'</p>';
					if(items[i].n_url === '') {
						appHTML += '</span>';
					} else {
						appHTML += '</a>';
					}
				}
				var elem = document.getElementById('megamenu_news_campaign');
				elem.insertAdjacentHTML('beforeend', appHTML);
			}
			jsonObjCampaign.send(null);
		})();
	</script>
