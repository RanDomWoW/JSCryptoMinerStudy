      function apply_style(){
      var fsize;
      jQuery(".ipsAttachLink").each(function () {
        var request;
        var dt=jQuery(this);
        var kilobyte = 1024;
        var megabyte = kilobyte * 1024;
        var gigabyte = megabyte * 1024;
        var terabyte = gigabyte * 1024;
        var precision=1;
        var cls="file";
        if (jQuery(this).find("img").length) {
          var imgalt=jQuery(this).find("img").attr("alt");          
          if(imgalt.length>10){
          imgalt=imgalt.substring(0, 10)+"..";
          }
          
          if(jQuery(this).attr("title")==undefined){
          	
			         //jQuery(this).append("<div class='custtitle' style='display:none;'>zzzzzz");
          	
          }
          
          if(jQuery(this).attr("title")==undefined){
            jQuery(this).attr("title",imgalt);
          }
        }else{
           htms=dt.html();
           //alert(jQuery(this).attr("title"));
           if(jQuery(this).attr("title")==undefined){
            jQuery(this).attr("title",htms);
           }
           if(htms.length>10){      
            htms=htms.substring(0, 10)+"..";
           }
           jQuery(this).html(htms);
        }
        request = jQuery.ajax({
          type: "HEAD",
          url: jQuery(this).attr("href"),
          success: function () {
            bytes=request.getResponseHeader("Content-Length");
            ftype=request.getResponseHeader("content-type");
            //alert(ftype);
            var icontext="fa-file";
            if(ftype=="image/png" || ftype=="image/jpg" || ftype=="image/jpeg" || ftype=="image/gif"){
              icontext="fa-file-image-o";
              cls="preview";
              //dt.removeClass("ipsAttachLink_image");
              //dt.removeClass("ipsAttachLink");
            }
            if(ftype=="text/plain;charset=UTF-8"){
              icontext="fa-file-text-o";
              cls="txtcls";
            }
            if(ftype=="video/mp4;charset=UTF-8"){
              icontext="fa-file-video-o";
              cls="videocls";
            }
            if(ftype=="application/pdf;charset=UTF-8"){
              icontext="fa-file-pdf-o";
              cls="videocls";
            }
            if(ftype=="application/msword;charset=UTF-8"){
              icontext="fa-file-word-o";
              cls="videocls";
            }
            if(ftype=="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet;charset=UTF-8"){
              icontext="fa-file-excel-o";
              cls="videocls";
            }
            if(ftype=="application/zip;charset=UTF-8"){
              icontext="fa-file-zip-o";
              cls="videocls";
            }
            if(ftype=="audio/mpeg;charset=UTF-8"){
              icontext="fa-file-audio-o";
              cls="videocls";
            }            
            if ((bytes >= 0) && (bytes < kilobyte)) {
              fsize= bytes + " B";
            } else if ((bytes >= kilobyte) && (bytes < megabyte)) {
              fsize= (bytes / kilobyte).toFixed(precision) + " KB";
            } else if ((bytes >= megabyte) && (bytes < gigabyte)) {
              fsize= (bytes / megabyte).toFixed(precision) + " MB";
            } else if ((bytes >= gigabyte) && (bytes < terabyte)) {
              fsize= (bytes / gigabyte).toFixed(precision) + " GB";
            } else if (bytes >= terabyte) {
              fsize= (bytes / terabyte).toFixed(precision) + " TB";
            } else {
              fsize= bytes + " B";
            }
            
            
            var htm=dt.html();
            dt.addClass(cls);
            htm=htm.replace(fsize,"");
            //var prep='<i class="fa fa-paperclip"> ';
            
            
            htm=htm.replace(icon,"");
            htm=htm+fsize;
              
            if(cls!="preview"){
            
            var clear='<div style="clear:both;"><\/div>';
            htm=htm.replace(clear,"");    
            dt.html(icon+htm+clear);
             
            }
            
          }
        });        
      });
  }
    jQuery(".ipsType_reset").prev('div.clearfix').remove();
