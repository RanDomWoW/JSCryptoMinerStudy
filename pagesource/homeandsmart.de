    <script type="text/javascript">
        document.addEventListener("DOMContentLoaded", function(){
            jQuery(window).resize(function () {
                jQuery('.preis_tracker_frame').each(function (i, obj) {
                        reseizePreisTrackerIFrame(obj);
                });
            });

            // add some funny dots on line endings if text is to long ..
            // see here: http://dotdotdot.frebsite.nl/
            jQuery('.dotdotdot').dotdotdot({
                ellipsis : '... ',
                wrap	 : 'word',
                watch	 : true,
                height   : null
            });



            jQuery(window).resize(function(){
                jQuery('#menu-main ul').each(function(){
                    jQuery(this).hide();
                });
            });


            /****  side bar menu  ***/
            jQuery('#side-panel-trigger').on('click, tap', function(){
                jQuery(document.body).addClass('side-menu-panel-active');
            });


            /*jQuery('#menu-main li a').on('click tap', function(e) {*/
            jQuery('#menu-main li a').each(function(){

                jQuery(this).on('click tap', function(e) {
                    if (jQuery('body').hasClass('side-menu-panel-active')) {

                        if (jQuery(this).hasClass("menuback")) {
                            var ul = jQuery(this).closest('ul');
                            ul.removeClass("sub-menu-active");
                            setTimeout(function(){
                              //set display none after animation has finished
                              ul.removeClass("prepare-animation");
                            }, 200);

                        } else if ($(this).next("ul").length) {
                            e.preventDefault();
                            window.scrollTo(0, 1);
                            var ul = jQuery(this).next('ul');
                            //the ul needs to have display none for overflow scrolling to work on android
                            //but needs to be block for the slide animation to work
                            //also we need !important, which cannot be set with jquery css
                            //this first class sets display block, after a timeout the position is animated
                            ul.addClass("prepare-animation").stop(false,true);
                            setTimeout(function(){
                              ul.addClass("sub-menu-active");
                            }, 30);
                        }
                    }
                });
            });

            jQuery('#side-panel-trigger-close, .top-search-trigger').on('click tap', function(e) {

                jQuery('.sub-menu-active').css('margin-left', '-100%');

                jQuery(document.body).removeClass('side-menu-panel-active');
                jQuery(".sub-menu-active").removeClass("sub-menu-active");

                setTimeout(function(){
                    jQuery('#menu-main ul').removeAttr( 'style' );
                }, 500);
            });

        });
    </script>

   

    
        <script type="text/javascript">

            $('.hs_anbieter_link a').on('click', function(event) {

                var $this = $(this);

                if((typeof $this.data('type')  !== 'undefined') &&
                    ($this.data('type') == 'Kategoriebox')){

                    var eventData = {
                        hitType: 'event',
                        eventCategory: 'Affiliate Clickout',
                        eventAction: $this.data('name') + ' ' +$this.attr('href') + ' ' + $this.data('type'),
                        eventLabel: window.location.href
                    };
                    ga('send', eventData);

                }

                else if(typeof $this.attr('data-affiliatePlatform') !== 'undefined') {

                    var affiliatePlatform = $(this).attr('data-affiliatePlatform');
                    ga('send', 'event', 'Affiliate Clickout', affiliatePlatform, window.location.href);

                } else {

                    var eventAction = ($this.data('variation') ? $this.data('variation') + ' ' :'')
                            + ($this.data('bundle') ? $this.data('bundle') + ' ' : '')
                            + ($this.data('category') ? $this.data('category') + ' ' : '')
                            + ($this.data('ean') ? $this.data('ean') + ' ' : '')
                            + $this.data('type') + ' '
                            + $this.data('partner');

                    var eventData = {
                            hitType: 'event',
                            eventCategory: 'Affiliate Clickout',
                            eventAction: eventAction,
                            eventLabel: window.location.href
                    };
                    ga('send', eventData);
                }
            });


            $('a.eness-link').on('click', function() {
                var affiliatePlatform = $(this).attr('data-affiliatePlatform');
