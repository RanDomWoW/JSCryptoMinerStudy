                <script type="text/javascript">
        var resizeTimeout = null;
        var content = $('#content');
        var skyscraper = $('#skyscraper');

        var skyscraperWP = skyscraper.waypoint(function(direction) {
            switch(direction) {
                case 'up':
                    $(this.element).css({
                        'position': 'absolute',
                        'top': 'auto',
                        'left': '100%'
                    });
                break;
                case 'down':
                $(this.element).css({
                    'position': 'fixed',
                    'top': '0',
                    'left': content.offset().left + content.width() + 'px'
                });
                break;
            }
        },
        {
            offset: function() {
                if($(document).scrollTop() <= $('#wrapper').offset().top) {
                    return 0;
                }
                else {
                    return $(document).scrollTop() - $('#wrapper').offset().top;
                }
            }
        });

        $(window).resize(function() {
            Waypoint.disableAll();
            if(resizeTimeout != null) window.clearTimeout(resizeTimeout);
            resizeTimeout = window.setTimeout(function() {
                Waypoint.enableAll();

                if((window.innerWidth - content.width()) / 2 < skyscraper.width())
                    skyscraper.css('visibility', 'hidden');
                else {
                    if($(document).scrollTop() >= skyscraperWP[0].triggerPoint) {
                        skyscraper.css({
                            'position': 'fixed',
                            'top': '0',
                            'left': content.offset().left + content.width() + 'px'
                        });
                    } else {
                        skyscraper.css({
                            'position': 'absolute',
                            'top': 'auto',
                            'left': '100%'
                        });
                    }
                    skyscraper.css('visibility', 'visible');
                }
            }, 50);
        });
        if((window.innerWidth - content.width()) / 2 < skyscraper.width())
            skyscraper.css('visibility', 'hidden');
        </script>
            
