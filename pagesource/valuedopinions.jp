<script>
(function() {
  var errorCount = 0;
  window.onerror = function(message, file, line, column, errorObj) {
    if (++errorCount > 10) return;
    
    //format arguments so they can be serialized properly
    var args = {
      message: message,
      file: file,
      line: line,
      column: column,
      errorObj: errorObj
    };
    var autoProps = ['message', 'file', 'line', 'column'];
    for( var i=0; i<autoProps.length; i++ ){
      if( typeof args[autoProps[i]] === 'object' && args[autoProps[i]].length ){
        //assume array-type object
        args[autoProps[i]] = Array.prototype.map.call(args[autoProps[i]], function (data){
          return data.toString();
        }).join(', ');
      }
      else {
        args[autoProps[i]] = args[autoProps[i]] ? args[autoProps[i]].toString() : undefined;
      }
    }

    //handle errorObj in special way
    if (!args.errorObj) args.errorObj = {};
    if (!args.errorObj.stack) args.errorObj.stack = "No stack available.";
    
    var xhr = new XMLHttpRequest();
    xhr.open("POST", window.corona.apiUrl + "log/browser/error", true);
    xhr.withCredentials = true;
    xhr.send(JSON.stringify({
      url: window.location.href,
      message: args.message, 
      file: args.file, 
      line: args.line, 
      column: args.column, 
      stack: args.errorObj.stack.toString()
    }));
  };
})();
</script>

