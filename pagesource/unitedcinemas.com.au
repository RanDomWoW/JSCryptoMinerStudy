		var options = { 
			dataType:'json',
			success:    function(response) { 
						var status = response.status;
						if(status == 'error'){
							$('#errorMessage').show();
						} else if(status == 'success') {
							$('#user-window').show();
							$('#join-window').hide();
							$('#errorMessage').hide();
							$('.username').html(response.username+' '+response.surname);
							$('#member-id').html(response.member_id);
							$('#points').html(response.points);
						}
					} 
		}; 
		$(function() { 
			
			$('#join-form').ajaxForm(options); 
		}); 
</script>
