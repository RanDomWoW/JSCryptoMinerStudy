<script>
var prefix = ["02", "03", "04", "08", "09", "072", "073", "074", "076", "077", "050", "052", "053", "054", "055", "056", "057", "058", "059", "012"];
function isInt(value){
      return !isNaN(value) && parseInt(value) == value;
}
$(document).ready(function(){
	
	
	$("#form_leads_footer select[name=interest_id]").change(function() {
		$("#submit_form_footer").attr('class', $("#form_leads_footer select[name=interest_id]").find('option:selected').text());
		
	});

    $.validator.addMethod("validphone", function(phone_number, element) {

      var seperator = phone_number.split('-');
        if(seperator.length > 1){
          if(isInt(seperator[0]) && isInt(seperator[1])) {

             var kidomet = prefix.indexOf(seperator[0]) != -1;
             if(!kidomet){
                 $.validator.messages.validphone = "קידומת אינה חוקית";
                 return false;
             }
             if(seperator[0].length > 1) {

                 if(seperator[1].length == 7) {
                     return true;
                 }else{

                     if(seperator[1].length < 7)
                                $.validator.messages.validphone =  "טלפון קצר מדי";
                     else $.validator.messages.validphone =  "טלפון ארוך מדי";

                     return false;
                 }

             } else return false;

          } else return false;

       } else {
          $.validator.messages.validphone = "הטלפון אינו תקין";
          var result = isInt(phone_number);
          if(!result)
               return false;
          if(phone_number.length == 10 || phone_number.length == 9)
                    return true;
           if(phone_number.length < 9){
               $.validator.messages.validphone = "טלפון קצר מדי";
           }else {
               $.validator.messages.validphone = "טלפון ארוך מדי";
           }
          return false;

      }
    },"הטלפון אינו תקין");

     $.validator.addMethod("validfname", function(first_name, element) {
	  if( !(RegExp(/[!$%@^&*()_+|~=`{}\[\]:";'<>?,.\/]/).test(first_name)) )
                    {
                        return true;
                    }
                    else
                    {
                        return false;
                    }
     }, "שם פרטי לא תקין");
     
     $.validator.addMethod("validlname", function(family_name, element) {
	  if( !(RegExp(/[!$#@%^&*()_+|~=`{}\[\]:";'<>?,.\/]/).test(family_name)) ) 
                    {   
                        return true;
                    }
                    else
                    {
                        return false;
                    }
     }, "שם משפחה לא תקין");

    $("#form_leads_footer").validate({
		rules: {
			first_name:				{ required:  function() {
											$('#form_leads_footer input[name=first_name].watermark').val('');											
											return true;  } , validfname: true
								},
			family_name:				{ required:  function() {	
											$('#form_leads_footer input[name=family_name].watermark').val('');											
											return true;  } , validlname: true
								},
			email:				{ required: true , email: true },
            phone:              { required: true  , validphone:true  }
		},
		messages: {
			first_name:				{ required:  "שדה נדרש" },
			family_name:				{ required:  "שדה נדרש" },
			email:				{ required:  "שדה נדרש", email: 'כתובת דוא"ל לא חוקית' },
		    phone:  { required: "יש למלא טלפון" , number: "יש להכניס מספרים בלבד"}
        }	,
		 submitHandler: function(form) {				
			var tel = '';
			if ($("#form_leads_footer input[name=phone]").hasClass('watermark')) {	
				tel = '';
			}
			else
				tel = $("#form_leads_footer input[name=phone]").val();
			$('#submit_form_footer').val('שולח');
            $('#submit_form_footer').prop('disabled', true);			
			var nl = 0;
			if ($("#form_leads_footer input[name=newsletter]").is(':checked'))
				nl = 1;
			
			$.post('http://www.hit.ac.il/?cmd=leads_manage&act=send_ajax',
				{  
				   first_name: $("#form_leads_footer input[name=first_name]").val(), 
				   family_name: $("#form_leads_footer input[name=family_name]").val(), 
				   email: $("#form_leads_footer input[name=email]").val(), 
				   phone: tel, 
				   interest_id: $("#form_leads_footer select[name=interest_id]").val(), 
				   newsletter: nl,
				   leads_page: $("#form_leads_footer input[name=leads_page]").val()
				  },
				function (data) {
                    _gaq.push(['_trackEvent', 'lead', 'Interested', $("#form_leads_footer select[name=interest_id]").find('option:selected').text() ]);
					dataLayer.push({'event': 'FormSubmitted'});
					$('#form_leads_footer').html(data);	
					$('#form_leads_footer').css('padding','0px');					
					if ($(form).parents('#leads_dialog').length == 0) {
						$('#form_leads_footer').width('990px');
					}
			});				 
			return false;
		 }
	});
	$("#form_leads_footer input[name=first_name]").watermark('שם פרטי');
	$("#form_leads_footer input[name=family_name]").watermark('שם משפחה');
	$("#form_leads_footer input[name=email]").watermark('דואר אלקטרוני');
	$("#form_leads_footer input[name=phone]").watermark('טלפון');
	//$("#form_leads_footer input[name=mobile]").watermark('Mobile');
	
	//$("#form_leads_footer input[name=field_of_interest]").watermark('Field of interest');
	/*$("#form_leads_footer").submit(function() {
		$("#form_leads_footer input[name=phone]").blur();
	});*/
});
</script>

