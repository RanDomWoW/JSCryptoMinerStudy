                if (typeof fullGamesList === 'undefined' || fullGamesList === null || fullGamesList === false || $(window).width() < 440) {

                var sizeLi = $(".lotteries-list").size();
                x = 6;
               
                var itemCount = 7; // The amount of additional lotteries to display when .showMore is clicked
                    $(".lotteries-list:lt(" + x + ")").show();
                    $(".showMore").css("display", "inline-block");

                    $(".showMore").click(function () {
                        x = (x + itemCount <= sizeLi) ? x + itemCount : sizeLi;
                        
                        $(".lotteries-list:lt(" + x + ")").slideDown();
                        if ($(window).width() < 440) {
                            $('.lotteries-list.syndicate_block').slideDown();
                        }
                        $(".showMore").fadeOut("fast", function () {
                            $(".showLess").css("display","inline-block");
                        });
                    });
                    $(".showLess").click(function () {
                        x = (x - itemCount < 0) ? 9 : x - itemCount;
                        $(".lotteries-list").not(":lt(" + x + ")").fadeOut();
                        if ($(window).width() < 440) {
                            $(".wlc_games ul li.syndicate_block").fadeOut();
                        }
                        $(".showLess").fadeOut("fast", function () {
                            $(".showMore").css("display", "inline-block");
                        });

                    });
                }
                else {
                    $('.wlc_gameList.main-page .wlc_games ul li').css("display", "inline-block");
                    $('.showMore').css("display", "none");
                    $('.showLess').css("display", "none");
                    $('.syndicate_block_last').remove();
                }
            });

        </script>
