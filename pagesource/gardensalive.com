<script type="text/javascript">
    function lookupfloating(inputString) {
    	$("#searchinput-floating").blur(function () { setTimeout("clearResultsfloating()", 500) });
        var searchstring = trim(inputString);
        if (searchstring.length > 3) {
          $("#LoadingSearch-floating").show();
          $.getJSON("/services/michiganbulb/get_search.php", { store: "gardens" , keyword: "" + inputString + "" },
          	function (data) { 
              if (data.length > 0) {
                $("#LoadingSearch-floating").hide();
                $("#suggestions-floating").show();
                var result = data[0].itemList;
                var itemCount = data[0].count;
                $('#autoSuggestionsList-floating').html('<div style=\"margin-left:45%;padding:5px;\"></div>');
                var searchstr = '<ul style=\"margin:0px; padding:0px;\">';
                if(data[0].no_matches == 'y'){
                  searchstr += '<li style=\"margin:0px; padding:0px;\"><div style=\"padding:15px 5px;background-color: #d4d4d4; font-size:11px; color:#98232b; position:relative; z-index:10;\">';
                  searchstr += 'No Matches found for keyword: <strong>' + inputString + '</strong>.<br><strong style=\"color:#3d3d3d;\">Showing the top 8 best sellers</strong>';
                  searchstr += '<div></li>';
                }else{
                  searchstr += '<li style=\"margin:0px; padding:0px;\"><div style=\"padding:15px 5px;background-color: #d4d4d4; font-size:11px; position:relative; z-index:10;\">';
                  if (itemCount < 5){
                      searchstr += 'Showing the top '+ itemCount  + ' results out of '+ itemCount  + ' for keyword: \"' + inputString + '\".';
                  }else{
                      searchstr += 'Showing the top 5 results out of '+ itemCount  + ' for keyword: <strong>' + inputString + '</strong>.';
                  }
                  searchstr += '<div></li>';
                }
                var actualProducts=[];
                var articles=[];
                var count1 = 0;
                var count2 = 0;
                $.each(result, function (i, item) {
                  if(i < 5){
                    var firstLetter = item.sku.charAt(0);
                    firstLetter = firstLetter.toUpperCase();
                    if ($.isNumeric( firstLetter) || item.sku == 'CM12') {
                      actualProducts[count1] = item;
                      count1++;
                    }else{
                      articles[count2] = item;
                      count2++;
                    }
                  }
                });
                
                if(actualProducts.length > 0){
                  searchstr += '<li style=\"margin:0px; padding:0px;\"><div style=\"padding:15px 5px;background-color: #efefef;\">';
                  searchstr += '<span style=\"font-size:13px;color:#3d3d3d;display: block;margin: 5px 0;\">Products:</span>';
                  searchstr += '<div></li>';
                  $.each(actualProducts, function (i, item) {
                    if(i < 5){
                      searchstr += '<li>';
                      searchstr += '<a href=\"/product/' + item.url_name + '\" class="gotopage" id=\'/product/' + item.url_name + '\'>';
                      searchstr += '<table border=\"0\" cellpadding=\"0\" cellspacing=\"3\"><tr><td>';
                      searchstr += '<img src=\"https://s3.amazonaws.com/cdn.gardensalive.com' + item.web_image + '\" width=\"50\" height=\"50\" />';
                      searchstr += ' </td><td valign=\"top\">';
                      searchstr += '<span style=\"font-size:13px;color:#3d3d3d;display: block;margin: 0 0 5px;\">' + item.name + '</span> <br>';
                      searchstr += '<strong style=\"font-size:11px; color:#3d3d3d;\">Item # '+ item.sku + '</strong>';
                      searchstr += '</td></tr></table>';
                      searchstr += '</a>';
                      if(i != 4 && (i+ 1) != itemCount){  
                        searchstr += '<hr>';
                      }             
                      searchstr += '</li>';
                    }
                  });
                }
                
                if(articles.length > 0){
                  searchstr += '<li style=\"margin:0px; padding:0px;\"><div style=\"padding:15px 5px;background-color: #d4d4d4;\">';
                  searchstr += '<span style=\"font-size:13px;color:#3d3d3d;display: block;margin: 5px 0;\">Articles:</span>';
                  searchstr += '<div></li>';
                  $.each(articles, function (i, item) {
                    if(i < 5){
                      searchstr += '<li>';
                      searchstr += '<a href=\"/product/' + item.url_name + '\" class="gotopage" id=\'/product/' + item.url_name + '\'>';
                      searchstr += '<table border=\"0\" cellpadding=\"0\" cellspacing=\"3\"><tr>';
                      searchstr += ' <td valign=\"top\">';
                      searchstr += '<span style=\"font-size:13px;color:#3d3d3d;display: block;margin: 5px 0;\">' + item.name + '</strong>';
                      searchstr += '</td></tr></table>';
                      searchstr += '</a>';
                      if(i != 4 && (i+ 1) != itemCount){
                        searchstr += '<hr>';
                      }
                      searchstr += '</li>';
                    }
                  });
                }
                searchstr += '<li style=\"margin:0px; padding:0px;\"><div style=\"padding:15px 5px;background-color: #d4d4d4;\">';
                searchstr += '<a href=\"/category/s?keyword='+ inputString +'\" style=\"font-size:11px;\">More >></a>';
                searchstr += '<div></li>';
                searchstr += '</ul>';
                $("#autoSuggestionsList-floating").html(searchstr);
              }else{
              	$("#LoadingSearch-floating").hide(); 
              }  
          });
        }else {
          $("#autoSuggestionsList-floating").html('');
          $("#suggestions-floating").hide();
        }
    }
    /*$('.gotopage').live('click', function () {
      var hreflid = $(this).attr('id');
      window.location = hreflid;     
    });*/
    // clear auto complete box
    function clearResultsfloating() {
      $("#autoSuggestionsList-floating").html('');
      $("#suggestions-floating").hide();
    }
    function trim(stringToTrim) {
    	return stringToTrim.replace(/^\s+|\s+$/g, "");
    }   
</script>

