			scrollingHeadlines();
		} else if($("#spn-content .ui-widget.app.headlines").length) {
			$("#spn-content .ui-widget.app.headlines div.ui-article").each(function() {
				$cont = $("<div class='headline-info-container'></div>");
				$cont.append($(".ui-article-title",this));
				$cont.append($(".ui-article-description",this));
				$cont.append($(".sub-link",this));
				$cont.appendTo(this);
				$(this).addClass("ui-clear");
			});
			
			$("#spn-content .ui-widget.app.headlines").each(function() {
				$(".app-level-social-rss",this).appendTo(".ui-widget-header",this);
				$(".ui-widget-header",this).addClass("ui-clear");
				
				var headlineNum = $("li",this).size();
				if(headlineNum > 2) {
					var headlineSplit = Math.floor(headlineNum / 2);
					$(".ui-articles",this).addClass("col1");
					$(".ui-widget-detail",this).addClass("ui-clear").append('<ul class="ui-articles col2"></ul>');
					
					for (var i = 0; i < headlineSplit; i++) {
						$(".col2",this).prepend($(".col1 li",this).last());
					}
				}
			});
			
			
		}
	}
	
	function scrollingHeadlines() {
		var getWidth = $(window).width();
		var headlineCount = $("#hp-slideshow-content .ui-widget.app.headlines div.ui-article").size();
		//DOC custom global icons scrolling
		if(headlineCount > 2) {
			$("#hp-slideshow-content .headline-btn").show();
			
			var articlesHeight = (214 * headlineCount) - 428;
			$("#hp-slideshow-content .headline-btn.scroll-down").click(function() {
				var position = $("#hp-slideshow-content .ui-widget.app.headlines .ui-articles").position();
				position = position.top - 214;
				clampedPosition = position.clamp(-articlesHeight, 0);
				$("#hp-slideshow-content .ui-widget.app.headlines .ui-articles").animate({ top: clampedPosition}, 250);
			});
			
			$("#hp-slideshow-content .headline-btn.scroll-up").click(function() {
				var position = $("#hp-slideshow-content .ui-widget.app.headlines .ui-articles").position();
				position = position.top + 214;
				clampedPosition = position.clamp(-articlesHeight, 0);
				$("#hp-slideshow-content .ui-widget.app.headlines .ui-articles").animate({ top: clampedPosition}, 250);
			});
		}
	}	
	
