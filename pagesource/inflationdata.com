							<!-- #BeginEditable "column%202%20middle%20bottom" -->
							<!--do no remove classes-->
							<h4 class="widget-title widgettitle">Inflation 
							Adjusted Prices</h4>							
							<div class="page type-page status-publish entry gfwa-1 gfwa-odd">
								<p>
