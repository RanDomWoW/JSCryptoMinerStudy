	// メニューチェックボックスの切り替え
	function checkMenu(index, targetName, doSearch) {
		var num = index;
		var tdName = "menu_" + targetName;
		var tdObj = document.getElementById(tdName);

		if(1 == checkFlg[num]) {
			checkFlg[num] = 0;
			tdObj.style.backgroundColor = colorCheckOff;

		} else {
			checkFlg[num] = 1;
			tdObj.style.backgroundColor = colorCheckOn;

		}

		//rent、sale
		if (num == 1 || num == 2) {
			setRentSaleElm();
		}

		if (doSearch == 1) {
			//検索実行
			var form = document.getElementById('search');
			form.submit();			
		}

	};

	function setRentSaleElm(){
		var rentElm = document.getElementsByClassName('rent')[0];
		var landAreaElm = document.getElementsByClassName('land_area')[0];
		var priceElm = document.getElementsByClassName('sale')[0];
		if(checkFlg[1]==1 && checkFlg[2]==0) {
			//rent
			rentElm.style.display='block';
			landAreaElm.style.display='none';
			priceElm.style.display='none';
		} else if(checkFlg[1]==0 && checkFlg[2]==1) {
			//sale
			rentElm.style.display='none';
			landAreaElm.style.display='block';
			priceElm.style.display='block';
		} else {
			rentElm.style.display='block';
			landAreaElm.style.display='block';
			priceElm.style.display='block';			
		}
	}

	// エリアチェックボックス処理
	function checkAria(index, targetName) {
		for (var i = 0; i < 13; i++) {
			var e_name_plus = "popup_area_id"+(i+1);
			var e_name = "ui-multiselect-area_id-option-"+i;
			if (document.getElementById(e_name).checked) {
				document.getElementById(e_name_plus).checked = true;
			} else {
				document.getElementById(e_name_plus).checked = false;
			}
		}
	};

	// 検索条件をクリア
	function clearSearch() {

		// 選択状態をクリア
		for (var i = 1; i < checkFlg.length; i++) {
			checkFlg[i] = 0;
		}

		// タイプをクリア
		document.getElementsByName("type[]").item(0).checked = false;
		document.getElementsByName("type[]").item(1).checked = false;
		document.getElementsByName("type2[]").item(0).checked = false;
		document.getElementsByName("type2[]").item(1).checked = false;
		document.getElementById("menu_td_rent").style.backgroundColor = colorCheckOff;
		document.getElementById("menu_td_sale").style.backgroundColor = colorCheckOff;

		// 種別をクリア
		document.getElementsByName("usage[]").item(0).checked = false;
		document.getElementsByName("usage[]").item(1).checked = false;
		document.getElementsByName("usage[]").item(2).checked = false;
		document.getElementById("menu_div_usage1").style.backgroundColor = colorCheckOff;
		document.getElementById("menu_div_usage2").style.backgroundColor = colorCheckOff;
		document.getElementById("menu_div_usage3").style.backgroundColor = colorCheckOff;

		// 特徴アイコンをクリア
		var h = 0;
		var k = 1;
		for (var i = 5; i < checkFlg.length; i++) {
			var name = "menu_div_icon" + k;
			var obj = document.getElementById(name);
			var objInput = document.getElementsByName("icon[]").item(h);
			if(objInput != null) objInput.checked = false;
			if(obj != null) obj.style.backgroundColor = colorCheckOff;
			k++;
			h++;
		}

		// フリーワードをクリア
		document.getElementById('k').value = "";

		// 賃料をクリア
		var rentfrom = document.getElementById("rent_from");
		var rentto = document.getElementById("rent_to");
		for (var i = 0; i < rentfrom.length; i++) {
			rentfrom[i].selected = false;
		}
		for (var i = 0; i < rentto.length; i++) {
			rentto[i].selected = false;
		}
		rentfrom.disabled = true;
		rentto.disabled = true;

		// 価格をクリア
		var salefrom = document.getElementById("sale_from");
		var saleto = document.getElementById("sale_to");
		for (var i = 0; i < salefrom.length; i++) {
			salefrom[i].selected = false;
		}
		for (var i = 0; i < saleto.length; i++) {
			saleto[i].selected = false;
		}
		salefrom.disabled = true;
		saleto.disabled = true;

		// 建物面積をクリア
		var buildingareafrom = document.getElementById("building_area_from");
		var buildingareato = document.getElementById("building_area_to");
		for (var i = 0; i < buildingareafrom.length; i++) {
			buildingareafrom[i].selected = false;
		}
		for (var i = 0; i < buildingareato.length; i++) {
			buildingareato[i].selected = false;
		}
		buildingareafrom.disabled = true;
		buildingareato.disabled = true;

		// 土地面積をクリア
		var landareafrom = document.getElementById("land_area_from");
		var landareato = document.getElementById("land_area_to");
		for (var i = 0; i < landareafrom.length; i++) {
			landareafrom[i].selected = false;
		}
		for (var i = 0; i < landareato.length; i++) {
			landareato[i].selected = false;
		}
		landareafrom.disabled = true;
		landareato.disabled = true;

		// エリアをクリア
		var sel = document.getElementById("area_id");
		for (var i = 0; i < sel.length; i++) {
			sel[i].selected = false;
		}
		for (var i = 0; i < 13; i++) {
			var e_name = "ui-multiselect-area_id-option-"+i;
			var e_name_plus = "popup_area_id"+(i+1);
			document.getElementById(e_name).checked = false;
			document.getElementById(e_name_plus).checked = false;
		}

		// jQuery UI MultiSelect Widgetのタイトル部を格納
		var elements = document.getElementsByClassName("ui-multiselect");
		var childs = elements[0].childNodes; //直下の子要素のみを格納
		childs[1].innerHTML = "エリア選択されていません";

		// その他の条件をクリア
		k = 1;
		for (var i = 0; i < 11; i++) {
			var name = "other"+k;
			document.getElementById(name).checked = false;
			k++;
		}

		return false;
	}

</script>

