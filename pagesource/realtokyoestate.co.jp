<link rel="stylesheet" href="/css/user-login.css" type="text/css" />
<script type="text/javascript">

var MODAL_HEIGHT = 687;
var MODAL_WIDTH = 400;

var LOGIN = {};
LOGIN.auth = false;
LOGIN.name = '';
LOGIN.email = '';

$(document).ready(function(){
    getLoginStatus();
});

function setModalWindow(srcUrl){
    var $loginWindow = $('#login-html-wrapper');
    $loginWindow.html('<iframe id="login-inner" src="'+srcUrl+'"/>');
    $('#login-background')
    .show()
    .on('click',function(){
        closeLoginWindow();
    });
    $('#login-inner').contents().find('#login-header').on('click',function(){
        closeLoginWindow();
    });
    $loginWindow.children('iframe').load(function(){
        resetPosLoginWindow();
    });
    $loginWindow.show();
}
function closeLoginWindow(){
    $('#login-html-wrapper').hide();
    $('#login-background').hide();
}
function resetPosLoginWindow(){    
    var windowHeight = $(window).height();
    var windowHalfWidth=$(window).width()/2;

    var $loginWindow = $('#login-html-wrapper');
    $loginWindow.css('margin-left',windowHalfWidth-200);
    if (windowHeight < MODAL_HEIGHT) {
        var _top = 10 + $('body').scrollTop();
        $loginWindow.css({
            'position':'absolute',
            'top':_top
        });
    } else {
        $loginWindow.css({
            'position':'fixed',
            'top': '50px'
        });    
    }
}

function getLoginStatus(){
	$.ajax({
		url: "/apiGetLoginStatus.php",
		type: "GET",
		dataType: "json",
		success: function(response){
			LOGIN.auth = response.auth;
			if(response.name !== '' && response.name !== null){
				LOGIN.name = response.name;
			}
			if(response.email !== '' && response.email !== null){
				LOGIN.email = response.email;
			}
			//表示
			getLoginHeader();
		}
	});
}

function getLoginHeader(){

	var result = "";
	if(LOGIN.auth === true){
		var dispName = '';
		if(LOGIN.name !== '' && LOGIN.name !== null){
			dispName = LOGIN.name;
		}else{
			dispName = LOGIN.email;
		}
		result = '<td id="head_user_url" class="td_menu2"><span class="dispName">'+dispName+'</span>|</td>'+
		'<td id="head_login" class="td_menu2"><a href="#" id="userTop">会員設定</a>'+
		'|<a href="#" id="userLogout">ログアウト</a></td>';
	}else{
		result = '<td id="head_login" class="td_menu2"><a id="userLogin" href="#">ログイン</a>|<a id="userRegister" href="#">新規登録</a></td>';
	}

	$('#head_menu2 tr').prepend(result);

    resetPosLoginWindow();
    $('a#userLogin').on('click',function(){
        setModalWindow('/login/userLogin.php');
    });
    $('a#userRegister').on('click',function(){
        setModalWindow('/login/userRegister.php');
    });
    $('a#userLogout').on('click',function(){
        setModalWindow('/login/userLogout.php');
    });
    $('a#userTop').on('click',function(){
        setModalWindow('/login/userTop.php');
    });
    $(window).resize(function(){
        resetPosLoginWindow();
    });

}
</script>

