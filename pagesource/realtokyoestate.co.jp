<!-- ポップアップエリアマップの操作 -->
<script type="text/javascript">
	var numChecked = 0;
	var selectedText;

	// 選択状態を反映
	function btnSet_onclick() {
		// jQuery UI MultiSelect Widgetのタイトル部を格納
		var elements = document.getElementsByClassName("ui-multiselect");
		var childs = elements[0].childNodes; //直下の子要素のみを格納

		// エリアリストを格納
		var sel = document.getElementById("area_id");

		// 選択状態をクリア
		for (var i = 0; i < sel.length; i++) {
			sel[i].selected = false;
		}
		

		// 選択状態を適用
		numChecked = 0;
		for (var i = 0; i < 13; i++) {
			var e_name_plus = "popup_area_id"+(i+1);
			var e_name = "ui-multiselect-area_id-option-"+i;
			if (document.getElementById(e_name_plus).checked) {
				sel[i].selected = true; 
				document.getElementById(e_name).checked = true;
				numChecked++;
			} else {
				sel[i].selected = false; 
				document.getElementById(e_name).checked = false;
			}
		}

		// jQuery UI MultiSelect Widgetのタイトル部を書換
		if (numChecked > 0) {
			selectedText = "# エリア選択中";

		} else {
			selectedText = "エリア選択されていません";

		}
		childs[1].innerHTML = selectedText.replace('#', numChecked);

		// チェックをクリア
//		for (var i = 0; i < 13; i++) {
//			var e_name_plus = "popup_area_id"+(i+1);
//			document.getElementById(e_name_plus).checked = false;
//		}

		// ポップアップ画面を消去
		document.getElementById("popup_map").style.display = "none";
		document.getElementById("overlay").style.display = "none";
	}
</script>


