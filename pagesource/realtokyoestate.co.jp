<script type="text/javascript">
	(function($){
		$(function(){
		    $(document)
		        .on('click', '.search_details_on_btn', function(){
		            $('#search_details_on_button').hide();
		            $('#search_details').show();
		            $('#search_details_off_button').show();
		            $('#display').val('inline');
		            return false;

		        })
		        .on('click', '.search_details_off_btn', function(){
		            $('#search_details_off_button').hide();
		            $('#search_details').hide();
		            $('#search_details_on_button').show();
		            $('#display').val('none');
		            return false;

		        })
		        .on('click', '.popup_btn', function(){
		            var $popup = $($(this).attr('href'));
		            var mT = ($popup.outerHeight() / -2) + 'px';
		            var mL = ($popup.outerWidth() / -2) + 'px';
		            $('.popup').hide();
		            $popup.css({
		                'margin-top': mT,
		                'margin-left': mL
		            }).show();
		            $('#overlay').show();
		            return false;

		        })
		        .on('click', '.close_btn, #overlay', function(){
		            $('.popup, #overlay').hide();
