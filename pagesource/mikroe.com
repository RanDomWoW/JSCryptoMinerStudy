		for(var i = 0; i < els.length; i++) {
                                    src = els[i].src;
                                    if (src.indexOf('2x.jpg') > -1) {
                                        continue;
                                    }
                                    src = src.replace('.jpg', '2x.jpg');
                                    var img = new Image();
                                    img.addEventListener('load', function() {
                                        if(img.height == 0)
                                            els[i].src = els[i].src;
                                    });
                                    img.src = src;
                                    els[i].src = src;
                }

		var gal = $("#viewed-products_block_left a.replace-2x").get();
		for(var i = 0; i < gal.length; i++)
		{
			href = gal[i].href;
			extension = href.substr( (href.lastIndexOf('.') +1) );
			href = href.replace("." + extension, "2x." + extension);

			var imgRet = new Image();
			imgRet.addEventListener("load", function() {
				if(imgRet.height == 0)
