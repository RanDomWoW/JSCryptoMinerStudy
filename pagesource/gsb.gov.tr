            function ToJavaScriptDate(value) {
                var pattern = /Date\(([^)]+)\)/;
                var results = pattern.exec(value);
                var dt = new Date(parseFloat(results[1]));
                return new Date(dt.getFullYear(), (dt.getMonth() + 1), dt.getDate(), dt.getHours(), dt.getMinutes(), 0, 0);

            }
