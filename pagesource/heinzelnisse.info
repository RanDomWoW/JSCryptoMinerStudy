$(document).ready(function() {
    $("input#searchItem").autocomplete({
        minLength: 2,
        delay: 300, // ms
        source: function( request, response ) {
            $.ajax({
                url: "/suggest",
                dataType: "json",
                data: {
                    searchItem: request.term,
                    type: "json"
                },
                success: function( data ) {
                    response( $.map( data[1], function( item ) {
                        return {
                            label: item,
                            value: item
                        }
                    }));
                }
            });
        },
        select: function(event, ui) {
            window.location.href = 'dict?searchItem=' + ui.item.value;
        }
    });
    // focus on textarea for comments when available
    $("textarea#commentArea").focus();
});
