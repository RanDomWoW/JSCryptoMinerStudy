        $(document).ready( function() {
            function getCookies() {
                var cookies = {};
                if (document.cookie && document.cookie != '') {
                    var cookieArray = document.cookie.split(';')
                    for (var i = 0, len = cookieArray.length; i < len; i++) {
                        var kvp = cookieArray[i].split("=")
                        kvp[0] = kvp[0].replace(/^ /, '')
                        var key = decodeURIComponent(kvp[0])
                        var cookieValue = decodeURIComponent(kvp[1])
                        cookies[key] = cookieValue
                    }
                }
                return cookies;
            }
            function cookieIsEmpty(cookieName) {
                var cookies = getCookies()
                var cookie = cookies[cookieName]
                return (cookie === '')
            }
            function cookieDoesNotExit(cookieName) {
                var cookies = getCookies()
                var cookie = cookies[cookieName]
                return (cookie === undefined)
            }
            if (cookieDoesNotExit('PlattForceUser') && cookieIsEmpty('PlattForceLogin')) {
                document.querySelector('.plattForcePanel').style.display = 'none'
                document.querySelector('.plattForcePanelSpacer').style.display = 'none'
            }
        });
    </script>
