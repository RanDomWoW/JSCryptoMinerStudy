            $('.commodity-pricing-label').click(function (event) {
                var userExternalValue = 'External Account No: ' + '';
                var userAccountValue = '';
                var profileID = '';
                if (profileID == "1" && '' == '') {
                    userExternalValue = 'Retail User'
                } else if (profileID == '' && '' == '') {
                    userExternalValue = 'Guest User'
                }
                if ('' != '') {
                    userAccountValue = 'Account No: ' + '';
                }
                ga('send', {
                    hitType: 'event',
                    eventCategory: 'Commodity Pricing Link',
                    eventAction: 'Click',
                    eventLabel: userExternalValue + " " + userAccountValue
                });
            });

