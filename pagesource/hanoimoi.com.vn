            }
        }
        var currentKey = '';
        $(document).ready(function () {
            $(".ads").each(function () {
                if ($(this).attr("title") != currentKey) {
                    currentKey = $(this).attr("title");
                    $(this).removeClass("hide_ads").addClass("firstAds");
                }

            });
            setInterval("swapAds()", 10000);
        });
        function swapAds() {
            $(".ads:not(.hide_ads)").each(
                function () {
                    if ($(this).nextAll(".ads").length <= 0)
                        $(this).prevAll(".firstAds").removeClass("hide_ads");
                    else
                        $(this).next(".ads").removeClass("hide_ads");
                    $(this).addClass("hide_ads");

                });
        }
    </script>
   
