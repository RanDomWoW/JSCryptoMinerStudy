<script>
$(document).ready(function(){
	
	var firstDate = new Date(); // Todays date
	var secondDate = new Date(2016,06,10, 11,49,01);

	var oneDay  = 24*60*60*1000;

	var diffDays = Math.floor(Math.abs((firstDate.getTime() - secondDate.getTime()) / oneDay));
	var daysLeft = diffDays;
	var dd = diffDays;
	$("#pkcounter-c").html(diffDays);
	var dayname = '';
	ds = ""+daysLeft;
	dd=parseInt(ds.substr(ds.length-1));
	if(daysLeft>4&&daysLeft<21) { dayname=" дней" }
	 else {	 
	 if(dd==1){ dayname=" день" }
	  else {
	   if(dd==2||dd==3||dd==4){dayname=" дня"}
	   else { dayname=" дней" }}}
	   //alert(dd+' '+dayname);

	   $("#pkcounter-n").html(dayname);


