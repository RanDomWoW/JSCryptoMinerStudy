<script>
    function updateQueue(obj, parentID, userID, itemID, isSeries, option, location){
        var result = false;

        if(option == "add"){
            result = addToQueueAjax({"userId" : userID, "filmId" : itemID}, obj, parentID, location);
        }
        else if(option == "remove"){
            result = removeFromQueueAjax({"userId" : userID, "filmId" : itemID}, obj, parentID, location);
        }
    }

    function addToQueueAjax(params, obj, parentID, location){
        var baseURL = "/watchlist/create";
        var stringParams = "?";
        var result = false;

        for(key in params){
            stringParams += key + "=" + params[key] + "&";
        }

        if (navigator.appName == 'Microsoft Internet Explorer' && window.XDomainRequest) {
            var xdr = new XDomainRequest();
            xdr.open("POST", baseURL+stringParams);
            xdr.send();
            xdr.onerror = function () {
                console.log("XDR Error (Add to Queue):\n" + xdr.responseText);
            }
            xdr.onload = function() {
                console.log("XDR Success (Add to Queue):\n" + xdr.responseText);
                jsonData = JSON.parse(xdr.responseText);
                if(jsonData.response.type == "success"){
                    result = true;
                    if(location == "carousel")
                    {
                        swapWatchlistButtonPopover(obj, parentID, "add");
                    }
                    else if(location == "social")
                    {
                        swapWatchlistButtonSocial(obj, "add");
                    }
                    else if(location == "thumbnail")
                    {
                        swapWatchlistButtonPopover(obj, parentID, "add");
                    }
                }
            }
        }
        else {
            jQuery.post(baseURL+stringParams, function(data, status){
                if(status == "success"){
                    if(data.success){
                        result = true;
                        if(location == "carousel")
                        {
                            swapWatchlistButtonPopover(obj, parentID, "add");
                        }
                        else if(location == "social")
                        {
                            swapWatchlistButtonSocial(obj, "add");
                        }
                        else if(location == "thumbnail")
                        {
                            swapWatchlistButtonPopover(obj, parentID, "add");
                        }
                    }
                }
            });
        }

        return result;
    }

    function removeFromQueueAjax(params, obj, parentID, location){
        var baseURL = "/watchlist/delete";
        var stringParams = "?";
        var result = false;

        for(key in params){
            stringParams += key + "=" + params[key] + "&";
        }

        if (navigator.appName == 'Microsoft Internet Explorer' && window.XDomainRequest) {
            var xdr = new XDomainRequest();
            xdr.open("DELETE", baseURL+stringParams);
            xdr.send();
            xdr.onerror = function () {
                console.log("XDR Error (Remove from Queue):\n" + xdr.responseText);
            }
            xdr.onload = function() {
                console.log("XDR Success (Remove from Queue):\n" + xdr.responseText);
                jsonData = JSON.parse(xdr.responseText);
                if(jsonData.response.type == "success"){
                    result = true;
                    if(location == "carousel")
                    {
                        swapWatchlistButtonPopover(obj, parentID, "remove");
                    }
                    else if(location == "social")
                    {
                        swapWatchlistButtonSocial(obj, "remove");
                    }
                    else if(location == "thumbnail")
                    {
                        swapWatchlistButtonPopover(obj, parentID, "remove");
                    }
                }
            }
        }
        else {
            $.ajax({
                url: baseURL+stringParams,
                type: 'DELETE',
                success: function(data, status) {
                    if(status == "success") {
                        if(data.success){
                            result = true;
                            if(location == "carousel")
                            {
                                swapWatchlistButtonPopover(obj, parentID, "remove");
                            }
                            else if(location == "social")
                            {
                                swapWatchlistButtonSocial(obj, "remove");
                            }
                            else if(location == "thumbnail")
                            {
                                swapWatchlistButtonPopover(obj, parentID, "remove");
                            }
                        }
                    }
                }
            });
        }

        return result;
    }

    function swapWatchlistButtonPopover(obj, parentID, option)
    {
        if(option == "remove")
        {
            jQuery(obj).addClass("hide");
            jQuery(obj).prev("button").removeClass("hide");
        }
        else if(option == "add")
        {
            jQuery(obj).addClass("hide");
            jQuery(obj).next("button").removeClass("hide");
        }

        popoverContent = jQuery(obj).parent("div").html();
        jQuery("#"+parentID).attr("data-content", popoverContent);
        var popover = jQuery("#"+parentID).data("bs.popover");
        popover.setContent();
    }

    function swapWatchlistButtonSocial(obj, option)
    {
        var siblings = jQuery(obj).siblings();
        jQuery(obj).addClass("hide");
        siblings.removeClass("hide");
    }
</script>

