<script>
    // Truncate number. Ex. 1.967 = 1.96, 1.912 = 1.91
    Number.prototype.toFixedDown = function(digits) {
        var re = new RegExp("(\\d+\\.\\d{" + digits + "})(\\d)"),
            m = this.toString().match(re);
        return m ? parseFloat(m[1]) : this.valueOf();
    };

    String.prototype.replaceAll = function(search, replacement) {
        var target = this;
        return target.replace(new RegExp(search, 'g'), replacement);
    };

    var feelnApp = feelnApp || {};
    feelnApp.GENERIC_ERROR_MESSAGE = "Oops. We're sorry- something went wrong. Please try again later. In the event the issue persists, please contact <a href='https://customercare.hmnow.com'>Customer Care</a>.";

    feelnApp.isValidEmail = function(email) {
        var regExp = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        // Trailing and leading spaces will be ignored.
        email = email.trim();
        return regExp.test(email);
    };
    feelnApp.isDevEnv = function(){
        return document.domain.indexOf("dev") > -1;
    };
    feelnApp.bindConfirmClosingWindowPopup = function(){
        window.onbeforeunload = function () {
            return ''; // When return empty string, the message will show "Are you sure you want to leave this page?".
        }
    };
    feelnApp.unbindConfirmClosingWindowPopup = function(){
        window.onbeforeunload = null;
    };
    feelnApp.isBlankInput = function(selector){
        if(typeof selector[0] == "undefined") { // If the element is not existed, return undefined.
            return selector[0];
        } else if (selector.val().length) {
            return false;
        } else {
            return true;
        }
    };
    feelnApp.carousel = function carousel(options) {
        var defaults = {
                interval : 3000,
                carouselClass : '.feeln-carousel',
                childrenClass : '.feeln-carousel-child'
            },
            options = $.extend(defaults, options),
            firstElement = $(options.carouselClass + ' ' + options.childrenClass).first(),
            nextActiveElement = firstElement;

        setInterval(function () {
            $(options.carouselClass + ' .active').removeClass('active');
            nextActiveElement = nextActiveElement.next().first();

            if (nextActiveElement.length) {
                nextActiveElement.addClass('active');
            } else {
                firstElement.addClass('active');
                nextActiveElement = firstElement;
            }
        }, options.interval);
    }
    feelnApp.cookie = {
        /**
         * @desc  set cookie
         * @param string cookieName     - cookie name
         * @param string cookieValue    - cookie value
         * @param int cookieActiveDays  - number of day before cookie expires
         * @param string path           - cookie path
         */
        setCookie : function(cookieName, cookieValue, cookieActiveDays, path) {
            var date = new Date();
            date.setTime(date.getTime() + (cookieActiveDays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + date.toUTCString(),
                path = path || '/';
            document.cookie = cookieName + "=" + cookieValue + "; " + expires + "; path =" + path;
        },
        /**
         * @desc   get cookie
         * @param  string cookieName  - cookie name
         * @return if existed     : string cookie value
         *         if not existed : empty string
         */
        getCookie : function(cookieName) {
            var name = cookieName + "=";
            var cookieArray = document.cookie.split(';');
            for(var i = 0; i < cookieArray.length; i++) {
                var cookie = cookieArray[i];
                while (cookie.charAt(0) == ' ') cookie = cookie.substring(1);
                if (cookie.indexOf(name) == 0) return cookie.substring(name.length, cookie.length);
            }
            return "";
        },
        /**
         * @desc   remove cookie
         * @param  string cookieName  - cookie name
         */
        deleteCookie : function(cookieName) {
            this.setCookie(cookieName, '', -1);
        }
    }
    feelnApp.debug = feelnApp.isDevEnv();

</script>

