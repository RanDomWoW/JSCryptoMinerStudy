<script>
    window.onscroll = function () { gbnavSticky()};

    var header = document.getElementById("header");
    var body = document.getElementById("body");
    var stickyOffset = header.offsetTop;

    function gbnavSticky() {
        if (window.pageYOffset >= stickyOffset) {
            header.classList.add("sticky");
            body.classList.add("respect-sticky");
        } else {
            header.classList.remove("sticky");
            body.classList.remove("respect-sticky");
        }
    }
</script>
<script>
    var giftcardPopup = 0;
    var isSliding = 0;

    featured_owl.on('translate.owl.carousel', function (event) {
        isSliding = 1;
    });

    featured_owl.on('translated.owl.carousel', function (event) {
        setTimeout(function () {
            isSliding = 0;
        }, 500);
    });

    $('.clickable-slide').on('click', function (event) {
        var url = $(this).data('url');
        if (isSliding == 0) {
            window.location = url;
        }
    });

    if (giftcardPopup) {
        $('#modal-giftcard').modal('show');
    }
</script>

