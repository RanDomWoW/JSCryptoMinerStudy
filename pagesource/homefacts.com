<script language="javascript" type="text/javascript">
    function check_default_value() {
        $(".hero-search-banner .errorMsg").hide();
        if (document.getElementById("fulladdress").value == "" || document.getElementById("fulladdress").value == "Enter a City, ZIP code, County, State or Address") {
            alert("Please enter an Address, City, State or ZIP code, then click the Search button");
            document.getElementById("fulladdress").focus();
            return false;
        }
        else if(!/^[0-9A-Za-z #,.]+$/.test(document.getElementById("fulladdress").value))
            {
                alert('Invalid address. Verify that your address, city & state or zip code are spelled correctly');
                document.getElementById("fulladdress").focus();
                return false;
            }
        else {
            return true;
        }
    }
    
