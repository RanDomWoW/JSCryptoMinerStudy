<script type="text/javascript">
function gotoBot(min_height) {
    var gotoBot_html = '<div class="DD_png gundong_t"><div class="gundong_t_in"></div></div>',
         max = $('body').height();
 
    $("body").append(gotoBot_html);
    $(".gundong_t_in").click(function(){
      $("html,body").animate({scrollTop:$(".foot").offset().top},1000,function(){
           //$(".gundong_t_in").fadeIn(100);
			var s = $(window).scrollTop();
			if( (s + $(window).height()) >= (max-15)){
				console.log((s + $(window).height())+'=='+max);
				$(".gundong_t_in").fadeOut(100);
			}else{
				$(".gundong_t_in").fadeIn(100);
			};
      });
    }).hover(function(){
		$(this).addClass("gd_hover");
	}, function(){
		$(this).removeClass("gd_hover");
    });

   // min_height ? min_height = min_height : min_height = 600;
	
    $(window).scroll(function(){
        var s = $(window).scrollTop();
		if( (s + $(window).height()) >= (max-15)){
			console.log((s + $(window).height())+'=='+max);
            $(".gundong_t_in").fadeOut(100);
        }else{
            $(".gundong_t_in").fadeIn(100);
        };
    });
};

$(function(){
	gotoBot();
});
</script>


