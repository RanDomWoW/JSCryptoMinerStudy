<script>

	var fh_sandwich = document.getElementById("fh_sandwich")
	var fh_mobile_bg = document.getElementById("fh_mobile_bg")
	var fh_body_class = document.body.getAttribute("class")
	var fh_mobile_active = false

	if ( Element.prototype.addEventListener ) {
		fh_sandwich.addEventListener("click", fhMobile, false)
		fh_mobile_bg.addEventListener("click", fhMobile, false)
	}

	function fhMobile () {
		if ( fh_mobile_active ) {
			document.body.className = fh_body_class
			fh_mobile_active = false
		} else {
			document.body.className = fh_body_class + " fh-mobile-active"
			fh_mobile_active = true
		}
	}
</script>
