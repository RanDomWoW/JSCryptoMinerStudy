<script>
    
    
    (function() {
        var banner_view = [];
        $('.wrap-full-banner .wrap-full-banner-top').on('init reInit', function(event, slick, currentSlide, nextSlide){
            $('#top-banner').on('inview', function(event, isInView) {
                var $this = $(this);
                if (isInView) {
                    $this.addClass("banner_view");
                    var num =  $('.slick-active').children('a').children('img').data('banner-num');
                    var category = 'topBanner_' + num;
                    if ((category in banner_view) === false) {
                        banner_view[category] = 0;
                    }
                    if (banner_view[category] === 0) {
                        dataLayer.push({'eventCategory':category, 'event':'impression', 'eventUrl':$('.slick-active').children('a').attr('href')})
                        banner_view[category] = 1;
                    }
                } else {
                    $this.removeClass("banner_view");
                }
            });
                            $('.wrap-full-banner .wrap-full-banner-top').on('afterChange', function(event, slick, currentSlide, nextSlide){
                    var num =  $('.slick-active').children('a').children('img').data('banner-num');
                    var category = 'topBanner_' + num;
                    if ((category in banner_view) === false) {
                        banner_view[category] = 0;
                    }
                    if ($('#top-banner').hasClass('banner_view') && banner_view[category] === 0) {
                        dataLayer.push({'eventCategory':category, 'event':'impression', 'eventUrl':$('.slick-active').children('a').attr('href')})
                        banner_view[category] = 1;
                    }
                });
                    });
    })();

    </script>
