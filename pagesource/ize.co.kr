<script>
	function magazinePaging(dir)
	{
		var curPage = $('#magazinePage').val();

		if(dir == "prev")
		{
			curPage = parseInt(curPage) - 1;
			if(curPage < 0)
			{
				curPage = 0;
			}
		}
		else
		{
			curPage = parseInt(curPage) + 1;
			if(curPage > 5)
			{
				curPage = 5;
			}
		}

		$('#magazinePage').val(curPage);

		for(i=0; i<10; i++)
		{
			$('#magazine_'+i).hide();

		}

		for(i=curPage; i<curPage+5; i++)
		{
			$('#magazine_'+i).show();
		}
	}
</script>


