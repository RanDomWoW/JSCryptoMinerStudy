<script>
	var issue_kill;

	function content_mouse(num,tab)
	{
		if( issue_kill ) clearTimeout(issue_kill);

		for (var i=0;i<=tab;i++)
		{
			document.getElementById('gntab'+i).className='';
			$("#gncon"+i).hide();
		}
		document.getElementById('gntab'+num).className='on';
		$("#gncon"+num).show();

		right_num = (num*1)+1;

		if (right_num>'3'){
			right_num=0;
		}

		issue_kill = setTimeout('content_mouse('+right_num+',3)',5000);
	}

	content_mouse(0,'3');


	function onMouseOver(num,tab)
	{
		clearTimeout(issue_kill);

		for (var i=0;i<=tab;i++)
		{
			document.getElementById('gntab'+i).className='';
			$("#gncon"+i).hide();
		}

		document.getElementById('gntab'+num).className='on';
		$("#gncon"+num).show();
	}

	function onMouseOut(num,tab)
	{
		right_num = (num*1)+1;

		if (right_num>'3')
		{
			right_num=0;
		}

		issue_kill = setTimeout('content_mouse('+right_num+',3)',5000);
	}
</script>

