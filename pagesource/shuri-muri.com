<script type="text/javascript">
    jQuery(function() {
        $('select[name=region]').change(
            function()
            {
                if ($('select[name=region]').val() != '0')
                {
                    $('select[name=country]').attr('disabled', 'disabled');
                    $('select[name=region]').attr('disabled', 'disabled');
                    $('select[name=city]').attr('disabled', 'disabled');
                    $.ajax(
                        {
                            type: "POST",
                            url: "/ajax/cities/",
                            data: "region="+$('select[name=region]').val(),
                            success:
                                function(msg)
                                {
                                    $('select[name=city]').html('<option value="0" selected="selected">Любой город</option>');
                                    $('select[name=country]').removeAttr('disabled');
                                    $('select[name=region]').removeAttr('disabled');
                                    $('select[name=city]').removeAttr('disabled');

                                    var cities = jQuery.parseJSON(msg);
                                    $.each(cities, function(i, item)
                                    {
                                        if (item['id'] == '0')
                                            item['name'] = '······················································';
                                        $('select[name=city]').append('<option value="'+item['id']+'">'+item['name']+'</option>');

                                    });
                                    $('select[name=city]').trigger('refresh');
                                }
                        });
                }
            }
        );

        $('#fo_checkbox').click(
            function ()
            {
                if ($(this).is(':checked'))
                    $('input[name=foto_only]').val('1');
                else
                    $('input[name=foto_only]').val('0');
            }
        );

        $('select[name=age_from]').styler({onSelectClosed: function() {
            var min = parseInt($(this).val());
            $('select[name=age_to] option').each(function(){
                if(parseInt($(this).val()) < min){
                    $(this).hide();
                }else{
                    $(this).show();
                }
            });
            $('select[name=age_to]').trigger('refresh');
        }});

    });
</script>                

