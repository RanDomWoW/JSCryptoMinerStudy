                if (re.exec(ua) != null)
                    rv = parseFloat( RegExp.$1 );
            }
            return rv;
        }
        function checkVersion(){
            var ver = getInternetExplorerVersion();
            if(ver == 9.0 && location.protocol != 'https:'){
                var url = window.location.href;
                url = url.replace('http','https');
                window.location.href = url;
            }
        }
        checkVersion();
