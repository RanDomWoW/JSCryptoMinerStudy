<script>

    $('.envia-email').submit(function(){
        $(this).fadeOut();
        var dados = $(this).serialize();
        $('#enviando').fadeIn('slow');
        
        $.ajax({
            type: 'post',
            url: '/envia-email.php',
            data: dados,
            success: function(result){
                // alert(result)
               $('.alert').hide();
               $('#'+result).fadeIn('slow');
               if(result=="erro"){
                   
                   $(this).show();
                   $('#erro').show();
               }else{
                   $('.limpa').val("");
                   $('.limpa').text("");
                   $('#sucesso').show();
               }
            }
        });
        return false;
    });
</script>
