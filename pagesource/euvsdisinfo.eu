                        if ( window.location.href.indexOf('disinformation-cases') == -1 ){
                            window.location.href = '/disinformation-cases/?disinfo_keywords[]=' + item[3];
                        } else {
                            $('.disinfo-db-filters form').submit();
                        }
                    },
                    backgroundColor: 'transparent'
                });
