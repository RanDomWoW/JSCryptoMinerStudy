                        action: 'get_keywords',
                        filter: filter
                    }, done);
                }
            });

            function renderCloud(items) {
                if(!items.length) {
                    $canvas.hide();
                    $message.text('No results');
                    return;
                }

                var maxItems = 60;
                var minItems = 10;
                var maxWidth = 1000;
                var minWidth = 320;
                var width = window.innerWidth;
                var offset;

                if(width >= maxWidth) {
                    offset = maxItems;
                } else if(width <= minWidth) {
                    offset = minItems;
                } else {
                    offset = Math.round(maxItems * (width / maxWidth));
                }

                var list = items.slice(0, offset);
                list.shuffle();

                $canvas.show();
                $message.text('');
                return WordCloud($canvas[0], {
                    list: list,
                    gridSize: 24,
                    weightFactor: 1,
                    fontFamily: 'Finger Paint, cursive, sans-serif', //Finger Paint, cursive, sans-serif
                    color: '#333',
                    minSize: 10,
                    minRotation: 0,
                    maxRotation: 0,
                    click: function(item) {
