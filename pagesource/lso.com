			    <script type="text/javascript" language="javascript">

	function imageExists(image_url){
	
		var http = new XMLHttpRequest();
	
		http.open('HEAD', image_url, false);
		http.send();
	
		return http.status != 404;
	
	}
	function buildProfile(profile,isactive) {
		var active = "";
		if (isactive) {
			active = " active ";
		}

		var img_url = "img/headshot/" + profile.name.replace(/ /g,"_") + ".jpg";
		if (!imageExists(img_url)) {
			img_url = "img/headshot/placeholder.jpg";
		}

		var html = 	'<div class="emp_profile item ' + active + ' pad-25" id="featured-employee">' +
			'<img alt="' + profile.name + '" src="' + img_url + '" width="152px" height="177px">' +
            '<p class="fe-name">' + profile.name + '</p>' +
            '<p><span class="fe-entry">LSO POSITION:</span><br>' +
            profile.position + '</p>' +
            '<p><span class="fe-entry">HOMETOWN:</span><br>' +
            profile.hometown + '</p>' +
            '<p><span class="fe-entry">' + profile.question1 + ':</span><br>' +
            profile.answer1 + '</p><br clear="all"/>' +
            '<p><span class="fe-entry">' + profile.question2 + ':</span><br>' +
            profile.answer2 + '</p>' +
			'</div>';

		$("#emp_carousel").append(html);

	}

	window.onload = function() {
		$.getJSON('htmlfragments/employee_profiles.html', function(data) {
			var emp_count = data.length;
			var active_key = Math.floor(Math.random()*emp_count);
			for (var key in data) {
				buildProfile(data[key],key == active_key);
			}
			});
			
	}
	</script>
