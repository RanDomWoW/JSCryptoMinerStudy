 <script id="splashJs" src="js/splash.js" data-splash-array='{"img1":"img2018022223510563031000.jpg","url":"https:\/\/www.sagan-tosu.net\/match\/schedule\/2018022301\/#st_gameDetailEvent","blank":false,"dispCnt":999999,"start":"2018-02-23 00:00","end":"2018-02-23 22:00"}'></script>
<script src="js/jquery.yycountdown.min.js"></script>
<script>
	$(function(){

		// maingallery
		$('.slider-for').slick({
			slidesToShow: 1,
			slidesToScroll: 1,
			arrows: true,
			fade: false,
			asNavFor: '.slider-nav',
			autoplay: true,
			autoplaySpeed: 5000
		});
		$('.slider-nav').slick({
			slidesToShow: 4,
			slidesToScroll: 1,
			asNavFor: '.slider-for',
			dots: false,
			centerMode: true,
			focusOnSelect: true,
			vertical: true,
			arrows: true,
			fade: false,
			autoplay: true,
			responsive: [
				{
				breakpoint: 768,
					settings: {
						centerMode: true,
						vertical: false
					}
				}
			]
		});

		// teamschedule
		var settings = function() {
			//SP
			var settings1 = {
				minSlides: 2.5,
				maxSlides: 4,
				responsive: true,
				slideMargin: 0,
				slideWidth: 150,
				moveSlides: 2,
				pager: false,
				infiniteLoop: false,
				hideControlOnEnd: true
			};
			//PC
			var settings2 = {
				minSlides: 4,
				maxSlides: 7,
				responsive: true,
				slideMargin: 0,
				slideWidth: 133,
				moveSlides: 1,
				pager: false,
				infiniteLoop: false,
				hideControlOnEnd: true
			};
			return ($(window).width()<767) ? settings1 : settings2;
		}

		var mySlider;

		function tourLandingScript() {
			mySlider.reloadSlider(settings());
		}

		mySlider = $('#st_teamSchedule').bxSlider(settings());
		$(window).resize(tourLandingScript);

		//footer設定
		if(window.matchMedia("(max-width:767px)").matches){
			$(".st_Loading.st_hiddenPcTb").css("display","block");
			$(".st_Loading.st_hiddenSp").css("display","none");
		}
		else{
			$(".st_Loading.st_hiddenPcTb").css("display","none");
			$(".st_Loading.st_hiddenSp").css("display","block");
		}

		$(".st_topWintosu").click(function() {

			var width = $(window).width();
			$('body').css({
				'overflow':'hidden',
				'width':width
			});

			$('.st_topWintosuBg').show().animate({
				'opacity': '0.5'
			});

			$('.st_wintosuBox').show().animate({
				'opacity': '1',
				'top': '12%'
			});

			$(document).on('touchmove.noScroll', function(e) {e.preventDefault();});
		});

		function CloseBtn() {
			$(document).off('.noScroll');

			$('body').css({
				'overflow':'',
				'width':''
			});

			$('.st_topWintosuBg').animate({
				'opacity': '0'
			})
			.promise().done(function(){
				$('.st_topWintosuBg').hide();
			});

			$('.st_wintosuBox').animate({
				'opacity': '0',
				'top': '30%'
			})
			.promise().done(function(){
				$('.st_wintosuBox').hide();
			});


		};

		$(".st_topWintosuBg").click(function() {
			CloseBtn();
		});

		$(".st_wintosuBox .st_close").click(function() {
			CloseBtn();
		});
		var gameStart = "";
		if (gameStart != "null") {
			$('.st_todayArea .st_count').yycountdown({
				endDateTime : gameStart,
				unit : {h: ':', m: ':', s: ''},//カウントダウン単位
				complete : function(_this){  //カウントダウン完了時のコールバック
					_this.find('.yycountdown-box').parent().parent().parent().parent().addClass("countEnd");
					$('.st_todayArea .st_buttonArea li').each(function() {
						if ($(this).text() == "メンバー") {
							$(this).find("a").text("試合詳細");
						}
					});
				}
			});
			$(".yyc-day").hide();//日にちは表示しない
		}

	});

	$(window).load(function () {
		$('.st_Loading.st_hiddenPcTb').fadeOut(800);
		$('.st_Loading.st_hiddenSp').fadeOut(800);
		$('.st_loadBlock').fadeIn(800);
	});
</script>
