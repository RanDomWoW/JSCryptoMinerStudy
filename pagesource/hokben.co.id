<script type="text/javascript">
            function add_to_basket(val) {
                var productIDVal = val;
                $("#submit_" + productIDVal + "").attr("disabled", "disabled");
                $("#submit_" + productIDVal + "").off("click", add_to_basket);

                var productX = $('#image_' + productIDVal + "").offset().left;
                var productY = $('#image_' + productIDVal + "").offset().top;

                var basketX = $("#cart").offset().left;
                var basketY = $("#cart").offset().top;

                var gotoX = basketX - productX;
                var gotoY = basketY - productY;

                var newImageWidth = $("#image_" + productIDVal + "").width() / 3;
                var newImageHeight = $("#image_" + productIDVal + "").height() / 3;

                $("#image_" + productIDVal + " img")
                        .clone()
                        .prependTo($("#image_" + productIDVal + ""))
                        .css({'position': 'absolute'})
                        .animate({opacity: 0.4}, 100)
                        .animate({opacity: 0.1, marginLeft: gotoX, marginTop: gotoY, width: newImageWidth, height: newImageHeight}, 1200, function () {
                            $(this).remove();
                            $.post('https://www.hokben.co.id/menu/reload_menu', {menu_id: productIDVal, qty: 1},
                            function (data) {
                                var result = $.parseJSON(data);
                                if (result.rcode === '00') {
                                    if (result.update) {
                                        $("#txt_" + result.id + "").val(result.qty);
                                    } else {
                                        if (result.count_cart > 5) {
                                            var last_hide = $("#last_hide").val();
                                            $("#cart_thumb_" + last_hide + "").hide();
                                            last_hide++;
                                            $("#last_hide").val(last_hide);
                                        }
                                        $("#list_cart").prepend(result.html);
                                    }
                                    var subtotal = parseFloat(result.item_price) + parseFloat($("#cart_subtotal").attr('data-value'));
                                    $("#subtotal_" + result.id + "").html("Rp " + number_format(result.new_subtotal, 0, ',', '.') + ",-");
                                    $("#cart_subtotal").html("Rp " + number_format(subtotal, 0, ',', '.') + ",-");
                                    $("#cart_subtotal").attr('data-value', subtotal);
                                    $("#cart_total").html("<strong>Rp " + number_format((subtotal + result.ongkir), 0, ',', '.') + ",-</strong>");
                                    $("#content_cart").attr('data-total', 1);
                                    $("#submit_" + productIDVal + "").removeAttr("disabled");
                                    $("#submit_" + productIDVal + "").on("click", add_to_basket);
                                    $("#continer_cart").css('display', '');
                                    $("#cart_empty").css('display', 'none');
                                    $(".new_bag").html(result.totalqty);

                                }
                            });
                        });

            }

            function number_format(number, decimals, dec_point, thousands_sep) {
                number = (number + '').replace(/[^0-9+\-Ee.]/g, '');
                var n = !isFinite(+number) ? 0 : +number,
                        prec = !isFinite(+decimals) ? 0 : Math.abs(decimals),
                        sep = (typeof thousands_sep === 'undefined') ? ',' : thousands_sep,
                        dec = (typeof dec_point === 'undefined') ? '.' : dec_point,
                        s = '',
                        toFixedFix = function (n, prec) {
                            var k = Math.pow(10, prec);
                            return '' + Math.round(n * k) / k;
                        };
                // Fix for IE parseFloat(0.55).toFixed(0) = 0;
                s = (prec ? toFixedFix(n, prec) : '' + Math.round(n)).split('.');
                if (s[0].length > 3) {
                    s[0] = s[0].replace(/\B(?=(?:\d{3})+(?!\d))/g, sep);
                }
                if ((s[1] || '').length < prec) {
                    s[1] = s[1] || '';
                    s[1] += new Array(prec - s[1].length + 1).join('0');
                }
                return s.join(dec);
            }
</script>

<script type="text/javascript">
    function update_menu(id, qty, reload) {
        $.post('https://www.hokben.co.id/menu/update_menu', {menu_id: id, type: qty, change: reload === 2 ? 1 : 0},
        function (data) {
            var result = $.parseJSON(data);
            if (result.rcode === '00') {
                if (reload > 0) {
                    location.reload();
                } else {
                    if (result.clear) {
                        $("#td_cart_" + id + "").parent().remove();
//                        $("#bag_container").css('display', 'none');
                        $("#bag_container").toggle();
                        var last_hide = $("#last_hide").val();
                        last_hide--;
                        $("#cart_thumb_" + last_hide + "").show();
                        $("#last_hide").val(last_hide);
                    } else {
                        $("#txt_" + id + "").val(result.qty);
                    }
                    if (result.count_cart === 0) {
                        $("#continer_cart").css('display', 'none');
                        $("#cart_empty").css('display', '');
                    } else {
                        $("#continer_cart").css('display', '');
                        $("#cart_empty").css('display', 'none');
                    }
                    var subtotal = parseFloat(result.item_price) + parseFloat($("#cart_subtotal").attr('data-value'));
                    $("#cart_subtotal").attr('data-value', subtotal);
                    $("#content_cart").attr('data-total', result.count_cart);
                    $("#subtotal_" + id + "").html("Rp " + number_format(result.new_subtotal, 0, ',', '.') + ",-");
                    $("#cart_subtotal").html("Rp " + number_format(subtotal, 0, ',', '.') + ",-");
                    $("#cart_total").html("<strong>Rp " + number_format((subtotal + result.ongkir), 0, ',', '.') + ",-</strong>");
                    $(".new_bag").html(result.totalqty);
                }
            }
        });
        return false;

    }
</script>

