            <script>
                $(document).ready(function () {
                    $('.click-modalpop').click(function () {
                        $("#overlay_poperror").show();
                        $("#poperror").show();
                        return false;
                    });
                    $('#overlay_poperror').click(function () {
                        $(this).hide();
                        $("#poperror").hide();
                        return false;
                    });
                    $('.closepop').click(function () {
                        $("#overlay_poperror").hide();
                        $("#poperror").hide();
                        return false;
                    });
                });

                function onfocus_text(el) {
                    $(".check_qty").hide();
                    var data_value = $(el).attr('data-value');
                    $("#check_" + data_value + "").show();
                    return false;
                }

                function commit_value(el) {
                    var data_value = $(el).attr('data-value');
                    var qty = $("#txt_" + data_value + "").val();
                    update_menu(data_value, qty, false);
                    $("#check_" + data_value + "").hide();
                }

                function key_commit(el, event) {
                    if (event.keyCode == 13) {
                        commit_value(el);
                    }
                }
            </script>

