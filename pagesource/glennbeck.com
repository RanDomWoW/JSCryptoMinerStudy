	<script>
	//This makes the Latest and Show Prep areas sticky
	jQuery(document).ready(function($){

		function allow_sticky(){
			if($(window).width() > 1023){
				latestRight = $(".showprep-right")[0].scrollHeight;
				latestLeft = $(".latest-left")[0].scrollHeight;


				outthis=Math.abs(latestRight - latestLeft);

				//console.log(outthis);

				$(document.body).trigger("sticky_kit:recalc");
				//$(document.body).trigger("sticky_kit:recalc");

				//only makes sections sticky if they are different enough heights
				if (Math.abs(latestRight - latestLeft) > 50) {

				$(document.body).trigger("sticky_kit:recalc");

					//this sticks the appropriate side, adding this allows the correct side to be sticky after loading more elements
					if (Math.abs(latestRight) < Math.abs(latestLeft)){
						$(function(){
						jQuery(".showprep-right").stick_in_parent({parent:'#latest_wrapper',offset_top: 50, spacer: '.prep-spacer'});
						});


						$('.showprep-right').on('sticky_kit:bottom', function(e){
							$(this).parent().css('position','static');

						})
						.on('sticky_kit:unbottom', function(e) {
							$(this).parent().css('position','relative');

						});
					}

					//this sticks the appropriate side, adding this allows the correct side to be sticky after loading more elements
					if (Math.abs(latestRight) > Math.abs(latestLeft)){
						$(function(){

						jQuery(".latest-left").stick_in_parent();

						});


						$('.latest-left').on('sticky_kit:bottom', function(e){
							$(this).parent().css('position','static');

						})
						.on('sticky_kit:unbottom', function(e) {
							$(this).parent().css('position','relative');

						});
					}

				}
			}
		}


		$('#load_more_latest').click(function(){
			setTimeout(allow_sticky, 1000);
			$(document.body).trigger("sticky_kit:recalc");
		});

