<script>
    var __gameinfo_url = "http://gameinfo.leagueoflegends.co.kr/ko/game-info/";
    var __recommendKeyword = [  ];
    var __currentSearchType = "";
        
    function setMainSearchTab(idx) {
        __currentSearchType = idx;
        document.getElementById("searchKeyword").value = "";        //기존 입력한 값 유지해야 하면 수정 필요.
        
        var targetObj = document.getElementById("mainRecommendKeywords");
        if(!targetObj) return;
        
        //해당 키워드
        var keywords = jQuery.grep(__recommendKeyword, function (item, i) {
            return item.type == __currentSearchType;
        });
        
        var htmlKeyword = "";
        for(var i=0;i<keywords.length;i++) {
            htmlKeyword += "<span><a href=\"" + keywords[i]["keyword_url"] + "\">" + keywords[i]["keyword"] + "</a></span>";
        }
        
        targetObj.innerHTML = htmlKeyword;
    }
    
    function mainSearchKeyword() {
        var $keyword = $("#searchKeyword");
        var keyword = $.trim($keyword.val());
        
        if(keyword.length == 0) {
            alert("검색어를 입력하세요.");
            $keyword.value = "";
            $keyword.focus();
            return false;
        }
        
        switch(__currentSearchType) {
            case '1':
                search_url = '/match/?summonerName=' + keyword;
                break;
            case '2':
                search_url = __gameinfo_url + 'champions/?search=' + keyword;
                break;
            case '3':
                search_url = __gameinfo_url + 'items/?search=' + keyword;
                break;
        }
        
        if(!search_url) return false;
        
        location.href = search_url;
    }
    
    setMainSearchTab('1');
</script>
