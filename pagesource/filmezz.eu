		hb=$.cookie('flz_hb');
		
	//  0: minden csukva
	//  1: kereső
	//  2: részletes cucc
	//  3: vissza a találatokhoz
	
	function hbtoogle(a){
        if(hb==a){
            $('#hb'+a).slideUp(300);
            hb=0;
            $.cookie('flz_hb',hb);
        }else{
            $('#hb'+hb).slideUp(300);
            $('#hb'+a).slideDown(300);
            hb=a;
            $.cookie('flz_hb',hb);
        }
		
		if(hb==a){
			if(a == 1) {
				$('#menu-categories').removeClass('active');
				$('#menu-filter').addClass('active');
			} else if(a == 2) {
				$('#menu-categories').addClass('active');
				$('#menu-filter').removeClass('active');
			}
		}
		else {
			$('#menu-categories').removeClass('active');
			$('#menu-filter').removeClass('active');
		}
    }
	</script>

