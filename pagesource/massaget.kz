                    xmlHttp.onreadystatechange = function () {
                        if (xmlHttp.readyState == 4) {
                            if (call_back) {
                                call_back(xmlHttp.responseText);
                            }
                        }
                    };
                    xmlHttp.send(null);

                    return xmlHttp.responseText;
