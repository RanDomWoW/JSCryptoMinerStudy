            window.addEventListener('DOMContentLoaded', function () {
                TNtools.Notify = {};

                TNtools.Notify.add = function (type, message, live) {
                    var container = document.querySelector('#notify-container');
                    if (container) {
                        var notify = document.createElement('div');
                        notify.classList.add("notify");
                        notify.classList.add(type);
                        notify.addEventListener('click', function () {
                            this.remove();
                        });
                        notify.innerHTML = message;
                        container.appendChild(notify);
                        var interval = setInterval(function () {
                            container.removeChild(notify);
                            clearInterval(interval);
                        }, live)
                    }
                };
                TNtools.Notify.light = function (object, type, live) {
                    if (object) {
                        object.classList.add('notify-error-light');
                        var interval = setInterval(function () {
                            object.classList.remove('notify-error-light');
                            clearInterval(interval);
                        }, live)
                    }
                };
            });
