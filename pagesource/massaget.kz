}

function getSrc (n, bg_w) {
	var sizes = n.attr('data-sizes');
	var size = sizes.split(',');

	if (size.length > 1) {
		var s = 0;

		for(var d = 0; d < size.length - 1; d++)
			if (bg_w >= size[size.length - 1])
				s = size[size.length - 1];
			else
				if ((bg_w >= size[0] && bg_w < size[1]) || bg_w < size[0])
					s = size[0];
				else
					if (bg_w >= size[d] && bg_w < size[d + 1])
						s = size[d];
	} else
		s = size;

	return s;
}
	function getImage(n, w, t, p) {
	if (t == true) {
		$(n).find('.lazyOwl').each(function (i) {
			var s = getSrc($(this), w);
			var img = new Image();
			var that = this;
			img.src = $(this).attr('data-w' + s);
			img.onload = function(){
				$(that).attr('data-src',img.src).attr('src',img.src);
			};
			//$(this).attr('data-src', $(this).attr('data-w' + s));
		}).end().owlSlider(n, p);
	} else {
		$(n).each(function (i) {
			var s = getSrc($(this), w);
			var img = new Image();
			var that = this;
			img.src = $(this).attr('data-w' + s);
			img.onload = function(){
				$(that).attr('src',img.src);
			};
			//$(this).attr('src', $(this).attr('data-w' + s));
		});
	}
}
