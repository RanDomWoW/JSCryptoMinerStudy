                $('.formDestinazioni select[name=fdest]').html('');
                $.each(nazioniTipologieDest[$(this).val()], function(indice, valore) {
                    $('.formDestinazioni select[name=fdest]').append($("<option />").val(indice).text(valore));
                });
            } else {
                $('.formDestinazioni select[name=fdest]').html('<option value="">Aeroporto/Città</option>');
            }
        });
        
        $('.formDestinazioni').submit(function(event) {
            
            event.preventDefault();
            
            $form = $(this);
            searchParam = '';
            error = false;
            
            var fields = new Array('fdest','ftype');
            var url_fields = new Array('fdest','ftype');
            
            for(var k=0;k<fields.length;k++){
                tmp = $form.find('select[name='+fields[k]+'],input[name='+fields[k]+']').val();
                if (tmp === undefined || tmp === '' ) {
                    error = true; 
                    break;
                }
                if($.inArray(fields[k],url_fields) > -1) {
                    searchParam += '/'+fields[k]+'-'+tmp;
                }
            }
            
            if(!error){
                window.location.href = 'http://www.veniceairport.it/voli/destinazioni-dei-voli' + searchParam + '.html#boxPartenze-tab';
            }
            
        });
         
    });
</script>

        
    </body>
</html>