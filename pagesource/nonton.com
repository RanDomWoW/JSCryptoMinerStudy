  <!-- google connect -->
  <script type="text/javascript">
    var clientId = '893353566119-1nceae9r7315eiq3rb87ug0rqtmdk42k.apps.googleusercontent.com';
    var apiKey = 'AIzaSyDDMa6AuwSgLvE8PfhU_mWfachMP1ApbdE';
    var scopes = 'https://www.googleapis.com/auth/plus.me https://www.googleapis.com/auth/plus.login https://www.googleapis.com/auth/userinfo.email';

    function handleClientLoad() {
        gapi.client.setApiKey(apiKey);
    }

    function checkAuth() {
      gapi.auth.authorize({client_id: clientId, scope: scopes, immediate: true}, handleAuthResult);
    }

    function handleAuthResult(authResult) {
      if (authResult && !authResult.error) {
        makeApiCall();
      } else {
        console.log("auth is error");
        handleAuthClick();   
      }
    }

    function handleAuthClick(event) {
      gapi.auth.authorize({client_id: clientId, scope: scopes, immediate: false}, handleAuthResult);
      return false;
    }

    function makeApiCall() {
      gapi.client.load('plus', 'v1').then(function() {
        var request = gapi.client.plus.people.get({
          'userId': 'me'
        });

        request.then(function(resp) {
          var res = resp.result;
          var pic = res.image.url;
          var data = {
            id      : res.id,
            name    : res.displayName,
            gender  : res.gender,
            email   : res.emails[0].value,
            picture : pic.substring(0, pic.length-2).concat("60")
          };

          sendData(data);

        }, function(reason) {
          console.log('Error: ' + reason.result.error.message);
        });

      });
    }

    function sendData(obj){
      $("#loading-gif").show();
      $.ajax({
        type: "POST",
        url: "http://www.nonton.com/member/glogin",
        cache: false,
        data: obj,
        success: function(response){
          if(response == "1") {
            window.location.href = "http://www.nonton.com/";
          } else {
            console.log("error ! response : "+response);
          }
        }
      });
    }

    $(document).ready(function(){
      $('#btnLoginNew').click(function(){
        var formData = $('#form_login').serialize();
        $.ajax({
          url:"http://www.nonton.com/member/authProcess",
          method:"post",
          dataType:"json",
          data:formData,
          beforeSend:function(){
            $('#div_response').text('checking...');
            $('#btnLoginNew').attr('disabled',true);
          },
          error:function(){
            $('#div_response').text('error, please try again.');
          },
          success:function(response){
            $('#div_response').text(response.message);
            if(response.status == 1) {
              document.location.href = "http://www.nonton.com/member/login";
            } else {
              $('#btnLoginNew').removeAttr('disabled');
            }
          }
        })
      });
    });
  </script>
  <script src="https://apis.google.com/js/client.js?onload=handleClientLoad"></script>
