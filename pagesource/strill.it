<script type="text/javascript">
	var $ = jQuery.noConflict();

	// Hide Header on on scroll down
	var didScroll;
	var lastScrollTop = 0;
	var delta = 5;
	var navbarHeight = $('#theme-header').outerHeight();
	
	$(document).ready(function() {
		// source: http://www.abeautifulsite.net/detecting-mobile-devices-with-javascript/
		var isMobile = {
			Android: function() {
				return navigator.userAgent.match( /Android/i );
			},
			BlackBerry: function() {
				return navigator.userAgent.match( /BlackBerry/i );
			},
			iOS: function() {
				return navigator.userAgent.match( /iPhone|iPad|iPod/i );
			},
			Opera: function() {
				return navigator.userAgent.match( /Opera Mini/i );
			},
			Windows: function() {
				return navigator.userAgent.match( /IEMobile/i );
			},
			any: function() {
				return ( isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows() );
			},
		};
		
		if( !isMobile.any() && $("#is_single").val() != "1" ) {
			/** SIDEBAR **/
			$.each($(".widget-top"), function(index, item) {
				if($.trim($("selector").html())=='') {
					$(item).find("h4").remove();
					$(item).find(".strip-line").remove();
				}
			});
						
			// var toggleHeaderFloating = function() {
				// if ( $( window ).scrollTop() > 700 ) {
					// $("#theme-header").addClass( 'floating' );
					// $("#top-nav").hide();
				// } else {
					// $("#theme-header").removeClass( 'floating' );
					// $("#top-nav").show();
				// };
			// };

			// $( window ).on( 'scroll', toggleHeaderFloating );
			// toggleHeaderFloating();
		}
		
		if( isMobile.any() ) {
			// var divMobileFooter = $("<div/>", {
				// class: 'mobile-menu-footer'
			// }).appendTo("#slide-out");
			
			// var divMobileFooterText = $("<p/>", {
				// class: 'mobile-small',
				// html: 'STRILL.IT fondato nel 2006 da Raffaele Mortelliti e Giusva Branca<br/>DIRETTORE EDITORIALE: <a href="mailto:raffaelemortelliti@strill.it">raffaelemortelliti@strill.it</a><br/>DIRETTORE RESPONSABILE: <a href="mailto:giusvabranca@strill.it">giusvabranca@strill.it</a>'
			// }).appendTo(".mobile-menu-footer");
			
			// var divMobileFooterCopyright = $("<div/>", {
				// class: 'mobile-menu-footer-copyright'
			// }).appendTo("#slide-out");
			// var divMobileFooterText = $("<p/>", {
				// class: 'mobile-small',
				// html: '<a href="http://www.evermind.it">Powered by Evermind</a>'
			// }).appendTo(".mobile-menu-footer-copyright");
			
						
			$(window).scroll(function(event){
				didScroll = true;
			});

			setInterval(function() {
				if (didScroll) {
					// hasScrolled();
					didScroll = false;
				}
			}, 250);
		}
	});
	
	function hasScrolled() {
		var st = $(this).scrollTop();
		
		// Make sure they scroll more than delta
		if(Math.abs(lastScrollTop - st) <= delta)
			return;
		
		// If they scrolled down and are past the navbar, add class .nav-up.
		// This is necessary so you never see what is "behind" the navbar.
		if (st > lastScrollTop && st > navbarHeight){
			// Scroll Down
			$("#theme-header ul.adv-top-list").slideUp();
			$("#slide-out-open").css("top", "25px");
		} else {
			// Scroll Up
			if(st + $(window).height() < $(document).height()) {
				if(st < 50) {
					$("#theme-header ul.adv-top-list").slideDown();
					$("#slide-out-open").css("top", "145px");					
				}				
			}
		}
		
		lastScrollTop = st;
	}
</script>


