function send_newsletter() {
                $.post( "/subscriber", {
                    email : $('#newsletter_mail').val()
                    }).done(function( data ) {
                        var obj = jQuery.parseJSON(data);
                        $('#newsletter_mail').val('');
                        if (obj.status =='subscribed') {
                        myFunction('Tο mail σας προστέθηκε στη λίστα μας!')
                        } else if (obj.status == 'already_subscribed') {
                        myFunction('Tο mail σας είναι ήδη αποθηκευμένο στη λίστα μας!')
                        } else {
                        myFunction('Κάποιο πρόβλημα παρουσιάστηκε.Προσπαθήστε ξανά αργότερα!!')
                        }
                });
        }



