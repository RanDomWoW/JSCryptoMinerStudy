</script>	<script>
		var page = 1;
		var cat = "all";
		var tabclick = false;
		var event_cnt = 0;
		var goto_event = false;

		(function($) {
			$(document).on('focus', '#search-store .form-area #search-store-name', function() {
				$('#search-store .form-area').addClass('focus');
			});
			$(document).on('blur', '#search-store .form-area #search-store-name', function() {
				if ($(this).val() == '') {
					$('#search-store .form-area').removeClass('focus');
				}
			});
			var bannerSlider1 = $("#banner-1 > ul").bxSlider({
				auto: true,
				controls: false,
				autoHover: true,
				buildPager: function(slideIndex) {
					return "<span class='sr-only'>"+ slideIndex +"</span>"
				}
			});
			var bannerSlider2 = $("#banner-2 > ul").bxSlider({
				auto: true,
				controls: false,
				autoHover: true,
				buildPager: function(slideIndex) {
					return "<span class='sr-only'>"+ slideIndex +"</span>"
				}
			});
			$('#popup').on('shown.bs.modal', function (e) {
				$('body, #popup').css('padding-right', 0);
				$('#popup .popup-list').bxSlider({
					auto: true,
					autoHover: true,
					adaptiveHeight: true,
					controls: false,
					buildPager: function(slideIndex) {
						return "<span class='sr-only'>"+ slideIndex +"</span>"
					}
				});
			});
			
						
			$(document).on("click", ".nav-item", function(){
				var idx = $(".nav-item").index(this);
				
				cat = $(this).attr("data-item");
				
				page = 1;

				tabclick = true;

				goto_event = false;

				$(this).addClass("active").siblings().removeClass("active");

				main_event_load();
			});

			$(window).load(function(){
				$(".nav-item").eq(0).trigger("click");
			});

			$(window).resize(function(){
				if($(window).width() < 975){ // 모바일
					var h = $(".event-item").height() * event_cnt;
				} else {
					var h = $(".event-item").height() * Math.ceil(event_cnt / 3);
				}

				$("#coupon_ul").css({"height":h});
			});
		})(jQuery);

		function main_event_load(){
			$.ajax({
				type: 'POST',
				dataType : "xml",
				url: '/ajax.event.xml.php',
				data: "cat="+cat+"&page="+page,
				cache: false,
				async: false,
				error: function(xhr,textStatus,errorThrown){
					alert('An error occurred! \n'+(errorThrown ? errorThrown : xhr.status));
				},
				success: function(xml){
					var tagTemp = "";
					var totalCnt = $(xml).find("pageInfo").attr("totalCnt");
					if(tabclick) event_cnt = 0;
					$(xml).find("dataList").each(function(){
						tagTemp +=	'<li class="event-item col-xs-12 col-md-4">';
						tagTemp +=		'<a href="javascript:;" onclick="coupon_down(\''+$(this).attr("fid")+'\');">';
						tagTemp +=			'<div class="item-img">';
						tagTemp +=				'<img src="'+$(this).attr("thumb_src")+'" class="img-responsive" alt="">';
						tagTemp +=			'</div>';
						tagTemp +=			'<div class="details" style="display:none;">';
						tagTemp +=				'<h4>'+$(this).attr("thumb_title")+'</h4>';
						tagTemp +=				'<p class="date">'+$(this).attr("sdate")+' - '+$(this).attr("edate")+'</p>';
						tagTemp +=			'</div>';
						tagTemp +=		'</a>';
						tagTemp +=	'</li>';
						event_cnt++;
					});

					if(goto_event){ 
						location.href = "/event/coupon_zone.php";
					} else {
						var anitime = 600;

						if(!tabclick){ // 더보기
							if($(window).width() < 975){ // 모바일
								var h = $(".event-item").height() * event_cnt;
							} else {
								var h = $(".event-item").height() * Math.ceil(event_cnt / 3);
							}

							$("#coupon_ul").stop().animate({"height":h}, anitime, function(){ 
								//alert(totalCnt + " " + (page * 3) + " " + event_cnt + " " + Math.ceil(event_cnt / 3));
								if((page * 3) > totalCnt || page > 4) goto_event = true;
								if(tagTemp == "") location.href="/event/coupon_zone.php";
								$(this).append(tagTemp); 
								page++;
							});
						} else { // 탭클릭
							if(event_cnt == 0){
								tagTemp = '<div class="none-coupon"><p>등록된 쿠폰이 없습니다.</p></div>';
								goto_event = true;
							}

							if($(window).width() < 975){ // 모바일
								var h = $(".event-item").height() * 3;
							} else {
								var h = $(".event-item").height();
							}

							if(h == $("#coupon_ul").height()){
								anitime = 100;
							}

							$("#coupon_ul").stop().animate({"height":h}, anitime, function(){ 
								$(this).html(tagTemp); 
								page++; 
							});
							
						}
					}
				}
			});
		}

		function coupon_down(fid){
			alert("쿠폰을 다운로드 합니다.");
			location.href = "/board/config/download.php?fid="+fid;
		}
	</script>
