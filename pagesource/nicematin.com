<script type="text/javascript">
    
    DFP.attachEvent('iasPublisher', function(show_ads_callback) {
        DFP.iasRefreshed = true;
        
        var IAS_PET_TIMEOUT     = 700;
        var adserverRequestSent = false;

        var __iasPET = window.__iasPET || {};
        
        __iasPET.queue = __iasPET.queue || [];
        __iasPET.pubId = "9748";

        var iasDataHandler = function (adSlotData) {

            __iasPET.setTargetingForGPT();

            show_ads_callback();
        };

        googletag.cmd.push(function() {
            var gptSlots = googletag.pubads().getSlots();
            var iasPETSlots = [];
 
            for (var i = 0; i < gptSlots.length; i++) { 
                // important to send sizemapping to ias :
                var sizes = gptSlots[i].getSizes().map(function(size) {
                    var width  = (typeof size.getWidth === "undefined") ? 1 : size.getWidth();
                    var height = (typeof size.getHeight === "undefined") ? 1 : size.getHeight();

                    return [width, height];
                });

                iasPETSlots.push({
                        adSlotId: gptSlots[i].getSlotElementId(),
                        size: sizes,
                        adUnitPath: gptSlots[i].getAdUnitPath()
                });
            }
 
            __iasPET.queue.push({
                adSlots: iasPETSlots,
                dataHandler: iasDataHandler
            });
        });
    });


</script>
