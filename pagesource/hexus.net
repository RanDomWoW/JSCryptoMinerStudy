<script type="text/javascript">

function switch_to(idx) {
    $('div.featuredreview').hide();
    $('div.featuredreview').eq(idx).show();
    $('#fr_bottom img').removeClass('highlighted');
    $('#fr_bottom img').eq(idx).addClass('highlighted');
}
var idx = -1;
function rotate() {
    if(idx > 0 && idx % 4 == 0){
        idx = 0;
    }else{
        idx++;
    }
    switch_to(idx);
}

$(function() {
    $('#fr_bottom a').mouseover(function() {
        idx = $('#fr_bottom a').index(this);
        switch_to(idx);
        clearInterval(switchy);
    });
    rotate();
    switchy = setInterval("rotate()",5000);
});


</script>

    
