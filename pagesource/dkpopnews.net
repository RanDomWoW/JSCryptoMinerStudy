function dkpopfeaturedposts(json) {
    var numposts = json.feed.entry.length;
    document.write('<div class="nbt-featured-content"><div class="nbt-featured-posts">');
    for (var i = 0; i < numposts; i++) {
        var entrybox = json.feed.entry[i];
        var postid = entrybox.id.$t;
        var posttitle = entrybox.title.$t;
        var posturl, postfeedurl, commentstxt, commentsurl;
        if (i == json.feed.entry.length) break;
        for (var k = 0; k < entrybox.link.length; k++) {
            if ("replies" == entrybox.link[k].rel && "text/html" == entrybox.link[k].type) {
                commentstxt = entrybox.link[k].title;
                commentsurl = entrybox.link[k].href;
            }
            if ("self" == entrybox.link[k].rel && "application/atom+xml" == entrybox.link[k].type) {
                postfeedurl = entrybox.link[k].href;
            }
            if ("alternate" == entrybox.link[k].rel) {
                posturl = entrybox.link[k].href;
            }
        }
        postfeedurl = (postfeedurl != undefined) ? postfeedurl : '#';
        commentstxt = (commentstxt != undefined) ? commentstxt : '';
        var commentscount = ("thr$total" in entrybox) ? entrybox.thr$total.$t : '';
        var postpublisheddate = entrybox.published.$t.substring(0, 10);
        var postcategory = (entrybox.category != null && entrybox.category[0].term) ? entrybox.category[0].term : 'Uncategorized';
        var postthumbnailurl;
        postthumbnailurl = (entrybox.media$thumbnail != null && entrybox.media$thumbnail.url) ? entrybox.media$thumbnail.url.replace("/s72-c/", "/w465-h330-c/") : 'https://4.bp.blogspot.com/-wjjkE7YUERo/WVi2INX_AYI/AAAAAAAARd0/6ki-A3c8V6snZEu1xJN0Eawz5DQxAzF7gCLcBGAs/w400-h300-c/slide-no-img.jpg';
        if ((postthumbnailurl.indexOf("img.youtube.com") != -1) || (postthumbnailurl.indexOf("ytimg.com") != -1)) {
            postthumbnailurl = postthumbnailurl.replace("/default", "/0");
        }
        document.write('<div class="nbt-featured-post">');
        document.write('<div class="nbt-entry-thumbnail"><a href="'+posturl+'"><img class="lazyOwl" data-src="'+postthumbnailurl+'" width="465" height="330"/></a></div>');
        document.write('<div class="nbt-entry-header"><h2><a href="'+posturl+'">'+posttitle+'</a></h2></div>');
        document.write('</div>');
    }
    document.write('</div></div><div style="clear:both;"></div>');
}

