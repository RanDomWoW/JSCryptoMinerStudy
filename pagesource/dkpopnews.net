.dkpop-social-widget li a{color:#fff !important;background:#333;padding:10px 10px;margin:0 0 5px 0;display:block;font-size:16px;line-height:16px;height:36px;}
.dkpop-social-widget li a[href*="facebook.com"]{background-color:#3b5998;}
.dkpop-social-widget li a[href*="twitter.com"]{background-color:#00aced;}
.dkpop-social-widget li a[href*="plus.google.com"]{background-color:#dd4b39;}
.dkpop-social-widget li a[href*="instagram.com"]{background-color:#517fa4;}
.dkpop-social-widget li a[href*="youtube.com"]{background-color:#bb0000;}
.dkpop-social-widget li a[href*="pinterest.com"]{background-color:#bd081c;}
.dkpop-social-widget li a[href*="/feeds/posts/default"]{background-color:#dc622c;}
.dkpop-social-widget li i{margin-right:10px;}
.dkpop-social-widget li a i{display:inline-block;background:#000;padding:10px;margin:-10px 10px -10px -10px;width:36px;height:36px;text-align:center;}
.dkpop-entry-meta-feed{margin:0 0 5px 0;line-height:1.7;font-size:90%;}
.dkpop-entry-meta-feed a{color:#ff921f;}
.dkpop-entry-meta-feed a:hover{color:#000000;}
.dkpop-sharethis-wrapper{margin:15px 0 20px;}
.dkpop-entry-meta{margin:0 0 15px 0;text-align:center;padding:5px;border-top:1px dashed #ddd;border-bottom:1px dashed #ddd;}
.dkpop-entry-meta a{color:#ff921f;}
.dkpop-entry-meta a:hover{color:#000000;}
.dkpop-blog-status-message{margin:0 0 15px 0;font-size:18px;text-align:center;}
.dkpop-alert{background:#333;display:inline-block;color:#fff;padding:5px 10px;}
.dkpop-alert a{color:#fff;text-decoration:underline!important;}
.dkpop-error-view #dkpop-main-wrapper{width:auto;float:none;margin:0 0 20px 0;padding:0;}
.dkpop-error-view #dkpop-sidebar-wrapper{width:auto;float:none;margin:0;padding:0;}
.dkpop-error-view #dkpop-crosscol-wrapper{display:none;}
.dkpop-item-view .post-body{font-size:120%;}
.dkpopnews-rp-posts{margin:15px auto 10px;}
.dkpopnews-rp-posts h4{margin:0 0 10px;text-transform:none;}
.dkpopnews-related-posts,.dkpopnews-related-post{margin:0;padding:0;word-wrap:break-word;overflow:hidden;}
.dkpopnews-related-posts{display:flex;flex-direction:row;justify-content:space-between;align-items:stretch;flex-wrap:wrap;align-content:stretch;}
.dkpopnews-related-posts .dkpopnews-related-post{float:left;width:49%;flex: 0 0 49%;position:relative;margin-bottom:10px;}
.dkpopnews-related-posts .dkpopnews-related-post:nth-of-type(2n+1){clear:both;}
.dkpopnews-related-posts .dkpopnews-related-post-thumbnail-wrap{overflow:hidden;}
.dkpopnews-related-posts .dkpopnews-related-post-thumbnail-link{display:block;}
.dkpopnews-related-posts .dkpopnews-related-post-thumbnail{width:100%;max-width:100%;height:auto;display:block;padding:0;margin:0;}
.dkpopnews-related-post-title{left:0;right:0;bottom:0;padding:20px 20px;position:absolute;background:-moz-linear-gradient(top,rgba(0,0,0,0) 0%,rgba(0,0,0,0.9) 100%);background:-webkit-gradient(linear,left top,left bottom,color-stop(0%,rgba(0,0,0,0)),color-stop(100%,rgba(0,0,0,0.9)));background:-webkit-linear-gradient(top,rgba(0,0,0,0) 0%,rgba(0,0,0,0.9) 100%);background:-o-linear-gradient(top,rgba(0,0,0,0) 0%,rgba(0,0,0,0.9) 100%);background:-ms-linear-gradient(top,rgba(0,0,0,0) 0%,rgba(0,0,0,0.9) 100%);background:linear-gradient(to bottom,rgba(0,0,0,0) 0%,rgba(0,0,0,0.9) 100%);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#00000000',endColorstr='#80000000',GradientType=0);font:normal normal 20px Oswald, Sans-serif;color:#fff;line-height:1.4;}
.dkpopnews-related-post-title a{color:#fff;text-decoration:none;text-shadow:0 1px 0 #000;}
@media screen and (max-width:419px){
.dkpopnews-related-posts .dkpopnews-related-post{float:none;clear:both;width:100%;flex: 0 0 100%;margin-bottom:10px;}
}
.dkpop-mini-search-box .dkpop-widget-title{display:none;}
.separator a[style="margin-left: 1em; margin-right: 1em;"] {margin-left: 0 !important; margin-right: 0 !important;}
