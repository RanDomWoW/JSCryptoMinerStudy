<script type="text/javascript">
function show_hide_div(obj_div) {
		 var obj = document.getElementById(obj_div);
		 if (obj.className=='show_widget') obj.className = 'hide_widget';
		 else obj.className = 'show_widget';
}

function getElementsByClassName(clsName) {
	var arr = new Array();
	var elems = document.getElementsByTagName("*");
	for ( var cls, i = 0; ( elem = elems[i] ); i++ ) {
		if ( elem.className == clsName ) {
			arr[arr.length] = elem;
		}
	}
	return arr;
}

function insertAfter(newElement,targetElement) {
	var parent = targetElement.parentNode;
	if (parent.lastChild == targetElement) {
		parent.appendChild(newElement);
	} else {
		parent.insertBefore(newElement,targetElement.nextSibling);
	}
}

function tfDate() {

	var targetClass = getElementsByClassName('tfdate');
	for (var i=0; i<targetClass.length; i++) {
		targetClass[i].style.display = 'none';

		var fullDate = targetClass[i].firstChild.nodeValue;
		var splitDate = fullDate.split("/");
		var splitDate2 = splitDate[2].split(" ");

		var dateYear = splitDate[0];
		var dateMonth = eval(splitDate[1]);
		var dateDay = eval(splitDate2[0]);

		var arr_m = new Array();
		arr_m = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];

		var wrapDiv = document.createElement('div');

		var wrapDiv1 = document.createElement('span');
		wrapDiv1.className = 'date-day';
		wrapDiv1.innerHTML = dateDay;
		wrapDiv.appendChild(wrapDiv1);

		var wrapDiv2 = document.createElement('span');
		wrapDiv2.className = 'date-month';
		wrapDiv2.innerHTML =  arr_m[dateMonth-1];
		wrapDiv.appendChild(wrapDiv2);

		var wrapDiv3 = document.createElement('span');
		wrapDiv3.className = 'date-year';
		wrapDiv3.innerHTML = dateYear;
		wrapDiv.appendChild(wrapDiv3);

		insertAfter(wrapDiv,targetClass[i]);
	}
}

function pageLoad()
{
	tfDate();
}

if (typeof window.addEventListener != 'undefined')	{
	window.addEventListener("load", pageLoad, false);
} else if (typeof window.attachEvent != 'undefined') {
	window.attachEvent("onload", pageLoad);
}

</script>
