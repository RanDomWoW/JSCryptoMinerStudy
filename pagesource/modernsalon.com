        $btn.click(function (e) {
            e.preventDefault();
            var $email = $(this).siblings('.tbxEnewsSignUpEmail');
            var $message = $(this).siblings('.divEnewsletterMessage');
            openPreferences($email, $message);
        });
        var $email = $('.tbxEnewsSignUpEmail');
        $email.keypress(function (e) {
            if (e.which == 13) { //enter key
                e.preventDefault();
                var $message = $(this).siblings('.divEnewsletterMessage');
                openPreferences($(this), $message);
            }
        });
