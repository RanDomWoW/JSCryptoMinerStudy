        function openPreferences($email, $message) {
            $message.html('').hide();
            var email = $email.val();
            if (email == '') {
                $message.html('Please enter e-mail address.').fadeIn(200);
            }
            else {
                // open signup form window
                var url = 'https://bobit.dragonforms.com/init.do?omedasite=BOB6226_hairpref&em=' + email;
                window.open(url, '_blank');
                window.focus();
            }
        }
    
