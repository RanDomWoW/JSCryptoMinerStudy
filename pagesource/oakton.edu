		$(document).foundation();
		
		(function() {
			// store the slider in a local variable
			  var $window = $(window),
				  flexslider;

			  // tiny helper function to add breakpoints
			  function getGridSize() {
				return (window.innerWidth < 600) ? 1.25 :
					   (window.innerWidth < 900) ? 2.25 : 3.25;
			  }
			
			// alert close
			$('.alert .close').on('click', function() {
				$('.alert').remove();
				$('body').css('top', '');
				return false;
			});
			
			if ($('.alert').length) {
				var height = $('.alert').height();
				$('body').css('top', height);
				$('.alert').show();
			}
			
			$(window).load(function(){
			  $('.flexslider').flexslider({
				animation: "slide",
				animationLoop: false,
				itemWidth: 200,
				itemMargin: 10,
				minItems: getGridSize(),
				maxItems: getGridSize(),
				controlNav: false,
				start: function(slider){
				  $('.row.loading').removeClass('loading');
				}
			  });
			  
			
			//Dynamically set min and max items on resize
			$window.resize(function() {
				var gridSize = getGridSize();
				$('.flexslider').data('flexslider').setOpts({minItems: gridSize, maxItems: gridSize});
				
				if ($('.alert').length) {
					var height = $('.alert').height();
					$('body').css('top', height);
					$('.alert').show();
				}
			  });
			});
			
			
		}());
	</script> 




