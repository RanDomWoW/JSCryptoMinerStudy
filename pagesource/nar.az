<script type="text/javascript">

	$(window).on("load", function() {
		$("#fullpage_loading").fadeOut("slow", function() {$(this).hide();});
		$("#fp-nav").show();
	});

	function preventDefault(e) {
		e = e || window.event;
		if (e.preventDefault)
			e.preventDefault();
		e.returnValue = false;
	}

	function disableScroll() {
		if (window.addEventListener) // older FF
			window.addEventListener('DOMMouseScroll', preventDefault, false);
		window.onwheel      = preventDefault; // modern standard
		window.onmousewheel = document.onmousewheel = preventDefault; // older browsers, IE
		window.ontouchmove  = preventDefault; // mobile
		// document.onkeydown  = preventDefaultForScrollKeys;
	}

	function enableScroll() {
		if (window.removeEventListener)
			window.removeEventListener('DOMMouseScroll', preventDefault, false);
		window.onmousewheel = document.onmousewheel = null;
		window.onwheel      = null;
		window.ontouchmove  = null;
		// document.onkeydown  = null;
	}

	scrolling = false;

	function fullpage() {
		$('#fullpage').fullpage({
			navigation         : true,
			navigationPosition : "left",
			paddingBottom      : '165px',
			css3               : true,
			scrollingSpeed     : 1500,
			autoScrolling      : true,
			easingcss3         : "ease-in-out",
        //
			onLeave: function(index, nextIndex, direction){
				// Torbalar
				$(".custom_campaign_image.active").animate({ opacity : 0 }, 800, function() {$(this).removeClass("active").removeAttr("style");});
				// Torbalar

				console.log(direction);

				if(direction == "down") {
					var h = $(".bg_icon").height();
					$('.icon_img[data-count="count_'+index+'"]').stop().animate({
						bottom : h + "px"
					}, 1200, "easeInQuad");

					setTimeout(function(){
						var bh = $('.icon_img[data-count="count_'+nextIndex+'"]').height();
						$('.icon_img[data-count="count_'+nextIndex+'"]').css({
							bottom : "-" + bh + "px", opacity : 1
						}).stop().animate({
							bottom : "10px"
						}, 1200, "easeOutQuad");
					}, 800);

				} else {
					var bh = $('.icon_img[data-count="count_'+index+'"]').height();
					$('.icon_img[data-count="count_'+index+'"]').stop().animate({
						bottom : "-" + bh + "px", opacity : 0
					}, 1200, "easeOutQuad", function() {
						$(this).css({ opacity : 1 });
					});

					setTimeout(function() {
						var h = $(".bg_icon").height();
						$('.icon_img[data-count="count_'+nextIndex+'"]')
						.css({
							bottom : h + "px"
						}).stop().animate({
							bottom : "10px"
						}, 1200, "easeOutQuad");
					}, 800);

				}
			},
			afterLoad: function(anchorLink, index){
				$('.custom_campaign_image[data-id="image_'+index+'"]').addClass("active");

					$(".custom_campaign_content").css({
						height : $(".section").height()
					});
			},
			afterRender: function() {}});
		$("#fp-nav").hide();

	}

