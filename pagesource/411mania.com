<script>

jQuery('.slider').ready(function($){
	//console.log('slider');
	jQuery('#loading').fadeOut('fast');
	jQuery('.slider .image img').fadeIn('fast');
});

jQuery(document).ready(function($){
	//console.log('document');
	/* start slider */


	// nbr of slides
	var count = jQuery(".slider .news .new").length-1;
	
	// set first slide
	jQuery(".slider").attr('data-current-id',0);
	jQuery(".slider").attr('looper',true);
	jQuery(".slider .news .new:eq(0)").addClass('current');
	
	var current_id = jQuery(".slider").attr('data-current-id');
	var timer_current_id = current_id+1; // for auto
	window.auto = true;
		
	// auto
	window.timer = setInterval(function() { slideshowSlider() }, 3500);
		
	function slideshowSlider() {
	
		// remove timer
		if( ! window.auto || ! jQuery(".slider").attr('looper') )
			clearInterval(window.timer);
			
		// reset timer
		if(timer_current_id>count)
			timer_current_id=0;
		
		// click
		var _emulate = jQuery(".slider .news .new a:eq("+timer_current_id+")");
		jQuery(".slider").attr('looper',true);
		move_slider(_emulate);
		
		// set next slide
		timer_current_id++;	

	}
	
	// destroy timer if auto false
	if( ! auto || ! jQuery(".slider").attr('looper') )
		clearInterval(window.timer);

	jQuery(".slider .news .new a").bind('click', function(e){
		if ( !jQuery(this).parent().hasClass('click') ){
			event.preventDefault();
		}
		
		jQuery(".slider .news .new").removeClass('click');
		clearInterval(window.timer);
		move_slider( jQuery(this) );		
		jQuery(this).parent().addClass('click');
		
	});
	
	function move_slider(obj, menuitemID){

		if (!menuitemID) {
			menuitemID = obj.attr('id').replace('menuitem-','');
		}
		jQuery(".slider").attr('looper',false);
		
		jQuery(".slider .news .new").removeClass('current');
		jQuery(".slider .image a").removeClass('current');
		
		obj.parent('.new').addClass('current');
		jQuery(".slider .image a#image-"+menuitemID).addClass('current');
		
	}
	
	$( "#sliderNext" ).click(function() {
		clearInterval(window.timer);

		obj = jQuery( ".slider .image" ).find( ".current" ).next();
		//console.log(obj);
		if (obj.length == 0) { obj = jQuery( ".slider .image a:first-child" ) }
		menuitemID = obj.attr('id').replace('image-','');	
		move_slider(obj, menuitemID);
		
	});


	$( "#sliderBack" ).click(function() {
		clearInterval(window.timer);

		obj = jQuery( ".slider .image" ).find( ".current" ).prev();
		if (obj.length == 0) { obj = jQuery( ".slider .image a:last-child" ) }
		menuitemID = obj.attr('id').replace('image-','');		
		move_slider(obj, menuitemID);
		
	});
		
	//resizeSlider();
	
	jQuery(window).resize(function(){
		//resizeSlider();
	});
});

