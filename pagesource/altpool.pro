                        },
                        complete: function() {

                            $('#btncalc').removeClass('m-loader m-loader--light m-loader--right');
                            shuffleCoins();
                        }
                    });
                }
            });
        });

        function shuffleCoins () {
            var Shuffle = window.Shuffle;

            var Demo = function (element) {
                this.element = element;

                this.shuffle = new Shuffle(element, {
                    itemSelector: '.coin',
                    speed: 1500
                });

                this.mode = 'exclusive';

                function sortByProfit(element) {
                    return element.getAttribute('data-profit');
                }

                this.shuffle.sort({
                    reverse: true,
                    by: sortByProfit
                });
            };

            window.demo = new Demo($('#result'));
        }

        function generateLink () {
            var params = "?";
            if ($("#equ_hashrate").val() != "") {params += 'eqr=' + $("#equ_hashrate").val() + '&';}
            if ($("#eth_hashrate").val() != "") {params += 'etr=' + $("#eth_hashrate").val()+ '&';}
            if ($("#equ_power").val() != "") {params += 'eqp=' + $("#equ_power").val()+ '&';}
            if ($("#eth_power").val() != "") {params += 'etp=' + $("#eth_power").val()+ '&';}
            if ($("#cost").val() != "") {params += 'e=' + $("#cost").val()+ '&';}
            if ($("#poolfee").val() != "") {params += 'f=' + $("#poolfee").val()+ '&';}
            params += "enjoy";

            if (params == "?enjoy") {params = "";}
            else {
                $('#permlink').fadeIn().html('<strong>Permanent link:</strong><br><input class="form-control m-input" value="http://altpool.pro/'+ params+'">');
            }
        }

	</script>

