<script type="text/javascript">
    function addAutocompletion(el, onSelectCb) {
        var lang = {
            searchWith: ''
        };
        var $el = $(el);

        new autoComplete({
            selector: el,
            minChars: 2,
            delay: 100,
            menuClass: 'autocompletion',
            source: function(term, response) {
                $.request('get', 'api_autocomplete.php', { query: term, pageSize: 8 }).then(function(txt) {
                    var json = $.parseJSON(txt);
                    json.Items.push({
                        PlainText: $el.get('value'),
                        Value: $el.get('value'),
                        CustomItem: true
                    });
                    response(json.Items);
                }).error(function() {
                    response([]);
                });
            },
            renderItem: function(item, search) {
                if ('CustomItem' in item && item.CustomItem)
                {
                    return '<li class="autocomplete-suggestion autocomplete-as-typed" data-val="' + item.Value + '">' +
                            '<span class="autocomplete-city">' + lang.searchWith.replace(':input', item.PlainText) + '</span>'
                        + '</li>';
                }

                var val = item.Plaintext;

                if (item.countryName && item.Plaintext.indexOf(item.CountryName) == -1)
                {
                    val = item.Plaintext + ', ' + item.CountryName;
                }

                return '<li class="autocomplete-suggestion" data-val="' + val + '">' +
                    '<span class="autocomplete-city">' + item.Plaintext + (item.CountryName ? ', ' : '') + '</span>' +
                    '<span class="autocomplete-country">' + item.CountryName + '</span>' +
                    '</li>';
            },
            onSelect: function(e) {
                e.preventDefault();
                if (onSelectCb) onSelectCb();
            }
        });
    }
</script>
<script type="text/javascript">
    (function() {
        addAutocompletion('#start', function() {
            var target = $('#end')[0];

            if (target.focus)
                target.focus();
            else
                target.select();
        });
        addAutocompletion('#end');

        var selectAll = function(e) {
            if (e.target.select)
                e.target.select();
            else
                e.setSelectionRange(0, e.target.value.length);
        };

        $('#start').onClick(selectAll);
        $('#end').onClick(selectAll);

    })();
</script>
