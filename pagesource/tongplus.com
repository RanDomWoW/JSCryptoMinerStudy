<script type="text/javascript">
var tong_now = new Date();

function getLatestNews () {
	var _rss = '/news/rss/news.xml?v='+tong_now.getTime(), _tot = 6
		, _exp = new RegExp("http://[^>]*(site/data|sitedata)/(img_dir|thumb_dir|image|thumbnail)[^>]*_thumb\\.jpg", "gim")
		, _htmlexp = new RegExp("http://news.tongplus.com/site/data/html_dir/*[^>]*\\.html", "gim");;
	$.ajax({
		url: _rss
		, type: 'GET'
		, async: false
		, dataType: 'xml'
		, success: function(xmlData){
			try {
				var items = $(xmlData).find("item");
				if (items == null || items.length == 0) return;

				var _ids = "", idlist = "";
				var bodystr = $(".wrap_wid .grid").html();
				if (bodystr != "") {
					idlist = bodystr.match(_htmlexp); 
					if (idlist != null) for (var no = 0; no < idlist.length; no++) _ids += ";" + idlist[no];
				}

				var htmlstr = new Array(), _tmp = '', _idx = 0;
				for (var no = 0; no < items.length; no++) {
					var title_element = $(items[no]).find("title")
						, link_element = $(items[no]).find("link")
						, desc_element = $(items[no]).find("description")
						, categ_element = $(items[no]).find("category")
						, image_element = $(items[no]).find("dc\\:source, source");
					var _title = $(title_element).text()
						, _link = $(link_element).text()
						, _desc = $(desc_element).text()
						, _categ = $(categ_element).text()
						, _image = $(image_element).text()
						, _thumb = '';
					if (_ids.indexOf(";"+_link) != -1) continue;

					var arr_thumb = _desc.match(_exp);
					if (arr_thumb != null) _thumb = arr_thumb[0];

					_tmp += '<div class="grid-item">'
						+ '<div class="list_img">'
						+ '<span class="cat"><a href="' + _link + '?main_news">' + _categ + '</a></span>'
						+ '<a href="' + _link + '?main_news">'
						+ '<p class="img_tit">' + _title + '</p>'
						+ '<div class="up_bg"></div>'
						+ (_image != '' ? '<img src="' + _image + '" onError="this.style.display=\'none\';">' : '')
						+ '</a>'
						+ '</div>'
						+ '</div>';
					if ((_idx+1) % 3 == 0) {
						htmlstr[htmlstr.length] = _tmp;
						_tmp = '';
					}
					if (++_idx >= _tot) break;
				}
				var tag = $(".wrap_wid .grid").find("div").eq(0);
				tag.before(htmlstr[0]);
				tag.after(htmlstr[1]);
			}
			catch (e) {}
		}
		, error: function(status, error) {
		}
	});
}
function getLatestMindline () {
	var _rss = '/mindline/rss/mindline.xml?v='+tong_now.getTime()
		, _exp = new RegExp("http://[^>]*(site/data|sitedata)/(img_dir|thumb_dir|image|thumbnail)[^>]*_thumb\\.jpg", "gim");
	$.ajax({
		url: _rss
		, type: 'GET'
		, async: false
		, dataType: 'xml'
		, success: function(xmlData){
			try {
				var items = $(xmlData).find("item");
				if (items == null || items.length == 0) return;

				var htmlstr = '';
				var title_element = $(items[0]).find("title")
					, link_element = $(items[0]).find("link")
					, desc_element = $(items[0]).find("description");
				var _title = $(title_element).text()
					, _link = $(link_element).text()
					, _desc = $(desc_element).text()
					, _thumb = '';
				var arr_thumb = _desc.match(_exp);
				if (arr_thumb != null) _thumb = arr_thumb[0];

				htmlstr = '<div class="grid-item">'
					+ '<div class="list_img_md">'
					+ '<a href="' + _link + '?main_card">'
					+ '<img src="' + _thumb + '" onError="this.style.display=\'none\';" alt="' + _title + '">'
					+ '</a>'
					+ '</div>'
					+ '</div>';
				$(".wrap_wid .grid").append(htmlstr);
			}
			catch (e) {}
		}
		, error: function(status, error) {
		}
	});
}
getLatestNews();
getLatestMindline();
</script>
