<!-- 飘窗 -->
<div id="ad" style="position: absolute; z-index: 999999; left: 163px; top: 341px;">

</div>
<script type="text/javascript">
		var x_fw = 50,y_fw = 60 ;
		var xin_fw = true, yin_fw = true; 
		var step_fw = 1; 
		var delay_fw = 30 ;
		var obj_fw;
		function floatAD() { 
			var L_fw=T_fw=0 
			var R_fw = $(window).width()-obj_fw.offsetWidth; 
			var B_fw = $(window).height()-obj_fw.offsetHeight ;
			$(obj_fw).css("left", x_fw + $(document).scrollLeft());
			$(obj_fw).css("top", y_fw + $(document).scrollTop());
			
			x_fw = x_fw + step_fw*(xin_fw?1:-1); 
			if (x_fw < L_fw) { xin_fw = true; x_fw = L_fw;} 
			if (x_fw > R_fw){ xin_fw = false; x_fw = R_fw;} 
			y_fw = y_fw + step_fw*(yin_fw?1:-1);
			if (y_fw < T_fw) { yin_fw = true; y_fw = T_fw; } 
			if (y_fw > B_fw) { yin_fw = false; y_fw = B_fw; } 
			} 
		$(function(){
			obj_fw=document.getElementById("ad"); 
			var itl_fw= setInterval("floatAD()", delay_fw); 
			obj_fw.onmouseover=function(){clearInterval(itl_fw)} 
			obj_fw.onmouseout=function(){itl_fw=setInterval("floatAD()", delay_fw)}
		})
</script>

