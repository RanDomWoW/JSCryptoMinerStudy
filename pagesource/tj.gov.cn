					if(title.length>20){
					title=title.substring(0,20)+"...";
					}
					$("#govnews").append("<li><span>["+json.pubDate+"]</span><a target='_blank' href='"+json.link+"'>"+title+"</a></li>");
					$("#govnews2").append("<li><span>["+json.pubDate+"]</span><a target='_blank' href='"+json.link+"'>"+title+"</a></li>");
					}
				})
			},
			 error:function(){
		           alert('fail');
		    }

		});

	})
	
</script>
<SCRIPT language="javascript">
function addDate(dd,dadd){
var a = new Date(dd)
a = a.valueOf()
a = a + dadd * 24 * 60 * 60 * 1000
a = new Date(a)
return a;
}
function changeDate(str){ 
    try{
      var newstr = str.replace(/-/g,'/'); 
      return new Date(newstr);
    }catch (e){
      alert("转换时间出错，原因：" + e.message);
    }
  }
$(document).ready(function(){
	$('.dczjj ul li span').each(function(){
	var endtime =$(this).parent().find("div").text();
	endtime=changeDate(endtime);

	//抓取现在日期
	var stoptime = new Date(endtime);//结束时间！！！
	stoptime = addDate(stoptime,1);
	var today= new Date();
	  if (stoptime<today){
		$(this).text('已结束');
	  }else{
		$(this).text('进行中');
	  }
	  })
})
</SCRIPT>

