        window.onload = function () {
            windowProxyCv = new Porthole.WindowProxy("https://form.jobnet.co.il/Forms/ApplicantRegisterRequestNarrowForm.aspx", "cvFrameNarrow");
            windowProxyCv.addEventListener(onMessageNarrow);
        };

        function onMessageNarrow(messageEvent) {
            /*
            messageEvent.origin: Protocol and domain origin of the message
            messageEvent.data: Message itself
            messageEvent.source: Window proxy object, useful to post a response 
            */
            dataArr = messageEvent.data.split('@');
            if (dataArr[0] == "ConfirmRequired" && windowProxyCv == messageEvent.source) {
                $(".applicantReqBodyWide").modal({
                    closeHTML: '',
                    containerCss: {
                        backgroundColor: "transparent",
                        borderColor: "transparent",
                        height: formH + 5,
                        padding: 0,
                        width: formW + 10,
                        maxWidth: formW + 5
                    },
                    overlayCss: {
                        backgroundColor: "#fff"
                    },
                    opacity: 75,
                    position: ["10%", null],
                    overlayClose: false,
                    onShow: function (dialog) {
                        $("#RegisterRequestConfirmHeader").text(dataArr[1]);
                        $("#RegisterRequestConfirmApprovalMsg").html(dataArr[2]);
                    }
                });
            }
        }

        function confirmClick(value) {
            windowProxyCv.post('ConfirmResponse@' + value);
            $.modal.close();
        }
