<script language="javascript" type="text/javascript">    
    //  ======  UI          ======
    $("div.simple-content :input").corner("round 4px");
    $("div.simple-content :select").corner("round 4px");

    function Load_profsJson(profID) {
        $.ajax({
            type: "GET",
            url: '/Handlers/ProfsFiller.ashx',
            data: {
                ProfID: profID
            },
            async: false,
            success: function (data) {
                var temp = $.parseJSON(data);
                var $select = $('div.simple-content select#subProfSel');
                var selValue = getCookie("searchsubprofid");
                var selected = '';

                $select.find('option').remove();
                $select.append('<option value="0">כל תתי התחומים</option>');
                $.each(temp, function (key, value) {
                    selected = (selValue != null && value.ID == selValue) ? 'selected' : '';
                    $select.append('<option value=' + value.ID + ' ' + selected + '>' + value.Name + '</option>');
                });
            },
            error: function (er) { }
        });
    }

    function SubmitSearch() {

        var freeTxtVal = trimAll(document.getElementById("freeTxt").value);
        var profSelVal = document.getElementById("searchControlUC_profSel").value;
        var subProfSelVal = document.getElementById("subProfSel").value;
        var areaSelVal = document.getElementById("searchControlUC_areaSel").value;
        var validSearch = false;

        var URL = "/positionresults.aspx?searchtype=freesearch";

        if (freeTxtVal != "" && freeTxtVal.length>2) {
            URL += (URL.indexOf('?') > 0 ? "&" : "?") + "freetxt=" + encodeURIComponent(freeTxtVal);
            validSearch = true;
        }

        if (profSelVal != "0" && subProfSelVal == "0") {
            URL += (URL.indexOf('?') > 0 ? "&" : "?") + "searchprofid=" + profSelVal;
            validSearch = true;
        }

        if (subProfSelVal != "0") {
            URL += (URL.indexOf('?') > 0 ? "&" : "?") + "searchsubprofid=" + subProfSelVal;
            setCookieForNumberOfDays("searchsubprofid", subProfSelVal, 0.1);
            validSearch = true;
        }

        if (areaSelVal != "0") {
            URL += (URL.indexOf('?') > 0 ? "&" : "?") + "checkarea=" + areaSelVal;
        }

        if (validSearch)
            window.open(URL, '_self');
        else
            alert('נא לבחור תחומים או להקליד מילת חיפוש תקינה ');
    }


    if (document.getElementById("searchControlUC_profSel").value != "0")
        Load_profsJson(document.getElementById("searchControlUC_profSel").value);

    $("#searchTbl input, select").keypress(function (event) {
        if (event.which == 13) {
            event.preventDefault();
            SubmitSearch();
        }
    });
</script>
