    var locks = [];
    function addXsellToCart(product,button) {
        locks[product] = locks[product] || false;
        if (locks[product]) return;
        locks[product] = true;
        $(button).addClassName('locked');
        $('layer-shader') && $('layer-shader').show();
        $('layer-spinner') && $('layer-spinner').show();
        new Ajax.Request('https://www.ellusionist.com/upsells/cart/add/', {
            method: 'post',
            parameters: {'product':product},
            onFailure: function(transport) {
                $('layer-shader') && $('layer-shader').hide();
                $('layer-spinner') && $('layer-spinner').hide();
            },
            onSuccess: function(transport) {
                try {
                    $('layer-shader') && $('layer-shader').hide();
                    $('layer-spinner') && $('layer-spinner').hide();
                    if( transport.responseText.isJSON() ) {
                        var response = transport.responseText.evalJSON();
                        if( !Object.isUndefined(response.update) )
                            $H(response.update).each(function(update) {
                                var el = $(update.key);

                                if (update.key == 'messages_product_view' && el ) {
                                    if (! $('xsell-xolder')) {
                                        var div = document.createElement('div');
                                        $(div).writeAttribute('id','xsell-xolder').setStyle({'position':'fixed','top':'20px','right':'10px','z-index':'99'});
                                        $$('body').first().insert(div);
                                    }
                                    var holder = $('xsell-xolder');

                                    var m = $('messages_product_view').clone(true);
                                    $(m).writeAttribute('id',null);
                                    $(m).innerHTML = update.value;
//								$(m).setStyle({'position':'fixed','top':'10px','right':'10px','z-index':'10'});
                                    $(m).addClassName('messages_product_view_float');
//								$$('body').first().insert(m);

                                    $(holder).insert(m);
                                    setTimeout(function(){try {$$('.messages_product_view_float').first().remove();} catch(e){}},5000);
                                }	else{
                                    if( el ) el.innerHTML = update.value;
                                }
                            });
                        if( !Object.isUndefined(response.action) )
                            $H(response.action).each(function(action) {
                                eval(action.key+'('+action.value+')');
                            });
                    } else{
                        //showMessage('Error: Invalid response.');
                    }
                } catch(e) {
                    //alert('Error: '+e.message);
                }

            },
            onComplete: function(transport){
                locks[product] = false;
                $(button).removeClassName('locked');
            }
        });
    };

    //]]>
</script>




