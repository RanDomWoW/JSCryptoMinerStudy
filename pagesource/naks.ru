<script type="text/javascript">
	(function($) {
		$.fn.dropDownBlock = function(block, options) {
			var defaults = {
				speed: 'fast',
				top: $(this).height(),
				left: 0
			},
			opts 	= $.extend(defaults, options),
			toggler = $(this),
			block 	= $(block);
			toggler.css({'outline': 'none'})

			toggler.click(function(e) {
				e.preventDefault();
				$(block).css({
					'position' 	: 'absolute',
					'top' 		: (toggler.offset().top + opts['top']) + 'px',
					'right' 		: (toggler.offset().right + opts['right']) + 'px'
				});
				if($(block).is(':visible')) {$(block).fadeOut(opts['speed']); this.innerHTML='Открыть<i class="fa fa-angle-down"></i>';}
				else {$(block).fadeIn(opts['speed']); this.innerHTML='Закрыть<i class="fa fa-angle-up"></i>';}
				this.focus();

			});

		};
	})(jQuery);
	
	$('#link').dropDownBlock($('#block'));
</script>



