//储存本地历史搜索记录
function set_localHistory(v){
	// console.log(v);
	if (window.localStorage) {
		if(!localStorage.searchHistory){
			var v_arr = [];
			v_arr.push(v);
			localStorage.setItem("searchHistory",JSON.stringify(v_arr));
		}else{
			var repeat_index = -1; //重复搜索内容的下标
			var storageStr = localStorage.searchHistory;
			var storageArr = eval('(' + storageStr + ')');
			for(var i = 0;i<storageArr.length;i++){
				if (v.content==storageArr[i].content) {repeat_index=i;}
			}
			if (repeat_index!=-1) {
				//有重复搜索内容
				storageArr.splice(repeat_index,1);
				storageArr.unshift(v);
			}else{
				if (v=='') {return false}
				// 无重复搜索内容
				if (storageArr.length<10) {
					// 历史记录小于十条
					storageArr.unshift(v);
				}else{
					// 历史记录超过十条
					storageArr.pop();
					storageArr.unshift(v);
				}
			}
			storageStr=JSON.stringify(storageArr);
			localStorage.setItem("searchHistory",storageStr);
		}
	}	
}
