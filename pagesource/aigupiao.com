<script type="text/javascript">
$(function(){
	var index=0;
	var totalHeight = 0;
	var autoTimer =0;
	express_refresh();
	window.setInterval(function(){
		autoTimer = clearInterval(autoTimer);
		express_refresh();
	},60000);
	function express_refresh(){
		//滚动要闻数据加载
		$.ajax({
			type:'get',
			url:'../../Api/Express/express_recommend&md='+_Libs.md,
			dataType:'json',
    		success:function(data){
    			//console.log(data);
    			if(data.rslt=='succ'){
    				$('.font_inner').html('');
    				for(var i=0;i<data.datalist.length;i++){
    					var html='<li><a href="/index?act=express" class="roll_pubTime" target="_blank">'+data.datalist[i].rec_time_new+'</a><a href="/index?act=express" class="roll_cont" target="_blank">'+data.datalist[i].content+'</a><a href="/index?act=express" class="roll_more" target="_blank">查看详情&gt;</a></li>'
    					$('.font_inner').append(html);
    				}
    				roll();
    			}
    		}
		});
	}

	//1文字轮播(2-5页中间)开始
	function roll(){
		$(".font_inner li:eq(0)").clone(true).appendTo($(".font_inner"));//克隆第一个放到最后(实现无缝滚动)
		var liHeight = 42;//一个li的高度
		//获取li的总高度再减去一个li的高度(再减一个Li是因为克隆了多出了一个Li的高度)
		totalHeight = ($(".font_inner li").length *  $(".font_inner li").eq(0).height()) -liHeight;
		$(".font_inner").height(totalHeight);//给ul赋值高度
	 	index = 0;
		autoTimer = 0;//全局变量目的实现左右点击同步
		var clickEndFlag = true; //设置每张走完才能再点击

		function tab(){
			$(".font_inner").stop().animate({
				top: -index * liHeight
			},400,function(){
				clickEndFlag = true;//图片走完才会true
				if(index == $(".font_inner li").length -1) {
					$(".font_inner").css({top:0});
					index = 0;
				}
			})
		}

		function next() {
			index++;
			if(index > $(".font_inner li").length - 1){
			//判断index为最后一个Li时index为0
				index = 0;
			}
			tab();
		}
		function prev() {
			index--;
			if(index < 0) {
				index = $(".font_inner li").size() - 2;//因为index的0 == 第一个Li，减二是因为一开始就克隆了一个LI在尾部也就是多出了一个Li，减二也就是_index = Li的长度减二
				$(".font_inner").css("top",- ($(".font_inner li").size() -1) * liHeight);//当_index为-1时执行这条，也就是走到li的最后一个
			}
			tab();
		}
		//自动轮播
		autoTimer = setInterval(next,3000);
		$(".font_inner a").hover(function(){
			clearInterval(autoTimer);
		},function() {
			autoTimer = setInterval(next,3000);
		})
		//切换到下一张
		//$(".roll_wrap .roll_gt").on("click",function() {
		//	if(clickEndFlag) {
		//		next();
		//		clickEndFlag = false;
		//	}
		//});
		//切换到上一张
		//$(".roll_wrap .roll_lt").on("click",function() {
		//	if(clickEndFlag) {
		//		prev();
		//		clickEndFlag = false;
		//	}
		//});
		//鼠标放到左右方向时关闭定时器
		//$(".roll_wrap .roll_lt,.roll_wrap .roll_gt").hover(function(){
		//	clearInterval(autoTimer);
		//},function(){
		//	autoTimer = setInterval(next,3000);
		//})
		//1文字轮播(2-5页中间)结束
	}		
})
</script>
