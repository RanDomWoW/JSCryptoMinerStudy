	//加关注
	$('body').on('click','.sea_livBox',function(){
		if(!userInfo.u_id){
			 $('.logPop,.bgPop').show();
			 return;
		}
		var l_id=$(this).attr('data-id');
		var _this=$(this);
		if ($(this).hasClass('add')){
			$.ajax({
				type:'get',
				url:"/api/live.php?act=follow&id="+l_id,
				dataType:'json',
				beforeSend:function(){
					_this.removeClass('add');
				},
				success:function(data){
					if (data.rslt=='succ') {
						_this.html('<span>已关注</span>');
					}
					common_popup(data.info);
					location.reload();	
				}
			});
		}else if($(this).hasClass('del')){
			$.ajax({
				type:'get',
				url:"/api/live.php?act=unfollow&id="+l_id,
				dataType:'json',
				beforeSend:function(){
					_this.removeClass('del');
				},
				success:function(data){
					if (data.rslt=='succ') {
						_this.html('<span><b style="color:#ed4040;font-size:16px;">+</b>关注</span>');
					}
					common_popup(data.info);
					location.reload();	
				}
			});
		}
	});

