function show_tip(reward_gold){
	if(isNaN(reward_gold)){
		reward_gold = 0;
	}
	if(userInfo.gold){
    	var tip = "应付："+reward_gold+"金币，账号余额："+userInfo.gold+"金币";
   	}
	else{
		var tip = "应付："+reward_gold+"金币，账号余额：0金币";
	}
    if (reward_gold > userInfo.gold){
        tip += "，请先充值";
    }
    jQuery('#reward_tip').html(tip);
}
show_tip(10);
function change_reward_gold(reward_gold){
    jQuery('input[name=reward_gold]').val(reward_gold);
    show_tip(reward_gold);
    jQuery('input[id^=reward_gold_]').removeClass('reveal_btn_cur');
    jQuery('input[id=reward_gold_'+reward_gold+']').addClass('reveal_btn_cur');
}
function focus_other_reward_gold(num){
    jQuery('input[id^=reward_gold_]').removeClass('reveal_btn_cur');
    if (jQuery('#other_reward_gold'+num).val() == '其他金额'){
        jQuery('#other_reward_gold'+num).val('');
    }
}
function blur_other_reward_gold(num){
    if (jQuery('#other_reward_gold'+num).val() == ''){
        jQuery('#other_reward_gold'+num).val('其他金额');
    }
}
function input_other_reward_gold(reward_gold){
    if (isNaN(reward_gold) == true){
        alert("请输入数字");
    }
    else{
        reward_gold = parseInt(reward_gold);
        jQuery('input[name=reward_gold]').val(reward_gold);
        show_tip(reward_gold);
    }
}
</script>
