		if(docTop < headH){
			$('.main_L_wrap').addClass('main_L_normal').removeClass('main_L_fix');
			$('.main_L_wrap').css({'top':'auto','bottom':'auto'});  //重置top bottom值
			$('.main_L_station').hide(); //占位元素
		}else if (docTop > headH && foot_outH < 0) {
			//距顶部距离大于头部高度&&底部未露出
			$('.main_L_wrap').addClass('main_L_fix').removeClass('main_L_normal');
			$('.main_L_wrap').css('bottom','auto');
			$('.main_L_wrap').css('top','65px');
			$('.main_L_station').show();
		}else if(docTop > headH && headH + navH + foot_outH > winH){
			//距顶部距离大于头部高度&&底部露出
			$('.main_L_wrap').addClass('main_L_fix').removeClass('main_L_normal');
			$('.main_L_wrap').css('top','auto');
			$('.main_L_wrap').css('bottom',foot_outH);
			$('.main_L_station').show();
		}
	})
})
