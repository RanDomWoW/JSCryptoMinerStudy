function ts_search_div(_val){
	var _url = "";
	$.ajax({
        type: 'get',
        url: "/index.php?s=/Api/Live/search&keyword="+_val+"&md="+_Libs.md,
        dataType: 'json',
        beforeSend: function (XMLHttpRequest){},
        success: function(data){
        	$('#tc_list').html("");
