	$(function(){
		$('.log_close').click(function(){
			//关闭登录框
			$('.bgPop,.logPop').hide();
		});
		$('.log_replace').click(function(){
			refreshCode();
		})
		function loginCallback(state){
			//console.log(state);
			if (state==0) {
				//alert('登录成功');
				pl_pop('登录成功');
				$('.bgPop,.logPop').hide();
				$('.log_item').removeClass('.log_itemErr');
				$('.log_msg').hide();
				$('.log_msgLink').hide();
				$('#log_name').val('');
				$('#log_pwd').val('');
				$('#log_code').val('');
				$('.item_fore3').hide();
				location.reload();
			}else if (state==1) {
				//账号密码错误
				$('.log_msg').html('账号或密码有误,');
				$('.log_msg').show(); //警示框
				$('.log_item').removeClass('log_itemErr');
				$('.item_fore1').addClass('log_itemErr'); //提示框标红
				$('.log_msgLink').show(); //找回密码按钮
				$('.item_fore3').show(); //验证码
			}else if(state==2){
				//验证码错误
				$('.log_msg').html('验证码错误');
				$('.log_msg').show(); //警示框
				$('.log_item').removeClass('log_itemErr');
				$('.item_fore3').addClass('log_itemErr'); //提示框标红
				$('.log_msgLink').hide(); //找回密码按钮
			}
		}
		$('#log_btn').click(function(){
			var _this=$(this);
			if(_this.hasClass('logable')){
				var _data={};
				_data.account=$('#log_name').val()||''; //用户名
				_data.passwd=$('#log_pwd').val()||''; //密码
				if ($("#codeBox").is(":hidden")) {
					
				}else{
					_data.log_code=$('#log_code').val()||''; //验证码
				}
				

				if (_data.account&&_data.passwd&&_data.account!=''&&_data.passwd!='') {
					_this.removeClass('logable');
					$.ajax({
						type:'post',
						dataType:'json',
						url:"/Api/User/login/md/"+_Libs.md,
						data:_data,
						beforeSend:function(){
							_this.html('正在登录..')
						},
						success:function(data){
							_this.html('登录');
							_this.addClass('logable');
							if(data.rslt =='succ'){
								//登录成功
								loginCallback(0);
							}else if(data.rslt =='fail'&& data.state==0){
								//账号或密码错误
								loginCallback(1);
							}else if(data.rslt =='fail'&& data.state==1){
								//验证码错误
								loginCallback(2);
							}
						}
					});
				}else if(_data.account==''||_data.passwd==''){
					alert('账号密码不能为空');
				}
			}
			
		});
		function pl_pop(info){
	    	//弹出层 
			var pop_html ='<div class="pop_html" style="position:fixed;z-index:999;top:20%;left:50%;margin-left: -120px;width:200px;padding: 20px 20px;box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);border-radius: 2px;background-color: #999;">'+
							'<i class="pop_html_close" style="position:absolute;top:5px;right:5px;width:12px;height:12px;cursor:pointer;background: url(https://cdn.5igupiao.com/static/pc/img/nav/close_white.png) no-repeat"></i>'+
							'<p style="font-size:13px;line-height: 25px;color: #fff;">'+info+'</p>'+
						'</div>';
			$('body').append(pop_html);
			$('body').find('.pop_html').fadeOut(3000);
			$('body').on('click','.pop_html_close',function(){
				$('.pop_html').hide()
			});
	    }
		function refreshCode(){
			var randomNum=Math.random()*1000;
			var _imgUrl="/Api/User/get_code.html"+"?n="+randomNum+"&md="+_Libs.md;
			// console.log(_imgUrl);
			$('#log_codeImg').attr('src',_imgUrl);
		}
		$('#login_btn').attr('href','javascript:void(0)');

		//头部登录框
		$('body').on('click','#login_btn',function(e){
			e.preventDefault();
			$('.logPop,.bgPop').show();
		});

		//内容登录框
		$('body').on('click','#v2_logBtn',function(e){
			e.preventDefault();
			$('.logPop,.bgPop').show();
		});

		//发布动态输入框
		$('body').on('click','#info_fabuBg',function(e){
			e.preventDefault();
			$('.logPop,.bgPop').show();
		});
		//右侧关注
		$('body').on('click','.v2_right_login',function(e){
			if (!userInfo.u_id) {
				e.preventDefault();
				$('.logPop,.bgPop').show();
			}
		});
		//我的组合
		$('body').on('click','.v2_mu_comb',function(e){
			if (!userInfo.u_id) {
				e.preventDefault();
				$('.logPop,.bgPop').show();
			}
		});
	})
	</script>
