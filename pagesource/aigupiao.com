	bulletin_roll:function(){
		//1文字轮播(2-5页中间)开始
		$(".font_inner2 li:eq(0)").clone(true).appendTo($(".font_inner2"));//克隆第一个放到最后(实现无缝滚动)
		var liHeight = 113;//85一个li的高度
		//获取li的总高度再减去一个li的高度(再减一个Li是因为克隆了多出了一个Li的高度)
		var totalHeight = ($(".font_inner2 li").length * $(".font_inner2 li").eq(0).innerHeight()) -liHeight;
		$(".font_inner2").height(totalHeight);//给ul赋值高度
		var index = 0;
		var autoTimer = 0;//全局变量目的实现左右点击同步
		var clickEndFlag = true; //设置每张走完才能再点击

		function tab2(){
			$(".font_inner2").stop().animate({
				top: -index * liHeight
			},500,function(){
				clickEndFlag = true;//图片走完才会true
				if(index == $(".font_inner2 li").length -1) {
					$(".font_inner2").css({top:0});
					index = 0;
				}
			})
		}

		function next2(){
			index++;
			if(index > $(".font_inner2 li").length - 1){
			//判断index为最后一个Li时index为0
				index = 0;
			}
			tab2();
		}
		function prev2(){
			index--;
			if(index < 0) {
				index = $(".font_inner2 li").size() - 2;//因为index的0 == 第一个Li，减二是因为一开始就克隆了一个LI在尾部也就是多出了一个Li，减二也就是_index = Li的长度减二
				$(".font_inner2").css("top",- ($(".font_inner2 li").size() -1) * liHeight);//当_index为-1时执行这条，也就是走到li的最后一个
			}
			tab2();
		}
		//自动轮播
		clearInterval(autoTimer);
		autoTimer = setInterval(next2,5000);
//		$(".font_inner2 li").hover(function(){
//			clearInterval(autoTimer);
//		},function(){
//			autoTimer = setInterval(next2,5000);
//		});
		$('.record_list').on('click','.rwcord_expand',function(e){
			index = 0;
			clearInterval(autoTimer);
			var r_length = $(".font_inner2 li").length;
			var totalHeight2 = 0;
			if(!$(this).hasClass('rwcord_all')){
				$(this).addClass('rwcord_all');
				$('.font_inner2').stop().css("top","0");
				for(var i=0;i<r_length-1;i++){
					totalHeight2 += $(".font_inner2 li").eq(i).css('height','auto').innerHeight();
				}
				$('.roll_wrap2').css('height',totalHeight2);
				$('.rwcord_expand span').html('收起');
				$('.rwcord_expand i').addClass('expand_i');
			}
			else{
				$(this).removeClass('rwcord_all');
				$('.font_inner2').css("top","0");
				$('.font_inner2 li').css('height','86px');
				$('.roll_wrap2').css('height',"113px");
				$('.rwcord_expand span').html('展开全部');
				$('.rwcord_expand i').removeClass('expand_i');
				autoTimer = setInterval(next2,5000);
			}
		});
	}
}
