        <script>
    $(document).ready(function () {
        // Fetch Facebook Likes once and every 30 seconds thereafter
        // Adjust setInterval to either fetch content in different frequency or remove to only fetch once.
        realtime_fb_likes();
        realtime_tw_likes();
        realtime_yt_likes();
        realtime_ins_likes();
        setInterval("realtime_fb_likes()", 30000);
        setInterval("realtime_yt_likes()", 30000);
        setInterval("realtime_tw_likes()", 30000);
        setInterval("realtime_ins_likes()", 30000);
    });
    function realtime_tw_likes(username, callback) {
        $.getJSON("https://query.yahooapis.com/v1/public/yql?q=select%20*%20from%20html%20where%20url%3D'http%3A%2F%2Ftwitter.com%2F" + username + "'%20and%20xpath%3D'%2F%2Finput%5B%40id%3D%22init-data%22%5D'&format=json&env=store%3A%2F%2Fdatatables.org%2Falltableswithkeys", function (data) {
            if (callback) { callback(data.query.results.input.value.match(/followers_count":(\d+)/)[1]); }
        });
    }

    realtime_tw_likes('kadikoybelediye', function (count) {
        var tw_likes = addCommas(count);
        tw_likes += ' Takipçi';
        $('#tw-likes-count').text(tw_likes)
    });
    function realtime_fb_likes() {
        $.getJSON('http://graph.facebook.com/kadikoybelediye/', function (data) {
            var fb_likes = addCommas(data.likes);
            fb_likes += ' Takipçi';
            $('#fb-likes-count').text(fb_likes);
        });
    }
    function realtime_yt_likes() {
        var part = 'statistics';
        var forUsername = 'kadikoybelediye';
        var key = 'AIzaSyARKfpISNL50-nUIf8XaxxYpG8IDfR7Fgg';
        $.getJSON('https://www.googleapis.com/youtube/v3/channels?part=' + part + '&alt=json&forUsername=' + forUsername + '&key=' + key + '&v=2', function (item, status, xhr) {
            var yt_likes = addCommas(item.items[0].statistics.subscriberCount);
            yt_likes += ' Abone';
            $('#yt-likes-count').text(yt_likes);
        })
    }
    function realtime_ins_likes() {
        var accestokem = '1050575735.1677ed0.d7dc52cd80be4276a8c366548546ed28';
        var $url = 'https://api.instagram.com/v1/users/206754318/?access_token=' + accestokem + '';
        jQuery.ajax({
            method: 'GET',
            url: $url,
            dataType: 'jsonp',
            jsonp: 'callback',
            success: function (data) {
                var ins_likes = addCommas(data.data.counts.followed_by);
                ins_likes += ' Takipçi';
                $('#ins-likes-count').text(ins_likes);
            }
        });
    }
    function addCommas(nStr) {
        nStr += '';
        x = nStr.split('.');
        x1 = x[0];
        x2 = x.length > 1 ? '.' + x[1] : '';
        var rgx = /(\d+)(\d{3})/;
        while (rgx.test(x1)) {
            x1 = x1.replace(rgx, '$1' + ',' + '$2');
        }
        return x1 + x2;
    }
