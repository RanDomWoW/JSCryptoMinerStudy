			})
			.live("mouseout", function () {
				$(".subMenu").css("display", "none");
				$(this).removeClass("hover");
			});

		$(".subMenu")
			.mouseenter(function () {
				$(this).css("display", "block");
				$("#menu" + $(this).attr("id").replace("subMenu", "")).addClass("hover");
			})
			.mouseleave(function () {
				$(".subMenu").css("display", "none");
				$("#menu" + $(this).attr("id").replace("subMenu", "")).removeClass("hover");
			});
	});
</script>

