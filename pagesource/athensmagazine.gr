    <script>
    $(function(){
			
		var offset = 300,
		offset_opacity = 1200,
		scroll_top_duration = 700,
		$back_to_top = $('#floating-buttons');
	
		$(window).scroll(function(){
			( $(this).scrollTop() > offset ) ? $back_to_top.addClass('fbt-is-visible') : $back_to_top.removeClass('fbt-is-visible fbt-fade-out');
			if( $(this).scrollTop() > offset_opacity ) { 
				$back_to_top.addClass('fbt-fade-out');
			}
		});
	
		$back_to_top.find('.back-to-top').on('click', function(event){
			event.preventDefault();
			$('body,html').animate({
				scrollTop: 0 ,
				}, scroll_top_duration
			);
		});


		var cookie_tag = 'youArticlesTrackerSession';
		var timeout = parseInt(Cookies.get(cookie_tag));

		//Cookies.set(cookie_tag, curstamp, { expires: 365 });	
		if(!timeout){
			var curstamp = Math.round((new Date()).getTime() / 1000);
			Cookies.set(cookie_tag, curstamp);
			timeout = curstamp
		}

		if(timeout){
			
			function reduceArticles(value) {
			  return function(element, index, array) {
				return (element >= value);
			  }
			}

			$.get('http://www.athensmagazine.gr/files/articles_list.data', function(data){
				var latest_articles = data.split('|');
				var new_articles = latest_articles.filter(reduceArticles(timeout)).length;
				if(new_articles>0){
					new_articles = new_articles>=95?new_articles+'+':new_articles;
					$('#with_new_articles .badge').html(new_articles);
					if(new_articles==1){ $('#with_new_articles .text').html('Νέο άρθρο'); }
					$('#without_new_articles').hide();
					$('#with_new_articles').show();
					
										
				}
				
							}, 'text');
		}
		$(document).on('click', '.button-share-facebook' ,function(e){
			e.preventDefault();
						window.open('https://www.facebook.com/sharer/sharer.php?sdk=joey&u=' + window.location + '&display=popup&ref=plugin&src=share_button', 'Facebook', 'width=640,height=580');
		});	
	
		$(document).on('click', '.back-to-home' ,function(e){
			e.preventDefault();
			var curstamp = Math.round((new Date()).getTime() / 1000);
			Cookies.set(cookie_tag, curstamp);
			window.location = $(this).data('href');
		});	
	
	});
    </script>
