        <script>
//Add 'sale' class to .ProductList li s ifthey contain a <strike>
//.sale adds an banner to the product indicating it's on sale
function tagSale() {
    $('.ProductList>li').each(function() {
        var product = $(this).find('strike').closest('.ProductList>li')
        product.addClass('sale');
    });
}
//Gets non-sale attribute information from the product summary
//Attribute information should be added to the start fo product
//description, enclosed between *** and ***. Attribute names are
//the same as class names: Closeout, Pre-Order, New, and Video Reviews, 
//(.Video.Reviews in CSS). Also calls tagSale
function processTags(){
    tagSale();
    $('.ProductList>li').each(function(index, value){
        var product = $(this);
        var text = product.find('.Summary').text().trim();
        var start = text.indexOf('***');
        var end = text.indexOf('***', start + 3);
        if (start == 0 && end != -1) {
            var arrayString = text.substring(start+3, end);
            var classArray = arrayString.split(', ');
            for (var i = classArray.length - 1; i >= 0; i--) {
                var className = classArray[i];
                product.addClass(className);
            };
        }
    });
}
//Page load call
$(document).ready(processTags);
//Ajax Complete call, for faceted search
$(document).ajaxComplete(processTags);

</script>
