function collectWeights()
{
	var totalWeight = 0;
	$('.answerRadio:checked').each(
	function()
	{
		totalWeight += parseFloat(weightArray[($(this).attr('id'))]);
	})
	$('.selectbox').each(
	function()
	{
		id = $(this).val();
		if(id)
		{
			totalWeight += parseFloat(weightArray['answer_'+id]);
		}
		
	});
	if(totalWeight == 0 || isNaN(totalWeight))
	{
		$('#surveyWeight').hide();
	}
	else
	{
		$('#surveyWeight').html(totalWeight);
		$('#surveyWeight').show();
	}
}


function deleteComment(id)
{
	var check = $('#deleteButton_'+id).val();
	if(check == 'Delete')
	{
		$('#deleteButton_'+id).val('Undo');
		$('#commentText_'+id).css('text-decoration','line-through');
		$('#comment_'+id).prop('disabled',null);
		$('#comment_'+id).prop('name','comment_'+id);
	}
	else
	{
		$('#deleteButton_'+id).val('Delete');
		$('#commentText_'+id).css('text-decoration','none');
		$('#comment_'+id).prop('disabled','true');
		$('#comment_'+id).prop('name','');
	}
}


function surveyValidation()
{
	$('#surveyForm').validationEngine('detach');
	$('#surveyForm').validationEngine();
}

function startSurvey()
{
	esbmGetPage('&command=esbmStartResponse','','surveyCheckpoint')
}

function surveyCheckpoint(data)
{
	var obj = $.parseJSON(data);
	var response_id = obj.response_id;
	var error = obj.error;
	if(response_id == 0)
	{
		$('#surveyStartMessage').html(error);
		$('#surveyStartMessage').css('background-color','#990000');
	}
	else
	{
		$('#surveyStartMessage').hide();
		$('#surveyOverlay').hide();
		surveyRestart();
	}
}

function saveSurveyChanges()
{
	var response_status = $('#response_status').val();
	$('#surveyForm').hide();
	sendResponse(response_status,'');
}

function surveySave(response_status,validate)
{
	$('.eSBMSignature').each(function()
	{
		var answer_id = $(this).data('reference');
		var svg = $(this).jSignature('getData','svg');
		$('#'+answer_id).val(svg);
	});

	// $('.emailOnly[value=""]').removeClass('validate[required,custom[email]]');
	var mainTest = $('#surveyForm').validationEngine('validate');
	var emailInput = $('.emailFail').length;
	var passwordInput = $('.passwordFail').length;
	if(emailInput > 0)
	{
		esbmMemberCheck();
	}
	else if(passwordInput > 0)
	{
		$('.memberPassword').each(
		function()
		{
			var id = $(this).attr('id');
			id = id.replace(/[^0-9]/g,'');
			esbmPasswordCheck(id);
		});
		$('.memberPasswordCompare').each(
		function()
		{
			var id = $(this).attr('id');
			id = id.replace(/[^0-9]/g,'');
			esbmPasswordCompare(id);
		});
	}
	else if((validate == 1 || response_status == 1) && mainTest)
	{
		if($('.surveyCaptcha').length)
		{
			$('.surveyCaptchaValidation').validationEngine('hide');
			var g_recaptcha_response = $('#g-recaptcha-response').val();
			esbmGetPage('&command=esbmValidateCaptcha&g-recaptcha-response='+g_recaptcha_response,'',function(data)
			{
				if(data == 1)
				{
					sendResponse(response_status,'');
				}
				else
				{
					$('.surveyCaptchaValidation').validationEngine('showPrompt','*Are you a robot?');
				}
			});
		}
		else
		{
			sendResponse(response_status,'');
		}
	}
	//this breaks some survey styles
	//review breaks
	else if(response_status === 0 && validate != 1)
	{
		//just saving form
		//$('#surveyForm').hide();
		//$('#surveyReview').hide();
		sendResponse(response_status,1);
	}
	else
	{
		// $('.emailOnly[value=""]').addClass('validate[required,custom[email]]');
	}
}


function sendResponse(response_status,end)
{
	eSBM.response_status = response_status;
	eSBM.end = end;
	if($('.fileUpload').length)
	{
		$('#surveyFrame').on('load',function()
		{
			$('.formError').remove();
			var test = $('#surveyFrame').contents().find('div').length;
			if(test)
			{
				$('#surveyFrame').contents().find('div').each(
				function()
				{
					var content = $(this).html();
					var contentArray = content.split(":");
					var answer_id = contentArray[0];
					var error = contentArray[1];
					$('#'+answer_id).validationEngine('showPrompt',error,'','',1);
				});
			}
			else
			{
				finishResponse();
			}


		});
		$("#command").val('esbmUploadResponse');
		$("#surveyForm").submit();
	}
	else
	{
		$('#surveyFrame').off('load');
		finishResponse();
	}
}

function finishResponse()
{
	$('#surveyForm').hide();
	response_status = eSBM.response_status;
	end = eSBM.end;
	$("#command").val('esbmSurveyResponse');
	var params = $('#surveyForm').serialize();
	if(params)
	{
		esbmGetPage('&command=esbmSurveyResponse&response_status='+response_status+'&'+params,'surveyContent',
		function()
		{
			if(end==1)
			{
				$('.surveyReview').hide();
				$('.surveySave').hide();
				$('.surveySubmit').hide();
			}
		});
	}
}
</script>
