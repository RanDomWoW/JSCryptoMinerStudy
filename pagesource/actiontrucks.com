<script type="text/javascript">
$(document).ready(
function()
{
	//$('html, body').animate({scrollTop:0}, 'slow');
	$('#surveyContent').on('click','.surveyCheckbox input:checkbox',function(){
		if($(this).is(':checked'))
		{
			$(this).closest('.surveyCheckbox').addClass('checked');
		}
		else
		{
			$(this).closest('.surveyCheckbox').removeClass('checked');
		}
	});
	$('#surveyContent').on('click','.surveyRadio input:radio',function(){
		var name = $(this).attr('name');
		$('.surveyRadio input:radio[name="'+name+'"]').closest('.surveyRadio').removeClass('checked');
		$(this).closest('.surveyRadio').addClass('checked');
	});

	$(window).on('resize',esbmResize);
	esbmResize();
});


var url = "/site/?";
var type = "POST";
var resetData = "";
function esbmResize()
{
	var container = $('.surveyContainer').first();
	var width = container.innerWidth();
	container.removeClass('surveyXS surveySM surveyMD surveyLG');
	if(width > 1200)
	{
		container.addClass('surveyLG');
	}
	else if(width > 850)
	{
		container.addClass('surveyMD');
	}
	else if(width > 480)
	{
		container.addClass('surveySM');
	}
	else
	{
		container.addClass('surveyXS');
	}
}
function esbmGetPage(params,div,func,async)
{
	if(async != 'false')
	{
		async = 'true';
	}
	params += '&ajax=1&ajdt=1';
	if(div == 'NEW')
	{
		window.open(url+params);
	}
	else
	{
		$.ajax
		({
			type: type,
			url: url,
			data: params,
			async: true,
			success: function(data)
			{
				resetData = data;
				if(div == 'COLORBOX')
				{
					$.fn.colorbox({html:data});
				}
				else if(div == 'DIALOG')
				{
					$('#dialogList').append(data);
				}
				else if(div != '')
				{
					$('#'+div).html(data);
				}

				if(typeof func !== 'undefined' && func != '')
				{
					if(typeof func === 'function')
					{
						func(data);
					}
					else if(typeof window[func] === 'function')
					{
						window[func](data);
					}
				}

			},
			complete: function(xhr,statusText)
			{
				if(xhr.status == 0 || xhr.status == '')
				{
					if(count < 5)
					{
						count++;
						esbmGetPage(params,div,func,async,count);
					}
				}
			}
		});
	}
}

function surveyRestart()
{
	esbmGetPage('&command=esbmGetPage&modulePage=surveyContent','surveyContent');
}

function closeWindow()
{
	if(confirm('Do you wish to leave this session of "Newsletter Signup"?'))
	{
		if(window.opener)
		{
			window.close();
		} 
		else 
		{
			window.location = '/';
		}
	}
}

function forceCloseWindow()
{
	if(confirm('Do you wish to leave this session of "Newsletter Signup"?'))
	{
		window.close();
	}
}
</script>
