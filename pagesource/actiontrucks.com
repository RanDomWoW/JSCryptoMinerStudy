function formatForm()
{
	$('.targetQuestion').removeClass('targetHidden');
	$('.targetAnswer').removeClass('targetHidden');
	$('.surveySection').removeClass('targetHidden');
	//#####################################################
	$('#surveyForm .targetQuestion').addClass('targetHidden');
$('#surveyForm .targetAnswer').addClass('targetHidden');
$('#surveyForm .targetAnswer').closest('.surveyCheckbox').addClass('targetHidden');
$('#surveyForm .targetAnswer').closest('.surveyRadio').addClass('targetHidden');
$('#surveyForm .fixedHidden').addClass('targetHidden');
$('#surveyForm .fixedHidden').closest('.surveyCheckbox').addClass('targetHidden');
$('#surveyForm .fixedHidden').closest('.surveyRadio').addClass('targetHidden');

	//#####################################################
	$('#surveyForm .surveyQuestion').each(
	function()
	{
		var id = $(this).attr('id');
		var show = 0;
		$(this).find('.answerField').each(
		function()
		{
			if(!$(this).parent().hasClass('targetHidden'))
			{
				show = 1;
			}
		});
		if(show == 0)
		{
			$(this).addClass('targetHidden');
		}

