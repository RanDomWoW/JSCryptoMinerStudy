					<script type="text/javascript">
						$(document).ready(function()
						{
							$('#subscribe-form form').validate(
							{
								submitHandler: function(form)
								{
									$.fancybox.showLoading();
									$.ajax({
										url: '/local/includes/area/subscribe.form.php',
										timeout: 10000,
										type: 'post',
										data: $(form).serialize(),
										dataType: 'json',
										success: function(data)
										{
											$.fancybox.hideLoading();
											if (data.success == true)
											{
												$(form)[0].reset();
												$("#subscribe-form").modal("hide");
											}
											if (data.message != '')
												showMessage(data.message);
										},
										error: function()
										{
											$.fancybox.hideLoading();
											alert('Произошла ошибка при отправке запроса');
										}
									});
								},
								focusInvalid: false,
								focusCleanup: false,
								rules:
								{
									'EMAIL': {
										"email": true,
										"required": true
									},
									'FIO': {
										"required": true
									},
									'agree': {
										"required": true
									},
								},
								messages:
								{
									'EMAIL': "Введите вашу почту",
									'FIO': "Введите ваше ФИО"
								}
							});
							var chb = $("#subscription_agree");
							var chbl = $("#subscription_agree_label");
							setInterval(function(){
								if (chb.hasClass('error'))
									chbl.css('color', '#ee622d');
								else
									chbl.css('color', '#000');
							}, 100);
						});
					</script>
			</div>
		</div>
	</div>
