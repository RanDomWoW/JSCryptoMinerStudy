                        //grab the width and calculate left value
                        var item_width = $('#bcarousel #slides li').outerWidth();
                        var left_value = item_width * (-1);

                        //move the last item before first item, just in case user click prev button
                        $('#bcarousel #slides li:first').before($('#bcarousel #slides li:last'));

                        //set the default item to the correct position
                        $('#bcarousel #slides ul').css({'left': left_value});

                        //if user clicked on prev button
                        $('#bcarousel #prev').click(function () {

                            //get the right position
                            var left_indent = parseInt($('#bcarousel #slides ul').css('left')) + item_width;

                            //slide the item
                            $('#bcarousel #slides ul:not(:animated)').animate({'left': left_indent}, 200, function () {

                                //move the last item and put it as first item
                                $('#bcarousel #slides li:first').before($('#bcarousel #slides li:last'));

                                //set the default item to correct position
                                $('#bcarousel #slides ul').css({'left': left_value});

                            });

                            //cancel the link behavior
                            return false;

                        });


                        //if user clicked on next button
                        $('#bcarousel #next').click(function () {

                            //get the right position
                            var left_indent = parseInt($('#bcarousel #slides ul').css('left')) - item_width;

                            //slide the item
                            $('#bcarousel #slides ul:not(:animated)').animate({'left': left_indent}, 200, function () {

                                //move the first item and put it as last item
                                $('#bcarousel #slides li:last').after($('#bcarousel #slides li:first'));

                                //set the default item to correct position
                                $('#bcarousel #slides ul').css({'left': left_value});

                            });

                            //cancel the link behavior
                            return false;

                        });

                        //if mouse hover, pause the auto rotation, otherwise rotate it
                        $('#bcarousel #slides').hover(
                                function () {
                                    clearInterval(run);
                                },
                                function () {
                                    run = setInterval('rotate()', speed);
                                }
                        );

                    });

                    //a simple function to click next link
                    //a timer will call this function, and the rotation will begin :)
                    function rotate() {
                        $('#bcarousel #next').click();
                    }

                </script>
            
            <div id="bcarousel">
                <div id="buttons" class="hidden">
                    <a id="prev">prev</a> <a id="next">next</a>

