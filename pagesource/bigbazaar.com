<script>
	$(document).ready(
		function()
		{
			$('#btnSubscription').on('click', submitSubscriptionForm);
			$('#subscription_msg a').click(function(evt){evt.preventDefault();
			$('#subscription_msg').removeClass('animated fadeInUp alert-show');return false;});
		}
	);	
	function submitSubscriptionForm()
	{
		var re = /^[0-9]+$/;
		var re_alpha = /^[A-Za-z ]+$/;
		var re_code = new RegExp("^(\\+)?(\\d+)$");
		var msg = '';
		var field_name = jQuery.trim($('#usr_name').val());
		var field_email = jQuery.trim($('#usr_mail').val());
		var field_mobile = jQuery.trim($('#usr_mobile').val());
		if(field_name == "")
		{
			msg = 'Please enter your name';
			$('#usr_name').focus();
		}
		else if (!re_alpha.test(field_name))
		{
			msg = 'Name should contain only alphabets';
			$('#usr_name').focus();
		}
		else if(field_name.length > 150)
		{
			msg = 'Name should not be more than 150 characters';
			$('#usr_name').focus();
		}
		else if(field_mobile == "")
		{
			msg = 'Please enter your contact number';
			$('#usr_mobile').focus();
		}
		else if (!re.test(field_mobile))
		{
			msg = 'Contact number should contain only digits';
			$('#usr_mobile').focus();
		}
		else if(field_mobile.length != 10)
		{
			msg = 'Contact number should be 10 digits';
			$('#usr_mobile').focus();
		}
		else if(field_email == "")
		{
			msg = 'Please enter your email id';
			$('#usr_mail').focus();
		}
		else if(field_email.length > 255)
		{
			msg = 'Email id should not be more than 255 characters';
			$('#usr_mail').focus();
		}
		else if(echeck(field_email) == false)
		{
			msg = 'Invalid E-mail ID';
			$('#usr_mail').focus();
		}	
		if(msg == '')
		{			
			$('#btnSubscription').off('click', submitSubscriptionForm);
			var ajxurl = "https://www.bigbazaar.com/index.php/subscription/subscription_process";
			$.post(
				ajxurl,
				$('#frmSubscription').serialize(),
				function(response)
				{
					if(response.err)
					{
						$('#err_container').html('').html(response.msg);
						$('#btnSubscription').on('click', submitSubscriptionForm);
					}
					else
					{
						$('#frmSubscription')[0].reset();
						$('#err_container').html('');
						$('#subscription_msg p').html('').html(response.msg);
						$('#subscription_msg').addClass('animated fadeInUp alert-show');
						$('#btnSubscription').on('click', submitSubscriptionForm);
					}
				},
				"json"
			);
		}
		else
		{$('#err_container').html(msg);}
		return false;
	}
</script>
