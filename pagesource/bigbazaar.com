															$('#loc-name').html('').html(mcity);
														}
													}
												);
												
												$('#txtSrchStoreLocality').val('').val(usr_street);
												
												$.get(
													"https://www.bigbazaar.com/store-locator/getStoreNearUser/",
													{ulat:usr_latitude, ulng:usr_longitude},
													function(data)
													{
														if(!data.err)
														{
															var strList='';															
															var storeopen = '';
															var storeclosed = '';
															var time_txt = ''

															$.each(data.records, function(key, val)
																{
																	arrBBStores[val.storeID] = val;

																	storeopen = val.storeopen;
