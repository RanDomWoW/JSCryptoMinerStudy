																	//mlink = '<a href="#" data-rec_id="'+val.storeID+'" class="ancMap">Locate Now</a>';
																	//strList+='<li><H2>'+val.storeName+'</H2><p>'+store_addr+'</p>'+mlink+'</li>';
																
																	strList+='<li><h3>'+val.storeName+', '+store_addr+'</h3><p class="str-time">'+time_txt+'</p><a href="#" data-rec_id="'+val.storeID+'" class="btn-get-direction ancMapFull" title="Get Directions">Get Directions</a></li>';
																}
															);
															
															var resp_container = $('#store_srch_result').find('ul');
															$(resp_container).html('').html(strList);
															if(sessionStorage.getItem('popState') != 'shown')
															{
																if(sessionStorage.getItem('SecInfoPopShown') == 'shown')
																{
																	$('#store_srch_result').show();
																	sessionStorage.setItem('popState','shown');
																	$('#store_srch').trigger('click');
																}
															}
														}
														else
														{
															alert(data.msg);
														}
													},
													'json'
												);
											}
											else
											{
												$('#loc-name').html('').html('Find your nearest store...');
											}
										}
										else
										{
											window.alert('No results found');
										}
									}
									else
									{
										//window.alert('Geocoder failed due to: ' + status);
									}
								});
							}
		
