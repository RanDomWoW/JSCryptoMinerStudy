							if(!data.err)
							{
							var strList='';
							var storeopen='';
							var storeclosed = '';
							var time_txt = '';

							var strList='';
							$.each(data.records, function(key, val)
							{
							store_addr = val.storeAddr;
							mlink = '<a href="#" data-rec_id="'+val.storeID+'" class="ancMap" title="Locate Now">Locate Now</a>';
							//strList+='<li><H2>'+val.storeName+'</H2><p>'+store_addr+'</p>'+mlink+'</li>';

							storeopen = val.storeopen;
							storeclosed = val.storeclosed;
							time_txt = 'Store Time: '+storeopen+' to '+storeclosed;
							
							strList+='<li><label>'+store_addr+'</label><h3>'+val.storeName+', '+store_addr+'</h3><p class="str-time">'+time_txt+'</p><a href="#" data-rec_id="'+val.storeID+'" class="btn-get-direction ancMapFull" title="Get Directions">Get Directions</a></li>';
