							function getUserCity()
							{
								var usr_city = '';
								var usr_street = '';
								var geocoder = new google.maps.Geocoder;		
								var latlng = {lat: usr_latitude, lng: usr_longitude};

								geocoder.geocode({'location': latlng}, function(results, status)
								{
									if (status === 'OK')
									{
										if(results[0])
										{
											$.each(results[0].address_components, function (i, address_component) {
												if (address_component.types[0] == "route")
												{
													usr_street = address_component.long_name;
												}											
												if (address_component.types[0] == "locality")
												{
													usr_city = address_component.long_name;
												} 
											});
											
											if(usr_city != '')
											{
												//$('#slctCitySrch option').filter(function(index){ return $(this).text() === usr_city; }).prop('selected', true);
												$('#slctCitySrch option').filter(
