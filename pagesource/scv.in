            .appendTo(ul);
            };

            $(".search_button").click(function () {
                if ($.trim($(".search_input_box").val()) != "" && $.trim($(".search_input_box").val()) != "Search for channels, programs, actors. . .") {
                    if (detectScript($(".search_input_box").val().trim()) == '1') {
                        $(".search_input_box").css("border", "1px solid red");
                    }
                    else {
                        var param = encodeURIComponent($(".search_input_box").val().trim());
                        window.location.href = 'Search.aspx?SearchStr=' + param;
                    }
                }
            });

            $(".search_input_box").keypress(function (e) {
                $(this).css("border", "none");
                if (e.which == 13) {
                    $(".search_button").trigger('click');
                    return false;
                }
            });

            function detectScript(str) {
                str = str.toLowerCase();
                if (str.indexOf("location.href") != -1)
                    return '1';
                else {
                    var lessThanPosition = str.indexOf("<");
                    var greaterThanPosition = str.indexOf(">");

                    if (lessThanPosition == -1 || greaterThanPosition == -1) {
                        if ((lessThanPosition == -1 && greaterThanPosition > -1) || (greaterThanPosition == -1 && lessThanPosition > -1))
                            return '1';
                        return '0';
                    }
                    else {
                        if (lessThanPosition < greaterThanPosition) {
                            return '1';
                        }
                        else {
                            var nxtCheckStr = str.substring(lessThanPosition + 1, str.length);
                            if (nxtCheckStr.indexOf(">") != -1)
                                return '1';
                            return '0';
                        }
                    }
                }
            }


        });
    </script>
