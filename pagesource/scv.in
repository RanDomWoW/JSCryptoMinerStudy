    <!-- Scv QuickPay Validation and Redirction -->
    <script type="text/javascript">
        function ValidateQuickPay() {
            var enteredValue = '';
            var key = CryptoJS.enc.Utf8.parse('7061737323313233');
            var iv = CryptoJS.enc.Utf8.parse('7061737323313233');
            var varValues = document.getElementById('txt_quickpay').value;

            if (varValues.length == 10) {
                /** Check right mobile number or not **/
                if ((varValues.charAt(0) != 9) && (varValues.charAt(0) != 8) && (varValues.charAt(0) != 7)) { alert('Invalid mobile number', 'red'); return false; }
                enteredValue = '@rmn=' + varValues;

            } else if (varValues.length == 11) {
                /** Check right smc number or not **/
                if ((varValues.charAt(0) != 4) && (varValues.charAt(0) != 2) && (varValues.charAt(0) != 1)) { alert('Invalid SMC or VC number'); return false; }
                //if ((varSmc.value.charAt(0) != 7)) { UpdateMessage('Invalid SMC or VC number', 'red'); preloader(); return false; }
                if ((varValues.charAt(0) == 4) || (varValues.charAt(0) == 2)) { varValues = varValues.slice(0, -1) + "0"; }

                enteredValue = '@smc=' + varValues;
            }
            else {
                alert('Invalid smc (or) mobile number');
                return false;
            }

            var min = 1;
            var max = 6;

            var now = new Date();
            var validDate = now.getDate() + '-' + now.getMonth() + '-' + now.getFullYear();
            var random = Math.floor(Math.random() * (max - min + 1)) + min;

            var encrypted = CryptoJS.AES.encrypt(CryptoJS.enc.Utf8.parse(getRandomKey(random, validDate + enteredValue)), key,
            {
                keySize: 128 / 8,
                iv: iv,
                mode: CryptoJS.mode.CBC,
                padding: CryptoJS.pad.Pkcs7
            });

            window.location.assign("http://myportal.scv.in/Home?" + encrypted);
            //window.location.assign("http://localhost:51314/ScvPortal/Home?" + encrypted);
        }

        function numericFilter(txb) {
            txb.value = txb.value.replace(/[^\0-9]/ig, "");
        }

    </script>
