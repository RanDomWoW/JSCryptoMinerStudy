 	<script>
	$(document).ready(function(){
		var in_menu = false;
		$("#lineTabs li a").each(function (i) {
			$("#lineTabs li:eq("+i+")").hover(function(){ if(!in_menu){



				var tab_id=i+1;
				var scrollfromtop = $(window).scrollTop();
				var navheight = $("#lineTabs li").scrollTop();
				var navoffset = $("#lineTabs li").offset().top;

			    intervalID=setTimeout(
			        function() {





				$("#lineTabs li > div").removeClass("active");
				$(this).children("div").addClass("active");


				if ($('.index-navigation').width() < 481)
				{

				$("#tab"+tab_id).show("fast");
				}
				else
				{
				$("#tab-content > div").hide();
				$("#tab"+tab_id).show();
				}




				$("#tab"+tab_id).hover(
				function(){
					$(this).css({display: 'block'});
				},
				function(){
					$(this).css({display: 'none'}),
					$("#lineTabs li > div").removeClass("active");
				});






				if ((scrollfromtop < 205) && ($('.index-navigation').width() > 720))
					{
						$("#tab"+tab_id).css({position: 'absolute', top: 70});
					}

				else if (($('.index-navigation').width() > 480) && ($('.index-navigation').width() < 721))
					{
						$("#tab"+tab_id).css({
							position: 'absolute',
							top: 141
					});
					}

				else if ($('.index-navigation').width() < 481)
					{
						$("#tab"+tab_id).css({
							position: 'fixed',
							'top': 0
					});
					}


				else if ((scrollfromtop > 205) && ($('.index-navigation').width() > 720) && ($('.index-navigation').width() < 1199))
					{
						$("#tab"+tab_id).css({position: 'absolute', top: scrollfromtop - 95});
					}

				else if ((scrollfromtop > 205) && ($('.index-navigation').width() > 1199))
					{
						$("#tab"+tab_id).css({position: 'absolute', top: scrollfromtop - 85});
					}



				        }, 500);



			}
			},function()
				{
					var tab_id=i+1;

					if  ($("#tab"+tab_id).css('display') !== 'none'){
				   		$("#tab"+tab_id).mouseenter(
						function(){
							$(this).css({display: 'block'});
							in_menu	= true;
						});
						$("#tab-content").mouseleave(
						function(){
							$("#tab1").css({display: 'none'}),
							$("#lineTabs li > div").removeClass("active");
							clearInterval(intervalID);
							in_menu	= false;
						});
				    	}
					else{
						$("#tab1").css({display: 'none'}),
						$("#lineTabs li > div").removeClass("active");
						clearInterval(intervalID);
						in_menu	= false;
					}
				});
		});


		$('#lineTabs a').click(function () {
			$('body,html').animate({
				scrollTop: 11
			}, 300);
			return false;
		});
		$("#contentTabs li a").each(function (i) {
			$("#contentTabs li a:eq("+i+")").click(function(){
				var contTab_id=i+1;
				$("#contentTabs li").removeClass("active");
				$(this).parent("li").addClass("active");
				$("#contentTabs-container > div").stop(false,false).hide();
				$("#contentTab"+contTab_id).stop(false,false).show();
				return false;
			})
		});
		$('#tab1').collapsible();
		$("#list-view-trigger").click(function(){
			$("div.items").removeClass("tiles-view");
			$("div.items").addClass("list-view");
			$("#list-view-trigger").css({opacity: '1'});
			$("#tiles-view-trigger").css({opacity: '0.5'});
		});
		$("#tiles-view-trigger").click(function(){
			$("div.items").removeClass("list-view");
			$("div.items").addClass("tiles-view");
			$("#tiles-view-trigger").css({opacity: '1'});
			$("#list-view-trigger").css({opacity: '0.5'});
		});

	});

	    $(document).ready(function(){
	        PopUpHide();
	    });
	    function PopUpShow(){
	        $("#popup1").show();
	    }
	    function PopUpHide(){
	        $("#popup1").hide();
	    }
	    function TabHide(){
	        $("#tab-content > div").hide();
			$("#lineTabs li > div").removeClass("active");
	    }

	$(function(){
	    $(window).scroll(function() {
	        var top = $(document).scrollTop();
			if (top < 205)
				{
