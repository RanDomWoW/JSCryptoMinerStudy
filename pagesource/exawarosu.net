$(function() {

	$(".scroll, .article-comment > a, .comment-reply > a").on("click",function(event){
		event.preventDefault();
		
		var elmID = $(this).attr("href");
		var posi = 0;
        
		if($(elmID).size()){
			posi = $(elmID).offset().top;
			bodySlider(posi);
			return false;
		}
		
	});

	function bodySlider(HashOffset){
		$("html,body").animate({
			scrollTop: HashOffset
		}, 700);
	}

});

