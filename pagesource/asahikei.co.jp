<script type="text/javascript">
$(function(){
	
	
	// 広告設定
	var server = getServerInfo();
	viewAd(
		 4					// 非表示期間(時間)
		,"2018/2/27 03:00"	// 掲載開始日
		,"2018/3/2 23:59"	// 掲載終了日
		,server.year +"/"+ server.month +"/"+ server.day +" "+ server.hour +":"+ server.minute
		,function(){
			var adModal = createModalAction("#modal__ad");
			adModal.openModal();
		}
		//,{debug: true}
	);
	
	
	// メインビジュアル
	var slide_length = $(".control_item").length;
	if(slide_length > 1){
		
		var posCounter = 0;
		slideAutoScroll({
			 wait: 7000
			,run: function(){
				posCounter++;
				slide( posCounter % slide_length );
			}
			,hoverToReset: [
				"#main_visual"
			]
		});
		
		$(".control_item").mouseover(function(){
			var banner_index = $(".control_item").index(this);
			slide( banner_index );
		});
	}
	
	function slide( n ){
		
		setPosCount( n );
		
		// main
		$("#main_visual .slide").removeClass("is--select");
		$("#main_visual .slide:eq("+ n +")").addClass("is--select");
		
		// side
		$("#main_visual .control").removeClass("is--bg_position0");
		$("#main_visual .control").removeClass("is--bg_position1");
		$("#main_visual .control").removeClass("is--bg_position2");
		$("#main_visual .control").addClass("is--bg_position"+ n);
		
	}
	
	function setPosCount(n){
		try{
			posCounter = n;
		}catch(e){
			return false;
		}
	}
	
});
</script>
