    <script src="http://s7.addthis.com/js/250/addthis_widget.js"></script>
    <script src="/Scripts/jquery.infieldlabel.min.js"></script>
    <script src="/Scripts/selectbox.js"></script>
    <script src="/Scripts/jquery.fancybox.pack.js"></script>
    <script src="/Scripts/script.js"></script>
    <script src="/Scripts/selectivizr-min.js"></script>
    <script type="text/javascript" src="/scripts/AutoCompleteJqueryUi.min.js"></script>
    <script type="text/javascript" src="/scripts/jquery.quicksand.js"></script>
    <script>
        $(function () {

            var autocompleteData = {};

            function log(message) {
                $("<div/>").text(message).prependTo("#log");
                $("#log").scrollTop(0);
            }

            $(".titleTextBox").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: "/services/autocomplete.ashx",
                        dataType: "json",
                        data: {
                            featureClass: "P",
                            style: "full",
                            maxRows: 12,
                            name_startsWith: request.term,
                            query: request.term,
                            publisher: "HansReitzelsForlag"
                        },
                        success: function (data) {
                            autocompleteData = {};
                            $.each(data, function (i, v) {
                                autocompleteData[v.value] = v.isbn;
                            });

                            response(data);
                        }
                    });
                },
                minLength: 3,
                select: function (event, ui) {
                    //console.log(ui.item ? "Selected: " + ui.item.label : "Nothing selected, input was " + this.value);
                    $(this).closest('.bookFields').find('input.input_isbn').val(autocompleteData[ui.item.value]);
                    $(this).closest('.bookFields').find('input.input_isbn').siblings('label').hide();
                },
                open: function () {
                    $(this).removeClass("ui-corner-all").addClass("ui-corner-top");
                },
                close: function () {
                    $(this).removeClass("ui-corner-top").addClass("ui-corner-all");
                }
            });


            $('.input_isbn').live('keyup', function (e) {
                var $this = $(this);
                if (13 === $this.val().length && $this.data('isbn-last') !== $this.val()) { //magic number 13 is standard ISBN number length
                    $this.data('isbn-last', $this.val());
                    $.getJSON('/services/titlefromisbn.ashx', {
                        isbn: $this.val(),
                        publisher: "HansReitzelsForlag"
                    }, function (json) {
                        if (!!json && !!json.value) {
                            $this.closest('.bookFields').find('input.input_title').val(json.value);
                        }
                    });
                }
            });


        });
    </script>
