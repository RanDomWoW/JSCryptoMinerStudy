<script type="text/javascript">
	/* rebuild menu to separate categories and brands*/
	$('nav').select('li').each(function(element) {
		if($(element).hasClassName('parent')) {
			var brands = [];
			var categories = [];
			var ul = $(element).select('ul')[0];
			ul.select('li').each(function(elem) {
				if($(elem).readAttribute('type') == 'brand') {
					brands.push($(elem));
				} else {
					categories.push(elem);
				}
			});
			if(brands.length > 0 && categories.length > 0) {
				var classname = ul.readAttribute('class');
				var brandlist = document.createElement('ul');
				var catlist = document.createElement('ul');
				var res = '';
				
				var titlecat = document.createElement('li');
				titlecat.innerHTML = '<a><span> Kategorier</span></a>';
				titlecat.addClassName('title');
				catlist.appendChild(titlecat);
				
				var titlebr = document.createElement('li');
				titlebr.innerHTML = '<a><span>Brands</span></a>';
				titlebr.addClassName('title');
				brandlist.appendChild(titlebr);
				for(var i = 0; i < brands.length; i++) {
					brandlist.appendChild(brands[i]);
				}
				
				brandlist.addClassName(classname);
				brandlist.addClassName('two-lists');
				brandlist.addClassName('shown-sub');
				
				for(var i = 0; i < categories.length; i++) {
					catlist.appendChild(categories[i]);
				}
				catlist.addClassName(classname);
				catlist.addClassName('two-lists');
				catlist.addClassName('shown-sub');
				
				res = '<div class="'+classname+' withshadow">'+catlist.outerHTML + brandlist.outerHTML+'</div>';
				ul.replace(res);
			}
		}
	});

	Event.observe(window, 'scroll', function(e) {
		/*desktop menu */
		if(document.viewport.getWidth() > 900) {
			if (document.viewport.getScrollOffsets()['top'] > 60) {
				/*remove from mobile to desktop*/
				$$('.menu-right-wrapper')[0].removeClassName('follow-your-master');
				$$('.menu-right-wrapper')[0].removeClassName('menu-right-wrap-fixed');
				$$('.main-container')[0].removeClassName('run-from-master2');

				$$('.header-menu')[0].addClassName('follow-your-master');
				$$('.main-container')[0].addClassName('run-from-master');
				/*position ajax search*/
				$$('.autocomplete-w2')[0].addClassName('autocomplete-w2-fixed ');
				$$('.autocomplete')[0].addClassName('autocomplete-fix');
			} else {
				$$('.header-menu')[0].removeClassName('follow-your-master');
				$$('.main-container')[0].removeClassName('run-from-master');
				jQuery('.autocomplete-w2')[0].removeClassName('autocomplete-w2-fixed');
				jQuery('.autocomplete')[0].removeClassName('autocomplete-fix');
				/*remove classes when changing orientation*/
				$$('.menu-right-wrapper')[0].removeClassName('follow-your-master');
				$$('.menu-right-wrapper')[0].removeClassName('menu-right-wrap-fixed');
			}
		} 
		else {
			if (document.viewport.getScrollOffsets()['top'] > 80) { 
				/*make right menu follow on mobile devices */
				$$('.menu-right-wrapper')[0].addClassName('follow-your-master');
				$$('.menu-right-wrapper')[0].addClassName('menu-right-wrap-fixed');
				$$('.main-container')[0].addClassName('run-from-master2');
				/*position ajax search*/
				jQuery('.autocomplete-w2')[0].addClassName('autocomplete-w2-fixed');
				jQuery('.autocomplete')[0].addClassName('autocomplete-fix');
			} else {
				$$('.main-container')[0].removeClassName('run-from-master2');
				$$('.header-menu')[0].removeClassName('follow-your-master');
				$$('.main-container')[0].removeClassName('run-from-master');
				$$('.menu-right-wrapper')[0].removeClassName('follow-your-master');
				$$('.menu-right-wrapper')[0].removeClassName('menu-right-wrap-fixed');
				jQuery('.autocomplete-w2')[0].removeClassName('autocomplete-w2-fixed');
				jQuery('.autocomplete')[0].removeClassName('autocomplete-fix');
			}
		} 
	});

