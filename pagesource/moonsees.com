<script type="text/javascript">
    $(function () {
        //课程的卡片hover 激活,与数据初始化
        $(".course-reco .links .link").hover(function () {
            var that = $(this);
            if (that.hasClass("active")) {
                return;
            }
            var cid = that.data("cid");
            getChapterCards(cid);
            that.addClass("active");
            that.siblings().removeClass("active");
        });
        /*
         *获取章节
         */
        function getChapterCards(cid) {
            $.post("/college/getChaptCard", {"cid": cid, "limit": 8}, function (result) {
                if (result.data) {
                    $(".course-reco .course-list ul").html("");
                    $.each(result.data, function (index, value) {
                        var card = "<li class=\"card animated bounceIn\">";
                        card += "<a href='http://www.moonsees.com/college/chapter/id-" + value['id'] + "' target='_blank'>";
                        if (value['thumb']) {
                            card += "<img src=\"" + value['thumb'] + imageOpts(200,211)+"\">";
                        } else {
                            card += "<img src=\"http://www.moonsees.com/static/images/nopic_226x126.jpg\">";
                        }
                        card += "</a>";
                        card += "<span>" + subConent(value['title'], 14) + "</span>";
                        card += "</li>";
                        $(".course-reco .course-list ul").append(card);
                    });
                }
            }, 'json');
        }
    })
</script>
<script type="text/javascript">
    //    getArticleArr
    $(function () {
        //新闻的hover激活,与数据初始化
        $(".news-list .submenu li").hover(function () {
            var that = $(this);
            if (that.hasClass("active")) {
                return;
            }
            var cid = that.data("cid");
            getArticleArr(cid);
            that.addClass("active");
            that.siblings().removeClass("active");
        });
        /*
         *获取新闻
         */
        function getArticleArr(cid) {
            var _load = layer.load();
            $.post("/article/index/m-getArticleArr", {"cid": cid}, function (result) {
                layer.close(_load);
                if (result.data) {
                    $(".news-list .news-items").html("");
                    $.each(result.data, function (index, value) {
                        var _item = "<div class=\"item  animated bounceIn\">";
                        _item += "<div class=\"cover\">";
                        _item += "<a href='http://www.moonsees.com/article/show/id-" + value['articleid'] + "' target='_blank'>";
                        if (value['photo']) {
                            _item += "<img src=\"" + value['photo'] + imageOpts(270,150)+"\">";
                        } else {
                            _item += "<img src=\"http://www.moonsees.com/static/images/nopic_270x150.jpg\">";
                        }
                        _item += "</a></div>";
                        _item += " <div class=\"item-info\">";
                        _item += " <a target=\"_blank\" href='http://www.moonsees.com/article/show/id-" + value['articleid'] + "' class=\"title\">";
                        _item += " <h2>" + subConent(value['title'],55) + "</h2>";
                        _item += "</a>";
                        _item += " <a target=\"_blank\" href='http://www.moonsees.com/article/show/id-" + value['articleid'] + "' >";
                        _item += "<div class=\"desc\">";
                        _item += subConent(value['content'], 113);
                        _item += "</div>";
                        _item += "</a>";
                        _item += "<span class=\"time\">" + transTime(value['addtime']) + "</span>";
                        _item += "</div>";
                        _item += "<i class='clearfix'><i>";
                        $(".news-list .news-items").append(_item);
                    });
                }
            }, 'json');
        }
    });
</script>
<script type="text/javascript">
    $(function () {
        //搜索
        $(".searcher-wrap .search-btn").on('click',function () {
            var keywords = $.trim($(".searcher-wrap .search-value").val());
            if(keywords==""){
                layer.msg("搜索内容不能为空");
                return false;
            }
            var urlSafe = new URLSafe();
            var _reurl = siteUrl+"article/search?keywords="+urlSafe.base64_encode(keywords);
            window.open(_reurl);
            return false;
        });
        $('.searcher-wrap .search-value').bind('keyup', function(event) {
            if (event.keyCode == "13") {
                //回车执行查询
                $(".searcher-wrap .search-btn").click();
            }
        });
    });

</script>

