            auto:true,
            pager:false,
			pause: 11000
        });

        $( ".datepicker-area" ).datepicker({
            
           dateFormat: 'dd/mm/yy',
          changeMonth: true,
          changeYear: true,
          minDate: 0
        });
        $('.contentslider').bxSlider({
            mode: 'fade',
            auto:true,
            pager:true,
            auto:true
        });
		$('.nav .opener').click(function(){
            $(this).parent('li').toggleClass('menu-active');
			$(this).siblings('.dropdown-menu').slideToggle();
        });
		//$('a img').attr('title','');
		
		//$('.slider .bx-wrapper .bx-caption').wrap( '<a href="link"></a>' );
    });
</script>
<script type="text/javascript" src="http://www.nesmaairlines.com/assets/frontEnd/intTelInput/js/intlTelInput.min.js"></script>
<script type="text/javascript">
   jQuery(document).ready(function($) { 
	  var lang_base_url='http://www.nesmaairlines.com/';
					$(".phone").intlTelInput({
						//onlyCountries: ["sa", "eg"], initialCountry: "sa",
						initialCountry: "sa",
						excludeCountries: ["il"],
						nationalMode: false,
						separateDialCode: true,
						utilsScript: lang_base_url + "assets/frontEnd/intTelInput/js/utils.js" // just for formatting/placeholders etc
					});
		
					

						$(document).on('focusout', '.phone', function (event) {

							 var intlNumber = $(".phone").intlTelInput("getNumber");
							$('.intTelNo').val(intlNumber);
						});
				
					$(".phone").on("countrychange", function (e, countryData) {

						var intlNumber = $(".phone").intlTelInput("getNumber");
						$('.intTelNo').val(intlNumber);
					});
});
</script>
</body>
</html>