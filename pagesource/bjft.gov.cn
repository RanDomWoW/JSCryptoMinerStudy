<script>
		var speed=20
		var demo = document.getElementById("rolldemo");
		var demo1 = document.getElementById("rolldemo1");
		var demo2 = document.getElementById("rolldemo2");
		var demol = 63;
		demo2.innerHTML=demo1.innerHTML
		function Marquee(){
			//alert(demo2.offsetWidth)
		if(demo2.offsetWidth-demo.scrollLeft<=0){
				demo.scrollLeft-=demo1.offsetWidth
			}else{
				demo.scrollLeft++;
			}
			if(demo.scrollLeft%demol ==0){
					clearInterval(MyMar); 
					isMove=false;
					begindemo();
				}
		}
		var MyMar = null;
		var isMove = false;
		function begindemo(){
			setTimeout(function(){if(!isMove){MyMar=setInterval(Marquee,speed);isMove=true;}},2000);
		}
		demo.onmouseover=function() {clearInterval(MyMar);isMove=false;}
		demo.onmouseout=function() {if(demo.scrollLeft%demol ==0){clearInterval(MyMar);isMove=false;begindemo();}else{if(!isMove){MyMar=setInterval(Marquee,speed);isMove=true;}}}
		begindemo();
		</script>
