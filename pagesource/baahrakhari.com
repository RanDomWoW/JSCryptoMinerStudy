<script>
    var sheight = $(".small-ad>a>img").height();
    var lheight = $(".big-ad>a>img").height();

    $(document).ready(function () {

        $(".small-ad").css("top", lheight + 'px');
        $("#large-ad").css("height", lheight);
        $("#btn-collapse").click(function () {
            ad_collapse();
        });
        $("#btn-expand").click(function () {
            ad_expand();
        });
        setTimeout(ad_collapse, 4000);
    });

    function ad_collapse() {
        $("#large-ad").css("height", sheight);
        $(".small-ad").css("top", -lheight + 'px');
    }

    function ad_expand() {
        $("#large-ad").css("height", lheight);
        $(".small-ad").css("top", "0px");
    }
</script>



