<script src="//solutions.invocacdn.com/js/pnapi_integration-latest.min.js" type="text/javascript"></script>
<script type="text/javascript">
// Creates the default Invoca campaign ID if invsrc parameter is NOT in the URL
function createFallBackId(){
    
  //gather organic/direct entry method
  var invocaCampaignId = Invoca.Tools.parseReferrer({
    "google.com" : "efax_goog_organic",
    "bing.com" : "efax_bing_organic",
    "yahoo.com" : "efax_gemini_organic",
    "gemini.com" : "efax_gemini_organic",
    "efax.com" : "efax_referral", //Can augment this list to include any referrer : campaignID mapping
    "j2global.com" : "efax_referral",
    "j2.com" : "efax_referral",
    "evoice.com" : "efax_referral",
    "onebox.com" : "efax_referral",
    "efaxcorporate.com" : "efax_referral",
    "" : "efax_direct"
  }, "efax_direct");
  
  return invocaCampaignId;
 }
//Grab Google Analytics Visitor ID
function getClientId() {
  
  // If ga is defined, check for the clientId and return it
  if (typeof ga !== 'undefined' && typeof ga.getAll === 'function') {
  
    var clientId = ga.getAll()[0].get('clientId');
    return clientId;
  // otherwise, return false so waitFor knows to try again within the time threshold
  } else {
    return false;
  }
}
//Callback Function to show the tracking number after Invoca Code Executes
function showNumber(){
  jQuery(".invocaNumber").css('visibility','visible'); //Insert the span class of the number location here, assuming you are setting visibility:hidden as default
}

//Invoca Tracking Number Call
function callInvoca(cid){
  
  //Get Fallback Campaign ID
  var fallback = Invoca.Tools.readInvocaData('campId', createFallBackId());
  
  //Solutions JS Settings
  Invoca.PNAPI.integration({
    networkId : 1181,
    numberSelector : ".invocaNumber", //Insert the span class of the number location here
    campaignIdOverrideParam : "invsrc",
    defaultCampaignId : fallback, //if no campaign, will use defined fallback campaign id
    cookieDays : 30, //last touch attribution window
    savePoolParams : true,
    onComplete : showNumber,
    poolParams: {
      campId : fallback, //fallback campaign ID holder
      g_cid : cid, //GA Param
      referrer: document.referrer //stores referring web URL
    },
    cookieNames : ["vid", "source_id"] //All URL attributes are read in by default, this settings associates cookie values to the number presented with these names
  });
}

//Attempts to gather GA Visitor ID for 1000ms before running Invoca Function
Invoca.Tools.waitFor(getClientId, callInvoca, 'noCid', 1000);
</script>
