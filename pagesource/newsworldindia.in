   $.ajax({
     url: "//www.newsworldindia.in/post/likePost", 
     type: 'POST',
     data: {postid:postid,useragent:useragent,ip:ip},
   })
   .success(function(data) {
     $('.likecount'+id).html(data);
   });
 }
}


//Desktop Notification
function deskNotify(){ 
  $.ajax({
    url : '//www.newsworldindia.in/frontfooter/desktop_notification/',
    type: 'POST',
    data: $(this).serialize(),
    success: function(data)
    {
  	var deData = jQuery.parseJSON(data);

            $.each(deData, function( index, value )
            {           
              var cookie = getCookie('desk_notification'+value.ID);
              if(cookie != '' && cookie == value.ID){
                return false;   
              }else{
                var title=value.post_title;
                var desc='Latest News';
                var urls="http://newsworldindia.in/story/"+value.post_seo_url+"/"+value.ID+"/";
                notifyBrowser(title,desc,urls);
                createCookie('desk_notification'+value.ID,value.ID);
              }

            });      
        } // end success
    });
}


setInterval(function()
{
	deskNotify();
      //return false;
}, 60000);


function notifyBrowser(title,desc,urls) 
{
  if (!Notification) 
  {
    console.log('Desktop notifications not available in your browser..'); 
    return;
  }
  if (Notification.permission !== "granted")
  {
    Notification.requestPermission();
  }
  else 
  {
    var notification = new Notification(title, {
      icon:'http://newsworldindia.in/public/media_uploads/mediagqb4site_logo-nwi.png',
      body: desc,
    });
    // Remove the notification from Notification Center when clicked.
    notification.onclick = function () {
      window.open(urls);      
    };

    // Callback function when the notification is closed.
    notification.onclose = function () {
      console.log('Notification closed');
    };
  }
}

</script>
