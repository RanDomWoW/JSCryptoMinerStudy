     var count = $('.likecount'+id).val();
   var postid = id;
   var useragent = 'CCBot/2.0 (http://commoncrawl.org/faq/)';
  var cookie = getCookie('postliked'+id);
 if(cookie != '' && cookie == postid){
   alert('You have already liked this post');
    return false;   
 }
 else{
   createCookie('postliked'+id,postid);
