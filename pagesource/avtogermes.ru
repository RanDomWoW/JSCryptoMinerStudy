    /* Функции */
    function initModelList(elem, type, bid) {
        if (!elem || !bid) {
            var selectСontent = '<option value data-placeholder="true">Модель</option>'
            updateModelSelect(selectСontent)
            return false;
        }
        var url = '';
        if ('new' == type) {
            url = '/include/ajax/getcarfilterdata.php';
        }else
        if ('second' == type) {
            url = '/include/ajax/getsecondfilterdata.php';
        }else
        if (!url.length) {
            return false;
        }

        /* получаем изначальные значения фильтра цен, если левый или правый бегунки не трогались */
        var slider_min_price = $('input.slider_min_price').val();
        var slider_max_price = $('input.slider_max_price').val();
        if(slider_min_price == 0){
            slider_min_price = $('input.slider_min_price').attr('init_value');
        }
        if(slider_max_price == 0){
            slider_max_price = $('input.slider_max_price').attr('init_value');
        }

        var params = {
            url: url,
            data: {
                js: 1,
                action: 'getmodels',
                data: bid,
                price_from : slider_min_price,
                price_to: slider_max_price,
            },
            type: 'POST',
            dataType: 'json',
            complete : function() {
            },
            success: function(r) {
                if (r.error) {
                    return false;
                }
                var selectСontent = '<option value=""></option> <option value="default">Модель</option>' + r.data;
                updateModelSelect(selectСontent)
                return true;
            },
            error: function() {
                return false;
            }
        };

        $.ajax(params);
    }

    function updateModelSelect(options){
        model.html('')
        model.html(options);
        model.select2({
            minimumResultsForSearch: -1,
            width: '100%',
        })
    }

    /**
     *
     * @param price
     * @returns {string}
     */
    function formatPrice(price) {
        var num = price;
        num = num.toString().replace(/[^\d\+]/g, '');
        num = num.split(/(?=.)/);
        var i = num.length;
        if (8 <= i) {
            num.splice(2, 0, ' ');
            num.splice(6, 0, ' ');
        }
        else if (7 <= i) {
            num.splice(1, 0, ' ');
            num.splice(5, 0, ' ');
        }
        else if (6 <= i) {
            num.splice(3, 0, ' ');
        }
        else if (5 <= i) {
            num.splice(2, 0, ' ');
        }
        else if (4 <= i) {
            num.splice(1, 0, ' ');
        }
        return num.join('');
    }
    function initGermesSelect(elem, html) {
        elem.germesSelect('destroy');
        elem.html(html);
        var options = {};
        options.className = elem.attr('data-extraClasses') || '';
        elem.germesSelect(options);
    }

    function resetGermesSelect(elem, text) {
        var selectContent = '<option value data-placeholder="true">' + text + '</option>';
        initGermesSelect(elem, selectContent);
    }
