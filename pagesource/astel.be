	<script language="JavaScript" type="text/javascript">
	document.onmousemove = move;

	var i = false;

	function move(e) {
		if (i) {
			if (navigator.appName != "Microsoft Internet Explorer") {
				GetId("curseur").style.left = e.pageX + 5 + "px";
				GetId("curseur").style.top = e.pageY + 10 + "px";
			}
			else {
				if (document.documentElement.clientWidth > 0) {
					GetId("curseur").style.left = 20 + event.x + document.documentElement.scrollLeft + "px";
					GetId("curseur").style.top = 10 + event.y + document.documentElement.scrollTop + "px";
				}
				else {
					GetId("curseur").style.left = 20 + event.x + document.body.scrollLeft + "px";
					GetId("curseur").style.top = 10 + event.y + document.body.scrollTop + "px";
				}
			}
		}
	}

	function toggleMobileGroup() {
		console.log('toggleMobileGroup');
		var obj = document.getElementById('checkbox-gsm');

		if (obj.checked) {
			document.getElementById("divmobile").classList.remove('uncheckk');
			// select first element by default if none is selected
			if (typeof $("input[name=mob_usage]:checked").val() == 'undefined') {
				$("#mob_usage1").attr('checked', true);
			}

			var mobileIcon = document.querySelectorAll('#divmobile .comparator-param-icon');

			if (mobileIcon.length > 0) {
				mobileIcon[0].classList.add('on');
			}
		}
		else {
			document.getElementById("divmobile").classList.add('uncheckk');

			var mobileIcon = document.querySelectorAll('#divmobile .comparator-param-icon');

			if (mobileIcon.length > 0) {
				mobileIcon[0].classList.remove('on');
			}
		}
	}

	function setMobileGroup(state) {
		var obj = document.getElementById('checkbox-gsm');

		obj.checked = state;

