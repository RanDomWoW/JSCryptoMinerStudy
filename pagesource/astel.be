<script>
	document.onkeydown = function (event) {
		if (event.keyCode == 13) {
			gotoComparator();
		}
	};

	function gotoComparator() {
		// get form variables to compose comparator url
		var zip = document.getElementById('zip').value;
		var mobileUsage = $('#checkbox-gsm').is(':checked') ? $("input[name=mob_usage]:checked").val() : 0;
		var fixUsage = $('#checkbox-fixe').is(':checked') ? $("input[name=fix_usage]:checked").val() : 0;
		var internetUsage = $('#checkbox-internet').is(':checked') ? $("input[name=int_usage]:checked").val() : 0;
		var tvUsage = $('#checkbox-tv').is(':checked') ? 1 : 0;
		var tv = tvUsage > 0 ? 99 : 0;
		var clasQ = $("input[name=leclassement]:checked").val();
		// compose compartor url
		var url = '//www.astel.be/comparateur?code_postal=' + zip + '&mobile=' + mobileUsage + '&fixe=' + fixUsage + '&internet=' + internetUsage + '&tv=' + tv + '&nbrtv=' + tvUsage + '&clasQ=' + clasQ;
		// goto comparator
		window.location = url;
	}
</script>

