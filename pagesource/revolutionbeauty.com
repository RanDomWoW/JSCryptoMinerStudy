            });
        });
    });
    function animatePopup(obj)
    {
        $(obj).hide();
        var animationDelay = 10.00;
        var animationDuration = 2.00;
        var animationType = 0;
        var endHorizontalPosition = 0;
        var endVerticalPosition = 2
        var startClass = $('.start-location');
        var endClass = $('.end-location');
        var startStyle = 'position: fixed;z-index: inherit;width: inherit;margin-left:auto;margin-right:auto;background-color:inherit;border:inherit;';
        var endStyle = 'position: fixed;z-index: inherit;width: inherit;margin-left:auto;margin-right:auto;background-color:inherit;border:inherit;';

        if(animationType!= 1)
        {
            endStyle += 'transition: all ' + animationDuration + 's ease 0s;';
        }

        switch(endHorizontalPosition)
        {
            case 0:
                startStyle += 'left:-' + $(obj).outerWidth() +'px;';
                endStyle += 'left:0px;';
                break;
            case 1:
                startStyle += 'left:0px;right:0px;';
                endStyle += 'left:0px;right:0px;';
                break;
            case 2:
                startStyle += 'right:-' + $(obj).outerWidth() + 'px;';
                endStyle += 'right:0px;';
                break;
        }

        //  Vertical animation is only used with central horizontal alignment
        switch(endVerticalPosition)
        {
            case 0:
                if(endHorizontalPosition==1)
                {
                    startStyle += 'top:-' + $(obj).outerHeight() + 'px;';
                }
                else
                {
                    startStyle += 'top:0px;';
                }
                endStyle += 'top:0px;';
                break;
            case 1:
                startStyle += 'top:44%';
                endStyle += 'top:44%';
                break;
            case 2:
                if(endHorizontalPosition==1)
                {
                     startStyle += 'bottom: -' + $(obj).outerHeight() + 'px;';
                }
                else
                {
                    startStyle += 'bottom:0px;';
                }
                endStyle += 'bottom:0px;';
                break;
        }
        
        if(animationType==1)
        {
            $('#' + 'NavTop_EmailSubscriptionsPopup1_DivEndLocationClass').append('<style type="text/css">.email-subscription-popup .end-location{' + endStyle + '}.email-subscription-popup .start-location{' + endStyle + '}</style>');
        }
        else
        {
            $('#' + 'NavTop_EmailSubscriptionsPopup1_DivEndLocationClass').append('<style type="text/css">.email-subscription-popup .end-location{' + endStyle + '}.email-subscription-popup .start-location{' + startStyle + '}</style>');
        }

        setTimeout(function() {
            
            if(animationType==1)
            {
                $(obj).fadeIn();
            }
            else
            {
                $(obj).show().switchClass('start-location', 'end-location', animationDuration);
            }
            
        }, animationDelay);
    }
    </script>
