    <script type="text/javascript">
        seajs.use(["jquery","plugin/picRoll"], function($)
        {
            /*如果是ipad，不要白色蒙层*/
            var regexiPad = new RegExp(/iPad/);
            if(!regexiPad.exec(navigator.userAgent)){
                $(".section-shadow").on("mouseenter",function(){
                    if($(this).hasClass("yohobuy-logo")) return;
                    $('<strong class="shadow"></strong>').appendTo($(this).find("a"));
                }).on("mouseleave",function(){
                    $(".shadow").remove();
                });
            }
           
            $.ajax
            ({
                type: "GET",
                url: "/default/default/getproduct",
                dataType: "json",
                success: function(response) 
                {
                    if (!response.status) {
                        return;
                    }
                    $("#yohobuy-img").attr("src", $("#yohobuy-img").attr("source"));
                    if (response.data.yohobuy.adCategory) {
                        $(".yohobuy-content").picRoll({datas:response.data.yohobuy.adCategory});
                    }
                    if (response.data.yohobuy.adActivity) {
                        $(".yohobuy-sale").picRoll({datas:response.data.yohobuy.adActivity});
                    }
                    if (response.data.yohoboys.newMagazine) {
                        $(".yohoboys-pic").picRoll({datas:response.data.yohoboys.newMagazine});
                        $(".yohoboys-pic").find('a').attr('rel', 'nofollow');
                    }
                    if (response.data.yohoshow.hotTopicMaster) {
                        $(".yohoshow-pic").picRoll({datas:response.data.yohoshow.hotTopicMaster});
                    }
                    if (response.data.yohoshow.recomTagBrand) {
                        $(".yhsj-pic").picRoll({datas:response.data.yohoshow.recomTagBrand});
                    }
                    if (response.data.yohogirls.newMagazine) {
                        $(".yohogirls-pic").picRoll({datas:response.data.yohogirls.newMagazine});
                        $(".yohogirls-pic").find('a').attr('rel', 'nofollow');
                    }
                }
            });
           
        });
    </script>
    <script>
