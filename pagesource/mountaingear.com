node.ready(function() {
function runRecommendationActivity() {
var anchor = jQuery('div.product-tile[data-recommendername] a');
if (!anchor.length) {
setTimeout(runRecommendationActivity, 100);
} else {
anchor.click(function(evt){
var reco=jQuery(this).parents('.product-tile');
var recommenderName = jQuery(this).parents('.product-tile[data-recommendername]').attr('data-recommendername');
localStorage.setItem('cq.recommenderName', recommenderName);
var anchors = jQuery('span[class="productname"]').html() || '';
localStorage.setItem('cq.anchors', anchors);
});
}
}
runRecommendationActivity();
});
clickThruAfter();
});
/* global CQuotient */
function clickThruAfter() {
var recommenderName = localStorage.getItem('cq.recommenderName');
var currentProductId = jQuery('span[class="productname"]').html() || '';
if (!recommenderName) {return;}
var anchors;
if (localStorage.getItem('cq.anchors')) {
anchors = localStorage.getItem('cq.anchors');
localStorage.removeItem('cq.anchors');
}
localStorage.removeItem('cq.recommenderName');
// do not send to CQ if anchors is not set, i.e. not coming from a Product page
if (window.CQuotient && anchors) {
CQuotient.activities.push({
activityType: 'clickReco',
parameters: {
cookieId: CQuotient.getCQCookieId(),
userId: CQuotient.getCQUserId(),
recommenderName: recommenderName,
anchors: anchors,
products: {
id: currentProductId
}
}
});
}
}
</script>
