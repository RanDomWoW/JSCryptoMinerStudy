	<script type="text/javascript">
		$(window).load(function (){
			var topMenu = function(c){
				var
					_this		= this,
					$menus 		= $(c);
				_this.mouseoverEvent = function(e){
						e.preventDefault();
						var 
							$this = $(this),
							d = (e.data);
						//console.log(d.to_desc[0],d.to_img[0]);
						$(d.to_img).attr('src','http://www.metrogas.cl/themes/hogar/menu-administrador/images/'+d.img)
						$(d.to_desc).text(d.desc);
					};
				$menus.each(function (i,item){
					var
						$item 	= $(item),
						$p 		= $item.parents('.js-menu-item');

					$item.bind(
						'mouseover',
						{
							to_img: $p.find('.js-menu-img'),
							to_desc: $p.find('.js-menu-desc'),
							desc: $item.attr('data-desc'),
							img: $item.attr('data-img')
						}, 
						_this.mouseoverEvent
					);
				});
				var $active = $menus.filter(function(index) { return ($(this).attr('data-idn') == window.menuIdn); });
				$active.parents('.js-menu-item').addClass('on');
			};
			var myMenu = new topMenu('.js-menu');
		});
	</script>
