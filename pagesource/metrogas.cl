	var someTabs = function(f,u){

	var 

	_this = this,

	$tabs = $(f),

	$contentTabs = $(u.tabs);

	_this.tabClick = function (e){

	//alert(e);

	e.preventDefault();

	var $thisTab = $(this);

	$tabs.removeClass('active')

	$thisTab.addClass('active');

	$contentTabs.removeClass('active');

	$actual = $contentTabs.filter(function(index) {

	return ($(this).data('tabcontent') == $thisTab.data('tab'));

	});

	$actual.addClass('active');

	if ($thisTab.data('tab') == 'payment' || $thisTab.data('tab') == 'payment2') {

	$exp = $tabs.filter(function(index) {

	return ($(this).data('tab') == 'payment');

	});

	$exp.addClass('active');

	}

	};

	$tabs.each(function (i,item){

	$item = $(item);

	$item.bind('click', {}, _this.tabClick);

	});

	$tabs.eq(u.first).trigger('click');

	};

	var hometabs = new someTabs('.js-tab',{tabs: '.js-tab-content', first: 0});



	var homeSlide = function(k,o){

	var

	_this 	 	= this,

	$pics 	 	= $(k),

	actual	 	= o.first || 0,

	$crtlHolder 	= $(o.controls),

	$controls 	= null,

	$contents 	= $('.js-slides-contents').children(),

	timer 	= setInterval(function (){_this.dance(actual);}, o.timeInterval);

	

	_this.stepClick = function (e){

	e.preventDefault();

	var

	$this 	= $(this),

	d 	= $this.data(),

	point 	= $controls.index($this);

	_this.movement(point);

	};

	_this.dance = function (h){

	_this.movement(((h+1) >= $controls.length) ? 0 : (h+1));

	};

	_this.movement = function(c){

	//$(element).is(":visible")

	var  

	$active = $controls.filter(function (index){

	return $(this).hasClass('active');

	});

	$next = $controls.eq(c);



	if ($active.length == 0) {

	$active = $controls.eq(((c-1)<= 0)?($controls.length-1):(c-1));

	}



	$($active.data('img')).fadeOut();

	$($active.data('content')).fadeOut();



	$($next.data('img')).fadeIn();

	$($next.data('content')).fadeIn();



