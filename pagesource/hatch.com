                var href = location.href.replace(/\/(en|fr-CA)\//i, "/" + lang + "/");

                if (href.indexOf(lang) === -1) {
                    if(lang == "en" && "True" == "False"){                        
                        location.href = "/" + lang + location.pathname;
                    }else{
                        location.pathname = "/" + lang;
                    }
                } else {                    
                    if(lang == "fr-CA"){
                        location.href = "/" + lang;
                    }else{
                        location.href = href;
                    }
                }
            }
        })
    }

