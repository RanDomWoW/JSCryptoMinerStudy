        function accordionTemplate(heading, groupId, parentId, active) {
            return '<div class="panel panel-default accordion-panel">' +
                ' <div class="panel-click ' + (active ? 'panelIsActive' : '') + ' " style="cursor:pointer">' +
                '   <div class="panel-heading">' +
