                '   <div id="' + groupId + '" class="panel-collapse collapse ' + (active ? 'in' : '') + '">' +
                '       <div class="panel-body js-tabcollapse-panel-body">' +
                '       </div>' +
                '   </div>' +
                '</div>';
        }
    })
    /* Manual Trigger Bootstrap Accordian Collapse from Panel Heading */
    var handled = false;

    function triggerBsCollapse(el) {
        if (handled == false) {
            handled = true;
            $(el).find("[data-toggle]").trigger("click");

            /* Set Filter Icon Defaults*/
            $(el).find(".plus-status-icon").css("display", "none");
            $(el).find(".minus-status-icon").css("display", "inline");

            /* Overide Filter Icon Defaults with Collapsed Filter Status */
            $(".collapsed").each(function() {
                var collapsedPanelHeading = $(this).parents(".panel-heading");
                $(collapsedPanelHeading).find(".plus-status-icon").css("display", "inline");
                $(collapsedPanelHeading).find(".minus-status-icon").css("display", "none");
            });
        }
        handled = false;
        equalProductNamesHeights();
        return true;
    }

    /* Handle Accordion Panel Heading Click Event */
    $(document).on("click", $(".panel-click"), function(e) {
        var el = e.target;

        if (!$(el).hasClass(".panel-click")) {
            el = $(el).parents(".panel-click");
        }
        triggerBsCollapse(el);
    });

    /* Google Analytics - Tag Product Impresssions from Active Tab on Page Load */    
    var target = $('a[data-toggle="tab"].active').attr("href");
    $(target + " [data-ga-product]").each(function (i, obj) {
        $(obj).attr("data-ga-product", "seen");
    });        

    /* Google Analytics - Tag Product Impresssions As Seen on tab click */
    $('a[data-toggle="tab"]').on('shown.bs.tab click', function (e) {
        var target = $(e.target).attr("href");
        $(target + " [data-ga-product]").each(function (i, obj) {
            $(obj).attr("data-ga-product", "seen");
        });
        window.eec.collatePage();
    });
</script>
