		<div class="blocktitle"><strong><a href="#" class="signin" style="color: #fff;"><span>Sign in</span></a></strong></div>
    <fieldset id="signin_menu">
        <form method="post" id="signin" action="/login">
          <p>
              <input type="text" name="data[Users][email]" value="E-mail / Username" class="logininputtext" onfocus="clearText(this)"  onblur="clearText(this)" maxlength="128"/>
          </p>
          <p>
            <input id="password-clear" type="text" value="Password" autocomplete="off" class="logininputtext" maxlength="128" />
            <input id="password-password" type="password" name="data[Users][password]" value="" autocomplete="off" class="logininputtext" maxlength="128" />
          </p>
          <p class="remember">
            <input type="hidden" name="lang" value="en"/>
            <a href="/signup/forgetpassword?lang=en" class="small">Forgot your password</a>
            <input type="image" src= "/images/login.png" class="floatright"/>
          </p>
        </form>
      </fieldset>
      <script src="/js/jquery.js" type="text/javascript"></script>
    <script type="text/javascript">
        $login = jQuery.noConflict();
        $login(document).ready(function() {

            $login(".signin").click(function(e) {          
				e.preventDefault();
                $login("fieldset#signin_menu").toggle();
				$login(".signin").toggleClass("menu-open");
            });
			
			$login("fieldset#signin_menu").mouseup(function() {
				return false
			});
			$login(document).mouseup(function(e) {
				if($login(e.target).parent("a.signin").length==0) {
					$login(".signin").removeClass("menu-open");
					$login("fieldset#signin_menu").hide();
				}
			});			
			
        });
    </script>
