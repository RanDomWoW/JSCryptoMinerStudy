    <script>
        var currentPageLanguage = 'fi';

        // map bloodhound remotely to search
        var pages = new Bloodhound({
            datumTokenizer: Bloodhound.tokenizers.obj.whitespace('value'),
            queryTokenizer: Bloodhound.tokenizers.whitespace,
            remote: {
                url: '/data/search/?query=%QUERY&lang=fi',
                wildcard: '%QUERY'
            },
            limit: Infinity
        });

        function openLink(obj, datum) {
            window.location.href = datum.url;
        }

        // wire up the search with autocomplete
        $('.typeahead').typeahead(null,
        {
            limit: Infinity,
            name: 'pages',
            display: 'value',
            valueKey: 'url',
            source: pages,
            templates: {
                empty: [
                    '<div class="empty-message">',
                    'Haulle ei l&#246;ytynyt tuloksia',
                    '</div>'
                ].join('\n'),
                suggestion: Handlebars.compile(
                    '<a class="typeahead-item" href="{{url}}">{{name}}</div>'
                )
            }
        }).bind('typeahead:selected', function (obj, datum) {
            openLink(obj, datum);
        });

        // lets not compete with the urls, we let the selection event take care of opening pages to support keyboard as well etc.
        $(document).on("click", ".typeahead-item", function (ev) {
            ev.preventDefault();
        });
    </script>
    <script>
        var lastScrollPos;
        var chatWasVisible = false;

        $('#main-nav').on('hide.bs.collapse', function () {
            $("#wrapper").toggleClass("active");
            $(document).scrollTop(lastScrollPos);
            $("body").css({ overflowY: '' });


            $(".giosg_chat_now").show();
            if (chatWasVisible) {
                $("#giosg_live_chat_dialog").show();
            }
        });

        $('#main-nav').on('show.bs.collapse', function () {
            lastScrollPos = $(document).scrollTop();
            $("#wrapper").toggleClass("active");
            $("body").css({ overflowY: 'scroll' });
            $(document).scrollTop(0);

            $(".giosg_chat_now").hide();
            var $chatWindow = $("#giosg_live_chat_dialog");
            if ($chatWindow.is(":visible")) {
                chatWasVisible = true;
                $chatWindow.hide();
            }
        });

        $(".EPiServerForms").on("reset", function () {
            setTimeout(function () {
                $(this).find(".range-input").trigger("change");
            }.bind(this));
        });

        $(".range-input").on("input change propertychange", function () {
            var el = $(this);
            var maxVal = el.attr('max');
            var val = el.val();

            var point = ((val / maxVal) * 100);
            point += "%";

            // negate increasing number of characters in value with margin
            var charOffset = (Math.pow(val.length * -1, 3) / 2);
            charOffset += "px";

            var output = el.prev(".range-output");

            var span = $("<span/>");
            span.html(output.find("span").html());

            output.html(el.val() + " ");
            output.append(span);

            output.css({ left: point, marginLeft: charOffset });
        }).trigger('input');

        $(".pagelink").click(function (e) {
            $("#main-nav").collapse('hide');
        });

        $(".dropdown a:not('.dropdown-toggle, .pagelink')").click(function (e) {
            $('.navbar-toggle').click();
        });
    </script>
