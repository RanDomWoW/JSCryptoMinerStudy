<script>
    $(function () {
        var $city = $("#City");
        var $area = $("#Area");

        $city.change(function() {
            $area.empty().prop('disabled', 'disabled');

            var url = '/Data/AreasForCity' + '?city=' + encodeURI($(this).find("option:selected").text()) + '&selectionValue=0' + '&lang=fi';
            $.get(url)
                .fail(function(failure) { alert("Failed: " + failure.responseText); })
                .done(function(data) {

                    $.map(data, function(val, i) {
                        var option = $("<option/>", {
                            value: val.Value,
                            text: val.Text
                        })
                        .prop('selected', i === 0);
                        option.appendTo($area);
                    });

                    $area.prop('disabled', false);
                }.bind(this));
        }); //.trigger("change");
    });
</script>

