    <script>
        // Can also be used with $(document).ready()
        $(window).load(function () {
            $('.full-flexslider').flexslider({
                animation: "fade",
                directionNav: false,
                slideshow: true,
                touch: false,
                keyboard: false,
                useCSS: false,
                start: function (slider) { // Fires when the slider loads the first slide
                    $(slider)
                      .find('img.lazy:eq(0)')
                      .each(function () {
                          var src = $(this).attr('data-src');
                          $(this).attr('src', src).removeAttr('data-src');
                      });
                },
                before: function (slider) { // Fires asynchronously with each slider animation
                    var slides = slider.slides,
                        index = slider.animatingTo,
                        $slide = $(slides[index]),
                        current = index,
                        nextSlide = current + 1,
                        prevSlide = current - 1;

                    $slide
                      .parent()
                      .find('img.lazy:eq(' + current + '), img.lazy:eq(' + nextSlide + '), img.lazy:eq(' + prevSlide + ')')
                      .each(function () {
                          var src = $(this).attr('data-src');
                          $(this).attr('src', src).removeAttr('data-src');
                      });
                }
            });
        });
    </script>

