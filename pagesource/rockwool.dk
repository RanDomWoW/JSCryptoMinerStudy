<script type="text/javascript">
/* Variables */
document.rwLabel = 'Jeg kommer som';
document.rwBusinessLabel = 'Professionel';
document.rwPrivateLabel = 'Privat';
document.rwDesc = 'Vi ønsker, at du skal få en endnu bedre oplevelse på vores hjemmeside. Derfor vil vi gerne vide, i hvilket ærinde du besøger vores hjemmeside. Tak fordi du hjælper os med at give dig en mere målrettet service.';

/* Functions */
function rwSetCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+ d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}

function rwGetCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}

function rwPopupSetChoice(choice) {
	//Save the choice in a cookie. If already avaliable, this will update/refresh it
	rwSetCookie("rwpopupchoice", choice, 365);
	
	if (typeof dataLayer !== 'undefined') {
		//Push for tracking
		dataLayer.push({
	        'event' : 'userSegment',
	        'eventCategory' : 'Segment pop-up',
	        'eventAction' : choice
	        });
	} else {
		//Error (debug)
		console.log('Error: dataLayer not initialized.');
	}
	
	if(document.getElementById('popup') !== null) {
		rwFade(false, true);
	}
	
	return false;
}

function rwFade(fadeIn, removeOnFadeOut) {
	if(fadeIn)
		document.rwEffectCounter = 0.1;
	else
		document.rwEffectCounter = 1;
	document.rwEffectContainer = setInterval(function() {
		document.getElementById('popup').style.opacity = document.rwEffectCounter;
		
		if(fadeIn)
			document.rwEffectCounter += 0.1; //= document.rwEffectCounter * 1.5;
		else
			document.rwEffectCounter -= 0.1;
		
		if((fadeIn && document.rwEffectCounter > 1) || (!fadeIn && document.rwEffectCounter < 0)) {
			if(fadeIn)
				document.getElementById('popup').style.opacity = 1;
			else {
				document.getElementById('popup').style.opacity = 0;
				if(removeOnFadeOut) {
					document.getElementById('popup').outerHTML = "";
				}
			}
			
			clearInterval(document.rwEffectContainer);
		}
	}, 50);
}

/* Popup */

var rwpopupchoice = rwGetCookie("rwpopupchoice");
if(rwpopupchoice != "") {
	/* A choice is registered */
	
	//Refresh choice
	rwPopupSetChoice(rwpopupchoice);
} else {
	/* No cookie = no prev choice = show popup */

	//Append HTML to body
	document.body.innerHTML += '<div id="popup"><div class="popup-content"><img src="https://static.rockwool.com/globalassets/dynamic/pressroom/logo/download/rockwool-logo---primary-colour-rgb.jpg" /><p style="margin-top: 0px">' + document.rwLabel + ':</p><p><a href="#" class="popup-btn business" onclick="return rwPopupSetChoice(\'Business\');">' + document.rwBusinessLabel + '</a><a href="#" class="popup-btn private" onclick="return rwPopupSetChoice(\'Private\');">' + document.rwPrivateLabel + '</a></p><p class="popup-desc">' + document.rwDesc + '</div></div><style>#popup {background:rgba(0, 0, 0, 0.75); position: fixed; top: 0; bottom: 0; right: 0; left: 0; z-index: 99999; opacity: 0;} #popup .popup-content {width: 600px; max-width: 80%; margin: 10% auto 0px; padding: 2% 4%; background: #FFF;-webkit-border-radius: 10px;-moz-border-radius: 10px;border-radius: 10px; text-align: center; font-size: 30px;} #popup img {width: 80%; max-width: 350px; height: auto; } .popup-btn {color: #FFF; -webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px; background: #919142;border: 1px solid #6d6d15; padding: 10px 15px; width: 125px; font-size: 20px; max-width: 40%; display: inline-block; margin: 5px 10px;} .popup-btn:hover {background: #6d6d15;border-color: #919142;} .popup-btn.private {background: #2ea8b2; border-color: #107881;} .popup-btn.private:hover {background: #107881; border-color: #2ea8b2;} .popup-desc {color: #979797; border-top: 1px solid #eee; padding: 10px 10px 0px 10px; font-size: 20px;} @media screen and (max-height: 420px), screen and (max-width: 420px) { .popup-desc { font-size: 14px; } #popup .popup-content {margin: 5% auto;font-size: 20px;} #popup img { max-height: 60px; width: auto;} .popup-btn { font-size: 16px; width: 100px;} }</style>';
	
	setTimeout(function() {
		rwFade(true, false);
	}, 500);	
}
