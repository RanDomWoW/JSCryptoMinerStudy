                </div>

                <div class="col-xs-6 fs-item-quantity">

                <div class="tsc-store store-qty-align pull-left">
                {{#iff quantity "<" 5 }}
                    <select class="chosen cart-quantity-chosen store-popup-qty-select fs-product-qty" data-path="{{path}}">
                        {{#select quantity}}
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                        <option value="5">5+</option>
                        {{/select}}
                    </select>
                {{ else }}
                    <input class="store-popup-qty-input t-store-popup-qty-input" inputmode="numeric" pattern="[0-9]*" id="qty-input-{{path}}" maxlength="3" value="{{quantity}}" data-path="{{path}}" {{#iff  attributes.maxQuantity '===' '1' }} disabled{{/iff}} />
                {{/iff}}
                </div>
                    <div class="fs-unit-price"  data-unit-price-value="{{unitPriceValue}}" >&#64;&nbsp;{{unitPrice}}</div>
                </div>
                <div class="col-xs-6 fs-item-total" >
                    <span class="fs-current-unit-price" data-fsc-item-path="{{path}}" data-fsc-item-total></span>
                </div>
                {{#each groups}}
                {{#iff type '===' 'config-many'}}
                {{#each items}}
                <div class="fs-maintenance-message text-center">
                   <div class="col-xs-12 fs-maintenance-checkbox">
                    {{#if selected }}
                        <input type="checkbox"  id="fs-single-product-maint" data-path="{{path}}" data-fsc-item-path-value="{{path}}" data-fsc-action="Remove" data-fsc-item-quantity-value="{{quantity}}" data-fsc-item-path="{{path}}"  {{checked selected}} /> <label for="fs-single-product-maint" >Add one year Maintenance -  <span class="fs-addon-unit-price" data-unit-price-value="{{unitPriceValue}}" >{{#currencyFormatter unitPriceValue ../../../../quantity }}{{/currencyFormatter}}</span></label>
                    {{ else }}
                        <input type="checkbox"  id="fs-single-product-maint" data-path="{{path}}" data-fsc-item-path-value="{{path}}" data-fsc-action="Update" data-fsc-item-quantity-value="{{quantity}}"  data-fsc-item-path="{{path}}"  {{checked selected}} /> <label for="fs-single-product-maint" >Add one year Maintenance -  <span class="fs-addon-unit-price" data-unit-price-value="{{unitPriceValue}}" >{{#currencyFormatter unitPriceValue ../../../../quantity }}{{/currencyFormatter}}</span></label>
                    {{/if}}
                    </div>
                    <div class="col-xs-12 fs-maintenance-list">
                        <ul>
