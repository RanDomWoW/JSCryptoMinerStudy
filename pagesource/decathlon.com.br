$(window).load(function(){
	$('.menu-search-input').on('input', function(event){
		if (!event.target.value) {
			this.firstLetter = false;
		}
	});
});
$(document).ready(function(){
	setTimeout(function(){
		$('form.search').removeAttr('onsubmit').off('submit').on('submit', function(event){
			event.preventDefault();
			var $this = $(this),
				$txt = $this.find('.tt-input'),
				val = $txt.val();
			val = val.replace(/[\\\/]/g, ' ').replace(/[\%]/g, '').trim();
			if (val !== $txt.attr('title') && val !== '') {
				if (window.filterStock === true) {
					window.location = acecIndexInformation.catalogDomain + 'search/'+encodeURIComponent(val) + (val.indexOf('?') > -1 ? '&stock=1' : '?stock=1');
				} else {
					window.location = acecIndexInformation.catalogDomain + 'search/'+encodeURIComponent(val);
				}
				return false;
			}
			$txt.fadeOut(100).fadeIn(100).fadeOut(100).fadeIn(100);
		});
	}, 200);
});
</script>


