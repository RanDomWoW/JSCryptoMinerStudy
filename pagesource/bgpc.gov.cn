	//设置首页的时间显示
	function setDateTime(){
		//获取系统时间
		var dateTime = '';
		var date = new Date();
		var Year = Mon = Day = '';
		var Hour = Min = Sec = '';
		var year = date.getFullYear();
		var month= date.getMonth()+1;
		var currDay = date.getDate();
		var hour    = date.getHours(); 
		var minutes = date.getMinutes();        
		var seconds = date.getSeconds();
		var week = date.getDay(),Week = '';
		year  <= 9 ? (Year = "0" + hour) : (Year = year);
		month <= 9 ? (Mon  = "0" + month) : (Mon = month);
		currDay <= 9 ? (Day  = "0" + currDay) : (Day = currDay);
		hour	<= 9 ? (Hour = "0" + hour) : (Hour = hour);
		minutes <= 9 ? (Min  = "0" + minutes) : (Min = minutes);
		seconds <= 9 ? (Sec  = "0" + seconds) : (Sec = seconds);       
		switch( week ){         
		   case 1 : Week="星期一";break;    
		   case 2 : Week="星期二";break;      
		   case 3 : Week="星期三";break;     
		   case 4 : Week="星期四";break;     
		   case 5 : Week="星期五";break;     
		   case 6 : Week="星期六";break;    
		   case 0 : Week="星期日";break;     
		}
		dateTime += Year+"年"+Mon+"月"+Day+"日 "+Hour+":"+ Min+":"+Sec+' '+Week;
		$('#dateTime').html( dateTime );
		setTimeout('setDateTime()',1000);
	}
	//滚动显示通知
	function scrollNews(){
		var currentEle = $('#scrollNews .show-news').next();
		currentEle = currentEle.length ? currentEle : $('#scrollNews li').first();
		$('#scrollNews').find('li').each(function(){
			$(this).removeClass('show-news');
		});
		currentEle.addClass('show-news');
		setTimeout(scrollNews,5000);
	}
	
