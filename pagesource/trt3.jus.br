			function onPaste(event){ 
				var clipboardData, pastedData, firstInput, secondInput, thirdInput, fourthInput, fifthInput;
	
					// Get pasted data via clipboard API
				clipboardData = event.clipboardData || window.clipboardData;
				pastedData = clipboardData.getData('Text').trim().replace(/-/g, "");
				var target = event.target || event.srcElement;

				if(target.id.toString()=="NUMPROPF"){

					pastedData = pastedData.substr(0,12)+ pastedData.substr(14,3);
					firstInput  =  pastedData.substr(0,5);//5
					secondInput =  pastedData.substr(5,4);//4
					thirdInput  =  pastedData.substr(9,3);//3
					fourthInput  =  pastedData.substr(12,2);//2
					fifthInput  =  pastedData.substr(14,1);//1
					$("#NUMPROPF").val(firstInput);
					$("#ANOPRO").val(secondInput);
					$("#CODJUN").val(thirdInput);
					$("#SEQPRO").val(fourthInput);
					$("#DIGVERPRO").val(fifthInput);
				}
				
				if(target.id=="NUMPRO"){

					pastedData = pastedData.substr(0,13)+ pastedData.substr(16,4);
					firstInput  =  pastedData.substr(0,7);//7
					secondInput =  pastedData.substr(7,2);//2
					thirdInput  =  pastedData.substr(9,4);//4
					fourthInput  =  pastedData.substr(13,4);//4
					$("#NUMPRO").val(firstInput); //
					$("#DIGVERPROCNJ").val(secondInput);
					$("#ANOPROCNJ").val(thirdInput);
					$("#CODJUNCNJ").val(fourthInput);
				}
        }	
		  
        </script>
