    <script>
  function updateMotivator(cart, callback){
    if(!window.shappify_motivator_goals || !shappify_motivator_goals.length) {
      if(typeof callback === 'function')
        callback(cart);
      return;
    }
    var motivatorUpdater = '';
    var needsUpdate = false;
    var messages = [];

    for(var i=0; i<shappify_motivator_goals.length; i++){
      var goal = shappify_motivator_goals[i];


      var foundInCart = false;
      var isGoalMet = (cart.total_price >= (goal.goal * 100) ? 1 : 0);
      //Does the shopper qualify for this goal?
      if (goal.variant_id){
        if(motivatorUpdater) motivatorUpdater += '&';
        motivatorUpdater += 'updates[' + goal.variant_id + ']=' + isGoalMet;
        for(var item in cart.items){
          if(cart.items[item].id == goal.variant_id){
            foundInCart = true;
            if(cart.items[item].quantity != isGoalMet) needsUpdate = true;
          }
        }

      }
      if(!foundInCart && isGoalMet) {
          messages.push("<span class='shapp_confirmation'>" + goal.confirmation + "</span>");
            //don't update if its a shipping goal
          if(goal.variant_id) needsUpdate = true;
      }

      //Do we actually need to update based on user's qualifications?


      if(typeof setCookie === 'function'){
      setCookie('motivator_confirmed_' + goal.id,isGoalMet, 24);
      setCookie('motivator_last_total',cart.total_price, 24);
      }
    }

    if(needsUpdate)  {
      jQuery.ajax({ success:callback, url:'/cart/update.js', type:'post', dataType:'json', data:motivatorUpdater });

    }
    else if(typeof callback === 'function')
      callback(cart);

    if(typeof show_messages === 'function')
        show_messages(messages);
  }




  //Bold: Update to make sure Motivator runs before hitting checkout
  jQuery(document).on('click', '[action*="/cart"]:not([action*="/cart/add"]) [type="submit"]', function(){

    if(typeof updateMotivator === 'function' && window.shappify_motivator_goals){
      var form = this.form;
      jQuery(form).append('<input type="hidden" name="' + this.getAttribute('name') + '" value="' + this.getAttribute('value') + '" />');

      jQuery.ajax({
        type:'post', 
        url:'/cart/update.js', 
        dataType:'json', 
        data:jQuery(form).serialize(), 
        success: function(cart){
          updateMotivator(cart, function(){ 
            jQuery('[name*=updates]', form).removeAttr('name'); 
            form.submit();
          })
        }
      });

      return false;
    }

