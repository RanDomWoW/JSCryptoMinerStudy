					type: "POST",
					url: "http://www.iapps.im/ajax/GetMsgByFriendId/",
					cache: false,
					dataType: 'json',
					data: {
						lastid: LAST_CHAT_ID,
						friendid: $("#chat #to_uid").val()
					},
					success: function(msg){
						var html = '';
						var msgs = msg.msgs;
						var l_t = $("#chat .row:last").attr('sendtime');

						if(!LAST_CHAT_ID && !msgs.length) {
							$("#chat-loading").html("无更多聊天记录");
						} else {
							$("#chat-loading").remove();
						}

