							CHAT_FLAG = 1;

							if(msgs[i].fromuid == user_id) {
								who = 'self';
								align = ' col-lg-offset-3';
							}

							if(message.length <= 12) {
								if(msgs[i].fromuid == user_id) {
									who = 'self';
									col = 'col-lg-5';
									align = ' col-lg-offset-7';
								} else {
									col = 'col-lg-5';
								}
							} else if(message.length > 12 && message.length <= 17 ) {
								if(msgs[i].fromuid == user_id) {
									who = 'self';
									col = 'col-lg-7';
									align = ' col-lg-offset-5';
								} else {
									col = 'col-lg-7';
								}
							} else {
								if(msgs[i].fromuid == user_id) {
									who = 'self';
									col = 'col-lg-9';
									align = ' col-lg-offset-3';
								} else {
									col = 'col-lg-9';
								}
