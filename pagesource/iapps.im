    <!-- 实名认证 -->
    <script>
    $(function() {
    	var uid = '0';
    	var mobile = '';
    	var seconds = 60;
    	var timer;
    	var objres = $('#verify-result');

    	var verifyCodeBtn = $('#verify-code-btn');
    	verifyCodeBtn.click(function(){
    		mobile = $('#verify-mobile').val();
    		if(mobile == '' || !/1\d{10}/.test(mobile)){
    			objres.html('请输入正确的手机号码');
    			return;
    		}
    		verifyCodeBtn.attr('disabled', 'disabled');
    		$.ajax({
    			url: 'http://bbs.iapps.im/plugin.php?id=smstong:iapps_verify&action=getverifycode',
    			type: 'post',
    			dataType: 'jsonp',
    			cache: false,
    			data: {mobile:mobile, 'uid':uid},
    			success: function(ret){
    				if(ret.errcode < 0){
    					alert(ret.errmsg);
    					verifyCodeBtn.removeAttr('disabled');
    					return;
    				}
					timer = setInterval(countdown, 1000);
    			},
    			error: function(){
    				objres.html('获取验证码失败，请稍后重试');
    				verifyCodeBtn.removeAttr('disabled');
    			}
    		});
    	});

    	function countdown(){
    		seconds--;
    		verifyCodeBtn.html(seconds+' 秒后重试');
    		if(seconds <= 0){
    			clearInterval(timer);
    			seconds = 60;
    			verifyCodeBtn.html('获取验证码').removeAttr('disabled');
    		}
    	}

    	$('#verify-btn').click(function(){
    		var mobile = $('#verify-mobile').val();
    		var code = $('#verify-code').val();
    		if(mobile == '' || !/1\d{10}/.test(mobile)){
    			objres.html('请输入正确的手机号码');
    			return;
    		}
    		if(code == '' || !/^\d+$/.test(code)){
    			objres.html('请输入收到的短信验证码');
    			return;
    		}
    		$.ajax({
    			url: 'http://bbs.iapps.im/plugin.php?id=smstong:iapps_verify&action=checkverifycode',
    			cache: false,
    			type: 'post',
    			dataType: 'jsonp',
    			data: {mobile:mobile, code:code, uid:uid},
    			success: function(res){
    				if(res.errcode < 0){
    					objres.html('实名认证失败，请检查短信验证码是否填写正确');
    					return;
    				}
    				$('#verifyDialog').modal('hide');
    			}
    		});
    	});
    });
    </script>



