							var user_id = 0;
							var who = 'target';
							var col = '';
							var align = '';
							var nickname = msgs[i].fromname;
							var message = msgs[i].msg;
							var s_time = '';
							var last_time = '';

							//判断时间

							var interval = 0;
							var showtime = false;

							if(LAST_CHAT_TIME == 0) {
								showtime = true;
							} else {
								interval = msgs[i].sendtime - LAST_CHAT_TIME;
								if(interval > 600) {
									showtime = true;
								}
							}

							if(showtime == true) {
								var d = new Date(msgs[i].sendtime * 1000);
								var s_time = (d.getMonth() + 1) + '-' + d.getDate() + ' ' + formatChatTime(d.getHours()) + ':' + formatChatTime(d.getMinutes());
								s_time = '<div class="sendtime">' + s_time + '</div>';
