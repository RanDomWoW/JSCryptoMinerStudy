					cache: false,
					data: {
							friendid: friendid,
							msg: chat_message
						  },
					success: function(msg){
						var data = JSON.parse(msg);
						if(data.code < 0) {
							console.log('sendmessage:' +data.msg);
							return;
						} else {
							$('textarea#chatMessage').val('');
							get_message(true);
							get_interval = setInterval(function(){get_message(false);}, 5000);
						}
						$("#chatButton").button('reset');
						$('textarea#chatMessage').focus();
					},
					error: function (xmlHttpRequest, error) {
        		$("#chatButton").button('error');
	        }
				});
				return false;
			});

		$('.recommend-btn').click(function() {
	      var $btn      = $(this),
	          $span     = $btn.children('span'),
	          oldval    = $span.html(),
	          commentid = $btn.attr('commentid'),
	          url       = _baseUrl + 'ajax/rating/';
	      $btn.unbind();
	      $.ajax({
	        url: url,
	        type: 'post',
	        dataType: 'json',
	        data: {
	        	articleid: _articleId,
	        	commentid: commentid
	        },
	        success: function(json) {
	          if(json.code == 0) {
	            $span.html(json.recommends);
	          } else {
	            $span.html(oldval);
	          }
	        },
	        error: function() {
	          $span.html(oldval);
	        }
	      });
  		});

		$('.overlookAll').live('click',function(){
			$.ajax({
				url:'/ajax/update_messenger_status',
				dataType: "json",
				success:function(data){
					if(data.code == 1) {
						$('.messenger-theme-future').hide();
					}
				}
			});
		});

		//------
		});
		</script>

