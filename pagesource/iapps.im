			function formatChatTime(t) {
				return (t > 9) ? t : '0'+t;
			}

			function get_message(f) {
				var CHAT_LAST_TIME = 0;
				var _wrap = $("#chat .modal-body");
				var wrapHeight = _wrap.height() + 40;
				var scrollHeight = _wrap[0].scrollHeight;
