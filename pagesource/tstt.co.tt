jQuery(function(){
    jQuery("#delCookie").click(function(){
        del_cookie("cookie");   
    });
    
    console.log(document.cookie);
    var visit = getCookie("cookie");
    if (visit == null) {
      jQuery('#overlayopen').trigger('click'); 
       var expire = new Date();
