body {
    color: #868484;
}

.container-wrap {
	background-color:white;
}

p {
    padding-bottom: 15px;
}

/* Headings */

h1, h2, h3, h4, h5, h6 {
    color: #868484;
}

.ft-title {
	text-transform:uppercase;
	color:#d80812;
	font-weight: bold;
}
.ft-title a:hover {
	color: #666;
}

.ft-title-white {
	text-transform:uppercase;
	color:#fff;
	font-weight: bold;
}
.ft-title-white a:hover {
	color: #666;
}

.header-title {
	font-size:16px;
}

.vc_separator.vc_separator_align_left h4 {
    /*color: #D80812;*/
    font-size:22px;
    font-weight: bold;
	text-transform:uppercase;
}


h5 {
    font-size:15px;
    font-weight:bold;
    color:#868484;
}
.cinza h4 {color:#7B7774;}
.red h4 {color:#D80812;}

.heading-line {
	text-transform:uppercase;
}
.heading-line::after {
    border-bottom: 2px solid #D80812;
    content: "";
    display: block;
    width: 58px;
    top: 70px;
	text-transform:uppercase;
}

.title-news {
    color: #D80812;
    font-size:22px;
    font-weight:bold;
}
.title {
    padding-top:10px;
}

.txt-destaque {
    font-size:16px;
}


/* UL */
#lista ul {
	margin: 0;
	padding: 0;
	list-style-type: none;
	width:100%;
}

#lista li { 
	margin: 2px 5px 0 0;
	color:#868484;
	border-bottom:1px solid #DCDCDC;
    font-weight:bold;
    background-color:#E5E4E4;
    padding-left:25px;
    padding-top: 3px;
    padding-bottom: 3px;
}

#lista-white ul {
	margin: 0;
	padding: 0;
	list-style-type: none;
	width:100%;
}

#lista-white li { 
	margin: 2px 5px 0 0;
	color:#868484;
	border-bottom:1px solid #DCDCDC;
    font-weight:bold;
    padding-left:25px;
    padding-top: 3px;
    padding-bottom: 3px;
	text-transform: uppercase;
}

#lista-white li ul li { 
	margin: 2px 5px 0 0;
	color:#868484;
	border:none;
    font-weight: normal;
    padding-left:25px;
    padding-top: 3px;
    padding-bottom: 3px;
	text-transform:none;
}

#lista-dstq ul {
	margin: 0;
	padding: 0;
	list-style-type: none;
	width:100%;
}

#lista-dstq li { 
	margin: 2px 5px 0 0;
	color:#868484;
	border-bottom:1px solid #DCDCDC;
    font-weight:bold;
    padding-left:25px;
    padding-top: 3px;
    padding-bottom: 3px;
	text-transform: uppercase;
}

#lista-dstq li ul li { 
	margin: 2px 5px 0 0;
	color:#868484;
	border:none;
    font-weight: normal;
    padding-left:25px;
    padding-top: 3px;
    padding-bottom: 3px;
	text-transform:none;
}


#lista-title ul {
	margin: 0;
	padding: 0;
	list-style-type: none;
	width:100%;
}

#lista-title li { 
	margin: 2px 5px 0 0;
	color:#868484;
	/*border-bottom:1px solid #DCDCDC;*/
    font-weight:bold;
    padding-left:25px;
    padding-top: 3px;
    padding-bottom: 3px;
}



.main-content ul, .main-content ol {
    margin-left: 0px;
}

.main-content ul li {
    list-style: none;
}


/* PADDING */
.pbottom {
    padding-bottom:20px;
}

/* COLOR TEXT */

.txt-cinza {
    color:#55565A;
}

.txt-red {
    color:#d80812;
}

/* TABELAS */

table.tb-geral-1 tr td {
   border: 2px solid #8D8D90 !important;
   color:#868484;
   background-color:#F2F2F2;
   padding-left:35px !important;
}

table.tb-geral-1 th {
    color:#DD0031;
    padding-left:35px !important;
    background-color:#F2F2F2;
    border: 2px solid #8D8D90 !important;
}

table.tb-geral-1 th:nth-child(1) {  
  text-align:right;
  padding-right:35px;
  background-color:#F2F2F2;
}

table.tb-geral-1 td:nth-child(1) {  
  text-align:right;
  padding-right:35px;
  font-weight:bold;
  background-color:#F2F2F2;
}

/* GERAL*/
.tcenter {
	text-align:center;
}
.tright {
	text-align:right;
}

.meta-author {
    font-size:13px;
}

.btn-lermais {
    padding: 4px 6px;
    background-color: #d80812;
    color: white;
    max-width: 136px;
    text-align: center;
}
.btn-lermais a {
    color: white;
}



/* COLORS */

.txt-red {color:#D80812;}

/* MENU */

header#top nav > ul > li > a {
    padding: 8px 0px !important;
}

#header-outer {
    border-bottom: 8px solid #D80812;
}

header#top nav > ul {
    text-transform: uppercase;
}

header#top nav > ul > li > a {
   
}

header#top nav > ul > li {
    margin: 0 5px;
}

header#top nav > ul > li > a {
    padding-bottom: 0px;
    padding-top: 0px;
}

header#top nav .sf-menu li.current-menu-item > a {
	color:#231F20 !important;
	font-weight:bold;
}

header#top nav .sub-menu li.current-menu-item > a {
	border-top: 0px solid  #D80812;
	color:#D80812 !important;
	font-weight:bold;
}

header#top nav .sf-menu li.current-menu-ancestor > a {
	color:#231F20 !important;
}

/* Submenu */
header#top nav .sf-menu li.current_page_item > a {
	color: #D80812 !important;
    border-top: 0px solid rgba(255,255,255,0.08);
    top: 0px;
}



/* Menu Activo */

.current-menu-item {
	border-top: 3px solid  #D80812;
	font-weight:bold;
	top: 0px;
}

.current-menu-ancestor {
	font-weight:bold;
	top: -3px;
}
.current-menu-parent {
	border-top: 3px solid  #D80812;
	font-weight:bold;
	top: -3px;
}

/* Menu Bottom */
.bottomMenu .current-menu-item {
    border-top: 0px !important;
}

.sub-menu {
	top:42px !important;
}

.sub-menu ul > li{
    background-color:#D80812;
}

.vc_separator .vc_sep_holder .vc_sep_line {
    border-top: 3px solid #DEDBDA;
}

/* HEADER */
header#top #logo {
    padding-bottom: 10px;
}

.box-top {
	padding-bottom:20px;
}

.box-top a {
	color:#B8B7B9;
	font-size:12px;
}
.box-top a:hover {
	color:#D80812;
}
/* FLAGS & Maps Icons*/
.box-top .flag {
	padding: 6px 7px;
	border: 1px solid #B8B7B9 !important;
	margin-right: 5px;
