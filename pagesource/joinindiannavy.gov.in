        $('.slide-out-div').addClass('open')
    });
        
    $('.closePopWindow').click( function(){
        $('.slide-out-div').removeClass('open');
        $('.slide-out-div1').removeClass('open');
        $('.fullDiv').removeClass('fullDiv1');
    });

    $('.slide-out-div1 h3').click( function(){
        $('.slide-out-div1').addClass('open')
    });
        
    $('.slide-out-div1 h3, .slide-out-div h3').click( function(){
        $('.fullDiv').addClass('fullDiv1')
    });
</script>
