<script>(function($) {
    $(document).ready(function() {
        //$("#mi-slider").catslider();
        //$("#owl-demo").owlCarousel();
        var owl = $("#owl-demo");
        //$(".sportlight_item_a img").attr("width", "90%");
        owl.owlCarousel({
            items: 4, //10 items above 1000px browser width
            itemsDesktop: [1000, 3], //5 items between 1000px and 901px
            itemsDesktopSmall: [900, 3], // betweem 900px and 601px
            itemsTablet: [768, 1], //2 items between 600 and 0
            mouseDrag: true,
        });
        // Custom Navigation Events
        $(".next").click(function() {
            owl.trigger("owl.next");
        })
        $(".prev").click(function() {
            owl.trigger("owl.prev");
        })
        $("#owl-demo + .border").hide();
        $(".sportlight_item_a,.spotlight-wrapper").click(function() {
            $("#owl-demo + .border").show();
            $(".hide_bio").removeClass("hide_bio_now");
            $(".hide_bio").slideUp();
            $(".hide_biotext").slideUp();
            var get_item_id = $(this).attr("id");
            var get_id_data = get_item_id.split("item_a_");
            var get_id_div_data = get_item_id.split("item_div_");
            if (get_id_data.length > 1) {
                var get_id = get_id_data[1];
            } else {
                var get_id = get_id_div_data[1];
            }
            //$("#item_cnt_"+get_id).show();
            $("#item_cnt_" + get_id).addClass("hide_bio_now");
            $(".border").addClass("border-click");
            $("#item_cnt_text_" + get_id).fadeIn();
