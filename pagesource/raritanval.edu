            $(".hide_bio").removeClass("hide_bio_now");
            //$(".hide_biotext svg").fadeOut();
            setTimeout(function() {
                //$(".hide_biotext").slideUp();
				$(".hide_biotext").fadeOut();
            }, 10);
            //$(".hide_bio").slideUp();
			$(".hide_bio").fadeOut();
            $("#block-rvcc-spotlight-slider-rvcc-spotlight-slider .border").hide();
            $(".slider_content .sportlight_item_a img").removeClass("hover-over-sportlight-detail");
            $(".border").removeClass("border-hover");
        });
        $(".spotlight-wrapper,.sportlight_item_a").hover(function() {
            //$(".sportlight_item_a img").attr("width","90%");
            //$(".hide_bio").removeClass("hide_bio_now");
            //$(".border").removeClass("border-click");
            $(".hide_bio").fadeOut();
            $(".slider_content .sportlight_item_a img").removeClass("hover-over-sportlight-detail");
            /* var get_item_id = $(this).attr("id");
            var get_id_div_data = get_item_id.split("item_div_");
            var get_id_data = get_item_id.split("item_a_"); */
            var get_item_id = $(this).attr("id");
            var get_id_data = get_item_id.split("item_a_");
