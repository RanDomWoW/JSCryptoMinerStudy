            if (get_id_data.length > 1) {
                var get_id = get_id_data[1];
            } else {
                var get_id = get_id_div_data[1];
                var get_item_a = "#item_a_" + get_id_div_data[1] + " img";
                $(get_item_a).addClass("hover-over-sportlight-detail");
            }
            //$(".border").addClass("border-hover");
           // $("#item_cnt_" + get_id).show();
             $("#item_cnt_" + get_id).fadeIn();
            // setTimeout(function() {
            //     $("#item_a_" + get_id + " img").attr("width", "86%");
            // }, 400);
        }, function() {
