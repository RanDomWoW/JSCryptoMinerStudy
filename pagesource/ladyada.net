function swapStop() { nomoreswapping = true; }
function swapStart() { nomoreswapping = false; }

function ImageSwap(Image, Source) {
	if (nomoreswapping)  return;
	if (document.images) {
		if ((typeof Image == 'string') || (typeof Image == 'number')) {
			document.images[Image].src = Source;
