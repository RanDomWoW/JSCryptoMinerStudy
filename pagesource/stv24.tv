	document.getElementById("time_16").innerHTML = stat_data[ind][4] + '%';
	document.getElementById("time_18").innerHTML = stat_data[ind][5] + '%';*/
	
	if(stat_data[ind][6].length > 0)
	   document.getElementById("video").innerHTML = '<iframe width="310" height="175" src="https://www.youtube.com/embed/' +            stat_data[ind][6] + 
	   '?autoplay=1" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>';
	
	document.getElementById("datatable").style.display = "block";
}
	
function setdata(name, data)
{
	if(data.length > 0)
		document.getElementById(name).innerHTML = data + '%';
	else
		document.getElementById(name).innerHTML = "";
}
	
function hidetable()
{
	document.getElementById("datatable").style.display = "none";
	document.getElementById("video").innerHTML = "";
}
		
