function showdata(ind)
{
	hidetable();
	document.getElementById("pict").innerHTML = '<img src="' + bild[ind] + '">';
	
	setdata("old", stat_data[ind][0]);
	setdata("time_10", stat_data[ind][1]);
	setdata("time_12", stat_data[ind][2]);
	setdata("time_14", stat_data[ind][3]);
	setdata("time_16", stat_data[ind][4]);
