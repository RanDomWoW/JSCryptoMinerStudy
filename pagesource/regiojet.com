<script>
//method="post" action="/sys/blocks/newsletter.jsp?lang=en" onsubmit="return send_newsletter(this)" 

$(function() {
    $('#form-newsletter-hp').submit(function(e) {
		e.preventDefault();
		send_newsletter(this);
	});
});

function send_newsletter(obj) {
	var invalid = false;
	var email = $('#form-newsletter-hp [name="email"]');
	var label = $('label[for="footer-newsletter"]');
	
	if(!$('#footer-newsletter').prop('checked')) {
		label.addClass('invalid');
		invalid = true;
	} else {
		label.removeClass('invalid');
	}
	if(email.val() == '') {
		email.parent().addClass('invalid');
		invalid = true;
	} else {
		var pattern = new RegExp(/^([0-9a-zA-Z]+[-._+&amp;])*[0-9a-zA-Z]+@([-0-9a-zA-Z]+[.])+[a-zA-Z]{2,6}$/);
		if(!pattern.test(email.val())) {
			email.parent().addClass('invalid');
			invalid = true;
		}else {
			email.parent().removeClass('invalid');
		}
	}
	
	if(invalid) {
		return;
	}
		
		
	var data = $(obj).serialize();
	$.ajax({
		method: 'Post',
		url: '/sys/blocks/newsletter.jsp?lang=en',
		data: data,
		dataType: 'json'
	}).done(function(data) {
		var ico = 'icon-fajfka';
		if(data.type === 'error') {
			ico = 'icon-krizek';
		}
		var opts = {
			"type": data.type,
        	"message": {
	            "icon": "sa-icon " + ico,
	            "title": data.message,
	            "content": "",
	            "close": "Close"
	        }
   		};
		var lb = $('#form-newsletter-hp').lightbox;
		console.log(opts)
		lb.create(null, "icon-lightbox", window.customRenderFunction, opts).hideX().closeOnBackground();
	});
		//$('.newsletter .nwsl-msg').html(data);
		$(obj).trigger("reset");
	return false;
}

function slightbox() {

	var content = `<h4>Consent with processing personal information.</h4><p>I agree with collecting, storing, and processing personal information contained in this form by the administrator, <strong>STUDENT AGENCY, k.s., with the registered office at Dům pánů z Lipé, Náměstí Svobody 17, 602 00 Brno, Czech Republic, IČ: 25317075</strong> (or by its employees) for the purpose stipulated below. I grant this agreement for all information contained in the form for the whole period of <strong>5 (five) years</strong> from the date it was granted. I am also aware of my rights according to <strong>§Sec. 2 and 21 pf Act. No. 101/2000 Soll., on  protection of personal information.</strong>  I have learnt all filled-in parts of the form, all given information is true and accurate, and it is given on the voluntary basis.</p><h5>Purpose of processing of personal information</h5><p>According to Sec. 5  of Law No. 101/2000  Coll., on protection of personal innformation, all information given in thiss formm is collected and processed solely for the purposes of <strong>sending commercial announcements</strong> to the subject of the information using electronic means according to <strong>Law No. 480/2004 Coll., on some services of an information company </strong>,  until the time when the subject directly and efficiently sends the administrator the information that he/she no longer wants to receive commercial infomormation from the administrator), maximum for the period of 5 (five) years. Summarized information from this form can be used by the administrator for statistical purposes for the internatl use of the administrator.</p><h5>Declaration by the administrator</h5><p>The administrator declares that it will collect personal information in the scope necessary for fulfilling the given purpose, and  process it only in accord with the purpose for which it has been collected. its emloyeed or other people processing the personal information on a contractual basis with the administrator are obliged to treat the information as confidential even after the termination of work contract or work as such.</p>`;
	var lb = $('#form-newsletter-hp').lightbox;
	lb.create(content, "text-lightbox").setHeight(620).closeOnBackground();
	return false;
}
