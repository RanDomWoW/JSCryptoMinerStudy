<script type="text/javascript">
$(document).ready(function(){
	var form = $("#fbuscador");	
	var busct;
	
	form.find("input[name=busqueda]").bind('keyup', function(e) {
		clearTimeout(busct);
		busct = setTimeout(function() {
			$('#cabecera-tienda-buscador-resultados').html('<p>Cargando resultados ...</p>');
			$('#cabecera-tienda-buscador-resultados').show();
			var form = $("#fbuscador");	
			var input = form.find("input[name=busqueda]").val();
			var tipo = form.find("input[name=concepto]").val();
			if(input.length >= 3)
			{
				$.post('/ajax/buscador', { concepto: input, tipo: tipo }, function(data) {
					if(data.ok == 1)
					{
						$('#cabecera-tienda-buscador-resultados').html(data.html);
						$('#cabecera-tienda-buscador-resultados').show();
					}else{
						$('#cabecera-tienda-buscador-resultados').html('');
						$('#cabecera-tienda-buscador-resultados').hide();	
					}
					
					$('#cabecera-tienda-buscador-resultados div.cerrar a').click(function(){
						$('#cabecera-tienda-buscador-resultados').html('');
						$('#cabecera-tienda-buscador-resultados').hide();
						$('#fbuscador input[name=busqueda]').val('');
						return false;
					});
					
				}, "json");
			}else{
				$('#cabecera-tienda-buscador-resultados').html('');
				$('#cabecera-tienda-buscador-resultados').hide();
			}
		}, 1000);
	});
});
