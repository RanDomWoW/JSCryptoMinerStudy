        });

        $('#notice').click(function () {
            $('#showall_notice').css("display", "block");
        });

        $('#updates').click(function () {
            $('#showall_notice').css("display", "none");
        });

        var loc = window.location.pathname + window.location.search;
        $('.sidebar-nav-v1').find('a').each(function () {
            if ($(this).attr('href') == loc) {
                $(this).attr("style", "color:#09568c !important;background-color: #ddd;padding-left: 18px !important;background: url(/resources/images/arrow-blue.gif) 5px 15px no-repeat;");
                //$("this").css("color", "red");
                //console.log($("this"));
                //$("this").parent().attr("class", "active");
                //$(this).toggleClass('active', $(this).attr('href') == loc);
            }
        });
        var arrBg = ['student', 'admission', 'phd', 'college', 'syllabus', 'course', 'academic','library'];

        for (i = 0; i < arrBg.length; i++) {
            if (loc.toLowerCase().indexOf(arrBg[i]) > 0) {
                $(".breadcrumbs").css("background-image", "url(/resources/images/edu-banner.jpg)");
                break;
            }
            else {
                $(".breadcrumbs").css("background-image", "url(/resources/images/gen-banner.jpg)");
            }
        }

    </script>

