        function checkCode(code)
        {
            var valid = "False";
            var soapMessage =
            '<?xml version="1.0" encoding="utf-8"?> \
            <soap:Envelope xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/"> \
              <soap:Body> \
                <ValidateCode xmlns="http://tempuri.org/"> \
                  <Code>' + code + '</Code> \
                </ValidateCode> \
              </soap:Body> \
            </soap:Envelope>';
 
            $.ajax({
            url: "/validatePostalCode.asmx?op=ValidateCode",
            type: "POST",
            dataType: "xml",
            data: soapMessage,
            complete: checkResult,
            contentType: "text/xml; charset=\"utf-8\""
            });
             //$("#postalCode").text(code);

            return false;
        }
 
        function checkResult(xmlHttpRequest, status)
        {
             $(xmlHttpRequest.responseXML)
                .find('ValidateCodeResult')
                .each(function()
             {
             valid = $(this).text();
             });

	            if (valid == "True")
	            {
		            $("#validResult").text("Your Postal Code has been confirmed.");
		            $("#imgBtn2").attr("src", "/_themes/Rheem/v2/images/btn_submit_b.png");
		            $("#imgBtn2").removeAttr("disabled"); 
	            }
	            else
	            {
		            $("#validResult").html("This is not a valid US/Canadian Postal Code. Please try again.");
		            $("#imgBtn2").attr("src", "/_themes/Rheem/v2/images/btn_submit_b_gray.png");
		            $("#imgBtn2").attr("disabled", "disabled");
	            }
	
        }

//        $("#zipcode2").keyup(function() { 
//            $("#imgBtn2").attr("src", "/_themes/Rheem/v2/images/btn_submit_b_gray.png");
//		    $("#imgBtn2").attr("disabled", "disabled");
//            if($("#zipcode2").val().length < 5)
//            {
//            $("#validResult").text("Validating Postal Code ...");
//            }
//            else
//            {
//            checkCode($("#zipcode2").val());
//            }
//        });


        function checkEntry()
        {
            $("#imgBtn2").attr("src", "/_themes/Rheem/v2/images/btn_submit_b_gray.png");
		    $("#imgBtn2").attr("disabled", "disabled");
	        if($("#zipcode2").val().length < 5)
	        {
	            $("#validResult").text("");
	        }
	        else
	        {
	            checkCode($("#zipcode2").val());
	        }
        }

        $("#zipcode2").keyup(function() { 
	        checkEntry();
        });

        $("#zipcode2").on('paste', function () {
	        checkEntry();
        });




    });

    function getCookie(c_name) {
        if (document.cookie.length > 0) {
            c_start = document.cookie.indexOf(c_name + "=");
            if (c_start != -1) {
                c_start = c_start + c_name.length + 1;
                c_end = document.cookie.indexOf(";", c_start);
                if (c_end == -1) c_end = document.cookie.length;
                return unescape(document.cookie.substring(c_start, c_end));
            }
        }
        return "";
    }
