        var tanklesswaterheaters = "&TanklessWaterHeaters=" + (document.getElementById("tanklesswaterheaters").checked ? 1 : 0);
        var solarwaterheaters = "&SolarWaterHeaters=" + (document.getElementById("solarwaterheaters").checked ? 1 : 0);
        var homegenerators = "&HomeGenerators=" + (document.getElementById("homegenerators").checked ? 1 : 0);
        var poolandspawaterheaters = "&PoolAndSpaWaterHeaters=" + (document.getElementById("poolandspawaterheaters").checked ? 1 : 0);
        var allCategories = zipcode + distance + heatingandcooling + residentialwaterheaters + commercialwaterheaters + tanklesswaterheaters + solarwaterheaters + homegenerators + poolandspawaterheaters;
        //window.location = "/contractors.aspx?" + allCategories;
        getPostalResults($("#zipcode").val(), allCategories);
    }

    function getPostalResults(zipcode, allCategories) {
        $.get("/setpostalcode.aspx?newPostalCode=" + zipcode, function (data) {
            var dataRetrieved = data;
            //alert("Before IF: " + data);
            if (dataRetrieved.indexOf('success') >= 0) {
                window.location = "/contractors.aspx?" + allCategories;
            }
            else {
                //$("#progressDialog").dialog("close");
                //alert("Invalid Postal Code!");
                window.location = "/contractors.aspx?" + allCategories;
            }

        });
    }

    function setNewPostal(zipcode) {
        $.get("/setpostalcode.aspx?newPostalCode=" + $("#zipcode2").val(), function (data) {
            var dataRetrieved = data;
            if (dataRetrieved.indexOf('success') >= 0) {
                var PATH = $(location).attr('pathname');
                //alert(PATH);
                if (PATH.indexOf('contractors') >= 0) {
                    var zipcode = "PostalCode=" + $("#zipcode2").val();
                    var distance = "&Distance=" + "25"; //$("#distance").val();

                    var heatingandcooling = "&HeatingAndCooling=" + getUrlVars()["HeatingAndCooling"];
                    var residentialwaterheaters = "&HeatingAndCooling=" + getUrlVars()["ResidentialWaterHeaters"];
                    var commercialwaterheaters = "&CommercialWaterHeaters=" + getUrlVars()["CommercialWaterHeaters"];
                    var tanklesswaterheaters = "&TanklessWaterHeaters=" + getUrlVars()["TanklessWaterHeaters"];
                    var solarwaterheaters = "&SolarWaterHeaters=" + getUrlVars()["SolarWaterHeaters"];
                    var homegenerators = "&HomeGenerators=" + getUrlVars()["HomeGenerators"];
                    var poolandspawaterheaters = "&PoolAndSpaWaterHeaters=" + getUrlVars()["PoolAndSpaWaterHeaters"];
                    var allCategories = zipcode + distance + heatingandcooling + residentialwaterheaters + commercialwaterheaters + tanklesswaterheaters + solarwaterheaters + homegenerators + poolandspawaterheaters;
                    window.location = "/contractors.aspx?" + allCategories;
                }
                else {
                    window.location.reload();
                }
            }
            else {
                //alert("Invalid Postal Code!");
                //window.location.reload();
                $("#dialog2").dialog("close");
                $(".modalBackground3").css("display", "none");
                $("#progressDialog").dialog("close");
            }

        });
    }

    function getUrlVars() {
        var vars = [], hash;
        var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
        for (var i = 0; i < hashes.length; i++) {
            hash = hashes[i].split('=');
            vars.push(hash[0]);
            vars[hash[0]] = hash[1];
        }
        return vars;
    }



    $(function () {
        $('#checkall').on('click', function () {
            $(this).closest('.row').find(':checkbox').prop('checked', this.checked);
        });
    });


</script>
