<script type="text/javascript">
var mobileURL = "//mobile.rheem.com";
var mainDomain = "rheem.com"
var windowsize = screen.width;
var pathname = window.location.pathname;
var querystring = window.location.search;
var mobileLink = mobileURL + pathname + querystring;
//console.log(pathname);

function checkCookie()
{
	var mobilePreference = "desktop";
	 if (document.cookie.indexOf("mobilePreference=mobile") > -1 )
	 {
		mobilePreference = "mobile";
	 }
	 return mobilePreference;
}

function setMobileCookie()
{
	var cookieName = 'mobilePreference';
	document.cookie = cookieName + "=; expires=Thu, 01 Jan 1970 00:00:00 UTC";
	var cookieValue = 'mobile';
	var myDate = new Date();
	myDate.setMonth(myDate.getMonth() + 3);
	document.cookie = cookieName + "=" + cookieValue + ";expires=" + myDate + ";domain=." + mainDomain + ";path=/";
	window.location = mobileLink;
}

function checkPathforMobile(pathname)
{
	var mobileReady = false;
	
	var whitelist = [
	"/products/",
	"/products",
	"/about/",
	"/about",
	"/TestingMobileResize.html",
	"/contractors.aspx", "/"];
	//console.log($.inArray(pathname, whitelist));
	//whitelisted URLs test
	if ($.inArray(pathname, whitelist) > -1)
	{
		mobileReady = true;
	}
	
	// Pattern checking
	// 		Contractors
	if (pathname.indexOf("/contractors") == 0)
	{
		mobileReady = true;
	}
	
	// Individual Products
	if (pathname.indexOf("/product/") == 0)
	{
		mobileReady = true;
	}
	
	var slashEndingPathName = (pathname + "/").replace("//","/");;

	var prodListingPagesList = [
		"/products/heating_and_cooling/",
		"/products/heating_and_cooling/air_conditioners/",
		"/products/heating_and_cooling/heat_pumps/",
		"/products/heating_and_cooling/gas_furnaces/",
		"/products/heating_and_cooling/air_handlers/",
		"/products/heating_and_cooling/oil_furnaces/",
		"/products/heating_and_cooling/package_units/",
		"/products/heating_and_cooling/cooling_coils/",
		"/products/water_heating/",
		"/products/water_heating/gas/",
		"/products/water_heating/electric/",
		"/products/water_heating/tank/",
		"/products/water_heating/hybrid/",
		"/products/water_heating/tank/gas/",
		"/products/water_heating/tank/electric/",
		"/products/water_heating/tankless/",
		"/products/water_heating/tankless/condensing_tankless/",
		"/products/water_heating/tankless/residential/",
		"/products/water_heating/tankless/tankless_electric/",
		"/products/commercial/water_heating/tankless/",
		"/products/commercial/water_heating/commercial_gas/",
		"/products/commercial/water_heating/commercial_electric/",
		"/products/water_heating/point_of_use/",
		"/products/water_heating/solar/",
		"/products/pool_spa/",
		"/testing-mobile-script/"
	];
	
	if ($.inArray(slashEndingPathName, prodListingPagesList) > -1)
	{
		mobileReady = true;
	}
	
	var blacklist = [
	"/h2ac/",
	"/h2ac",
	"/econet/",
	"/econet"];

	//blacklisted URLs test
	if ($.inArray(pathname, blacklist) > -1)
	{
		mobileReady = false;
	}
	
	return mobileReady;
}

function goMobile(event)
{
	setMobileCookie();
	window.location = event.data.param1;
}

function checkMobility() 
{ 
	var windowsize = screen.width; 
	$("#widthDiv").html(windowsize); 
	//console.log('checkPathforMobile is ' + checkPathforMobile(pathname));
	//console.log('windowsize is '  + windowsize);		
	//console.log('pathname is ' + pathname);
	if (windowsize > 767 || (checkPathforMobile(pathname) == false)) 
		{
		//console.log('checkPathforMobile is ' + checkPathforMobile(pathname));
		//console.log('windowsize is '  + windowsize);		
		//console.log('pathname is ' + pathname);
		//console.log('DISPLAY NONE');
			$("#mobile_link").css('display', 'none');
			$("#mobile_link").hide();
			$( "#footer" ).removeClass( "pi-display-mobile-link" );
		}
		else 
		{
			//console.log('checkPathforMobile is ' + checkPathforMobile(pathname));
			//console.log('windowsize is '  + windowsize);		
			//console.log('pathname is ' + pathname);
			//console.log('DISPLAY BLOCK');
			$("#mobile_link_url").attr("href", mobileLink);
			$("#mobile_link").css('display', 'block');
			$("#mobile_link").show();
			$( "#footer" ).addClass( "pi-display-mobile-link" );
			
			if (document.cookie.indexOf("mobilePreference") >= 0) 
			{
			  // We already have a mobile / desktop preference 
			}
			else if(checkCookie() == "mobile")
			{
				window.location = mobileLink;
			}
			else
			{
				// no cookie set for mobile preference so set one
				setMobileCookie();
			 }
		}				
}

$(document).ready(function() 
{
	checkMobility();
	$("#mobile_link_url").click({param1: mobileLink}, goMobile);
});


</script>
