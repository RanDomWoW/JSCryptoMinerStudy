    <script>
        function processGetQuoteForm(e) {
            if (e.preventDefault) e.preventDefault();
            var postcode = "";
            var postcodeTextBoxes = $(".txtPostcode");
            if (postcodeTextBoxes.length === 1) {
                postcode = postcodeTextBoxes.eq(0).val();
            } else {
                var postcode1 = postcodeTextBoxes.eq(0).val();
                var postcode2 = postcodeTextBoxes.eq(1).val();
                postcode = !!postcode1 ? postcode1 : postcode2;
            }
            var formattedPostcode = postcode.replace(/[^a-z0-9]/ig, "").toLowerCase();
            var standAloneUrl = $(".standAloneUrl").val();
            var url = standAloneUrl;
            if (formattedPostcode) {
                url = standAloneUrl + "/getaquote/" + formattedPostcode;
            }
            var referrer = $(".referrer").val();
            if (!!referrer) {
                url += "?referrer=" + referrer;
            }
            window.location = url;
            return false;
        }
        $(".get-quote-standalone").submit(processGetQuoteForm);
    </script>
