<script type="text/javascript">//<![CDATA[
function check_email(event) {
	if(event.type == 'submit') {
		event.stop();
	}
	var mail = $('idLoginEmail').value;
	if(mail && emailcheck(mail)) {
		$('idLoginEmail').removeClassName('error');
		new Ajax.Request('/ajax/check_email_exists.php', {
			method: 'post',
			parameters: { 'email' : mail },
			onSuccess: function(transport){
				if(transport.responseText != 'true') {
					$('txtRequestEmail').setValue(mail);
					$('login').hide();
					$('register').show();
					if(transport.responseText == 'pending') {
						$('email_pending').show(); $('email_unknown').hide();
					} else {
						$('email_pending').hide(); $('email_unknown').show();
					}
				}
				else if(event.type == 'submit') {
					$('login').submit();
				}
			}
		});
	}
	else {
		$('idLoginEmail').addClassName('error');
		$('idLoginEmail').focus();
	}
}
function resend_email_confirm()
{
	$('register_action').value = 'resend_email_change';
	$('register').submit();
}
//]]></script>

	
