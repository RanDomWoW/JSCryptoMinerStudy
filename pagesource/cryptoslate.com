        <script type="text/javascript">
          jQuery(document).ready(function ($) {
            var is_sending = false,
                failure_message = 'Whoops, looks like there was a problem. Please try again later.';

            $('#modal-ico-submission-form').submit(function (e) {
              if (is_sending || !validateInputs()) return false;

              e.preventDefault();
              var $this = $(this);
              $.ajax({
                url: 'https://cryptoslate.com/wp-admin/admin-ajax.php',
                type: 'post',
                dataType: 'JSON',
                data: $this.serialize(),
                beforeSend: function () {
                  is_sending = true;
                },
                error: handleFormError,
                success: function (data) {
                  if (data.status === 'success') {
                    $this.hide();
                    $('.form-success').fadeIn();
                  } else {
                    handleFormError();
                  }
                }
              });
            });

            function handleFormError () {
              is_sending = false; // Reset the is_sending var so they can try again...
              alert(failure_message);
            }

            function validateInputs () {
              var $name = $('#modal-ico-submission-form > input[name="name"]').val(),
                  $email = $('#modal-ico-submission-form > input[name="email"]').val(),
                  $ico = $('#modal-ico-submission-form > input[name="ico"]').val(),
                  $website = $('#modal-ico-submission-form > input[name="website"]').val();

              if (!$name || !$email || !$ico || !$website) {
                alert('Before sending, please make sure to provide your name, email, ICO name, and website.');
                return false;
              }
              return true;
            }
          });
        </script>
