<script>
    $('.carousel').carousel({
        interval: 10000,
        wrap: false
    });

    $("#carousel-example-generic").on('slid.bs.carousel', function () {
        var first_slide = $('.carousel-inner .item:first-child');
        var left_control = $('.left.carousel-control');
        if (first_slide.hasClass('active')) {
            left_control.css({'display': 'none'});
        } else left_control.css({'display': 'block'});

        var last_slide = $('.carousel-inner .item:last-child');
        var right_control = $('.right.carousel-control');
        if (last_slide.hasClass('active')) {
            right_control.css({'display': 'none'});
        } else right_control.css({'display': 'block'});
    });

    var carouselInner = $('.carousel.press-list .carousel-inner'),
        maxCarouselHeight = 400,
        carouselExpandBtn = $('<a>')
            .addClass('carousel-expand-btn')
            .text('Развернуть')
            .attr('href', '#')
            .on('click', function (e) {
                e.preventDefault();

                var active_item = carouselInner.find('.item.active');

                carouselInner.toggleClass('expanded');

                doCarouselExpand(active_item);

                if (!carouselInner.hasClass('expanded') && active_item.height() <= maxCarouselHeight) {
                    carouselExpand.hide();
                }
            }),
        carouselExpand = $('<div>')
            .addClass('carousel-expand')
            .append(carouselExpandBtn);

    carouselInner.after(carouselExpand);

    $('.carousel.press-list').on('slid.bs.carousel', function (e) {
        var active_item = $(this).find('.item.active');

        if (carouselInner.hasClass('expanded')) {
            carouselExpandBtn.click();
        } else {
            carouselExpand.css('display', active_item.height() <= maxCarouselHeight ? 'none' : 'block');
        }
    });

    $('.carousel.press-list').trigger('slid.bs.carousel');

    function doCarouselExpand (active_item) {
        carouselInner.css(
            'height',
            carouselInner.hasClass('expanded') ? (active_item.height() + 100) : maxCarouselHeight);
        carouselExpandBtn.text(carouselInner.hasClass('expanded') ? 'Свернуть' : 'Развернуть');
    }
</script>
