        // 5: 하락
        //▼↓↑▲0
        var rtnVal="";
        if (sval == 1) {
            rtnVal = "<font color='#ff0000' size='1'>↑ "+sval1+"</font>";
        }else if (sval ==2) {
            rtnVal = "<font color='#ff0000' size='1'>▲ "+sval1+"</font>";
