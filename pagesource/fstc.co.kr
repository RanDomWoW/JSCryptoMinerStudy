	// 사업분야
	function busiToggle(obj,status)
	{
		if (status == "on")
		{
			J(obj).find("span").addClass("on");
			J(obj).find(".bodyView").slideDown('fast');
			imageBtnSwap(J(obj).find(".icoSpace"),"_minus.");
		}
		else
		{
			J(obj).find("span").removeClass("on");
			J(obj).find(".bodyView").slideUp('fast');
			imageBtnSwap(J(obj).find(".icoSpace"),"_plus.");
		}
		return busiToggle;
	}
	// 사업분야 이미지 버튼 스왑
	function imageBtnSwap(obj,src)
	{
		var imgBtn = J(obj).attr("src");
		if (typeof imgBtn != "undefined")
		{
			if (imgBtn.indexOf("_minus.")>=0)
			{
				J(obj).attr("src",imgBtn.replace("_minus.",src));
			}
			else if (imgBtn.indexOf("_plus.")>=0)
			{
				J(obj).attr("src",imgBtn.replace("_plus.",src));
			}
			else if (imgBtn.indexOf("_on.")>=0)
			{
				J(obj).attr("src",imgBtn.replace("_on.",src));
			}
			else
			{
				J(obj).attr("src",imgBtn.replace(".",src));
			}
		}
		return imageBtnSwap;
	}
