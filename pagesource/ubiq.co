<script type="text/javascript">

$(document).ready(function(){

 
  // Fix input element click problem for dropdown forms specific this page only. It should not be included in any js files.
  $('#sign_in_form input, #sign_in_form label, #sign_in_form a').click(function(e) {
    e.stopPropagation();
  });

	$('#chat_link').css('cursor','pointer');
	$('#chat_link').click(function(){
	var opacity=$('#chat_box').css('opacity');
		if(opacity==0){
			$('#chat_box').css('filter','alpha(opacity=100)');
			$('#chat_box').css('opacity',1);
			$('#chat_link').html('Click to close');
		}
		else{
			$('#chat_box').css('filter','alpha(opacity=0)');
			$('#chat_box').css('opacity',0);
			$('#chat_link').html('Need help? Click to chat!');
		}
	});
	
	$('.carousel').carousel({interval: 6000});

	$(".carousel-indicators li").click(function(e){
		e.preventDefault();
		var index = parseInt($(this).attr('data-slide-to'));
		$('#myCarousel').carousel(index);
		var nav = $('.carousel-indicators');
		var item = nav.find('li').get(index);
		nav.find('li.active').removeClass('active');
		$(item).addClass('active');
	});

	$("#myCarousel").bind('slide', function(e) {
		var elements = 3;
		var nav = $('.carousel-indicators');
		var index = $('#myCarousel').find('.item.active').index();
		index = (index == elements - 1) ? 0 : index + 1;
		var item = nav.find('li').get(index);
		nav.find('li.active').removeClass('active');
		$(item).addClass('active');
	});

	$(function(){
		$(".video1").videopopup({
						'videoid' : 'i618WpnXyuo',
						'videoplayer' : 'youtube', // options - youtube or vimeo
						'autoplay' : 'true',// options - true or false
						'width':'640',
						'height':'360'
					});
		});
	
});



</script>

