$(function(){
	var contentNB = 4;
	
    $.ajax({
        url: "https://voice-laser.com/blog/feed/",
        type: "GET",
        dataType: "xml",
        success: function(xml)
        {
            $("item", xml).each(function(entryNum) {
				if(entryNum > contentNB){}else{
					
					// 表示指定 //
					var titleTxt = $("title", this).text();
					var dataTxt = dateSetFunc($("pubDate", this).text());
					var linkUrl = $("link", this).text();
					var descTxt = $("description", this).text();
					var thumImg = $("description",this).text().match(/src="(.*?)"/igm);
					
					// HTML書き出し指定 //
					var htmlstr = "";
					htmlstr += '<div class="feed_onect">';
					htmlstr += '<div class="feed_eye"><a href="' +linkUrl + '"><img ' + thumImg +' alt="' + titleTxt + '"></a></div>';
					htmlstr += '<div class="feed_block">';
					htmlstr += '<h3 class="feed_title"><a href="' + linkUrl + '" target="_blank">' + titleTxt + '</a></h3>';
					htmlstr += '<p class="feed_desc">';
					htmlstr += descTxt.replace(/<("[^"]*"|'[^']*'|[^'">])*>/g,'');
					htmlstr += '</p>';
					htmlstr += '<div class="clearfix"><p class="feed_date">' + dataTxt + '</p>'
					htmlstr += '<p class="feed_more"><a href="' + linkUrl + '">▽続きを読む</a></p>';
					htmlstr += '</div></div></div>';
					
	
					$("div#feed_rss_contents").append($(htmlstr	));
				}
            });
        },
        error: function()
        {
            $('div#feed_rss_contents').append('XMLの読み込みが出来ませんでした。');
        }
    });
     
     
    function dateSetFunc(str)
    {
        var objDate = new Date(str);
        var y = objDate.getFullYear();
        var m = twoDigit(objDate.getMonth() + 1);
        var d = twoDigit(objDate.getDate());
        var ymd = y + "." + m + "." + d;
        str = ymd;
        return str;
    }
     
    function twoDigit(d)
    {
        d = "0" + d;
        d2 = d.substr(d.length - 2, 2);
        return d2;
    }
});
</script>

