<script type="text/template" id="imgBlockInnerBlock">
    <% var innerBlock = '';%>
    <% var innerTitle = '';%>
    <% var innerDescription = '';%>
    <% if((typeof title != 'undefined' && title != null && title != '') || (typeof description != 'undefined' && description != null && description != '')){%>

        <% var textBlockClass = "img-block-inner"; %>
        <% var textBlockStyle = ""; %>
        <% if(typeof textposition != 'undefined' && textposition != null && textposition != '') textBlockClass+= ' img-block-inner-position-'+textposition; %>
        <% if(typeof textalign != 'undefined' && textalign != null && textalign != ''){%>
            <% textBlockClass+= ' img-block-inner-text-align-'+textalign; %>
            <% if(typeof innerblocksize != 'undefined' && innerblocksize != null && innerblocksize != '') textBlockClass += " img-block-inner-innerblocksize-"+innerblocksize; %>
        <% } %>
        <% if(typeof textcolor != 'undefined' && textcolor != null && textcolor != '') textBlockStyle+= ' color:'+textcolor+';'; %>
        <% if(typeof title != 'undefined' && title != null && title != '') {%>
            <% innerTitle= '<div class="img-block-inner-title">'+title+'</div>'; %>
        <% } %>
        <% if(typeof description != 'undefined' && description != null && description != '') {%>
            <% innerDescription= '<div class="img-block-inner-description">'+description+'</div>'; %>
        <% } %>
        <div class="<%-textBlockClass%>" style="<%-textBlockStyle%>"><%=innerTitle%><%=innerDescription%></div>
    <% } %>
</script>
<script type="text/template" id="imgBlock">
    <% var gradientClass = ""; %>
    <% if(typeof gradient != 'undefined' && gradient != null && gradient != ''){%>
        <% gradientClass = "img-block-bg-gradient-"+gradient; %>
        <% if(typeof gradientsize != 'undefined' && gradientsize != null && gradientsize != ''){%>
            <% gradientClass += " "+gradientClass+"-"+gradientsize; %>
        <% } %>
    <% } %>
    <% var innerBlockTmpl = getTemplate("imgBlockInnerBlock"); %>
    <% var innerBlock = _.template(innerBlockTmpl)(arguments[0]); %>
    <% if(typeof link != 'undefined' && link != null && link != ''){%>
        <a 
            <% if(typeof target != 'undefined' && target != null && target != ''){ %> 
            target="<%-target%>"
            <% } %>
            href="<%-link%>"
        >
            <div class="img-block-bg <%=gradientClass%>" style="background-image:url(<%-imagelink%>);" >         
                <%=innerBlock%>
            </div>
        </a>
    <% } else { %>
        <div class="img-block-bg <%=gradientClass%>" style="background-image:url(<%-imagelink%>);">         
            <%=innerBlock%>
        </div>
    <% } %>
</script>
