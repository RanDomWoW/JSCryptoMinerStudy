	$('.searchimg').css('display', 'none');
	$('.searchimg.active').fadeIn('slow');
	$searchtype.click(function() {
            var $this = $(this),
            $target = $($this.attr('href')),
            action = $this.attr('data-action');
            $form.attr('action', action);
            $searchtype.removeClass('active');
            $this.addClass('active');
            $searchimg.css('display', 'none').removeClass('active');
            $target.fadeIn('fast').css('display', 'block').addClass('active');
            return false;
	});
	$form.submit(function(){
            var $this = $(this),
            searchvalue = $searchtext.val(),
            action = $this.attr("action");
            window.open(action + searchvalue,'_blank');
            return false
	});
        $searchtext.autocomplete({
            source: function(request, response) {
                $.getJSON("http://suggestqueries.google.com/complete/search?callback=?",
                    {
                        "hl":"en", // Language
                        "ds":"yt", // Restrict lookup to youtube
                        "jsonp":"suggestCallBack", // jsonp callback function name
                        "q":request.term, // query term
                        "client":"youtube" // force youtube style response, i.e. jsonp
                    }
                );
                suggestCallBack = function (data) {
                    var suggestions = [];
                    $.each(data[1], function(key, val) {
                        suggestions.push({"value":val[0]});
                    });
                    suggestions.length = 10; // prune suggestions list to only 5 items
                    response(suggestions);
                };
            },messages: {
                noResults: '',
                results: function() {}
            }
	});
    });
</script>      
