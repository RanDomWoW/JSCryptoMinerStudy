<script language="javascript" type="text/javascript">

    // Retrieve the value of the cookie with the specified name.
    function getCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
        }
        return null;
    }


    function setCookie(NameOfCookie, value, expiredays, domain){

        var ExpireDate = new Date (); 
        ExpireDate.setTime(ExpireDate.getTime() + (expiredays * 24 * 60 * 60 * 1000));
        document.cookie = NameOfCookie + "=" + escape(value) + 
        ((expiredays == null) ? "" : "; expires=" + ExpireDate.toGMTString())+
        ((domain == null) ? "" : "; domain=" + "rcs-rds.ro");
    }

   
    $(document).ready(function () {

    	//setCookie for popUP
	    var cookieName = "fbPopUp";
	    var cookieValue = true;
	    var cookieExpireDays = 3;
	    var cookieStatus = getCookie(cookieName);


        if (cookieStatus === null) {   
            $("#popUpFb").css("z-index","1000000");              	
            $("#popUpFb .overlay").removeClass("hidden").addClass("visible");   
            $("#popUpFb .fbpopup").removeClass("slide-out").addClass("slide-in");
            $("#popUpFb .close").removeClass("hidden").addClass("visible");
           
        	setCookie(cookieName, cookieValue, cookieExpireDays)
        }
          
            
        $('#popUpFb .close').on('click', function () {
            $("#popUpFb .overlay").removeClass("visible").addClass("hidden");   
            $("#popUpFb .fbpopup").removeClass("slide-in").addClass("slide-out");
            $("#popUpFb .close").removeClass("visible").addClass("hidden");
            $("#popUpFb").css("display","none");
        });
        
        $('#popUpFb .popup-content').on("click", function(){
        	$("#popUpFb").css("display","none");
        });
     

	});

    </script>           
