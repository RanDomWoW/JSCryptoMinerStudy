<script>
    $(document).ready(function () {
        var startPosition = -1;
        window.onscroll = function () {
            var swidth = screen.width;
            var bmVideo = document.getElementById('beh');
            if (startPosition < 0)
                startPosition = findPosY(bmVideo);
            if ($('#beh .closeVideo').length
                && !$('.adform-adbox').length) {
                $('#beh .closeVideo').remove();
            }

            if (pageYOffset > startPosition) {
                bmVideo.style.position = 'fixed';
                if (!$('#beh .closeVideo').length
                    && $('.adform-adbox').length) {
                    var timeleft = 10;
                    $(bmVideo).append($("<span id='countdownTimer' class='closeVideo'>Annoncen kan lukkes om: "+ timeleft + "</span>"));
                    var downloadTimer = setInterval(function () {
                        timeleft--;
                        var texts = 'Annoncen kan lukkes om: ';
                        document.getElementById("countdownTimer").textContent = texts + timeleft;

                        if (timeleft <= 0) {
                            clearInterval(downloadTimer);
                            document.getElementById("countdownTimer").textContent = 'X';
                            $('.closeVideo').click(function () {
                                $('#beh').hide();
                                $('#beh').addClass('hideforever');
                            });
                        }
                    }, 1000);
                }
                if (swidth >= 768) {
                    $('#beh').addClass('screen');
                    $('#beh').removeClass('mobile');
                }
                else {
                    $('#beh').removeClass('screen');
                    $('#beh').addClass('mobile');
                }
            }
            else {
                $('#beh').removeClass('screen');
                $('#beh').removeClass('mobile');
                bmVideo.style.position='static';
                bmVideo.style.display='block';
            }
        };

        window.onresize = function () {
            var swidth = screen.width;
            var bmVideo = document.getElementById('beh');

            if(startPosition < 0)
                startPosition = findPosY(bmVideo);

            if (pageYOffset > startPosition) {
                bmVideo.style.position ='fixed';
                if (swidth >= 768) {
                    $('#beh').addClass('screen');
                    $('#beh').removeClass('mobile');
                }
                else {
                    $('#beh').removeClass('screen');
                    $('#beh').addClass('mobile');
                }
            }
            else {
                bmVideo.style.position = 'static';
                bmVideo.style.width = '100%';
                $('#beh #adform-outstream').css('width','100%');
            }
        };

        function findPosY(obj) {
            var curtop = 0;

            if (typeof(obj.offsetParent) != 'undefined'
                && obj.offsetParent) {
                while (obj.offsetParent) {
                    curtop += obj.offsetTop;
                    obj = obj.offsetParent;
                }
                curtop += obj.offsetTop;
            }
            else if (obj.y)
                curtop += obj.y;

            return curtop;
        }
    });
</script>

