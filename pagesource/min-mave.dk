<script type="text/javascript">
        var allthreadspagenum = 2;
        var filterthreadspagenum = 2;
        var userthreadspagenum = 2;

        $(document).ready(function ()
        {
            $('.readmorebtn').click(function ()
            {
                var type = $('.threadtable tbody:visible').attr('data-id');

                if (type == 1)
                {
                    $.ajax(
                    {
                        url: "/Document/GetThreads",
                        type: "POST",
                        data:
                        {
                            page: allthreadspagenum
                        },
                        success: function (data) {
                            allthreadspagenum = allthreadspagenum + 1;
                            $('.allthreads').append(data);
                        }
                    });
                }
                else if (type == 2)
                {
                    var subjectIds = _filtersubjects();

                    $.ajax(
                    {
                        url: "/Thread/GetFavoriteThreads",
                        type: "POST",
                        data:
                        {
                            SubjectIds: subjectIds,
                            page: filterthreadspagenum
                        },
                        success: function (data)
                        {
                            filterthreadspagenum = filterthreadspagenum + 1;
                            $('.filterthreads').append(data);
                        }
                    });
                }
                else if (type == 3)
                {
                    $.ajax(
                    {
                        url: "/Thread/GetUserThreads",
                        type: "POST",
                        data:
                        {
                            page: userthreadspagenum
                        },
                        success: function (data) {
                            userthreadspagenum = userthreadspagenum + 1;
                            $('.userthreads').append(data);
                        }
                    });
                }

            });
        });
    </script>
