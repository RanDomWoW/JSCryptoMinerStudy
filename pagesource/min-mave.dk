<script type="text/javascript">
        function initTitleCounter() {
            var titleSelector = $('#threadCreateTitle');
            $(titleSelector).keyup(function () {
                var length = $(titleSelector).val().length;
                var counter = $(titleSelector).siblings('div.titleCounter');
                $(counter).children('span').html(length);

                if (length >= 5) {
                    $(counter).css('color', 'green');
                }
                else {
                    $(counter).css('color', 'red');
                }
            });
        }

        var threadselectsubjectdialog = null;
        var prev_catboxno = '0';
        // see also startsubjthread on subjectview.cshtml
        $('.forumfilter .startthread').click(function (e) {
            var _categoryid = $(this).attr('data-categoryid');
            if (!$('.threadselectsubjectdialog').attr('loaded')) {
                MMC.Ajax('/subject/SelectSubjectsDialog' + (_categoryid ? '?categoryid=' + _categoryid : ''), MMC.AjaxReturnType.html, null, function (htm) {

                    $('.threadselectsubjectdialog').html(htm).attr('loaded', true);
                    $('.threadselectsubjectdialog').css('width', '510px');
                    threadselectsubjectdialog = new MMC.MMdialog('.threadselectsubjectdialog', '',
                        function () {
