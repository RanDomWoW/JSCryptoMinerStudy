                        });
                    threadselectsubjectdialog.SetCloseBtnHtmStr("<span>&nbsp;</span>");
                    threadselectsubjectdialog.Show();

                    $('div[data-mm-boxno]').click(function () {
                        var currsbox = $(this);
                        var currsboxno = currsbox.attr('data-mm-boxno');

                        if (currsboxno != prev_catboxno) {
                            var cmbox = $(currsbox.find('.maincat'));
                            $('div[data-mm-boxno="' + prev_catboxno + '"] .catsubbox').slideUp(300);
                            prev_catboxno = currsboxno;
                            $(currsbox.find('.catsubbox')).slideDown(300);
                        }
                    });

                    // subject click
                    $('._threadselectsubject a[subjectid]').click(function (evt) {

                        var subjectid = $(this).attr('subjectid');

                        MMC.Ajax('/thread/CreateDialog?subjectid=' + subjectid, MMC.AjaxReturnType.html, null, function (_htm) {

                            threadselectsubjectdialog.Hide();
                            $('.threadcreatedialog').html(_htm);
                            threadcreatedialog.SetCloseBtnHtmStr("<span>&nbsp;</span>");
                            MMC.SetConfiguredEditor('.threadcreatedialog #ThreadText');
                            threadcreatedialog.Show();

                            initThreadCreateValidation();

                            var titleSelector = $('#threadCreateTitle');
                            $(titleSelector).keyup(function () {
                                var length = $(titleSelector).val().length;
                                var counter = $(titleSelector).siblings('div.titleCounter');
                                $(counter).children('span').html(length);

                                if (length >= 5) {
                                    $(counter).css('color', 'green');
                                }
                                else {
                                    $(counter).css('color', 'red');
                                }
                            });
                        });
                    });

                    $('._threadselectsubject a[data-mm-subject]').click(function () {
                        window.location.href = '/thread/createthread?subjectid=' + $(this).attr('data-mm-subject');
                    });
                });
            }
            else {
                threadselectsubjectdialog.Show();
            }
        });

        var threadcreatedialog = null;

        $(document).ready(function () {
            threadcreatedialog = new MMC.MMdialog('.threadcreatedialog');
        });

        function initThreadCreateValidation() {
            $.validator.addMethod("notEmptyHtml", function (value, element) {

                //Split the html on the <
                var tagsArray = value.split("&lt;");

                //Remove all text to the left of > including >
                var contentArray = tagsArray.map(function (item) {
                    var pos = item.indexOf("&gt;");
                    return item.substr(pos + 4);
                });

                var trimmedArray = contentArray.map(function (item) {
                    return item.replace('&amp;nbsp;', '').trim();
                });

                var isEmpty = true;

                for (var i = 0; i < trimmedArray.length; ++i) {
                    var s = trimmedArray[i];
                    if (s || s.length > 0) {
                        isEmpty = false;
                        break;
                    }
                }

                return this.optional(element) || !isEmpty;
            }, "Skriv venligst en brødtekst");

            // validate -------------------------------------------------------------------------------------------------------------------------------------------------------------
            $('._threadcreate form').validate({
                ignore: [],
                rules: {
                    ThreadTitle: { required: true, minlength: 5 },
                    ThreadText: {
                        required: function (input) {
                            $(input).val('');
                            var threadEditor = CKEDITOR.instances.ThreadText;

                            if (threadEditor.document.getBody().getText().trim().length > 0)
                                threadEditor.updateElement();
                        }
                    }
                }
                , messages: {
                    ThreadTitle: { required: 'Angiv venligst en overskrift', minlength: 'Titlen skal være minimum 5 bogstaver' },
                    ThreadText: { required: 'Skriv venligst en brødtekst' }
                }
                , errorLabelContainer: ".errorlabelcontainer[data-mm-modeco]"
            });


            MMC.SetCheckboxIcons('._threadcreate  #ThreadSubscribe');
        }
    </script>
