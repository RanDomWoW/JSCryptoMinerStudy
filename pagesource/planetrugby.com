    <script type="text/javascript">
    jQuery(document).ready(function($){

        $('#viewmorefixtures').click(function(){
            var hidden = $('.fixtures--list').filter(function(index){
                return $(this).css('display') === 'none';
            });

            if (hidden.length <=3 ) { var more_button = document.getElementById('viewmorefixtures');
                var less_button = document.getElementById('viewlessfixtures');
                more_button.style.display = 'none';
                less_button.style.display = 'block';}

            $.each(hidden, function(idx, table) {
                if (idx < 3) { table.style.display = 'block'; }
            });
        });

        $('#viewlessfixtures').click(function(){
            var visible = $('.fixtures--list').filter(function(index){
                return $(this).css('display') === 'block';
            });

            var more_button = document.getElementById('viewmorefixtures');
            var less_button = document.getElementById('viewlessfixtures');
            less_button.style.display = 'none';
            more_button.style.display = 'block';

            $.each(visible, function(idx, table) {
                if (idx >= 3) { table.style.display = 'none'; }
            });

            $('html,body').scrollTop(0);
        });

        $('#viewmoreresults').click(function(){
            var hidden = $('.results--list').filter(function(index){
                return $(this).css('display') === 'none';
            });

            if (hidden.length <=3 ) { var more_button = document.getElementById('viewmoreresults');
                var less_button = document.getElementById('viewlessresults');
                more_button.style.display = 'none';
                less_button.style.display = 'block';}

            $.each(hidden, function(idx, table) {
                if (idx < 3) { table.style.display = 'block'; }
            });
        });

        $('#viewlessresults').click(function(){
            var visible = $('.results--list').filter(function(index){
                return $(this).css('display') === 'block';
            });

            var more_button = document.getElementById('viewmoreresults');
            var less_button = document.getElementById('viewlessresults');
            less_button.style.display = 'none';
            more_button.style.display = 'block';

            $.each(visible, function(idx, table) {
                if (idx >= 3) { table.style.display = 'none'; }
            });

            $('html,body').scrollTop(0);
        });

    });

    </script>



