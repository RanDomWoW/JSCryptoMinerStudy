    <script type="text/javascript">
    // demo video start modal
      jQuery('#myModal_video').on('hidden.bs.modal', function (e) {
      jQuery('#myModal_video video').attr("src", jQuery("#myModal_video  video").attr("src"));
      });

      var url = "https://www.youtube.com/embed/yvPE4emZnFg?autoplay=1&modestbranding=1&autohide=1&showinfo=0";

      $("#myModal_video").on('hide.bs.modal', function(){
        $("#demoVideo").attr('src', '');
        });

      $("#myModal_video").on('show.bs.modal', function(){
        $("#demoVideo").attr('src', url);
        });
    // demo video end modal

    // demo form request start
      $('#demo_error').html('');

        $('#demo_submit_btn').click(function() {
            // validate and process form here
        $("#demo_submit_btn").html('Submiting...');
        var name = $("input#demo_user_name").val();
        var userName = name.charAt(0);
        if (userName === ' ') {
          $("#demo_error").html('Name cannot start with a space');
          $("input#demo_user_name").focus();
          $("#demo_submit_btn").html('GET DEMO');
          return false;
        } else if (name === '') {
          $("#demo_error").html('Enter the name');
          $("input#demo_user_name").focus();
          $("#demo_submit_btn").html('GET DEMO');
          return false;
        }
        var email = $("input#demo_user_email").val();
        var validEmail = email.match(/^[A-Z0-9._%+-]+@([A-Z0-9-]+\.)+[A-Z]{2,4}$/i);
        if (validEmail === null) {
          $("#demo_error").html('Enter the valid email');
          $("input#demo_user_email").focus();
          $("#demo_submit_btn").html('GET DEMO');
          return false;
        }
        var phone = $("input#demo_user_mobile").val();
        if (phone == "") {
          $("#demo_error").html('Enter the mobile number');
          $("input#demo_user_mobile").focus();
          $("#demo_submit_btn").html('GET DEMO');
          return false;
        }
        $("#demo_error").html(' ');
        sumbitDemoForm(name, email, phone);
      });

      var sumbitDemoForm = function(name, email, phone) {
        var dataString = 'name=' + name + '&email=' + email + '&mobile=' + phone;
          $.ajax({
            type: "POST",
            url: "http://learnyst.com/admin/mobileAppReq.json",
            data: dataString,
            success: function(resp) {
              if (resp.error_code) {
                $("#demo_error").html(resp.msg);
                $("#demo_submit_btn").html('GET DEMO');
              }
              else {
                $('#demo_request_form')[0].reset();
                $('#demo_error').html('');
                $("#demo_request_form").html('<p style="text-align:center;font-size:16px;font-weight:600;color:#4FBA68">'+resp.message+'</p>');
                $("#demo_submit_btn").disabled(true);
              };
            },
            error: function(resp) {
              $("#demo_submit_btn").html('GET DEMO');
              alert('error');
            }
          });
          return false;
        }

        $('#getDemoModal').on('hidden.bs.modal', function() {
          $('#demo_request_form')[0].reset();
          $('#demo_error').html('');
        });

        $('#getDemoModal').on('shown.bs.modal', function() {
          $('#demo_user_name').focus();
        });
    // demo form request end
    </script>
