/*
* 提交前验证
*
* */
function checkTag(o){
let v,newtag = '',tag = o.attr('data-tag');
o.find('select').each(function(){
v = jQuery(this).val();
newtag += ':' + v;
});
return newtag !== tag;
}
//个性化批量粘贴标签
function threadTagBatch(){
let arr = [];
jQuery('#waterfall .grid__item .wf_styleAction').each(function(){
let tag,id,tid,obj = {'ptsid':[],'id':[],'tid':[]};
jQuery(this).find('select').each(function(){
id = jQuery(this).attr('data-ptsid');
obj.id.push(id);
id = jQuery(this).val();
obj.ptsid.push(id);
});
tid = jQuery(this).find('input').val();
obj.tid.push(tid);
tag = ':' + obj.ptsid.join(':');
let re = '';
for(let i in obj.id){
re += ':0';
}
let v = checkTag(jQuery(this));
if(re != tag && v){
arr.push(obj);
}
});
if(arr.length != 0){
arr = JSON.stringify(arr);
ajax_threadtag(arr,'threadTagBatch');

}

//console.log(arr);
}
//单个帖子粘贴标签
jQuery('.wf_styleAction a').click(function(){
let v,re = '',tag,obj = {'ptsid':[],'id':[],'tid':[]};
jQuery(this).parent().find('select').each(function(){
let id = jQuery(this).attr('data-ptsid');
obj.id.push(id);
id = jQuery(this).val();
obj.ptsid.push(id);
});
let tid = jQuery(this).parents('.grid__item').find('.wt-check input').val();
obj.tid.push(tid);
tag = ':' + obj.ptsid.join(':');
for(let i in obj.id){
re += ':0';
}
v = checkTag(jQuery(this).parent());
if(tag != re && v){
obj = JSON.stringify(obj);
ajax_threadtag(obj,'threadTag');
}
console.log(v);

});
//批量给帖子贴标签
function threadTag(){
var obj = {'tid':[],'ptsid':[],'id':[]};
jQuery('.grid__item .wt-check input:checked').each(function(){
let v = jQuery(this).val();
obj.tid.push(v);
});
jQuery('.wt-bd-fix select').each(function(){
let v = jQuery(this).attr('data-pstid');
obj.id.push(v);
v = jQuery(this).val();
obj.ptsid.push(v);
});
//console.log(obj);
if(obj.tid.length !== 0){
obj = JSON.stringify(obj);
ajax_threadtag(obj,'threadTag');
}
}
