//图片流根据body宽度自适应
function auto_falls(){
var w = jQuery('body').width();

if( w < 800 && w > 400){
jQuery('#containers').css('width','400px');
}else if( w < 1200 && w > 800){
jQuery('#containers').css('width','800px');

}else if( w <1550 && w > 1200){
jQuery('#containers').css('width','1200px');
}else if(w > 1550){
jQuery('#containers').css('width','1550px');

}

//console.log(w);

}
