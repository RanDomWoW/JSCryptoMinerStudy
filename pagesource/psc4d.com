function ajax_bd(a){
var arr = [];
jQuery(".wt-check input:checked").each(function(){
//console.log(111);
arr.push(jQuery(this).val());
});
arr = JSON.stringify(arr);
console.log(arr);
jQuery.post(
'http://psc4d.com/abc.php',
{bd_tid_arr:arr,act:a,cmd:'plsj'},
function(data){
var u = window.location.href;
