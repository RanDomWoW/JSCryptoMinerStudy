<script>
    /**
     * Call this function when a user clicks on a product link. This function uses the event
     * callback datalayer variable to handle navigation after the ecommerce data has been sent
     * to Google Analytics.
     * @param {Object} productObj An object representing a product.
     */
    productClick = function(productObj, event) {
        if (event === 'productClick') {
            dataLayer.push({
                'event': event,
                'ecommerce': {
                    'click': {
                        'actionField': {'list': productObj.list},
                        'products': [{
                            'name': productObj.name,
                            'id': productObj.id,
                            'price': productObj.price,
                            'brand': productObj.brand,
                            'category': productObj.category,
                            'variant': productObj.variant
                        }]
                    }
                },
                'eventCallback': function() {
                    document.location = productObj.url
                }
            });
        } else if (event === 'addToCart') {
            var productObj = jQuery.parseJSON(productObj);
            dataLayer.push({
                'event': event,
                'ecommerce': {
                    'currencyCode': productObj.currencyCode,
                    'add': {
                        'products': [{
                            'name': productObj.name,
                            'id': productObj.id,
                            'price': productObj.price,
                            'brand': productObj.brand,
                            'category': productObj.category,
                            'variant': productObj.variant,
                            'quantity': productObj.quantity
                        }]
                    }
                }
            });
        } else if (event === 'removeFromCart') {
            var productObj = jQuery.parseJSON(productObj);
            dataLayer.push({
                'event': event,
                'ecommerce': {
                    'remove': {
                        'products': [{
                            'name': productObj.name,
                            'id': productObj.id,
                            'price': productObj.price,
                            'brand': productObj.brand,
                            'category': productObj.category,
                            'variant': productObj.variant,
                            'quantity': productObj.quantity
                        }]
                    }
                }
            });
        } else if (event === 'quick_view') {
            dataLayer.push({
                'event': 'productClick',
                'ecommerce': {
                    'click': {
                        'actionField': {'list': productObj.list},
                        'products': [{
                            'name': productObj.name,
                            'id': productObj.id,
                            'price': productObj.price,
                            'brand': productObj.brand,
                            'category': productObj.category,
                            'variant': productObj.variant
                        }]
                    }
                },
                'eventCallback': function() {
                }
            });
        }

    };
    /**
     * A function to handle a click on a checkout button. This function uses the eventCallback
     * data layer variable to handle navigation after the ecommerce data has been sent to Google Analytics.
     */
    onCheckout = function (productObj, event, option) {
        if (event === 'event_1') {
            dataLayer.push({
                'event': 'checkout',
                'ecommerce': {
                    'checkout': {
                        'actionField': {'step': 1, 'option': option.trim()},
                        'products': productObj
                    }
                },
                'eventCallback': function () {

                }
            });
        }
    };
    onRefund = function (id, productObj, event) {
        if (event === 'refund_all') {
            // Refund an entire transaction by providing the transaction ID. This example assumes the details
            // of the completed refund are available when the page loads:
            dataLayer.push({
                'ecommerce': {
                    'refund': {
                        'actionField': {'id': id}
                    }
                }
            });
        } else if (event === 'refund') {
            // Measure a partial refund by providing an array of productFieldObjects and specifying the ID and
            // quantity of each product being returned. This example assumes the partial refund details are
            // known at the time the page loads:
            dataLayer.push({
                'ecommerce': {
                    'refund': {
                        'actionField': {'id': id},
                        'products': productObj
                    }
                }
            });
        }
    }
</script>
