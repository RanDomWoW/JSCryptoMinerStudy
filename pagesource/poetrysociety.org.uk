		// Poem modal
		$('.poetrylist a').on('click', function(){
			if($(window).width()>600){
				if( ! $('#boxcontent').length )
					$("body").append("<div id='boxcontent'><div class='inner'>Loading</div></div>");

				$("#boxcontent .inner").load($(this).attr('href')+" #content",function( responseTxt, statusTxt, xhr ){
					$('#boxcontent').find( '#content' ).addClass( 'poetrylist_remodal' );
				});
				$("#boxcontent").remodal().open();
				return false;
			}
		});

		//empty modal when close

		$(document).on( 'closed', '.remodal', function( evt ){
			var self = $(this);

			if( self.find( '.poetrylist_remodal' ) ) self.children( '.inner' ).empty();
		});
		
		
		// Random position of tweets on home page – only if enough space
		if ($(window).width()>1000){
			if($("body.home").size()>0){
				var tweetIndex = 0;
					$(".fetch-tweets-content").each(function(){
						$(this).css('position','relative');
						if(tweetIndex%2==0){
							$(this).css('left',(0 - (Math.random()*80))+'px');
						} else {
							$(this).css('left',((Math.random()*80))+'px');
						}
						tweetIndex++;
				});	
			}
			var scatterIndex = 0;
			if($(".scatter li").size()>0){
				$(".scatter li").each(function(){
					$(this).css('position','relative');
							if(tweetIndex%2==0){
								$(this).find("a").css('margin-left',(0 - (Math.random()*180))+'px');
							} else {
								$(this).find("a").css('margin-left',((Math.random()*180))+'px');
							}
							scatterIndex++;
				});
				$("iframe").insertAfter($(".scatter li").eq(1));
