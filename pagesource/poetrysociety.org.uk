		}
	});
	
	$(window).on("load", function(){
		// background
		setTimeout(function(){
			if($("body.home").size()>0){
				$("#circles").height($(document).height());
				var totalCircles = 50;
				var spanX = $(document).width() * 1.1;
				var spanY = ($(document).height() - $("#colophon").height())-150;
				var store = [];
				var avoid = ["#tweetheader",".row-1",".home-poem",".home-poem-info","#introduction","#masthead","#breadcrumbs", ".row > div"];
				var dotsize = 90;
				var margin = 40;
				var i;
				while(totalCircles>0){
		
					var available=true;
					var x = (Math.random()*spanX)-50;
					var y = (Math.random()*spanY)+200;
					for(j=0;j<avoid.length;j++){
						var dont = avoid[j];
						
						if((x+margin) > $(dont).offset().left && (x-margin) < ($(dont).offset().left+$(dont).width()) && (y+margin) > $(dont).offset().top && (y-margin) < ($(dont).offset().top+$(dont).height())){
							available = false;
							break;
						}
					}
					for(point in store){
						if(Math.abs(point.x-x) < dotsize && Math.abs(point.y-y) < dotsize) {
							available = false;
							break;
						}
					}
					
					if(available){
						$("#circles").append("<span class='dot'></span>");		
						$(".dot:last").css("left",x+'px');
						$(".dot:last").css("top",y+'px');
						store.push({x:x,y:y});		
						totalCircles--;
						//$(".dot:last").html(totalCircles);
					}
					
					// double check
					for(point in store){
						if(Math.abs(x-point.x) < (dotsize+margin) && Math.abs(y-point.y) < (dotsize+margin)) {
							$(".dot:last").css("border","2px solid black");
						}
					}	
				}
				colorfy_me_dots();
				
			}
			
		},750);	
	});
})( jQuery );	
</script>

