			$(".entry-content a[title]").tooltip();
		}
		if ($('.wp_user_stylesheet_switcher').length){
			$('.wp_user_stylesheet_switcher_icon').each(function(){
				var $alt = $(this).prop('alt');
				//if(window.console){console.log('foo'+$alt)};
				$("<span style='display:block'>"+$alt+"</span>").insertAfter($(this));
			});
		}	
	});
})( jQuery );

</script>

