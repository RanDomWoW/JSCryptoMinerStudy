  cursor: hand;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.carrusel-momentos.owl-loaded {
  display: block;
}
.carrusel-momentos.owl-loading {
  opacity: 0;
  display: block;
}
.carrusel-momentos.owl-hidden {
  opacity: 0;
}
.carrusel-momentos .owl-refresh .owl-item {
  display: none;
}
.carrusel-momentos .owl-item {
  position: relative;
  min-height: 1px;
  float: left;
  -webkit-backface-visibility: hidden;
  -webkit-tap-highlight-color: transparent;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.carrusel-momentos .owl-item img {
  width: 100%;
  /*z-index: 10;*/
