  -webkit-transform-style: preserve-3d;
}
.carrusel-momentos.owl-text-select-on .owl-item {
  -webkit-user-select: auto;
  -moz-user-select: auto;
  -ms-user-select: auto;
  user-select: auto;
}
.carrusel-momentos .owl-grab {
  cursor: move;
  cursor: -webkit-grab;
  cursor: -o-grab;
  cursor: -ms-grab;
  cursor: grab;
}
.carrusel-momentos.owl-rtl {
  direction: rtl;
}
.carrusel-momentos.owl-rtl .owl-item {
  float: right;
}

