<script>

 $( document ).ready(function() {
    $('#carrusel-portadas .owl-prev').html("<i class='fa fa-angle-left'></i>");
    $('#carrusel-portadas .owl-prev').addClass('colortuvoz_vzlafotos');
    
    $('#carrusel-portadas .owl-next').html("<i class='fa fa-angle-right'></i>");
