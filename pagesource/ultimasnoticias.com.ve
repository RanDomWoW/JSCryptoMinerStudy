.slider-principal1 {
  display: none;
  width: 100%;
  -webkit-tap-highlight-color: transparent;
  /* position relative and z-index fix webkit rendering fonts issue */
  position: relative;
  z-index: 1;
  
}
.dominical-estracto{
  padding: 10px 15px;
}
.slider-principal1-img{
  overflow: hidden;
  position: relative;
}
.slider-principal1-img img{
  webkit-transition: all 0.6s;
    -moz-transition: all 0.6s;
    -o-transition: all 0.6s;
    transition: all 0.6s;
}
.slider-principal1-img img:hover{
  -webkit-transform: scale(1.05);
  -moz-transform: scale(1.05);
  transform: scale(1.05);
  webkit-transition: all 0.6s;
    -moz-transition: all 0.6s;
    -o-transition: all 0.6s;
    transition: all 0.6s;
}

.slider-principal1 .owl-stage {
  position: relative;
  -ms-touch-action: pan-Y;
}
.slider-principal1 .owl-stage:after {
  content: ".";
  display: block;
  clear: both;
  visibility: hidden;
  line-height: 0;
  height: 0;
}
.slider-principal1 .owl-stage-outer {
  position: relative;
  overflow: hidden;
  /* fix for flashing background */
  -webkit-transform: translate3d(0px, 0px, 0px);
}

.slider-principal1 .owl-controls {
	position: absolute;
    top: 50%;
    width: 100%;
    display:none;
}

.slider-principal1:hover .owl-controls {
	display:block;
}

.slider-principal1 .owl-controls .owl-nav .owl-prev,
.slider-principal1 .owl-controls .owl-nav .owl-next,
.slider-principal1 .owl-controls .owl-dot {
  cursor: pointer;
  cursor: hand;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.slider-principal1.owl-loaded {
  display: block;
}
.slider-principal1.owl-loading {
  opacity: 0;
  display: block;
}
.slider-principal1.owl-hidden {
  opacity: 0;
}
.slider-principal1 .owl-refresh .owl-item {
  display: none;
}
.slider-principal1 .owl-item {
  position: relative;
  min-height: 1px;
  float: left;
  -webkit-backface-visibility: hidden;
  -webkit-tap-highlight-color: transparent;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.slider-principal1 .owl-item img {
  width: 100%;
  z-index: 10;
  display: block;
  -webkit-transform-style: preserve-3d;
}
.slider-principal1.owl-text-select-on .owl-item {
  -webkit-user-select: auto;
  -moz-user-select: auto;
  -ms-user-select: auto;
  user-select: auto;
}
.slider-principal1 .owl-grab {
  cursor: move;
  cursor: -webkit-grab;
  cursor: -o-grab;
  cursor: -ms-grab;
  cursor: grab;
}
.slider-principal1.owl-rtl {
  direction: rtl;
}
.slider-principal1.owl-rtl .owl-item {
  float: right;
}

