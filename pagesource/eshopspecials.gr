<script>

var seconds_passed = 0;
var seconds_passed2 = 0;
   
		
	Todays_Date = (new Date(18,3,17,6,02,25)).getTime();
	Target_Date = (new Date(18,3,17,11,59,59)).getTime();

      
function countdownxx(format)
{       	  
	seconds_passed++;
			
	Time_Left = Math.round((Target_Date - Todays_Date) / 1000)-seconds_passed;
         
	if(Time_Left < 0)
	{
		Time_Left = 0;
		window.location='http://www.eshopspecials.gr';
	}
       
	switch(format)
	{
		case 0:
			document.all.countdown.innerHTML = Time_Left + ' seconds';
			break;
		case 1:
			days = Math.floor(Time_Left / (60 * 60 * 24));
			Time_Left %= (60 * 60 * 24);
			hours = Math.floor(Time_Left / (60 * 60));
			Time_Left %= (60 * 60);
			minutes = Math.floor(Time_Left / 60);
			Time_Left %= 60;
			seconds = Time_Left;
             
			h0 = m0 = s0 = '';
            if(hours<10 && hours>=0) h0 ='0';
            if(minutes<10 && minutes>=0) m0 ='0';
            if(seconds<10 && seconds>=0) s0 ='0';

			document.all.countdown.innerHTML = '<font color="black">' + h0 + hours + '</font>h ' + '<font color="black">' + m0 + minutes + '</font>m ' + '<font color="black">' + s0 + seconds + '</font>s';
			break;
		default: 
			document.all.countdown.innerHTML = Time_Left + ' seconds';
	}
               
	setTimeout('countdownxx(' + format + ');', 1000);
}

      
function countdownxxx(format)
{       	  
	seconds_passed2++;
			
	Time_Left = Math.round((Target_Date - Todays_Date) / 1000)-seconds_passed2;
         
	if(Time_Left < 0)
	{
