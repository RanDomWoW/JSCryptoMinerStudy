	switch(format)
	{
		case 0:
			document.all.countdownSmall.innerHTML = Time_Left + ' seconds';
			break;
		case 1:
			days = Math.floor(Time_Left / (60 * 60 * 24));
			Time_Left %= (60 * 60 * 24);
			hours = Math.floor(Time_Left / (60 * 60));
			Time_Left %= (60 * 60);
			minutes = Math.floor(Time_Left / 60);
			Time_Left %= 60;
			seconds = Time_Left;
             
			h0 = m0 = s0 = '';
            if(hours<10 && hours>=0) h0 ='0';
            if(minutes<10 && minutes>=0) m0 ='0';
            if(seconds<10 && seconds>=0) s0 ='0';

			document.all.countdownSmall.innerHTML = '<font color="black">' + h0 + hours + '</font>h ' + '<font color="black">' + m0 + minutes + '</font>m ' + '<font color="black">' + s0 + seconds + '</font>s';
			break;
		default: 
			document.all.countdownSmall.innerHTML = Time_Left + ' seconds';
	}
               
	setTimeout('countdownxxx(' + format + ');', 1000);
}

countdownxx(1);
