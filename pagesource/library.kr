<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"  lang="ko" xml:lang="ko">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<title>경기도사이버도서관</title>
<script type="text/javascript">
	function goUrl() {
		if( document.location.href.indexOf("bookmagic") > -1 ) {
			location.href="http://www.library.kr/bookmagic/index.do";
		} else if( document.location.href.indexOf("mobile") > -1 ) {
			location.href="http://www.library.kr/mobile/index.do";
		} else if( document.location.href.indexOf("kid") > -1 ) {
			location.href="http://www.library.kr/cyber/elect/ebook_main_child.do?ebookGubun=children ";
		} else if( document.location.href.indexOf("gu") > -1 ) {
			location.href="http://www.library.kr/memory/index.do";
		} else if( document.location.href.indexOf("golibrary") > -1 ) {
			location.href="http://www.library.kr/cyber/index.do";
		} else if( document.location.href.indexOf("library.kr") > -1 ) {
			location.href="http://www.library.kr/cyber/index.do";
		} else {
			location.href="/cyber/index.do";
		}	
	}
</script>
</head>
<body onload="goUrl();">
</body>
</html>