<script type="text/javascript">

    $(document).ready(function(){

    $(document).on('click', 'a[href^="#"]', function(e) {
        // target element id
        var id = $(this).attr('href');

        // target element
        var $id = $(id);
        if ($id.length === 0) {
            return;
        }

        // prevent standard hash navigation (avoid blinking in IE)
        e.preventDefault();

        // top position relative to the document
        var pos = $id.offset().top;

        // animated top scrolling
        $('body, html').animate({scrollTop: pos});
    });


    var slideCount = $('#slider ul li').length;
    var slideWidth = $('#slider ul li').width();
    var slideHeight = $('#slider ul li').height();
    var sliderUlWidth = slideCount * slideWidth;
    var TmpName = document.getElementById('tmptitle');
    var TmpDesc = document.getElementById('tmp-desc');

    function moveLeft() {
        $('#slider ul').animate({
            left: + slideWidth
        }, 200, function () {
            var items = $('#slider ul li');
            var firstindex = items.index($('#slider ul li:first'));
            $('#slider ul li:last-child').prependTo('#slider ul');
            $('#slider ul').css('left', '');
            var final_name = items.eq(firstindex).attr('data-tmp-name'); 
            var final_desc = items.eq(firstindex).attr('data-tmp-desc'); 
            var final_link = items.eq(firstindex).attr('data-tmp-link'); 
            TmpName.innerHTML  = (final_name);
            TmpDesc.innerHTML  = (final_desc);
    //        TmpLink.href = ("http://yy"+final_link);
        });
    };

    function moveRight() {
        $('#slider ul').animate({
            left: - slideWidth
        }, 200, function () {
            var items = $('#slider ul li');
            var firstindex = items.index($('#slider ul li:first'));
            $('#slider ul li:first-child').appendTo('#slider ul');
            $('#slider ul').css('left', '');
            var final_name = items.eq(firstindex + 2).attr('data-tmp-name'); 
            var final_desc = items.eq(firstindex + 2).attr('data-tmp-desc'); 
            var final_link = items.eq(firstindex + 2).attr('data-tmp-link'); 
            TmpName.innerHTML  = (final_name);
            TmpDesc.innerHTML  = (final_desc);
    //        TmpLink.href = ("http://xx"+final_link);
        });
    };
    function moveSpecific(pos) {
        $('#slider ul').animate({
            //left: - slideWidth
        }, 200, function () {

            if(pos == 0)
            {
                pos = "0";
            }
            if(!pos)
            {
                pos ="m";
            }

             $('#slider ul li[data-tmp-link="wp'+pos+'"]').prependTo("#slider ul");  
            var final_name = $('#slider ul li[data-tmp-link="wp'+pos+'"]').attr('data-tmp-name'); 
            var final_desc = $('#slider ul li[data-tmp-link="wp'+pos+'"]').attr('data-tmp-desc'); 
            var final_link = $('#slider ul li[data-tmp-link="wp'+pos+'"]').attr('data-tmp-link'); 
            TmpName.innerHTML  = (final_name);
            TmpDesc.innerHTML  = (final_desc);
    //        TmpLink.href = ("http://xx"+final_link);
        });
    };

    $('a.control_prev').click(function () {
        moveLeft();
    });

    $('a.control_next').click(function () {
        moveRight();
    });

      $('[id^=carousel-selector-]').click( function(){
          var id_selector = $(this).attr("id");
          var id = id_selector.substr(id_selector.length -1);
          id = parseInt(id);
          moveSpecific(id);
          $('[id^=carousel-selector-]').removeClass('active');
          $(this).addClass('active');
        });

});    
</script>

