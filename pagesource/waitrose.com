function getCookie(c){
	var i,x,y
	var ARRcookies=document.cookie.split(";");
	for (i=0;i<ARRcookies.length;i++){
		x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
		y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
		x=x.replace(/^\s+|\s+$/g,"");
		if (x==c){
			return unescape(y);
		}
	}
}
function toggleBlock(e, state){
	try{
		document.getElementById(e).style.display = state ? 'block' : 'none';
	}catch(e){}
}
function initialize(){
	var isAnonUser = (!getCookie("wtrstatus")) && (!getCookie("WTR_PERSISTENT"));
	
