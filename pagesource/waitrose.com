	toggleBlock('headerBookSlot', false);
	toggleBlock('headerEditSlot', false);
	
	toggleBlock('headerSignInRegister', signInRegisterSection);
	toggleBlock('headerMiniTrolley', miniTrolleySection);
}
initialize();

