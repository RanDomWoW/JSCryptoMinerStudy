var countdown=60;
function popSetButtonDisable(p_obj){
   if (countdown == 0) {
      p_obj.removeAttribute("disabled");
      p_obj.value="重新獲取驗證碼";
      countdown=60;
      return;
   }else {
      p_obj.setAttribute("disabled", true);
      p_obj.value="重新獲取驗證碼(" + countdown + ")";
      countdown--;
   }
   setTimeout(function() {
      popSetButtonDisable(p_obj)
   },1000)
}

