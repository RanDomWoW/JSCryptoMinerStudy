<script class="subscribe-login-modal-template" type="text/x-handlebars-template">

  <form id="finish_registration_overlay" action="{{secureurl}}" class="form-layout-1 expandable use-form-validation without-submit {{#ifCond signup_expanded '==' '1'}}expanded{{/ifCond}}" method="post">
    <div class="top-content">
      <h1 class="style-3 expansion-trigger">JOIN FOR FREE</h1>
      {{#if signup_message}}
        <div class="textrow" style="padding:8px 0px 8px 0px;">{{{signup_message}}}</div>
      {{else}}
        Get handpicked deals from food editors who wonâ€™t settle.
      {{/if}}

        <a href="#" class="button-fb-login">Login With Facebook</a>
        <input type="hidden" name="fb_token" value="" />
        <div class="text-or">or</div>
    </div>
    <div class="form-group"><input type="text" name="email" id="email_overlay" class="text-input-4 block required-email expansion-trigger" value="{{signup_default_email}}" placeholder="email" /></div>

    <div class="expansion-container">
      <div class="form-group"><input type="text" name="name" id="name_overlay" class="text-input-4 block required" value="{{signup_default_name}}" placeholder="name" /></div>

      <div class="form-group" style=""><input type="text" name="zip" id="zip_overlay" class="text-input-4 block required-zip" value="{{signup_default_zip}}" placeholder="zip" /></div>
      <div class="form-group"><input type="password" name="password" id="password_overlay" class="text-input-4 block required-password" placeholder="password" /></div>
      <div class="form-group"><input type="password" name="password_confirm" id="confirm_password_overlay" class="text-input-4 block required-password-confirm required-retype-password" placeholder="confirm password"></div>
      <h3 class="style-2 padding-1">YOUR EDITIONS</h3>
      <ul class="checkbox-list-1 require-one-checked" data-error-message="You must select at least one edition.">
        <li class="left-column">
          <input type="checkbox" name="la_subscription" id="la_subscription_overlay" class="checkbox" value="1" {{#ifCond la_edition_checked '==' '1'}}CHECKED{{/ifCond}} />
          <label for="la_subscription_overlay">Los Angeles</label>
        </li>
        <li class="middle-column">
          <input type="checkbox" name="ny_subscription" id="ny_subscription_overlay" class="checkbox" value="1" {{#ifCond ny_edition_checked '==' '1'}}CHECKED{{/ifCond}} />
          <label for="ny_subscription_overlay">New York</label>
        </li>
        <li class="right-column">
          <input type="checkbox" name="chi_subscription" id="chi_subscription_overlay" class="checkbox" value="1" {{#ifCond chi_edition_checked '==' '1'}}CHECKED{{/ifCond}} />
          <label for="chi_subscription_overlay">Chicago</label>
        </li>
        <li class="left-column">
          <input type="checkbox" name="sf_subscription" id="sf_subscription_overlay" class="checkbox" value="1" {{#ifCond sf_edition_checked '==' '1'}}CHECKED{{/ifCond}} />
          <label for="sf_subscription_overlay">San Francisco</label>
        </li>
        <li class="">
          <input type="checkbox" name="market_subscription" id="market_subscription_overlay" class="checkbox" value="1" {{#ifCond market_edition_checked '==' '1'}}CHECKED{{/ifCond}} />
          <label for="market_subscription_overlay">National</label>
        </li>
      </ul>

      <div class="form-details">
        By subscribing, I agree to the <a href="terms_and_conditions" target="_new">Terms &amp; Conditions</a> and <a href="privacy_policy" target="_new">Privacy Policy</a> and that I am over 21.
        <input type="hidden" name="terms" id="terms_input_overlay" value="1">
      </div>

      <div class="button-container">
        <input type="submit" class="button-1 size-plus-1 mixpanel-track-click" data-mp-event="Sign Up Attempt Through Modal Signup" value="SUBSCRIBE" />
      </div>

      <div class="footer-details">
        <input type="checkbox" name="keep_logged_in" id="keep_logged_in_value_overlay" class="checkbox" value="1" /> <label for="keep_logged_in_value_overlay">Keep me logged in</label>
      </div>

      <div id="registration_status_overlay"></div>
      <input type="hidden" name="a" value="fullsignup">
      <input type="hidden" name="s" value="{{section}}">
      <input type="hidden" name="id" value="{{identifier}}">
      <input type="hidden" name="psa" value="{{signup_page_secondary_action}}">
      <input type="hidden" name="return_s" value="{{return_section}}">
      <input type="hidden" name="return_id" value="{{return_identifier}}">


    <input type="hidden" name="rid" class="signup_rid" value="">
    <input type="hidden" name="referral_contest_id" value="">
    
    <input type="hidden" name="rc" class="signup_rc" value="" />
    
    <input type="hidden" name="ftoken" value="">
    {{force_nonmobile_site_html}}
  </div>

</form>


  <form action="{{secureurl}}" id="login" class="form-layout-1 use-form-validation without-submit has-top-border" method="post">
      {{#if get_deal}}
        <h3 style="border-bottom:1px solid #969696; text-align:center;"><img src="{{rooturl}}img/overlay_blue.header.one-click.gif" border="0" width="271" height="43"></h3>
      {{else}}
        <h3 class="style-2">MEMBER LOGIN</h3>
      {{/if}}
      <div class="textrow" style="text-align:center; padding:8px 0px 8px 0px;">{{{message}}}</div>
      <div class="form-group"><input type="text" name="email" id="login_email" class="text-input-4 block required-email" placeholder="email" /></div>
      <div class="form-group">
        <input type="password" name="password" id="login_password" class="text-input-4 med-3 required" placeholder="password" onkeypress="checkFormSubmitEnter(event,'login');">
        <input type="submit" value="login" class="button-1 size-minus-1" />
      </div>

      <div class="footer-details">
        <a href="#" class="show-forgot-password-link" style="float:right;">Forgot password?</a>
        <div class="checkbox-keep-logged-in-overlay-2" onclick="updateKeepLoggedInCheckbox(this);" id="keep_logged_in_checkbox_overlay">
          <input type="checkbox" name="keep_logged_in" id="keep_logged_in_value_overlay_2" class="checkbox" value="1" checked /> <label for="keep_logged_in_value_overlay_2">Keep me logged in</label>
        </div>
      </div>

      <input type="hidden" name="a" value="login">
      <input type="hidden" name="s" value="{{section}}">
      <input type="hidden" name="id" value="{{identifier}}">
      <input type="hidden" name="psa" value="{{login_page_secondary_action}}">
      <input type="hidden" name="return_s" value="{{return_section}}">
      <input type="hidden" name="return_id" value="{{return_identifier}}">
      <input type="hidden" name="eid" value="{{edition_id}}">
      <input type="hidden" name="fb_token" value="" />
      {{force_nonmobile_site_html}}
  </form>

</script>

