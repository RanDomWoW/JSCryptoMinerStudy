<script class="forward-special-modal-template" type="text/x-handlebars-template">
<form id="{{id}}_send_to_friends" class="form-layout-1 use-form-validation form-forward-special" action="{{rooturl}}ajax.php">
  <h1 class="style-3">SEND TO FRIENDS</h1>
  Send To Friends Enter friends' email addresses separated by commas.

  <div class="input-row padding form-group">
    <textarea name="to" id="{{id}}_send_to_friends_to"  class="textarea-2 send_to_friends_to required-emails" style="height:58px" placeholder="To"></textarea>
  </div>
  <div class="input-row padding form-group">
    <input type="text" name="from" id="{{id}}_send_to_friends_from" class="text-input-4 required block text_long send_to_friends_from" value="{{user_fullname}}" placeholder="From" data-error-message="You must specify who the special is from." />
  </div>
 <div class="input-row padding form-group">
    <textarea name="note" id="{{id}}_send_to_friends_note" class="textarea-2 send_to_friends_note" placeholder="Note"></textarea>
    <input type="hidden" name="id" id="{{id}}_send_to_friends_id" value="{{id}}"/>
    <input type="hidden" name="a" id="{{id}}_send_to_friends_action" value="send_{{type}}_to_friend" />
  </div>
 <div class="input-row padding">
  <input type="submit" id="{{id}}_send-to-a-friend-button" class="button-1 size-minus-1 send-to-a-friend-button" value="SEND" />
  <span id="{{id}}_send_to_friends_status"></span>
 </div>


</form>
</script>

