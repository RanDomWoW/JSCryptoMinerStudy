            <script>
                function makeStreamingSwipe (day) {
                    var data = $('div[data-day="'+day+'"] > .swiper-slide').clone();
                    var active = $('div[data-day="'+day+'"] > .swiper-slide.live').index();
                    var activeIndex = 0;
                    if (active != '-1') {
                        activeIndex = active;
                    }
                    if (data.length > 0) {
                        $('.streamingswipe .swiper-wrapper').hide();
                        $('.streamingswipe .swiper-wrapper').empty().append(data);
                        $('.streamingswipe .swiper-wrapper').show('fast');
                        if (typeof streamingswipe != 'undefined') {
                            streamingswipe.update();
                        } else {
                            var streamingswipe = new Swiper('.streamingswipe', {
                                slidesPerView: 4,
                                spaceBetween: 1,
                                autoHeight: true,
                                paginationClickable: true,
                                nextButton: '.nextstreamingswipe',
                                prevButton: '.prevstreamingswipe',
                                breakpoints: {
                                    1280: {
                                        slidesPerView: 3,
                                    },
                                    1023:{
                                        slidesPerView: 2,
                                    },
                                    980: {
                                        slidesPerView: 2,
                                    },
                                    767: {
                                        slidesPerView: 1,
                                    },
                                    425: {
                                        slidesPerView: 2,
                                    }
                                }
                            });
                        }
                        streamingswipe.slideTo(activeIndex);
                    }
                }
                $(document).ready(function () {
                    var today = '2018-03-19';
                    makeStreamingSwipe(today);
                    $('.streaming-dropdown a').click(function (event) {
                        $(this).closest('.dropdown.white').find('.d-title').text($(this).text());
                        event.preventDefault();
                        var day = $(this).data('day');
                        makeStreamingSwipe(day);
                    });
                });

            </script>
