                items: 1,
                lazyLoad: true,
                dotsEach: false
            });

            var secondBannerInterval;
            var secondBanner = $("#SecondBanner");
            secondBanner.owlCarousel({
                loop: true,
                margin: 10,
                lazyLoad: true,
                responsive: {
                    0: {
                        items: 2,
                        dots: false
                    },
                    740: {
                        items: 3
                    }
                }
            }).mousemove(function () {
                window.clearInterval(secondBannerInterval);
            }).mouseout(function () {
                secondBannerInterval = setInterval(function () {
                    secondBanner.trigger('next.owl.carousel');
