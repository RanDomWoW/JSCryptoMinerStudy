                myNextSec = window.setTimeout(function () { owl.trigger('next.owl.carousel'); NextSec(true); }, Number(timeout_img) /*console.log(setSilderBarSec + "," + aa[setSilderBarSec]); , aa[setSilderBarSec]*/);
            };
            function settingCenterDialog() {
                if (awardDialog) awardDialog.dialog("option", "position", { my: "center", at: "center", of: window });
            };
            owl.mousemove(
                function () {
                    window.clearTimeout(myNextSec);
                }).mouseout(
                function () {
                    NextSec(false);
                });
            NextSec(true);
        });
