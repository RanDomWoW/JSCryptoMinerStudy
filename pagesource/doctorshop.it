    function LoadSummaryCart() {
        var carrello = $("#summarybodycart");
        cbsummarycart.PerformCallback();
        cbsummarycart2.PerformCallback();

        $.ajax({
            cache: false,
            type: "POST",
            url: "/Services/service.asmx/GetProductCart",
            data: {},
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {                
                var pList = JSON.parse(data.d);
                if (pList.length > 0) {
                    ShowSummary(1);
                    carrello.text("");
                    $.each(pList, function (id, product) {
                        var template = $('#summaryrowcart').html();
                        var html = Mustache.render(template, product);
                        carrello.append(html);
                    });
                    
                }
                else {
                    ShowSummary(0);
                }
            },
            error: function (xhr, ajaxOptions, thrownError) {
                
            }
        });

    }
    </script>
