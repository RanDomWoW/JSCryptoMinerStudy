        function GetPromo()
        {
            $.ajax({
                cache: false,
                type: "POST",
                url: "/Services/service.asmx/GetProdottiPromozioniHP",
                data: {},
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: OnSuccessPromo,
                error: function (xhr, ajaxOptions, thrownError) {
                    
                }
            });
        }

        function OnSuccessPromo(response) {
            $('#loader_promo').hide();
            var table = $("#owl-product");
            var pList = JSON.parse(response.d);
         
                var source = $('#BoxPromotions').html();
                var template = Handlebars.compile(source);

                Handlebars.registerHelper('ifCond', function (v1, operator, v2, options)
                {
                    switch (operator) {
                        case '==':
                            return (v1 == v2) ? options.fn(this) : options.inverse(this);
                        case '===':
                            return (v1 === v2) ? options.fn(this) : options.inverse(this);
                        case '!=':
                            return (v1 != v2) ? options.fn(this) : options.inverse(this);
                        case '!==':
                            return (v1 !== v2) ? options.fn(this) : options.inverse(this);
                        case '<':
                            return (v1 < v2) ? options.fn(this) : options.inverse(this);
                        case '<=':
                            return (v1 <= v2) ? options.fn(this) : options.inverse(this);
                        case '>':
                            return (v1 > v2) ? options.fn(this) : options.inverse(this);
                        case '>=':
                            return (v1 >= v2) ? options.fn(this) : options.inverse(this);
                        case '&&':
                            return (v1 && v2) ? options.fn(this) : options.inverse(this);
                        case '||':
                            return (v1 || v2) ? options.fn(this) : options.inverse(this);
                        default:
                            return options.inverse(this);
                    }
                });

                table.append(template({ objects: pList }));
     
            
                $('#owl-product').owlCarousel({
                    items: 5,
                    lazyLoad: true,
                    loop: true,
                    margin: 10,
                    nav: true,
                    responsiveClass: true,
                    responsive: {
                        0: {
                            items: 1,
                            nav: false
                        },
                        600: {
                            items: 3,
                            nav: true
                        },
                        1000: {
                            items: 5,
                            nav: true
                        }
                    },
                    navText: ['<span style="font-size:41px;"><</span>', '<span style="font-size:41px;">></span>']
                });

        }

        function GetProdottiEvidenza() {
            $.ajax({
                cache: false,
                type: "POST",
                url: "/Services/service.asmx/GetFeaturedProducts",
                data: {},
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: OnSuccessProdEvidenza,
                error: function (xhr, ajaxOptions, thrownError) {
                    
                }
            });
        }

        function OnSuccessProdEvidenza(response) {
            $('#loader_evidenza').hide();
            var table = $("#owl-product1");
            var pList = JSON.parse(response.d);

            var source = $('#BoxEvidenza').html();
            var template = Handlebars.compile(source);

            table.append(template({ objects: pList }));

            $('#owl-product1').owlCarousel({
                items: 5,
                lazyLoad: true,
                loop: false,
                margin: 10,
                nav: true,
                responsiveClass: true,
                responsive: {
                    0: {
                        items: 1,
                        nav: false
                    },
                    600: {
                        items: 3,
                        nav: true
                    },
                    1000: {
                        items: 5,
                        nav: true
                    }
                },
                navText: ['<span style="font-size:41px;"><</span>', '<span style="font-size:41px;">></span>']
            });
        }

        function GetCategorieEvidenza() {
            $.ajax({
                cache: false,
                type: "POST",
                url: "/Services/service.asmx/GetElencoCategorieInEvidenza",
                data: {},
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: OnSuccessCatEvidenza,
                error: function (xhr, ajaxOptions, thrownError) {
                    
                }
            });
        }

        function OnSuccessCatEvidenza(response) {
            $('#loader_Catevidenza').hide();
            var table = $("#catEvidenza");
            var pList = JSON.parse(response.d);
            var source = $('#BoxCatEvidenza').html();
            var template = Handlebars.compile(source);

            

            table.append(template({ objects: pList }));
        }

