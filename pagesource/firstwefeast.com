<script type="text/javascript">
setTimeout(function(){
  // cache element selectors  
  var newsletter_feed_input = document.querySelectorAll('.newsletterEmail')[1];
  var newsletter_feed_submit = document.querySelectorAll('.newsletter-submit')[1];
  
  function _validateEmail(email) {
    var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(email);
  }
  
  function _resetEmailInput(message){
    newsletter_feed_input.value = '';
    newsletter_feed_input.placeholder = message;
  }


  (function() {
    newsletter_feed_submit.addEventListener('click', function(){
      var email = newsletter_feed_input.value;
      if( !_validateEmail(email) ) {
        _resetEmailInput('Not Valid Email Address');
      } else {
        signup(email);
      }
    });
 })();
  
  function signup(email) {
    try {
      HIVE_SDK( 'emailSignup', 
        {email: email}, 
        function(data){  // Success Callback, data.user contains info about the currently auth'ed user
          console.log('hive.co signup success: ',data);
          _resetEmailInput('Success! You\'ve been signed up.');
        },
        function(data){  // Error Callback, data contains error information
          console.log('hive.co sdk callback error: ', data);
          _resetEmailInput('Sorry. Please try again later.');
        }
      );
    } catch(err){
      console.log('ComplexNewsletter signup error', err);
    }
  }
},2000);
</script>
    
