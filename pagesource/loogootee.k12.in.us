        <script>

            $(document).ready(function ()
            {
                var images = $('#slideshow .slides > img');

                images.wrap('<div class="slideshow-image"></div>');
                var caption = $("<div class='slideshow-caption'></div>");

                $('.slideshow-image').each(function () { $(this).append('<div class="slice-shadow"></div>'); });
                $('.slideshow-image').css('cursor', 'pointer');

                $('#slideshow').append(caption);
                $('#slideshow').append('<div class="topShadow"></div>');


                function redrawSlideshow()
                {
                    var firstSlide = $('#slideshow .slides > div.slideshow-image');

                    firstSlide
						.css({
						    width: '100%',
						    overflow: 'hidden',
						    top: '0',
						    right: 'auto',
						    "z-index": 14
						})
						.removeClass('rotated')
						.find('img').css({
						    width: '100%',
						    height: 'auto',
						    top: 0,
						    'margin-left': '0',
						    position: 'relative',
						    left: "0"
						});

                    var left = $('.masterC').offset().left;

                    firstSlide.find('.slice-shadow').hide();

                    var captionText = firstSlide.find('img').attr('title');

                    if (captionText.indexOf('|') > -1)
                    {
                        var parts = captionText.split('|');
                        captionText = "<div class='caption-line-1'>" + parts[0] + "</div><div class='caption-line-2'>" + parts[1] + "</div>";
                    }

                    caption.html(captionText);

                    caption.css({ 'padding-left': left + 'px' });

                    if (captionText == '') caption.hide();
                    else caption.show();

                    var fi = $('#slideshow .slides > div.slideshow-image > img:first');
                    var height = fi.height();
                    var width = fi.width();


                    if (height > 430)
                    {

                        firstSlide.find('img').css({
                            top: (-(height - 430) / 2) + "px"
                        });

                        height = 430;
                    }

                    caption.find('.caption-line-1').css('font-size', (height * .08) + 'px');
                    caption.find('.caption-line-2').css('font-size', (height * .06) + 'px');
                    console.log(height);
                    $('#slideshow .slides').height(height);

                    var slidesAfter1 = $('.slideshow-image:gt(0)');
                    slidesAfter1
						.css({
						    width: (.28 * height) + 'px',
						    overflow: 'hidden',
						    top: -((height * .05) + 10) + 'px',
						    "z-index": 17
						})
						.addClass('rotated')
						.find('img').css({
						    width: 'auto',
						    height: ((height * 1.5) + 20) + 'px',
						    top: (height * -.2) + 'px',
						    left: -(width / 2.2) + 'px'
						});


                    var slideCount = slidesAfter1.length;

                    caption.css({ 'height': 'auto', 'padding-right': (.28 * height * slideCount) + 'px' });


                    slidesAfter1.each(function (idx, itm)
                    {
                        $(itm).css({ right: (((slideCount - idx - 1) * (.20 * height)) - (.223 * height)) + 'px' });

                        $(itm)
							.find('.slice-shadow').css({
							    left: -(.28 * height) + 'px',
							    width: (.60 * height) + 'px',
							    height: ((height * 1.5) + 20) + 'px',

							}).show();
                    });
                }

                var slideshowInterval = null;
                var redrawCount = 0;

                function startTheSlideshowTimer()
                {
                    slideshowInterval = setInterval(function ()
                    {
                        rotateSlideshow(0);
                        redrawCount++;
                    }, 5000);
                }

                function rotateSlideshow(position)
                {
                    console.log("position: " + position);

                    if (position > 0)
                    {
                        for (var i = 0; i < position; i++)
                        {
                            $('.slideshow-image:first').appendTo("#slideshow .slides");
                        }
                        redrawSlideshow();
                        if (slideshowInterval == null)
                        {
                            startTheSlideshowTimer();
                        }
                        return;
                    }

                    var timeout = 100;

                    $('.slideshow-image:first').fadeTo(timeout, .1, function ()
                    {
                        $(this).appendTo("#slideshow .slides");
                        $('.slideshow-image:first').css({ opacity: .1 }).fadeTo(timeout, 1);

                        $(this).fadeTo(timeout, 1);
                        redrawSlideshow();
                        $('#slideshow').fadeTo(timeout, 1);
                    });
                }

                var to = null;
                $(window).resize(function ()
                {
                    if (to != null) clearTimeout(to);
                    to = setTimeout(redrawSlideshow, 50);
                    redrawCount++;
                });

                $('.slides').on('click', '.slideshow-image', function ()
                {
                    if (slideshowInterval != null)
                    {
                        clearInterval(slideshowInterval);
                        slideshowInterval = null;
                    }
                    var idx = $(this).index();
                    rotateSlideshow(idx);
                });

                var imagesNeedingLoaded = images.length;

                $(images.each(function ()
                {
                    var tmpImg = new Image();
                    tmpImg.onload = function ()
                    {
                        imagesNeedingLoaded--;
                        if (imagesNeedingLoaded === 0)
                        {
                            redrawSlideshow();
                            startTheSlideshowTimer();
                        }
                    };
                    tmpImg.src = $(this).attr('src');
                }));

                $('.newsletters-feature-area > h1.feature-content-title').prepend('<i class="fa fa-file-text-o"></i>');
            });

        </script>
    
