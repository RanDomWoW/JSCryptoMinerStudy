<script type="text/javascript">
    jQuery('.play-video-destaque').click(function(ev){
        
        jQuery(this).fadeToggle();
        jQuery("#video-destaque")[0].src += "&autoplay=1";
        ev.preventDefault();
        jQuery(".video-destaque").addClass('no-hover').addClass('remove-bg');
        jQuery(".video-destaque:before").css('display','none');


    });

    jQuery(document).ready(function($){
        if($(window).width()>265 && $(window).width()<450){
            
            $('#video-destaque').attr('height','170');
        }else if($(window).width()>450 && $(window).width()<769){
            
            $('#video-destaque').attr('height','275');
            $('#video-destaque').attr('width','560');
        }else if($(window).width()>768){
            
            $('#video-destaque').attr('height','483');
             $('#video-destaque').attr('width','855');
        }
    });

    
</script>



<script type="text/javascript">
    
    var iframe           = jQuery('iframe:first');
    var iframe_src       = iframe.attr('src');
    var youtube_video_id = iframe_src.match(/youtube\.com.*(\?v=|\/embed\/)(.{11})/).pop();


    if (youtube_video_id.length == 11) {
        var video_thumbnail = jQuery('<img src="//img.youtube.com/vi/'+youtube_video_id+'/maxresdefault.jpg">');
        jQuery(".play-video-destaque").css("background","url("+video_thumbnail[0].src+")no-repeat center / cover");
        
    }
    
</script>

			
