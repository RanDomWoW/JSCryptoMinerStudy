<script type="text/javascript">
   var blnSliderMouseDown = false;
   document.onmouseup = sliderMouseUp;

   function sliderMouseDown(ctrl, e) {
       blnSliderMouseDown = true;
       sliderMove(ctrl, e);
   }

   function sliderMouseUp(ctrl, e) {
       blnSliderMouseDown = false;
   }

   function sliderMouseMove(ctrl, e) {
       if (blnSliderMouseDown == true) {
           sliderMove(ctrl, e);
       }
   }

   function sliderMove(ctrl, e) {

      if (blncphMainColumn_search_wctlRadiusSliderEnabled) {
      var intSliderPos;
      var txt_Value;
      var ctl_Knob;

      ctl_Knob = document.getElementById(ctrl.id.replace('_divContainer','') + '_ctlKnob');
      txt_Value = document.getElementById(ctrl.id.replace('_divContainer','') + '_hitTextValue');

      if (window.event || !window.event) {
          //IE
          var ctrlParent;
          intSliderPos = e.clientX - ctrl.offsetLeft;
          ctrlParent = ctrl.offsetParent;
          while (ctrlParent) {
              intSliderPos -= ctrlParent.offsetLeft;
              ctrlParent = ctrlParent.offsetParent;
          }
          } else {
              //FF
              intSliderPos = e.clientX - ctrl.offsetLeft;
alert('e.clientX:' + e.clientX + ' ctrl.offsetLeft:' + ctrl.offsetLeft + '  A-B = ' + intSliderPos);          }

      if (intSliderPos > ctrl.offsetWidth) {
          intSliderPos = ctrl.offsetWidth;
      } else if (intSliderPos < 0) {
          intSliderPos = 0;
      }

      intValue = (intSliderPos / ctrl.offsetWidth) * (eval('maxValue' + ctrl.parentNode.id) - eval('minValue' + ctrl.parentNode.id)) + eval('minValue' + ctrl.parentNode.id);
      if (String(intValue).indexOf('.') > 0) {
          txt_Value.value = String(intValue).substring(0,String(intValue).indexOf('.'));
      } else {
          txt_Value.value = String(intValue).substring(0,3)
      }
      ctl_Knob.style.marginLeft = intSliderPos - 7 + 'px';
       }
   }

</script>
<script type="text/javascript">
    var blncphMainColumn_search_wctlRadiusSliderEnabled = true;

    function disableRadiusSlider(strContainer) {
        var hit_TextValue = document.getElementById(strContainer + '_ctl00_hitTextValue');

        document.getElementById(strContainer).className = 'slider disabled';
        hit_TextValue.disabled = true;
        blncphMainColumn_search_wctlRadiusSliderEnabled = false;
    }

    function enableRadiusSlider(strContainer) {
        var hit_TextValue = document.getElementById(strContainer + '_ctl00_hitTextValue');

        document.getElementById(strContainer).className = 'slider';
        document.getElementById(strContainer).parentNode.className = 'radius';
        hit_TextValue.disabled = false;
        blncphMainColumn_search_wctlRadiusSliderEnabled = true;
    }

    function checkLocation(ctlLocation) {
        if (ctlLocation.value.length <= 0) {
            disableRadiusSlider('cphMainColumn_search_wctlRadiusSlider');
        } else {
            enableRadiusSlider('cphMainColumn_search_wctlRadiusSlider');
        }
    }

   var minValuecphMainColumn_search_wctlRadiusSlider = 1;
   var maxValuecphMainColumn_search_wctlRadiusSlider = 100;
</script>
