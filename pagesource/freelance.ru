var offersOnMainpage = {
    parentBlock : '.offers-on-mainpage',
    init: function () {
        if(!$(this.parentBlock).exists()) return;
        $.ajax({
            url: '/offer/to/main',
            dataType: 'html',
            success: function(result) {
                $(offersOnMainpage.parentBlock)
                    .addClass('well-gray')
                    .html(
                        $('<section class="container" style="margin-top:-10px; margin-bottom:40px"/>')
                        .append('<div class="row"><div class="col-xs-12"><h2 style="margin-top:70px; margin-bottom:40px"><a href="/offer"  style="color:black; text-decoration: none;"><b>Специальные предложения</b></a></h2></div></div>')
                        .append( $(result)).append('<div class="text-center" style="margin-top:20px; margin-bottom:40px"><a href="/offer" class="btn btn-link text-center">Все cпециальные предложения</a></div>')
                    )
                ;
            }
        });

    }
}

$(function(){ offersOnMainpage.init();});
