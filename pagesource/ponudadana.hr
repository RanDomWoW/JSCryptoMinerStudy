						return;
				    }
				    var minLength = 8;
				    var regex = [];
				    regex.push("[a-zA-Z]"); // lowercase & uppercase alphabet
				    regex.push("[0-9]"); // digits
				    regex.push("[$@$!%*#?&/\\()]"); // special characters.

				    var passed = 0;
				    for (var i = 0; i < regex.length; i++) {
						if (new RegExp(regex[i]).test($(this).val()) && $(this).val().length >= minLength) {
						    passed++;
						}
				    }

				    var color = "";
				    var strength = "";
				    switch (passed) {
						case 0:
						case 1:
						    strength = 'NEDOVOLJNO <i class="fa fa-times" aria-hidden="true"></i>';
						    color = "red";
						    break;
						case 2:
						    strength = 'DOVOLJNO <i class="fa fa-check" aria-hidden="true"></i>';
						    color = "green";
						    break;
						case 3:
						    strength = 'ODLIČNO <i class="fa fa-check" aria-hidden="true"></i>';
						    color = "green";
						    break;
				    }
				    $("#pr-password-strength").html(strength);
				    $("#pr-password-strength").css("color", color);
				});
			});
			
		}	

		if(offerRenderer == null) {
			var offerRenderer = new OfferRenderer();
		}
