				var offerUrl = offer.offerUrl + (cat == 'popular' || cat == 'new' ? '?' + cat + '=1' : '');
				var imgUrl = layout.img[c] ? 
						window.imagesBaseUrlS + 'images/offers/' + offerId + '/main-' + layout.img[c] + '.jpg?v=' + (Math.floor(Math.random() * 100000) + 900) :
						offer.mainImage.imgUrl;
				
				html += '<div class="' + layout.css[c] + '">';
				html += '	<div class="offer" ' + (offer.hideDI == 0 ? 'data-discount-per="' + offer.discountPer + '"' : 'data-discount-per="0"') + '>';
