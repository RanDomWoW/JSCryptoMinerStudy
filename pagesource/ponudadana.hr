		function initCarousels() {
			$('.carousel').carousel({
			    interval: false,
			    pause: true
			});
	
			$('.carousel').on('slide.bs.carousel.empty', function(event) {			
				
				var cat = $(event.target).data('cat');
				var catId = $(event.target).data('cat-id');
	
				var direction = event.direction == 'left' ? 'next' : 'prev';				
	
				if(!activeOffersByCategory[cat]) {
					
					var listener = activeOffersLoaded;
					
					listener.cat = cat;
					listener.catId = catId;
					listener.direction = direction;
					listener.first = true;
	
					var offerManager = new OfferManager(null, 360000);
					
					offerManager.addOfferListener(listener);					
					
					showLoadingSpinner(true, '#carousel-' + catId);					
					
