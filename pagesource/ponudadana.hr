						offerManager.loadActiveOffers(1, 0, -1, 0, 200);
					} else {
						offerManager.loadActiveOffers(1, cat);
					}			
				} 
			});
			
			
			$('.carousel').on('slide.bs.carousel', function(event) {
							
					var cat = $(event.target).data('cat');
					var catId = $(event.target).data('cat-id');
	
					var direction = event.direction == 'left' ? 'next' : 'prev';					
	
					$(event.currentTarget).addClass("animated fadeIn").one('animationend webkitAnimationEnd oAnimationEnd', function() {
