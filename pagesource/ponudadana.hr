				html += '				<h2>' + offer.indexTitle + '</h2>';

				var subTitle = offer.indexSubTitle ? offer.indexSubTitle : offer.subTitle;
				
				html += '				<p>' + subTitle + '</p>';						
				html += '				<div class="price">';			
				if(offer.hideDI == 0) {
					html += '					<span class="regular-price">' + format_money(offer.totalPrice, 0, '', '.') + offer.currencyShortName + '</span>';
				}
				html += '					<span class="reduced-price">' + format_money(offer.reducedPrice, 0, '', '.') + offer.currencyShortName + '</span>';
				html += '				</div>';
				html += '			</figure>';
				html += '		</a>';
				html += '	</div>';
				html += '</div>';
			}
			html += '	</div>';

			var elem = $("#carousel-" + id).find('.carousel-inner')[0];
			elem.style.height = elem.offsetHeight + "px";
			
			$("#carousel-" + id).find('.carousel-inner').append(html);

