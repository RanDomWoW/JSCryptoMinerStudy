					gaTrackEvent('home-slider-' + $(this).data('cat'), 'click', 'prev', $(this).data('cat'), false);
				});

				$('.right').on('click', function() {
					gaTrackEvent('home-slider-' + $(this).data('cat'), 'click', 'next', $(this).data('cat'), false);
				});
			}

			$("img.lazy").lazyload({ threshold : 200, data_attribute: windowManager.isMobile() ? 'original-mobile' : 'original' });

			var url = new Url(document.location.href);
			var regAct = url.GetParam('rgt-act');
			if(regAct == null) {
				regAct = url.GetParam('reg-act');
			}

			if(regAct == 'subscription-confirmed') {
				SUBSCRIBE_POPUP_ENABLED = false;
				subscriptionManager.showSubscriptionConfirm(url.GetParam('id'));
			} else if(regAct == 'confirmed') {
				SUBSCRIBE_POPUP_ENABLED = false;
				userManager.showRegistrationConfirmForm();
			} else if(regAct == 'unsubscribe') {
				SUBSCRIBE_POPUP_ENABLED = false;
				subscriptionManager.showUnsubscribeForm(url.GetParam('id'), url.GetParam('bcid'), url.GetParam('bvar'));
			} else if(regAct == 'change-subscription') {
				SUBSCRIBE_POPUP_ENABLED = false;				
				subscriptionManager.showSubscriptionSuccess(url.GetParam('email'), url.GetParam('id'), url.GetParam('source'), 1, url.GetParam('cat'));
			} else if(regAct == 'register' && !userManager.isLoggedIn()) {
				userManager.register();
			} else if(regAct == 'login' && !userManager.isLoggedIn()) {
				userManager.login(url.GetParam('targetUrl'));		
			} else if(regAct == 'lost-password') {
				userManager.sendPassword();
			} else if(regAct == 'subscribe') {
				SUBSCRIBE = url.GetParam('rgt-dat');
				if(SUBSCRIBE == null) {
					SUBSCRIBE = url.GetParam('reg-dat');
				}
				subscriptionManager.openSubscribeForm('fb-invitation-share', 100);
			} else if(regAct == 'change-subscription-type') {	
				var sid = url.GetParam('id');  
				subscriptionManager.showUnsubscribeForm(sid, '', '', function() {
					subscriptionManager.changeSubscriptionType(sid, 'WEEKLY');
				});				
			} else if(regAct == 'update-unsubscribe-reason') {
				subscriptionManager.showUnsubscribeNotification(url.GetParam('id'), function() {
					$("#unsubscribe-reason-intro").html("Prije nego odeš, molimo reci nam razlog otkazivanja:<br/>");
				});
			} else if(regAct == 'notify-me') {
				var notifActivity = url.GetParam('nm_activity');
				var notifOfferIds = url.GetParam('nm_oids');				
				var notifContactBy = url.GetParam('nm_contact_by');
				subscriptionManager.notifyMeSimilarOffers(notifActivity, notifOfferIds, notifContactBy, function(response) {
					subscriptionManager.processNotifyMeResponse(response);			
				});
			} else if(regAct == 'unsubscribe-message-subscription') {
				SUBSCRIBE_POPUP_ENABLED = false;
				subscriptionManager.unsubscribeMessageSubscription(url.GetParam('id'));
			}

			var offerManager = new OfferManager(null, null);
			procManager.addListener('user', {
				event: function(eventType, initial, value) {
					var user = value;
					var userId = user != null ? user.userId : null;
					if(userId == null) {
						userId = readCookie("puid");
					}
						
					if(userId != null) {
						offerManager.showRecommendationsRow('recommendations', 'recommendation', 'front-page');
					}
				}
			});

			if(window.location.href.indexOf('/ponude') > -1 || window.location.pathname === '/') {
				$('.newOffer').each(function(i, elem) {
					$(elem).attr('href', $(elem).attr('href') + '?new=1');
				});
			}
			$('.topOffer').each(function(i, elem) {
				$(elem).attr('href', $(elem).attr('href') + '?popular=1');
			});
		});

		deleteCookie("listingSet");
		setCookieShort("listingSet", "cat", 5);
				
