<script type="text/javascript">
                    
	var url = new Url(document.location.href);
	if(url.GetParam('odaberiLokaciju')) {
		$(document).ready(function() {
			 toggle_choose_location('fast');
		});
	}

	function get_selected_region() {
		var region = $("#current-region").val();
		return region.split("_");
	}

	function get_selected_quart() {
		var quart = $("#current-quart").val();
		return quart.split("_");
	}

	function get_selected_category() {
		var category = $("#current-category").val();
		return category.split("_");
	}
	
	$(document).ready(function() {
		$("#current-region").change(function() {
			filter_changed();
		});

		$("#current-category").change(function() {
			filter_changed();
		});		

		$("#current-quart").change(function() {
			filter_changed();
		});	
	});

	function filter_changed() {
		var region = get_selected_region()[0];
		var quart = get_selected_quart()[0];
		var category = get_selected_category()[0];
		
		switch_location(quart != DEFAULT_CITY && region == DEFAULT_CITY ? quart : region, category);
	}

	var infowindow = null;
	function ActiveOffersListener(map) {
		this.map = map;
	}
	ActiveOffersListener.prototype.onActiveOffersLoaded = function(offers) {

		var markers = [];
		for(var offerId in offers.activeOffersById) {
			var offer = offers.activeOffersById[offerId];
			
			for(var i=0; i<offer.locations.length; i++) {
				var location = offer.locations[i];

				var contentString = '<div class="offer transform-none">' +
					'<a href="' + offer.offerUrl + '" target="_blank">' +
						'<figure>' +
							(offer.hideDI == 0 ? '<span class="discount-star-s60">-' + offer.discountPer + '%</span>' : '') +
							'<img src="' + offer.mainImage.imgUrl + '" style="float: left; width: 90px!important;" alt="">' +					
							'<h2 style="font-size: 16px; padding: 0 8px 2px 8px;">' + offer.title + '</h2>' +
							'<p style="font-size: 12px;">' + offer.subTitle + '</p>' +						
							'<div class="price">';
				if(offer.totalPrice > 0) {			
					contentString += '<span class="regular-price">' + offer.totalPrice + offer.currencyShortName + '</span>';
				}
				contentString += '<span class="reduced-price">' + offer.reducedPrice + offer.currencyShortName + '</span>' +
							'</div>' +
						'</figure>' +
					'</a>' +
				'</div>';

				var point = new mxn.LatLonPoint(location.latitude, location.longitude);
		    	var marker = new mxn.Marker(point);
		    	var options = {
		    			infoBubble: contentString,
		    			icon: '/img5/marker.png'
				};

				marker.addData(options);
		    	this.map.addMarker(marker);	    	
		    	markers[markers.length] = marker;

			}	
					
		}		
	};

	function toggle_choose_location(speed) {

		if($("#choose-location-header").is(":visible")) {
			$("#choose-location-header").hide();
			$("#choose-location-body").hide();
		} else {	

			$("#choose-location-header").show();
			$("#choose-location-body").show();

			if(locationManager.isMapsLoaded) {

				var region = get_selected_region()[1];
				var quart = get_selected_quart()[1];
				var category = get_selected_category()[1];

				var coords = locationManager.getCurrentCityCoordinates().split(",");

				var point = new mxn.LatLonPoint(coords[0], coords[1]);

				var map = new mxn.Mapstraction(document.getElementById('choose-location-map'), locationManager.provider);
				var config = locationManager.getConfig();
				map.addControls(config.controls);
				map.setCenterAndZoom(point, parseInt(coords[2]));
				map.enableScrollWheelZoom();
				
				var offerManager = new OfferManager(null, 0);
				offerManager.addOfferListener(new ActiveOffersListener(map));			
				offerManager.loadActiveOffers(quart > 1 ? quart : region, category);	
				
									
			} else {

				locationManager.apiKey = 'AIzaSyD_upjmJTQXshLMzU_-CQZJydU48V2AJ_w';	
				var provider = locationManager.OPENLAYERS;
				locationManager.initMaps(provider, function() {
					mapLoaded = true;

					var region = get_selected_region()[1];
					var quart = get_selected_quart()[1];
					var category = get_selected_category()[1];

					var coords = locationManager.getCurrentCityCoordinates().split(",");

					var point = new mxn.LatLonPoint(coords[0], coords[1]);

					var map = new mxn.Mapstraction(document.getElementById('choose-location-map'), provider);
					var config = locationManager.getConfig();
					map.addControls(config.controls);
					map.setCenterAndZoom(point, parseInt(coords[2]));
					map.enableScrollWheelZoom();

					var offerManager = new OfferManager(null, 0);
					offerManager.addOfferListener(new ActiveOffersListener(map));			
					offerManager.loadActiveOffers(quart > 1 ? quart : region, category);	
				});
			}
		}
	}

	function switch_location(region, category) {
		
		if(region == DEFAULT_CITY) {
			if(category == '') {
				region = '/';
			} else {
				region = '';
			}
		} else {
			region = '/' + region;
		}
				
		var url = region + category + '?odaberiLokaciju=1';
		console.log('Open: ' + url);
		document.location = url;		
	}

	$(function() {


		var category = get_selected_category()[1];		
		if(category == CATEGORY_TRAVEL) {
			$("#term-search").show();
		} else {
			$("#term-search").hide();
		}

		$("#search-df-top").DatePicker({
			date: new Date(),
			flat: false,
			mode: 'single',
			onChange: function(formated, dates){
				var d = addLeadingZero(dates.getDate()) + "." + addLeadingZero(dates.getMonth() + 1) + "." + dates.getFullYear() + ".";
				$("#search-df-top").val(d);
				$(this).hide();					

				var dt = $("#search-dt-top").val();
				if(dt == '') {
					$('#search-dt-top').DatePickerSetDate(dates, true);
				}
			}				
		});
		$("#search-dt-top").DatePicker({
			date: new Date(),
			flat: false,
			mode: 'single',
			onChange: function(formated, dates){
				var d = addLeadingZero(dates.getDate()) + "." + addLeadingZero(dates.getMonth() + 1) + "." + dates.getFullYear() + ".";
				$("#search-dt-top").val(d);
				$(this).hide();
			}				
		});
	});
</script>

