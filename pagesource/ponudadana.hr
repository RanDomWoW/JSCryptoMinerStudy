					if($("#carousel-" + catId).find('.active').nextAll().length >= 0 && direction == 'prev') {
						$("#carousel-" + catId).find('.right').removeClass('arrow-inactive');
					}			
	
					if(direction == 'next' && activeOffersByCategory[cat]) {
						renderCarouselItem(catId, cat, direction);						
					}					
			});
		}
		
		function renderCarouselItem(id, cat, direction, first, renderOnly) {
	 		if(direction != 'next') return;
	
			if( ! activeOffersByCategory[cat]) { console.log('active offers not found'); return; }
			
			var layouts = [
	               { css: ['col-sm-6', 'col-sm-6'], img: ['580x176', '580x176'] },
	               { css: ['col-sm-3', 'col-sm-3', 'col-sm-6'], img: ['', '', '580x176']},
