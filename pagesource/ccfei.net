    var submitflag = 1;

    function DisableA(id) {
        var aId = document.getElementById(id);
        aId.href = 'javascript:void(0);';
        aId.target = '';
        aId.style.color = 'gray';
    }
    function DisableANew(arr) {
        for (i = 0; i < arr.length; i++) {
            var aId = document.getElementById("column_" + arr[i]);
            aId.href = 'javascript:void(0);';
            aId.target = '';
            aId.style.color = 'gray';
        }
    }
    function search() {
        var _key = document.getElementById('key');
        if (_key.value.replace(' ', '') == "") {
            _key.focus();
        }
        else {
            location.href = '/article/search.aspx?title=' + encodeURIComponent(_key.value);
        }
    }

    $(document).ready(function () {
        $("#ctl00_headers_txtAccountCode").keyup(function () {
            submitflag = 2;
        });
        $("#ctl00_headers_txtAccountPwd").keyup(function () {
            submitflag = 2;
        });
        $("#key").keyup(function () {
            submitflag = 1;
        });

        var lock = false;
        $("#imgHeaderLogin").click(function () {
            if (lock)
                return false;
            var code = document.getElementById("ctl00_headers_txtAccountCode").value.trim();
            if (code == "") {
                lock = true;
                alert("Please type in User Name");
                $("#ctl00_headers_txtAccountCode").focus();
                setTimeout(function () { lock = false; }, 200);

                return false;
            }
            var pwd = document.getElementById("ctl00_headers_txtAccountPwd").value.trim();
            if (pwd == "") {
                lock = true;
                alert("Please type Password");
                $("#ctl00_headers_txtAccountPwd").focus();
                setTimeout(function () { lock = false; }, 200);
                return false;
            }
            $.ajax({
                type: "POST",
                url: "/ajax/UserInfo.asmx/MyUserLogin",
                data: "{ 'user': '" + escape(code) + "', 'pwd': '" + escape(pwd) + "', 'contentflag': '1' }",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    var model = $.parseJSON(msg.d);
                    if (model.Flag == "100") {
                        window.location.reload();
                    }
                    else {
                        lock = true;
                        alert(model.Content);
                        setTimeout(function () { lock = false; }, 200);
                    }
                    return false;
                },
                error: function (x) {
                    alert(x.responseText);
                }
            });
        });

        $("#btnSearch").on("click", function () {
            search();
        });
        $('#key').keydown(function (event) {
            if (event.keyCode == 13)
                search();
        })
        $(document).keyup(function (e) {
            e = e || event;
            var keycode = e.which || e.keyCode;
            if (keycode == 13) {
                if (submitflag == 2) {
                    $("#imgHeaderLogin").triggerHandler("click");
                    return false;
                }
                else if (submitflag == 1) {
                    $("#key").triggerHandler("click");
                    return false;
                }
                return false;
            }
        });
    });

    function HeaderLogout() {
        $.ajax({
            type: "POST",
            url: "/ajax/UserInfo.asmx/MyUserLogout",
            data: "",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (msg) {
                var res = msg.d;
                if (res == "1" || res == "-1") {
                    window.location.reload();
                }
                else if (res == "0") {
                    alert("You haven’t signed out.\r\nPlease check the net and try again. Please contact us if fail again.");
                }
            },
            error: function (x) {
                alert(x.responseText);
            }
        });
    }
</script>

