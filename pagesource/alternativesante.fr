	$(function(){

		$('#filtres_accueil input[type="checkbox"]').on('change', function() {
			$('#filtres_accueil').submit();
		});


		actualise_categories = function() {
			$('.bloc_categorie').removeClass('_loading');
			$('.bloc_categorie').each(function(index, element){
				setTimeout(function(){
					actualise_categorie($(element));
				},500*index)
			})
		}

		actualise_categorie = function(lien){
			var ancien_href = lien.attr('href');

			espace = $(window).WR('getEspace');
			if(navigator.userAgent.match(/OS 5_[0-9_]+ like Mac OS X/i))
				espace = 'reduit';

			lien.attr('href', ancien_href+'/'+espace+'?cache_page=get_categorie&cache_on=startup');
			lien.trigger('click');
			lien.attr('href', ancien_href);
		}

		actualise_categories();

	})
</script>


	

