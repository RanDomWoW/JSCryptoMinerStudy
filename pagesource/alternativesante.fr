		<script type="text/javascript">

			var cookiesConsent = {
				hasConsent : false,

				checkConsent : function(){
					if(!this.getCookie('hasConsentBanner')){
						this.showBanner();
					}
					if(!this.getCookie('hasConsent')){
						document.cookie = 'hasConsent=true; '+ this.getCookieExpireDate() +' ; path=/'; 
					}
				},

				showBanner : function(){
					var bodytag = document.getElementsByTagName('body')[0];
					var div = document.createElement('div');
					div.setAttribute('id','cookie-banner');
					// Le code HTML de la demande de consentement
					div.innerHTML =  '<div id="cookie-banner-message">En poursuivant votre navigation sur ce site, vous acceptez l\'utilisation de cookies permettant d\'améliorer votre expérience utilisateur. <a href="javascript:cookiesConsent.closeBanner();">Fermer</a><a href="/mentions-legales"> En savoir plus</a></div>';
