    <script type="text/javascript">
        $(document).ready(function () {
            $('#btnMevzuatAra').click(function () {
                MevzuatAra();
            });
            $('#btnDetsisAra').click(function () {
                DetsisAra();
            });
            $('#mevzuatText').keyup(function (e) {
                if (e.keyCode === 13) {
                    MevzuatAra();
                }
            });
            $('#tbxSearch').keyup(function (e) {
                if (e.keyCode === 13) {
                    DetsisAra();
                }
            });

            $.ajax({
                type: "POST",
                url: "Anasayfa.aspx/DuyuruYukle",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: true,
                success: function (data) {
                    if (data.d) {
                        var jj = $.parseJSON(data.d);
                        $('#duyurular').html('');
                        $.each(jj, function (value, obj) {
                            var duyuru = "<li><a href=\"Devlet_Teskilat_Duyurulari/" + obj.ID + " \">" + obj.Baslik + " </a></li>"
                            $('#duyurular').append(duyuru);
                        });
                    }
                }
            });


        });




        function DetsisAra() {
            var names = [];
            var wordToSearch = $("#tbxSearch").val().trim();
            if (wordToSearch.length == 0) {
                alert("Aramak istediğiniz Kelime/Kelimeleri arama kutusuna yazınız");
            } else {
                var base = window.location.origin;
                window.location = base + "/Devlet_Teskilat_Detsis/0/" + encodeURIComponent(wordToSearch.replace('.', '|'));
            }
            return false;

        }



        function MevzuatAra() {
            var mevzuatAd = $('#mevzuatText').val();
            if (mevzuatAd == "" || mevzuatAd.length < 3) {
                $('#cvmevzuatText').removeClass("hidden");
            } else {
                $('#cvmevzuatText').addClass("hidden");
                var turs = $('#ddlKmsTur option:selected');
                var secilen = [];
                $(turs).each(function (value, tur) {
                    secilen.push([$(this).val()]);
                });

                if (secilen.length == 0) {
                    window.location = 'https://kms.kaysis.gov.tr?mevzuatAd=' + mevzuatAd;
                } else {
                    window.location = 'https://kms.kaysis.gov.tr?mevzuatAd=' + mevzuatAd + '&turs=' + secilen;
                }
            }
        }

        var prevExpandingNode;
        var prevExpandingNodeSrc;
        var prevExpandingNodeAlt;
        function OnExpandClick(evt) {
        }


        function alwaysLoad(para) {
            if ($('#ddlKmsTur option').size() == 0) {
                $.ajax({
                    type: "POST",
                    url: "Anasayfa.aspx/KMSTurYukle",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    async: false,
                    success: function (data) {

                        if (data.d) {
                            var jj = $.parseJSON(data.d);
                            $('#ddlKmsTur').empty();

                            $.each(jj, function (value, obj) {

                                $('#ddlKmsTur').append(new Option(obj.Ad, obj.ID));


                            });
                        }
                    }
                });
            }


            $(".childnode, .mainnode").click(function (evt) {
                //  evt = evt || window.event; // event object 
                var obj = evt.target || window.event.srcElement; // event target
                if (obj.tagName.toLowerCase() != "img") {
                    if (obj.attributes['data-id'].value > 10000) {
                        $(".tooltip").tooltip('hide');
                        var detsisno = obj.attributes['data-id'].value;
                        $.ajax({
                            type: "POST",
                            url: "anasayfa.aspx/MainSearch",
                            data: '{"detsisno" : ' + detsisno + '}',
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                            async: true,
                            success: function (data) {
                                if (data.d.length > 2) {
                                    var birim = $.parseJSON(data.d);
                                    $(".DetsisTreviewYan").empty();
                                    $(".DetsisTreviewYan").removeClass("hide");
                                    var hf = $get('hfSelectedID');
                                    hf.value = obj.attributes['data-id'].value;
                                    var anaBilgiler = "<div class=\"panel panel-info\"><div class=\"panel-heading\"><h2 class=\"panel-title\">" + birim[0].Ad + "</h2></div>";
                                    anaBilgiler += "<div class=\"panel-body\"> <div class=\"list-group\">"
                                    anaBilgiler += "  <a href=\"#\" class=\"list-group-item\"><span style=\"width:420px;font-weight:bold\"> DETSİS No:</span> " + birim[0].ID + "</a>";
                                    anaBilgiler += "  <a href=\"#\" class=\"list-group-item\"><span style=\"width:420px;font-weight:bold\"> Kurum Hiyerarşisi:</span> " + birim[0].KurumHiyerarsik + "</a>";
                                    anaBilgiler += "</div>";
                                    anaBilgiler += "<div class=\"list-group\">";
                                    anaBilgiler += "<a href=\"#\" class=\"list-group-item\"><span style=\"min-width:220px;font-weight:bold\"> Yönetici:</span> " + (birim[0].Yonetici === undefined ? "" : birim[0].Yonetici) + "</a>";
                                    if (birim[0].TuzelKisilik != null) {
                                        anaBilgiler += "<a href=\"#\" class=\"list-group-item\"><span style=\"min-width:220px;font-weight:bold\"> Tüzel Kişilik:</span> " + birim[0].TuzelKisilik + "</a>";
                                    }

                                    if (birim[0].ButceTuru != null) {
                                        anaBilgiler += "<a href=\"#\" class=\"list-group-item\"><span style=\"min-width:220px;font-weight:bold\"> Bütçe Türü :</span> " + birim[0].ButceTuru + "</a>";
                                    }
                                    if (birim[0].Tip1Ad == birim[0].Tip2Ad) {
                                        anaBilgiler += "<a href=\"#\" class=\"list-group-item\"><span style=\"min-width:220px;font-weight:bold\"> Kategori/Statü:</span> " + (birim[0].Tip1Ad === undefined ? "" : birim[0].Tip1Ad) + "</a>";
                                    } else {
                                        anaBilgiler += "<a href=\"#\" class=\"list-group-item\"><span style=\"min-width:220px;font-weight:bold\"> Kategori:</span> " + (birim[0].Tip1Ad === undefined ? "" : birim[0].Tip1Ad) + "</a>";
                                        anaBilgiler += "<a href=\"#\" class=\"list-group-item\"><span style=\"min-width:220px;font-weight:bold\"> Statü:</span> " + (birim[0].Tip1Ad === undefined ? "" : birim[0].Tip2Ad) + "</a>";
                                    }
                                    anaBilgiler += "</div>";
                                    anaBilgiler += " <div class=\"list-group\">";
                                    $.each(birim[0].Iletisim, function (key, value) {
                                        anaBilgiler += " <a href=\"#\" class=\"list-group-item\"><span style=\"min-width:220px;font-weight:bold\"> " + value.GorunenAd + ":</span> " + (value.Iletisim === undefined ? "--" : value.Iletisim) + "</a>";
                                    });
                                    anaBilgiler += "</div>";
                                    anaBilgiler += "<div id=\"divButtons\" style=\"text-align:center\"><button style=\"margin-right:5px\" onclick=\"return false\" class=\"btn btn-sm btn-danger\" data-toggle=\"modal\" data-target=\"#detsisModal\" data-trtab=\"#kMevzuat\" data-all=\"" + birim[0].HiyerarsiSirasi + "_" + birim[0].Ad + "_" + birim[0].ID + "\">Kuruluş Mevzuatı</button>";
                                    anaBilgiler += "<button style=\"margin-right:5px\" onclick=\"return false\" class=\"btn btn-sm btn-danger\" data-toggle=\"modal\" data-target=\"#detsisModal\" data-trtab=\"#hizmetler\" data-all=\"" + birim[0].HiyerarsiSirasi + "_" + birim[0].Ad + "_" + birim[0].ID + "\">Hizmetler</button>";
                                    anaBilgiler += "<button style=\"margin-right:5px\" onclick=\"return false\" class=\"btn btn-sm btn-danger\" data-toggle=\"modal\" data-target=\"#detsisModal\" data-trtab=\"#belgeler\" data-all=\"" + birim[0].HiyerarsiSirasi + "_" + birim[0].Ad + "_" + birim[0].ID + "\">Belgeler</button>";
                                    anaBilgiler += "<button style=\"margin-right:5px\" onclick=\"return false\" class=\"btn btn-sm btn-danger\" data-toggle=\"modal\" data-target=\"#detsisModal\" data-trtab=\"#mevzuat\" data-all=\"" + birim[0].HiyerarsiSirasi + "_" + birim[0].Ad + "_" + birim[0].ID + "\">Mevzuat</button>";
                                    anaBilgiler += "<button style=\"margin-right:5px\" onclick=\"return false\" class=\"btn btn-sm btn-danger\" data-toggle=\"modal\" data-target=\"#detsisModal\" data-trtab=\"#sdp\" data-all=\"" + birim[0].HiyerarsiSirasi + "_" + birim[0].Ad + "_" + birim[0].AnaKurumIdareKimlikKodu + "\">SDP</button></div>";
                                    anaBilgiler += "</div></div>";
                                    $(".DetsisTreviewYan").append(anaBilgiler);
                                }

                            }
                        });
                        return false;
                    }
                }
                return true;

            });
            $(".DetsisTreview tbody td div").tooltip({ 'container': 'body' });
            var hf = $get('hfSelectedID');
            if (hf.value != '0') {
                $(".DetsisTreviewYan").removeClass("hide");
                var s = $(".DetsisTreviewYan");
                var pos = s.position();
                if (pos.top + 300 < $(window).scrollTop()) {
                    s.addClass("stick");
                }
                $(window).scroll(function () {
                    var windowpos = $(window).scrollTop();
                    // s.html("Distance from top:" + pos.top + "<br />Scroll position: " + windowpos);
                    if (windowpos > pos.top + 342) {
                        s.addClass("stick");
                    } else {
                        s.removeClass("stick");
                    }
                });
            }








            $(".preloader").hide();

            /*MOdal icin */
            $("#detsisModal").on("show.bs.modal", function (e) {
                var trigger = $(e.relatedTarget);
                var trtab = trigger.data("trtab");
                var data_arr = trigger.data("all").split('_');

                var modal = $(this);
                modal.find('.modal-title').text(data_arr[1]);
                modal.find('.modal-body .nav li').each(function () {
                    $(this).removeClass('active');
                    if ($(this).find("a").attr("href") == trtab) {
                        $(this).addClass('active');
                    }
                    $(this).find("a").attr("data-mainid", data_arr[2]);
                });
                modal.find(".modal-body .tab-content div").each(function () {
                    $(this).removeClass('active');
                    if ($(this).attr('id') == trtab.substring(1)) {
                        $(this).addClass('active');
                        //var iframeSrc = $(this).find("iframe").attr("src");

                    }
                    if ($(this).attr('id') == "hizmetler") {
                        $(this).find("iframe").attr("src", "https://envanter.kaysis.gov.tr/KaysisHizmet.aspx?Kurum=" + data_arr[2]);
                    }
                    else if ($(this).attr('id') == "belgeler") {
                        $(this).find("iframe").attr("src", "../../chart/KurumBelgeleriForChart.aspx?kurumId=" + data_arr[2]);
                    }
                    else if ($(this).attr('id') == "mevzuat") {
                        $(this).find("iframe").attr("src", "https://kms.kaysis.gov.tr/Home/Kurum/" + data_arr[2] + "?embed=true");
                    }
                    else if ($(this).attr('id') == "hsys") {
                        $(this).find("iframe").attr("src", "https://www.kaysis.gov.tr/HSYS/KurumHizmetStandartlari.aspx?IdareKimlikKodu=" + data_arr[2]);
                    }
                    else if ($(this).attr('id') == "sdp") {
                        $(this).find("iframe").attr("src", "https://www.kaysis.gov.tr/SDP/KodGoster.aspx?IdareKimlikKodu=" + data_arr[2]);
                    }
                    else if ($(this).attr('id') == "kMevzuat") {
                        $(this).find("iframe").attr("src", "../../chart/KMevzuat.aspx?kurumId=" + data_arr[2]);
                    }
                });
                if (data_arr[0] != "1") {
                    $("#tabHsys").addClass("hide");
                } else {
                    $("#tabHsys").removeClass("hide");
                }

            });

        }

        $(document).ready(function () {
            alwaysLoad();
            Sys.WebForms.PageRequestManager.getInstance().add_endRequest(alwaysLoad);
        });
    </script>
