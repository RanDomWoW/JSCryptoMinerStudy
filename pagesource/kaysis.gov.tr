    <script type="text/javascript">


        function sendForm(parameters) {
            var _name = $("#inputad").val();
            var _email = $("#inputmail").val();
            var _phone = $("#inputtelefon").val();
            var _subject = $("#inputkonu").val();
            var _captcha = $("#inputcaptcha").val();
            if (_name != "" && _email != "" && _phone != "" && _subject != "" && _captcha != "") {
                if (isValidEmailAddress(_email)) {
                    $.ajax({
                        type: "POST",
                        url: "anasayfa.aspx/AcceptForm",
                        data: '{"captcha":"' + _captcha + '", "name": "' + _name + '", "email": "' + _email + '","telefon":"' + _phone + '","subject":"' + _subject + '" }', //'{"data_all" : "mmmm"}',
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",

                        success: function(data) {

                            switch (data.d) {
                            case 2:
                                alert("Mesajınız iletilmiştir.");
                                $(".Contact-Btn").removeClass("Selected");
                                $(".Contact-Form").toggle(200);

                                break;
                            case 11:
                                alert("Mesajınız gönderilirken hata oluştu, lütfen daha sonra tekrar deneyin.");
                                break;
                            case 13:
                                alert("Lütfen resimdeki harfleri doğru giriniz.");

                            default:
                            }


                        },
                        complete: function(data) {

                        },
                        error: function(e, request, settings) {

                            //alert("Verilerde hata oluştu. Lütfen sayfayı yenileyin");
                            //alert(event.responseText);
                            // location.reload();
                        }
                    });
                } else {
                    $("#inputmail").addClass("tbxError");
                }
            } else {
                var o;
                if (_name == "")
                    o = $("#inputad");
                else if (_email == "")
                    o = $("#inputmail");
                else if (_phone == "")
                    o = $("#inputtelefon");
                else if (_subject == "")
                    o = $("#inputkonu");
                else if (_captcha == "")
                    o = $("#inputcaptcha");
                o.addClass("tbxError");
            }

        }
       
    </script>
