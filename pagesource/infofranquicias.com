<script type="text/javascript">
    jQuery(document).ready(function () {
        jQuery("#ctl00_Top1_search_ddlSector1").change(function () {
            getActivitats1();
        });
        jQuery("#ddlActivitat1").change(function () {
            getActividadID1();
        });
        getActivitats1();
    });

    function getActividadID1() {
        var strAct = jQuery("#ddlActivitat1").val();
        jQuery("#ctl00_Top1_search_txtActividad1").val(strAct);
    }

    function getActivitats1() {
        var strSector;
        strSector = jQuery("#ctl00_Top1_search_ddlSector1").val();
        if (strSector == "-1") {
            var todas = "<option value='-1'>Cargando...</option>";
            jQuery("#ddlActivitat1").html(todas);
            getActividadID1();
        }
        else {
            var strUrl = document.location.href;
            var params = { sector: strSector };
           
            var page = strUrl;
            if (page.indexOf('?') > -1)
                page += '&cb=actividades';
            else
                page += '?cb=actividades';

            jQuery.getJSON(page, params,
			   function (data) {
			       var options = "";
			       for (var i = 0; i < data.length; i++) {
			           options += "<option value='" + data[i].First + "'>" + data[i].Second + "</option>";
			       }
			       jQuery("#ddlActivitat1").html(options);
			       var jsActivitat = jQuery("#ctl00_Top1_search_txtActividad1").val();
			       if (jsActivitat != null) {
			           jQuery("#ddlActivitat1").val(jsActivitat);
			       }
			   });
        }
    }
