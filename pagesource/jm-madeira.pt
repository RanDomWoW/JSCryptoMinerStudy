                    <a ng-href="{{vm.baseurl}}{{oc.categoria == 'cronica' ? 'cronicas' : 'opinioes'}}/ver/{{oc.codigo}}/{{oc.alias}}">
                        <div class="col-md-12 pl-0 pr-0 delay-01 margin-20 main-sl">
                            <div ng-if="oc.image_file_name" class="img-author">
                                <img ng-src="{{vm.baseurl}}file/index/cronistas/{{oc.image_file_name}}"
                                class="img-circle pic"
                                ng-if="oc.categoria == 'cronica'">
                                <img ng-src="{{vm.baseurl}}file/index/opinantes/{{oc.image_file_name}}"
                                class="img-circle pic"
                                ng-if="oc.categoria == 'opiniao'">
                            </div>

                            <div class="row name"> {{ oc.autor_nome | decodeHTML }} </div>
                            <div class="row title"> {{ oc.titulo | decodeHTML }} </div>
                            <div class="row data"> {{ oc.data_publicacao | date }}</div>
                        </div>
                    </a>

                    <div class="row mb-15" ng-if="($index + 1) % 4">
                        <span class="separator mt-30">_____</span>
                    </div>
                </div>

                <!-- prev -->
                <div class="controls mt-20">
                    <a class="mr-30" role="button" ng-click="vm.desfila('back', size.size)"
                    ng-if="vm.getCurrentSize(size.size)['offset'] > 0">
                        <span class="icon-arrowup" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>

                    <a role="button"
                    ng-click="vm.desfila('next', size.size)">
                        <span class="icon-arrowdown" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>
</div>

