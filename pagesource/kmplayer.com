    function customizeForDevice() {

        var device_os = "";
        var device_type = "";
        var md = new MobileDetect(window.navigator.userAgent);

        if (md.os() == "iOS")
        {
            if (md.is("iPhone") == true) {
                device_type = "iphone";
            } else if (md.is("iPod") == true) {
                device_type = "iPod";
            } else if (md.is("iPad") == true) {
                device_type = "iPad";
            }

            var agent = window.navigator.userAgent.toLocaleLowerCase();
            if (agent.indexOf("pandoraplus") > -1) {
                device_type += "_a";
            } else {
                device_type += "_w";
            }
        }
        else if (md.os() == "AndroidOS")
        {
            device_type = "android";

            var agent = window.navigator.userAgent.toLocaleLowerCase();
            if (agent.indexOf("pandoraplus") > -1) {
                device_type += "_a";
            } else {
                device_type += "_w";
            }
        }
        else
        {
            device_type = 'pc';
        }

        return device_type;
    }

    // 광고 통계
    // kind -> 1:다윈, 2:프리즘, 3:페이스북, 4:프리즘프리
    // mode -> 1:광고로드, 2:광고재생, 3:광고없음
    function sendLogAD(kind, mode)
    {
        var sendKind    = '';
        var strLogID    = '';
        var strOutput   = '';

        console.log('sendLogAD', kind, mode);

        if(kind == '10000' || kind == '')
        {
            return;
        }

        if(kind == '100')
        {
            sendKind = '2';
        }
        else if(kind == '101')
        {
            sendKind = '4';
        }
        else if(kind == '22')
        {
            sendKind = '1';
        }
        else if(kind == '201')
        {
            sendKind = '3';
        }
        var url = 'http://log.kmplayer.com/kmpboxad?location=4&kind='+sendKind+'&mode='+mode;
        // console.log('log.kmplayer :: sendLogAD( '+url+' )');

        strLogID    = 'kmpboxad_'+sendKind+'_'+String(Math.floor((Math.random() * 10000)));
        strOutput   = '<iframe id="'+strLogID+'" style="display:none;" src="'+url+'"></iframe>'
        // console.log('sendLogAD',strOutput);
        jQuery('body').append(strOutput);
        jQuery('#'+strLogID).load(function(){
            jQuery(this).remove();
            console.log('sendLogAD', "complete");
        });
    }
