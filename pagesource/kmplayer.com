<script type="text/javascript">
// url download 적용을 위한 스크립트
function setDownloadUrl(data) {
    console.log(data);
    var len = data.length;
    for(var i = 0; i < len; i++) {
        var d = data[i];
        if(d.btnId == undefined || d.downloadUrl == undefined)
            continue;

        var btn = $("#"+d.btnId);
        if (btn != undefined) {
            var downloadUrl = d.downloadUrl;
            if(downloadUrl.indexOf("http://") == -1)
                downloadUrl = "http://"+downloadUrl;

            if(btn.is("a")) {
                if(userCountry != 'KR') {

                    if ((d.btnId == 'btn1' || d.btnId == 'btn2')/* && getCromVersion()*/)
                    {   // btn1이고 크롬이면 다운로드 경로 추가 안함
                        // btn.attr('href', 'javascript:void(0);');
                        // btn.removeAttr('target');
                        btn.attr('href', downloadUrl);
                        btn.bind('click',function() {
                            userConversion();
                        });
                    } else if(d.btnId == 'btn3') {

                        // btn.bind('click',function() {
                        //     location.href = downloadUrl;
                        //     userConversion();
                        // });
                        btn.attr('href', downloadUrl);
                        btn.click(function() {
                            userConversion_mac();
                        });

                    }
                    else{
                        btn.attr('href', downloadUrl);
                    }

                } else {
                    btn.attr("href", downloadUrl);
                }

            } else {
                btn.click(function(){
                    location.href = downloadUrl;
                });
            }
        }
    }
}

function getDownloadUrl() {
    var url = "/url/get/index";
    $.ajax({
        type:"GET",
        url:url,
        async:false,
        success: function(data){
            if (data.length > 0) {
                setDownloadUrl(data);
                console.log(data);
            }
        }
    });
}

$(function(){
    getDownloadUrl();
});
</script>
