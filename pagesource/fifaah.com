<script type="text/javascript">
    (function () {
        var s = $(".cf-mbtitle li"),
            t = $(".cf-mbt div"),
            l = $(".cf-mbproduct"),
            loadStatus = false,
            gamesort = "FIFA18";

        $.prototype.current = function () {
            var e;
            this.each(function () {
                if (!e && $(this).hasClass("on")) {
                    e = $(this);
                }
            });
            return e;
        };
        var serverEvent = function () {
            if (loadStatus || $(this).hasClass("on")) { return; }

            s.removeClass("on");
            $(this).addClass("on");
            var gn = $(this).attr("data-game");
            var sn = $(this).attr("servername");
            var tn = t.attr("data-id");
            showLoad("");
            $.ajax({
                type: "POST",
                url: "/index/coins2",
                data: { gamesort: gn || gamesort, servername: sn, tmethod: tn },
                success: function (data) {
                    loadStatus = false;
                    closeLoad();
                    $(".cf-mainbuy").html(data);
                    //重新绑定事件
                    bindEvents();
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    loadStatus = false;
                    closeLoad();
                }
            });
        };
        var selltous = function () {
            var servername = s.current().html();
            location.href = "/selltous?gamesort=" + gamesort + "&servername=" + servername;
        };
        var addmore = function () {
            $(l.selector).each(function () {
                var show = function () {
                    var div = $(this).parents(l.selector);
                    div.find(".cf-list").css({ height: "auto" });
                    div.find(".cf-more a").text("Hide -").unbind().click(function () { hide(div); });
                };
                var hide = function (div) {
                    $(div).find(".cf-list").css({ height: "400px", overflow: "hidden", position: "relative" });
                    $(div).find(".cf-more a").text("More +").unbind().click(show);
                };
                hide(this);
            });
        };
        var tab = function (source, target) {
            var change = function () {


                if ($(this).hasClass("exclude")) { return; }
                $(source).removeClass("on");
                $(this).addClass("on");
                var index = $(source).index($(this));
                index = (index % 2 == 0) ? "0" : "1";
                var targetElement = $(target).get(index);
                $(target).hide();
                $(targetElement).show();
            };
            $(source).on("click", change);
            //$(source).on("click", change);
        };
        var bindEvents = function () {
            s = $(".cf-mbtitle li");
            t = $(".cf-mbt div");
            s.unbind();
            s.on("click", serverEvent);
            tab(t.selector, l.selector);
            addmore();
            //$(t.selector).last().click(selltous);
        };
        bindEvents();
    })();
</script>
