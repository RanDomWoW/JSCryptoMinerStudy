      jQuery("#"+id).append(jQuery("<option />", {"value":"", "text":"Altura"}));
    }else if(id == "rim-lst"){
      jQuery("#"+id).append(jQuery("<option />", {"value":"", "text":"Diámetro"}));
    }else if(id == "speed-lst"){
      jQuery("#"+id).append(jQuery("<option />", {"value":"", "text":"Velocidad"}));
    }else if(id == "load-lst"){
      jQuery("#"+id).append(jQuery("<option />", {"value":"", "text":"Carga"}));
    }else if(id == "brand-lst"){
      jQuery("#"+id).append(jQuery("<option />", {"value":"", "text":"Marca"}));
    }

    jQuery.each(list, function(index, value) {
      var opt = jQuery("<option />", {
        "value": value,
        "text": value
      });

      jQuery("#" + id).append(opt);
    });
  }

  function isBlank(str) {
    return (str == null || jQuery.trim(str) == "");
  }

  function buildSearchUrl() {
    var section = jQuery("#width-lst").val();
    var profile = jQuery("#profile-lst").val();
    var rim = jQuery("#rim-lst").val();
    var load = jQuery("#load-lst").val();
    var speed = jQuery("#speed-lst").val();
    var brands = jQuery("#brand-lst").val();
    var seasons = jQuery("#season-lst").val();
    var xl = jQuery("#cb-xl").is(':checked');
    var rf = jQuery("#cb-rf").is(':checked');
    var wt = jQuery("#cb-wt").is(':checked');

    // check required fields
    if (isBlank(section)) {
      throw "The section is required";
    }

    if (isBlank(profile)) {
      throw "The profile is required";
    }

    if (isBlank(rim)) {
      throw "The rim diameter is required";
    }

    // everything fine so build the URL
    var url = searchUrlPrefix;

    // check brands
    if (brands != null && brands.length > 0) {
      var brandList = brands.join(":");
      url += brandList.toLowerCase() + "--";
    }

    // dimension
    url += section;
    url += "-" + profile;
    url += "-" + rim;
    if (!isBlank(speed)) { url += "-" + speed; }
    if (!isBlank(load)) { url += "-" + load; }

    // specificities
    var seasonList = "";
    if (seasons != null && seasons.length > 0) {
      for (var i = 0 ; i < seasons.length ; i++) {
        if (seasons[i] == "SUMMER") { seasonList += "summer" + (i<seasons.length-1 ? ":" : ""); }
        if (seasons[i] == "WINTER") { seasonList += "cold-weather" + (i<seasons.length-1 ? ":" : ""); }
        if (seasons[i] == "ALL") { seasonList += "all-seasons" + (i<seasons.length-1 ? ":" : ""); }
      }
    }
    if (xl || rf || wt || seasonList.length > 0) {
      url += "--";
    }
    if (xl) { url += "extra-load"; }
    if (rf) { url += (xl ? "-" : "") + "run-flat"; }
    if (wt) { url += (rf ? "-" : "") + "invierno"; }

    // neumaticos-reforzados neumaticos-runflat
    if (seasonList.length > 0) { url += (xl || rf || wt ? "-" : "") + seasonList; }

    return url;
  }

  // ################## CALL-BACK FUNCTION ###########################

  function brandsCallback(brandResponse) {
    if(brandResponse != null) {
      var data = brandResponse.brandList;
      populateList("brand-lst", data);
    }
  }

  function seasonsCallback(seasonResponse) {
    if(seasonResponse != null) {
      var data = seasonResponse.seasonList.entry;
      jQuery("#season-lst").empty();
      jQuery("#season-lst").append(jQuery("<option />", {"value":"", "text":"-"}));
