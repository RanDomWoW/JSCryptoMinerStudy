  function populateList(id,list) {
    jQuery("#"+id).empty();

    //jQuery("#"+id).append(jQuery("<option />", {"value":"", "text":"-"}));

    if(id == "width-lst"){
      jQuery("#"+id).append(jQuery("<option />", {"value":"", "text":"Anchura"}));
