  bottom: 10px;
  left: 0;
  display: block;
  width: 12px;
  height: 12px;
  background-color: rgba(127,127,127, 1);
  opacity: 0;
  content: '';
  animation: loading-square-base 5.4s linear forwards infinite;
}
