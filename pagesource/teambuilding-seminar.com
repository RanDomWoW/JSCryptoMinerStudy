  left: 0px;
  width: 100%;
  height: 100%;
  width: 100vw;
  height: 100vh;
  z-index: 99999;
}
#site_loader_animation {
  margin: -22px 0 0 -22px;
  width: 44px;
  height: 44px;
  position: fixed;
  top: 50%;
  left: 50%;
}
#site_loader_animation:before {
  position: absolute;
  bottom: 0;
  left: 0;
  display: block;
  width: 12px;
  height: 12px;
