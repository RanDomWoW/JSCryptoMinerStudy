<script type="text/javascript">
    $(document).ready(function() {
        $(".search-toggle-btn").click(function(e) {
            e.preventDefault();
            var img_type=$(this).attr("data-img-type");
            if(img_type == "search"){
	            $(this).addClass('close-search');
                $(this).attr("data-img-type","close");
                $(".dp-search").fadeIn("fast");
            }
            else{
                $(this).removeClass('close-search');
                $(this).attr("data-img-type","search");
                $(".dp-search").fadeOut("fast");
            }
        });
        $(window).scroll(function() {
            var sct=$(this).scrollTop();
            if(sct >= "200"){
                $(".go-top-btn").addClass("show-go-top-btn");
            }
            else{
                $(".go-top-btn").removeClass("show-go-top-btn");
            }
        });
    });
</script>
