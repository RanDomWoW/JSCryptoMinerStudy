      $('#conts .tabBox').each(function () {
        $(this).find('.panel').hide();
        $(this).find('.panel').eq(0).show();
        $(this).find('.menu li').eq(0).addClass('on');
        $(this).find('.menu li a').click(function () {
          var index = $(this).parent('li').index();
          $(this).parent('li').addClass('on').siblings().removeClass('on');
          $(this).parents('.tabBox').find('.panel').hide();
          $(this).parents('.tabBox').find('.panel:eq(' + index + ')').show();
          return false;
        });
      });
	});
</script>
