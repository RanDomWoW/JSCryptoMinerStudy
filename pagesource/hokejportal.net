<script>




var my_pos=0;

function move_kurzy_up()
{
    vtop('#kurzy-scroll-block0');
    vtop('#kurzy-scroll-block1');
    vtop('#kurzy-scroll-block2');
}

function vtop(element)
{
    var my_block_scroller_id=element+' .inside';
    number_of_lines=$(element+' .bet-group').length;
    my_pos=$(element).attr('data-my-pos')*1;
    my_pos++;
    if (my_pos>=number_of_lines)
    {
        my_pos=0;
    }
    $(element).attr('data-my-pos',my_pos);
    $(my_block_scroller_id).css('top',-(my_pos*40));
    
    //$('title').text(my_pos+' '+number_of_lines);
    
   
}

