     $(document).ready(function() {

         var isRunningLeft = false;
         $("[data-carousel_video_go]").click(function () {
             if($(this).data("carousel_video_go") === "left"){
                 if(isRunningLeft == false){
                     $('#carousel_video div.item:last').css({"margin-left":"-240px"});
                     $('#carousel_video div.item:first').before($('#carousel_video div.item:last'));
                 }
                 isRunningLeft = true;
                 $('#carousel_video div.item:first:not(:animated)').animate({'margin-left' : "0px"},250, function(){
                     isRunningLeft = false;
                 });
             }else{
                 $('#carousel_video div.item:first:not(:animated)').animate({'margin-left' : "-240px"},250, function(){
                     $('#carousel_video div.item:last').after($('#carousel_video div.item:first'));
                     $('#carousel_video div.item:last').css({"margin-left":"0px"});
                 });
             }
         });


        var video_auto_slide = 0;
        var video_hover_pause = 0;
        var video_auto_slide_seconds = 5000;
        //$('#carousel_video div:first').before($('#carousel_video div:last'));

        if(video_auto_slide == 1){
            var timer = setInterval('slide_carousel("right", "carousel_video", "240")', video_auto_slide_seconds);
            $('#video_hidden_auto_slide_seconds').val(video_auto_slide_seconds);
        }

        if(video_hover_pause == 1){
            $('#carousel_video').hover(function(){
                clearInterval(timer)
            },function(){
                timer = setInterval('slide_carousel("right", "carousel_video", "240")', video_auto_slide_seconds);
            });

        }

      });
    </script>
