    <script>
        $(document).ready(function () {

            var isRunningLeft = false;
            $("[data-carousel_najave_go]").click(function () {
                if($(this).data("carousel_najave_go") === "left"){
                    if(isRunningLeft == false){
                        $('#carousel_najave div.item:last').css({"margin-left":"-240px"});
                        $('#carousel_najave div.item:first').before($('#carousel_najave div.item:last'));
                    }
                    isRunningLeft = true;
                    $('#carousel_najave div.item:first:not(:animated)').animate({'margin-left' : "0px"},250, function(){
                        isRunningLeft = false;
                    });
                }else{
                    $('#carousel_najave div.item:first:not(:animated)').animate({'margin-left' : "-240px"},250, function(){
                        $('#carousel_najave div.item:last').after($('#carousel_najave div.item:first'));
                        $('#carousel_najave div.item:last').css({"margin-left":"0px"});
                    });
                }
            });

            var video_auto_slide = 0;
            var video_hover_pause = 0;
            var video_auto_slide_seconds = 5000;
            //$('#carousel_video div:first').before($('#carousel_video div:last'));

            if (video_auto_slide == 1) {
                var timer = setInterval('slide_najave("right", "carousel_najave", "240")', video_auto_slide_seconds);
                $('#video_hidden_auto_slide_seconds').val(video_auto_slide_seconds);
            }

            if (video_hover_pause == 1) {
                $('#carousel_najave').hover(function () {
                    clearInterval(timer)
                }, function () {
                    timer = setInterval('slide_najave("right", "carousel_najave", "240")', video_auto_slide_seconds);
                });

            }

        });
    </script>
