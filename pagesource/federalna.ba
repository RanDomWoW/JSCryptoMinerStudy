    .btn-quick {
        display: none;
        position: fixed;
        color: #fff;
        background-color: #bbb;
        background-image: url('/templates/default/images/arrow-top.png');
        background-repeat: no-repeat;
        background-position: center;
        width: 42px;
        height: 42px;
        cursor: pointer;
        -webkit-transition: all 200ms ease-out 0ms;
        -moz-transition: all 200ms ease-out 0ms;
        -o-transition: all 200ms ease-out 0ms;
        transition: all 200ms ease-out 0ms;
        opacity: 0.5;
        -webkit-opacity: 0.5;
        -moz-opacity: 0.5;
        filter:alpha(opacity=50);
        z-index: 99;
        text-align: center;
        border-radius: 1px;
    }

    .btn-quick:hover {
        opacity: 1;
        -webkit-opacity: 1;
        -moz-opacity: 1;
        filter:alpha(opacity=100);
    }

    #to-top {
        bottom: 26px;
        right: 39px;
    }

