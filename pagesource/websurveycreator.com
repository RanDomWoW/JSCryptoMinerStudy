        <section class="generic" style="width: 100%; background: url(brochure/images/bg-gallery.jpg) repeat-x; /*background: linear-gradient(0deg, #fff 10%, #fe9900 10%)*/">
            <div class="generic-holder">
                <h1 style="color: #fff; font-size:56px;">Cloud based web surveys for any purpose!</h1>
                <div class="visual" style="height:410px;">
                    <img style="float:left;padding-left:40px;" src="brochure/images/web-survey-creator-brochure.png" width="896" height="396" alt="Multi Modal Surveys" />
