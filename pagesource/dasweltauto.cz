<script type="text/javascript">
	/*
	 * odstraneni porovnavaneho vozu z listu
	 */
function compareCarListAddAjax( event, $this ) {
    var $this = $this,
        $targetDiv = $('#compare-cars-wrap'),
        toolbarUrl = '/CarSet/toolbar',
        addUrl = $this.data('href');

    event.preventDefault();

    $.ajax({
        type: "html",
        url: addUrl,
        success: function(data)
        {
            $.ajax({
                type: "html",
                url: toolbarUrl,
                success: function(data)
                {
                    $( $targetDiv ).html( data );
                    var $compareCars = $('#compare-cars');
                    if( !$compareCars.hasClass('filter-toggle-open') ) {
                        $compareCars.click();
                    }
                }
            });
        }
    });

    return false;
}
	function compareCarListRemoveAjax( event, $this ) {
		var $this = $this,
			$targetDiv = $('#compare-cars-wrap'),
			toolbarUrl = '/CarSet/toolbar',
			removeUrl = $this.attr('href');

		event.preventDefault();

		$.ajax({
			type: "html",
			url: removeUrl,
			success: function(data)
			{
				$.ajax({
					type: "html",
					url: toolbarUrl,
					success: function(data)
					{
						/*
						$( $targetDiv ).html( data );
						var $compareCars = $('#compare-cars');
						if( !$compareCars.hasClass('filter-toggle-open') ) {
							$compareCars.click();
						}
						*/

						$( $targetDiv ).html( data );
						$( "#compare-cars" ).addClass('filter-toggle-open');
						$( "#compare-cars-content" ).addClass('filter-toggle-open');

					}
				});
			}
		});

		return false;
	}

	$( document ).ready(function() {

		
		/*
		 * odstraneni porovnavaneho vozu z listu
		 */

		$( '#compare-cars-wrap' ).on( 'click', '.compare-car-remove', function( event ) {
			compareCarListRemoveAjax( event, $(this) );
		});

		/*
		 * odstraneni porovnavaneho vozu z listu
		 */

		$( '#compare-cars-wrap' ).on( 'click', '#compare-cars-count a', function( event ) {
			var	$this = $(this),
				thisDataTab = $this.data('tab'),
				$compareCars = $('#compare-cars');

			event.preventDefault();

			$('#compare-cars-count a').removeClass('active');
			$this.addClass('active');

			if ( thisDataTab == 'compare-tab-1' ) {
				$('#compare-tab-1').show();
				$('#compare-tab-2').hide();
				document.cookie = 'compare-tab=1;path=/';
			} else {
				$('#compare-tab-2').show();
				$('#compare-tab-1').hide();
				document.cookie = 'compare-tab=2;path=/';
			}

			if (!($compareCars.hasClass( "filter-toggle-open" ))) {
				//$compareCars.trigger('click');
				//$compareCars.trigger('click');
				$compareCars.click();
				$('#filter-car-count').addClass('compare-closed');
				$('#filter-car-count').removeClass('compare-opened');
			} else {
				//console.log('xx');
				//$compareCars.trigger('click');
				$('#filter-car-count').addClass('compare-opened');
				$('#filter-car-count').removeClass('compare-closed');
			}
			/*
			if (!($compareCars.hasClass( "filter-toggle-open" ))) {
				$compareCars.click();
				$('#filter-car-count').addClass('compare-closed');
				$('#filter-car-count').removeClass('compare-opened');
			} else {
				$('#filter-car-count').addClass('compare-opened');
				$('#filter-car-count').removeClass('compare-closed');
			}*/
		});
		$( '#compare-cars-wrap' ).on( 'click', '#compare-cars', function( event ) {
			//console.log('xx');
			if (($('#compare-cars').hasClass( "filter-toggle-open" ))) {
				$('#filter-car-count').addClass('compare-closed');
				$('#filter-car-count').removeClass('compare-opened');
			} else {
				$('#filter-car-count').addClass('compare-opened');
				$('#filter-car-count').removeClass('compare-closed');
			}
		});


		/*
     * pridani dalsich aut
     */

    $( '#result-footer-buttons' ).on( 'click', 'a.next-cars', function( event ) {
        addNextCarsAjax( event, $(this) );
    });

    /*
     * pridani porovnavaneho vozu do listu
     */

    $( '#searchForm' ).on( 'click', '.car-review-stickers a.add-car', function( event ) {
        compareCarListAddAjax( event, $(this) );
		$('#compare-cars-count').find('a[data-tab="compare-tab-1"]').trigger('click');
    });

	});
</script>


