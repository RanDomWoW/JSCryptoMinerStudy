function trackMedia(link) {
	var att = link.attributes;
	var tl = att.length;
	var i = 0;
	for(i ; i < tl ; i++) {
		if(att[i].name === "href") {
			var hrefValue = att[i].value;
			var regexp = new RegExp("\.([a-z]+)[^a-z]*$","i");
			var type = regexp.exec(hrefValue);
			if(type.length === 2) {
				type = type[1];
				ga('send' , 'event', 'Medias', type, hrefValue);
			}
			i = tl;
		}
	}
}
function trackAmazon(link) {
	var att = link.attributes;
	var tl = att.length;
	var i = 0;
	for(i ; i < tl ; i++) {
		if(att[i].name === "href") {
			var hrefValue = att[i].value;
			var regexp = new RegExp("^http://www.amazon.(fr|com)/(.+)$","i");
			var cleanHrefValue = regexp.exec(hrefValue);
			if(cleanHrefValue.length === 3) {
			    cleanHrefValue = cleanHrefValue[2];
			    ga('send' , 'event', 'Amazon', cleanHrefValue);
			}
			i = tl;
		}
	}
}
