		function loadImgOrg(loadImgOrg)
		{

			if(loadImgOrg == undefined || loadImgOrg == "")
			{

				$('.loadImgOrg').each(function(){
					if($(this).attr('data-src') != undefined
						&& $(this).parent().css('display') != 'none'
						&& $(this).parent().css('top') == '0px'
						&& $(this).parent().css('left') == '0px'
					)
					{
						$(this).attr("src", $(this).attr('data-src'));
						$(this).removeAttr('data-src');
						$(this).removeClass('loadImgOrg');
						$(this).addClass('loadImg');
					}
				});
				//console.log('ende loadImgOrg:');
			}else
			{
				if($(loadImgOrg).attr('data-src') != undefined)
				{
					$(loadImgOrg).attr("src", $(loadImgOrg).attr('data-src'));
					$(loadImgOrg).removeAttr('data-src');
					$(loadImgOrg).removeClass('loadImgOrg');
					$(loadImgOrg).addClass('loadImg');
				}
			}

		}

