<!-- *** SVG CSS Selector *** -->
  <script>
	function supportsSvg() {
		return document.implementation.hasFeature("http://www.w3.org/TR/SVG11/feature#Image", "1.1");
	}
	var styleTag = document.createElement ("style");
    var head = document.getElementsByTagName ("head")[0];
    head.appendChild (styleTag);

    var sheet = styleTag.sheet ? styleTag.sheet : styleTag.styleSheet;
	if (supportsSvg()) {
		if (sheet.insertRule) {
			sheet.insertRule(".SVG { visibility: visible; max-height: 1000px; display: inline;}", 0);
			sheet.insertRule(".non_SVG { visibility: hidden; max-height: 0px; display:none;}", 0);
			}
		else if (sheet.addRule) {
			sheet.addRule(".SVG", "visibility: visible; max-height: 1000px; display: inline;", 0);
			sheet.addRule(".non_SVG", "visibility: hidden; max-height: 0px; display:none;", 0);
		}
	}
	else {
		if (sheet.insertRule) {
			sheet.insertRule(".SVG { visibility: hidden; max-height: 0px; display:none;}", 0);
			sheet.insertRule(".non_SVG { visibility: visible; max-height: 1000px; display: inline;}", 0);
			}
		else if (sheet.addRule) {
			sheet.addRule(".SVG", "visibility: hidden; max-height: 0px; display:none;", 0);
			sheet.addRule(".non_SVG", "visibility: visible; max-height: 1000px; display: inline;", 0);
		}  
	}
  
  </script>
