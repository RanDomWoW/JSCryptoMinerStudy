 <!--*** Accordian Menu Script *** -->
function getUrlVars() {
	var vars = {};
	var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
		vars[key] = value;
	});
	return vars;
}

// Mobile Accordion
	$("#accordion > li > div").click(function(){
	
		$('#accordion > #mobile_menu > ul').slideUp();
		
		if(false == $(this).next().is(':visible')) {
			$('#accordion ul').slideUp(300);
		}
		$(this).next().slideToggle(300);
	});

// desktop/Tablet Accordion	
  var menu_position = getUrlVars()["menu"];
  if (!menu_position){
	  menu_position = "0";
  };
  menu_position = (menu_position.charAt(0));

    


$(document).ready(function () {
	
  $('#nav > li > a').click(function(){
    if ($(this).attr('class') != 'active'){
      	$(this).next().slideToggle();
      	$(this).addClass('active');
    }
	else {
		$(this).next().slideToggle();
		$('#nav li a').removeClass('active');
	}
  });
  
    if (menu_position == 2){
    	$("#nav > li > a[href='shop.php?menu=2']").trigger("click");
	}
	else {
		$("#nav > li > a[href='#"+menu_position+"']").trigger("click");
	};

});

