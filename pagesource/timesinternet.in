                url: "/applyJob.php",
                type: "POST",
                data: form_data,
                contentType: false,
                cache: false, 
                processData:false,
                success: function(data) {
                 var result = data.split(':#:');    
                if(result[0] == 1){
                     $('#jobsmsg').text(result[1]);
                     clearForm();
                     window.location = '/thanks?t=am9icw==';
                }else{
                    $('#jobsmsg').text(result[1]);
                    $('#jobsmsg').focus();
                    $('#submit').removeAttr("disabled");
                    $('#submit').val('Submit');
                 }
                },
                error: function(){
                     $('#jobsmsg').text('Error!');
                }
            });
        } else {
            $('#submit').removeAttr("disabled");
            $('#submit').val('Submit');
        }
    return false;
}));

