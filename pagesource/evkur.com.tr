                $(this).next('.Sub_kategoriler').slideToggle();
                $(this).parent().siblings().children().next().slideUp();
                return false;
            });
            $('.sort_dropdown > a').click(function (e) {
                e.preventDefault();
                var displaySw = $('.filterDroupDown').data("swdisplay");
                if (displaySw == 0) {
                    $('.sort_dropdown > a').toggleClass('open');
                    $(this).parent().find('.Sub_Sirala').slideToggle();
                    $('.filterShowDiv').hide();
                    $('.pageoverlay').toggle();
                    var ownDisplaySw = $('.sort_dropdown').data("swdisplay");
                    if (ownDisplaySw == 0) { $('.sort_dropdown').data("swdisplay", 1); }
                    else { $('.sort_dropdown').data("swdisplay", 0); }
                }
                else {
                    $('.filterDroupDown > a').toggleClass('open');
                    $('.filterDroupDown > a').parent().find('.filterShowDiv').slideToggle();
                    $('.sort_dropdown .Sub_Sirala').hide();
                    $('.pageoverlay').toggle();
                    $('.filterDroupDown').data("swdisplay", 0);
                }
            });
            $('.filterDroupDown > a').click(function (e) {
                e.preventDefault();
                var displaySw = $('.sort_dropdown').data("swdisplay");
                if (displaySw == 0) {
                    $('.filterDroupDown > a').toggleClass('open');
                    $(this).parent().find('.filterShowDiv').slideToggle();
                    $('.sort_dropdown .Sub_Sirala').hide();
                    $('.pageoverlay').toggle();
                    var ownDisplaySw = $('.filterDroupDown').data("swdisplay");
                    if (ownDisplaySw == 0) { $('.filterDroupDown').data("swdisplay", 1); }
                    else { $('.filterDroupDown').data("swdisplay", 0); }
                }
                else {
                    $('.sort_dropdown > a').toggleClass('open');
                    $('.sort_dropdown > a').parent().find('.Sub_Sirala').slideToggle();
                    $('.filterShowDiv').hide();
                    $('.pageoverlay').toggle();
                    $('.sort_dropdown').data("swdisplay", 0);
                }
            });
            $('.pageoverlay').click(function () {
                var displaySw = $('.sort_dropdown').data("swdisplay");
                if (displaySw == 1) {
                    $('.sort_dropdown > a').removeClass('open');
                    $('.sort_dropdown > a').parent().find('.Sub_Sirala').hide();
                    $('.filterShowDiv').hide();
                    $('.sort_dropdown').data("swdisplay", 0);
                }
                displaySw = $('.filterDroupDown').data("swdisplay");
                if (displaySw == 1) {
                    $('.filterDroupDown > a').removeClass('open');
                    $('.filterDroupDown > a').parent().find('.filterShowDiv').hide();
                    $('.sort_dropdown .Sub_Sirala').hide();
                    $('.filterDroupDown').data("swdisplay", 0);
