<script>

	  function calcWidth(name) {

	  	  var spwidth = window.innerWidth || document.documentElement.clientWidth  || document.body.clientWidth;
			if(spwidth <= 1000) {
				return;
			}

		  var navwidth = 0;
		  var morewidth = $(name+' .more').outerWidth(true) + 30;
		  
		  $(name+' > li:not(.more)').each(function() {
		      
		      navwidth += $(this).outerWidth( true );
		  });
		    
		  var availablespace = $(name).parent('div').outerWidth(true) - morewidth - parseInt($(name).css('padding-left'), 10);


		  if (navwidth > availablespace && navwidth > 0 && availablespace > 0) {
		    var lastItem = $(name+' > li:not(.more)').last();

		    if(lastItem.hasClass('menu-sep')){
		        lastItem.remove();
		    }else{
		        lastItem.attr('data-width', lastItem.outerWidth(true));
		        lastItem.prependTo($(name+' .more ul'));
		        
		        if($(name+' .more ul li').size() > 7 && name =="#sub_menu"){
		            $(name+' .more ul').css('-moz-column-count','2');
		            $(name+' .more ul').css('-webkit-column-count','2');
		            $(name+' .more ul').css('column-count','2');
		        }
		    }
		    
		    calcWidth(name);

		  } 
		  
		  if ($(name+' .more li').length > 0) {
		    $(name+' .more ul').css('display','none');
		    $(name+' .more').css('display','inline-block');
		    $(name+' .more').css('float','right');
		  } else {
		    $(name+' .more').css('display','none');
		  }

	}


	$(window).on('resize',function(){
	  calcWidth('#main_menu');
	  if($('#sub_menu').length){
	        calcWidth('#sub_menu');
	    }
	});

  calcWidth('#main_menu');
  if($('#sub_menu').length){
        calcWidth('#sub_menu');
  }
</script>            </div>
            	
