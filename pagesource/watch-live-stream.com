    monthts[8] = "Sept";
    monthts[9] = "Oct";
    monthts[10] = "Nov";
    monthts[11] = "Dec";

	function updateTimer(){

		var date = new Date();

		var currentDate = date.getDate() +' '+ monthts[date.getMonth()] +' '+date.getFullYear();
		$('.timer .date').text(currentDate);

		$('.timer .day').text(weekday[date.getDay()]);

		var currentTime = date.toLocaleTimeString([], {hour: '2-digit', minute:'2-digit'});
		$('.timer .time').text(currentTime);

		var offset = getGMTOffset1(date);

		$('.timer .zone').text(offset);

		var t = setTimeout(updateTimer, 1000);
	}
	updateTimer();

</script>
