<script>

	function getGMTOffset1(date) {
        var timezoneOffset = date.getTimezoneOffset();
        timezoneOffset = (timezoneOffset/60)*-1;
        var gmt = 'GMT';
        
        gmt += (timezoneOffset > 0 ? ' +' : ' ')+timezoneOffset;
        gmt += ':00';
        
        return gmt;
    }
    var weekday = new Array(7);
    weekday[0]=  "Sunday";
