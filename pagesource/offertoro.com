<script type="text/javascript">

    // GET parameters
    var _get = (function (a) {
        if (a == "") return {};
        var b = {};
        for (var i = 0; i < a.length; ++i) {
            var p = a[i].split('=', 2);
            if (p.length == 1)
                b[p[0]] = "";
            else
                b[p[0]] = decodeURIComponent(p[1].replace(/\+/g, " "));
        }
        return b;
    })(window.location.search.substr(1).split('&'));

    function ValidateEmail(emailAddress)
    {
        var pattern = /^[_a-z0-9-]+(\.[_a-z0-9-]+)*@[a-z0-9-]+(\.[a-z0-9-]+)*(\.[a-z]{2,4})$/;
        return pattern.test(emailAddress);
    }

    $(document).ready(function () {
        var isAdminEmail = false;
        var adminEmails = [];

        $('#wEmail').on('change', function (e) {
            var emailAddress = $(this).val();

            if(adminEmails.indexOf(emailAddress) > -1) {
                isAdminEmail = true;
                $('#optional_key_block').show();
            } else if (ValidateEmail(emailAddress)) {
                var requesting = false;
                if(requesting == false) {
                    $.ajax({
                        url: '/ajax/isme',
                        data: {email: emailAddress},
                        type: 'post',
                        success: function(data) {
                            requesting = false;
                            if(data.result == 1) {
                                isAdminEmail = true;
                                $('#optional_key_block').show();
                                adminEmails.push(emailAddress);
                            } else {
                                isAdminEmail = false;
                                $('#optional_key_block').hide();
                            }
                        }
                    });
                }
            } else {
                $('#optional_key_block').hide();
            }
        });

        var isCirculeSectionLoded = false;
        if (screen && screen.width > 768) {
            $('#fullpage').fullpage({
                //Navigation
                anchors: ['section0', 'section1', 'section2', 'section3', 'section4', 'section5', 'section6', 'section7'],
                paddingTop: '85px',
                css3: true,
                navigation: false,
                scrollOverflow: true,
                scrollOverflowOptions: {
                    click: false
                },
                onLeave: function (index, nextIndex, direction) {
                    console.log('onLeave: ', index);
                    if (index == 3) {
                        $("#publishers-circle").empty();
                        $("#Conversions-circle").empty();
                        $("#Users-circle").empty();
                        isCirculeSectionLoded = false;
                    }
                },
                afterLoad: function (anchorLink, index) {
                    console.log('afterLoad: ', index);
                    if (!isCirculeSectionLoded) {
                        animate_circles();

                        isCirculeSectionLoded = true;
                    }

                    var loadedSection = $(this);
                    //console.log('called!!');
                    $('.se-pre-con').fadeOut('slow');
                }

            });
        } else {

            if ($('#monetize_section').length > 0 && window.location.hash == '#section1') {
                var allowance_height = 65; // margin top: 15, navbar height: 50

                if ($('#monetize_section').offset().top > 0) {
                    $('html,body').animate({
                        scrollTop: $('#monetize_section').offset().top - allowance_height
                    }, 'slow');
                }
            }

            $('#scroll_monetize').on('click', function (e) {

                if ($('#monetize_section').length > 0) {
                    e.preventDefault();

                    $('.navbar-toggle').trigger('click'); // hide navbar

                    var allowance_height = 65; // margin top: 15, navbar height: 50

                    if ($('#monetize_section').offset().top > 0) {
                        $('html,body').animate({
                            scrollTop: $('#monetize_section').offset().top - allowance_height
                        }, 'slow');
                    }
                }

            });

            var isCirculeSectionLoded = false;
            $(document).on('scroll', function () {
                if (
                    $(this).scrollTop() >= $('#user_acquisition_section').position().top
                    && $(this).scrollTop() <= $('#user_acquisition_section').position().top + $('#user_acquisition_section').height()
                ) {
                    if (!isCirculeSectionLoded) {
                        animate_circles();

                        isCirculeSectionLoded = true;
                    }
                } else {
                    $("#publishers-circle").empty();
                    $("#Conversions-circle").empty();
                    $("#Users-circle").empty();
                    isCirculeSectionLoded = false;
                }
            })
        }

        if (("loginPage" in _get) && (_get.loginPage == "1")) {
            $('#login-modal').modal('show');

            if($('#wEmail').val() != '' && ValidateEmail($('#wEmail').val())) {
                var emailAddress = $('#wEmail').val();
                var requesting = false;
                if(requesting == false) {
                    $.ajax({
                        url: '/ajax/isme',
                        data: {email: emailAddress},
                        type: 'post',
                        success: function(data) {
                            requesting = false;
                            if(data.result == 1) {
                                isAdminEmail = true;
                                $('#optional_key_block').show();
                            } else {
                                isAdminEmail = false;
                                $('#optional_key_block').hide();
                            }
                        }
                    });
                }
            }
        }
    });

    function animate_circles() {
        $("#publishers-circle").circliful({
            animation: 1,
            animationStep: 5,
            foregroundBorderWidth: 15,
            backgroundBorderWidth: 15,
            percent: 63,
            textSize: 28,
            textStyle: 'font-size: 12px;',
            backgroundColor: "#fff",
            fillColor: 'none',
            foregroundColor: '#ee3839',
            textColor: '#666',
            multiPercentage: 1,
            percentages: [10, 20, 30]
        });
        $("#Users-circle").circliful({
            animation: 1,
            animationStep: 5,
            foregroundBorderWidth: 15,
            backgroundBorderWidth: 15,
            percent: 70,
            textSize: 28,
            textStyle: 'font-size: 12px;',
            backgroundColor: "#fff",
            fillColor: 'none',
            foregroundColor: '#ee3839',
            textColor: '#666',
            multiPercentage: 1,
            percentages: [10, 20, 30]
        });
        $("#Conversions-circle").circliful({
            animation: 1,
            animationStep: 5,
            foregroundBorderWidth: 15,
            backgroundBorderWidth: 15,
            percent: 80,
            textSize: 28,
            textStyle: 'font-size: 12px;',
            backgroundColor: "#fff",
            fillColor: 'none',
            foregroundColor: '#ee3839',
            textColor: 'transparent',
            multiPercentage: 1,
            percentages: [10, 20, 30]
        });
    }

    $(".open_login_btn").click(function(e) {
        e.preventDefault();

        var hostname = window.location.hostname;
        var dev_hosts = ['dev1.offertoro.com', 'sergiy.dev.offertoro.com'];

        if(dev_hosts.indexOf(hostname) < 0) {
            if (location.protocol == "http:") {
                if(hostname.indexOf('www.') == -1)  {
                    hostname = 'www.' + hostname;
                }
                location.href = "https://" + hostname + "/?loginPage=1";
            } else if(hostname.indexOf('www.') == -1) {
                location.href = "https://www." + hostname + "/?loginPage=1";
            } else {
                $('#login-modal').modal('show');
            }
        } else {
            $('#login-modal').modal('show');
        }
    });


</script>

