<script>
    jQuery(document).ready(function($) {

        



        $(window).scroll(function () {
            if( $(window).scrollTop() > $('header.desktopheader').offset().top && !($('header.desktopheader').hasClass('posi'))){
                $('header.desktopheader').addClass('stickyheader');
                $('body').addClass('hasstickyheader');
            } else if ($(window).scrollTop() == 0){
                $('header.desktopheader').removeClass('stickyheader');
                $('body').removeClass('hasstickyheader');
            }
        });

        cank_formwrapper();


    });


    function hpscrollToElement(elementselector){
        jQuery('html, body').animate({
            scrollTop: jQuery(elementselector).offset().top-80
        }, 800);
    }

    function cank_formwrapper(){
        jQuery(".cank_formwrapper_sub2").hide();
        jQuery(".cank_formwrapper_sub3").hide();
        jQuery(".cank_formwrapper_sub4").hide();
        jQuery(".cank_formwrapper_sub5").hide();
        jQuery(".cnk_employee_error").hide();

    }

    var cnk_employee = false;
    var cnk_employee_type = false;
    var cnk_delivery = false;
    function cnk_employee_yes(){
        jQuery(".cank_formwrapper_sub2").show(400);
        cnk_employee = true;
        jQuery(".cank_formwrapper_sub3").hide(400);

    }
    function cnk_employee_no(){
        jQuery(".cank_formwrapper_sub2").hide(400);
        cnk_employee = false;
        jQuery(".cank_formwrapper_sub3").show(400);
        setCampaign("snk3819473");
    }
    function cnk_employee_check(){
        jQuery(".cnk_employee_error").hide();
        jQuery(".cank_formwrapper_sub3").hide();
        var empid = jQuery("#cnk_employee_id").val();
        if(empid=="555111"){
            cnk_employee_type = "full";
            setCampaign("snk1949184");
        } else if(empid=="555222"){
            cnk_employee_type = "casual";
            setCampaign("snk2817492");
        } else {
            jQuery(".cnk_employee_error").show(400);
            return false;
        }
        jQuery(".cank_formwrapper_sub3").show(400);
    }
    function cnk_facetoface(){
        jQuery(".cank_formwrapper_sub4").hide();
        cnk_delivery = "facetoface";
        jQuery('.axcelerateType option[value="practical"]').prop('selected', true).trigger('change');
        jQuery(".cank_formwrapper_sub4").show(400);
    }
    function cnk_employee_online(){
        jQuery(".cank_formwrapper_sub4").hide();
        cnk_delivery = "online";
        jQuery('.axcelerateType option[value="online"]').prop('selected', true).trigger('change');
        jQuery(".cank_formwrapper_sub4").show(400);
    }
    jQuery(".axcelerateLocation").change(function(){
        jQuery(".cank_formwrapper_sub5").show(400);
    });
</script>

