	}
	function popup_social_twitter(){
		var width  = 575,
        height = 400,
        left   = (screen.width/2)-(width/2),
        top    = (screen.height/2)-(height/2),
        url    = 'http://twitter.com/share?text=Make%20science%20at%20home%20with%20GPUGRID!',
        opts   = 'status=1' +
                 ',width='  + width  +
                 ',height=' + height +
                 ',top='    + top    +
                 ',left='   + left;
    
    	window.open(url, 'twitter', opts);
 
    return false;
		
	}
