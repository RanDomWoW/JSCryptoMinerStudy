function seachGoPostResetFun(){
    var paramsInd={ic2:'',ic5:'',ic4:'',ic3:'',ic6:'',aric:[],pay1:'',pay2:'',keyWord:'',usIde:'',ping1:'',ping2:'',RoomN1:'',RoomN2:'',Floot1:'',Floot2:'',housYear1:'',housYear2:'',carKin:[],bFloot:[],kid:[],housYear:[],ping:[],roomNum:[],totalP:[],special:[]};
    var htmlSrcdoc='';
    paramsInd.ic2 = $('#ic2').val();  //縣市
    paramsInd.ic5=$('#ic5').val();
    paramsInd.ic4=$('#ic4').val();
    paramsInd.ic6=$('#ic6').val();
    var a=$('#ic3').val();
    paramsInd.ic3=a;
    if(a){paramsInd.aric=a.split(",");}else{paramsInd.aric=[];}  //區
    //總價
    paramsInd.pay1=$("#totalPS").val();
    paramsInd.pay2=$("#totalPB").val();
    //關鍵字
    paramsInd.keyWord=$("#keyWord").val();
    if(paramsInd.keyWord=='請輸入關鍵字或物件編號搜尋'){paramsInd.keyWord='';}
    //用途
    paramsInd.usIde=$("#usIde").val();
    switch(paramsInd.usIde){
        case '1': var kidObp='.ik1'; break;
        case '2': var kidObp='.ik1'; break;
        case '3': var kidObp='.ik1'; break;
        case '4': var kidObp='.ik2'; break;
        case '5': var kidObp='.ik3'; break;
        case '6': var kidObp='.ik4'; break;
        case '7': var kidObp='.ik5'; break;
        case '8': var kidObp='.ik6'; break;
        default : var kidObp='.ik6'; break;
    }
    //類型
    var b=a_b2(kidObp,''); //console.log(b);
    if(b[4]){paramsInd.type=b[4].split(",");}else{paramsInd.type=[];}
    //坪數
   if(paramsInd.usIde==4){
        var inputPingS=$("#textfieldS1").val(),inputPingB=$("#textfieldB1").val();    
   }else{
        var inputPingS=$("#textfieldS").val(),inputPingB=$("#textfieldB").val();
   }
    paramsInd.ping1=inputPingS;
    paramsInd.ping2=inputPingB;
    //房數
    var room1=$("#textfieldRS").val(),room2=$("#textfieldRB").val();
    paramsInd.RoomN1=room1;
    paramsInd.RoomN2=room2;
    //樓層
    if(paramsInd.usIde==3 || paramsInd.usIde==5){
        var foot1=$("#textfieldFS1").val(),foot2=$("#textfieldFB2").val();
    }else{
        var foot1=$("#textfieldFS").val(),foot2=$("#textfieldFB").val();
    }
    paramsInd.Floot1=foot1;
    paramsInd.Floot2=foot2;
    //屋齡
    var houseYear1=$("#housYear1").val(),houseYear2=$("#housYear2").val();
    paramsInd.housYear1=houseYear1;
    paramsInd.housYear2=houseYear2;
    //車位
    var cK=a_b('carKin[]','');
    if(cK[4]){paramsInd.carKin=cK[4].split(",");}else{paramsInd.carKin=[];} 
    var flo=a_b('bFloot[]','');
    if(flo[4]){paramsInd.bFloot=flo[4].split(",");}else{paramsInd.bFloot=[];} 
    var hou=a_b('housYear[]','');
    if(hou[4]){paramsInd.housYear=hou[4].split(",");}else{paramsInd.housYear=[];} 
    var kid=a_b('kid[]','');
    if(kid[4]){paramsInd.kid=kid[4].split(",");}else{paramsInd.kid=[];} 
    var ping=a_b('ping[]','');
    if(ping[4]){paramsInd.ping=ping[4].split(",");}else{paramsInd.ping=[];} 
    var roomNum=a_b('roomNum[]','');
    if(roomNum[4]){paramsInd.roomNum=roomNum[4].split(",");}else{paramsInd.roomNum=[];} 
    var totalP=a_b('totalP[]','');
    if(totalP[4]){paramsInd.totalP=totalP[4].split(",");}else{paramsInd.totalP=[];} 
    //特色環境
    var sc=a_b('special[]','');
    paramsInd.special=sc[4].split(",");
    $.each(paramsInd,function(lin,ljn){
      if($.isArray(ljn)){
        for(var co=0;co<ljn.length;co++){
          htmlSrcdoc +='<input type="hidden" name="'+lin+'[]" value="'+ljn[co]+'" />';
        }
      }else{
        htmlSrcdoc +='<input type="hidden" name="'+lin+'" value="'+ljn+'" />';
      }
  });
    $('#formResetInPost').empty().html(htmlSrcdoc);
  return 'yes';
}
