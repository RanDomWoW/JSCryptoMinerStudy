				var params = $('#vacation_form').serialize();
				$('.error_msg').html('');
				$('#success_message').html('');

				$.ajax({
					type:'POST',
					url: '/home/create_member_user',
					data: params,
					dataType:'JSON',
					success: function(resp) {
						btn.removeAttr('disabled');
						btn.html('<span class="FreeButtonMain"> Yes! Send My Free Vacation </span><span class="FreeButtonMain txt_13">You will receive an instant email</span>');
						if(resp.status == 'error') {
							$.each(resp.errors, function(key,val){
								$('.'+key+'Error').html(val);
							});
						}
						else {

							$('#vacation_form').trigger('reset');
							$('#success_message').html(resp.message);
						}
					}
				});
			});
		});
	</script>

