            $("#lingq-video").on('show.bs.modal', function (e) {
                $('#youtube-player').get(0).contentWindow.postMessage('{"event":"command","func":"playVideo","args":""}', '*');
            });
            $("#lingq-video").on('hidden.bs.modal', function (e) {
                console.log("closuing")
                $('#youtube-player').get(0).contentWindow.postMessage('{"event":"command","func":"stopVideo","args":""}', '*');

            });
        }
        

        $(function ($) {
            $('.column-circle div a')
                .mouseover(function () {
                    $($(this).attr('rel')).show();
                })
                .mouseout(function () {
                    $($(this).attr('rel')).hide();
                });

            $(window).scroll(function () {
                $(window).scrollTop() > 140 ? $("#small_logo").addClass("FadeIn") : $("#small_logo").removeClass("FadeIn")
            });

            $('.video-thumb').click(openIntroVideo);

            $('.enter .signup').on('click', function () {
                ga('send', 'event', 'Frontsite', 'Sign Up Free Button Click', 'Header');
            });
            $('.video_section .get_started').on('click', function () {
                ga('send', 'event', 'Frontsite', 'Sign Up Free Button Click', 'Video Section');
            });
            $('.info_block .sign_up').on('click', function () {
                ga('send', 'event', 'Frontsite', 'Sign Up Free Button Click', 'Footer');
            });
            $('.thelinguist-method').on('click', function () {
                ga('send', 'event', 'Frontsite', 'The Linguist link', 'Home page');
            });

            $(document).scroll(function(){
                var offset = $(window).scrollTop();
                offset = parseInt(offset) ;
                $(".hero-image .image-wrap").css({"transform": "translate3d(0px, "+offset * (.2)+"px, 0px)" });
                $(".cta").css({"transform": "translate3d(0px, -"+offset * (.5) +"px, 0px)"});
            });

            var $learnedWordsCounter = $('.index-learned-words');
            var learnedWordsCount = sessionStorage.getItem('learnedWordsCount');
            var MAX_STEP = 100;
            var MAX_INTERVAL = 5000;

            function renderLearnedWordsCount() {
                $learnedWordsCounter.text(Number(learnedWordsCount).toLocaleString());
            }

            function incrementLearnedWordsCount() {
                learnedWordsCount = Math.floor(Math.random() * MAX_STEP + learnedWordsCount);
                sessionStorage.setItem('learnedWordsCount', learnedWordsCount)
                renderLearnedWordsCount();
                setTimeout(incrementLearnedWordsCount, Math.random() * MAX_INTERVAL);
            }

            if (!learnedWordsCount) {
                learnedWordsCount = Number($learnedWordsCounter.text());
            }
            else {
                learnedWordsCount = Number(learnedWordsCount);
                setTimeout(function () { sessionStorage.removeItem('learnedWordsCount') }, 60000);
            }
            renderLearnedWordsCount();
            incrementLearnedWordsCount();
        });
        window.onload = function () {
            $('.ph-modal').modal('show');

        }
    </script>



