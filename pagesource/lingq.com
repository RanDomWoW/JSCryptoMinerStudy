    <script type="text/javascript">
        var $videoDialog;
        var $ = jQuery;

        function openIntroVideo() {
            var src = '//www.youtube.com/embed/f21NDFNrzPM?enablejsapi=1&amp;autoplay=1';
            $('#lingq-video').modal('show');
            $('#lingq-video iframe').attr('src', src);
