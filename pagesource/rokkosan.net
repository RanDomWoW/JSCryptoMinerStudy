   $(function() {
        //PAGE TOPボタン用
        var topBtn = $('#page-top');    
        topBtn.hide();
        //スクロールが100に達したらボタン表示
        $(window).scroll(function () {
            if ($(this).scrollTop() > 100) {
                topBtn.fadeIn();
            } else {
                topBtn.fadeOut();
            }
        });
        //スクロールしてトップ
        topBtn.click(function () {
            $('body,html').animate({
                scrollTop: 0
            }, 500);
            return false;
        });
    });
</script>

<a id="page-top" href="#top"><img src="http://rokkosan.net/cms/resource/themes/rokkosan_net/images/common/base/page_top.png" width="85" height="85" alt="page_top"></a>

