    <header data-ng-class="headerClass" ui-view="header" class="shadow-2"></header>
    <div class="all-bg" data-ng-show="showMenuBg==true" data-ng-cloak></div>

    <section role="main">

        <div class="up-btn" data-up-to-page>
            <img src="https://bimeks.akamaized.net/assets/img/genel/up.png" alt="up" />
        </div>

        <div ui-view="main">
</div>

        <section class="loader-root" data-ng-controller="PreloaderLBController" data-ng-show="togglePreloaderLB" data-ng-cloak>
            <div class="loader-bg"></div>
            <div class="img-item">
                <img src="https://bimeks.akamaized.net/assets/img/common/bimeksLoader.gif" alt="loading" />
            </div>
        </section>

        <section class="lb-root" data-ng-cloak data-ng-controller="CartLBController" data-ng-show="toggleCartLB && isCartLbOK==true" data-no-scroll="{{toggleCartLB}}">

            <div class="lb-bg"></div>

            <div class="lb-block added-cart-root">

                <div class="header-block-2">
                    <h2 class="type-5">Ürün başarıyla sepetinize eklendi.</h2>
                    <span class="sprite-common" data-ng-click="toggleLB(false)"></span>
                    <div class="clearfix"></div>
                </div>

                <div class="process-block">
                    <div class="process-item" data-ng-repeat="pr in prodInfo">
                        <div class="bg-item">
                            <span class="sprite-common done"></span>
                        </div>
                        <!-- <div class="cost-block">
                             <span>{{pr.Price}} {{pr.Currency}}</span>
                         </div>-->
                             <div
                        <div class="text-block">
                            <p class="segoe_uilight" data-ng-bind-html=pr.Desc></p>
                            
                        </div>
                        <div class="img-block">
                            <img data-ng-src="{{pr.ImageUrl}}" alt="{{pr.Desc}}" data-on-error-src="/img/common/noImg.jpg" />
                        </div>

                        <div class="clearfix"></div>
                    </div>
                </div>



                

                <div class="suggestion-container t-2" data-slider-per-item="othersAlsoBought" data-ng-show="othersAlsoBought.length>0">

                    <div class="header-block-3">
                        <h3 class="type-9">Bu ürünü alanlar aşağıdaki ürünleri de aldılar</h3>
                    </div>

                    <div class="nav-block arrow-block">
                        <span class="sprite-common"></span>
                        <span class="sprite-common"></span>
                    </div>

                    <div class="suggestion-block type-2">

                        <div class="suggestion-item" data-ng-repeat="boughtProd in othersAlsoBought">

                            <a href="{{boughtProd.ProductLink}}">
                                <img data-ng-src={{boughtProd.ImageUrl}} alt="{{boughtProd.Desc}}" />

                                <div class="cost-block">
                                    <span>{{boughtProd.Price}} {{boughtProd.Currency}}</span>
                                </div>
                                <p>{{boughtProd.Desc}}</p>
                            </a>

                            <span class="btn-3" data-ng-click="addToCart([boughtProd.Id])" data-ng-if="boughtProd.IsAdded==false || boughtProd.IsAdded==null">Ekle </span>
                            <span class="btn-3" data-ng-if="boughtProd.IsAdded==true">Eklendi</span>

                        </div>

                        <div class="clearfix"></div>
                    </div>
                </div>

                <div class="btn-item">
                    <button data-ng-click="gotoPayment()" class="btn-4">Sepete Git</button>
                </div>

                <div class="clearfix"></div>
            </div>
        </section>

        <section class="lb-root" data-ng-cloak data-ng-controller="CartLBSupportPackController" data-ng-show="toggleSupportCartLB" data-no-scroll="{{toggleSupportCartLB}}">

            <div class="lb-bg"></div>

            <div class="lb-block added-cart-root">

                <div class="header-block-2 t-2">
                    <h2 class="type-5">Tamamlayıcı ürün ya da hizmet ekleyin.</h2>
                    <span class="sprite-common" data-ng-click="toggleLBSupport(false)"></span>
                    <div class="clearfix"></div>
                </div>

                <div class="suggestion-container" data-slider-per-item="supportPackages" data-ng-show="supportPackages.length>0">

                    <div class="nav-block arrow-block">
                        <span class="sprite-common"></span>
                        <span class="sprite-common"></span>
                    </div>

                    <div class="suggestion-block">

                        <div class="suggestion-item" data-ng-repeat="supportPackage in supportPackages">
                            <img data-ng-src="{{supportPackage.ImageUrl}}" alt="{{supportPackage.Desc}}" />
                            <div class="cost-block">
                                <span>{{supportPackage.Price}} {{supportPackage.Currency}}</span>
                            </div>

                            <span class="btn-3" data-ng-click="addToCart([supportPackage.Id],true,supportMainProdId)" data-ng-if="supportPackage.IsAdded==false || supportPackage.IsAdded==null">Ekle</span>
                            <span class="btn-3" data-ng-if="supportPackage.IsAdded==true">Eklendi</span>

                            <p data-ng-bind-html="supportPackage.Desc"></p>
                        </div>

                        <div class="clearfix"></div>

                    </div>

                </div>
                <div class="suggestion-container t-2" data-slider-per-item="othersAlsoBought" data-ng-show="othersAlsoBought.length>0">

                    <div class="header-block-3">
                        <h3 class="type-9">Bu ürünü alanlar aşağıdaki ürünleri de aldılar</h3>
                    </div>

                    <div class="nav-block arrow-block">
                        <span class="sprite-common"></span>
                        <span class="sprite-common"></span>
                    </div>

                    <div class="suggestion-block type-2">
                        <div class="suggestion-item" data-ng-repeat="boughtProd in othersAlsoBought">

                            <a href="{{boughtProd.ProductLink}}">

                                <img data-ng-src={{boughtProd.ImageUrl}} alt="{{boughtProd.Desc}}" />

                                <div class="cost-block">
                                    <span>{{boughtProd.Price}} {{boughtProd.Currency}}</span>
                                </div>
                                <p>{{boughtProd.Desc}}</p>
                            </a>

                            <span class="btn-3" data-ng-click="addToCart([boughtProd.Id])" data-ng-if="boughtProd.IsAdded==false || boughtProd.IsAdded==null">Ekle </span>
                            <span class="btn-3" data-ng-if="boughtProd.IsAdded==true">Eklendi</span>

                        </div>

                        <div class="clearfix"></div>
                    </div>
                </div>
                <div class="clearfix"></div>
            </div>
        </section>

        <!--TODO  Hata mesajalrını kullanan lightbox -->
        <section class="lb-root" data-ng-cloak data-ng-controller="MessageLBController" data-ng-show="toggleMessageLB == true" data-no-scroll="{{toggleMessageLB}}">
            <div class="lb-bg"></div>

            <div class="lb-block safe-area general-lb" data-is-parent-shown="toggleMessageLB" data-ng-cloak>
                <!--TODO is parentshown alignonresize directive içinde kullanılıyor-->
                <i class="sprite-common" data-ng-click="closeMessageLb()"></i>
                <div class="head-item">
                    <span class="sprite-box"></span>
                    <h2>{{messageTitle}}</h2>
                </div>
                <div class="content-item">
                    <p data-ng-bind-html="messageDesc"></p>
                    <span data-ng-if="buttonOKText.length>0" class="btn-1 t-2" data-ng-click="callBackFunc()">{{buttonOKText}}</span>
                    <span data-ng-if="buttonCancelText.length>0" class="btn-1" data-ng-click="closeMessageLb()">{{buttonCancelText}}</span>
                </div>
            </div>
        </section>

        <section class="lb-root iframe-root t-2" data-ng-cloak data-ng-controller="ContentLBController" data-ng-show="toggleContentLB" data-no-scroll="{{toggleContentLB}}">
            <div class="lb-bg"></div>

            <div class="lb-block safe-area general-lb" data-is-parent-shown="toggleContentLB" data-ng-cloak>
                <!--TODO is parentshown alignonresize directive içinde kullanılıyor-->
                <i class="sprite-common" data-ng-click="closeMessageLb()"></i>
                <div class="head-item">
                    <h2>{{title}}</h2>
                </div>
                <div class="content-item">
                    <div data-ng-bind-html="content"></div>
                </div>
            </div>
        </section>

        <section class="lb-root compare-root" data-ng-cloak data-ng-controller="CompareLBController" data-ng-show="toggleCompareLB" data-no-scroll="{{toggleCompareLB}}">
            <div class="lb-bg"></div>

            <div class="lb-block">

                <div class="head-block">
                    <i class="sprite-box delete" data-ng-click="closeCompareLB()"></i>
                    <i class="sprite-box icon"></i>
                    <h3>Karşılaştırma Listesi</h3>
                </div>

                <div class="compare-block" data-ng-if="comparedList.length>0">
                    <ul>

                        <li data-ng-repeat="compareItem in comparedList">
                            <a ui-sref="productDetail({id:compareItem.Link})">

                                <div class="img-item">
                                    <img src="https://bimeks.akamaized.net/assets/img/common/noImg.jpg" data-fit-to-parent data-ng-src="{{compareItem.ImageUrl}}">
                                </div>

                                <div class="text-item">
                                    <p>{{compareItem.Desc}}</p>
                                    <span class="cost" data-ng-if="compareItem.Price">{{compareItem.Price}} {{compareItem.Currency}}</span>
                                </div>
