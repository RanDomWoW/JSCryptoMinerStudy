<script type="text/javascript">
$(function(){
var target = $('#mainImage'),timer,size;
var el = {
nav : target.children('.imageNav'),
main: target.children('.imageMain')
}
var config = {
opacity : 0.5,        //    透明度
navDration: 300,    //    ナビフェードアニメーション時間(ms)
dration: 600,        //    フェードアニメーション時間(ms)
delay: 5000            //    切り替え時間(ms)
};
var getMainImgPath = function(path){
var t = path.replace('s.','.');
return t;
};
var autoSlideShow = function(i){

var stay = $(el.main.children('div').get(i++));
if(size<=i) i=0;
var next = $(el.main.children('div').get(i));
var nav = $(el.nav.find('a').get(i));
stay.css("background", "#ffffff");
next.css("background", "#ffffff");
nav.css("background", "#ffffff");
stay.animate({'opacity':0},config.dration).css('zIndex','1');
next.animate({'opacity':1},config.dration).css('zIndex','2');
nav.animate({'opacity':config.opacity},config.dration,function(){
nav.animate({'opacity':1},config.dration);
});


//autoSlideShow(i);
timer = setTimeout(function(){
autoSlideShow(i);
},config.delay);

}
size = el.nav.find('a').length;


// format
el.nav.find('a').each(function(i){

var t = $(this),url,path,alt;
url = t.attr('href');
path = t.children('img').attr('src');
alt = t.children('img').attr('alt');
target = t.attr('target');
el.main.append('<div><a href="'+url+'" title="'+alt+'" target="'+target+'"><img src="'+getMainImgPath(path)+'" width="412" height="248" alt="'+alt+'" class="fadeover" /></a></div>');

t.click(function(){
var next = $(el.main.children('div').get(i));
clearTimeout(timer);
el.main.children('div').each(function(){
if($(this).css('opacity') != 0)
$(this).animate({'opacity':0},config.dration).css('zIndex','1');
});

next.animate({'opacity':1},config.dration).css('zIndex','2');
timer = setTimeout(function(){
autoSlideShow(i);
},config.delay);
return false;
}).hover(function(){
$(this).css("background", "#ffffff");
$(this).animate({'opacity':config.opacity},config.navDration);
},function(){
$(this).animate({'opacity':1},config.navDration)
});
});

$(el.main.children('div').get(0)).css({'opacity':1,'zIndex':2,'backgroundColor':'#ffffff'});

// timer
timer = setTimeout(function(){
autoSlideShow(0);
},config.delay);

// stop timer
el.main.children('div').each(function(index){
$(this).hover(function(){
clearTimeout(timer);
},function(){
timer = setTimeout(function(){
autoSlideShow(index);
},config.delay/2);
});
})


});
</script>
