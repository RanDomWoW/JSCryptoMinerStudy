<script>
$(".promotion-slot").on("click", "a", function(e) {
	e.preventDefault();
 	var gtagdata = JSON.parse($(this).closest(".promotion-slot").attr("data-gtag"));
 	var url = this.href;
 	
 	dataLayer.push({
 		'event': 'promotionClick',
		'ecommerce': {
			'promoClick': {
				'promotions': [{
					'id': gtagdata.id,
					'name': gtagdata.name,
					'position': gtagdata.position,
				}]
			}
		},
		'eventCallback': function() {
			document.location = url
		}
	});
	
	return true;
});
</script>
