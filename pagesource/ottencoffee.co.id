          <div data-ng-repeat="variant in productModal.variants" data-ng-class="{clearfix: !(($index+1)%4) || $last}">

            <div class="col-sm-4 col-md-3 col-lg-3 no-padding">
              <label class="variant-card">
                <div class="label-sold" data-ng-show="variant.is_out_of_stock">
                  <p>Habis</p>
                </div>
                <div>
                  <img width="100%" data-ng-src="{{ variant.default_image.thumbnail_url }}" alt="{{ variant.name }}" />
                </div>
                <div class="variation-modal-attributes-name" data-ng-bind="variant.attributes_name"></div>
                
                  <div data-ng-if="productModal.is_flash_sale && !variant.is_out_of_stock">
                    <input type="radio" name="flash_sale_product" data-ng-value="{{ variant.id }}" data-ng-model="productForm.product_variant">
                  </div>

                  <div data-ng-if="!productModal.is_flash_sale && !variant.is_out_of_stock">
                    <input type="radio" name="product_variant" data-ng-value="{{ variant.id }}" data-ng-model="productForm.product_variant">
                  </div>
                
              </label>
            </div>
          </div>

          <div class="row text-center">
            <div data-ng-repeat="option in productModal.options" data-ng-class="{clearfix: !(($index+1)%4) || $last}">
              <div class="col-sm-4 col-md-3 col-lg-3 no-padding">
                <label class="option-card">

                  <div>
                    <img width="100%" data-ng-src="{{ option.image.thumbnail_url }}" alt="{{ option.name }}" />
                  </div>
                  
                    <div>{{ option.key }}</div>
                    <p class="option-name">{{ option.value }}</p>
                    <input type="radio" name="option" data-ng-value="{{ option.id }}" data-ng-model="productForm.optionId">
                  
                </label>
              </div>
            </div>
          </div>

        </div>
