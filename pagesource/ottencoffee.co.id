            <label class="option-card">
              <div>
                <img width="100%" data-ng-src="{{ option.image.thumbnail_url }}" alt="{{ option.name }}" />
              </div>
              
                <div>{{ option.key }}</div>
                <p class="option-name">{{ option.value }}</p>
                <input type="radio" name="option" data-ng-value="{{ option.id }}" data-ng-model="productForm.optionId">
              
            </label>
          </div>
        </div>
      </div>

      <div class="row text-center">
        <div data-ng-if="productModal.is_flash_sale">
          <button type="button" class="btn btn-buy btn-modal-close-variant"
            data-ng-click="addFlashSaleProductToCart(productForm.product_variant, productForm.quantity, productForm.optionId)"
            data-ng-disabled="!productForm.product_variant">
            BELI
          </button>
        </div>
        <div data-ng-if="!productModal.is_flash_sale">
          <button type="button" class="btn btn-buy btn-modal-close-variant"
            data-ng-click="addProductVariantToCart(productForm.product_variant, productForm.quantity, productForm.optionId)"
            data-ng-disabled="!productForm.product_variant">
            BELI
          </button>
        </div>
      </div>
    </div>
  </div>
</div>
