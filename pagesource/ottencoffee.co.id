              </div>
              <label>
                <div>
                  <img width="100%" data-ng-src="{{ variant.default_image.thumbnail_url }}" alt="{{ variant.name }}" />
                </div>
                
                  <div class="variation-modal-attributes-name" data-ng-bind="variant.attributes_name"></div>
                  <p data-ng-if="variant.formatted_price == variant.formatted_discount_price" data-ng-bind="variant.formatted_price"></p>
                  <s data-ng-if="variant.formatted_price !== variant.formatted_discount_price" data-ng-bind="variant.formatted_price"></s>
                  <div
                  class="text-highlight "
                  data-ng-if="variant.formatted_price !== variant.formatted_discount_price"
                  data-ng-bind="variant.formatted_discount_price">
                  </div>
                  <input data-ng-show="!variant.is_out_of_stock" type="radio" name="product_form_variant" data-ng-value="{{ variant.id }}" data-ng-model="productForm.product_variant">
                
                                <input data-ng-show="variant.is_out_of_stock" placeholder="Email" class="form-control stock-reminder" type="text" data-ng-init="email[$index] = ''" data-ng-model="email[$index]" style="width: 100%">
                <button class="btn btn-primary btn-block" data-ng-show="variant.is_out_of_stock" data-ng-click="submitRestockSubscription(variant.id, $index)">INGATKAN</button>
              </label>
            </div>
          </div>
        </div>
