    class="section list student_groups-widget">
      <div id="your-students-groups-wrap" class="list-wrap">
        	    	
	<div class = "uc-widget-groups">
		<div id="uc-widget-groups-49834" class="uc-widget-groups-wrapper">
			<input type="hidden" name="widget_id" id="widget_id" value="uc-widget-groups-49834" />
			<script>
				var offset = 0;
				$().ready(function() {
					$.get("/widget/display_groups_widget?id=uc-widget-groups-49834&amp;page_id=49834",
						function(){
				        }).done(function() {
				        	offset = 0;
				        	$("#group_search_keyword").keypress(function(e) {
							  	$("#group_search_keyword").keyup(function(e) {
								  	if ($(this).val().length > 3){
								  		offset = 0;
										load_groups();
								  	}
							  	});
						     });

							$('#group_search_keyword').keydown(function(e) {
								offset = 0;
								if (e.keyCode == 13) {
									load_groups();
								}
							});

						});

						function load_groups() {
							var search_text = $('.uc-widget-group-search #group_search_keyword').length > 0 ? encodeURIComponent($('.uc-widget-group-search #group_search_keyword').val()) : "";
							$('.uc-widget-groups-wrapper').block({
					           overlayCSS: { backgroundColor: '#000' },
					           message: '<img src="/assets/default/unioncloud-processing.gif" alt=""/>',
					           css: {backgroundColor: 'none',border: '0px solid #000000',padding : '10px 10px 10px'}
					   		});

              group_cat_and_groups_path = rewrite_url_for_js("/widget/load_more_groups");
							$.ajax({
								type : "GET",
								url : group_cat_and_groups_path,
								style : "inherit",
								dataType : 'script',
								data : "&offset=" + offset+ "&search_text="+search_text,
								success : function(data) {
								  $('.uc-widget-groups-wrapper').unblock();
									return true;
								},
								failure : function(data) {
									alert("Server facing some issue while performing your request, Please try again after few minutes...");

								}
							});
						}

						$(".uc-widget-groups-container #groups-next-page").live("click",function(){
							offset = offset + 6;
							load_groups();
						});

						$(".uc-widget-groups-container #groups-previous-page").live("click",function(){
							offset = offset - 6;
							load_groups();
						});

					    /* On click on event type link, un select all
					     * and set current one  selected.
					     */
						$(".uc-widget-group-search .uc-widget-group-search-button").live("click", function() {
							offset = 0;
							load_groups();
						});
				});
			</script>
		</div>
	</div>
		    	

