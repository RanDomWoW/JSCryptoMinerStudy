  <script>
    // When the fade-in animation stops, set the element's opacity to 1 instead of using
    // -webkit-animation-fill-mode -- this is so we can parallax it after the fact
    jQuery(document).ready( function(){
      $(".front-page-hero span").one('webkitAnimationEnd oanimationend msAnimationEnd animationend',
      function(e) {
        $(".front-page-hero span").css("opacity", "1")
      });
      initPos = $(".front-page-hero span").css("right");
      $(".front-page-hero span").attr("data-init", initPos );
      // Now we parallax!
      $(window).scroll( function(event) {
        var xPos = ($(window).scrollTop() / -3) + parseInt( $(".front-page-hero span").attr("data-init"), 10 );
        $(".front-page-hero span").css("right", xPos);
      });
    });
  </script>

