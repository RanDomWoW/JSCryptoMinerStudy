        function fblogin() {
            document.getElementById('ctl00_hidFb').value = "";
            FB.login(function (response) {
                if (response.authResponse) {
                    //登入成功
                    FB.api('/me', function (response) {
                        var html = '';
                        for (var key in response) {
                            if (html != "")
                                html += ",";
                            html += (key + ";" + response[key]);
                        }
                        //html += ";email:" + response.email;
                        document.getElementById('ctl00_hidFb').value = html;
                        appGetPermission = true;
                        document.getElementById("ctl00_btn1").click();
                        return false;
                    });
                }
                else {
                    //登入失敗
                    alert("登入失敗。");
                }
            }, { "scope": "publish_actions" }); //email,            
        }

        
    </script>
