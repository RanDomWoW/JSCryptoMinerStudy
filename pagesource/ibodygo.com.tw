        var appGetPermission = false;

        


        function oplogin() {

            FB.init({ appId: '593158390759311', status: true, cookie: true, xfbml: true, channelUrl: 'https://www.ibodygo.com.tw/index.aspx' }); //appid請去FB申請應用程式
