<script>
var cindex=0;

function autorotate()
{
	if(cindex > 4)
	{
		cindex=0;
	}
	else
	{
		cindex++;
	}
	goto(cindex);
}

var hCarousel = null;

var oldOnLoad = window.onload;
window.onload = onload_function;
function onload_function()
{
	oldOnLoad();
	hCarousel = new UI.Carousel("horizontal_carousel");
	carousel = setInterval ( "autorotate()", 12700 );
}

function goto(index)
{
