.filmBlock {
height: 300px;
width: 204px !Important;
float: left;
transition: .3s height ease , .3s margin-top ease;
margin-left: 8px;
margin-bottom: 15px;
overflow: hidden;
list-style: none;
box-sizing: border-box;
box-shadow: 0px 4px 10px 1px rgba(0, 0, 0, 0.55);
position: relative;
border: 3px solid #242121;
}
.filmBlock img {
width: 100%;
height: 100%;
transition: .3s all ease;
position: relative;
z-index: 2;
}
.filmBlock .views {
position: absolute;
bottom: 0;
transform: rotate(150deg) scale(0);
left: 0;
opacity: 0;
color: white;
transition: .3s .2s all ease;
padding-top: 13px;
font-size: 14px;
width: 100%;
height: 39px;
background: rgb(32, 32, 32);
z-index: 3;
text-align: center;
}
.filmBlock .title {
position: absolute;
bottom: 0;
right: 0;
width: 100%;
background: rgba(0, 0, 0, 0.9);
z-index: 3;
color: white;
text-align: center;
font-size: 12px;
padding: 6px;
box-sizing: border-box;
transition: .6s transform ease , opacity .3s .2s;
}
.filmBlock:hover img {
transform: scale(1.2);
}
.filmBlock:hover .views {
-webkit-animation: bounceInRight 1s both ease;
animation: bounceInRight 1s both ease;
opacity: 1;
}
.filmBlock:hover .title {
-webkit-animation: wobble 1s both ease;
animation: wobble 1s both ease;
opacity: 0;
}
