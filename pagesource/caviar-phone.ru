        (function(_, $) {
            $(document).ready(function($){
                var rotateBanners = $("#rotate-banner-left-big-banner .rotate-banner-item");

                // читаем куку
                var currentRotateNumberOfBanner = getCookie('currentRotateNumberOfBanner');

                // если ошибка, то приравниваем к 1
                if(!currentRotateNumberOfBanner || !parseInt(currentRotateNumberOfBanner)){
                    currentRotateNumberOfBanner = 1;
                }

                $(rotateBanners).hide();
                $(rotateBanners[currentRotateNumberOfBanner - 1]).show();

                if(currentRotateNumberOfBanner >= rotateBanners.length){
                    setCookieWithProps('currentRotateNumberOfBanner', 1, {  expires: 60*60*24*365, path: '/', domain: global.gdomain});
                }else{
                    setCookieWithProps('currentRotateNumberOfBanner', parseInt(currentRotateNumberOfBanner) + 1, {  expires: 60*60*24*365, path: '/', domain: global.gdomain});
                }

            });

        }(Tygh, Tygh.$));

    </script>
