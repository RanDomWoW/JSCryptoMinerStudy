    function search()
    {
        var w = document.getElementById("keyword").value;
        var c = document.getElementById("search-country").value;
        var search_segment = "search";

        if (w.length == 0 || (w.length == 0 && c.length == 0) )
        {
            return false;
        }

        var url =  "/" + c + "/" + search_segment + "/" + url_encode(w.toLowerCase());
        
        window.location.href = url;
        
        return false;
    }

    function url_encode(str)
    {
      str = (str + '').toString();
      return encodeURIComponent(str).replace(/!/g, '%21').replace(/'/g, '%27').replace(/\(/g, '%28').replace(/\)/g, '%29').replace(/\*/g, '%2A').replace(/%20/g, '+');
    }

