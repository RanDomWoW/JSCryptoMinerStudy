                        $(document).ready(function() {
                            var allElems = document.getElementsByClassName("homeNewsKey")[0].innerHTML;

                            $("#newsFilter").click(function(){
                                if ($(this).hasClass("opened")) {
                                    $(this).removeClass("opened");
                                    $("#newsFilterList").hide();
                                } else {
                                    $(this).addClass("opened");
                                    $("#newsFilterList").show();
                                }
                            });

                            var val,key;
                            var keyArr = [];

                            $("#newsKeys div.homeNewsKey").click(function(){
                                $(this).remove();

                                newsAjax();

                            });

                            $("#newsFilterList ul li").click(function(){

                                var elems = document.getElementsByClassName("homeNewsKey");
                                var length = elems.length;

                                keyArr = [];

                                for(x=0;x<length;x++){
                                    keyArr.push(elems[x]["innerHTML"]);
                                }

                                val = $(this).html();

                                if(jQuery.inArray(val,keyArr) <= -1){
                                    $("#newsKeys").prepend("<div class='homeNewsKey fl'>"+val+"</div>");
                                    for(y=0;y<length;y++){
                                        if(keyArr[y] == "ALL") {
                                            keyArr.splice(y,1);
                                            $(".homeNewsKey:last-child").remove();
                                        }
                                    }
                                    $("#newsKeys div.homeNewsKey").click(function(){
                                        $(this).remove();

                                        var elems = document.getElementsByClassName("homeNewsKey");
                                        var length = elems.length;

                                        keyArr = [];
                                        for(x=0;x<length;x++){
                                            keyArr.push(elems[x]["innerHTML"]);
                                        }
                                        console.log(length);
                                        if (length == 0) {
                                            newsAjax();
                                            $("#newsKeys").prepend("<div class='homeNewsKey fl'>ALL</div>");
                                        } else {
                                            newsAjax();
                                        }
                                    });
                                    if(length === 3){
                                        $(".homeNewsKey:last-child").remove();
                                    }
                                    newsAjax();
                                }
                            });

                            function newsAjax(){
                                keyArr = [];
                                key = "";

                                elems = document.getElementsByClassName("homeNewsKey");
                                length = elems.length;
                                for(x=0;x<length;x++){
                                    keyArr.push(elems[x]["innerHTML"]);
                                }
                                for(z=0;z<length;z++){
                                    if(keyArr[z] == "ALL") {
                                        keyArr.splice(z,1);
                                    }
                                }

                                for(y=0;y<keyArr.length;y++){
                                    if(y == 0){
                                        key += keyArr[y];
                                    } else {
                                        key += "||"+keyArr[y];
                                    }
                                }
                                $.ajax({
                                    url: "_ajax.php",
                                    data: "ajax=getNewsContent&key="+key+"&cookie=",
                                    type: "POST",
                                    success: function(data){

                                        $("#homeItemAjax").remove();
                                        $("#homeItemGroup").html(data);

                                    }
                                });

                            }
                        });
