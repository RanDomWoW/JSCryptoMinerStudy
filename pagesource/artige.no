<!-- ALERT -->
<style>
    .a-alert-placeholder {
        position: fixed; 
        bottom: 0px; 
        width: 100%;
        z-index: 1150;
        text-align: center;
        pointer-events: none;
    }

    .a-alert {
        text-transform: uppercase;
        padding: 18px 30px;
        margin: 0;
        vertical-align: middle;
        /*visibility: hidden;*/
    }
</style>

<div id="js-alert-show" class="a-alert-placeholder" style="display: none;">
    <div id="js-alert-type" class="a-alert alert">
        <span id="js-alert-icon" aria-hidden="true"></span> 
        <span id="js-alert-text">An alert!</span></div>
    </div>
</div>

<script type="text/javascript">
    'use strict';

    //moduleAlert
    //.send(info)
    //moduleAlert.send({type: 'success', message: 'testing alerts'});
    //type: [success, warning, danger]
    artige.moduleAlert = (function(){ //Pass dependencies from global namespace
        var $show = $('#js-alert-show');
        var $type = $('#js-alert-type');
        var $icon = $('#js-alert-icon');
        var $text = $('#js-alert-text');
        var _timeoutHandle;
        var _config = {alertTimeoutTime: 4000};

        var send = function(info){
            clearTimeout(_timeoutHandle);
            $type
                .removeClass()
                .addClass('a-alert alert ' + 'alert-' + info.type);
            _changeAlertIcon(info.type);
            $text.text(info.message);
            $show.show();
            _addAlertTimeout();
        };

        var _changeAlertIcon = function(type){
            $icon.removeClass();
            switch(type){
                case 'success':
                    $icon.addClass('icon-checkmark');
                    break;
                case 'warning':
                    $icon.addClass('icon-warning');
                    break;
                case 'danger':
                    $icon.addClass('icon-lightning');
                    break;
            }
        };

        var _addAlertTimeout = function(){
            _timeoutHandle = setTimeout(function(){
                $show.hide();
            }, _config.alertTimeoutTime);
        };

        var tryDisplayFlash = function(){
            var raw = $.cookie('flash');
            $.removeCookie('flash', { path: '/' });
            if (raw) {
                var obj = $.parseJSON(raw);
                send(obj);
            }
        };

        /**
         * artige.moduleAlert.addFlash({type: 'danger', message: 'yolo'});
         */
        var addFlash = function(info){
            var flash = JSON.stringify(info);
            $.cookie('flash', flash, { path: '/' });
        };

        return {
            send: send,
            tryDisplayFlash: tryDisplayFlash,
            addFlash: addFlash
        };
    })();

    $(document).ready(function(){
        artige.moduleAlert.tryDisplayFlash();
    });
</script>
<!-- /ALERT -->

