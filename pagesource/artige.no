<!-- GIFLOAD JS -->
<script type="text/javascript">
    'use strict';

    artige.moduleGifLoad = (function(){ //pass dependencies
        var init = function(){

            var $gifsOnPage = $('.a-gif');

            $gifsOnPage.each(function(){
                var $gif = $(this);
                var $anchor = $gif.parent('div').parent('a');
                var $loader = $gif.next('.a-gif-play');
                var bigUrl = $gif.attr('data-big-src');

                var clicked = false;
                $anchor.click(function(event) {
                    if ( ! clicked ) {
                        event.preventDefault();
                        event.stopPropagation();
                    }
                    $loader.attr('src', BASEURL + '/img/load.gif')
                    $gif.attr('src', bigUrl);

                    $gif.load(function(){
                        $loader.hide();
                        clicked = true;
                    });
                });
            });

        };

        return {
            init: init,
        }
    })();

    $(document).ready(function(){
        artige.moduleGifLoad.init();
    });
    
</script>
