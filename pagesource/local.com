<script type="text/javascript">
	$("#search").autocomplete({
	    source: function(request, response) {
	        $.ajax({
	            url: '/business/autocomplete',
	            type:'POST',
	            data: {
	                q : request.term,
	                type : 'cat'
	            },
	            success: function(data) {
	              var result=JSON.parse(data);
	                response($.map(result['keywordResults'], function (item) {
	                                return {
	                                    label: item.name,
	                                    value: item.name
	                                }
	                            }));
	            }
	        });
	    },
	    min_length: 2
	});

	$("#nearby").autocomplete({
	    source: function(request, response) {
	        $.ajax({
	            url: '/business/autocomplete',
	            type:'POST',
	            data: {
	                q : request.term,
	                type : 'area'
	            },
	            success: function(data) {
	              var result=JSON.parse(data);
	                response($.map(result['locationResults'], function (item) {
	                                return {
	                                    label: item.name,
	                                    value: item.name
	                                }
	                            }));
	            }
	        });
	    },
	    min_length: 2
	});
</script>


