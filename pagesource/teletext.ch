    <script type="text/javascript">
        Txt.root = '/';
        var parameters = {"mode":"teletext","category":"home","channel":"srf1","page":"100"};
        if(parameters) {
            parameters.language = 'de';
        }
        Txt.Ads.renderAds(parameters);

        $('header .logo').click(function() {
            if (Txt.Teletext != undefined) {
                var teletext = Txt.Teletext.instance;
                teletext.navigateTo(teletext.channel(), 100);
            } else {
                var url = '/de/Webtext';
                window.location.href = url;
            }
        });

        // Check if user has already seen the teaser
        if(!$.cookie("seenOverlay")){
            setTimeout(function(){$('.webteaser').fadeIn(300);}, 1000);
        }

        $('.webteaser .close').on('click', function(e){
            e.stopPropagation();
            e.preventDefault();

            $.cookie("seenTeaser", true, {expires: 20*365});
            $('.webteaser').fadeOut(200);
        });
    </script>
