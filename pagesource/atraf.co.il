<script language="javascript" type="text/javascript">
    function check_fields(isMobile) {
        var form_valid = true;
        var username = $("#username").val();
        var password = $("#password").val();
        if (username.length > 0 && password.length > 0) {
            form_valid = true;
        }
        else {
            form_valid = false;
        }
        if (form_valid) {
            window.document.getElementById("btnSubmit").disabled = true;
            if ($("body").outerWidth(true) < 975) {
                //                var link = 'http://m.atraf.co.il' + "/login/checkLogin.aspx";
                var link = 'http://dating.atraf.co.il' + "/login/checkLogin.aspx";
            }
            else {
                var link = 'http://dating.atraf.co.il' + "/login/checkLogin.aspx";
            }
            document.getElementById("Form1").action = link;
            document.getElementById("Form1").submit();
        }
        else {
            window.document.getElementById("btnSubmit").disabled = false;
        }
        return false;
    }

    function PopUpOpen(urlstr) {
        popup_win = window.open(urlstr, '', 'alwaysRaised, left=150, top=100, height=350, width=700, status=no,toolbar=no,menubar=no,location=no,scrollbars=1');
        popup_win.opener = self;
    }

</script>
<script language="javascript" type="text/javascript">
    function doSearch() {
        var strS = new String(document.getElementById('searchInput').value);
        var srchType = "atraf";
        if (strS.length >= 3) {
            //document.location.href = '/search/default.aspx?s=' + encodeURIComponent(strS);
            if (srchType == "atraf") {
                document.location.href = '/search/default.aspx?s=' + encodeURIComponent(strS) + '&t=' + srchType;
            }
            else if (srchType == "dating") {
                document.location.href = 'http://dating.atraf.co.il/MembersSearch/membersNameSearch.aspx?s=' + encodeURIComponent(strS) + '&t=' + srchType;
            }
            else if (srchType == "atrafblogs") {
                document.location.href = '/search/default.aspx?s=' + encodeURIComponent(strS) + '&t=' + srchType;
            }
            else {
                return false;
            }
        }
        else {
            window.alert('נא להזין לפחות 3 תווים');
            document.getElementById('searchInput').focus();
        }
        return false;
    }

    function entsubsearch(e) {
        if (typeof (e) == "undefined" && typeof (window.event) != "undefined") {
            e = window.event;
        }
        if (e && e.keyCode == 13) {
            return doSearch();
        }
        else {
            return true;
        }
    }

</script>
<script>
    $(function () {
        $("form input").keypress(function (e) {
            if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
                $('button[type=submit] .default').click();
                check_fields(false);
            } else {
                return true;
            }
        });
    });

</script>
