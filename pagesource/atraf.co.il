<script>
    function openSearch() {
        if (document.getElementById("searchInput").style.display == "none") {
            document.getElementById("searchIcon").style.display = "none";
            document.getElementById("searchIconWhite").style.display = "none";
            document.getElementById("searchInput").style.display = "block";
            document.getElementById("searchInput").focus();
            document.getElementById("searchbtn").style.display = "block";
            //            document.getElementById("searchIcon").style.visibility = "hidden";

        }
        else {

            document.getElementById("searchIcon").style.display = "block";
            document.getElementById("searchIconWhite").style.display = "block";
            document.getElementById("searchInput").style.display = "none";
            document.getElementById("searchbtn").style.display = "none";
            //            document.getElementById("searchIcon").style.visibility = "visible";
        }

    }
</script>
<script type="text/javascript">
    function changeLang(myId) {
        var ChangeLang = document.getElementById(myId);
        var currentUrl = document.URL;
        if (currentUrl.indexOf("atraf.co.il/") > -1) {
            currentUrl = currentUrl.replace("atraf.co.il/", "atraf.co.il/en/");
            ChangeLang.setAttribute('href', currentUrl);
        }
        else if (currentUrl.indexOf("atraf.co.il") > -1) {
            currentUrl = currentUrl.replace("atraf.co.il", "atraf.co.il/en");
            ChangeLang.setAttribute('href', currentUrl);
        }
    }
</script>

