.dugka li:nth-child(7)::before { content: 'ё)';}
.dugka li:nth-child(8)::before { content: 'ж)';}
.dugka li:nth-child(9)::before { content: 'з)';}
.dugka li:nth-child(10)::before { content: 'и)';}
