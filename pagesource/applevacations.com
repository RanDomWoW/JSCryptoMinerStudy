<script>
 	document.getElementById("selectedGatewayTemp").value = returnGateway("selectedGateway");
	document.getElementById("selectedDestinationTemp").value = returnGateway("selectedDestination");
	function _addDest(value, name) {
	
		document.getElementById("selectedDestinationTemp").value = name;
		document.getElementById("selectedDestination").value = value; 
		//HideContent('destinationlist');

	}
	function _add(value, name) {
		document.getElementById("selectedGatewayTemp").value = name;
		document.getElementById("selectedGateway").value = value;
		//HideContent('gatewaylist');

	}

	function returnGateway(obj){
	var myselect = document.getElementById(obj);
		for ( var i = 0; i < myselect.options.length; i++) {
			if (myselect.options[i].selected == true)
				return myselect.options[i].text;
		}
		return "";
	}	
	function SelectAll(id) {
		document.getElementById(id).focus();
		str = id.substring(0, id.lastIndexOf('T'));
		num = document.getElementById(str).value.length;
		if (num == 3)
			document.getElementById(id).select();
		else
			document.getElementById(id).value = "";

	}
	
</script>


