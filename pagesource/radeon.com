        <script>
            function bgSetup(){
                width = $("#background").outerWidth();
                height = $("#background").outerHeight();
                if(width/height > 16/9){
                    console.log("need height");
                    new_height = width*9/16;
                    $("#bgplayer-wrapper").height(new_height);
                    $("#bgplayer-wrapper").width(width);
                }else{
                    console.log("need width");
                    new_width = height*16/9;
                    $("#bgplayer-wrapper").height(height);
                    $("#bgplayer-wrapper").width(new_width);
                }
            }

            var tag = document.createElement('script');
            tag.src = "https://www.youtube.com/iframe_api";
            var firstScriptTag = document.getElementsByTagName('script')[0];
            firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

            var player;
            function onYouTubeIframeAPIReady() {
                player = new YT.Player('bgplayer', {
                    events: {
                        'onReady': onPlayerReady,
                        'onStateChange': onPlayerStateChange
                    }
                });
            }

            var volume = 0;
            function onPlayerReady(event) {
                bgSetup();
                event.target.playVideo();
                volume = event.target.getVolume();
            }

            var status = "";
            var play = "undefined";
            var end = 65;
            function onPlayerStateChange(event) {
                play = setInterval(function () {
                    var ptime = player.getCurrentTime();
                    if(ptime >= end-10){
                        player.setVolume(Math.ceil(volume*((end-ptime)/10)));
                    }
                    if (ptime >= end) {

                        $("#bgplayer-wrapper").fadeOut(1000, function () {
                            $(".play").css("opacity",1);
                            player.setVolume(volume);
                            player.stopVideo();
                            $(".radeon-network").css("opacity",1);
                        });
                    }
                }, 1000);

                if (event.data == YT.PlayerState.PLAYING) {
                    status = "play";

                    $(".play").css("opacity",0);
                    $(".radeon-network").css("opacity",0.15);
                    $("#bgplayer-wrapper").fadeIn(300);
                }


                if (event.data == YT.PlayerState.ENDED) {
                    clearInterval(play);
                    status = "stop";
                    $(".radeon-network").css("opacity",1);
                    $("#bgplayer-wrapper").fadeOut(1000, function(){
                        $(".play").css("opacity",1);

                    });
                }

                if (event.data == YT.PlayerState.BUFFERING) {
                    status = "buffer";
                    $(".radeon-network").css("opacity",1);
                }

                if (event.data == YT.PlayerState.PAUSED) {
                    status = "pause";
                    $(".radeon-network").css("opacity",1);
                }
            }

            $(window).resize(function(){
                bgSetup();
            });

            $(".shadow-top, .shadow-bottom").click(function(){
                if(status == "play") {
                    player.pauseVideo();
                }else if(status == "pause"){
                    player.playVideo();
                }
                return false;
            });
            $(".play").click(function(){
                player.playVideo();
            })
        </script>
    </div>
