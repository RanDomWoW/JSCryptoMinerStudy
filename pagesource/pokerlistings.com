					context = '<div class="success">' + "Success!" + '</div>';

					$('.Bnewsletter-form-box .af-element').hide();
					$('.Bnewsletter-form-box .affirmation').html(context).show();
				} else {
					$('.Bnewsletter-form-box .email-capture-controls').css('margin-top', '-15px');
					switch (response.type) {
						case 'firstname':
							context = "<div class=\"fail\"><span class=\"error\">Firstname cannot be blank</span></div>";
							break;
						case 'lastname':
							context = "<div class=\"fail\"><span class=\"error\">Lastname cannot be blank</span></div>";
