		jQuery(document).ready(function(){
			var list = $("#upcomingFreerollList-USP");
			var tmplt = list.children().html();
			var template = tmplt.replace('display: none;','');
			list.html('<center>loading...</center>');
			$(this).each(function() {
				list.html('');
				var jsonData = [];
				if( !( jsonData.items instanceof Array ) ) {
					return;
				}
				$.each(jsonData.items.slice(0,4), function(i, rowData) {
					list.append(renderRow(template, rowData));
				});
				function renderRow(temp,data){
					infos = temp.match(/\{(\w+)\}/g);
					for (var i=0;i<infos.length;i++){
						val = data[infos[i].replace(/[\{\}']+/g,'')];
						temp = temp.replace(infos[i], val);
					}
					return temp;
				}
			});
		});

