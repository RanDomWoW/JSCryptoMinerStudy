    <script>
        if (typeof authSuccess == 'undefined')
            authSuccess = function()
            {
                window.location.reload();
            };
        
        $(document).ready(function()
        {
            $('#sign-in-form input[name=phone]').mask('+7 (999) 999-9999');
            $('#sign-in-form input[name="card"]').mask('3 299999 999999');
            
            $('#sign-in-form input[name="login-way"]').on('click', function() {
                $('#login-by-email, #login-by-phone, #login-by-card').hide();
                $('#login-by-'+$('#sign-in-form input[name="login-way"]:checked').val()).show();
            });
            
            $('#sign-in-form a.forget').click(function()
            {
                var sign = encodeURIComponent($('#sign-in-form input[name="' + $('#sign-in-form input[name="login-way"]:checked').val() + '"]').val());
                $(this).prop('href', '/forget/?' + $('#sign-in-form input[name="login-way"]:checked').val() + '=' + sign);
            });
            
            $form = $('#sign-in-form form');
            $form.submit(function(event)
            {
                event.preventDefault();
                
                var login = $form.find('input[name="login"]').val();
                var email = $form.find('input[name="email"]').val();
                var phone = $form.find('input[name="phone"]').val();
                var card = $form.find('input[name="card"]').val();
                var login_way = $('#sign-in-form input[name="login-way"]:checked').val();
                var password = $form.find('input[name="password"]').val();
                
                $.get('/ajax/auth/login/', { 'login' : login, 'email' : email, 'password' : password, 'phone' : phone, 'card' : card, 'login_way' : login_way }, function(result)
                {
                    if (result.success)
                    {
                        mindbox('identify', {
                            operation: 'Web.Authorization',
                            identificator: {
                                provider: 'DbRiglaID',
                                identity: result.user
                            }
                        });
                        
                        authSuccess();
                    }
                    else if (result.error)
                    {
                        $('#sign-in-form label').removeClass('error');
                        switch (result.error.code)
                        {
                            case 200: $('#sign-in-form input[name="email"]').parents('label').addClass('error'); break;
                            case 201: $('#sign-in-form input[name="phone"]').parents('label').addClass('error'); break;
                            case 202: $('#sign-in-form input[name="card"]').parents('label').addClass('error'); break;
                            case 400: $('#sign-in-form input[name="password"]').parents('label').addClass('error'); break;
                        }
                        
                        $('#sign-in-form .warnings').html(result.error.message);
                    }
                }, 'json');
            });
        });
    </script>
