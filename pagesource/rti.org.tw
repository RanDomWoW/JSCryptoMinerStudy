<script type="text/template" id="news-template">
        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
            <% items.forEach(function(item, i) { %>
                <%
                    var url = item.url;
                    try {
                        var match = item.url.match(/\?recordId=(\d+)/);
                        var recordId = match[1];
                        url = '//news.rti.org.tw/news/view/id/'+recordId;
                    } catch (e) {
                        url = item.url;
                    }
                %>
                <% if(i === 0) { %>
                    <% if (item.enclosureUrl) { %>
                        <div id="collapse-<%= i %>" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading-<%= i %>">
                            <h4>
                                <a href="<%= url %>" target="_blank"><%= ellipsis(item.title, 30) %></a>
                                <small class="pull-right" style="line-height: 24px"><date><i class="fa fa-fw fa-clock-o"></i><%= dateFormat(Date.parse(item.publishedDate.date), 'HH:MM') %></date></small>
                            </h4>
                            <div class="row">
                                <div class="col-md-4">
                                    <a href="<%= url %>" target="_blank">
                                        <img class="media-object thumbnail" width="100%" src="<%= item.enclosureUrl %>" alt="<%= item.title %>" />
                                    </a>
                                </div>
                                <div class="col-md-8">
                                    <p><%= ellipsis(item.content) %></p>
                                    <div class="text-right">
                                        <a href="<%= url %>" target="_blank" class="more">more</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    <% } else { %>
                        <div class="panel-heading" role="tab" id="heading-<%= i %>">
                            <p class="panel-title">
                                <date><i class="fa fa-fw fa-clock-o"></i><%= dateFormat(Date.parse(item.publishedDate.date), 'HH:MM') %></date>
                                <%= ellipsis(item.title, 30) %>
                            </p>
                        </div>
                        <div id="collapse-<%= i %>" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading-<%= i %>">
                            <div class="panel-body">
                                <%= ellipsis(item.content) %>
                                <div class="text-right">
                                    <a href="<%= url %>" target="_blank" class="more">more</a>
                                </div>
                            </div>
                        </div>
                    <% }; %>
                <% } else { %>
                    <div class="">
                        <div class="panel-heading" role="tab" id="heading-<%= i %>">
                            <p class="panel-title">
                                <a href="<%= url %>" target="_blank">
                                    <date><i class="fa fa-fw fa-clock-o"></i><%= dateFormat(Date.parse(item.publishedDate.date), 'HH:MM') %></date>
                                    <%= ellipsis(item.title, 30) %>
                                </a>
                            </p>
                        </div>
                    </div>
                <% }; %>
            <% }); %>
        </div>
    </script>
<script type="text/template" id="player-template">
        <div class="embed-responsive embed-responsive-16by9">
            <iframe class="embed-responsive-item" src="https://www.youtube.com/embed/<%= videoId %>?autoplay=<%= autoplay %>" frameborder="0" allowfullscreen></iframe>
        </div>
    </script>
<script type="text/template" id="playlist-template">
        <% items.forEach(function(item) { %>
            <div class="col-xs-3 col-md-4 col-sm-6 col-xs-6 item" data-video-id="<%= item.snippet.resourceId.videoId %>" data-autoplay="1">
                <img src="<%= item.snippet.thumbnails.medium.url %>" class="img-responsive" />
                <p class="caption text-center"><%= item.snippet.title %></p>
            </div>
        <% }); %>
    </script>
<script type="text/template" id="banner-template">
        <% items.forEach(function(item) { %>
            <% if (item.link) { %>
                <a href="<%= item.link %>" target="<%= item.target ? '_blank' : '_self' %>">
                    <img src="<%= item.desktopImage %>" class="img-responsive thumbnail" />
                </a>
            <% } else { %>
                <img src="<%= item.desktopImage %>" class="img-responsive" />
            <% }; %>
        <% }); %>
    </script>
<script src="http://www.rti.tw/js/app.js?id=d02c83c2cc74519acc9e"></script>
</body>
</html>