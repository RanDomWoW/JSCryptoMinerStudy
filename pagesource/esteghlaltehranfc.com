    <script type="text/javascript">
        $(document).ready(function () {
            $('.carousel').carousel({
                pause: true,
                interval: false,
                wrap: false
            });

            var $this = $('.carousel');
            if ($('.carousel-inner .item:first').hasClass('active')) {
                $this.children('.sliderArrowWrapper .leftArrow').hide();
                $this.children('.sliderArrowWrapper .rightArrow').show();
            } else if ($('.carousel-inner .item:last').hasClass('active')) {
                $this.children('.sliderArrowWrapper .leftArrow').show();
                $this.children('.sliderArrowWrapper .rightArrow').hide();
            } else {
                $this.children('.carousel-control').show();
            }

            forceEqualHeights();
        });

        $('.carousel').on('slid.bs.carousel', function () {
            var carouselData = $(this).data('bs.carousel');
            var currentIndex = carouselData.getItemIndex(carouselData.$element.find('.item.active'));

            $(this).find('.carousel-control').show();

            if (currentIndex == 0) {
                $(this).find('.leftArrow').hide();
            }
            else if (currentIndex + 1 == carouselData.$items.length) {
                
                $(this).find('.rightArrow').hide();
            }
        });

        function forceEqualHeights() {
            var higherHeight = 0;

            $(".sportSlider .carousel .item").each(function () {
                higherHeight = $(this).height() > higherHeight ? $(this).height() : higherHeight;
            });
            $('.sportSlider .carousel .item').height(higherHeight);
        }

    </script>


