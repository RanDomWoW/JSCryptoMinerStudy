		<script type="text/javascript">
    
 var Webex = Webex || {};
    Webex.videoContent = {
        // resize the modal box
        videoModalSize : function($vBox, vRatio , constant_width) {

            var mWidth = document.body.clientWidth-30, mHight;
            mWidth = mWidth > 800 ? 800 : mWidth;
            if(constant_width){
                mWidth = constant_width;
            }
            mHight = mWidth * vRatio;
            $vBox.closest('.modal-dialog').css({
                "width" : mWidth,
                "height" : mHight
            });
            $vBox.height(mHight);
            $(window).resize(function() {
                mWidth = document.body.clientWidth * 0.8;
                mWidth = mWidth > 800 ? 800 : mWidth;
                mHight = mWidth * vRatio;

                $vBox.closest(".modal-dialog").css({
                    "width" : mWidth
                });
                $vBox.height(mHight);
            });
            if (navigator.userAgent.indexOf("MSIE 8.0") <= 0) {
                window.addEventListener("orientationchange", function(obj) {
                    mWidth = $vBox.parent().width();
                    mHight = mWidth * vRatio;
                    $vBox.height(mHight);
                });
            }

        },
        //load video JS
        videoLoadJs : function(videoAccount, videoPlayer) {
            //if (!!($("head").children("#video2"))) {
            var videoScript = document.createElement("script");
            videoScript.id = "video2";
            videoScript.src = "https://players.brightcove.net/" + videoAccount + "/" + videoPlayer + "_default/index.min.js";
            $("head").prepend(videoScript);
            //}
        },
        clearVideoJs : function () {
            $("script[id='video2']").remove();
        },
        // videoId,videoAccount,videoPlayer,
        //vRatio:Video aspect ratio
        //$videoBoxMobal:Mobile version of the video container
        //$videoBoxIpadPc:ipad/PC version of the video container
        //$videoModalIpadPc:ipad/PC version of the modal container

        videoPlayer : function(videoId, videoAccount, videoPlayer, vRatio,$videoBoxMobal, $videoBoxIpadPc, $videoModalIpadPc) {
            var videoHtml = '<video data-video-id="'+ videoId + '" data-account="' + videoAccount + '" data-player="' + videoPlayer + '" data-embed="default" class="video-js" controls width="100%" height="100%"></video>';
            var modal_shown_count = 0;
            if (document.body.clientWidth < 768) {
                if($videoBoxMobal.is(":empty") || !($videoBoxMobal.html().trim()) ){
                    /*when click the play btn, if it is not current video, del lod video and load new video*/
                    var all_box_class = $videoBoxMobal.attr("class");
                    $("."+all_box_class).hide();
                    Webex.videoContent.clearVideoJs();
                    $("."+all_box_class).empty();

                    $videoBoxMobal.css("display", "block").html(videoHtml);
                    Webex.videoContent.videoModalSize($videoBoxMobal,vRatio);
                    Webex.videoContent.videoLoadJs(videoAccount,videoPlayer);
                    $videoBoxMobal.show();
                }else{
                    videojs($videoBoxMobal.children(".video-js").attr("id")).currentTime(0);
                    videojs($videoBoxMobal.children(".video-js").attr("id")).pause();
                }

            } else {
                $videoModalIpadPc.on('shown.bs.modal', function(e) {
                    /*Fix modal execution many times*/
                    if(modal_shown_count < 1){
                        /*if one page have many video ,we need do something*/
                        if (!($videoBoxIpadPc.html().trim())) {
                            /*for webex, if  the container is null*/
                            Webex.videoContent.videoModalSize($videoBoxIpadPc,vRatio);
                            $videoBoxIpadPc.html(videoHtml);
                            Webex.videoContent.videoLoadJs(videoAccount,videoPlayer);
                        }else {
                            var curent_video_id = $videoBoxIpadPc.children(".video-js").attr("data-video-id");
                            if(curent_video_id && curent_video_id != videoId){
                                /*the current video is not same as the last video*/
                                Webex.videoContent.clearVideoJs();
                                Webex.videoContent.videoModalSize($videoBoxIpadPc,vRatio);
                                $videoBoxIpadPc.html(videoHtml);
                                Webex.videoContent.videoLoadJs(videoAccount,videoPlayer);
                            }else{
                                videojs($videoBoxIpadPc.children(".video-js").attr("id")).play();
                            }
                        }
                        modal_shown_count++;
                    }

                });
                $videoModalIpadPc.on('hidden.bs.modal', function(e) {
                    videojs($videoBoxIpadPc.children(".video-js").attr("id")).currentTime(0);
                    videojs($videoBoxIpadPc.children(".video-js").attr("id")).pause();
                });
            }
        }
    };
    
</script>
	
