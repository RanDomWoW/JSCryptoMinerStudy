var typingTimer;                //timer identifier
var doneTypingInterval = 300;  //time in ms

$.xhrPool = []; // array of uncompleted requests

$.xhrPool.abortAll = function() { // our abort function
	$(this).each(function(idx, jqXHR) {
        jqXHR.abort();
    });
    $.xhrPool.length = 0;
};

$.ajaxSetup({
    beforeSend: function(jqXHR) { // before jQuery send the request we will push it to our array
        $.xhrPool.push(jqXHR);
    },
    complete: function(jqXHR) { // when some of the requests completed it will splice from the array
        var index = $.xhrPool.indexOf(jqXHR);
        if (index > -1) {
            $.xhrPool.splice(index, 1);
        }
    }
});

$('#NSwhere, #ASwhere, #BSwhere, #CPwhere').keyup(function(e){

	if(e.which == 40) {

		if($(this).parent().find('ul.geolist').length > 0) {

			document.getElementById("ocultar").value = 1;
			$(this).parent().find('div.result').css("display", "block");
			$('#list_0').css("background-color", "#ccc");
			$('#list_0').focus();

			return;

		}
	}
	
	$( "#NSwhere, #ASwhere, #BSwhere, #CPwhere" ).attr('style',"background: #FFF url('/assets/img/ajax-loader.gif') no-repeat right center !important;");

	clearTimeout(typingTimer);
    if ($(this).val) {

    	var objtext = new Object();
		objtext = $(this);
        typingTimer = setTimeout(function(){
        	doneTyping(objtext);
        }, doneTypingInterval);
    }

});



function doneTyping (objtext) {

