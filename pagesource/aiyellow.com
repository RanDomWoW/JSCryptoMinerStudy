// city list navigation //
$('div.result').on('focus', 'li', function() {
    $this = $(this);
    $this.addClass('active').siblings().removeClass('active');
    $this.closest('div.result').scrollTop($this.index() * $this.outerHeight());
}).on('keydown', 'li', function(e) {
    $this = $(this);
    if (e.keyCode == 40) {        
    	$this.css('background-color', 'white');
        $this.next().focus();       
        $this.css('background-color', '#ccc');
        return false;
    } else if (e.keyCode == 38) { 
    	$this.css('background-color', 'white');       
        $this.prev().focus();
        $this.css('background-color', '#ccc');
        return false;
    } else if (e.keyCode == 13) {
    	setcity($(this));
    }
}).find('li').first().focus();
