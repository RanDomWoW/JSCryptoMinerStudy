    function returnDate(obj) 
    {
        var str = $(obj).val();
        if (str != "") {
            var dC = str.split('.');
            var td = new Date(Date.parse(dC[2] + "-" + dC[1] + "-" + dC[0]));
            if (Object.prototype.toString.call(td) === "[object Date]") {
                if (isNaN(td.getTime())) {
                    $(obj).focus();
                    $('#DynContentBtn').trigger('click');
                    $('#dynText').html('Не верный формат даты');
                    return false;
                } else {
                    return true;
                }
            }
            else {
                $(obj).focus();
                $('#DynContentBtn').trigger('click');
                $('#dynText').html('Не верный формат даты');
                return false;
            }
        }
    }

