    function goSearch(num) 
    {
        var flag = true;
        var searchpar = true;
        var _url = "";
        var query = ($('#query_'+num).val().length > 200 ? $('#query_'+num).val().substring(0, 200) : $('#query_'+num).val());
        var titleonly = "0";
        if ($('#query_6'+num).val() != '') {
            query += "&searchtitle=" + ($('#query_6'+num).val().length > 200 ? $('#query_6'+num).val().substring(0, 200) : $('#query_6'+num).val());
            titleonly = "1";
        }

        if(query != ""){ 
            _url += "&query="+query;
            _url += "&titleonly="+titleonly;
        }
        
        if(query!="" && query.length < 3)
        {
            $('#DynContentBtn').trigger('click');
            $('#dynText').html('<div style="width:100%; text-align:center;">Изланаётган сўз учта символдан ортиқ бўлиши керак!<div>');
            $('#query_'+num).focus();
            return false;
        }
        var form_id = $('#form_'+num).val();// form_id
        if(num != "5")
        {
            if(form_id != "")
            { 
                _url += "&form_id="+form_id;
            }
        }
//        alert(form_id);
//        return false;

        var body_id = $('#organ_'+num).attr('value');// organ принявший
        if(body_id != "" && body_id != null){ 
            _url += "&body_id="+body_id;
        }
        var exact;
        if ($('#exact_'+num).is(':checked') || $('#exact_2'+num).is(':checked')) {
            exact = 1;
        } else {
            exact = 0;
        }

        var regnummj = $('#regnummj_'+num).is(':checked') ? "1" : "0";
       
        var from = $('#df_'+num).val();// date from        
        var to = $('#dt_'+num).val();// date to
        if(from != ""){
            _url += "&from="+from+"&to="+to;
        } 
        var lng = "1";// lang_id
        
        if($('#lang'+num+'0').is(':checked'))
        {
            lng = 0;
        }
        else if($('#lang'+num+'1').is(':checked'))
        {
            lng = 1;
        }
        else if($('#lang'+num+'3').is(':checked'))
        {
            lng = 3;
        }
        else if($('#lang'+num+'4').is(':checked'))
        {
            lng = 4;
        }
        else if($('#lang'+num+'5').is(':checked'))
        {
            lng = 5;
        }
        
        if(lng != ""){
            _url += "&lng="+lng;
        }

        var status = "Y";

        if($('#statusr'+num).is(':checked'))
        {
            status = "N,R";
        }
        else if($('#statusall'+num).is(':checked'))
        {
            status = "";
        }

        _url += "&status="+status;

        if(num!="1"){
            var sort_id = $('#sort_id_'+num).val();
            if(sort_id != "" && sort_id != null){
                _url += "&sort_id="+sort_id;
            }
        }
        var actnum = "";
        actnum = $('#actnum_'+num).val();// act number
        if(actnum != ""){
            _url += "&actnum="+actnum;
        }

        var year="";
        
        if(_url == "" && num != 5)
        {
            $('#DynContentBtn').trigger('click');
            $('#dynText').html('<div style="width:100%; text-align:center;">излаш учун шартни киритиш зарур<div>');
            return false;
        }
        
        if (num == "1") {
            _url += "&a1=0&a2=0&a3=0&a4=0";
        }

        if(num == "3"){
            var choose_country = "";
            $('input[name=choose_country]').each(function () {
                if (this.checked) {
                    choose_country=$(this).val();
                }
            });
            if(choose_country != ""){
                _url += "&choose_country="+choose_country;
            }
            
            //level_id
            var level_id = "";
            $('input[name=level_id]').each(function () {
                if (this.checked) {
                    level_id=$(this).val();
                }
            });
            if(level_id != ""){
                _url += "&level_id="+level_id;
            }
            
            //countries
            var countries = $('#countries').val();
            if(countries != ""){
                _url += "&countries="+countries;
            }
        }        

//        var number_pref = $('#number_pref').val();
//        if(num == "1" && number_pref !="&nbsp;&nbsp;"){
//            _url = _url + "&number_pref="+ number_pref;
//        }        

        if(num == "5")
        {
            //court_form
            var court_form = "";
            $('input[name=court_form]').each(function () {
                if (this.checked) {
                    court_form=$(this).val();
                }
            });
            if(court_form != ""){
                _url += "&court_form="+court_form;
            }
            //instance
            var instance = "";
            $('input[name=instance]').each(function () {
                if (this.checked) {
                    instance=$(this).val();
                }
            });
            if(instance != ""){
                _url += "&instance="+instance;
            }
            //court_type
            var court_type = $('#court_type').val();
            if(court_type != ""){
                _url += "&court_type="+court_type;
            }
            //court_branch
            var court_branch = $('#court_branch').val();
            if(court_branch != ""){
                _url += "&court_branch="+court_branch;
            }
            _url = "/pages/SearchResult.aspx?f=" + num + "&a4=1&sid=0&action=search&exact=" + exact + "&regnummj=" + regnummj + _url;
        }
        else
        {
            _url = "/pages/SearchResult.aspx?f="+num+"&sid=0&action=search&exact="+exact+"&regnummj="+regnummj + _url;
        }

        //alert(_url);
        //return false;
        var rndurl = Math.random();
        //$('#top_tabs').html("<div class='pre-loader' style='text-align:center;padding-top: 100px;'><img src='/images/preloader-uz.gif'></div>");
        myFunctionLoad();
        window.location.href = _url+"&_t="+rndurl;
        return flag;
    }

