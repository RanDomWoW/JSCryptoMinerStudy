                            $('#'+menu_id).css('zIndex', zIndexNum);
                            $("#"+button_id).addClass(button_id+"_hover");

                          }, 
                          function () {
                            $('#'+menu_id).animate({opacity: 1.0}, 200,function() {
                                  if (!sp_b_hov){
                                        $('#'+menu_id).hide();
                                        $("#"+button_id).removeClass(button_id+"_hover");
