    var leftcart = $('div.cart-small').offset().left;
    var topcart = $('div.cart-small').offset().top;

    $('div.floattxt img').animate({"width": "50px", "height": "16px",}, 1000, function () {});

    clone.animate({
        "top": topcart + 15,
        "left": leftcart,
        "width": "50px",
        "height": "16px",
        opacity: 0.4,
    }, 1000, function () {
        clone.remove();
    });

}
