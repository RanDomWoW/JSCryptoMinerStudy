html('');}}function trocarEstado(comboEstado,cinemaValue){var estado=$(comboEstado).val();var url="/includes/libs/_cinemas_combo.php?estado="+estado;if(estado!=""){$.get(url,function(data,status,aa){var cinemas=JSON.parse(data);if(cinemas.length==0||cinemas.length==undefined){$("#comboCinema").html('');$("#divCinema").show("slow");try{var combo="";if($(window).width()<600)combo+='<option value="'+cinemas.id+'">'+limitaString(25,cinemas.nome)+'</option>';else if($(window).width()>601)combo+='<option value="'+cinemas.id+'">'+limitaString(25,cinemas.nome)+'</option>';$("#comboCinema").append(combo);}catch(e){console.log("erro: "+e.message);}$("#comboCinema").val(cinemas.id);atualizaSessoes(cinemas.id);}else{$("#comboCinema").html('');$("#divCinema").show("slow");var combo="<option value=''>SELECIONE</option>";var temMaisProximo=false;var cinemaEstadoCookie=getCinemaCookie();for(var i=0;i<cinemas.length;i++){if(cinemaEstadoCookie!=null&&cinemas[i].id==cinemaEstadoCookie.cinema){temMaisProximo=true;}if($(window).width()<600)combo+='<option value="'+cinemas[i].id+'">'+limitaString(25,cinemas[i].nome)+'</option>';else if($(window).width()>601)combo+='<option value="'+cinemas[i].id+'">'+limitaString(25,cinemas[i].nome)+'</option>';if(cinemas.length==1){setCookieCinema(cinema[i].id,estado);}}$("#comboCinema").append(combo);if(temMaisProximo){$("#comboCinema").val(cinemaEstadoCookie.cinema);atualizaSessoes(cinemaEstadoCookie.cinema);}else{if(existeCinemaProximo()){$("#comboCinema").val(CINEMA_PROXIMO.id);atualizaSessoes(CINEMA_PROXIMO.id);}else{calculaCinemaProximo(function(cinema){$("#comboCinema").val(cinema.id);atualizaSessoes(cinema.id);});}}}});}else{$("#divCinema").hide("slow");$("#comboCinema").html('');}}function atualizaSessoes(cinema_id){destroiCarouselSessoes("bg-filme .owl-carousel");var data=$("#caledario").datepicker("getDate");var datarev=getDatarev(data);var cinema=$("#comboCinema").val();var estado=$("#comboEstado").val();if(cinema!=undefined&&estado!=undefin