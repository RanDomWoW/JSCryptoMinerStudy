toastr.info("CLIQUE AQUI e faça login para receber avisos sobre lançamento de filmes");document.getElementById(amigavelFilme+idFilme).onclick=function(){aviseMe(idFilme,amigavelFilme)};return;}var url='/includes/_avise_me.php?idFilme='+idFilme;$.get(url,function(data,status,aa){var dados=JSON.parse(data);if(dados.status==200){toastr.success('Você será avisando sobre a estreia do filme: '+$("#titulo-"+idFilme).html());$("#"+amigavelFilme+idFilme).prop("onclick","");$("#"+amigavelFilme+idFilme).removeClass("btn-avise-me").addClass("btn-avisado");$("#"+amigavelFilme+idFilme).html('<i class="fa fa-check"></i>AVISE-ME')}else{if(dados.message!=undefined)toastr.warning(dados.message);else
toastr.warning(dados.descricao);document.getElementById(amigavelFilme+idFilme).onclick=function(){aviseMe(idFilme,amigavelFilme)};}setTimeout(function(){document.getElementById("label-"+idFilme).innerHTML="AVISE-ME";$("#loader-"+idFilme).hide();},600);});}function destroiCarouselSessoes(idCarousel){$("#"+idCarousel).trigger('destroy.owl.carousel');}function loadImageBackground(imgName){$("#bg-filme").css({'background-image':'url(../filmes/images/poster/background/'+imgName+'.jpg)'});}function truncateDate(date){return new Date(date.getFullYear(),date.getMonth(),date.getDate());}$("#progCompletaSessoes").on('click',function(){var cinema=$("#comboCinema").val();var cinemaEstadoCookie=getCinemaCookie();if(cinemaEstadoCookie!=null&&cinema==null||cinema==undefined)cinema=cinemaEstadoCookie.cinema;location.href="/cinemas/completo/"+cinema;});$("#progCompletaSessoesMobile").on('click',function(){var cinema=$("#comboCinema").val();if(cinema==null||cinema==undefined)cinema=cinemaEstadoCookie.cinema;location.href="/cinemas/completo/"+cinema;});if($(window).width()<600)limitaCaracteresComboCinema(10,"comboEstado");else if($(window).width()>601)limitaCaracteresComboCinema(20,"comboEstado");</script>
