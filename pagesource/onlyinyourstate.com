		<script type="text/javascript" >
		jQuery(document).ready(function() {
			jQuery('form.getresponse-signup-form').submit(function(event) {
				var objForm = jQuery(this);
				var objContainer = objForm.parents('.getresponse-signup-container');
				var objLoading = objContainer.find('.getresponse-signup-ajaxloading');
				var objError = objContainer.find('.getresponse-signup-error'); 
				var objThankYou = objContainer.find('.getresponse-signup-thankyou');				
				// Hide all but the loading animation
				objLoading.show();
				objForm.hide();
				objThankYou.hide();
				// Send the AJAX request
				jQuery.ajax({
					type        : 'POST',
					url         : 'http://www.onlyinyourstate.com/wp-admin/admin-ajax.php?action=oiys_newsletter_signup',
					data        : objForm.serialize(),
					dataType    : 'json',
					encode		: true
				}).done(function(data) {
					objLoading.hide();	
					if(data['success']=='true'){
						objThankYou.show();
						// BEGIN Hide any pop ups
						$('.popup-close-permanent').hide();
						var objSignup = objThankYou.parents('.getresponse-signup');
						var intCampaignId = objSignup.data('campaignid');
						strCookieDismiss = fnCookieGet('OiysNewsletterPopupDismiss');
						aryCookieDismiss = JSON.parse(strCookieDismiss);
						if(Array.isArray(aryCookieDismiss)==false){aryCookieDismiss = [];}
						aryCookieDismiss.push(intCampaignId);
						fnCookieSet('OiysNewsletterPopupDismiss', JSON.stringify(aryCookieDismiss), 30);
						// END Hide any pop ups
					}else{
						objError.html('Error: '+data['error']);
						objError.show();
						objForm.show();
					}
				});
				event.preventDefault(); // stop the form from submitting the normal way and refreshing the page
			});
		});
		</script>
