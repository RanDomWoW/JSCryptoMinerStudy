    function calculateRoute(from, to) {
    var mapObject = map2;
    var directionsService = new google.maps.DirectionsService();
    var directionsRequest = {
    origin: from,
    destination: to,
    travelMode: google.maps.DirectionsTravelMode.WALKING,
    unitSystem: google.maps.UnitSystem.METRIC
    };
    directionsService.route(
    directionsRequest,
    function(response, status) {
    if (status == google.maps.DirectionsStatus.OK) {

        directionsDisplay.setMap(null);
        directionsDisplay.setDirections({
        routes: []
        });
        directionsDisplay.setMap(mapObject);
        directionsDisplay.setDirections(response);

    } else {
    }
    }
    );
    }


    function findNearestLocation(myLocation, locations) {
    var distance = [];
    var closest = -1,d;
    for (var i = 0; i < locations.length; i++) {
    d = google.maps.geometry.spherical.computeDistanceBetween(myLocation.position, new google.maps.LatLng(locations[i][1], locations[i][2]));
    distances[i] = [];
    distances[i][0] = i ;
    distances[i][1] = d;
    distance[i] = d;
    if (closest == -1 || d < distance[closest]) {
    closest = i;
    }
    }
    return closest;
    }

