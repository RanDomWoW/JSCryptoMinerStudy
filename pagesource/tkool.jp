<script>
	// ajaxPager
	var w = window.innerWidth ? window.innerWidth: $(window).width()
	var x = 640;
	var maxpage = $('.librarys__more__num').text();
	if(maxpage < 12){
		$('#next').hide();
		$('.librarys__wrap').append('<a href="" class="librarys__box librarys__box--last"><img src="/wp-content/themes/tkool/img/sp/bg_librarys.png" class="librarys__box__bg ul--sp"><h3></h3><span><img src="/wp-content/themes/tkool/img/common/img_comingsoon.png"></span><p></p></a>')
	} else if(maxpage == 0) {
		$('#next').hide();
		$('.librarys__wrap').append('<a href="" class="librarys__box librarys__box--last"><img src="/wp-content/themes/tkool/img/sp/bg_librarys.png" class="librarys__box__bg ul--sp"><h3></h3><span><img src="/wp-content/themes/tkool/img/common/img_comingsoon.png"></span><p></p></a>')
	}
	$.autopager({
		content: '.librarys__box',
		link: '#next a',
		autoLoad: false,
		start: function(current, next){
			$('#loading').css('display', 'block');
		},
		load: function(current, next){
			if (w < x) {
				var librarysImg = $('.librarys__box .librarys__box__bg').height();
				$('.librarys__box').css('height', librarysImg+'px');
			}
			$('#loading').css('display', 'none');
			$('#next a').css('display', 'block');
			if(current.page >= maxpage){
				$('#next').hide();
				$('.librarys__wrap').append('<a href="" class="librarys__box librarys__box--last"><img src="/wp-content/themes/tkool/img/sp/bg_librarys.png" class="librarys__box__bg ul--sp"><h3></h3><span><img src="/wp-content/themes/tkool/img/common/img_comingsoon.png"></span><p></p></a>')
			}
		}
	});
	$('#next a').click(function(){
		$.autopager('load');
		return false;
	});
</script>

