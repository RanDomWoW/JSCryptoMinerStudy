<script type="text/javascript">
// easy function for building the results query description
var searchQueryDescription = function(query) {
  return "Showing results for: " + query;
}

// Wait till the document is all loaded :)
document.addEventListener('DOMContentLoaded', function() {
  var searchQuerySpan 	= document.querySelector('.wpsl-search-query');
  var searchButton 		= document.querySelector('#wpsl-search-btn');
  var searchTextField 	= document.querySelector('#wpsl-search-input');
  var storesList		= document.querySelector('#wpsl-stores ul');
  
  // if these fields don't exist on the page, forget it!
  if (searchQuerySpan == null 
      || searchButton == null
      || searchTextField == null) { return; }
  
  // Create observer since there are no hooks for WPSL we have
  // to listen for when the html is actually updated
  var observer = new MutationObserver(function() {
        // Get search field value
    let query = searchTextField.value;
    
    // the items loaded into the store list
    var storeItems = document.querySelectorAll('#wpsl-stores ul li');
    
    // if the query is empty erase the results query description
    if (query == "" || 
        // if store items equals zero
        storeItems.length == 0 || 
        // if the store items don't exist
        storeItems == null || 
        // if the list is filled with the "currently searching"
        storeItems[0].classList.contains('wpsl-preloader')) {
      
      searchQuerySpan.innerHTML = "";
      return;
    }
    
    // fill the results query description with the searchQueryDescription
    searchQuerySpan.innerHTML = searchQueryDescription(query);
  });
  
  observer.observe(storesList, { childList: true })
  
});

</script>
