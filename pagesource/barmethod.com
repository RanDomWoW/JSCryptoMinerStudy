<script type="text/javascript">
document.addEventListener('DOMContentLoaded', function() {
  setTimeout(setupStateSelectionListener, 1000);
});

var changeEvent = new Event('change');

function setupStateSelectionListener() {
  // Get our form field elements
  var stateSelectionField 		= document.querySelector('.state-selection-field');
  var studioSelectionField 		= document.querySelector('.studio-selection-field');
  var studioSelectionContainer 	= document.querySelector('.studio-selection-container');
  var addressField 				= document.querySelector('[data-key="studio_address"]');
  var emailField 				= document.querySelector('[data-key="studio_contact_email_address"]');
  var studioIdField		 		= document.querySelector('[data-key="studio_id"]');
  var studioNameField			= document.querySelector('[data-key="studio_name"]');
  var studioPhoneField			= document.querySelector('[data-key="studio_phone"]');
  var productIDField			= document.querySelector('[data-key="product_id"]');
  var offerPriceField 			= document.querySelector('[data-key="promotion_pricepoint_q2"]');
  var mboStudioIdField 			= document.querySelector('[data-key="mbo_studio_id"]');
  //var newClientUrlField 		= document.querySelector('[data-key="new_client_special_url"]'); ** NOT USED IN Q2 **
  //var offerField 				= document.querySelector('[data-key="studio_offer"]'); **NOT USED IN Q2**

  //var promotionDetailContainer= document.querySelector('.promotion-detail .nf-field-element'); **NOT USED IN Q2**
  
  // If the field elements don't exist on this page
  // fuhgetaboutit
  if (stateSelectionField == null || studioSelectionField == null) { return }
  
  // Disable the Studio selection one
  studioSelectionField.disabled = true;
  
  // Save these Studio selection options for later
  var studioOptions = studioSelectionField.querySelectorAll('option');
  
  stateSelectionField.options[0].disabled = true;
  studioSelectionField.options[0].disabled = true;
  
  // Listen for changes to State selector
  stateSelectionField.addEventListener('change', function(e) {
    // Get current state value selected
    var stateValue = e.target.value;
    
    var disabledOption = studioOptions[0];
    disabledOption.selected = true;
    
    // If it's empty or null disable studio selection, otherwise, enabled it
    if (stateValue == "" || stateValue == null || e.target.selectedOptions[0].disabled == true) {
      studioSelectionField.innerHTML = "";
      studioSelectionField.append(disabledOption);
      //promotionDetailContainer.innerHTML = ""; ** Not Needed Q2 **
      studioSelectionField.disabled = true;
      studioSelectionContainer.style.opacity = 0.5;
      return;
    } else {
      studioSelectionField.innerHTML = "";
      studioSelectionField.append(disabledOption);
      studioSelectionField.disabled = false;
      studioSelectionContainer.style.opacity = 1;
    }

    // Fill that field up with matching studios!
    studioOptions.forEach( function(option) {
      var state = option.dataset.state;
      if (state == stateValue) {
	    option.selected = false;
        studioSelectionField.append(option);
      }
    });
    
    // Trigger studio selection field change event to update promo selection
    // studioSelectionField.dispatchEvent(changeEvent);
    
    jQuery(studioSelectionField).change()
    
  });
  
  studioSelectionField.addEventListener('change', function() {
	// Get the currently selected studio    
    var selectedStudio = studioSelectionField.selectedOptions[0];
    
    // var studioPromotionDescription = selectedStudio.dataset.promotionDescription; ** Not used in Q2 **
    
    // Check if a studio is selected ** NOT NEEDED Q2 2018 **
    //if (selectedStudio.disabled == true) { 
    //  promotionDetailContainer.innerHTML = ""
    //  return;
    //}
    
    // Update offer description
    // promotionDetailContainer.innerHTML = "<p>You'll receive " + studioPromotionDescription + "</p>";
    
    // Update hidden fields
    offerPriceField.value 		= selectedStudio.dataset.promotionPricepoint;
	addressField.value 			= selectedStudio.dataset.address;
    emailField.value 			= selectedStudio.dataset.email; 
    studioIdField.value 		= selectedStudio.dataset.studioId;
    studioNameField.value		= selectedStudio.dataset.studioName;
    studioPhoneField.value		= formatUSPhone(selectedStudio.dataset.studioPhone);
    productIDField.value		= selectedStudio.dataset.mboProductId;
    mboStudioIdField.value		= selectedStudio.dataset.mboStudioId;
    //offerField.value 			= studioPromotionDescription; ** NOT USING Q2 **
    //newClientUrlField.value 	= selectedStudio.dataset.newClientUrl; ** NOT USING Q2 **
    
    // Trigger change on the fields so Ninja Forms knows
    // have to do this with jQuery (YUUUUCKKK) because Ninja forms
    // Backbone implementation is not listening to pure JS for
    // whatever reason. Ticket in: https://github.com/wpninjas/ninja-forms/pull/2435#issuecomment-354079176
    //jQuery(offerField).change(); ** NOT USING Q2 **
	jQuery(offerPriceField).change();
    jQuery(addressField).change();
    jQuery(emailField).change();
    jQuery(studioIdField).change();
	jQuery(studioSelectionField).change();
    jQuery(studioNameField).change();
    jQuery(studioPhoneField).change();
    jQuery(productIDField).change();
    jQuery(mboStudioIdField).change();
    //jQuery(newClientUrlField).change(); ** NOT USING Q2 **
  });
  
  // Trigger state selection field
  stateSelectionField.dispatchEvent(changeEvent);
  
}

// Format Phone Number
function formatUSPhone(phone) {

    var phoneTest = new RegExp(/^((\+1)|1)? ?\(?(\d{3})\)?[ .-]?(\d{3})[ .-]?(\d{4})( ?(ext\.? ?|x)(\d*))?$/);

    phone = phone.trim();
    var results = phoneTest.exec(phone);
    if (results !== null && results.length > 8) {

        return "(" + results[3] + ") " + results[4] + "-" + results[5] + (typeof results[8] !== "undefined" ? " x" + results[8] : "");

    }
    else {
         return phone;
    }
}

