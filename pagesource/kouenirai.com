<script>
$(document).ready(function() {
	$('section.teachers .tab a').click(function(event) {
		var gcode = $(this).data('gcode');
		if (gcode == 1) {return};
		var $ul = $('.tabContents #tab0'+gcode).find('ul');
		$.ajax({
			url: '/ajax/lecturer_basic',
			dataType: 'json',
			data: {
				type: 'getTopGenre',
				gcode: gcode
			},
		})
		.done(function(res) {
			var li = '';
			$.each(res, function(index, item) {
				li +=
				'<li>'
					+'<div class="clearfix">'
						+'<p class="teacherImage piecss3"><a href="/profile/'+item.lcode+'"><img src="'+item.lecturer_file_name+'" alt="'+item.lecturer_name+'"></a></p>'
						+'<div class="teacherInfo">'
							+'<p class="name fixHeightChildBox"><a href="/profile/'+item.lcode+'">'+item.lecturer_name+'</a></p>'
							+'<p class="performance fixHeightChildTitle">'+item.top_genre_recommend_lecturer_incumbent+'</p>'
							+'<p class="office fixHeightChildText">'+item.top_genre_recommend_lecturer_lecturetheme_theme+'</p>'
						+'</div>'
					+'</div>'
				+'</li>';
			});
			$ul.find('li:not(:last)').remove();
			$ul.prepend(li);
			$(".fixHeight").unbind().removeData();
			if ($.fn.fixHeight) $(".fixHeight").fixHeight();
		})
		.fail(function() {
			$ul.find('li:not(:last)').remove();
		});

	});
});
</script>


