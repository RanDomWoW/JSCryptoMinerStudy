      $win = $(window),
      $birthday_popup_bg = $('#birthday_popup_bg'),
      $birthday_popup_con = $('#birthday_popup_con');
      function win_scroll(){
        var 
        win_t = $win.scrollTop(),
        win_l = $win.scrollLeft();
        //$birthday_popup_bg.css({top : win_t, left : win_l});
        $birthday_popup_bg.css({top : 0, left : 0});
        $birthday_popup_con.css({'top' : _top + win_t, 'left' : _left + win_l});
      }
      function win_resize(){
        var 
        win_w = $win.width(),
        win_h = $win.height();
        _top = (win_h - h)/2;
        _left = (win_w- w)/2;
        if(_top < 0)_top = 0;
        if(_left < 0)_left = 0;
        $birthday_popup_bg.css({'width' : win_w, 'height' : win_h});
        win_scroll();
      }
      $birthday_popup_bg.css({'opacity' : 0.6, 'background-color' : '#000'});
      win_resize();
      $win.scroll(win_scroll);
      $win.resize(win_resize);
