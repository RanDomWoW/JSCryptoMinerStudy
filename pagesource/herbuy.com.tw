<script>
(function($){
	//伸縮廣告
	var $top_banner = $('#top_banner'),
	$s_banner = $top_banner.find('.s_banner'),
	$b_banner = $top_banner.find('.b_banner'),
	$switch = $top_banner.find('.switch'),
	s_height = 40,
	b_height = 300,
	cookie_name = 'top_banner',
	close_timer = 8000;
	if($b_banner.length > 0){
		var banner_open = function(){
			$switch.removeClass('open').hide();
			$b_banner.stop(true).slideDown(function(){
			  $switch.addClass('close').show();
			  $s_banner.hide();
			  $top_banner.height(b_height );
			  $.cookie(cookie_name, 1, {path: '/', expires: 1});
			});
		};
		var banner_close = function(){
			$switch.removeClass('close').hide();
			$s_banner.show();
			$b_banner.stop(true).slideUp(function(){
			  $switch.addClass('open').show();
			  $top_banner.height(s_height);
			});
		};
		$switch.click(function(){
			if($switch.hasClass('open')){
				banner_open();
			}else{
				banner_close();
			}
		});
		var top_banner_cookie = ($.cookie(cookie_name) == null) ? 0 : $.cookie(cookie_name);
		if(top_banner_cookie == 0){
			banner_open();
			setTimeout(function(){banner_close()},close_timer);
		}
	}
})(jQuery);
</script>
