        $(".outvideo").stop().fadeIn(600).find('.video').html(videohtml)
    })
    $(".outvideo .close").click(function(){
        $(".outvideo").stop().fadeOut(600).find(".video").empty()
    })*/

    $(window).scroll(function(){
    	if($(window).scrollTop() > $(".imain").offset().top - $(window).height() / 2){
    		$(".imain .left").addClass('show')
    		Enter($(".imain .box").eq(0), "top", 0, 2, 600, 400)
    	}
    })
})
</script>
