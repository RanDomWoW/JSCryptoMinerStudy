<script src="http://yuyue.com.cn/static/web/js/main.js?v=v3" type="text/javascript" charset="utf-8"></script><script>
$(function(){

	var $Ban   = $(".ibanner"),
		BanImg = []
	for (var i = 0; i < $Ban.find("li").length; i++) {
		BanImg[i] = $Ban.find("li").eq(i).find("img").attr("src")
		$Ban.find(".btns").append('<span class="span'+i+' fl"></span>');
	};
	BanImg[$Ban.find("li").length] = "http://yuyue.com.cn/static/web/img/img02.jpg?v=v3";
	BanImg[$Ban.find("li").length + 1] = "http://yuyue.com.cn/static/web/img/img02.jpg?v=v3";

	$Ban.find(".btns").css("margin-left",-$(".btns").width()/2).find("span").eq(0).addClass("cur");

	_PreLoadImg(BanImg, function(){
		$(".imain .left").height($(".imain .right").height()).find("li").eq(0).css("opacity", 1)
		$(".imain .left .btns").css("margin-left", - $(".imain .left .btns").width() / 2 ).find("span").eq(0).addClass('cur')

		$Ban.find(".loader").stop().fadeOut(600)
		$Ban.find("li").eq(0).addClass('cur').css({
			left: "50%",
			marginLeft: -960
		}).stop().animate({
			opacity: 1
		}, 600).find(".image img").addClass('show')
		Enter($Ban.find("li").eq(0).find(".tips").children().eq(0), "top", 0, 4, 600, 400)

		var banTime = 0,
			imgTime = 0,
			banAuto = setInterval(function(){
				banTime ++
				if(banTime == $Ban.find("li").length){
					banTime = 0
				}
				Banner(banTime)
			}, 5000),
			imgAuto = setInterval(function(){
				imgTime ++
				if(imgTime == $(".imain .left li").length){
					imgTime = 0
				}
				ImageLeft(imgTime)
			}, 3500)
		$Ban.find(".btns span").click(function(){
			if(!$(this).hasClass('cur')){
				banTime = $(this).index()
				Banner(banTime)
			}
		})
		$(".imain .left .btns span").click(function(){
			if(!$(this).hasClass('cur')){
				imgTime = $(this).index()
				ImageLeft(imgTime)
			}
		})
		$Ban.hover(function(){
			if(banAuto){
				clearInterval(banAuto);
			}
		}, function(){
			clearInterval(banAuto);
			banAuto = setInterval(function(){
				banTime ++
				if(banTime == $Ban.find("li").length){
					banTime = 0
				}
				Banner(banTime)
			}, 5000)
		})
		$(".imain .left").hover(function(){
			if(imgAuto){
				clearInterval(imgAuto);
			}
		}, function(){
			clearInterval(imgAuto);
			imgAuto = setInterval(function(){
				imgTime ++
				if(imgTime == $(".imain .left li").length){
					imgTime = 0
				}
				ImageLeft(imgTime)
			}, 3500)
		})
	})

	
	function Banner(n) {
		$Ban.find(".btns span").eq(n).addClass('cur').siblings().removeClass('cur')
		var $licur = $Ban.find("li.cur")
		$licur.stop().animate({
			left: -1920,
			marginLeft: 0
		}, 1000, function(){
			$licur.removeClass('cur').attr("style", "")
		})
		$Ban.find("li").find(".image img").removeClass('show')
		$Ban.find("li").find(".tips").children().attr("style", "")
		var $li = $Ban.find("li").eq(n)
		$li.stop().animate({
			left: "50%",
			marginLeft: -960,
			opacity: 1
		}, 1000, function(){
			$li.addClass('cur').find(".image img").addClass('show')
			Enter($li.find(".tips").children().eq(0), "top", 0, 4, 600, 400)
		})
	}

	function ImageLeft(n) {
		$(".imain .left li").eq(n).stop().animate({opacity: 1}, 800).siblings().stop().animate({opacity: 0}, 800)
		$(".imain .left .btns span").eq(n).addClass('cur').siblings().removeClass('cur')
	}

