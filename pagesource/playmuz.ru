    <script type="text/javascript" src="/static/js/bxslider/jquery.bxslider.min.js"></script>
    <script type="text/javascript">
        var slides_preview = ["/media/thumbnails/homepage/slides/95/195/c/Untitled-1_6hIJa08.jpg", "/media/thumbnails/homepage/slides/95/195/c/Gareth-Emery-e1374456571406_5ATshR3.jpg", "/media/thumbnails/homepage/slides/95/195/c/cefe724affbb7e0a6420ee13a66f32d8.jpg", "/media/thumbnails/homepage/slides/95/195/c/1490107059.jpg", "/media/thumbnails/homepage/slides/95/195/c/Taylor-Swift-GettyImages-617547962-920x584.jpg", "/media/thumbnails/homepage/slides/95/195/c/delfin-vypustit-albom-andrej-9-dekabrya-1.jpg", "/media/thumbnails/homepage/slides/95/195/c/Untitled-1_WnxU1rt.jpg"];

        function get_slider_options(slider) {
            return {
                'controls': true,
                'nextSelector': '#slide_' + slider + '_right',
                'prevSelector': '#slide_' + slider + '_left',
                'pager': false,
                'prevText': '',
                'nextText': ''
            }
        }

        $(document).ready(function () {

            $('#bxslider1').bxSlider(get_slider_options(1));
            $('#bxslider2').bxSlider(get_slider_options(2));
            $('#bxslider3').bxSlider(get_slider_options(3));
            $('#bxslider4').bxSlider(get_slider_options(4));
            $('#bxslider5').bxSlider(get_slider_options(5));
            $('#bxslider6').bxSlider(get_slider_options(6));

            $("#bxslider_main").bxSlider({
    'controls': true,
    'nextSelector': '#main_slide_right',
    'prevSelector': '#main_slide_left',
    'pager': false,
    'prevText': '',
    'nextText': '',
    'auto': true,
    'pause': 5000,
    'onSliderLoad': function () {
        $("#main_slide_left").css({"backgroundImage": "url(" + slides_preview[slides_preview.length - 1] + ")"});
        $("#main_slide_right").css({"backgroundImage": "url(" + slides_preview[1] + ")"});
    },
    'onSlideNext': function(slideElement, oldIndex, newIndex) {

        var nextIndex = newIndex + 1;
        if (nextIndex >= slides_preview.length ) {
            nextIndex = 0;
        }
        //$("#main_slide_left .bx-prev").css({"backgroundImage": "url(" + slides_preview[oldIndex] + ")"});
        //$("#main_slide_right .bx-next").css({"backgroundImage": "url(" + slides_preview[nextIndex] + ")"});

        $("#under_msl").css({"backgroundImage": "url(" + slides_preview[oldIndex] + ")"});
        $("#under_msr").css({"backgroundImage": "url(" + slides_preview[nextIndex] + ")"});

        $("#main_slide_left").animate({opacity: 0}, 300, function() {
            $(this)
                .css({'backgroundImage': "url(" + slides_preview[oldIndex] + ")"})
                .animate({opacity: 1});
        });

        $("#main_slide_right").animate({opacity: 0}, 300, function() {
            $(this)
                .css({'backgroundImage': "url(" + slides_preview[nextIndex] + ")"})
                .animate({opacity: 1});
        });


    },
    'onSlidePrev': function(slideElement, oldIndex, newIndex) {

        var prevIndex = newIndex - 1;
        if (prevIndex < 0) {
            prevIndex = slides_preview.length - 1;
        }

        //$("#main_slide_left .bx-prev").css({"backgroundImage": "url(" + slides_preview[prevIndex] + ")"});
        //$("#main_slide_right .bx-next").css({"backgroundImage": "url(" + slides_preview[oldIndex] + ")"});

        $("#under_msl").css({"backgroundImage": "url(" + slides_preview[prevIndex] + ")"});
        $("#under_msr").css({"backgroundImage": "url(" + slides_preview[oldIndex] + ")"});

        $("#main_slide_left").animate({opacity: 0}, 300, function() {
            $(this)
                .css({'backgroundImage': "url(" + slides_preview[prevIndex] + ")"})
                .animate({opacity: 1});
        });

        $("#main_slide_right").animate({opacity: 0}, 300, function() {
            $(this)
                .css({'backgroundImage': "url(" + slides_preview[oldIndex] + ")"})
                .animate({opacity: 1});
        });
    }
}) // bxslider_main

        })
    </script>




