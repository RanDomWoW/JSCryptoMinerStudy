<script>
        var loadedDependencies = {
            stylesLoaded: false,
            pluginsLoaded: false,
            commonjsLoaded: false
        };

        function onDependencyLoaded() {
            var flag = true;
            for (var key in loadedDependencies) {
                flag = flag && loadedDependencies[key];
            }

            if(flag) {
                deferredFunctions();
                blogDeferredFunctions && blogDeferredFunctions();
            }
        }

        (function initialiseDeferredScripts() {
            for (var key in loadedDependencies) {
                if(key === 'stylesLoaded') continue;
                (function(key) {
                    if(document.getElementById(key)) {
                        document.getElementById(key).onload = function () {
                            loadedDependencies[key] = true;
                            onDependencyLoaded();
                        };
                    }
                })(key);
            }
        })();

        var loadDeferredStyles = function() {
            var addStylesNode = document.getElementById("deferred-styles");
            var replacement = document.createElement("div");
            replacement.innerHTML = addStylesNode.textContent;
            document.body.appendChild(replacement);
            addStylesNode.parentElement.removeChild(addStylesNode);
            document.getElementById('stylesLoaded').onload = function() {
                loadedDependencies['stylesLoaded'] = true;
                onDependencyLoaded();
            };
        };

        var raf = requestAnimationFrame || mozRequestAnimationFrame || webkitRequestAnimationFrame || msRequestAnimationFrame;

        if (raf) {
            raf(function() { window.setTimeout(loadDeferredStyles, 0); });
        }else{
            window.addEventListener('load', loadDeferredStyles);
        }

        /*
         * non-critical content is hidden until styles load,
         * this will show them after 3 seconds in the event
         * that the stylesheet fails to load
         */
        setTimeout(function(){
            var css = document.createElement('style');
            css.type = 'text/css';
            var styles = '.not-critical > *  {opacity:1;transition-duration:0.5s;}.not-critical {background:transparent;min-height:auto;}';
            css.appendChild(document.createTextNode(styles));
            document.getElementsByTagName('head')[0].appendChild(css);
        },3000);
    </script>
