<script>
    (function () {

        
        /*
           首次请求市场报价

           _Enum  所有的枚举

           _HTML 所有的渲染

           Init  启动项
        Marker.Init(类型的id,条数);
        */
        //不同类别请求列表
        var Marker = {
            _Variable: {
                Varieties:[] //品种
            },
            _Enum: {
                requestQueue: {//所有种类的id集合
                    'forex': 1,
                    'goods': 2,
                    'indices': 3,
                    //'bonds': 4,
                    'bitcoin': 9,
                    //'stockIndex': 10,
                    'metals': 11
                },
                requestArr:[1,2,3,9,11],
                ColorClass: {
                    red: { backgroundColor: 'red', img: '/Images/1.0.0/Icon/MarketRed.png' },
                    green: { backgroundColor: 'green', img: '/Images/1.0.0/Icon/MarketGreen.png' }
                }
            },
            _DOM: {
                Market: function () { return $('.Market-content'); },
                tbody: function () { return this.Market().find('tbody') },
                tr: function () { return this.tbody().find('tr') },
                td: function () { return this.tr().find('td') }
            },
            _HTML : {
                ajax: function () {
                    var productId = Marker._Enum.requestArr.shift();
                    var _that = this;
                        $.ajax({
                            type: 'post',
                            data: { product: productId, pageindex: 1, pagesize: 6 },
                            url: '/HomeView/QueryRecommendByProductShowIndex',
                            success: function (data) {
                                if (data.code == 0) {
                                    initData = data.bodyMessage;
                                    var dataMain = eval('(' + data.bodyMessage + ')');
                                    _that._Content(dataMain,productId);
                                }
                            }
                        }).always(function () {                            
                            if (Marker._Enum.requestArr.length > 0) {
                                Marker._HTML.ajax();
                            } else {
                                _that.addIframe();
                            }
                          //  getPriceList();
                        })
                },
                _Content: function (data,id) {
                    var _that = this, html = "";
                    for (var i = 0; i < data.length; i++) {
                        Marker._Variable.Varieties.push(data[i].Code);
                        //html += _that.HTML(data[i]);
                    }
                    //$('.Market-content[data-id="' + id + '"] tbody').html(html);
                    //$('.MarketIf').closeMask();
                    //_that.addIframe();
                },
                HTML: function (data) {
                    var _this = this, color, img;
                    var calculateValue =  calculate(data);
                    if (calculateValue.priceLimit.indexOf('-') >= 0) {
                        color = Marker._Enum.ColorClass.red.backgroundColor;
                        img = Marker._Enum.ColorClass.red.img;
                    } else if (calculateValue.priceLimit.indexOf('+') >= 0) {
                        color = Marker._Enum.ColorClass.green.backgroundColor;
                        img = Marker._Enum.ColorClass.green.img;
                    } else {
                        color = "";
                        img = "";
                    }
                    var html = '<tr class="' + color + '" data-id="' + data.Code + '"> <td class="title">' + data.Cn_Name + '</td> <td class="td2">' + data.CurrentPrice + '</td><td class="td3">' + (img && ('<em><img src="' + img + '"></em>')) + calculateValue.priceLimit + '</td> <td class="td4">' + calculateValue.priceExchange + '</td></tr>';
                    return html;
                },
                addIframe: function () {
                    var iframe = document.createElement('iframe');
                    iframe.setAttribute('block', 'none');
                    iframe.setAttribute('height', '0');
                    iframe.setAttribute('width', '0');
                    iframe.name = "Market";
                    iframe.id = "Market";
                    iframe.src = "/Static/Forex/Market.html?Varieties=" + encodeURIComponent(Marker._Variable.Varieties.join());
                    document.body.appendChild(iframe);
                }
            },
            Init: function () {
                this._HTML.ajax();
            }

        }

        function calculate(data) { 
        //统一涨跌幅涨跌额
        function calculateUpDown(data) {
            var priceLimit, priceExchange;
            if (data.PriceLimit != "") {
                priceLimit = data.PriceLimit ? data.PriceLimit : '0.000';
                priceExchange = data.PriceExchange ? data.PriceExchange : '0.00%';
            } else {
                priceLimit = subtr(data.CurrentPrice, data.OpenPrice);
                priceExchange = (data.OpenPrice <= 0 ? toFixeds(parseFloat(data.OpenPrice), 2) : toFixeds((parseFloat(data.CurrentPrice - data.OpenPrice) / data.OpenPrice * 100), 2));
                priceLimit = (priceLimit > 0 ? '+' : '') + priceLimit;
                priceExchange = (priceLimit > 0 ? '+' : '') + priceExchange + '%';
            };
            return {
                priceLimit: priceLimit,
                priceExchange: priceExchange
            }
        }
        //小数点精度
        function subtr(arg1, arg2) {
            var r1, r2, m, n;
            try {
                r1 = arg1.toString().split('.')[1].length;
            } catch (e) {
                r1 = 0;
            }
            try {
                r2 = arg2.toString().split('.')[1].length;
            } catch (e) {
                r2 = 0;
            }
            m = Math.pow(10, Math.max(r1, r2));
            n = r1 > r2 ? r1 : r2;
            return ((arg1 * m - arg2 * m) / m).toFixed(n);
        }

            return calculateUpDown(data);
        }

        //点击切换选项卡
        $('.Marker .r-title a').on('mouseover', function () {
            newsTab($(this));
        })

        function newsTab(obj) {
            if (!obj.hasClass('selected')) {//切换市场报价
                obj.addClass('selected').siblings().removeClass('selected');
                $('.table-group').children('[data-id="' + obj.attr('data-id') + '"]').addClass('active')
                    .siblings().removeClass('active');
            }
        }

        Marker.Init();
        
        //Marker.Init(Marker._Enum.requestQueue['forex'], 6);
    })()
</script>
