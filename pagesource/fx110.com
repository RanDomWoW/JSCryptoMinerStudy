                    News: function (data) {
                        var SmallImg = '';
                        if (!!data.SmallImg && data.SmallImg.length > 0) {
                            SmallImg = '<div class="img-place"><img src="' + JSON.parse(data.SmallImg)[0].Small + '" data-viewer="' + JSON.parse(data.SmallImg)[0].Big + '" /></div>';
                        }

                        return '<div class="details"><div class="news"><p>' + data.Title + '</p>' + SmallImg + '</div><em class="jt"></em></div>'


                    },
                    //日历

                    Calendar: function (data) {

                        var Revised = data.Revised;
                        var IsRevised = Revised ? 'block' : 'none';

                        var html = '<div class="details"><div class="calendar">'
                               // +'<img src="' + data.CountryImg + '" /> ',
                                + '<div class="content">'
                                + '<p>' + data.Title + '</p>'
                                + '<p>前值：' + data.Previous + '    预期：' + data.Consensus + '</p>'
                                + '<p style="display:' + IsRevised + '" class="revised">修正：' + Revised + '（前值）</p>'
                                + '<p style="margin:0;">公布：' + data.Actual + '</p>'
                                //+ '<p class="' + dataHandle.ImpactEm[data.Impact].color + '"><span>' + dataHandle.ImpactEm[data.Impact].content + '</span></p>'
                                //+ '<p><a href="' + dataHandle.Newslinks[data.Website] + '" target="_blank">查看详情</a></p>'
                                + '</div>'
                                + '</div><em class="jt"></em></div>';

                        return html;
                    }

                }                    

                function addIframe() {
                    var iframe = document.createElement('iframe');
                    iframe.src = "/Static/Forex/News.html";
                    iframe.style.display = 'none';
                    iframe.style.opacity = '0';
                    document.body.appendChild(iframe);
                }

                //图片点击放大效果
                function imgViewer() {
                    $(".img-place").Viewer({
                        imgSelector: "img", closeBgImg: "/images/x.png",
                        prevBgImg: "/images/zuojiantou-01.png", // 上一张按钮图片
                        nextBgImg: "/images/icon_youjiantou.png",
                    });
                }

                imgViewer();
                addIframe();

            })()

    </script>
