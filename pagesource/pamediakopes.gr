        <script>
//<![CDATA[
/**
 * Generic error handler with tracking to Analytics.
 *
 * @todo Replace 'document.location.href' with a more logical text like 'Flights results'.
 *       This could probably be read from a data attribute on BODY, the TITLE attribute
 *       or something like that...
 */
window.onerror = function(message, url, line) {
    setTimeout(function(){
        // Initialize _gaq if it's not already initialized.
        // This way even if analytics is not already initialized,
        // we can keep track of the exceptions until it is.
        _gaq = window._gaq || [];

        _gaq.push([
            '_trackEvent',
            'JS Exception',
            message,
            document.location.href,
            0, // Value
            true // Non interactive
        ]);
    }, 10);

    return true;
};
//]]>
</script>

    <script type="text/javascript">
    //<![CDATA[
    window.eTravel = window.eTravel || {};
    window.eTravel.common = window.eTravel.common || {};
    window.eTravel.common.debug = window.eTravel.common.debug || {};

    window.unexpectedPageExit = true;

    window.eTravel.common.debug.reportDebugInformation = function (debugInfo, async) {
        if (async === undefined) {
            async = true;
        }
        var xmlHttp = new XMLHttpRequest();
        var url = '/debug';
        xmlHttp.open('POST', url, async);
        xmlHttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
        xmlHttp.send('debug=' + debugInfo);
    };

    window.eTravel.common.debug.reportCancelByKey = function (e) {
        if (e.keyCode == 27 || e.keyCode == 8) {
            window.eTravel.common.debug.reportDebugInformation('Escape/Backspace key pressed');
        }
    };

    window.eTravel.common.debug.reportUnexpectedPageUnload = function () {
        if (window.unexpectedPageExit) {
            window.eTravel.common.debug.reportDebugInformation('Page loading canceled due to back/stop button or user navigated away.', false);
        }
    };

    window.eTravel.common.debug.reportJavascriptError = function (message, url, line, column) {
        var cause = JSON.stringify({
            message: message,
            url: url,
            line: line,
            column: column
        });
        window.eTravel.common.debug.reportDebugInformation("JS Error: " + cause);
    };

    window.eTravel.common.debug.initReportUnexpectedPageExits = function () {
        if (window.addEventListener) {
            window.addEventListener("keyup", window.eTravel.common.debug.reportCancelByKey);
            window.addEventListener("unload", window.eTravel.common.debug.reportUnexpectedPageUnload);
        } else if (window.attachEvent) {
            window.attachEvent("onkeyup", window.eTravel.common.debug.reportCancelByKey);
            window.attachEvent("onunload", window.eTravel.common.debug.reportUnexpectedPageUnload);
        }
    };

    window.eTravel.common.debug.initReportJavascriptErrors = function () {
        window.onerror = window.eTravel.common.debug.reportJavascriptError;
    };
    //]]>
</script>


    
    
