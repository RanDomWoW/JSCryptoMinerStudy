	<script type="text/javascript">
		function validateEmail() {
			var x = document.forms["form-news"]["email"].value;
			var email = document.getElementById("email").value;
			var atpos = x.indexOf("@");
			var dotpos = x.lastIndexOf(".");
			
			if(email === ''){
				document.getElementById('msg-error').innerHTML  = "กรุณากรอกอีเมล์";
				return false;
			}
			
			else if (atpos<1 || dotpos<atpos+2 || dotpos+2>=x.length){
			  document.getElementById('msg-error').innerHTML  = "รูปแบบอีเมล์ไมม่ถูกต้อง";
			  return false;
			}
			document.getElementById('msg-error').innerHTML  = "";
			return true;
		}
				
