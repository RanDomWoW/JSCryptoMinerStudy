<script>

jQuery(document).ready(function (){
   	jQuery('.attr-attribute_pa_matureheight').each(function(){
		jQuery(this).parent().parent().hide();
	});
	jQuery('.attr-attribute_pa_sunlight').each(function(){
		jQuery(this).parent().parent().hide();
	});
	jQuery('.attr-attribute_pa_categoryofplant').each(function(){
		jQuery(this).parent().parent().hide();
	});

	var didInit = false

	function updateZoneInfo() {
		var infoEl = jQuery('#growing-zone-info')
		var zone = localStorage.getItem('growing-zone')
		if (zone) {
			infoEl.text('My Growing Zone: ' + zone)
		} else {
			infoEl.text('My Growing Zone')
		}

		if (!didInit) {
			didInit = true
			var zip = localStorage.getItem('growing-zone-zip')
			jQuery('#input-growing-zone-zip').val(zip)
			jQuery('#top-custom-link-widget').show()
		}
	}

	updateZoneInfo()

	if (jQuery('#input-growing-zone-zip').length) {
		jQuery('#input-growing-zone-zip').on('keyup', function (e) {
			var key = e.keyCode
			if (key == 13 || key == 27) {
				jQuery('#top-custom-link-widget .hb-dropdown-box').removeClass('dropdown-visible')
				return
			}

			var zip = jQuery('#input-growing-zone-zip').val()
			var regex = /^[0-9+]{3,5}$/
			var resultEl = jQuery('#growing-zone-result')
			resultEl.text('')

			if (zip && regex.test(zip)) {
				resultEl.show()
				resultEl.text('Loading...')
				jQuery.ajax('/wp-content/themes/HighendWP-child/growing-zone/?zip=' + zip)
				.done(function (data) {
					if (data.zip == zip) {
						resultEl.text('You are in Growing Zone: ' + data.zone)
						localStorage.setItem('growing-zone', data.zone)
						localStorage.setItem('growing-zone-zip', data.zip)
					} else {
						resultEl.text('Please enter a valid Zip Code.')
						localStorage.removeItem('growing-zone')
					}
					updateZoneInfo()
				}).fail(function () {
					resultEl.text('Request failed, please try again later.')
					localStorage.removeItem('growing-zone')
					updateZoneInfo()
				})
			} else if (zip) {
				resultEl.show()
				resultEl.text('Please enter a valid Zip Code.')
				localStorage.removeItem('growing-zone')
			} else {
				resultEl.hide()
			}
			
			updateZoneInfo()
		})
	}
})
</script>

