function readMail(mailid){
    if(location.host=="secure.verywed.com"){
        location.href="/mailBox/mail/show?mail_id="+mailid;
    }else{
	    var url='/mailBox/mail/readMail/'+mailid;     //網頁位置;
	    var mail=document.createElement("div");
	    var node=document.createElement("div");
        var close=document.createElement("div");
	    var mask=document.createElement("div");
        mask.className="mask";
	    close.innerHTML="X";
	    close.id="close";
	    node.id="mail-text";
	    mail.id="mail-div";
	    mail.appendChild(node);
	    mail.appendChild(close);
	    document.body.appendChild(mask);
	    document.body.appendChild(mail);
	    jQuery("#mail-text").load(url,function(){ jQuery("#mail-text").fadeIn(100);});
	    document.getElementById('mail-div').style.left=(document.body.clientWidth-document.getElementById('mail-div').offsetWidth)/2+"px";
            document.getElementById('mail_list').style.display="none";
            mail_onclick=0;
	    document.getElementById('close').addEventListener("click", function(){
		    window.history.go(0);
	    });
    }
}

function readMsn(mailid,is_new){  //讓通知信變成已讀取
        var url='/mailBox/mail/readMail/'+mailid;     //網頁位置;
        var name='';    //網頁名稱;
        var iWidth=1;  //視窗的寬度;
        var iHeight=1; //視窗的高度;
        var iTop=0;  //視窗的垂直位置;
        var iLeft=0;   //視窗的水平位置;
	var myWindow;
//若信件為未讀取狀態則開啟新視窗載入信件 2015.09.24 eva
	if(is_new==0){
        myWindow=window.open(url,name,'height='+iHeight+',innerHeight='+iHeight+',width='+iWidth+',innerWidth='+iWidth+',top='+iTop+',left='+iLeft+',status=no,location=no,status=no,menubar=no,toolbar=no,resizable=no,scrollbars=no');
        myWindow.opener=null;
        myWindow.close();
	}
}

