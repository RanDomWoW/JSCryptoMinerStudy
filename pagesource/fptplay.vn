    $(document).ajaxComplete(function(e, x, o) {
        if (/\/tim-kiem\/auto$/ig.test(o.url)) {
            isSearching = true;

            setTimeout(function() {
                var idx = 0;

                $('.auto_search a:not(.tracking)').each(function() {
                    $(this).attr('href', $(this).attr('href')+'#tap-1/s/'+(idx++)+'/k/'+ encodeURIComponent($('#search_keyword').val()) +'/sug');
                }).addClass('tracking').click(function() {
                    var _s = new tracker({
                        link: '/v5.1_w',
                        category: 'enter',
                        id: $('#search_keyword').val()
                    });

                    _s.track(_s.TRACK_EVENT_NORMAL, 'search');
                    document.location = $(this).attr('href');
                    return false;
                });
            }, 100);
        }
    });

    $('body').click(function() {
        if (isSearching) {
            isSearching = false;
            var _s = new tracker({
                link: '/v5.1_w',
                category: 'quit',
                id: $('#search_keyword').val()
            });

            _s.track(_s.TRACK_EVENT_NORMAL, 'search');
        }
    });

    $('#search_keyword').keydown(function(e) {
        if ((e.keyCode || e.which) === 13) {
            var _s = new tracker({
                link: '/v5.1_w',
                category: 'enter',
                id: $('#search_keyword').val()
            });

            _s.track(_s.TRACK_EVENT_NORMAL, 'search');
        }
    });

    if (document.location.pathname.indexOf('/tim-kiem/') === 0) {
        var keyword = document.location.pathname.substr('/tim-kiem/'.length);
        var idx = 0;
        $('#box_key_search a').each(function() {

            $(this).attr('href', $(this).attr('href')+'#tap-1/s/'+(idx++)+'/k/'+keyword);
        });
    }
</script>
