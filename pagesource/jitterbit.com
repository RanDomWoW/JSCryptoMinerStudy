/*========== User Custom CSS Styles ==========*/
h2 {
    font-size: 30px;
    color: #22042D;
    line-height: 43px;

    @include media(max-width 767px) {
        font-size: 26px;
        line-height: 37.266666658px;
    }
}

h3 {
    font-weight: normal !important;
    font-size: 30px;
    color: #22042D;
    line-height: 48px;
}

h4 {
    font-size: 22px;
    color: #22042D;
    line-height: 24px;
}

p {
    font-size: 20px;
    line-height: 30px;
    color: #35363C;
    letter-spacing: 0px;
}

li {
    font-size: 20px;
    line-height: 30px;
    color: #35363C;
    letter-spacing: 0px;
}

.endpoint-icon {
display: block;
    border: 1px solid #E3E3E3;
    border-radius: 5px;
    text-align: center;
float: none;
    font-size: 14px;
    font-weight: bold;
padding: 0 0 10px 0;
line-height: 1.5;
height: auto;
width: 108px;
margin: 0 auto;
overflow: hidden;
transition: all 0.2s ease-out;
}
.endpoint-icon:hover {
box-shadow: 0 1px 4px rgba(0,0,0,0.2);
}
.endpoint-icon:hover {
text-decoration: none;
}
.endpoint-icon img {
    width: 108px;
    height: 108px;
margin: 0 0 8px;
    border-bottom: 1px solid #E3E3E3;
}
.endpoint-icon-featured {
font-size: 18px;
margin-bottom: 30px;
width: 162px;
height: 236px;
}
.endpoint-icon-featured img {
    width: 162px;
    height: 162px;
}

.skinny-container {
max-width: 970px;
margin: 0 auto;
}
.col-sm-2.five-column {
width: 20%;
}
.sf-icon-box-content p {
font-size: 16px;
line-height: 24px;
}
.platform-section .sf-icon-box.sf-icon-box-left-icon-alt > i[class*="fa-"] {
    color: #fff;
    line-height: 90px;
    line-height: 1.5;
}

.section-intro-orange p {color: #22042D ; font-size: 20px; line-height: 30px;}
.section-intro-orange h3 {color: #FF7C44 ; font-weight:700 !important}

.col-sm-6 .sf-icon-box-standard-title h3,
.col-sm-4 .sf-icon-box-standard-title h3 {
padding-left: 0;
}

.text-arrow-cta {
font-weight: bold;
    font-size: 16px;
    line-height: 20px;
    color: #FF7C44;
    letter-spacing: 1px;
    text-transform: uppercase;
}

a, .ui-widget-content a {
    color: #FF7C4C;
}

.connector-container figure.animated-overlay:hover figcaption {
opacity: 0;
}

.sf-icon-box-hr {display: none;}

.section-two.platform-section .sf-icon-box-content p,
.section-two.platform-section .sf-icon-box-content ul li {
    font-size: 16px;
    line-height: 24px;
color: #35363C;
opacity: 1;
}
.section-two.platform-section .sf-icon-box-content-wrap h3 {
font-weight: 700 !important;
color: #35363C;
}

.hero.whyjitterbit .hero-headline {
margin: 0 auto 11px;
max-width: 90%;
}

.solutions-by-banners {
transition: opacity 0.2s ease-out;
}

.solutions-by-banners h3 {
font-weight: 700 !important;
color: #fff;
}

.solutions-by-banners:hover {
opacity: 0.7
}

.swiper-slide .testimonial-picture {
display: none;
}

.featured-resources-post .post-thumbnail-container img {
margin-top: -16%;
}

#comment-area,
#respond-wrap {
display: none;
}

#mktoForm_1261 {
display: none;
}

@media screen and (max-width: 767px) {
#mktoForm_1260 {
display: none;
}
#mktoForm_1261 {
display: block;
}
}

.masonry-items.grid .category-datasheet .masonry-item-wrap .details-wrap {
display: none;
}

ul.blog-aux-options li {
display: none;
}


ul.blog-aux-options li:first-child {
display: block;
}

ul.blog-aux-options li:first-child a {
border-radius: 4px;
}

.search-items .blog-item.type-attachment {
display: none;
}

strong {
font-weight: 600!important;
}

.sf-button.large.lightblue.slightlyroundedarrow {
border: 1px solid #222;
border: 1px solid rgba(34, 34, 34, .2);
color: #35363c !important;
border-radius: 2px;
}

.sf-button.large.lightblue.slightlyroundedarrow:hover {
background: #adadad;
border: 1px solid #adadad;
color: #fff !important;
}

@media only screen and (min-width: 768px) {
    .category-webinar-videos-private .col-sm-12 {
        position: relative;
        padding-left: 47px !important;
    }
    .category-webinar-videos-private .single-post .container .body-text>.container {
        margin-left: auto !important;
    }
}
@media only screen and (min-width: 768px) and (max-width: 990px) {
    .team-member figure.gallery-style img {
        -webkit-transition: all 0.4s;
        -moz-transition: all 0.4s;
        transition: all 0.4s;
    }
    .team-member figure.gallery-style:hover img {
        opacity: 0
    }
}

/* BEGIN FLYDUO: CUSTOM CSS */
.whyjitterbit-section-two--padding-mod {
  padding-bottom: 0 !important;
}
/* MEDIUM WIDTHS */
@media screen and (min-width: 768px) {/* 10003px TODO: Make this am 'em' flexible unit value */
  .whyjitterbit-section-two--padding-mod {
    padding-bottom: 80px !important;
  }
}
/* WIDE WIDTHS */
@media screen and (min-width: 1003px) {/* 10003px TODO: Make this am 'em' flexible unit value */
  .whyjitterbit-section-two--padding-mod {
    padding-bottom: 0 !important;
  }
}

/* COMPANY PAGE */
.recent-posts--mod.news-and-press figure {
  display: none;
}
/* INNOVATE WITH API'S */
.platform-section.section-two.platform-section-two--mod img {
  max-width: 100%;/* enable images to be wider than the default template allows */
}
/* INTEGRATION MADE INTUITIVE */
.platform-section.section-four.platform-section-four--mod .bucket {
  height: auto;/* overrides 93px fixed height value that causes container over run and breaks layout */
}
/* SOLUTIONS */
.solutions-by-banners h3 {/* make text over banners larger (block #1) */
    font-size: 28px !important;/* was 18px */
    line-height: 33px !important;/* was 23px */
}
.solutions-by-banners h3 strong {/* make text over banners larger | also requires the use of the <strong> tag to work (block #2) */
    font-weight: initial;
    display: block;
}

/* Dropdown Arrow in select menu */
.dropdown-arrow {/* ensure select menu arrow icon works - force relative URL path for icon so SSL doesn't break it */
    background: #290834 url(http://jitterbit.flywheelsites.com/wp-content/themes/jitterbit-theme/images/dropdownicon.png) no-repeat scroll center center / 30% auto !important;
