    $('.videovaci').click(function() {
      $('.videovaci').removeClass('actbutton2');
      var vid = $(this).attr('href').substr(1);
      $(this).addClass('actbutton2');
      
      jQuery.ajax({
       type: "GET",
       url: "https://www.kfilmu.net/ajax/videohp-load/",
       data: "vid="+ vid,
       cache: false,
