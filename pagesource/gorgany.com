<script>
    function showNotification(type, message, limit) {
        if (typeof limit == 'undefined') limit = 5000;
        var msg = jQuery('<div class="alert alert-notification alert-dismissible fade in alert-wish alert--fixed '+type+'" role="alert">'+
                             '<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true"><i class="fi fi-close"></i></span></button>'+
                             '<p class="">'+message+'</p>'+
                           '</div>');
        jQuery('body').append(msg);
        if (jQuery('.alert-notification').length) {
            setTimeout(function(){ $(msg).show(); }, 300);
            setTimeout(function(){ $(msg).remove(); }, limit);
        }
    }

    function getURLVar(urlVarName) {
        var urlHalves = String(document.location).toLowerCase().split('?');
        var urlVarValue = '';

        if (urlHalves[1]) {
            var urlVars = urlHalves[1].split('&');

            for (var i = 0; i <= (urlVars.length); i++) {
                if (urlVars[i]) {
                    var urlVarPair = urlVars[i].split('=');

                    if (urlVarPair[0] && urlVarPair[0] == urlVarName.toLowerCase()) {
                        urlVarValue = urlVarPair[1];
                    }
                }
            }
        }

        return urlVarValue;
    }

