            "ecommerce": {
                "remove": {
                    "products": [{
                        "id": jQuery(el).data('product_id'),
                        "name": jQuery(el).data('product_name'),
                        "price": jQuery(el).data('product_price'),
                        "brand": jQuery(el).data('product_brand'),
                        "category": jQuery(el).data('product_category'),
                        "variant": jQuery(el).data('product_variant'),
                        "position": 0,
                        "quantity": jQuery(el).data('product_quantity')
                    }]
                }
            }
        });
        console.log('GTM add Remove From Card Product Click', dataLayer);
        setTimeout(function() {
            if (action == "redirect") {
                window.location = 'index.php?route=checkout/cart&remove=' + jQuery(el).data('product_key');
            } else {
                $('.header-cart').load('index.php?route=module/cart&remove='+jQuery(el).data('product_key') + ' .header-cart > *')
            }
        }, 300);
        return false;
    }


    jQuery(document).ready(function() {
        jQuery('.subscribe_button').on('click', function(){
            console.log('button clicked ' + jQuery('input[name=subscribe_email]').val());
            process_subscribe(jQuery('input[name=subscribe_email]').val());
        });

        $('input[name=subscribe_email]').keypress(function (e) {
            if (e.which == 13) {
                console.log('input enter clicked ' + jQuery('input[name=subscribe_email]').val());
                process_subscribe(jQuery('input[name=subscribe_email]').val());
                return false;    //<---- Add this line
            }
        });

        $('.catalog-item__label-like i').click(function(){
            if ($(this).hasClass('inWishlist')) {
                //return false; // it means that product in wishlist and lets load normal url in this case.
            }
            var that = $(this);
            var product_id = $(this).attr('data-product_id');
            var model_id = '';
            $.ajax({
                url: 'index.php?route=product/wishlist/add',
                type: 'POST',
                //dataType: 'json',
                //contentType: false,       // The content type used when sending data to the server.
                cache: false,             // To unable request pages to be cached
                //processData:false,
                data: {product_id: product_id, model_id: model_id},
                beforeSend: function() {
                    $('.success, .warning').remove();
                    $('#notification').html('');
                    $('.add_to_wishlist').addClass('loading');

                },
                complete: function() {
                    $('.add_to_wishlist').removeClass('loading');
                },
                success: function(data) {
                    var data = JSON.parse(data);
                    console.log(data);
                    if (typeof data.error != 'undefined' ) {
                        $('#notification').html('<div class="warning" style="display: none;">' + data.error + '<img src="catalog/view/theme/default/image/close.png" alt="" class="close" /></div>');
                        $('#notification > div').fadeIn('slow');
                        showNotification('error', data.error);
                    }

                    if (typeof data.success != 'undefined') {
                        // notification add/remove product to wish list
                        showNotification('success', data.success);
                        $(that).addClass('inWishlist');
                    }

                }
            });
            return false;
        });


        
        <!-- Google Tag Manager | Enhanced Ecommerce BOF -->
        jQuery('a.product_link').on('click', function(e) {
            e.preventDefault;
            dataLayer.push({
                "event": "productClick",
                "ecommerce": {
                    "click": {
                        "actionField": {
                            "list": jQuery(this).data('product_list')
                        },
                        "products": [{
                            "id": jQuery(this).data('product_id'),
                            "name": jQuery(this).data('product_name'),
                            "price": jQuery(this).data('product_price'),
                            "brand": jQuery(this).data('product_brand'),
                            "category": jQuery(this).data('product_category'),
                            "position": jQuery(this).data('product_position')
                        }]
                    }
                }
            });
            console.log('GTM add Product Click', dataLayer);
            var link = jQuery(this).attr('href');
            setTimeout(function() {
                window.location = link; // go to the product page.
            }, 300);
            return false;
        });
        <!-- Google Tag Manager | Enhanced Ecommerce EOF -->


    });
</script>
