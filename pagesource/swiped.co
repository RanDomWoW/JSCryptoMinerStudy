<script type="text/javascript"> 
	     
	/// Mobile Menu 
	$(document).ready(function(){
		$('#mobilemenu').css('cursor', 'pointer').click(function(){
				$('.main-navigation ul').slideToggle();
			});
	});
	
	// Hide Empty Paragraphs
	$(document).ready(function(){
		$('p:empty, li:empty').remove();
	});
			
  	// Dropdown Menus
  	$(document).ready(function(){
		jQuery('a.welcome').mouseenter(function(){
			jQuery('.user-dropdown').fadeIn('fast');
		});
		jQuery('#userlink').mouseleave(function(){
			jQuery('.user-dropdown').fadeOut('fast');
		});
		jQuery('a.insiders').mouseenter(function(){
			jQuery('.insiders-dropdown').fadeIn('fast');
		});
		jQuery('.insiders-link').mouseleave(function(){
			jQuery('.insiders-dropdown').fadeOut('fast');
		});
	});
	
	
	// Hide Website & Dropdown Fields on Click
	$(document).ready(function(event){
		$('.popup-with-form').css('cursor', 'pointer').click(function(){
			setTimeout(function(){
				$('#field_4_9').hide();
				$('#field_4_3').hide();
				$('#input_4_10').val('Pop-Up');
			}, 10);
		});
	});
	
	// Add Footer Tag If Form Is Submitted In Footer
	$(document).ready(function(event){
		$('#subscribeform #input_4_2').on('focus', function(){
			setTimeout(function(){
				$('#input_4_10').val('Footer');
			}, 10);
		});
	});

	// Signup Popup Form
	$(document).ready(function() {
	
		$('.popup-with-form').magnificPopup({
			type: 'inline',
			preloader: false,
			focus: '#name',
			callbacks: {
				open: function() {
					$('#short-url input').select(); 	
				},
				close: function() {
				  // For collections pop-up - hiding the form
				  // Only show if on a swipe (non-edit) page
				 /*if ($('#edit-collection').length == 0) {
					 $('.collections-form').hide();
					 $('.collections-popover--form-trigger').show(); 
				  } */
				},
				beforeOpen: function() {
					if($(window).width() < 700) {
						this.st.focus = false;
					} else {
						this.st.focus = '#name';
					}
				}
			}
		});
			
	});
	
	// If Popup With Demo Vids Closes, Also Close & Reset Videos
	$(document).ready(function() {
		$(document).bind('click', function(e) {
		  // If video link is active, clicking on anything outside the popup will close it
		  if($('#help_popup li a.active').length > 0 && $(e.target).closest('#help_popup').length === 0) {
			var active_video = $('#help_popup li a.active').attr('data-content');
			document.getElementById(active_video).pause();
			$('#help_popup li a').removeClass('active');
			$('#help_popup li video').hide();
		  }
		});
	});	
	
	// Youtube Popup Form
	$(document).ready(function() {
	
		// If Is Not Swipe Page
		if ($('.single-swipefiles').length == 0) {
		
			// If It Contains Start Time vs. Regular Link
			var youtube_link = $('.youtube-popup').attr('href');
		
			if (youtube_link && youtube_link.indexOf('start') == -1) {
				var youtube_src = '//www.youtube.com/embed/%id%?rel=0&autoplay=1'
			} else {
				var youtube_src = '//www.youtube.com/embed/%id%&rel=0&autoplay=1'	
			}
	
			$('.youtube-popup').magnificPopup({
			  type: 'iframe',
			  iframe: {
					markup: '<div class="mfp-iframe-scaler">'+
							'<div class="mfp-close"></div>'+
							'<iframe class="mfp-iframe" frameborder="0" allowfullscreen></iframe>'+
							'</div>', 
					patterns: {
						youtube: {
							  index: 'youtube.com/', 
							  id: 'v=', 
							  src: youtube_src
							}
						 },
					srcAction: 'iframe_src',
					callbacks: {
						open: function() {
							$('.collection-content').magnificPopup('close');
						},
					}
				
				 }
			});
		
