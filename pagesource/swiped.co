					var i = 0;
					$('.emailcta_links li:contains("' + cta_text + '")').each(function() {
						if (i > 0) {
							var email_num = $(this).attr('data-email');
							var first_item = $('.emailcta_links li:contains("' + cta_text + '"):eq(0)');
							var first_item_email_num = first_item.attr('data-email');
							if (email_num != first_item_email_num) {
								first_item.attr("data-email", first_item.attr("data-email") + ', ' + email_num);
							}
							$(this).remove();
						}
						i++;
					});
					// Sort attr list & remove dupes
					function sortNumber(a,b) {
						return a - b;
					}
					var list = $(this).attr("data-email");
					var arr = $.unique(list.split(', '));
					list = arr.sort(sortNumber).join(", ");
					$(this).attr('title', amt + ' instances found. From email #' + list + '.');
				} else {
					var list = $(this).attr("data-email");
