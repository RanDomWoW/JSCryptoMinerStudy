			}); 
		
			// Closing Swipe Nav & Changing Links
			$('.navtag_nums .fa-times-circle').css('cursor', 'pointer').click(function(){
				$('#compass').toggle();
				$('#tagline').toggle();
			
				var next_url = $('#jj-next-post a').attr('href');
				var prev_url = $('#jj-prev-post a').attr('href');	
				if (next_url) {			
					var next_noparam = next_url.substring(0, next_url.indexOf("?"));
					$('#jj-next-post a').attr('href', next_noparam);
				}
