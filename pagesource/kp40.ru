		if ($('#pf_place').val() == "") {
			alert("Заполните место мероприятия.");
			$('#pf_place').focus();
			return (false);
		}
		if ($('#pf_time').val() == "") {
			alert("Заполните время мероприятия.");
			$('#pf_time').focus();
			return (false);
		}
		if ($('#pf_descr').val() == "") {
			alert("Заполните описание мероприятия.");
			$('#pf_descr').focus();
			return (false);
		}
		if ($('#pf_contact').val() == "") {
			alert("Заполните контактные данные.");
			$('#pf_contact').focus();
			return (false);
		}

		$('#post_form > *').attr('disabled', 'true');
		$('#inner_post_form').hide();
		$('#status_post_form').show();
		$.post(
			"/index.php",
			{a: 'lets_af_event_post', place: $('#pf_place').val(), time: $('#pf_time').val(), descr: $('#pf_descr').val(), contact: $('#pf_contact').val()},
			function(data) {
				if(data.status == 'ok') {
					$("#status_post_form").html('Спасибо, Ваше событие будет добавлено после проверки модератором.');
				} else if (data.status == 'error') {
					alert('Ошибка отправки запроса. Попробуйте позже.');
					$('#post_form > *').attr('disabled', 'false');
					$('#inner_post_form').show();
					$('#status_post_form').hide();
				}
			},
			"json"
		);
	  //return;
	}
</script>

