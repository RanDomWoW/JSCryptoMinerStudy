<script type="text/javascript">
$(document).ready(function(){
	// IF NOT LOGGED IN
	$('.sapna-logged-out').on('click', function(e){
		e.preventDefault();
		var element_id=$(this).attr("id");
		var login_type = $(this).attr("aff-login-type");
		if( login_type == 'affiliate' ){
			$('#login-form .login-submit').attr("aff-login-type",login_type);
		}
		if(element_id != "sapna-logged-out-area"){
				titanUI.showDialog('#login-modal-wrapper');
		}
	});
	// TOPBAR SEARCH
	var stoppedTyping=null;
	$(".main-search-input").keyup( function(e) {
		if (e.keyCode == 38) { // up
			var selected = $('.layout-search-autocomplete .main-search-ul .selected');
			if ( selected.length != 0 ){
			$('.layout-search-autocomplete .main-search-ul li').removeClass('selected');
			if (selected.prev().length == 0) {
				selected.siblings().last().addClass('selected');
			} else {
				selected.prev().addClass('selected');
			}
			} else {
				$('.layout-search-autocomplete .main-search-ul li').first().addClass('selected');
				selected = $('.layout-search-autocomplete .main-search-ul .selected');
				$('.layout-search-autocomplete .main-search-ul li').removeClass('selected');
				if (selected.prev().length == 0) {
					selected.siblings().last().addClass('selected');
				} else {
					selected.prev().addClass('selected');
				}
			}
			selected = $('.layout-search-autocomplete .main-search-ul .selected');
			$('.main-search-input').val(selected.find('.main-search-title .product-name').text().trim());
			return;
		}
		if (e.keyCode == 40) { // down
			var selected = $('.layout-search-autocomplete .main-search-ul .selected');
			if ( selected.length != 0 ){
			$('.layout-search-autocomplete .main-search-ul li').removeClass('selected');
			if (selected.next().length == 0) {
				selected.siblings().first().addClass('selected');
			} else {
				selected.next().addClass('selected');
			}
			} else {
				$('.layout-search-autocomplete .main-search-ul li').first().addClass('selected');
			}
			selected = $('.layout-search-autocomplete .main-search-ul .selected');
			$('.main-search-input').val(selected.find('.main-search-title .product-name').text().trim());
			return;
		}
		if( $(this).val().trim().length > 2 ) {
			var text = $(this).val().replace(/\s\s+/g, ' ');
			text = text.replace(/["%&\t\r\n]+/g, "").trim();
			var text_value = text.toLowerCase();
			text=text.replace(/\s+/g, '+');
			if (e.keyCode == 13) {
				//alert("text="+text);
				window.location.href=$('#base_url').val()+'/general-search?searchkey='+text;
				return;
			}
			var self = $(this);
			// is there already a timer? clear if if there is
			if (stoppedTyping) clearTimeout(stoppedTyping);
			 // set a new timer to execute 300 ms from last keypress
			stoppedTyping = setTimeout( function () {
			self.parent().find(".main-search-loader").fadeIn(300);
			$.ajax({
                type: "POST",
                url: $('#base_url').val()+"/generalTopSearch?searchkey="+text,
            }).done(function(data) {
				self.parent().find(".main-search-loader").fadeOut(300);
				// Decode JSON Response for removing back slashes
				if ( data == '' ) {
					$('.layout-search-autocomplete').find('.main-search-ul').html('').hide();
					return;
				}
				data=data.trim();
				var element_url='';
				var html='';
				var author_tag='';
				var new_str="";
				var new_text = properCase(text_value);
					var obj = jQuery.parseJSON(data);
	                var jsonResponse=obj.searchResults
					for(var i=0; i< jsonResponse.length;i++) {
						new_str=jsonResponse[i]['shortName'].split(" ");
						var highlight_str="";
						highlight_str=(new_str[new_str.length-1].indexOf("...") != -1)?jsonResponse[i]['shortName'].substring(0,(jsonResponse[i]['shortName'].length-3)):jsonResponse[i]['shortName'];
						highlight_str = highlight_str.replace((new RegExp(new_text,"g")), '<b>'+new_text+'</b>');
						highlight_str = highlight_str.replace((new RegExp(text_value,"g")), '<b>'+text_value+'</b>');
						if ((typeof jsonResponse[i]['productURL']) != 'undefined' && jsonResponse[i]['productURL'] != null && jsonResponse[i]['productURL'] != '') {
							element_url=$("#base_url").val()+"/books/"+jsonResponse[i]['productURL'];;
						}else{
							element_url=$("#base_url").val()+"/general-search?searchkey="+jsonResponse[i]['shortName'];
						} 
						author_tag = '';
						if ((typeof jsonResponse[i]['shortAuthorName']) != 'undefined' && jsonResponse[i]['shortAuthorName'] != null && jsonResponse[i]['shortAuthorName'] != '') {
							author_tag =jsonResponse[i]['shortAuthorName'];
							author_tag = author_tag.replace((new RegExp(new_text,"g")), "<b>"+new_text+"</b>");
							author_tag = author_tag.replace((new RegExp(text_value,"g")), "<b>"+text_value+"</b>");
						} 
						if ( author_tag != '' ) author_tag="-by "+author_tag;
						html=html+'<li ><a href="'+element_url+'" class="main-search-link"><p class="main-search-title"><span class="product-name">'+highlight_str+'</span></p>&nbsp;<p class="main-search-description">'+author_tag+'</p></a></li>';
					}
				
				 $('.layout-search-autocomplete').find('.main-search-ul').html(html).show();
				$('.layout-search-autocomplete').fadeIn(200,function(){
					$(document).click(function(event) {
						if($(event.target).parents().index($('.layout-search-autocomplete')) == -1) {
							if($('.layout-search-autocomplete').is(':visible')) {
								$('.layout-search-autocomplete').hide()
								$(document).unbind('click');
								event.stopPropagation();
							}
						}
					});
					$('.layout-search-autocomplete .main-search-ul li').mouseover(function(){
						$('.layout-search-autocomplete .main-search-ul li').removeClass('selected');
						$(this).addClass('selected');
						return;
					});
				}); 
				stoppedTyping=null;
				}) 
			},400 );
		}
		else{
			$('.layout-search-autocomplete').fadeOut(200, function(e){
				$('.layout-search-autocomplete').find('.main-search-ul').html('');
			});
			$('.main-search-input').unbind('keydown');
		}
	});
	$('.main-search-button').click(function() {
		var text = $('.main-search-input').val();
		var selectedOne="";
		if ( text ) {
			//selectedOne = $(this).val();
			text = text.replace(/["%&\t\r\n]+/g, "").trim().replace(/\s+/g, '+');
			text = text.replace(/[^a-zA-Z0-9]/g,'_');
			window.location.href = $('#base_url').val()+'/general-search?searchkey='+text;
		}
		return false;
	});
	 
});
</script>
         
