<script type="text/javascript">
jQuery(document).ready(function($) {
	jQuery('#main-nav1').stellarNav({
		theme: 'light'
	});
	
	$(".topnavi").mouseover(function () {
	    navi_open();
	});
	
	$(".topnavi a").focus(function() {
	    navi_open();
	});
	
	$("#header").mouseleave(function() {
	    navi_close();
	});
	
	$(".topnavi:last-child > ul > li:last-child > a").blur(function() {
	    navi_close();
	});
	
	function navi_open(){
      $(".topnavi").each(function() {
	        $(this).children("ul").removeClass("active");
	        if(!$(this).children("ul").is(':visible')){
	            $(this).children("ul").slideDown(400);
	        }
	    });
	
	    $(this).children("ul").addClass("active");
	    if(!$("#bgGnb").is(':visible')){
	        $("#bgGnb").slideDown(400);
	    }
	}
	
	function navi_close(){
	    $("#bgGnb").slideUp(400);
	    
	    $(".topnavi").each(function() {
	        $(this).children("ul").slideUp(400);
	    });
	}
});

$("#meta-img").slick({
	arrows: true,
	dots: true,
	autoplay:true
});

$("#m_author").slick({
	arrows: true,
	dots: false,
	adaptiveHeight: true,
	autoplay:true
});

//project
$('.pjbox').each(function(mrb){
    $(this).attr('idx',mrb);
}).hover(function(){
	num = $(this).attr('idx');
	$('.pjbox:eq('+num+') .pibb').stop().animate({top:'-0vw'},100);
},function(){
	num = $(this).attr('idx');
	$('.pjbox:eq('+num+') .pibb').stop().animate({top:'65%'},100);
});

//quick
$('.quick-m ul li a div img:last-child').addClass('qdn');
$('.quick-m ul li').each(function(qdn){
	$(this).attr('idx',qdn);
}).hover(function(){
	num = $(this).attr('idx');
	$('.quick-m ul li:eq('+num+') a div img:first-child').stop().addClass('qdn');
	$('.quick-m ul li:eq('+num+') a div img:last-child').stop().removeClass('qdn');
},function(){
	num = $(this).attr('idx');
	$('.quick-m ul li:eq('+num+') a div img:last-child').stop().addClass('qdn');
	$('.quick-m ul li:eq('+num+') a div img:first-child').stop().removeClass('qdn');
});

$('#mailing').on('show.bs.modal', function () {
    setTimeout(function (){
        $('#mailing_name').focus();
    }, 500);
});

function js_mailing(){
    if($('#mailing_name').val()==''){
        alert('이름을 입력해 주세요.');
        $('#mailing_name').focus();
        return false;
    }
    
    if($('#mailing_address').val()==''){
        alert('이메일 주소를 입력해 주세요.');
        $('#mailing_address').focus();
        return false;
    }
    
    $.ajax({
        url: "./mailing1.php",
        type: "POST",
        data: {
            "mailing_name" : $('#mailing_name').val(),
            "mailing_address" : $('#mailing_address').val(),
            "homepage" : '홈페이지'
        },
        dataType: "json",
        async: false,
        cache: false,
        success: function(data) {
            alert('메일링 서비스에 가입되었습니다.');
            $('#mailing').modal('hide');
        }
    });
}
</script>
