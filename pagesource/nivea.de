<script type="text/javascript">

    jQuery(document).ready(function($) {
        Nivea.initUi.init();
        Nivea.headerHandler.init();
        Nivea.footerHandler.init();
        Nivea.sitecoreHandler.init();
        if (Nivea.WelcomePage)
            Nivea.WelcomePage.init();
        if (Nivea.contactUsHandler)
            Nivea.contactUsHandler.init();
        if (Nivea.localeHandler)
            Nivea.localeHandler.init();

        $(".personalized-content").each(function() {
            var jEl = $(this);
            var placeholderName = jEl.data("placeholder-name");
            var itemName = jEl.data("item-name");
            if (placeholderName && itemName) {
                var url = itemName + "?useAjax=true&placeholder=" + placeholderName;
                $.ajax({
                    url: url
                }).done(function(data) {
                    if ($.trim(data).length > 0) {
                        if ($(data).find('.teaser').length) {
                            jEl.after(data);
                            jEl.remove();
                        } else
                            jEl.html(data);
                    }
                });
            }
        });


    });
</script>

<script>
    ;
    (function($) {
        $(function() {
            $('.iframe-module').responsiveIframe({
                xdomain: '*',
                scrollToTop: false
            });
        });
    })(jQuery);
</script>





</body>
</html>