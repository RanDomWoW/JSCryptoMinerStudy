                  $scope.SharedServicesUrl = "https://services.edmc.edu";
                  $scope.OnlineLocationName = _.find($scope.Locations, function(entry) { return entry.ID == 21; }).Name;

                  $scope.changeLocationClick = function() {
                     $scope.CampusType = null;
                     $scope.Rif.LocationID = null;
                     $scope.ProgramOfferingEmptyOptionText = "First select a Campus or Online";
                  }

                  $scope.$watch(
                     'CampusType', 
                     function(newVal, oldVal) {
                        if (newVal == 1) {
                           $scope.Rif.LocationID = 21;
                        }
                     });

                  var firstDigest = { LocationID: true, ProgramOfferingID: true, Email: true };
                  $scope.$watch(
                     'Rif.LocationID', 
                     function(newVal, oldVal) {
                        if (firstDigest.LocationID) {
                           firstDigest.LocationID = false;
                           return;
                        }

                        if (newVal != null) { localStorage.setItem("MiniRif_LocationID", JSON.stringify(newVal)); }
                        else { localStorage.removeItem("MiniRif_LocationID"); }
                     });

                  $scope.$watch(
                     'Rif.ProgramOfferingID', 
                     function(newVal, oldVal) {
                        if (firstDigest.ProgramOfferingID) {
                           firstDigest.ProgramOfferingID = false;
                           return;
                        }

                        if (newVal != null) { localStorage.setItem("MiniRif_ProgramOfferingID", JSON.stringify(newVal)); }
                        else { localStorage.removeItem("MiniRif_ProgramOfferingID"); }
                     });

                  $scope.$watch(
                     'Rif.Email',
                     function(newVal, oldVal) {
                        if (firstDigest.Email) {
                           firstDigest.Email = false;
                           return;
                        }

                        if (newVal != null) { localStorage.setItem("MiniRif_Email", newVal); }
                        else { localStorage.removeItem("MiniRif_Email"); }
                     });

                  $scope.GoToMainForm = function(form) {
                     if ($scope.SubmitPending || form.$invalid) { return; }

                     $scope.SubmitPending = true;

                     var validateEmail = function(email) {
                        var re = /^[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+(?:[A-Z]{2}|com|org|net|edu|gov|mil|biz|info|mobi|name|aero|jobs|museum)\b$/i;
                        return re.test(email);
                     }

                     if ($scope.Rif.Email != null && $scope.Rif.Email.length > 0 && !validateEmail($scope.Rif.Email)) {
                        if (!confirm("The email address that you provided (" + $scope.Rif.Email + ") appears to be incomplete. Do you wish to continue anyway?")) {
                           $scope.SubmitPending = false;
                           return;
                        }
                     }

                     var href = ($scope.Rif.LocationID != null
                        ? '/' + _.find($scope.Locations, function(entry) { return entry.ID == $scope.Rif.LocationID; }).LogicalPath + '/request-information'
                        : '/request-information');

                     if ($scope.Rif.ProgramOfferingID != null) {
                        href += '?program=' + $scope.Rif.ProgramOfferingID;
                     }

                     if ($scope.Rif.Email != null && $scope.Rif.Email.length > 0) {
                        document.cookie = "minirif-email=" + $scope.Rif.Email + "; path=/; expires=expires=Sat, 24 Mar 2018 06:08:39 UTC";
                     }
                     else {
                        document.cookie = "minirif-email=; path=/; expires=expires=Sat, 10 Mar 2018 07:08:39 UTC";
                     }

                     window.location.href = href;
                  };
               },
               PostPrep: function($scope) {
                  var cached = localStorage.getItem("MiniRif_Email");
                  if (cached != null) { $scope.Rif.Email = cached; }

                  cached = localStorage.getItem("MiniRif_LocationID");
                  if (cached == null) { return; }

                  var locationID = JSON.parse(cached);
                  $scope.Rif.LocationID = locationID;
                  $scope.CampusType = (locationID == 21 ? 1 : 2);

                  var setOfferingFromCache = function() {
                     cached = localStorage.getItem("MiniRif_ProgramOfferingID");
                     if (cached != null) { $scope.Rif.ProgramOfferingID = JSON.parse(cached); }
                  };

                  if (locationID == $scope.InitialLocationID) {
                     setOfferingFromCache();
                  }
                  else {
                     var cancelWatch = $scope.$watch(
                        'Rif.ProgramOfferings',
                        function(newVal, oldVal) {
                           if (newVal != null) { cancelWatch(); }

                           setOfferingFromCache();
                        });
                  }
               }
            }
         };

         MakeRifApp("MiniRifApp", "MiniRifController", PrepScope);
         setTimeout(function() {
            var rfiform = document.getElementById('mini-rif-form-container');
            var appScope = angular.element(rfiform).scope();
            if (appScope == null) {
               angular.bootstrap(rfiform, ['MiniRifApp']);
            }
         }, 400);
      }());
   </script>

