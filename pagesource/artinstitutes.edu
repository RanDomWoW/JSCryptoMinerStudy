   <section class="mini-rif-form" id="mini-rif-form-container"
      ng-app="MiniRifApp"
      ng-controller="MiniRifController">
      <form name="MiniRifForm" novalidate
         ng-submit="GoToMainForm()">
         <h2 class="mini-rif-form__title heading-center">
            Request a Brochure <span class="mini-rif-form__toggle-btn"></span>
         </h2>
         <div class="column-wrapper">
            <div class="mini-rif-form__location">
               <label for="MiniRif_LocationOfInterest">
                  Location of Interest
               </label>

               <div class="mini-rif-form__radio-buttons"
                  ng-show="CampusType == null">
                  <input type="radio" id="miniRifSelectOnline" name="miniRifSelectGroundOnline" class="mini-rif-form__ground-online" value="1"
                     ng-model="CampusType" />
                  <label for="miniRifSelectOnline">Learning Online</label>
               </div>

               <div class="mini-rif-form__radio-buttons"
                  ng-show="CampusType == null">
                  <input type="radio" id="miniRifSelectGround" name="miniRifSelectGroundOnline" class="mini-rif-form__ground-online" value="2"
                     ng-model="CampusType" />
                  <label for="miniRifSelectGround">Learning on a Campus</label>
               </div>

               <select class="required mini-rif-form__select" id="mini-rif-locations" name="MiniRif_LocationOfInterest" aria_invalid="false"
                  ng-cloak
                  ng-class="{ 'hide': CampusType != 2, 'show-slidefromright': CampusType == 2 }"
					   ng-options="location.ID as location.Name group by location.State for location in Locations"
					   ng-model="Rif.LocationID">
                  <option value="" disabled>- Select a Campus -</option>
               </select>

               <div class="online-location-name"
                  ng-cloak
                  ng-class="{ 'hide': CampusType != 1, 'show-slidefromright': CampusType == 1 }">
                  {{ OnlineLocationName }}
               </div>

               <a 
                  ng-cloak
                  ng-if="CampusType != null"
                  ng-click="changeLocationClick()">
                  Change Location
               </a>

               <label for="MiniRif_ProgramOfInterest">Program Of Interest</label>

					<select class="required mini-rif-form__select" name="MiniRif_ProgramOfInterest" aria-labelledby="Program of Interest"
						ng-options="offering.ProgramOfferingID as offering.ProgramName + ' (' + offering.OutcomeCode + ')' group by offering.OutcomeName for offering in Offerings"
						ng-model="Rif.ProgramOfferingID">
						<option value="" disabled>{{ ProgramOfferingEmptyOptionText }}</option>
					</select>

               <div class="mini-rif-form__email">
                  <input type="email" class="mini-rif-form__input" id="mini-rif-email" placeholder="Email Address"
                     ng-model="Rif.Email"
                     ng-model-options="{ updateOn: 'blur' }"
                     ng-maxlength="100" />

                  <a class="mini-rif-form__btn" id="mini-rif-submit"
                     ng-click="GoToMainForm(MiniRifForm)">
                     <span>NEXT</span>
                  </a>
               </div>
            </div>
      </form>
   </section>
