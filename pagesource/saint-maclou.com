<script type="text/javascript">
    jQuery(document).ready(function() {
        /* SELECT PROJET */
        jQuery('.popin-fullscreen-devis #estimate-project-detail-block').hide();

        /* BTN RDV */
        jQuery('#contact-address').hide();
        jQuery('#contact-address-complement').hide();
        jQuery('#contact-postal-code').hide();
        jQuery('#contact-city').hide();

        jQuery('.popin-fullscreen-devis #estimate-contact-type-list li').click(function () {
            var btnPopin = jQuery(this).attr('data-display-form');

            if (btnPopin==='phone' || btnPopin==='shop')
            {
                /* Initialisation */
                jQuery('.popin-fullscreen-devis #estimate-contact-type-list li').removeClass('active');
                jQuery('.icon-house-orange').hide();
                jQuery('.icon-house').show();
                jQuery('.icon-phone-orange').hide();
                jQuery('.icon-phone').show();
                jQuery('.icon-shop-orange').hide();
                jQuery('.icon-shop').show();
                /* Fin Initialisation */

                jQuery(this).addClass('active');
                jQuery(this).removeClass('smc-form-error');
                jQuery('.icon-'+btnPopin).hide();
                jQuery('.icon-'+btnPopin+'-orange').show();

                jQuery('#contact-address').hide();
                jQuery('#contact-address-complement').hide();
                jQuery('#contact-postal-code').hide();
                jQuery('#contact-city').hide();

                jQuery('#contact-address').prop('required', false);
                jQuery('#contact-postal-code').prop('required', false);
                jQuery('#contact-city').prop('required', false);

                jQuery("input[name=contact_mode]").val(btnPopin);
            }
            else if (btnPopin==='house')
            {
                /* Initialisation */
                jQuery('.popin-fullscreen-devis #estimate-contact-type-list li').removeClass('active');
                jQuery('.icon-phone-orange').hide();
                jQuery('.icon-phone').show();
                jQuery('.icon-shop-orange').hide();
                jQuery('.icon-shop').show();
                /* Fin Initialisation */

                jQuery(this).addClass('active');
                jQuery(this).removeClass('smc-form-error');
                jQuery('.icon-'+btnPopin).hide();
                jQuery('.icon-'+btnPopin+'-orange').show();

                jQuery('#contact-address').show();
                jQuery('#contact-address-complement').show();
                jQuery('#contact-postal-code').show();
                jQuery('#contact-city').show();

                jQuery('#contact-address').prop('required', true);
                jQuery('#contact-postal-code').prop('required', true);
                jQuery('#contact-city').prop('required', true);

                jQuery("input[name=contact_mode]").val('home');
            }
        });

        jQuery('.popin-fullscreen-devis #estimate-project-detail-block').hide();
        jQuery('.popin-fullscreen-devis #estimate-project-type-select').change(function () {
            if(jQuery(this).val()==='floor') {
                jQuery('.popin-fullscreen-devis #estimate-project-detail-block').show();
            }
            else {
                jQuery('.popin-fullscreen-devis #estimate-project-detail-block').hide();
            }
        });
    });
</script>
