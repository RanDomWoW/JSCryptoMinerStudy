        msgBox = (msgBox == 'undefined' || msgBox == null || msgBox == "") ? 'แถวที่เลือกมีการใช้งานอยู่   จึงไม่สามารถลบได้' : msgBox;
        CaseBox(msgBox, "ierror", IsNotAlert, time);
    }

