				strUrl += $("#idproperty").val() + "/";
			if ($("#keyword").val().length > 0)
				strUrl += "keyword=" + clean_up_url($("#keyword").val()) + '/';
				
			if ( $('#hid_page_nr').val() != '' && $('#hid_page_nr').val() != undefined){
					strUrl += 'p=' + $('#hid_page_nr').val() + '/';
			}
			location.href = strUrl;
			
			$('#faq_search').attr('action',strUrl);
			$('#faq_search').submit();
			return false;	
			
