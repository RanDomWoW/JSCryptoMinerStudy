    function getUserData(res){

        getUserInfo(function(res) {
          //  onGetUserData(res);
            updateProfile(res);
        })
    }

    function updateProfile(res)
    {
        $(".mobilelogin").hide();
        $('.iflogin').hide();
        $("#profile-pic").attr("src", res.profilepic);
        $(".fone-menu").children("img").attr("src", res.profilepic);
        var tmp_point = '('+res.points+' RUNS)';
        $(".points").html(tmp_point);
        $(".firstname").html(res.firstname);



        if(typeof onUpdateProfile !== 'undefined') {
            onUpdateProfile(res);
        }

    }

    function onBotRender() {
            hideFcyBot();
