        function httpRequest(url, data, app_id, method, callBack) {

            jQuery.ajax({
                url: url,
                type: method,
                data: data,
                cache: false,
                dataType: 'jsonp',
                callback: 'callback',
                headers: {'app_id': app_id},
                success: function (data, textStatus, jqXHR) {

                    callBack(true, data);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    callBack(false, textStatus);
                }


            });

        }

