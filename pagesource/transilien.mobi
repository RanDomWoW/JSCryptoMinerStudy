				$("#alertEmail").show();
				$("#addFav").show();
			}
			
			
			//pour coller le header en haut de la page quand la bannière de pub est visible
			var yPosition; // save original y position of the header here

			window.onload = function(){ // once entire page is loaded this function is fired
			    // save original y position of header before it is scrolled
			    yPosition = document.getElementById("header").offsetTop;
			}

			window.onscroll = function(){ // scrolling fires this function      

			    var header = document.getElementById("header"); // for cleaner code

			    // compare original y position of header to y position of page			    
			    if( yPosition <= window.pageYOffset*2 ){ 

			        // snap header to the top by changing css values
			        header.style.position = "fixed";
			        header.style.top = "0px"; 

			    }else{          

			        // re-position to original flow of content
			        header.style.position = "fixed";
			        header.style.top = ""; // set to default         
			    }               
			} 
			//****************************************************************************
			
		});
		
		// open / close the navigation drawer menu
		function openCloseNav() {
			var width = document.getElementById("mySidenav").offsetWidth;
			
			$("mySidenav").css("margin-top", "71px");
			
			if(width == 0){
				document.getElementById("drawerIcon").src="/assets/images/Navigation/burgermenu_close.png";
				document.getElementById("mySidenav").style.display = "block";
			    document.getElementById("mySidenav").style.width = "100%";
			    //$("body").css("overflow","hidden");
			    
			 	// Disable scrolling on iOS browser.
			    document.ontouchmove = function(event){
			        event.preventDefault();
			    }
			}else{
				document.getElementById("drawerIcon").src="/assets/images/Navigation/burgermenu.png";
			    document.getElementById("mySidenav").style.width = "0";
			    document.getElementById("mySidenav").style.display = "none";
			    $("body").css("overflow","auto");
			    
			 	// Enable scrolling on iOS browser.
			    document.ontouchmove = function (e) {
			      return true;
			    }
			}//if
		}
		
		// switch the contrast of all website
		function switchContrast() {
		
			if (typeof(Storage) !== "undefined") {
			    // Store
			    if(localStorage.getItem("theme") === "default"){  
			    	localStorage.setItem("theme", "contrasted");
			    }else {
			    	localStorage.setItem("theme", "default"); 
			    }
			} else {
			    console.log("Sorry, your browser does not support Web Storage...");
			}			
		}
		
		// set the stored theme if selected or let the default one 
		function setTheme() {
			if (typeof(Storage) !== "undefined") {
			    // Store
			    if(localStorage.getItem("theme") === null 
			    		|| localStorage.getItem("theme") === "undefined" 
			    		|| localStorage.getItem("theme") === "default"){  
			    	
			    	localStorage.setItem("theme", "default");
			    	// set text 
			    	document.getElementById("contrastLink").innerHTML= "Accentuer les contrastes";
			    	
			    }else if (localStorage.getItem("theme") === "contrasted") {
			    	// load the contrastedTheme.css and apply it
			    	var head  = document.getElementsByTagName("head")[0];
			        var link  = document.createElement("link");
			        link.id   = "contrastedTheme";
			        link.rel  = "stylesheet";
			        link.type = "text/css";
			        link.href = "/assets/stylesheets/contrastedTheme.css";
			        link.media = "all";
			        head.appendChild(link);
		    	
			    	localStorage.setItem("theme", "contrasted");
			    	// set text 
			    	document.getElementById("contrastLink").textContent= "Supprimer l’accentuation des contrastes";
			    }
		    
			} else {
			    console.log("Sorry, your browser does not support Web Storage...");
			}		
		}

		try {
