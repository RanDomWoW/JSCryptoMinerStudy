		$(document).ready(function () {
			//add the correct marginTop to the menu drawer, to place it right bellow the toolbar
			var marginTop = document.getElementById("header").clientHeight;
			var marginLeft = $("#container").offset().left;
			var elm = document.getElementById("mySidenav");
			if(elm !== null){			
				elm.style.marginTop = marginTop+"px";
				elm.style.marginLeft = marginLeft +"px";
			}
			
			setTheme();
			
			$("#navIcon").click(function(){
				if($("#cache_afficher").css("display") === "inline"){
					$("#cache_afficher").css("display",  "none");
					$("#cache_masquer").css("display",  "inline");
				}else{
					$("#cache_afficher").css("display",  "inline");
					$("#cache_masquer").css("display",  "none");
				}	
			});
			
			if(getCookie("mobiLang") === "en"  || getCookie("mobiLang") === "es"){
				$("#favorisNav").hide();
