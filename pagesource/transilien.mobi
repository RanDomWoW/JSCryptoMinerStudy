    function decodeHtml(html) {
        var txt = document.createElement("textarea");
        txt.innerHTML = html;
        return txt.value;
    }
	
	function generateFavItineraryHrefParams(departureType, departureLambertX, departureLambertY, departureLibelle, arrivalType, arrivalLambertX, arrivalLambertY, arrivalLibelle){
		var href = "";
		
		href += 'idOrigin='+departureType+'||||||'+departureLambertX+'|'+departureLambertY+'|!!;!!;!!;';
		href += '&nomOrigin=' + encodeURIComponent(decodeHtml(departureLibelle));
		href += '&idDest='+arrivalType+'||||||'+arrivalLambertX+'|'+arrivalLambertY+'|!!;!!;!!;';
		href += '&nomDest=' + encodeURIComponent(decodeHtml(arrivalLibelle));
		
		return href;
	}
	
