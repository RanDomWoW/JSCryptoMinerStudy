	function deleteFavorisItinerary(id, idElm, element) {
		favElmToDelete = element;
		var depart = $("#departureFavItinerary_"+idElm).html();
		var dest = $("#arrivalFavItinerary_"+idElm).html();
		
		localStorage.setItem("favItineraryToDelete", id);
		$('#favorites_modal').modal('show');
		
		$('#recapDepartFav').html(depart);
		$('#recapDestinationFav').html(dest);
		$('#deleteFavTitle').html('Etes-vous sûr de vouloir supprimer cet itinéraire favori ?');
		if(dest.length == 0){
			
			$("#recapDestinationFavPrefix").hide();
		}else{
			$("#recapDestinationFavPrefix").show();
		}
		$('#removeFavValidateBtn').attr('onclick', 'confirmDeleteFavorisItinerary();');
	}
	
	function confirmDeleteFavorisItinerary() {
		$.ajax({
	        url: "/deleteFavItinerary",
	        dataType: "json",
	        type: "post",
	        data: 'id=' + localStorage.getItem("favItineraryToDelete"),
	        success: function( data ) 		       	
	       	{
	        	$(favElmToDelete).parent().parent().remove();
	        	if($("#favorisContent").children().length == 0){
	        		location.reload();
	        	}
	       	},
	        error: function(data)
	        {
	        	console.log("error when sending deleteFavItinerary with ajax post");
	        	console.log("error : ", data.errors);
	        	console.log("message : ", data.message);
	        }
	    });
	}
	
