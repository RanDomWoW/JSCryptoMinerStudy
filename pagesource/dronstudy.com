	jQuery("#stickypop").css("bottom", pheight+"px");
	jQuery("#stickypop .panel-heading").click(function(){
		var bot = jQuery("#stickypop").attr("data-status");
		if(bot == 0){
			jQuery("#stickypop").animate({bottom:"-21px"});
			jQuery("#stickypop").attr("data-status","1");
		}else{
			jQuery("#stickypop").animate({bottom:pheight+"px"});						
			jQuery("#stickypop").attr("data-status","0");
		}
	});
});

</script>
