    <script>
    cityWay.journeyPlanner.getLocation = function (context) {

        document.getElementById('Keyword' + context).value = "";
        $('#Keyword' + context).addClass("loading");
        document.getElementById('Point' + context).value = "";

        cityWay.eventManager.publish(cityWay.events.geolocation.get, {});

        cityWay.eventManager.subscribe(cityWay.events.geolocation.found, function (position) {
            var latitude = position.coords.latitude;
            var longitude = position.coords.longitude;
            document.getElementById('Lat' + context).value = latitude;
            document.getElementById('Lng' + context).value = longitude;
            $('#Keyword' + context).removeClass("loading");

            // Chercher la voirie la plus proche
            cityWay.mapDataService.getNearestRoad(latitude, longitude)
                .then(function (result) {
                    if (result.Data) {
                        $('#Keyword' + context).val(result.Data.Name + " - " + result.Data.Locality.Name);
                    } else {
                            $("#errorNearMe" + context).html("Aucun lieu trouv&#233; proche de votre position");
                        }
                    });
            });

            cityWay.eventManager.subscribe(cityWay.events.geolocation.error, function (code) {
                var message = "Error";

                if (code == cityWay.GeolocationErrorCode.PermissionDenied) {
                    message = "Vous avez refus&#233; le partage de votre position";
            } else if (code == cityWay.GeolocationErrorCode.PositionUnavailable || code == cityWay.GeolocationErrorCode.Timeout) {
                message = "Votre position n&#39;a pas pu &#234;tre d&#233;termin&#233;e";
            } else if (code == cityWay.GeolocationErrorCode.NotSupported) {
                message = "Votre navigateur n&#39;est pas compatible avec cette fonctionnalit&#233;";
            }
            $('#Keyword' + context).removeClass("loading");
            $("#errorNearMe" + context).html(message);
        });
    };
</script>


