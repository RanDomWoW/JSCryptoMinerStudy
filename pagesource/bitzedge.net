//////app

.list-item {
    width: 300px;
    margin-bottom: 30px;
    clear: both;
}
  
.item-block {
    margin-top: 6px;
    width: 300px;
}
  
.app-ranking {
    height: 90px;
    width: 300px;
    margin-bottom: 6px;
}
  
.app-number {
    background-color: #ccc;
    color: #FFFFFF;
    float: left;
    font-size: 15px;
    font-weight: bold;
    height: 30px;
    text-align: center;
    width: 30px;
    border-radius: 20px;
    z-index: 99999;
    opacity: 0.9;
    position: relative;
    left: 10px;
    bottom: 10px;
}
  
.ranking {
    color: #333;
    display: block;
    margin-top: 5px;
}
  
.app-info {
    display: block;
    float: left;
    height: 60px;
    margin-top: 1px;
    text-align: left;
    width: 270px;
    z-index: -1;
}
  
.app-info .inner-app {
    float: left;
    height: 60px;
    margin-top: 3px;
    width: 260px;
}
  
.app-info img {
    border: 0px solid #CCCCCC;
    float: left;
    height: 75px;
    padding: 0px;
    width: 75px;
    display: block;
}
 
.app-price{
    font-size:10px;
    font-weight:bold;
}

#settings{
background-color: rgb(245,245,245);
padding: 10px;
}

/* 単一ページソーシャルボタン */
a.mt-share-inline-bar-sm img {
  width: 87%;
  height: auto;
  border: 0px;
}
a.mt-share-inline-bar-sm:hover {
  z-index: 50;
  -webkit-transform: scale3d(1.075, 1.075, 1.075);
}
a.mt-share-inline-bar-sm {
  display: inline-block;
  width: 50%;
  height: 45px;
  float:left;
  text-align: center;
  position: relative;
  transition: all 100ms ease-in;
  -webkit-transition: all 100ms ease-in;
  -webkit-transform: scale3d(1, 1, 1);
}
.mt-google:hover {
  background-color: rgb(225, 95, 79);
}
.mt-google {
  background-color: rgb(221, 75, 57);
}
.mt-linkedin:hover {
  background-color: rgb(16, 135, 192);
}
.mt-linkedin {
  background-color: rgb(14, 118, 168);
}
.mt-twitter:hover {
  background-color: rgb(34, 194, 255);
}
.mt-twitter {
  background-color: rgb(0, 172, 238);
}
.mt-facebook:hover {
  background-color: rgb(83, 116, 184);
}
.mt-facebook {
  background-color: rgb(59, 89, 152);
}
.mt-pinterest:hover {
  background-color: rgb(221, 42, 48);
}
.mt-pinterest {
  background-color: rgb(204, 33, 39);
}
.mt-pocket:hover {
  background-color: rgb(247, 90, 109);
}
.mt-pocket {
  background-color: rgb(238, 64, 85);
}
.mt-hatena:hover {
  background-color: rgb(55, 115, 180);
}
.mt-hatena {
  background-color: rgb(42, 101, 169);
}
.mt-feedly:hover {
background-color: rgb(156, 218, 55);
}
.mt-feedly {
background-color: rgb(133,192,36) ;
}
.bitzedgefeedly{
background-color: rgb(133,192,36);
margin-bottom:5px;
margin-top: 10px;
}
.bitzedgefeedly img{
border: none;
width: 100%;
margin: 0;
}

/* 単一ページナビ */
.kakokiji{
clear: both;
padding: 0 10px;
}

.tugikiji {
text-align:right;
padding: 0 10px;
}


/* エラーコンテンツ */
.error-content {
height: 100%;
width: 100%;
margin: 0;
top: 0;
position: absolute;
color: #f8f8f8;
text-align: center;
display: flex;
background: url(../wp-content/uploads/error/404.gif) center center no-repeat;
background-size: cover;
}

.bottomad {
background: rgb(245, 245, 245);
float:none;
}

/* 関連記事 */
.related {
margin-bottom: 15px;
}

.related dl{
font-size: 16px;
}

.related dt {
font-weight:normal;
font-size: 15px;
border-left-width: 15px;
border-left-style: solid;
border-left-color: rgba(204, 0, 24, 0.48);
line-height: 100%;
padding-left: 8px;
margin-top: 5px;
}

.list2 li{
margin: 6px 0px;
}

/* clearbothの設定 */
.clearboth{
clear:both;
}

/* グレーの背景 */
.glay-back{
margin: 6px 0;
padding: 15px;
background: rgb(243, 243, 243);
}

/* 項目のブロック（青と赤） */
.red-block {
margin-top:1px;
margin-right: 5px;
float: left;
background: rgba(204, 0, 0, 0.6);
width: 18px;
height: 18px;
-moz-border-radius: 30%;
-webkit-border-radius: 30%;
border-radius: 30%;
clear: both;
}

.blue-block {
margin-top:1px;
margin-right: 5px;
float: left;
background: rgba(0, 153, 204, 0.6);
width: 18px;
height: 18px;
-moz-border-radius: 30%;
-webkit-border-radius: 30%;
border-radius: 30%;
clear: both;
}

.sosicals {
margin-top: 25px;
}

.source {
margin-bottom: 10px;
padding: 0 5px;
font-size: 8px;
border: 1px solid #ccc;
float: right;
}

.source a {
color: #969696;
}

/* 埋め込み動画 */
.arve-wrapper{
margin-bottom:20px
}

/* tips記事用画像リサイズ */
.tipscontent img{
max-width:168px;
margin: 0 5px;
display: inline!important;
border: none!important;
}

/* テーマ記事用画像リサイズ */
.themecontent img{
max-width:168px!important;
margin: 0 5px;
display: inline!important;
border: none!important;
}

.themecontent_small img{
max-width:130px!important;
margin: 0 5px;
display: inline!important;
border: none!important;
}

/* 画像のキャプション */
.wp-caption {
background: #ECECEC;
}

.wp-caption img {
width: 98%;
}

/* リンク色（青） */
a.zip {
color: #0057A4;
font-weight: bold;
font-size: 14px;
}

/* コメント折り返し */
.comment-meta{
word-wrap: break-word;
}

#commentsAdd{
padding-bottom:0px;
margin-bottom:0px;
}


#twitter-fast {
padding: 10px 3%;
margin: 0 3%;
