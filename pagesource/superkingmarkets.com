<script>
  $(function() {
    
    // Changes buttons text and state
    function changeButtonState($btns) {
      var loc = $.cookie('weekly-ad-bookmark');
      if (loc) {
        $btns.each(function(_, btn) {
          var $btn = $(btn);
          if (loc.indexOf($btn.data('href')) !== -1 || location.href.indexOf(loc) !== -1) {
            $btn.attr('disabled', 'disabled');
            $btn.text('Your favorite store');
          } else {
            $btn.removeAttr('disabled');
            $btn.text('Bookmark');
          }
        });
      }
    }
    
    var $bookmarkBtns = $('.js-bookmark');
    
    // Set notification
    $bookmarkBtns.popover({
      content: 'bookmarked as your favorite store',
      placement: 'bottom'
    });
    
    // Set cookie, show notification and change button text
    $bookmarkBtns.on('click', function() {
      var $this = $(this);
      $.cookie('weekly-ad-bookmark', $this.data('href') || location.href, {
        path: '/',
        expires: 30
      });
      setTimeout(function() {
        $this.popover('hide');
      }, 3000);
      
      changeButtonState($bookmarkBtns); 
    });
    
    changeButtonState($bookmarkBtns);
    
    // Redirect to bookmarked page
    $('a[href="/pages/weekly-ad"]').on('click', function(ev) {
      var loc = $.cookie('weekly-ad-bookmark');
      if (loc) {
        ev.preventDefault();
        location.href = loc;
        return false;
      }
    });
    
    // Select viewmode
    var viewType = $('#columns').data('view-type');
    $('[data-viewmode="' + viewType + '"]').addClass('active');
  });
</script>

