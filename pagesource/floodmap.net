<script type="text/javascript" >
    var elev,resultElevation=0;
    var floodOptions = {
        getTileUrl: function(coord, zoom) {
        //document.getElementById("elev").value=polyCount;  
            return "getTile.aspx?" +
            //return "http://174.36.227.69/getTile.aspx?" +
    "zoom=" + zoom + "&x=" + coord.x + "&y=" + coord.y + "&elev=" + elev + "&poly=" + polyCount;
        },
        tileSize: new google.maps.Size(256, 256),
        opacity: 0.5,
        isPng: true
    };

    var floodMapType = new google.maps.ImageMapType(floodOptions);
    var locationid,initZoom,center;
    var map, marker, infowindow, latlng, elevator;
    function initialize() {
        geocoder = new google.maps.Geocoder();
        var lat,lng;
        
        var myOptions = {
            mapTypeId: google.maps.MapTypeId.ROADMAP,
            disableDoubleClickZoom:true
        }
        
		if (navigator.geolocation) {
        		navigator.geolocation.getCurrentPosition(
			function(position) {
    			lat=position.coords.latitude;
            		lng=position.coords.longitude;
            		initZoom=12;
            		latlng = new google.maps.LatLng(lat,lng);
			drawMap();
  			},
			function(){
			lat=20;
            		lng=-10;
            		initZoom=3;
            		latlng = new google.maps.LatLng(lat,lng);
			drawMap();});
    		} 
		else {
        		lat=20;
            		lng=-10;
            		initZoom=3;
            		latlng = new google.maps.LatLng(lat,lng);
			drawMap();
    		}
		
            
        
function drawMap()
{
        map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);

        elevator = new google.maps.ElevationService(); ;
        if(initZoom<0||initZoom>21)
            initZoom=12;
        map.setZoom(initZoom);
        map.setCenter(latlng);
        infowindow = new google.maps.InfoWindow({ maxWidth: 200 }); 
        marker = new google.maps.Marker({
            position: latlng,
            map: map
        });
        google.maps.event.addListener(marker, 'click', function() {
            codeLatLng(marker.getPosition());
        });
        elev = document.getElementById("elev").value;
        
        document.getElementById("curElev").innerHTML = elev;
        initPoly();
        google.maps.event.addListener(map, 'rightclick', function(event) {
            codeLatLng(event.latLng);
        }
        );
        google.maps.event.addListener(map, 'bounds_changed', function(event) {
            updateLink();
        }
        );
        
            codeLatLng(marker.getPosition(),true);
        
    }
}
    var elevFlag;
    function setElev(elevS, flag) {
        elevFlag = flag;
        if(isNaN(elevS))
        {
            alert("Please enter only numeric value!");
            return;
        }
        elevS=Math.round(elevS); 
        if (elevS < 0 || elevS > 800)
        {
            alert("Water level should be from 0 to 800 meters only!");
            return;
        }
        
        document.getElementById("elev").value = elevS;
        elev = elevS;
        updateLink();
        infowindow.close();
        hideCities();
        refresh();
    }

    function refresh() {
        map.overlayMapTypes.setAt(0, floodMapType);
        document.getElementById("curElev").innerHTML = elev; 
    }

    function codeAddress(address) {
        geocoder.geocode({ 'address': address }, function(results, status) {
        if (status == google.maps.GeocoderStatus.OK) {
            
                var latlng = results[0].geometry.location;
                map.setZoom(11);
                map.panTo(latlng);
                var name = document.getElementById("myinput").value;
                marker.setPosition(latlng);
                
                var locations = [];
                locations.push(latlng);
                var positionalRequest = {
                    'locations': locations
                }
                elevator.getElevationForLocations(positionalRequest, function(results, status) {
                    if (status == google.maps.ElevationStatus.OK) {
                        // Retrieve the first result

                        var elevContent = '';
                        if (results[0]) {
                            elevContent = getElevContent(results[0]);
                        }
                        var content;
                        content = "<h1>" + name + "</h1>" + "<p>" + elevContent + "</p>";

                        infowindow.setContent(content);
                        infowindow.open(map, marker);

                    }
                });
            } else {
                alert("Geocode was not successful for the following reason: " + status);
            }
        });
    }

    function codeLatLng(latlng,setEl) {
        geocoder.geocode({ 'latLng': latlng }, function(results, status) {
            if (status == google.maps.GeocoderStatus.OK) {
                //if (pan) {
                //    map.setZoom(11);
                //    map.panTo(latlng);
                //}

                var name;
                if (results[1])
                    name = results[1].formatted_address;
                else
                    name = "";
                marker.setPosition(latlng);

                var locations = [];
                locations.push(latlng);
                var positionalRequest = {
                    'locations': locations
                }
                elevator.getElevationForLocations(positionalRequest, function(results, status) {
                    if (status == google.maps.ElevationStatus.OK) {
                        // Retrieve the first result

                        var elevContent = '';
                        if (results[0]) {
                            elevContent = getElevContent(results[0]);
                        }
                        var content;
                        content = "<h1>" + name + "</h1>" + "<p>" + elevContent + "</p>";

                        infowindow.setContent(content);
                        infowindow.open(map, marker);

                        if (setEl) {
                            if (resultElevation > 5 && resultElevation <= 800)
                                setElev(resultElevation - 5);
                            else if (resultElevation > 800)
                                setElev(800);
                            else
                                setElev(0);
                        }
                    }
                });
            } else {
                alert("Geocode was not successful for the following reason: " + status);
            }
        });
    }
  </script>
 <script type="text/javascript">
