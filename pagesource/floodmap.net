    function getLocation(id) {
//        if (id != undefined) {
//            var query = '{"extended":true,"query":{  "id":   "' + id + '",  "name": null,  "/common/topic/article": [{    "text": {      "maxlength": 250,      "chars":     null    }  }],  "/location/location/geolocation": {    "latitude":  null,   "longitude": null  },"/location/location/containedby":[],  "key": {    "namespace": "/wikipedia/en_id",    "value":     null  }}}';
