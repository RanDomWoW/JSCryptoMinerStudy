            locate(null);
//        }  
	}
	function locate(result) {
	    locationid = "";
	    var lat, lng, containedby="", name="";
	    try {
	        name = result.result.name;
	        if (result.result['/location/location/containedby'][1])
	            containedby = result.result['/location/location/containedby'][1] + "," + result.result['/location/location/containedby'][0];
	        else if (result.result['/location/location/containedby'][0])
	            containedby = result.result['/location/location/containedby'][0];

	        lat = result.result['/location/location/geolocation'].latitude;
	        lng = result.result['/location/location/geolocation'].longitude;
	    
	    
	    latlng = new google.maps.LatLng(lat,lng);
	    

	    map.setZoom(11);
	    map.panTo(latlng);
	    
	    marker.setPosition(latlng);

	    var locations = [];
	    locations.push(latlng);
	    var positionalRequest = {
	        'locations': locations
	    }

	    elevator.getElevationForLocations(positionalRequest, function(results, status) {
	        if (status == google.maps.ElevationStatus.OK) {
	            // Retrieve the first result

	            var elevContent = '';
	            if (results[0]) {
	                elevContent=getElevContent(results[0]);
	            }
	            var content;
	            if (containedby == "")
	                content = "<h1>" + name + "</h1>";
	            else
	                content = "<h1>" + name + " (" + containedby + ")</h1>";

	            content = content
        + "<img style='padding-right:5px;float:left' src='http://api.freebase.com/api/trans/image_thumb" + result.result.id + "'/>"
	    + "<p>" + result.result['/common/topic/article'][0].text.chars + "<br/><br/>"
	    + "<a href='http://en.wikipedia.org/wiki/index.html?curid=" + result.result.key.value + "' target='_blank'>Wikipedia Article</a><br/>" + elevContent + "</p>";

	            infowindow.setContent(content);
	            infowindow.open(map, marker);

	        }
	    });
	    
	    
	    
	}
	catch (ex) {
	    if (name == "")
	        codeAddress(document.getElementById("myinput").value); 
	    else
	        codeAddress(name + "," + containedby); 
	}
}
function getElevContent(result) {
    var elevContent = "";
    resultElevation = Math.round(result.elevation);
    if (elev > result.elevation)
        elevContent = elevContent + "<span style='color:red'><b>Probable Water Level:" + Math.round(elev - result.elevation) + " meters.</b></span>";
    else if (result.elevation - elev <= 5)
        elevContent = elevContent + "<span style='color:orange'><b>Above Water Level:" + Math.round(result.elevation - elev) + " meters.</b></span>";
    else if (result.elevation - elev > 5)
        elevContent = elevContent + "<span style='color:green'><b>Above Water Level:" + Math.round(result.elevation - elev) + " meters.</b></span>";

    elevContent = elevContent + "<br><b>Elevation:" + Math.round(result.elevation) + " meters + <input id='addElev' type='text' value='0' style='width:20px'/></b><input type='button' onclick='setElev(" + Math.round(result.elevation) + " + parseInt(document.getElementById(\"addElev\").value),true)' value='Set Water Level'/><br/>";
    return elevContent;
}

function updateLink() {
    center = map.getCenter();
    document.getElementById("link").value = "http://www.floodmap.net/?ll=" + center.toUrlValue() + "&z=" + map.getZoom() + "&e=" + elev;
}

