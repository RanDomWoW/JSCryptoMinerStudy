<script type="text/javascript">
	$(document).ready(function() {
		$("#more").click(function() {
			$(".gununfirli").slideDown();
			$(".more").hide();
		});
		$(window).scroll(function () {
			scr=$(document).scrollTop();
			yuk=$(".in").height(); sip=$("#left_bar").height(); fark=yuk-sip;
			if(scr>220) $("#left_bar").css({"top": scr-220+"px"});
			else if(scr<220) $("#left_bar").css({"top": "0px"});
		});
		$(".list li").click(function() {
			$(".list li").removeClass("selected");
			$(this).addClass("selected");
			$('html,body').animate({scrollTop: $("#"+$(this).attr("source")).offset().top},'normal');
		});
		$(".onecikandetay").mousemove(function(e) {
			var instop = $(".in").offset().top;
			var inslft = $(".in").offset().left;
			var id = $(this).attr('id');
			var src = $(this).attr('data-src');
			$("#onecikan"+id).show();
			$("#onecikan"+id).css("left",e.pageX-inslft+20);
			$("#onecikan"+id).css("top",e.pageY-instop+20);
			$("#rsm"+id).attr("src",src);
		});
		$(".onecikandetay").mouseout(function() {
			var id = $(this).attr('id');
			$("#onecikan"+id).hide();
		});
	});
</script>
