//图片预加载
!function (obj){
    obj=obj || function(){};
    var imgList = [
      '/Public/Home/temp/banner1.jpg',
      '/Public/Home/temp/pic1.jpg'
   	];
    var imgSize = imgList.length, loadnum = 0;
    var pic = [];

    function imagesLoad(){
        loadnum++;
        var shu = parseInt(loadnum/imgSize*100);

        if(loadnum == pic.length){
               	$('.wrapBox').fullpage({
	               	verticalCentered: !1,
	               	navigation: !0,
	               	verticalCentered: false,
	                onLeave: function (index, nextIndex, direction){
	                	var $mlNav = $('.headerWrap');
	                	var $bossbody=$('.wrapBox');
	                  	if(index == 2 && direction == 'up'){
							$mlNav.removeClass('head-menu');
							$bossbody.css('margin-top',0);
						} else if(index == 1 && direction == 'down') {
							$mlNav.addClass('head-menu');
							$bossbody.css('margin-top',0);
						} else if(index == 3 && nextIndex ==1 ) {
							$mlNav.removeClass('head-menu');
							$bossbody.css('margin-top',0);
						} else if(index == 4 && nextIndex ==1 ) {
							$mlNav.removeClass('head-menu');
							$bossbody.css('margin-top',0);
						} else if(index == 5 && nextIndex ==1 ) {
							$mlNav.removeClass('head-menu');
							$bossbody.css('margin-top',0);
						} else if(index == 4 && direction == 'down' ) {
							$mlNav.addClass('head-menu');
							$bossbody.css('margin-top','36%');
						} else {
							$mlNav.addClass('head-menu');
							$bossbody.css('margin-top',0);
						}
	                }
               });
            obj();
        }
    }
    for (var i = 0; i < imgSize; i++) {
        pic[i] = new Image();
        pic[i].src = imgList[i];
        pic[i].onload = function(){
            imagesLoad();
        }
        pic[i].onerror = function(){
            imagesLoad();
        }
    }
}();
</script>
