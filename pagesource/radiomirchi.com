<script>
function user_login()
{
	var emailval = $("#loginemail").val();
	var passval = $("#loginpassword").val();
	
	if(emailval == '')
	{
		alert('Enter your email.');
		$("#loginemail").focus();
		return false;
	}
	
	if(passval == '')
	{
		alert('Enter password.');
		$("#loginpassword").focus();
		return false;
	}
	
	$.ajax({
	   type: "POST",
	   url: "/common-include/check-login.php",
	   data: "email="+emailval+"&password="+passval,
	   success: function(msg){
		 if(msg == '1')
		 {
		 	location.reload();
		 }
		 else
		 {
		 	alert(msg);
		 }
	   }
	 });
}
function password_recover()
{ 
	
	var emailval = $("#forgotemail").val();
	var captchaval = $("#mcaptcha").val();
	if(emailval == '')
	{
		alert('Enter your email.');
		$("#forgotemail").focus(); 
		return false;
	}
	
	if(captchaval == '')
	{
		alert('Enter your answer.');
		$("#ecaptcha").focus(); 
		return false;
	}
	
	$.ajax({
	   type: "POST",
	   url: "/globalfiles/forget-password.php",
	   data: "email="+emailval+"&captcha="+captchaval,
	   success: function(msg){
		   if((msg == 'Your Password has been Emailed to you.') )
		   {
			   alert(msg);
			   $("#forgotemail").val("");
			   $("#emailli").hide();
			   $("#captchali").hide();
		   }  
			else
			{
				alert(msg);
				
				var current_time = new Date();
	    		var normalPath = '/MathCaptcha/ecaptcha.php?w=50&h=24&t=c&c=000000&n=verify&time='+current_time;
	    		$("#ecaptcha").attr({ src: normalPath });
	    		
				$("#forgotemail").val("");
				$("#mcaptcha").val("");
				$("#forgotemail").focus();
				return false;
			}
	   }
	 });
	
}	
</script>
