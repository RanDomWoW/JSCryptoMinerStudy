<script>

$(document).ready(function(){
	
	var mySwiper = new Swiper('.swiper-container', {
	    keyboardControl: true,
	    loop: true,
	    speed: 800,
	    autoplay: 4000,
	    onSlideChangeStart: function( swiper ){

        var new_active = parseInt( $('.swiper-slide-active').attr('data-swiper-slide-index') );
        
        console.log( "new_active: " , new_active );
        $('.swipei').removeClass('bg_blue0b');
        $('.swipei_' + new_active).addClass('bg_blue0b');
        
        $('.swipei').find('.tri_txt').hide();
        $('.swipei_' + new_active).find('.tri_txt').show();
      },
        
      
	});
	
	
		$(".swipei").hoverIntent({
		interval: 50, //before - default is 100
		timeout: 0, //before - default is 0
		over:function(){	
			var ii = $(this).data('i');
			mySwiper.slideTo(ii);			
		},
		out: function () {
		}
	});
	
	$('.swipei').on({ 'click' : function(){ 
		var aurl = $(this).data('aurl');
		if(aurl != 'no'){
			window.location = aurl;
		}
	 } });
	 
	 	

});


</script>





