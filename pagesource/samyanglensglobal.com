<script>
    var popup = new Popup({
        animate:true,
        isResponse:true,
        closeBtn: { src: '/asset/images/common/pop_close.png' }
    });

    var rsFn = function($t){
        $('.icoo-popup .photo .img img').autoFit({mode:'inside'});
    };

    var vis_slide1_now = 0, vis_slide1 = $('#vis-slide-1').slidesjs({width: 320, height: 340,callback: {
        start: function(number) {
            var dir = number > vis_slide1_now ? 1 : -1;
            $('#vis-slide-1 p').css({'margin-left':dir*50,opacity:0}).delay(300).animate({opacity:1,'margin-left':0});
        }
    }}).data('plugin_slidesjs');
    var vis_slide2 = $('#vis-slide-2 li').length > 1 ? $('#vis-slide-2').slidesjs({ width: 320, height: 340 }).data('plugin_slidesjs') : {options:{},update:function(){}};
    var prod_slide = $('.prod .slides').slidesjs({width: 300, height: 240}).data('plugin_slidesjs');
    var now = true, t, fn;
    RS.add(fn = function($v){
        switch ($v){
            case 'm':
                t = now ? 0:'-100%';

                vis_slide1.options.width = vis_slide2.options.width = 320;
                vis_slide1.options.height = vis_slide2.options.height = 340;
                prod_slide.options.width = 300;
                prod_slide.options.height = 240;

                flow_ins.reflow(150,2);
                flow_is.reflow(150,2);
                break;
            case 't':
                t = now ? 200:-200;

                vis_slide1.options.width = vis_slide2.options.width = 790;
                vis_slide1.options.height = vis_slide2.options.height = 540;
                prod_slide.options.width = 600;
                prod_slide.options.height = 350;

                flow_ins.reflow(300,2,true);
                flow_is.reflow(200,3);
                break;
            case 'p':
                t = now ? 300:-300;

                vis_slide1.options.width = vis_slide2.options.width = 1570;
                vis_slide1.options.height = vis_slide2.options.height = 540;
                prod_slide.options.width = 550;
                prod_slide.options.height = 400;

                flow_ins.reflow(300,3,true);
                flow_is.reflow(200,3);
                break;
            case 'd':
                t = now ? 300:-300;

                vis_slide1.options.width = vis_slide2.options.width = 1570;
                vis_slide1.options.height = vis_slide2.options.height = 540;
                prod_slide.options.width = 700;
                prod_slide.options.height = 400;

                flow_ins.reflow(300,4,true);
                flow_is.reflow(200,3);
                break;
        }
        vis_slide1.update();
        vis_slide2.update();
        prod_slide.update();
        $('.visual .inner-wrap').animate({ 'left':t });
        $('.visual').height( $('#vis-slide-1').height());
    });


    $('.visual .btn-wrap a').click(function(){
        now = $(this).is('.btn-lt');
        fn(RS.state());
    });


    $(window).resize(function(){
        $('.visual').height( $('#vis-slide-1').height());
    });



    var flow_ins = new BoxFlow('ins-container',150,2,function(data){
        data.closeFn = function(){ RS.remove(rsFn) };
        var t0;
        switch (data.MEDIA_ST) {
            case 0:
                popup.open('photo-pop',data,function(){
                    RS.add(rsFn);
                });
                break;
            case 1:
                t0 = data.LINK.substring(data.LINK.lastIndexOf('/')+1);
                if (data.LINK.indexOf('vimeo') != -1) {
                    data.MOV = 'https://player.vimeo.com/video/'+t0;
                } else {
                    data.MOV = 'https://www.youtube.com/embed/'+t0;
                }
                popup.open('mov-pop',data);
                break;
            case 2:case 3: window.open(data.LINK);break;
        }
    });

    var flow_is = new BoxFlow('is-container',150,2,function(data){
