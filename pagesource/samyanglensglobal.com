    var footer_popup = new Popup({
        animate: true,
        isResponse: true,
        closeBtn: {src: '/asset/images/common/pop_close.png'}
    });

    $('footer .desc .util li').click(function () {
        footer_popup.open('util-pop-' + ($(this).index() + 1), function () {
            $(".utilPop .scroll-wrap").html(function () {
                return $(this).text().replace(/\n/g, "<br>");
            });
        });
    });

    var lang = ['ko', 'en', 'zh', 'ja'];
    $('footer .lang li').click(function () {
        var uri = location.pathname.replace("product/detail.do", "product/index.do");
        location.href = uri + "?lang=" + lang[$(this).index()];
    }).eq(lang.indexOf(locale)).addClass('on');

    function newsletterSend($f) {
        if( $($f).find('[type=checkbox]').length && !$($f).find('[type=checkbox]:checked').length) {
            alert("개인정보 수집 및 이용방침에 동의해주세요.");
            return;
        }
        if (!$c.valiEmail($f.EML.value)) {
            alert( locale == 'kr'?'이메일 주소를 확인해주세요':'Please check your email address');
            return;
        }

        ox.AX('/util/newsletter.ac', 'POST', {f: $f, LNG: (locale == 'ko' ? 'ko' : 'en')}, function ($r) {
            $r = JSON.parse($r);
            if ($r.success) {
                setCookie('newsletter-apply',1,365);
                Pop.open('newsletter-complete',{closeAll:true});
            } else if ($r.data == 'duplicate') {
                Pop.open('newsletter-duplicate');
            } else {
                alert($r.data);
            }
        });
    }
</script>


