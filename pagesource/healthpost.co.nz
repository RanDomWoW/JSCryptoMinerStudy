<script type="text/javascript">
    //<![CDATA[

    function sliCookies() {
        var sliNl = Mage.Cookies.get("sli_customer");
        var sliSubscribed = Mage.Cookies.get("sli_subscribed");

        if (sliNl && (sliSubscribed != 0)) {
            $$('.form-subscribe')[0].hide();
            $$('.newsletter-bst-btn')[0].hide();
            $$('.top-form-element')[0].addClassName('sli-hidden');
            $$('.page')[0].addClassName('sli-remove-border');
            $$('.header-panel')[0].addClassName('logged-out-header');
        }
    }

    function nlResponsive() {
        var width = document.viewport.getWidth();

        if ($$('.top-form-element').length > 0) {
            var stateNl = Mage.Cookies.get("StateNl") || "shown";
            if (width <= 950) {
                $$('.top-form-element')[0].addClassName('nl-mobile');
                $$('.top-form-element')[0].removeClassName('nl-desktop');
                $$('.top-form-element')[0].show();
                if (stateNl == "hidden") {
                    $$('.mobile-hidden')[0].hide();
                    $$('.form-subscribe')[0].show();
                    $$('.top-form-element')[0].show();
                }
            } else {
                $$('.top-form-element')[0].addClassName('nl-desktop');
                $$('.top-form-element')[0].removeClassName('nl-mobile');
                $$('.mobile-hidden')[0].show();
                if (stateNl == "hidden") {
                    $$('.form-subscribe')[0].hide();
                    $$('.page')[0].addClassName('page-form-hidden');
                }
                if (stateNl == "shown") {
                    $$('.form-subscribe')[0].show();
                    $$('.page')[0].removeClassName('page-form-hidden');
                }
            }
        }
    }

    function initialStates() {
        var width = document.viewport.getWidth();

        if ($$('.top-form-element').length > 0) {
            var stateNl = Mage.Cookies.get("StateNl") || "shown";
            var expCookies = new Date(new Date().getTime() + 45000000 * 10000);
            $$('.form-subscribe')[0].show();

            if (stateNl == "hidden") {
                $$('.top-form-element')[0].addClassName('bst-hide');
            }
            if (stateNl == "shown") {
                $$('.top-form-element')[0].removeClassName('bst-hide');
            }
                $$('.top-form-element')[0].addClassName('nl-desktop');
                $$('.top-form-element')[0].removeClassName('nl-mobile');
                $$('.mobile-hidden')[0].show();
                if (stateNl == "hidden") {
                    $$('.form-subscribe')[0].hide();
                    $$('.page')[0].addClassName('page-form-hidden');
                }
                if (stateNl == "shown") {
                    $$('.form-subscribe')[0].show();
                    $$('.page')[0].removeClassName('page-form-hidden');
                }
            // DESKTOP
            $$('.bst-newsletter-toggle, .bst-toggle-desk').each(function (el) {
                el.observe('click', function (event) {
                    Effect.toggle($$('.form-subscribe')[0], 'Blind', {duration: 0.5});
                    $$('.top-form-element')[0].toggleClassName('bst-hide');
                    $$('.page')[0].toggleClassName('page-form-hidden');
                    Mage.Cookies.set('StateNl', $$('.top-form-element')[0].hasClassName('bst-hide') ? 'hidden' : 'shown', expCookies);
                    event.stop();
                });
            });
            // MOBILE
            $$('.bst-toggle-mobile, .bst-label-toggle').each(function (el) {
                el.observe('click', function (event) {
                    Effect.toggle($$('.form-subscribe')[0], 'Blind', {duration: 0.5});
                    $$('.top-form-element')[0].toggleClassName('bst-hide');
                    $$('.page')[0].toggleClassName('page-form-hidden');
                    Mage.Cookies.set('StateNl', $$('.top-form-element')[0].hasClassName('bst-hide') ? 'hidden' : 'shown', expCookies);
                    event.stop();
                });
            });
        } else {
            $$('.page')[0].addClassName('page-form-hidden');
        }
    }

    document.observe("dom:loaded", function () {
        initialStates();
        sliCookies();
    });

//    Event.observe(window, "resize", nlResponsive, true);
    jQuery('#link_accountService').hover(function(){
