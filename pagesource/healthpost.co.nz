    },function(){
        jQuery('#link_accountService').removeClass('dropdown-activated');
        jQuery('#link_accountService').find('.switcher').removeClass('list-opened');
        jQuery('#menu_accountService').fadeOut(100);
    });
    jQuery('#link_customerService').hover(function(){
        jQuery(this).find('.switcher').addClass('list-opened');
        jQuery('#link_customerService').addClass('dropdown-activated');
