    'messages.lines.message.item': '<div class="alert alert-info"></div>' 
  });
  
  
  ritos.ready(function() {
    $('body').show();
    
    $('#sidebar-collapse-btn').click(function() {
  		$('#app').toggleClass('sidebar-open');
  		
  		return false;
  	});
  });
  
  ritos.on('loader.loading', function() {
  });
  
  ritos.on('loader.success', function(event) {
    $('body > .container-fluid').fadeIn(300);
    
    if ($('redirect', event.data).length === 0) {
      $(document.body).addClass('loaded');
      
      
      // lang
      
      $('.backoffice_to_client .lang_check, .backoffice_to_guest .lang_check').each(function() {
        var thiz = $(this);
        
        var select = thiz.find('> select');
        
        if (select.length > 0) {
          var options = select.find(' > option');
          
          if (options.length > 0) {
            var caption = options.filter(':selected');
            
            if (caption.length === 0)
              caption = options.first();
            
            thiz.append('<a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><img src="/data/images/global/flags/' + utils.filter(caption.attr('value')) + '.png" alt="" />' + utils.filter(caption.text()) + ' <span class="fa fa-angle-down"></span></a><ul class="dropdown-menu dropdown-usermenu pull-right"></ul>');
            
            options.each(function() {
              thiz.find('ul.dropdown-menu').append('<li><a href="' + utils.filter($(this).attr('href')) + '" class="dropdown-item"><img src="/data/images/global/flags/' + utils.filter($(this).attr('value')) + '.png" align="left" />' + utils.filter($(this).text()) + '</a></li>');
            });
          }
          
          select.remove();
        }
      });
      
      // sidebar
      
      $('div.menu_section ul.side-menu li > a').each(function() {
        var thiz = $(this);
        
        var href = thiz.attr('href');
        
        if (utils.startsWith(href, '#'))
          href = href.substring(1);
        
        if (utils.startsWith(event.uri, href) || utils.lower(href) === 'index/vdoc?doc=%2finformation%2ehtml' && utils.startsWith(utils.lower(event.uri), 'index/vdoc?doc=%2finformation%2f'))
          thiz.parent().addClass('active');
      });
      
      $('div.menu_section ul.side-menu > li > ul').each(function() {
        var thiz = $(this);
        var link = thiz.parent().children('a');
        
        if (link.length > 0) {
          thiz.addClass('collapse');
          
          if (link.children('span.fa-chevron-down').length === 0)
            link.append('<span class="fa fa-chevron-down"></span>');
          
          if (thiz.children('li.active').length > 0 || thiz.parent().is('li.active'))
            thiz.addClass('in').parent().addClass('open');
          
          link.click(function() {
            if (!thiz.is(':animated')) {
              var li = thiz.parent();
              
              if (li.is('.open')) {
                thiz.slideUp(200, function() { $(this).removeClass('in'); }).parent().removeClass('open');
              } else {
                li.parent().find('> li.open > ul.collapse.in').slideUp(200, function() { $(this).removeClass('in'); }).parent().removeClass('open');
                
                thiz.slideDown(200, function() { $(this).addClass('in'); }).parent().addClass('open');
              }
            }
            
            return false;
          });
        }
      });
      
      // content_menu
      
      $('ul.content_menu', this).each(function() {
        var ul = $(this).wrap('<div class="col-sm-4"></div>');
        var li = ul.children('li');
        var thiz = ul.parent();
        
        ul.remove();
        
        li.each(function() {
          var chli = $(this);
          
          var item = $('<div class="card card-info content-menu"></div>');
          
          thiz.append(item);
          
          
          var cha = chli.children('a');
          var chul = chli.children('ul');
          
          item.append(cha);
          item.append(chul);
          
          cha.wrap('<div class="card-header"><div class="header-block"><p class="title"></p></div></div>');
          chul.wrap('<div class="card-block"></div>');
        });
      });
      
      // news
      
      var menu_plate = $('.main-menu-plate', this);
      
      if (menu_plate.length > 0) {
        var content_table = menu_plate.find('> .content-wrap > table');
        
        var news_table = content_table.find('td.right_column > table');
        
        if (news_table.length > 0) {
          var news = '<ul class="list-group">';
          news_table.find('td .lines').each(function() { news += '<li class="list-group-item">' + $(this).html() + '</li>'; });
          news += '</ul>';
          
          var content = $('<div class="col-sm-9"></div>');
          
          content.append(news_table.parent().parent().find('> td > div'));
          
          content_table.after(content).remove();
          
          content.after('<div class="col-sm-3">' + news + '</div>');
        }
      }
      
      // tables
      
      var table = $('div.round-table').wrap('<div></div>').addClass('x_panel').wrap('<div class="table-responsive"></div>').children('table').addClass('table table-striped mt-table');
      
      var left = table.find('td.bottom div.left').addClass('col-xs-12 col-sm-4');
      var center = table.find('td.bottom div.center').addClass('col-xs-12 col-sm-4');
      var right = table.find('td.bottom div.right').addClass('col-xs-12 col-sm-4');
      
      left.find('input[type=button], input[type=submit]').addClass('btn btn-warning');
      center.find('input[type=button], input[type=submit]').addClass('btn btn-primary');
      right.find('input[type=button], input[type=submit]').addClass('btn btn-success');
      
      if (center.length > 0 && left.length < 1)
        center.addClass('col-sm-offset-4');
      
      if (right.length > 0) {
        if (left.length < 1 && center.length < 1) {
          right.addClass('col-sm-offset-8');
        } else if (center.length < 1) {
          right.addClass('col-sm-offset-4');
        }
      }
      
      var table_pages = table.find('td.pages').parent().remove().last().find('a, b, span');
      
      if (table_pages.length > 0) {
        var table_pages_content = '';
        table_pages.each(function() {
          var thiz = $(this);
          
          if (thiz.is('a')) {
            table_pages_content += '<a href="' + utils.filter(thiz.attr('href')) + '" class="btn btn-primary">' + utils.filter(thiz.text()) + '</a>';
          } else if (thiz.is('span')) {
            if (utils.trim(thiz.text()) !== '-')
              table_pages_content += '<a class="btn btn-primary">' + utils.filter(thiz.text()) + '</a>';
          } else {
            table_pages_content += '<a class="btn btn-primary active">' + utils.filter(thiz.text()) + '</a>';
          } 
        });
        table.parent().parent().after('<nav class="btn-group btn-group-sm">' + table_pages_content + '</nav><div class="clearfix"></div>');
      }
      
      // articles
      
      $('.office-plate:not(.main-menu-plate)').addClass('x_panel').children('.title-wrap').addClass('x_title').wrapInner('<h2></h2>').children('h2').after('<div class="clearfix"></div>').parent().parent().children('.content-wrap').addClass('x_content').wrapInner('<div style="padding: 8px;"></div>');
      
      // main menu
      
      $('.office-plate.main-menu-plate').after('<div class="clearfix"></div>');
    }
    
    
    $('.main-wrapper').scrollTop(0);
  });
  
  ritos.on('loader.error', function(event) {
    $('body > .container-fluid').fadeIn(300);
    
    $('.loader-content').wrapInner('<div class="row" style="margin-top: 40px;"><div class="col-md-4 col-md-offset-3 page-error"></div></div>').
        children('div').
        children('div').
        append('<br /><br /><a class="btn btn-primary" href="#" onClick="if (window.history.length > 0) { window.history.back(); return false; }"> <i class="fa fa-angle-left"></i> Back</a>');
    
    $('.main-wrapper').scrollTop(0);
  });
  
  
  global_home = '/';
  
  conf_datepicker_format = 'dd.mm.yy';
</script>

</head>
