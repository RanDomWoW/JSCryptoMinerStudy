        <script>
        function banner_pod_ingrediencemi() {
            var bannerFilename = "";
            if (($("#ingredients-list").length) && ($("#directions_wrapper").length)) {
                if (($("#ingredients-list .banner").css("display") != "none") && ($("#directions_wrapper .banner").css("display") != "none")) {
                    var ingredHeight = $("#recipe_detail_wrapper > .float-left-print").height();
                    var directHeight = $("#recipe_detail_wrapper > .float-right-print").height();
                    var bannerHeight = Math.abs(ingredHeight - directHeight);
                    var boxForAdWidth = 0;
                    var boxForBanner = "";

                    if (ingredHeight >= directHeight) {
                        boxForBanner = "directions_wrapper";
                        $('#ingredients-list .banner').remove();
                    } else {
                        boxForBanner = "ingredients-list";
                        $('#directions_wrapper .banner').remove();
                    }

                    if (($('#' + boxForBanner + ' .banner').length) > 0) {

                        boxForAdWidth = $('#' + boxForBanner).width();

                        if ((bannerHeight >= 280) && (boxForAdWidth >= 336)) {
                            bannerFilename = 'ads-square-2-336-280';
                        } else {
                            if (bannerHeight >= 100) {
                                if ((boxForAdWidth >= 250) && (boxForAdWidth < 300)) {
                                    bannerFilename = 'ads-square-2-250-250';
                                } else if ((bannerHeight < 280) && (boxForAdWidth >= 300)) {
                                    bannerFilename = 'ads-square-2-300-250';
                                }
                            }
                        }
                    }
                }
            }
            return bannerFilename;
        }

