<script>
var CATEGORIES_CONST = {"A7":"\uacf5\uc608","J7":"\uadf8\ub798\ud53d \ub514\uc790\uc778","B7":"\ub514\uc9c0\ud138 \uc544\ud2b8","C7":"\uc601\uc0c1\/\ubaa8\uc158\uadf8\ub798\ud53d","D7":"\ube0c\ub79c\ub529\/\ud3b8\uc9d1","E7":"\uc0b0\uc5c5 \ub514\uc790\uc778","F7":"UI\/UX","G7":"\uc77c\ub7ec\uc2a4\ud2b8\ub808\uc774\uc158","K7":"\ud0c0\uc774\ud3ec\uadf8\ub798\ud53c","H7":"\ud30c\uc778\uc544\ud2b8","I7":"\ud3ec\ud1a0\uadf8\ub798\ud53c"};
var IS_MOBILE = false;
var MOBILE_SEARCH = (function(){
	var isLoading, scrollTop = 0, init = function(){
		setTimeout(function(){
			$('select.mobile-select').css({display:'inline-block'});
			$('.bootstrap-select.mobile-select').css({display:'none'});	
		}, 100);
		$('#mobile_category').on('change', submit);
		$('#mobile_order').on('change', submit);
		$('#mobile_from').on('change', submit);
		$('#btnMobileSearch').on('click', showMobileSearchField);
		$('.btn-search-close').on('click', closeMobileSearchField);
		
		$('#mobile-search-modal input').on('keydown', keydown);
		
		var t0 = queryString(location.search.replace(/\?/g, ''));
		$('#mobile-search-modal input').val(t0.q ? decodeURIComponent(t0.q):'');
	},
	
	keydown = function(e){
		if(e.keyCode == 13){
			_search();	
		}
	},
	
	showMobileSearchField = function(){
		/*$('#mobile-search-modal .block').height('100%');*/
		$('#mobile-search-modal').css({display:'block'});
		$("body").bind('touchmove', function(e){e.preventDefault()});
		scrollTop = $("body").scrollTop();
		$("body").scrollTop(0);
	},
	closeMobileSearchField = function(){
		$('#mobile-search-modal').css({display:'none'});
		$("body").unbind('touchmove');
		$("body").scrollTop(scrollTop);
	},
	_search = function(){
		if(isLoading) return;
		
		if($('#mobile_order').val() == 'pick'){
			$('#mobile_from').attr('disabled', true);
			isLoading = true;
			$('#mobile_from').val('all');
		}else{
			isLoading = true;
			$('#mobile_from').attr('disabled', false);
			if($(this)[0] != $('#mobile_from')[0]){
				
				$('#mobile_from').val('all');
			}
		}
		queryStringSet('#mobile-search-modal input');
		return false;
	},
	
	submit = function(){
		if(isLoading) return;
		$('#mobile-search-field').blur();
		 
		if(this == $('#mobile_category')[0]){
			/*if($('#mobile_category').val()) $('#mobile_order>option[value="nofol_rank"]').attr('disabled', true);
			else $('#mobile_order>option[value="nofol_rank"]').attr('disabled', false);*/
			//$('#mobile_order').selectpicker('refresh');	
			
			//$('#mobile_order').val('newest');
			//$('#mobile_from').val('all');	
		}
		
		if($('#mobile_order').val() == 'pick'){
			$('#mobile_from').attr('disabled', true);
			isLoading = true;
			$('#mobile_from').val('all');
		}else{
			isLoading = true;
			$('#mobile_from').attr('disabled', false);
			if($(this)[0] != $('#mobile_from')[0]){
				
				$('#mobile_from').val('all');
			}
		}
		//$('#mobile_from').selectpicker('refresh');
		
		queryStringSet('#mobile-search-field');
		return false;
	},
	queryStringSet = function(searchField){
		History.pushState({isSearch:true, isDetail:false, isClose:false, isReplace:false}, null, 
			'?work_categories=' + $('#mobile_category').val() + '&order=' + $('#mobile_order').val() + '&from=' + $('#mobile_from').val()
			 + '&q=' + ($(searchField).val()?$(searchField).val():'')
		);	
	},
	search = function(){
		$('#loading-indicator2').css({display:'block'});
		$('#listing-block').fadeIn(100);
		$.get('/main/listing', queryString(location.search.replace(/\?/g, ''))).done(searchEnd);
	},
	searchEnd = function(v){
		viewUpdate();
		$('#work-container').html(v);
		$('#loading-indicator2').css({display:'none'});
		$('#listing-block').css({display:'none'});
		$("html, body").scrollTop(400);
		isLoading = false;
		$('#mobile-menu').trigger('close.mm');
	},
	viewUpdate = function(){
		isLoading = true;
		var t0 = queryString(location.search.replace(/\?/g, ''));
		$('#mobile_category').val(t0.work_categories);
		$('#mobile_from').val(t0.from);
		$('#mobile_order').val(t0.order);
		$('#mobile-search-modal input').val(t0.q ? decodeURIComponent(t0.q):'');
		closeMobileSearchField();
	},
	queryString = function(v){
		var t0 = v.split('&'), t1, t2, i, j;
		for(t1 = {}, i = 0, j = t0.length ; i < j ; i++){
			t2 = t0[i].split('=');
			t1[t2[0]] = t2[1];
		}
		return t1;
	};
	
	if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
		console.log(navigator.userAgent, ' / ', 'mobile!');
