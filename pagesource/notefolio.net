var FUNC = (function workInfoPopup(){
	var isOpened, init = function(){
		$('body').on('click', '.gallery-info-container', function(e){
			if(
				$(e.target).attr('id') == 'work-sidebar' ||
				$(e.target).attr('id') == 'popup-work-info' ||
				$(e.target).attr('id') == 'work-info-wrapper' ||
				$(e.target).attr('id') == 'sidebar' ||
				$(e.target).attr('id') == 'galleryInfoContainer'
				
			){
				/*if(site.isLoadingInfinit) return;*/
				$('#work-list>div').waypoint('destroy');
				state = getBeforeState();
				url = state.hash;
				History.pushState({isSearch:false, isDetail:false, isClose:true, isReplace:false}, null, url);
			}
		});
		/*$('body').on('click', '#work-list .block-image>a', function(){
			gumoFancy.open($(this).attr('href'));
			return false;
		});*/
	},
	getBeforeState = function(){
		var i, j, state;
		for(i = 1, j = History.getCurrentIndex(); i <= j ; i++){
			if(!(state = History.getStateByIndex(j - i)).data.isReplace && !state.data.isDetail) return state;
		}
		return null;
	},
	workInfoView = function(url){
		if(site.isLoadingInfinit) return;
		if(IS_MOBILE){
			location.href = url;	
		}else{
			$.get(url, {ajax:1}).done(workInfoGetEnd);
		}
	},
	workInfoGetRouter = function(e,target){
		e.preventDefault();
		if($(e.target).hasClass('go-profile-area') || 
			$(e.target).parent().hasClass('go-profile-area') || 
			$(e.target).parent().parent().hasClass('go-profile-area') || 
			$(e.target).parent().parent().parent().hasClass('go-profile-area')
		) return;
		if(site.isLoadingInfinit) return;
		var url = $(target).attr('data-link');
		if(IS_MOBILE){
			location.href = url;	
		}else{
			if(GLOBAL_KEY_CHECK.shift) window.open(url);
			else if(GLOBAL_KEY_CHECK.ctrl || GLOBAL_KEY_CHECK.cmd) window.open(url);
			else History.pushState({isSearch:false, isDetail:true, isClose:false, isReplace:false}, null, url);
			GLOBAL_KEY_CHECK.init();
		}
		
		return false;
	},
	workInfoGetEnd = function(response){
		$('#galleryInfoContainer').html(response).fadeIn(300, 'easeOutQuad');
		site.workPopupOpen = true;
		$('html').css({'overflow-y':'hidden'});
		setTimeout(function(){
			$('#galleryInfoContainer').scrollTop(0);	
		}, 100);
	},
	hide = function(e){
		$('#galleryInfoContainer').fadeOut(0, '', function(){
			site.workPopupOpen = false;
			$('#galleryInfoContainer').html('').scrollTop(0);
			$('html').css({'overflow-y':'scroll'});
		});	
		
	};
	init();
	
	return {
		workInfoRouter:workInfoGetRouter,
		workInfo:workInfoView,
		hide:hide	
	}
})();




