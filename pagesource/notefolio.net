<script>
(function(){
	var init = function(){
		$('#profile-menu').on({
			mouseenter : function(){
				$(document).off('click.profile');
			},
			mouseleave : function(){
				$(document).one('click.profile', function(){
					close();
				})
			}
		});
		$('#btn-profile-icon').on({
			click:function(){
				if($('#profile-menu').hasClass('open')) close();
				else{
					$('#profile-menu').removeClass('close');
					setTimeout(function(){
						$('#profile-menu').addClass('open');
					}, 20);
				}
			},
			mouseenter : function(){
				$(document).off('click.profile');
			},
			mouseleave : function(){
				$(document).one('click.profile', function(){
					close();
				})
			}
		})
	},
	close = function(){
		$('#profile-menu').addClass('close').removeClass('open');
	};
	init();
})();
</script>

