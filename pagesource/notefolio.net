var CATESORT = (function categorySort(){
	var noChange, isLoading, init = function(){
		$('.category-list-item').click(catClick);
		$('#order').on('change', submit);
		$('#from').on('change', submit);
	},
	catClick = function(e){
		var cat = $(this).attr('data-value');
		isLoading = true;
		$('#category').val(cat);
		/*if(cat) $('#order>option[value="nofol_rank"]').attr('disabled', true);
		else $('#order>option[value="nofol_rank"]').attr('disabled', false);*/
		$('#order').selectpicker('refresh');	
		//$('#order').selectpicker('val', 'newest');
		//$('#from').selectpicker('val', 'all');
		isLoading = false;
		submit();
	},
	submit = function(){
		//$(this).submit();
		if(isLoading) return;
		
		
		if($('#order').val() == 'pick'){
			$('#from').attr('disabled', true);
			isLoading = true;
			$('#from').selectpicker('val', 'all');
		}else{
			$('#from').attr('disabled', false);
			if($(this)[0] != $('#from')[0]){
				isLoading = true;
				//$('#from').selectpicker('val', 'all');
			}
		}
		$('#from').selectpicker('refresh');
		isLoading = false;
		queryStringSet();
	},
	queryStringSet = function(){
		var q = $('#q').val()?$('#q').val():'',
		cat = CATEGORIES_CONST[$('#category').val()],
		ord = $('#order').val(),
		title = q ? (q + ' 검색결과') : (ord ? '노트폴리오 - 크리에이티브 네트워크' : '갤러리 - ' + cat);
		History.pushState({isSearch:true, isDetail:false, isClose:false, isReplace:false}, title, 
			'?work_categories=' + $('#category').val() + '&order=' + ord + '&from=' + $('#from').val()
			 + '&q=' + q
		);	
	},
	search = function(){
		$('#loading-indicator2').css({display:'block'});
		$('#listing-block').fadeIn(100);
		isLoading = true;
		$.get('/main/listing', queryString(location.search.replace(/\?/g, ''))).done(searchEnd);
	},
	searchEnd = function(v){
		viewUpdate();
		$('#work-container').html(v);
		$('#loading-indicator2').css({display:'none'});
		$('#listing-block').css({display:'none'});
		$("html, body").scrollTop(438);
		isLoading = false;
	},
	_search = function(){
		if(IS_MOBILE) MOBILE_SEARCH.search();	
		else search();
	},
	viewUpdate = function(){
		var t0 = queryString(location.search.replace(/\?/g, ''));
		$('.category-list-item').removeClass('curr');
		$('.category-list-item[data-value="'+t0.work_categories+'"]').addClass('curr');
		$('#from').selectpicker('val', t0.from);
		$('#order').selectpicker('val', t0.order);
		$('#from').selectpicker('refresh');
		$('#order').selectpicker('refresh');
		$('#q').val(t0.q ? decodeURIComponent(t0.q) : '');
		//$('#q').tagsinput('add', t0.q ? decodeURIComponent(t0.q) : ''); 
		if(!t0.q) $('#q').val('');
		else $('#q').val(decodeURIComponent(t0.q)); 
		placeholder();
	},
	queryString = function(v){
		var t0 = v.split('&'), t1, t2, i, j;
		for(t1 = {}, i = 0, j = t0.length ; i < j ; i++){
			t2 = t0[i].split('=');
			t1[t2[0]] = t2[1];
		}
		return t1;
	},
	placeholder = function(){
		var text = $('#q').val();
		if(text != '') text = text.split(',');
		else text = [];
		if(text.length > 0) $('.bootstrap-tagsinput>input[type="text"]').attr('placeholder', '');
		else $('.bootstrap-tagsinput>input[type="text"]').attr('placeholder', '검색어를 입력해주세요');
	};
	init();
	
	return {
		placeholder:placeholder,
		search:_search,
		queryStringSet:queryStringSet,	
	}
})();

