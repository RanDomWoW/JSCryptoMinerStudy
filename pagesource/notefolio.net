(function newsSlide(){
	var curr = 0, width = 0, total, isTransition = false, interval, slideTime = 10000, 
	startX, endX,
	init = function(){
		total = $('#news .news-item').length,
		width = $('#news .news-item').width();
		console.log(width,'/',total,(width*total) + 'px');
		$('#news .news-slide').width((width*total) + 'px').on('touchstart', touchStart).on('touchend', touchEnd);
		$('#news .news-item').width(width);
		$('.news-slide-container .dot').click(dotClick);
		$(window).resize(resize);
		
		setSize();
		visibleBtn();
		//console.log(width,'/',total,(width*total) + 'px');
		if(!IS_MOBILE) start();
	},
	start = function(){
		interval = setInterval(autoSlide, slideTime);
	},
	autoSlide = function(){
		calcIndex('next');
		slide();
	},
	stop = function(){
		clearInterval(interval);
	},
	touchStart = function(e){
		startX = e.originalEvent.changedTouches[0].pageX;
	},
	touchEnd = function(e){
		var distX;
		endX = e.originalEvent.changedTouches[0].pageX;
		distX = endX - startX;	
		
		if(Math.abs(distX) < 40) return;
		
		if(distX < 0) calcIndex('next');
		else if(distX > 0) calcIndex('prev');
		slide();
	},
	btnClick = function(e){
		$('#news-navi>.dot').removeClass('curr');
		calcIndex($(this).hasClass('next') ? 'next' : ($(this).hasClass('prev') ? 'prev':''));
		$('#news-navi>.dot[data-index='+curr+']').addClass('curr');
		slide();
	},
	dotClick = function(e){
		var idx = $(this).attr('data-idx');
		
		curr = idx;
		slide();
		stop();
		start();
	},
	calcIndex = function(type){
		switch(type){
		case'next':
			if(curr < total - 1) curr++;
			else curr = 0;
			break;
		case'prev':
			if(curr > 0 ) curr--;
			else curr = total - 1;
			break;
		}
	},
	setTransition = function(){
		if(!isTransition){
			isTransition = true;	
		}
	},
	removeTransition = function(){
		if(isTransition){
			isTransition = false;	
		}
	},
	move = function(noAnimate){
		if(noAnimate) $('#news .news-slide').css({left:'-'+ (width*curr) +'px'});
		else $('#news .news-slide').animate({left:'-'+ (width*curr) +'px'}, 300, 'easeOutQuad');	
	},
	visibleBtn = function(){
		$('#news .slide-btn').removeClass('disabled');
		if(curr == 0) $('#news .slide-btn.prev').addClass('disabled');
		else if(curr == total - 1) $('#news .slide-btn.next').addClass('disabled');
	},
	slide = function(){
		setTransition();
		move();
		dot();
	},
	dot = function(){
		$('.news-slide-container .dot').removeClass('curr');
		$('.news-slide-container .dot.dot'+curr).addClass('curr');
	},
	setSize = function(){
		width = $('.news').width();
		
		$('.news .news-item').width(width);		
		$('#news .news-slide').width((width*$('#news .news-item').length) + 'px')
		
	},
	resize = function(){
		setSize();
		removeTransition();
		move(1);
	};
	init();
})();

