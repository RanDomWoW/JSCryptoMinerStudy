    <script>
        function ConfirmPay(strId) {
            $("#pay-account-id").val(strId);
            var inst = $('[data-remodal-id=modal-confirm-pay]').remodal();
            inst.open();
        }

        function AccountPay() {
            var strId = $("#pay-account-id").val();
            var strCouponCode = $("#txt-couponCode").val();
            $(".loading-main").show();

            $.ajax({
                url: '/Panel/PayPlan',
                type: 'POST',
                dataType: "json",
                data: {
                    planId: strId,
                    couponCode: strCouponCode,
                    __RequestVerificationToken: $('[name=__RequestVerificationToken]').val()
                },
                success: function (data) {
                    if (data.success === true) {
                        var inst = $('[data-remodal-id=modal-confirm-delete]').remodal();
                        inst.close();
                        $(".loading-main").hide();
                        showNotification('اتصال به درگاه', data.message, 'success', 'topLeft');
                        $("#lbl-paneActivityTime").html(data.userActivityTime);
                        window.location.href = "https://www.zarinpal.com/pg/StartPay/" + data.Authority;
                    }
                    else {
                        $(".loading-main").hide();
                        showNotification('اتصال به درگاه', data.message, 'danger', 'topLeft');
                    }
                },
                error: function (xhr, status, error) {
                    $(".loading-main").hide();
                    showNotification('اتصال به درگاه', 'خطای نامعلوم', 'danger', 'topLeft');
                }
            });
        }
    </script>


    <script>
        var controllerName = 'Home';
        var actionName = 'Index';
        $(document).ready(function ($) {
            if (window.location.href.indexOf("#modal-authorize") > 0) {
                var type = "signin";
                $("#modal-content").load("/Home/LoadAuthorizePartial?type=" + type, function (responseText, textStatus, XMLHttpRequest) {
                    if (textStatus === "success") {
                        $("#modal-authorize-title").text("ورود به حساب کاربری");
                        var imageurl = $("#captchaContainer").data('request-url');
                        var d = new Date();
                        $("#imgCaptcha").attr('src', imageurl + "?" + d.getTime());
                        $('form').removeData("validator");
                        $.validator.unobtrusive.parse('form');
                        window.location.href = "#modal-authorize";
                    }
                });
            }
        });

        $('[data-remodal-id]').remodal();

        $(".authorize-button").click(function (event) {
            event.preventDefault();
            var type = $(this).attr("data-type");
            $("#modal-content").load("/Home/LoadAuthorizePartial?type=" + type, function (responseText, textStatus, XMLHttpRequest) {
                if (textStatus === "success") {
                    switch (type) {
                        case "signin":
                            $("#modal-authorize-title").text("ورود به حساب کاربری");
                            break;
                        case "signup":
                            $("#modal-authorize-title").text("ساخت حساب کاربری");
                            break;
                        case "forget":
                            $("#modal-authorize-title").text("بازیابی رمز عبور");
                            break;
                    }
                    var imageurl = $("#captchaContainer").data('request-url');
                    var d = new Date();
                    $("#imgCaptcha").attr('src', imageurl + "?" + d.getTime());
                    $('form').removeData("validator");
                    $.validator.unobtrusive.parse('form');
                    window.location.href = "#modal-authorize";
                }
            });
        });

        $("#subscriberForm").submit(function (event) {
            event.preventDefault();
            $('#subscriberForm').removeData("validator");
            $.validator.unobtrusive.parse('#subscriberForm');
            if ($("#subscriberForm").valid()) {
                var email = $("#txtEmail").val();
                var phone = $("#txtPhone").val();
                if (email !== "" || phone !== "") {
                    $(".loading-main").show();
                    $.ajax({
                        url: '/Home/RegisterSubscriber',
                        type: 'POST',
                        dataType: "json",
                        data: {
                            __RequestVerificationToken: $('[name=__RequestVerificationToken]').val(),
                            email: email,
                            phone: phone
                        },
                        success: function (data) {
                            $(".loading-main").hide();
                            if (data.success === true) {
                                showNotification('خبرنامه', "عضویت در خبرنامه با موفقیت انجام شد.", 'success', 'topLeft');
                                $("#txtEmail").val('');
                                $("#txtPhone").val('');
                            }
                            else {
                                showNotification('خبرنامه', data.message, 'danger', 'topLeft');
                            }
                        },
                    });
                }
                else {
                    showNotification('خبرنامه', 'اطلاعات را وارد نمایید', 'danger', 'topLeft');
                }
            }
        });
    </script>
</body>
</html>