<script type="text/javascript">

    var currentSrc = "";

    $(document).ready(function () {       
        $(".featured-list__item--editable").on("mouseover", function(){
            var img = $(this).children('a').children('img');
            //console.log("onmouseover => gif: " + $(this).data("gif"));
            if($(this).data("gif") != ""){
                currentSrc = $(img).attr("src");
                $(img).attr("src", $(this).data("gif"));
            }
        }).on("mouseout", function(){
            var img = $(this).children('a').children('img');
            //console.log("mouseout => currentSrc: " + currentSrc);
            if(currentSrc != ""){
                $(img).attr("src", currentSrc);
                currentSrc = "";
            }
        });

        //Mix Panel Test
        mixpanel.track("Home");
    });
</script>
