	$(".hide-panal span").click(function()
	{
		clearInterval(rhsfixtab_AH);
		
		var tabSelected = $(this).attr("data-tab");
		$('.hide-panal span').removeClass('active');
		$(this).addClass('active');
		
		if(tabSelected == "rhs-fixtab")
		{
			$('.hide-panal span.toggle-btn').css("background-position", 'right');
		}
		else
		{
			$('.hide-panal span.toggle-btn').css("background-position", 'left');
		}
		
		if(prevSelected == "" && !tabStatus)
		{	// If the tab is closed and previous selected is null or empty
			// Will open the tab
			//console.log("PrevSelected == null and TabStatus = false");

			$('.hide-panal ul').css('display', 'none');
			$('#'+tabSelected).css('display', 'block');
			//-- Animation opening the tab
			$(".hide-panal").stop().animate({
				right: startPos+'px'
			}, {
				duration:200,
				complete:function(){
					tabStatus = true; // Set true to close the tab
					prevSelected = tabSelected;
				}
			});
		}
		else if(prevSelected != tabSelected && tabStatus)
		{	// If the tab is opened and previous selected is not equal to tab selected
			// Will change the tab content
			//console.log("PrevSelected != Selected Tab and TabStatus = True");
			
			$('.hide-panal ul').css('display', 'none');
			$('#'+tabSelected).css('display', 'block');
			prevSelected = tabSelected;
		}
		else if(prevSelected == tabSelected && tabStatus)
		{	// If the tab is opened and previous selected is equal to tab selected
			// Will close the tab
			//-- Animation closing the tab
			//console.log("PrevSelected == Selected Tab and TabStatus = True");
			
			$(".hide-panal").stop().animate({
				right: endPos+'px'
			}, {
				duration:200,
				complete:function(){
					tabStatus = false; // Set false to open the tab
					prevSelected = "";
					$('.hide-panal span').removeClass('active');
					$('.hide-panal ul').css('display', 'none');
					$('.hide-panal span.toggle-btn').css("background-position", 'left');
				}
			});
		}
	});
