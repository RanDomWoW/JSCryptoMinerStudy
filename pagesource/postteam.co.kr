<script type="text/javascript">
(function($) {
	var doc = '#header';
	$(doc + ' .header-agency-selected').click(function() {
		var target = $(this).parents('li:first');
		$('.agencyCode', target).hide();
		$('.' + this.value, target).show();
	});
	$(doc + ' .top_quick_menu ul li.list a.link').click(function() {
		$(this).css({width: $(this).width() + 'px', height: $(this).height() + 'px'});
		if ($(this).parents('li:first').is('.selected')) {
			$('.top_quick_layer', $(this).parents('li:first')).hide();
			$(this).parents('li:first').removeClass('selected');
			return false;
		}
		$('li.selected .top_quick_layer', $(this).parents('ul:first')).hide();
		$('li.selected', $(this).parents('ul:first')).removeClass('selected');
		$(this).parents('li:first').addClass('selected');
		$('.top_quick_layer', $(this).parents('li:first')).show().addClass('selected');
		return false;
	});
	$(doc + ' .event-click-tariff').click(function() {
		var target = $(this).parents('li:first');
		if ($('.tariff-date', target).text()=='') {
			$.getJSON('/tool/postteam.php', {"mode": "noti-currency-date"}, function(data) {
				if (data.result===true) {
					$('.tariff-date', target).text(data.date);
				}
			});
		}
	});
	$(doc + ' .event-click-tariff-submit').click(function() {
		var target = $(this).parents('tbody:first');
		var tariff = parseFloat($('.currency>option:selected', target).data('tariff'));
		var price = parseFloat($('.price', target).val());
		if (tariff<=0) {
			alert('고시환율 등록이 되지않아 계산할 수 없습니다.');
			return;
		}
		if (price<=0) {
			alert('금액을 입력하세요.');
			$('[name=price]', target).focus();
			return;
		}
		$('.tariff', target).text(number_format(price*tariff, 0));
	});
	$(doc + ' .event-change-convert').change(function() {
		var target = $(this).parents('tr:first');
		var type = $(this).parents('li:first').data('type');
		var index = $('>option:selected', this).index();
		$('[data-type="' + (type=='from'?'to':'from') + '"]>select>option:eq(' + index + ')', target).prop('selected', true);
		if ($(doc = ' .event-keyup-convert', target).val()) {
			$(doc = ' .event-keyup-convert', target).trigger('keyup');
		}
	});
	$(doc + ' .event-keyup-convert').keyup(function() {
		var target = $(this).parents('tr:first');
		var convert = parseFloat($('[data-type="from"]>select>option:selected', target).val());
		var value = parseFloat(this.value);
		$('[data-type="to"]>input', target).val(number_format(value*convert, 6));
	});
	$(doc + ' .event-click-sum').click(function() {
		if ($(this).data('isclick')=='1') return;
		$(this).data('isclick', '1');
		var This = $(this).parents('li:first');
		$.getJSON('/tool/agencyCategory.php', function(data) {
			if (data.list.length) {
				var html = '';
				html += '<option value="">선택하세요.</option>';
				$.each(data.list, function(key, value) {
					html += '<option value="' + value.no + '">' + value.title + '</option>';
				});
				$('select.category', This).html(html);
			}
		});
		$.getJSON('/tool/postteam.php', {"mode": "noti-currency"}, function(data) {
			$('[name="_header_agencyCode"]', This).each(function() {
				var currency = $(this).data('currency');
				$(this).data('noticurrency', data.money[currency]);
			});
		});
	});
	$(doc + ' .sum-field .category').change(function() {
		var category = this.options[this.selectedIndex].value;
		if (category=='') {
			$(this).next('select').remove();
			return;
		}
		var self = this;
		var html = '';
		$.getJSON('/tool/agencyCategory.php', {category: category}, function(data) {
			if (data.children.length) {
				html += '<option value="">선택하세요.</option>';
				$.each(data.children, function(key, value) {
					html += '<option value="' + value.no + '"' + (value.selected ? ' selected="selected"' : '') + ' data-tariff="' + value.tariff + '" data-tariff2="' + value.tariff2 + '" data-vat="' + value.vat + '">' + value.title + '</option>';
				});
				if (typeof($(self).next('select.category2')[0])=='undefined') {
					$(self).parents('td:first').append('<select class="category2"><option value="">선택하세요.</option></select>');
				}
				$(self).next('select.category2').html(html);
			}
		});
	});
	$(doc + ' [name="_header_agencyCode"]').click(function() {
		var target = $(this).parents('li:first');
		$('input.noti-currency', target).val($(this).data('noticurrency'));
		$('.currency-iso-code', target).text($(this).data('currency'));
		$('.weight-unit', target).text($(this).data('weightunit'));
		$('select.delivery', target).html('<option value="">' + $(this).data('deliverytype') + '</option>');
	});
	$(doc + ' .sum-field .sum-area .submit').click(function() {
		var target = $(this).parents('.sum-area:first');
		if (!$('[name="_header_agencyCode"]:checked', target).length) {
			alert('물류센터를 선택하세요.');
			$('[name="_header_agencyCode"]:first', target).focus();
			return;
		}
		if (parseFloat($('input.price', target).val() ? $('input.price', target).val() : 0)<=0) {
			alert('현지 총 지불금액을 입력하세요.');
			$('input.price', target).focus();
			return;
		}
		if (parseFloat($('input.weight', target).val() ? $('input.weight', target).val() : 0)<=0) {
			alert('무게를 입력하세요.');
			$('input.weight', target).focus();
			return;
		}
		var agencyCode = $('[name="_header_agencyCode"]:checked', target).val();
		var discount = $('select.member-level>option:selected', target).val();
		var itemPrice = parseFloat($('input.price', target).val());
		var weight = parseFloat($('input.weight', target).val());
		var tariff = 0, vat = 0;
		var noticurrency = parseFloat($('input.noti-currency', target).val());
		var isOption3 = ($('[name="_header_option3"]:checked', target).val()=='1' ? true : false);
		if (typeof($('select.category2', target)[0])!='undefined') {
			tariff = parseFloat($('select.category2>option:selected', target).data('tariff'));
			vat = parseFloat($('select.category2>option:selected', target).data('vat'));
		}
		var params = {
			"mode": "sum",
			"agencyCode": agencyCode,
			"discount": discount,
			"itemPrice": itemPrice,
			"weight": weight,
			"tariff": tariff,
			"vat": vat,
			"noticurrency": noticurrency,
			"isOption3": isOption3
		};
		var report = $(doc + ' .sum-report');
		$.getJSON('/tool/postteam.php', params, function(data) {
			$('.price', report).html(getCurrencyPrice(data.itemPrice, data.currency));
			$('.delivery-price', report).html(_i('무게(%s %s) : %s', data.weight, data.weightUnit, getCurrencyPrice(data.deliveryPrice, data.deliveryCurrency)));
			$('.option3-price', report).html(getCurrencyPrice(data.optionPrice3, data.currency));
			$('.tariff', report).html(getCurrencyPrice(data.getTariff, 'KRW'));
			$('.vat', report).html(getCurrencyPrice(data.getVat, 'KRW'));
			$('.noti-currency', report).html(data.noticurrency);
			$('.total-price', report).html(getCurrencyPrice(data.totalPrice, 'KRW'));
			$('.delivery-ship-price', report).html(getCurrencyPrice(data.deliveryShipPrice, 'KRW'));

			$(doc + ' .sum-field .sum-area').hide();
			$(doc + ' .sum-field .sum-report').show();
		});

		return false;
	});
	$(doc + ' .sum-field .sum-report .cancle').click(function() {
		$(doc + ' .sum-field .sum-area').show();
		$(doc + ' .sum-field .sum-report').hide();

		return false;
	});
})(jQuery);
</script>


