			var label = self.next ('label');
			html += label.text() + ', ';
		});

		$ ('#sel_' + box_name).html (html.slice (0, -2));

		NoChecks (box_name);
	}

	function syncStaticKeyword ()
	{
		var html = "";
		var self = null;

		$ ('#keywords_tr input[value!=""]').each (function () {
			self = $ (this);
			var span = $ ('span', self.closest ('.tag'));
			html += span.text() + ', ';
		});

		$ ('#sel_keywords').html (html.slice (0, -2));
	}

	function syncStaticCat ()
	{
		syncStatic ('categories');
	}

	function showSearchCategories ()
	{
		createCallbackOverlay ('syncStaticCat');
		openAlertPopup ($ ('#categories_tr'));
	}

	function syncStaticJobLanguages ()
	{
		syncStatic ('job_languages');

		if ($('input[name="job_languages[]"]:checked').length) {
			$('#is_cyrillic').attr('checked', false);
			$('#is_cyrillic_tr').hide();
		} else {
			$('#is_cyrillic_tr').show();
		}
	}

	function showSearchJobLanguages ()
	{
		createCallbackOverlay ('syncStaticJobLanguages');
		openAlertPopup ($ ('#job_languages_tr'));
	}

	function showSearchBoxWithSelected (box_name)
	{
		if ($ ('#' + box_name + '_tr').css ('display') == 'block') {
			$ ('#' + box_name + '_tr').hide ();
			$ ('#' + box_name + '_arr').removeClass ('arr_up').addClass ('arr_d');

			if (box_name == 'keywords') {
				syncStaticKeyword ();
			}
			else {
				syncStatic (box_name);
			}

			$ ('#sel_' + box_name).show ();
		}
		else {
			$ ('.search_section_div:visible').prev ('a').trigger ('click');

			$ ('#' + box_name + '_tr').show ();
			$ ('#' + box_name + '_arr').removeClass ('arr_d').addClass ('arr_up');
			$ ('#sel_' + box_name).hide ();
		}
	}

	function clearLocation () {
		if ( checkAddLocation() ) {
			return;
		}
		$('#location_sid').val(0);
		$('#clearLocation').hide();
		$('#location_cnt').empty();
		$('#location').val('');

		toggleResetLink();
	}

	function toggleClear (val) {
		$('.add_location').prop( 'checked' , false);

		if ( val == 'Всички' || val == '') {
			$('#clearLocation').hide();
		}
		else {
			$('#clearLocation').show ();
		}
	}

	$('#location')
		.autocomplete ({
			html: true,
			//autoFocus: true,
			source: function (request, response) {
				$.getJSON("https://www.jobs.bg/geolocation/autocomplete.php", { term: request.term }, response);
			},
			minLength: 0,
			open: function (event, ui) {
				$('#location_sid').val(0);
			},
			select: function (event, ui) {
				$('#location_sid').val(ui.item.id);
				$('#location_cnt').empty();

				if (ui.item.id) {
					$('#location_cnt').html('&nbsp;(1)');
				}
				else {
					$('#location').blur();
				}

				toggleClear (ui.item.value);

				toggleResetLink();
			}
		})
		.focus(function () {
			if ( checkAddLocation() ) {
				return;
			}
			if (this.value == 'Всички' || this.value == '') {
				this.value = '';
				$(this).autocomplete('search', '');
			}
		})
		.keyup(function () {
			toggleClear (this.value);
			toggleResetLink();
		})
		.blur (function () {
			if ( checkAddLocation() ) {
				return;
			}

			var options = $(this).data ("ui-autocomplete").menu.element [0].children,
			optionsLength = options.length,
			term = $('#location').val().toLowerCase();

			if (term) {
				for (var i = 1; i < optionsLength; i++) {
						var currentOption = $(options [i]).data ();

						if (currentOption.uiAutocompleteItem.value.toLowerCase() == term) {
							$('#location_sid').val(currentOption.uiAutocompleteItem.id);
							toggleClear (currentOption.uiAutocompleteItem.value);
							$('#location_cnt').empty().html('&nbsp;(1)');
							break;
						}
				}
			}

			if ($('#location_sid').val() == 0) {
				$('#location').val('Всички');
				$('#location_cnt').empty();
				toggleResetLink();
			}
		});

	$('#location').data ("ui-autocomplete")._renderItem = function (ul, item) {
		return $("<li></li>")
			.data("item.autocomplete", item)
			.append("<a>" + item.label + "</a>")
			.appendTo(ul);
	}

	function showSearchBox ( box_name ) {
		if ($( '#' + box_name +'_tr' ).css( 'display' ) == 'block') {
			$('#' + box_name + '_tr').hide();
			$('#' + box_name + '_arr').removeClass( 'arr_up' ).addClass( 'arr_d' );
		} else {
			$('#' + box_name + '_tr').show();
			$('#' + box_name + '_arr').removeClass( 'arr_d' ).addClass( 'arr_up' );
		}
	}

	function checkAddLocation () {
		if ( $('input[name="abroad"]').prop('checked') || $('input[name="distance_job"]').prop('checked') || $('input[name="seaside_resort"]').prop('checked') || $('input[name="mountain_resort"]').prop('checked')) {
			return true;
		} else {
			return false;
		}
	}

	function AddLocation (name) {
		$('#distance').prop( "disabled", true );
		$('#location').prop( "disabled", true );
		$('#location_cnt').empty();
		$('#location_tr').hide();
		$('#job_type_tr').show();
		var checked = false;
		if ($('input[name="' + name + '"]').prop( 'checked' )) {
				checked = true;
				$('.add_location').prop( 'checked' , false);
				$('input[name="' + name + '"]').prop( 'checked', true );
				$('#location_cnt').empty().html('&nbsp;(1)');
		} else {
			$('#distance').prop( "disabled", false );
			$('#location').prop( "disabled", false );
			$('.add_location').prop( 'checked' , false);
			$('#location_tr').show();
			if ($('#location').val() != 'Всички' || $('#location').val()) {
				$('#location_cnt').empty().html('&nbsp;(1)');
			}
		}

		if (name == 'distance_job' && checked) {
			$('#job_type_tr').hide();
			$('input[name="job_type[]"]').prop('checked', false);
			selType ();
		}
	}

	function NoChecks (name) {
		if ($ ('#' + name + '_tr input:checked').length == 0) {
			$ ('#' + name).prop ('checked', true);
		}
	}

	function AllGroup (name) {
		if ($('input[name="all_' + name + '"]').prop('checked')) {
			$('input[name="'+name+'[]"]').prop('checked', false);
			$('#'+name+'_cnt').empty();
		}
	}

	function AllGroupByClass (name) {
		if ($('input[name="all_' + name + '"]').prop('checked')) {
			$('input[name="'+name+'[]"]').prop('checked', false);
			$('.'+name+'_cnt').empty();
		}
	}

	function selGroup (name) {
		if ($('input[name="'+name+'[]"]:checked').length) {
			$('#'+name+'_cnt').empty().html('&nbsp;('+ $('input[name="'+name+'[]"]:checked').length + ')');
			$('input[name="all_'+name+'"]').prop('checked', false);
		} else {
			$('input[name="all_'+name+'"]').prop('checked', true);
			$('#'+name+'_cnt').empty();
		}
	}

	function selGroupByClass (name) {
		if ($('input[name="'+name+'[]"]:checked').length) {
			$('.'+name+'_cnt').empty().html('&nbsp;('+ $('input[name="'+name+'[]"]:checked').length + ')');
			$('input[name="all_'+name+'"]').prop('checked', false);
		} else {
			$('input[name="all_'+name+'"]').prop('checked', true);
			$('.'+name+'_cnt').empty();
		}
	}

	function selType () {
		cnt = 0;
		cnt += $('input[name="job_type[]"]:checked').length;
		if ($('input[name="is_student"]').prop('checked') ) {
		 cnt += 1;
		}
		if ($('input[name="is_entry_level"]').prop('checked') ) {
		 cnt += 1;
		}
		if ($('input[name="full_time"]').prop('checked') ) {
		 cnt += 1;
		}
		if ($('input[name="part_time"]').prop('checked') ) {
		 cnt += 1;
		}

		if (cnt) {
			$('#type_cnt').empty().html('&nbsp;('+cnt+')');
			$('input[name="all_type"]').prop('checked', false);
		} else {
			$('input[name="all_type"]').prop('checked', true);
			$('#type_cnt').empty();
		}
	}

	function AllType() {
		if ($('input[name="all_type"]').prop('checked')) {
			$('input[name="job_type[]"]').prop('checked', false);
			$('input[name="is_student"]').prop('checked', false);
			$('input[name="is_entry_level"]').prop('checked', false);
			$('input[name="full_time"]').prop('checked', false);
			$('input[name="part_time"]').prop('checked', false);
			$('#type_cnt').empty();
		}
	}

	function enableClearButton () {
		if ($ ('input[name="keyword"]').val() == '') {
			$ ('#clearserchformbtn').hide();
			$ ('#addKeywordLink').hide ();
		}
		else {
			$ ('#clearserchformbtn').show();
			$ ('#addKeywordLink').show ();
			$ ('#clearserchformbtn').removeClass ("hidden");
		}
	}

	function clearSearchForm () {
		$('input[name="keyword"]').val('').focus();
		enableClearButton();

		toggleResetLink();
	}

	function showSearchForm () {
		$('#search_div').show();
		$('#search_results_div').hide();
	}

	$ ("#keyword").autocomplete ({
		source: function (request, response) {
			$.getJSON ("keyword_autocomplete.php", { term: request.term }, response);
		},
		select: function (event, ui) {
			$ ('#keyword').val (ui.item.value);
			addKeyword ();
			return false;
		},
		minLength: 2,
	})
	.keyup(function () {
		toggleResetLink();
	});

	function addKeyword ()
	{
		var input = $ ('#keyword');
		var val = $.trim(input.val ());
		if (val != '') {
			var tag = $ ('<div class="tag"><span></span> &nbsp;&nbsp;<a href="javascript:;" alt="Изтрий" title="Изтрий" onclick="removeKeyword ($ (this))">X</a><input type="hidden" name="keywords[]" value="" /></div>');
			$ ('#keywordsContainer').append (tag);
			$ ('span', tag).text (val);
			$ ('input', tag).val (val);

			input.val ('');
			input.trigger ('keyup');
			//$ ('#clearserchformbtn').hide();

			$ ('#addKeywordLink').hide ();

			keywordCounter ();
			toggleResetLink();
		}
	}

	function removeKeyword (link)
	{
		link.closest ('.tag').remove();
		keywordCounter ();
		toggleResetLink();
	}

	function keywordCounter ()
	{
		var cnt = $ ('.tag').length;

		if (cnt) {
			$ ('#keywords_cnt').html ('&nbsp;(' + cnt + ')');
		}
		else {
			$ ('#keywords_cnt').html ('');
		}
	}

	function resetSearchForm () {
		//$('#search_frm')[0].reset();
		$('#location, #keyword').val('');
		$('#location_sid').val(0);
		$('#distance').val(0);

		$('input[name="categories[]"]:checked, input[name="job_type[]"]:checked, input[name="company_type[]"]:checked, input[name="position_level[]"]:checked, input[name="job_languages[]"]:checked').prop ('checked', false);
		$('input[name="full_time"]:checked, input[name="part_time"]:checked, input[name="is_student"]:checked, input[name="is_entry_level"]:checked, input[name="is_cyrillic"]:checked').prop ('checked', false);
		$('.add_location').prop ('checked', false);
		$('#all_categories').prop ('checked', true);
		$('#all_type').prop ('checked', true);
		$('#all_position_level').prop ('checked', true);
		$('#all_company_type').prop ('checked', true);


		$('#clearLocation').hide();
		$('#location, #distance').prop( "disabled", false );

		$('#location_tr').show();
		$('#job_type_tr').show();


		$('.cnt_box').empty();
		$('.search_section_div').hide();
		$('.arr_up').removeClass( 'arr_up' ).addClass( 'arr_d' );
		$('#sel_categories, #sel_type, #sel_company_type, #sel_position_level').empty().html('Всички').show();
		$('#sel_job_languages').empty();
		$('#is_cyrillic_tr').show();
		$('#keywordsContainer').html('');
		$('#sel_keywords').empty().hide();
		$('#addKeywordLink, #clearserchformbtn').hide();

		$('#enable_salary').prop ('checked', false);
		changeEnableSalary ();

		$('#resetSearchFormBtnSpan').hide();
	}

	function toggleResetLink () {
		if (
				$('.add_location[value!="0"]:checked').length ||
				($('#location').val() && $('#location').val() != 'Всички') || $('#location_sid').val() > 0 || $('#distance option:selected').val() > 0 || $('#keyword').val() || $('.tag').length ||
				$('input[name="categories[]"][value!="0"]:checked').length ||
				$('input[name="company_type[]"][value!="0"]:checked').length ||
				$('input[name="position_level[]"][value!="0"]:checked').length ||
				$('input[name="job_type[]"][value!="0"]:checked').length ||
				$('input[name="job_languages[]"][value!="0"]:checked').length ||
				$('input[name="salary_from"][value!="0"]:checked').length ||
				$('input[name="full_time"]:checked, input[name="part_time"]:checked, input[name="is_student"]:checked, input[name="is_entry_level"]:checked, input[name="is_cyrillic"]:checked').length
			) {
			$('#resetSearchFormBtnSpan').show();
		} else {
			$('#resetSearchFormBtnSpan').hide();
		}
	}

	$("#search_frm").change(function() {
		toggleResetLink();
	});

</script>
