	display:block;
	position:absolute;
	left:0;
	top:0;
	width:100%;
	height:490px;
	background:#000;
	filter:alpha(opacity=50);
	opacity:0.5;
}
.slider .pnBtn .arrow {
	display:none;
	position:absolute;
	top:80px;
	z-index:1;
	width:60px;
	height:490px;
}
.slider .pnBtn .arrow:hover {
	filter:alpha(opacity=60);
	opacity:0.6;
}
.slider .prev .arrow {
	right:0;
	background:url(http://img.wondanck.co.kr/home/slider/images/slider-arrow.png) -120px 0 no-repeat;
