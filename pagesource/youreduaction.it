.thenavbar a {
    color: #FF2D55;
    text-decoration: none
}

.thenavbar a:focus,
.thenavbar a:hover {
    color: #006fd7;
    text-decoration: underline
}

.thenavbar a:focus {
    outline: dotted thin;
    outline: -webkit-focus-ring-color auto 5px;
    outline-offset: -2px
}

.thenav>li,
.thenav>li>a {
    position: relative;
    display: block
}

.thenavbar-toggle {
    display: none
}

.button_container:hover {
    opacity: .7
}

.button_container.active .top {
    -webkit-transform: translateY(7px) translateX(0) rotate(45deg);
    transform: translateY(7px) translateX(0) rotate(45deg);
    background: #FFF
}

.button_container.active .middle {
    opacity: 0;
    background: #FFF
}

.button_container.active .bottom {
    -webkit-transform: translateY(-7px) translateX(0) rotate(-45deg);
    transform: translateY(-7px) translateX(0) rotate(-45deg);
    background: #FFF
}

.button_container span {
    background: #FF2D55;
    border: none;
    height: 2px;
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    -webkit-transition: all .35s ease;
    transition: all .35s ease;
    cursor: pointer
}

.button_container span:nth-of-type(2) {
    top: 7px
}

.button_container span:nth-of-type(3) {
    top: 14px
}

.theoverlay {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 1;
    visibility: hidden;
    -webkit-transition: opacity .35s, visibility .35s, width .35s;
    transition: opacity .35s, visibility .35s, width .35s;
    z-index: 50
}

.theoverlay:after,
.theoverlay:before {
    background: #72d5cd;
    top: 0;
    width: 50%;
    position: absolute;
    height: 100%;
    content: ''
}

.theoverlay:before {
    left: -55%;
    -webkit-transition: left .35s ease;
    transition: left .35s ease
}

.theoverlay:after {
    right: -55%;
    -webkit-transition: all .35s ease;
    transition: all .35s ease
}

.theoverlay.open {
    opacity: 1;
    visibility: visible;
    height: 100%
}

.theoverlay.open:before {
    left: 0
}

.theoverlay.open:after {
    right: 0
}

.theoverlay.open li {
    -webkit-animation: fadeInRight .5s ease forwards;
    animation: fadeInRight .5s ease forwards;
    -webkit-animation-delay: .35s;
    animation-delay: .35s
}

.theoverlay.open li:nth-of-type(2) {
    -webkit-animation-delay: .45s;
    animation-delay: .45s
}

.theoverlay.open li:nth-of-type(3) {
    -webkit-animation-delay: .55s;
    animation-delay: .55s
}

.theoverlay.open li:nth-of-type(4) {
    -webkit-animation-delay: .65s;
    animation-delay: .65s
}

.theoverlay nav {
    position: relative;
    height: 80%;
    top: 45%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    font-size: 40px;
    font-family: Oswald, sans-serif;
    font-weight: 400;
    text-align: center;
    z-index: 100
}

.theoverlay ul {
    list-style: none;
    padding: 0;
    margin: 0 auto;
    display: inline-block;
    position: relative;
    height: 100%
}

.theoverlay ul li,
.theoverlay ul li a {
    display: block;
    position: relative
}

.theoverlay ul li {
    opacity: 0;
    padding: 0 0 15px
}

.theoverlay ul li a {
    color: #FFF;
    text-decoration: none;
    overflow: hidden
}

.theoverlay ul li a:active:after,
.theoverlay ul li a:focus:after,
.theoverlay ul li a:hover:after {
    width: 100%
}

.overlay-menu input[type="email"],
.overlay-menu input[type="text"],
.overlay-menu input[type="tel"] {
    padding: 3px 29px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    border: none;
    width: 100%;
    max-width: 75%;
    margin-bottom: 4px;
}

.overlay-menu input[type="submit"] {
    padding: 5px 12px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    border: none;
    background: #66b0ab;
    width: 100%;
    max-width: 200px;
    color: white;
    cursor: pointer;
    margin: 0px;
}

.theoverlay ul li a:after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 50%;
    width: 0;
    -webkit-transform: translateX(-50%);
    transform: translateX(-50%);
    height: 3px;
    background: #FFF;
    -webkit-transition: .35s;
    transition: .35s
}

@-webkit-keyframes fadeInRight {
    0% {
        opacity: 0;
        left: 20%
    }
    100% {
        opacity: 1;
        left: 0
    }
}

.button_container {
    background-image: url('http://www.youreduaction.it/wp-content/uploads/2017/10/celebrationicon.png');
    background-size: auto 100%;
    background-repeat: no-repeat;
}

.button_container.active {
    background: none;
}

.button_container .top,
.button_container .middle,
.button_container .bottom {
    display: none;
}

.button_container.active .top,
.button_container.active .middle,
.button_container.active .bottom {
    display: block;
}

.thenav>li {
    float: right;
}

.buttoninside {
    background-color: #f1691d;
    border: none;
    color: white;
    padding: 15px 32px;
    text-align: center;
    text-decoration: none;
    display: inline-block;
    font-size: 30px;
    margin: 4px 2px;
    cursor: pointer;
    font-family: raleway, sans-serif;
    font-weight: bold;
    border-radius: 50px;
}


.button_container {
	position: fixed;
    bottom: 3%;
    right: 2%;
    height: 75px;
    width: 75px;
    cursor: pointer;
    z-index: 900;
    -webkit-transition: opacity .25s ease;
    transition: opacity .25s ease
}

.buttontitle {
	text-align: center;
	color:white;
}

@media screen and (min-width: 768px) {
.button_container {
display: none!important;
}
}</style>
