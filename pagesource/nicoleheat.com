                    <input type="text" placeholder="Usermane" class="form-control" name="user[user]" id="login_user"/>
                </div>
                <div class="form-group">
                    <input type="password" placeholder="Password" class="form-control" name="user[password]"
                           id="login_pass"/>
                    <input type=submit style="display: none;">
                </div>
                <div class="clearfix">
                    <div class="btn-holder"><a href="#" class="btn-sig btn-success" id="login">Enter</a></div>
                    <!--<div class="links"><a href="#" class="link">Members Entrance</a><span class="sep">|</span><a href="#" class="link">Resend / Forgot password</a></div>-->
                    <div class="error">
                        <div style="display: none;" id="login_err">Username or password is incorrect</div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
