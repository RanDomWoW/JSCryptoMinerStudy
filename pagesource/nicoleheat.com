<script src="//adultium.com/gallery/mobile/js/pushy.min.js"></script>

<script src="/templates/1/phfiles/js/main.js"></script>

<script>

    function show_form(param) {
        if (param == 1) {
            $('#login-div').hide();
            $('#signup-div').show();
            //rep 2 stat
            AdultiumAnalytic.getAsyncTracker().trackCustomEvent({
                event: 'adultium_com_email_form_view',
                signup_ref: $('#signup_ref').val()
            });
        }
        else {
            $('#login-div').show();
            $('#signup-div').hide();
        }
    }

    function validateEmail($email) {
        var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
        return emailReg.test($email);
    }

    function getRegisterModel() {
        var model = {
            email: $('#signup_email').val(),
            channel: $('#channel').val(),
            channel_name: $('#channel_name').val(),
            channel_img: $('#channel_img').val(),
            var: $('#signup_var').val(),
            ae_avs_id: 14011,
            set_id: null,
            set_hash: null,
        };

        return model;
    }

    function showSignUpError2(error) {
        $('#signup_email').css('border-color', '#d00');
        $('#signup_err2').text(error).show();
        $('.btn-success').css('background-color', '#018b01');
    }


    $(document).ready(function () {

        //form
        $('.signup-link').on('click', function (e) {

            e.preventDefault();

            show_form(1);
        });

        $('.login-link').on('click', function (e) {
            e.preventDefault();

            show_form(2);
        });

        SignupPrepareLogicDone = false;
        //signup form handler
        $('#signup-form').on('submit', function (e) {

            if (SignupPrepareLogicDone) {
                return true;
            }

            $('.btn-success').css('background-color', 'gray');
            if (!validateEmail($('#signup_email').val()) || !$('#signup_email').val()) {
                e.preventDefault();
                //alert('Please enter valid e-mail');
                $('#signup_email').css('border-color', '#d00');
                $('#signup_err').show();
                $('.btn-success').css('background-color', '#018b01');

                return false;
            }

            //register through authService
            var model = getRegisterModel();
            app.authServiceLink.signUp(model, function (data) {
                if (data.success) {

                    window.location = 'https://www.adultium.com/channel/3a02e56b28a43139ce3678dcb78826a8?welcome'
                }
                else {

                    showSignUpError2(data.error_message);
                }
            })

            e.preventDefault();

            return false;
        });

        $('#sgnup').on('click', function (e) {
            $('#signup-form').submit();
        });

        //login form handler
        $('#login-form').on('submit', function (e) {
            e.preventDefault();
            $('.btn-success').css('background-color', 'gray');
            $('#login_err').hide();

            var url = 'https://www.adultium.com/gallery/index.php?act=login&channel=3a02e56b28a43139ce3678dcb78826a8&user=' + jQuery("#login_user").val() + '&pass=' + encodeURIComponent(jQuery("#login_pass").val());
            window.open(url, '_balnk')
            $('#login-div').hide();

        });

        $('#login').on('click', function (e) {
            $('#login-form').submit();
        });

        //srch_form
        $('#srch-form').on('submit', function (e) {
            e.preventDefault();
            show_form(1);
        });

        $('.newsletter-form').on('submit', function (e) {

            if (SignupPrepareLogicDone) {
                return true;
            }

            if (!validateEmail($('input[name=email]').val()) || !$('input[name=email]').val()) {
                e.preventDefault();
                alert('Please enter valid e-mail');
                return false;
            }

            //register through authService
            var model = getRegisterModel();
            app.authServiceLink.signUp(model, function (data) {
                if (data.success) {
                    window.location = data.response;
                }
                else {

                    showSignUpError2(data.error_message);
                }
            })

            e.preventDefault();
            return false;
        });

        $('.modal').click(function (e) {
            //console.log('outside clk');
            console.log(e);
            $('#signup-div, #login-div').hide();
        });

        $('.modal-dialog').click(function (event) {
            event.stopPropagation();
            //console.log('inside clk');
        });

        $('#play-button').click(function (e) {
            $('#demolay').hide();
        });

        $('#player_close').on('click touchstart', function (e) {
            $('#vh').get(0).pause();
            $('#vh_div').hide();
        });
    });

</script>



</body>

</html>