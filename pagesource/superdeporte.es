<script type="text/javascript">
	
	String.prototype.startsWith = function(str)
	{return (this.match("^"+str)==str)}

	$(document).ready(function() {	
		$("ul.qtabs li").hover(function(){
			$(this).addClass("hover");
		},
		function(){
			$(this).removeClass("hover");
		});
		
		
		var deporteSelec = "";
		var competicionSelec = "";
		
		var url = location.href;
		var uri = url.substring("http://".length);
		var aux = "";
		if(uri.indexOf("/")>=0)
		{
			uri = uri.substring( uri.indexOf("/")+1);
			

			
			aux = uri.substring( uri.indexOf("/")+1);
			deporteSelec = aux;
			if(aux.indexOf("/")>0)
			{
				deporteSelec = aux.substring( 0, aux.indexOf("/"));
				aux = aux.substring( aux.indexOf("/")+1);
			}

		}
		
		
		if(aux.length==0 && deporteSelec=="futbol")
			competicionSelec = "primera-division";
		else if(aux.indexOf("/")==-1)
			  competicionSelec = aux;					
		else competicionSelec = aux.substring( 0, aux.indexOf("/"));
		
		if(competicionSelec=="directo")
		{
			aux = aux.substring( aux.indexOf("/")+1);						
			competicionSelec = aux.substring( 0, aux.indexOf("/"));
		}
		
		if(deporteSelec=="futbol")
		{
			if(competicionSelec.startsWith("segunda-division-b"))
				competicionSelec = "segunda-division-b";
			else if(competicionSelec.startsWith("tercera-division"))
				competicionSelec = "tercera-division";
			else if( competicionSelec.startsWith("preferente") || competicionSelec.startsWith("primera-catalana") || competicionSelec.startsWith("primera-andaluza"))
				competicionSelec = "preferente";
	    	else if( competicionSelec.startsWith("division-honor") )
				competicionSelec = "division-honor";
	    	else if( competicionSelec.startsWith("ligas-") || competicionSelec.startsWith("premier-league") || competicionSelec.startsWith("liga-") || competicionSelec.startsWith("calcio-"))
				competicionSelec = "ligas-mundiales";
			else if(competicionSelec.startsWith("quiniela"))
				competicionSelec = "primera-division";
			else if(competicionSelec.startsWith("eurocopa") || competicionSelec.startsWith("amistosos-selecciones") || competicionSelec.startsWith("selecciones") || competicionSelec.startsWith("clasificacion"))
				competicionSelec = "selecciones";
		}

		else if(deporteSelec=="")
		{
			deporteSelec = "futbol";
			competicionSelec = "primera-division";
		}	
		
		else if(deporteSelec=="baloncesto" && competicionSelec=="")
			competicionSelec = "acb";
								
		// alert("deporteSelec:"+deporteSelec+" competicionSelec:"+competicionSelec);		
