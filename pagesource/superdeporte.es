	{
		var pestanyaDeporte;
		var nombrePestanyaDeporte = "divReclamoGECADeporte_";
		var deporteSelecOK = false;
		
		var indiceGuion;
		var nombreCapaCompeticion;
		var capaCompeticion;
		var competicionSelecOK = false;
		
		$("[id^="+nombrePestanyaDeporte+"]").each(function()
		{
			pestanyaDeporte = $(this).attr("id").substring(nombrePestanyaDeporte.length);
		
			if(pestanyaDeporte==deporteSelec)
			{
				  $("#"+nombrePestanyaDeporte+deporteSelec).attr("class","tabbertab");
				  $("#pestanyaGECA_"+deporteSelec).attr("class","tabberactive");
				  
				  deporteSelecOK = true;
			}		
			else
			{
			 	$("#"+nombrePestanyaDeporte+pestanyaDeporte).attr("class","tabbertab tabbertabhide");
			 	$("#pestanyaGECA_"+pestanyaDeporte).attr("class","");			 	
			}			
		});
		
		if(deporteSelecOK==false)
		{
			deporteSelec = "futbol";
			$("#"+nombrePestanyaDeporte+deporteSelec).attr("class","tabbertab");
			$("#pestanaGECA_"+deporteSelec).attr("class","tabberactive");
		}
		
		$('.qtcontent').hide();
		$('ul.qtabs li').removeClass("open");
		
		
		if(competicionSelec.length==0)
			;
			
		else
		{
			nombreCapaCompeticion = "capaReclamoGECA_";
			
			$("[id^="+nombreCapaCompeticion+"]").each(function()
			{
				capaCompeticion = $(this).attr("id").substring(nombreCapaCompeticion.length);
				indiceGuion = capaCompeticion.indexOf("_"); 
				pestanyaDeporte = capaCompeticion.substring(0, indiceGuion);
				capaCompeticion = capaCompeticion.substring(indiceGuion+1);
			
				if(pestanyaDeporte==deporteSelec && capaCompeticion==competicionSelec)
				{
					$("#liReclamoGECA_"+deporteSelec+"_"+competicionSelec).attr("class","open");
					$("#"+nombreCapaCompeticion+deporteSelec+"_"+competicionSelec).show();
			
					competicionSelecOK = true;
				}		
				else
				{
					$("#liReclamoGECA_"+pestanyaDeporte+"_"+capaCompeticion).attr("class","");
				 	$("#"+nombreCapaCompeticion+pestanyaDeporte+"_"+capaCompeticion).hide();
				}
				
			});
			
			if(competicionSelecOK==false)
			{
				if(deporteSelec == "futbol")
					competicionSelec = "primera-division";
				else if(deporteSelec == "baloncesto")						
					competicionSelec = "acb";
					
				$("#"+nombreCapaCompeticion+deporteSelec+"_"+competicionSelec).show();
				$("#liReclamoGECA_"+deporteSelec+"_"+competicionSelec).attr("class","open");
					
			}	
		}
		
				
	}
	
</script>
