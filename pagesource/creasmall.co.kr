	<script type="text/javascript">
	(function($){
		$(function(){
			//메인 정렬
			function randomRange(n1, n2) {
				return Math.floor( (Math.random() * (n2 - n1 + 1)) + n1 );
			}
			
			var mainSlideEle = $("#main_list");
			
			//랜덤 정렬
			var mainSlideIdx = randomRange(0,3);
			
			//console.log(mainSlideIdx);
			if(mainSlideIdx > 0) {            
				var getMainSlide = $("#main_list .list_box").clone();
				mainSlideEle.empty();
				var mainSlidePosition = [
					[],
					[1,2,7,0,9,10,8,5,6,12,4,11,3],
					[8,0,3,4,5,6,7,12,11,9,1,2,10],
					[1,6,8,5,2,0,12,11,3,4,7,9,10]
				];
			
				$.each(mainSlidePosition[mainSlideIdx], function(key,value) {
					mainSlideEle.append(getMainSlide.eq(value));
				});
			}
			
			//메인 슬라이드 설정
			var mainSlideArray = [];
			function mainSlideCreate() {
				$(".main_list_wrap .main_list_slide").each(function(key) {
					var mainSlideBox = $(this).parent().parent();					
					var mainSlideThumb = mainSlideBox.attr("id","main_slide_" + key).find(".slide_thumb");
					var mainSlideImgBg = $(this).find("li").each(function(keys) {
						mainSlideThumb.append('<a href="#" data-slide-index="' + keys + '"></a>');
					}).eq(0).find(".img").clone();
					mainSlideBox.append(mainSlideImgBg.addClass("bg_img"));
					
					var mainSlide = $(this).removeClass("load").bxSlider({
						auto:true,
						speed:500,
						touchEnabled:false,
						controls:false,
                        autoHover:true,
						pagerCustom:"#main_slide_" + key + " .slide_thumb",
                        onSlideBefore:function() {
                            mainSlide.stopAuto();
                        },
                        onSlideAfter:function() {
                            mainSlide.stopAuto();
                            mainSlide.startAuto();
                        }
					});
					mainSlideBox.find(".btn_slide").off("click").on("click", function() {
                        mainSlide.stopAuto();
						if($(this).hasClass("btn_prev")) {
							mainSlide.goToPrevSlide();
						}
						else {
							mainSlide.goToNextSlide();
						}
						return false;
					});
					mainSlideArray.push(mainSlide);

				});
			}
			
			//메인 이미지 미리불러오기
			mainSlideEle.imagesLoaded().done(function() {
				mainSlideEle.removeClass("load").animate({ opacity:1 }, 200).masonry({
					initLayout: false,
					itemSelector: ".list_box",
					columnWidth: 9
				});
				
				//메인 리사이징
				var slideResizeInterval = null;
				var slidePrevWIdth = $(window).width();
				mainSlideEle.masonry( 'on', 'layoutComplete', function() {
					clearTimeout(slideResizeInterval);
					slideResizeInterval = setTimeout(function() {
						//mainSlideEle.masonry('layout');
						$.each(mainSlideArray, function() {
							this.reloadSlider();
						});
					}, 100);
				});
				$(window).on("resize", function() {
					   
				});
				mainSlideCreate();
				mainSlideEle.masonry();
			});
			

			//메인 롤링 상품
			$('.main_ctrwrap .item_boxwrap').cycle({
				fx :'scrollHorz',
				//fx :'fade',
				speed:1000,
				timeout:4300,
				pagerEvent :'mouseover',
				pause:true,
				pauseOnPagerHover:true,
				pager :'.main_ctrwrap .pager',
				prev : '.roll_wrap_prev',
				next : '.roll_wrap_next'
			});



			//tap
			$('.main_tapbdy > div').hide();
			$('.main_tapbdy > div').eq(0).show();
			$('.main_taphdr div').on('hover',function(){
				$('.main_tapbdy > div').hide();
				$('.main_taphdr div').removeClass('activz');
				$(this).addClass('activz');

				var _idxz = $(this).index();
				$('.main_tapbdy > div').eq(_idxz).show();
			});


		});
	})(jQuery)
	</script>

