<script type="text/javascript">
$(function(){
$("a[href*='#']").slideScroll();
});

var artists = new Array();

$(function (){
			
	function Artist(name, search_tag){
		this.name = name;
		this.search_tag = search_tag;
	}
	
	$.ajax({
		type: "GET",
		url: "/js/searchWord.xml", //xmlのパスは、ページによって変えてください。
		dataType: "xml",
		async: true,
		cache: false,
		success: function(xml){
			$(xml).find("searchWord").each(function(){
												
				var name = $(this).find("name").text();
				var search_tag = new Array();
				$(this).find("search_tag").each(function(){
					search_tag.push($(this).text());
				});
				
				var artist = new Artist(name, search_tag);
				artists.push(artist);
			})
		 }
	});
});

function startSuggest(){
  new Suggest.Local("searchtxt","suggest",artists, {prefix:true});
}


window.addEventListener ?
  window.addEventListener('load', startSuggest, false) :
  window.attachEvent('onload', startSuggest);
</script>
