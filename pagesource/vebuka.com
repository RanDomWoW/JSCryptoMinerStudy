    <script type="text/javascript">
      $(document).ready(function() {
        $("#download").click(function() {
          stepOne();
          return false;
        });

        function stepOne()
        {
          var url = $("#issuuurl")[0].value;
          if($.trim(url) == '')
          {
            return false;
          }

          $("#status").html('<img src="img/ajax-loader.gif" />&nbsp;' + 'Please wait while we verify the url...');
          $.ajax({
            method: "GET",
            url: "common.php",
            data: { url: url, 'step': 'one'},
            dataType: "json"
          })
            .done(function( msg ) {
              if(msg.success == 2)
              {
                $("#status").html('Invalid ISSUU document URL...');
                return false;
              }
              else if(msg.success == 0)
              {
                $("#status").html('Auto retrying...');
                stepOne();
                return false;
              }

              $("#status").html('<img src="img/ajax-loader.gif" />&nbsp;' + 'Processing document ' + msg.name + ' with ' + msg.count + ' pages...');
              window.location = "download.php?documentId=" + msg.documentId + "&count=" + msg.count + "&title=" + msg.title +  "&ck=f";
              //nextStep(msg.documentId, msg.count, msg.name, msg.title);
            });
            return false;
        }

        function nextStep(documentId, count, name, title)
        {
          $.ajax({
            method: "GET",
            url: "common.php",
            data: { documentId: documentId, count: count, title: title, name: name, 'step': 'two'},
            dataType: "json"
          })
            .done(function( msg ) {
              //$("#status").html('Please <a href>click here to download the document.');
              if(msg.success == 0)
              {
                $("#status").html('Invalid ISSUU document URL. Please check again...');
                return false;
              }
              window.location = "pdf/" + msg.download;
            });
        }
      });
    </script>
    
    
    
    
            
