<script type="text/javascript">

   (function(PB) { 

      // Node Event hooks
      // ************************
      var E_next = '.js-pbNext',
          E_prev = '.js-pbPrev',
          E_paging = '.js-pbPaging';


      // Node State
      // ************************
      var A_paging = 'pb_paging_indicator--active';

      // Cached Selectors
      // *******************************
      var $pbWrap = $('.pb'),
          $pbSlides = $('.pb_slide');
          $paging = $('.pb_paging_indicator'),
          $currentSlide = {},
          $newSlide = {};
          

      // Configuration
      // *******************
      var bannerImageSources = ["vpi/Certify_ProductLine_1b.png","vpi/iMac_Invoice_WithPaper_feb_17.png","vpi/pb-image-two.png","vpi/pb-image-three.png"],
          slideDuration = 15000,
          timeOnSlide = 0,
          navPaused = false
          timerPaused = false,
          numSlides = $pbSlides.length,
          newSlideId = 0,
          currentSlideId = 1;
          

      // Private Methods
      // ******************************

      var getPagerById = function(id) { return $('#pbPaging' + id); }
      var getSlideById = function(id)  { return $('#pb' + id); };

      var updateActivePager = function(){
         $('.' + A_paging).removeClass(A_paging);
         getPagerById(currentSlideId).addClass(A_paging);
      }

      var progressSlideShow = function(reverse) {
         if (reverse) { newSlideId = currentSlideId > 1 ? currentSlideId - 1 : numSlides; }
         else { newSlideId = currentSlideId < numSlides ? currentSlideId + 1 : 1; }
         showSlideById(newSlideId);
      };

      var showSlideById = function(id) {
         $currentSlide = getSlideById(currentSlideId);
         $newSlide = getSlideById(id);

         // pause navigation while animations resolve
         navPaused = true;
         $currentSlide.fadeOut(400,function() {
            $newSlide.fadeIn(400);
         });
         setTimeout(function() { 
            navPaused = false;
         },800);

         timeOnSlide = 0;
         currentSlideId = id;
         updateActivePager();
      };

      var loadSlideImageById = function(id) {
         $currentSlide = getSlideById(id);
         $slideImg = $currentSlide.find('img');

         $slideImg.attr('src',bannerImageSources[id-1]);
      };
 
      var loadRemainingSlideImages = function() {
         var i = 2;
         while(i <= numSlides) {
            loadSlideImageById(i);
            i++;
         }
      };

      var defineIntervalEvents = function() {
         $pbWrap.on('mouseEnter','.pb_slide',function pauseBannerPaging() {
            isPaused = true;
         });
         $pbWrap.on('mouseLeave','.pb_slide',function pauseBannerPaging() {
            isPaused = false;
         });
      };

      var defineNavEvents = function() {
         $pbWrap.on('click',E_paging,function pbPagingHandler() {
            if(navPaused) { return; }
            console.log($(this).data());
            showSlideById($(this).data().slideid);
         });

         $pbWrap.on('click',E_next,function pbNextHandler(e) {
            if(navPaused) { return; }
            progressSlideShow();
         });
         $pbWrap.on('click',E_prev,function pbPrevHandler(e) {
            if(navPaused) { return; }
            progressSlideShow(true);
         });
      };

      // Public Methods
      // *************************

      PB.init = function() {
         loadSlideImageById(1);

         var initSlideShow = window.setInterval(function() {
            if(timerPaused) { return; }
            timeOnSlide = timeOnSlide + 100;
            if(timeOnSlide % slideDuration == 0) {
               progressSlideShow();
               // reset time to -1200 to compensate for transitions
               timeOnSlide = 0;
            }
         },100);

         defineIntervalEvents();

         $(window).on('load',function() {
            // load additional slide images
            defineNavEvents();
            loadRemainingSlideImages();
         });
      };


   }(window.PB = window.PB || {}));
  
   PB.init();

</script>


