<script>
var $mediaImage = $('.featured-press_source');
var $citation = $('.featured-press_clipping');
var $citationWrap = $('.featured-press_clipping-wrapper');
function setMediaCitation($elem) {
  if($elem.hasClass('selected')) {
    return false;
  }
  var index = $mediaImage.index($elem);
  var matchedCitation = $($citation[index]);

  $elem.siblings('.featured-press_source').removeClass('featured-press_source--active');
  $elem.addClass('featured-press_source--active');

  // match to article
  matchedCitation.siblings('.featured-press_clipping').removeClass('featured-press_clipping--active');
  matchedCitation.addClass('featured-press_clipping--active');

  setMediaNubLocation(index);
}

function setMediaNubLocation() {
  var $mediaNub = $('#medianub');
  var $currentImage = $('.featured-press_source--active');
  var posFix = ($currentImage.outerWidth() / 2) - ($mediaNub.width() / 2);
  var leftOffset = $currentImage.offset().left - $('.featured-press_clipping-wrapper').offset().left + posFix;

  $citationWrap.height($('.featured-press_clipping--active').outerHeight());
  $mediaNub.css('left',leftOffset);
}
setMediaCitation($('.featured-press_source--active'));
$('.featured-press_source').on('click',function(e) {
  e.preventDefault();
  setMediaCitation($(this));
});
$(window).on('load resize',function(){
  setMediaNubLocation($('.featured-press_source--active'));
});
</script>


