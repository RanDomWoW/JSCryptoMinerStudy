        function funcMessageOpen(iframeBody,message,errMessage,nextPage){
            if(iframeBody != ''){
                var displayMessage = message;
    
                modalPopObj = funcCommonOpenPopMessage('layerPopMessage',true,'알림',400,200, displayMessage);
                
                if(nextPage != ''){
                    modalPopObj.bind( "dialogclose", function(event, ui) {
                        location.href = nextPage;
                    });
                    
                    var dialog_buttons = {}; 
                    dialog_buttons['닫기'] = function(){ modalPopObj.dialog('close'); };
                    modalPopObj.dialog({ buttons: dialog_buttons });               
                }
                
            }
        }
        

/*         $jQ(document).on("keydown", function (e) {
            if (e.which === 8 && !$jQ(e.target).is("input[type='text']:not([readonly]), textarea")) {
                e.preventDefault();
            }
        });  */       
        $jQ(document).unbind('keydown').bind('keydown', function (event) {
            var doPrevent = false;
            if (event.keyCode === 8) {
                var d = event.srcElement || event.target;
                if ((d.tagName.toUpperCase() === 'INPUT' && 
                     (
                         d.type.toUpperCase() === 'TEXT' ||
                         d.type.toUpperCase() === 'PASSWORD' || 
                         d.type.toUpperCase() === 'FILE' || 
                         d.type.toUpperCase() === 'EMAIL' || 
                         d.type.toUpperCase() === 'SEARCH' || 
                         d.type.toUpperCase() === 'DATE' )
                     ) || 
                     d.tagName.toUpperCase() === 'TEXTAREA') {
                    doPrevent = d.readOnly || d.disabled;
                }
                else {
                    doPrevent = true;
                }
            }

            if (doPrevent) {
                event.preventDefault();
            }
        });
