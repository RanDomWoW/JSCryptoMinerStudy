// FUNCIÓN QUE CARGA LA URL DE 'data-original' DENTRO DE UN DIV AL APARECER EN PANTALLA CON SCROLL
(function($){
	$.fn.extend({
		load_con_escroll: function (){
			var objs_destinos = this;
			var windowHeight = $(document).height();
			$.fn.load_con_escroll.add_lazy(objs_destinos, windowHeight, 300);
			$(document).scroll(function() {
				$.fn.load_con_escroll.add_lazy(objs_destinos, windowHeight, 300);
			});
		}
	});

	$.fn.load_con_escroll.add_lazy = function(objs_destinos, windowHeight, espacio_precarga){
		objs_destinos.each(function(){
			var obj_destino = $(this);
			var obj_destino_top_pos = obj_destino.offset().top - espacio_precarga;
			if(windowHeight >= obj_destino_top_pos  && obj_destino.attr('data-original') != '') {
				obj_destino.load(obj_destino.attr('data-original'));
				obj_destino.attr('data-original', '');
			}
		});
	};
})(jQuery)


