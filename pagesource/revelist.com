_url_desktop":"","background_url_mobile":"","click_tracking":"","client_tracking":"","display_start":null,"display_end":null,"tag_type_name":"Franchise","user_first_name":"April","user_last_name":"Walloga","user_display_name":"AprilWalloga","tagged_count":77,"non_cat":1,"tag_type_5":1,"order_by":1},{"content_id":8111,"hashtag_id":21,"tag_type":2,"base":1,"user_id":1,"created_at":"2015-12-08 15:13:53","updated_at":"2015-12-08 15:13:53","id":21,"name":"Body Positive","display_name":"Body Positive","h1":"BodyPositive","meta_name":"Body Positive & Plus Size News","url_name":"body-positive","description":"Changing the way you look at your body, one story at a time.","background_url_desktop":null,"background_url_mobile":null,"click_tracking":null,"client_tracking":null,"display_start":null,"display_end":null,"tag_type_name":"Topic","user_first_name":"","user_last_name":"","user_display_name":"ectogon","tagged_count":939,"is_topic":true,"tag_type_2":1,"order_by":4},{"content_id":8111,"hashtag_id":3,"tag_type":1,"base":1,"user_id":1,"created_at":"2015-12-08 14:50:18","updated_at":"2015-12-08 14:50:18","id":3,"name":"Style","display_name":"Style","h1":"Style","meta_name":"Fashion News, Trends & How-Tos","url_name":"style","description":"How to look fly and not go broke -- no matter what size you wear.","background_url_desktop":null,"background_url_mobile":null,"click_tracking":null,"client_tracking":null,"display_start":null,"display_end":null,"tag_type_name":"Category","user_first_name":"","user_last_name":"","user_display_name":"ectogon","tagged_count":2477,"tag_type_1":1,"order_by":5}],"au4":"lifestyle","sec":"style_body-positive","author_display":"Jessica Torres","author":"JessicaTorres","author_email":"jessicat@revelist.com","author_avatar":"2016\/01\/21\/15\/c8\/9g\/pofwu2q1c83.jpg","author_role":8,"author_social_link":{"type":"@instagram","url":"https:\/\/www.instagram.com\/thisisjessicatorres\/","is_primary":1},"author2_display":"Alle Connell","author2":"AlleConnell","author2_email":"aconnell@revelist.com","author2_avatar":"2017\/02\/02\/10\/am\/hy\/poi4gfjh4c9.png","author2_role":8,"author2_social_link":{"type":"@instagram","url":"http:\/\/www.instagram.com\/helloalle","is_primary":1},"team_display":"Revelist Editors","team":"teamrevelist","team_email":"general@revelist.com","team_avatar":"2016\/03\/22\/10\/dn\/wx\/povd15drks3.jpg","team_role":8,"team_social_link":{"type":"@instagram","url":"https:\/\/www.instagram.com\/heyrevelist\/","is_primary":0},"publish_datetime":"Jun 15, 2017 12:52 PM","ago":"9 months ago","status_name":"Published","lg":1,"image_url":"2017\/06\/14\/11\/88\/68\/powb6kzeo0a.png","in_pod":1,"content_id":8111,"hashtag_id":0,"flex_type_id":1,"flex_id":241,"start_at":"2018-02-26 11:28:19","type_1":1,"image_url_ugc_width":0,"image_url_ugc_height":0,"cropped_image_url":"2017\/06\/14\/11\/88\/68\/powb6kzeo0a.png","cropped_image_ugc_width":0,"cropped_image_ugc_height":0,"labels_json":"[{\"name\":\"TruthOrWear\"},{\"name\":\"Product Pick\"},{\"name\":\"Revelist Pick\"},{\"name\":\"Revelist Insider\"},{\"name\":\"Opinion\"},{\"name\":\"Exclusive\"}]"}},{"position_type":"ad"},{"position_type":"flex","content":{"id":11973,"content_type_id":3,"title":"27 crazy facts about Lime Crime founder Doe Deere","meta_title":"27 controversial facts about Lime Crime founder Doe Deere","url_title":"doe-deere-controversy","canonical_url":"","description":"She's had a tumultuous past.","meta_desc":"Read surprising facts about indie beauty brand Lime Crime's controversial founder Xenia Vorotova, known to most as Doe Deere.","show_desc":1,"intro":"[{\"title\":\"\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"instagram.com\\\/doedeere\",\"alt_text\":\"doe deere\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/16\\\/bi\\\/y8\\\/phku21mbcw2qbwe.jpg\",\"width\":\"1080\",\"height\":\"1349\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75756\",\"order_by\":\"\",\"status\":\"1\",\"position\":\"1\",\"zoom\":1,\"module_id\":\"75756\",\"type_3\":1},{\"title\":\"\",\"text\":\"<p>Aside from <a target=\\\"_self\\\" href=\\\"http:\\\/\\\/www.revelist.com\\\/bloggers\\\/jeffree-star-biggest-drama\\\/10600\\\">Jeffree Star<\\\/a>, there is no one in the internet beauty world who is as controversial as Doe Deere, known to most as the founder of Lime Crime. Her connection to the brand is without a doubt the number one cause for Lime Crime's 10-year infamy and, arguably, its mainstream success, too.<\\\/p><p>To be a person this polarizing, you've got to have a lot of personality, strong branding, and a very questionable past. Deere has all three of those in spades. What makes the internet love and hate her so passionately? Here are just 27 tidbits of information on her for you:<\\\/p>\",\"module_type_id\":1,\"id\":\"30267\",\"order_by\":\"1\",\"status\":\"1\",\"position\":\"2\",\"module_id\":\"30267\",\"type_1\":1}]","modules":"[{\"title\":\"<p>Her real name is Xenia Vorotova.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/BYW9TQ_j0Hn\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/17\\\/72\\\/jw\\\/phl0oomygw2qbwe.png\",\"width\":\"944\",\"height\":\"1184\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75769\",\"order_by\":\"\",\"status\":\"1\",\"position\":\"1\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75769\",\"type_3\":1},{\"title\":\"<p>She was born in June 1981 \\u2014 that makes her 37, and a Gemini.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/BcAxX9oDnpw\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/17\\\/4c\\\/3q\\\/phgprglge82qbwe.png\",\"width\":\"1056\",\"height\":\"1194\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75768\",\"order_by\":\"1\",\"status\":\"1\",\"position\":\"2\",\"content_type_split_module\":\"1\",\"zoom\":1,\"module_id\":\"75768\",\"type_3\":1},{\"title\":\"<p>She's originally from&nbsp;Izhevsk, Russia and speaks both Russian and English.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"instagram.com\\\/doedeere\",\"alt_text\":\"doe deere\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/14\\\/14\\\/rj\\\/ui\\\/phkknkc40c2qbwe.png\",\"width\":\"954\",\"height\":\"1188\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75901\",\"order_by\":\"2\",\"status\":\"1\",\"position\":\"3\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75901\",\"type_3\":1},{\"title\":\"<p>She moved to the US at the age of 17.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/4uLtmYR2q4\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/16\\\/ah\\\/nk\\\/phhqdmcdck2qbwe.png\",\"width\":\"1190\",\"height\":\"1186\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75760\",\"order_by\":\"3\",\"status\":\"1\",\"position\":\"4\",\"content_type_split_module\":\"1\",\"zoom\":1,\"module_id\":\"75760\",\"type_3\":1},{\"title\":\"<p>She claimed to have studied fashion at New York City's Fashion Institute of Technology.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/-wa_jRx2rJ\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere style\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/14\\\/15\\\/eo\\\/p6\\\/ph3r5d0kgk2qbwe.png\",\"width\":\"944\",\"height\":\"1192\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75918\",\"order_by\":\"4\",\"status\":\"1\",\"position\":\"5\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75918\",\"type_3\":1},{\"title\":\"<p>She currently lives in a two-story victorian house in the Los Angeles area.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/BYugv4-D1AQ\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere house\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/17\\\/6m\\\/hi\\\/phele1y8002qbwe.png\",\"width\":\"942\",\"height\":\"1184\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75764\",\"order_by\":\"5\",\"status\":\"1\",\"position\":\"6\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75764\",\"type_3\":1},{\"title\":\"<p>She's married to Mark Dumbleton, who helped her co-found Lime Crime.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/xlSLbkR2kg\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere husband\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/16\\\/8m\\\/n9\\\/phja0waa8s2qbwe.png\",\"width\":\"1186\",\"height\":\"1188\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75759\",\"order_by\":\"6\",\"status\":\"1\",\"position\":\"7\",\"content_type_split_module\":\"1\",\"zoom\":1,\"module_id\":\"75759\",\"type_3\":1},{\"title\":\"<p>She says they've been together for nearly 20 years.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"instagram.com\\\/doedeere\",\"alt_text\":\"doe deere husband\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/14\\\/15\\\/55\\\/5y\\\/ph6b1vxhgk2qbwe.png\",\"width\":\"1132\",\"height\":\"1190\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75923\",\"order_by\":\"7\",\"status\":\"1\",\"position\":\"8\",\"content_type_split_module\":\"1\",\"zoom\":1,\"module_id\":\"75923\",\"type_3\":1},{\"title\":\"<p>In the days of Myspace, she was in a \\\"fairy tale rock band\\\" called <a target=\\\"_blank\\\" href=\\\"https:\\\/\\\/www.youtube.com\\\/watch?v=GLFDfsAFDp8\\\">Sky Salt<\\\/a>.<\\\/p>\",\"credit\":\"Sky Salt Myspace Archives\",\"credit_url\":\"\",\"alt_text\":\"sky salt doe deere\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/14\\\/14\\\/b3\\\/vp\\\/phnxbufpk42qbwe.jpg\",\"width\":\"400\",\"height\":\"600\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75907\",\"order_by\":\"8\",\"status\":\"1\",\"position\":\"9\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75907\",\"type_3\":1},{\"title\":\"<p>She later had a short solo music career starting in 2008.<\\\/p>\",\"url\":\"https:\\\/\\\/www.youtube.com\\\/watch?v=NXkB5qxmaXw\",\"youtube_id\":\"NXkB5qxmaXw\",\"code\":\"\",\"caption\":\"\",\"module_type_id\":5,\"id\":\"5862\",\"order_by\":\"9\",\"status\":\"1\",\"position\":\"10\",\"content_type_split_module\":\"1\",\"module_id\":\"5862\",\"type_5\":1},{\"title\":\"<p>Before she launched the beauty brand of the same name, Deere used the moniker Lime Crime to sell DIY clothing on eBay in the early '00s.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"instagram.com\\\/doedeere\",\"alt_text\":\"doe deere style\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/14\\\/14\\\/9w\\\/q5\\\/phgj3cvrc42qbwe.png\",\"width\":\"1186\",\"height\":\"1186\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75904\",\"order_by\":\"10\",\"status\":\"1\",\"position\":\"11\",\"content_type_split_module\":\"1\",\"zoom\":1,\"module_id\":\"75904\",\"type_3\":1},{\"title\":\"<p>Shortly after launching Lime Crime as a makeup company in 2007, she dressed as Adolf Hitler for Halloween for a reason she wouldn't reveal until years later.<\\\/p>\",\"credit\":\"Doe Deere Blogazine Archive\",\"credit_url\":\"\",\"alt_text\":\"doe deere hitler\",\"image_link\":\"\",\"caption\":\"<p>Earlier this year, Deere addressed the costume to a commenter on Instagram with the following: \\\"As far as my costume goes, yes, I dressed up as Hitler 12 years ago to push back on a crippling fear of Nazis. I\\u2019m a proud Russian Jewish immigrant who worked hard and is grateful for everything I have.\\u201d<\\\/p><p>Lime Crime's global general manager Kim Walls also recently explained Deere's costume choice to <a target=\\\"_blank\\\" href=\\\"https:\\\/\\\/www.racked.com\\\/2018\\\/3\\\/6\\\/17081186\\\/lime-crime-comeback-controversy-new-stores-ulta\\\">Racked<\\\/a>: \\u201cWhen she came to the US as a teenager, she didn\\u2019t have the cultural references and understandings of things that a typical American has. Her understanding of Halloween was that this was a time when people dress up as monsters. And in her life and in her history and in her heritage, the biggest monster was Hitler, so she put on a Hitler-like costume and thought she was doing something American, and boy, did that backfire.\\u201d<\\\/p>\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/14\\\/14\\\/5c\\\/d2\\\/ph4b1vwh0k2qbwe.jpg\",\"width\":\"527\",\"height\":\"534\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75895\",\"order_by\":\"11\",\"status\":\"1\",\"position\":\"12\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75895\",\"type_3\":1},{\"title\":\"<p>Her questionable actions around that time were what inspired many of the <a target=\\\"_blank\\\" href=\\\"https:\\\/\\\/www.instagram.com\\\/riplimegrime\\\/\\\">multiple<\\\/a> <a target=\\\"_blank\\\" href=\\\"https:\\\/\\\/ohdeardoedeere.tumblr.com\\\/post\\\/65612552332\\\/doe-deere-lime-crime-a-sordid-history\\\">online<\\\/a> <a target=\\\"_blank\\\" href=\\\"https:\\\/\\\/www.reddit.com\\\/r\\\/Indiemakeupandmore\\\/comments\\\/1qkmn5\\\/doe_deere_lies_updates_and_alternatives_to_this\\\/\\\">communities<\\\/a>&nbsp;that are dedicated to documenting her and her brand's many <a target=\\\"_self\\\" href=\\\"http:\\\/\\\/www.revelist.com\\\/makeup\\\/lime-crime-controversies\\\/8202\\\">controversies<\\\/a>.<\\\/p>\",\"credit\":\"Oh Dear, Doe Deere!\",\"credit_url\":\"https:\\\/\\\/ohdeardoedeere.tumblr.com\\\/post\\\/65612552332\\\/doe-deere-lime-crime-a-sordid-history\",\"alt_text\":\"doe deere lies\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/14\\\/15\\\/bs\\\/rm\\\/phuid1cfco2qbwe.png\",\"width\":\"1814\",\"height\":\"706\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75928\",\"order_by\":\"12\",\"status\":\"1\",\"position\":\"13\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75928\",\"type_3\":1},{\"title\":\"<p>For instance, in response to backlash from Lime Crime's China Doll palette, she called accusations of cultural appropriation \\\"a little silly.\\\"<\\\/p>\",\"credit\":\"Lime Crime\",\"credit_url\":\"\",\"alt_text\":\"lime crime china doll\",\"image_link\":\"\",\"caption\":\"<p>\\\"What is cultural appropriation anyway?\\\" she wrote on her&nbsp;<a target=\\\"_blank\\\" href=\\\"https:\\\/\\\/web.archive.org\\\/web\\\/20150317010038\\\/http:\\\/\\\/www.doedeereblogazine.com\\\/articles\\\/chinadoll-lives-on\\\/\\\" title=\\\"Link: https:\\\/\\\/web.archive.org\\\/web\\\/20150317010038\\\/http:\\\/\\\/www.doedeereblogazine.com\\\/articles\\\/chinadoll-lives-on\\\/\\\">blog<\\\/a>&nbsp;in 2012. \\\"To summarize, it\\u2019s the&nbsp;borrowing of certain cultural elements by another cultural group.&nbsp;To be honest, I find the notion a little silly.&nbsp;Not all that pertains to race has to be racist, just like not every cultural reference has to be met with opposition.&nbsp;What matters is intent.&nbsp;As an artist and a human being, I have the right to be inspired by and wanting to explore, adapt, and otherwise express myself through&nbsp;things I find wonderful.\\\"<br><\\\/p>\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/14\\\/14\\\/6g\\\/bw\\\/ph8qr39bks2qbwe.jpg\",\"width\":\"620\",\"height\":\"400\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75898\",\"order_by\":\"13\",\"status\":\"1\",\"position\":\"14\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75898\",\"type_3\":1},{\"title\":\"<p>For around seven years she created her own custom hair colors that she would later turn into Lime Crime's Unicorn Hair dyes.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/xlSLbkR2kg\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/16\\\/25\\\/o4\\\/ph910wd3us2qbwe.png\",\"width\":\"1178\",\"height\":\"1190\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75758\",\"order_by\":\"14\",\"status\":\"1\",\"position\":\"15\",\"content_type_split_module\":\"1\",\"zoom\":1,\"module_id\":\"75758\",\"type_3\":1},{\"title\":\"<p>She's had every hair color on the rainbow spectrum, but she's most known for this magenta hue that's now a Unicorn Hair dye named <a target=\\\"_blank\\\" href=\\\"https:\\\/\\\/www.ulta.com\\\/unicorn-hair-semi-permanent-hair-color-full-coverage?productId=xlsImpprod17141007#\\\">Pony.<\\\/a><\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"instagram.com\\\/doedeere\",\"alt_text\":\"doe deere hair\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/14\\\/14\\\/2q\\\/u9\\\/phqkzmvw002qbwe.png\",\"width\":\"1194\",\"height\":\"1190\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75914\",\"order_by\":\"15\",\"status\":\"1\",\"position\":\"16\",\"content_type_split_module\":\"1\",\"zoom\":1,\"module_id\":\"75914\",\"type_3\":1},{\"title\":\"<p>She's a natural redhead.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/9wkoP2R2jj\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere natural hair\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/17\\\/a0\\\/vz\\\/phvtedxoo42qbwe.png\",\"width\":\"1194\",\"height\":\"1190\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75763\",\"order_by\":\"16\",\"status\":\"1\",\"position\":\"17\",\"content_type_split_module\":\"1\",\"zoom\":1,\"module_id\":\"75763\",\"type_3\":1},{\"title\":\"<p>That's her on the front of the Venus palette.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"instagram.com\\\/doedeere\",\"alt_text\":\"doe deere venus palette\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/16\\\/9o\\\/59\\\/ph6o1ypqg42qbwe.png\",\"width\":\"1182\",\"height\":\"1184\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75757\",\"order_by\":\"17\",\"status\":\"1\",\"position\":\"18\",\"content_type_split_module\":\"1\",\"zoom\":1,\"module_id\":\"75757\",\"type_3\":1},{\"title\":\"<p>Her mother might as well be her twin.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/BUIAcymDagu\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/17\\\/7l\\\/mk\\\/ph6xxyehgc2qbwe.png\",\"width\":\"948\",\"height\":\"1190\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75761\",\"order_by\":\"18\",\"status\":\"1\",\"position\":\"19\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75761\",\"type_3\":1},{\"title\":\"<p>She collects vintage hats.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/BSwBBJIjOF7\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/17\\\/1y\\\/xi\\\/ph8ids3jgk2qbwe.png\",\"width\":\"948\",\"height\":\"1182\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75762\",\"order_by\":\"19\",\"status\":\"1\",\"position\":\"20\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75762\",\"type_3\":1},{\"title\":\"<p>She also has a mad habit for extravagant high-heeled shoes.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"instagram.com\\\/doedeere\",\"alt_text\":\"doe deere shoes\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/14\\\/14\\\/8c\\\/ue\\\/phe5vulxc02qbwe.png\",\"width\":\"948\",\"height\":\"1196\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75912\",\"order_by\":\"20\",\"status\":\"1\",\"position\":\"21\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75912\",\"type_3\":1},{\"title\":\"<p>She has no tattoos, but did get her belly button pierced in 2015.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/9KlTR5x2ov\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/17\\\/7f\\\/1g\\\/ph7dhofgo42qbwe.png\",\"width\":\"1184\",\"height\":\"1186\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75765\",\"order_by\":\"21\",\"status\":\"1\",\"position\":\"22\",\"content_type_split_module\":\"1\",\"zoom\":1,\"module_id\":\"75765\",\"type_3\":1},{\"title\":\"<p>She has a cat named Puffy Fluffles.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/BZjIhSOjYY6\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere cat\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/17\\\/9p\\\/sw\\\/phnx6rqhgc2qbwe.png\",\"width\":\"948\",\"height\":\"1188\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75766\",\"order_by\":\"22\",\"status\":\"1\",\"position\":\"23\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75766\",\"type_3\":1},{\"title\":\"<p>And another cat named Chester.<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"instagram.com\\\/doedeere\",\"alt_text\":\"doe deere cat\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/14\\\/15\\\/6j\\\/or\\\/phfwpb1bwg2qbwe.png\",\"width\":\"946\",\"height\":\"1190\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75921\",\"order_by\":\"23\",\"status\":\"1\",\"position\":\"24\",\"content_type_split_module\":\"1\",\"zoom\":0,\"module_id\":\"75921\",\"type_3\":1},{\"title\":\"<p>She has a knack for giving antique furniture \\\"dollhouse makeovers.\\\"<\\\/p>\",\"credit\":\"Instagram\\\/doedeere\",\"credit_url\":\"https:\\\/\\\/www.instagram.com\\\/p\\\/q7_uStR2gP\\\/?taken-by=doedeere\",\"alt_text\":\"doe deere instagram\",\"image_link\":\"\",\"caption\":\"\",\"bin_id\":\"\",\"image_url\":\"2018\\\/03\\\/13\\\/17\\\/1y\\\/rb\\\/phtpfzoacg2qbwe.png\",\"width\":\"1194\",\"height\":\"1196\",\"is_animated\":\"0\",\"module_type_id\":3,\"id\":\"75767\",\"order_by\":\"24\",\"status\":\"1\",\"position\":\"25\",\"content_type_split_module\":\"1\",\"zoom\":1,\"module_id\":\"75767\",\"type_3\":1},{\"title\":\"<p>She and her friends once made a live-action \\\"Jem and the Holograms\\\" music video.<\\\/p>\",\"url\":\"https:\\\/\\\/www.youtube.com\\\/watch?v=tTXXLR000XM\",\"youtube_id\":\"tTXXLR000XM\",\"code\":\"\",\"caption\":\"\",\"module_type_id\":5,\"id\":\"5848\",\"order_by\":\"25\",\"status\":\"1\",\"position\":\"26\",\"content_type_split_module\":\"1\",\"module_id\":\"5848\",\"type_5\":1},{\"title\":\"<p>She also once provided back-up vocals and appeared in a music video for Lola Blanc's \\\"Shut the Damn Door,\\\" which has since been removed from YouTube.<\\\/p>\",\"source\":\"instagram\",\"code\":\"<blockquote class=\\\"instagram-media\\\" data-instgrm-permalink=\\\"https:\\\/\\\/www.instagram.com\\\/p\\\/mTSFX8R2vV\\\/\\\" data-instgrm-version=\\\"8\\\" style=\\\" background:#FFF; border:0; border-radius:3px; box-shadow:0 0 1px 0 rgba(0,0,0,0.5),0 1px 10px 0 rgba(0,0,0,0.15); margin: 1px; max-width:658px; padding:0; width:99.375%; width:-webkit-calc(100% - 2px); width:calc(100% - 2px);\\\"><div style=\\\"padding:8px;\\\"> <div style=\\\" background:#F8F8F8; line-height:0; margin-top:40px; padding:50% 0; text-align:center; width:100%;\\\"> <div style=\\\" background:url(data:image\\\/png;base64,iVBORw0KGg