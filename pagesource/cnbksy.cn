<script type="text/javascript">
    $(function () {
        //从首页选中的内容传递过来
        var ranges = ''.split(",");
        var types = ''.split(",");

        searchHint([$("#searchText")]);

        //文章类型和文献分类联动的选择控制
        //by Simon.HJ.Song
        bksy.post("/common/searchOptions", null,function (result) {
            {
                var findfail = false;
                var allfail = true;
                $.each(result.type, function () {
                    if (types.indexOf(this.value) != -1) {
                        this.checked = true;
                        allfail = false;
                    }
                    else {
                        findfail = true;
                    }
                });
                if (findfail && !allfail) {
                    $("#atAll").prop("checked", false);
                }
                else {
                    setTimeout(function () {
                        $("#searchType > li:not(.invisible) input:not(#atAll)").each(function () {
                            $(this).iCheck('uncheck');
                        });
                    }, 100);

                    $.each(result.type, function () {
                        this.checked = false;
                    });
                }
                $("#searchType").append(Handlebars.templates.type({data: result.type}));
                if (ranges.length == 1) {
                    for (var ix = 0; ix < result.range.length; ix++) {
                        if (result.range[ix].value == ranges[0]) {
                            if ('' == Enumerable.from(result.range[ix].linked).select("$.value").toArray().join(",")) {
                                $("#atAll").prop("checked", true);
                            }
                            break;
                        }
                    }
                }
                $('#searchType input').on('ifClicked', function (event) {
                    that = $(this);
                    if (that.attr("id") == "atAll") {
                        $("#searchType > li:not(.invisible) input:not(#atAll)").each(function () {
                            $(this).iCheck('uncheck');
                        });
                        if (that.prop("checked")) {
                            setTimeout(function () {
                                $("#atAll").iCheck("check");
                            }, 100);
                            return false;
                        }
                    }
                    else {
                        if (!that.prop("checked")) {
                            var ret = true;
                            $("#searchType > li:not(.invisible) input:not(#atAll)").each(function () {
                                if (this.id != that.attr("id")) {
                                    ret &= $(this).prop("checked");
                                }
                            });
                            if (ret) {
//                                $("#atAll").iCheck('check');
//                                setTimeout(function () {
//                                    $("#searchType > li:not(.invisible) input:not(#atAll)").each(function () {
//                                        $(this).iCheck('uncheck');
//                                    });
//                                }, 100);
                            }
                            else {
                                $("#atAll").iCheck('uncheck');
                            }
                        }
                        else {
                            var ret = false;
                            if (!ret) {
                                $("#atAll").iCheck('check');
                                $("#searchType > li:not(.invisible) input:not(#atAll)").each(function () {
                                    $(this).iCheck('uncheck');
                                });
                            }
                            else {
                                $("#atAll").iCheck('uncheck');
                            }
                        }
                    }
                }).iCheck({
                    checkboxClass: 'icheckbox_minimal-red',
                    radioClass: 'iradio_minimal-red'
                });
            }
            {
                findfail = false;
                allfail = true;
                $.each(result.range, function () {
                    if (ranges.indexOf(this.value) != -1) {
                        this.checked = true;
                        allfail = false;
                    }
                    else {
                        findfail = true;
                    }
                });
                if (findfail && !allfail) {
                    $("#searchAll").iCheck('uncheck');
                    setTimeout(function () {
                        refreshRT(result, null);
                    }, 100);
                }
                else {
                    setTimeout(function () {
                        $("#searchRange > li:not(.invisible) input:not(#searchAll)").each(function () {
                            $(this).iCheck('uncheck');
                        });
                    }, 100);
                }

                $("#searchRange").append(Handlebars.templates.range({data: result.range}));
                $('#searchRange input').on('ifClicked', function (event) {
                    that = $(this);
                    if (that.attr("id") == "searchAll") {
                        $("#searchRange > li:not(.invisible) input:not(#searchAll)").each(function () {
                            $(this).iCheck('uncheck');
                        });
                        if (that.prop("checked")) {
                            setTimeout(function () {
                                $("#searchAll").iCheck("check");
                            }, 100);
                            return false;
                        }
                    }
                    else {
                        if (!that.prop("checked")) {
                            var ret = true;
                            $("#searchRange > li:not(.invisible) input:not(#searchAll)").each(function () {
                                if (this.id != that.attr("id")) {
                                    ret &= $(this).prop("checked");
                                }
                            });
                            if (ret) {
                                $("#searchAll").iCheck('check');
                                setTimeout(function () {
                                    $("#searchRange > li:not(.invisible) input:not(#searchAll)").each(function () {
                                        $(this).iCheck('uncheck');
                                    });
                                }, 100);
                            }
                            else {
                                $("#searchAll").iCheck('uncheck');
                            }
                        }
                        else {
                            var ret = false;
                            $("#searchRange > li:not(.invisible) input:not(#searchAll)").each(function () {
                                if (this.id != that.attr("id")) {
                                    ret |= $(this).prop("checked");
                                }
                            });
                            if (!ret) {
                                $("#searchAll").iCheck('check');
                            }
                            else {
                                $("#searchAll").iCheck('uncheck');
                            }
                        }
                    }
                    refreshRT(result, that);
                }).iCheck({
                    checkboxClass: 'icheckbox_minimal-red',
                    radioClass: 'iradio_minimal-red'
                });
            }
            if (ranges[0] != "") {
                setTimeout("doSearching();", 200);
            }
        });

        function refreshRT(result, that) {
            // 上面选了全部，那么就不要隐藏
            if ($("#searchAll").prop("checked") || (that != null && that.attr("id") == "searchAll" && !$("#searchAll").prop("checked"))) {
                $("#searchType > li").removeClass("invisible");
            }
            else {
                var pt1 = false, pt2 = false, pt3 = false;
                $("#searchRange > li:not(.invisible) input:not(#searchAll)").each(function () {
                    if ((that == null && $(this).prop("checked")) || ( that != null && (($(this).prop("checked") && this.id != that.attr("id")) || (this.id == that.attr("id") && !that.prop("checked"))))) {
                        for (var i = 0; i < result.range.length && (!(pt1 && pt2 && pt3)); i++) {
                            if (result.range[i].id == $(this).prop("id")) {
                                for (var j = 0; j < result.range[i].linked.length && (!(pt1 && pt2 && pt3)); j++) {
                                    if (result.range[i].linked[j].id == "pt1") {
                                        pt1 = true;
                                    } else if (result.range[i].linked[j].id == "pt2") {
                                        pt2 = true;
                                    } else {
                                        pt3 = true;
                                    }
                                }
                            }
                        }
                    }
                });
                if (pt1) {
                    $("#ptpt1").removeClass("invisible");
                }
                else {
                    $("#ptpt1").addClass("invisible");
                }
                if (pt2) {
                    $("#ptpt2").removeClass("invisible");
                }
                else {
                    $("#ptpt2").addClass("invisible");
                }
                if (pt3) {
                    $("#ptpt3").removeClass("invisible");
                }
                else {
                    $("#ptpt3").addClass("invisible");
                }
                var ret = false;
                var allt = true;
                $("#searchType > li:not(.invisible) input:not(#atAll)").each(function () {
                    ret = ret || $(this).prop("checked");
                    allt = allt && $(this).prop("checked");
                });
                if (!ret) {
                    $("#atAll").iCheck('check');
                }
                if (!allt && ret) {
                    $("#atAll").iCheck('uncheck');
                }
            }
        }

        function doSearch() {
            var params = prepareRequest();
                console.log(params)
                if (!params.searchContent || params.searchContent.trim() == "" || params.searchContent.trim() == "*") {
                    msgError("不支持进行空检，请输入检索内容!");
                    return;
                }
            if (window.location.href.indexOf("search") == -1) {
                window.location.href = '/search' + "?" + $.param(params);
            }
            else {
                doSearching();
            }
        }

        //检索按钮相应函数
        $('.cls_search_text').keypress(function (event) {
            if (event.keyCode == "13") {
                if (typeof(facetComponent) != "undefined") {
                    facetComponent.clearFacets();
                }
                doSearch();
            }
        });

        $('#searchButton').click(function () {
            if (typeof(facetComponent) != "undefined") {
                facetComponent.clearFacets();
            }
            doSearch();
        });

        function searchAuthor(author) {
            bksy.formPost("/search/author", {"author": author},{target: "_blank"});
        }
    });


</script>        </div>
