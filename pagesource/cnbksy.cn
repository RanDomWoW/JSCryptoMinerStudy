<script type="text/javascript">
    var substringMatcher = function(strs) {
        return function findMatches(q, cb) {
            console.log(q);
            var matches, substringRegex;
            matches = [];
            var count = 0;
            substrRegex = new RegExp(q, 'i');
            $.each(strs, function (i, str) {
                if (substrRegex.test(str) && count < 4) {
                    count++;
                    matches.push(str);
                }
            });
            cb(matches);
        };
    };

    var searchHint=function (fields,hideclass) {
        bksy.post("/common/hints", null, function (result) {
            var conditions = result;
            $.each(fields, function (index, item) {
                $(item).typeahead({
                        hint: true,
                        highlight: true,
                        minLength: 1
                    },
                    {
                        name: 'conditions',
                        source: substringMatcher(conditions)
                    });
            });
            if(hideclass) {
                $(".twtypeahead").removeClass("twtypeahead");
            }
        });
    };

    var applyHint=function (item,otherdata) {
        $(item).typeahead({
                hint: true,
                highlight: true,
                minLength: 1
            },
            {
                name: 'conditions',
                source: substringMatcher(otherdata)
            });
    };
</script></head>
