    <script type="text/javascript">
        var  holderlight=false;
        var ele=$("#shine");

        function am() {
            if (!holderlight) {
                var seed = Math.floor(Math.random() * (3000) + 1000);
                ele.css("background-position", "-200px 0");
                ele.animate({backgroundPositionX: 100, backgroundPositionY: 0}, seed, am);
            }
        }

        $(function () {
            //无缝滚动代码
            var step = 1;
            var delay = 20;
            var direction = -1;//默认向左滚动 1 右边  -1 左边

            var size = $('#id_picSliderBg .picWrapper').length;
            var width = 150 * size;
            $('#id_picSliderBg .picPosition').css("width", width);
            var timer = buildTimer(direction);

            function buildTimer(direction) {
                return setInterval(scroll(direction), delay);
            }

            function _scroll(direction) {
                var left = $('#id_picSliderBg .picPosition').position().left;
                //alert(left+ "," + width/2 )
                if (left < -(width / 3)) {
                    $('#id_picSliderBg .picPosition').css("left", "0px");
                }
                else if (left > 0) {
                    $('#id_picSliderBg .picPosition').css("left", (-(width / 3 - 1)) + "px");
                }
                else {
                    $('#id_picSliderBg .picPosition').css("left", left + direction * step + "px");
                }
            }

            function scroll(direction) {
                return function () {
                    _scroll(direction);
                }
            }

            $('.picWrapper').mouseenter(function () {
                clearInterval(timer);
                $(this).find(".cls_pic").removeClass('picUnSelecedEffect');
                $(this).find(".cls_pic").addClass('picSelecedEffect');
            });

            $('.picWrapper').mouseleave(function () {
                $(this).find(".cls_pic").removeClass('picSelecedEffect');
                $(this).find(".cls_pic").addClass('picUnSelecedEffect');
                timer = buildTimer(direction);
            });
            $('#rightNav').mouseenter(function () {
                clearInterval(timer);
                direction = 1;
                timer = buildTimer(direction);
            });
            $('#leftNav').mouseenter(function () {
                clearInterval(timer);
                direction = -1;
                timer = buildTimer(direction);
            });

            am();
            $("#firstLink").hover(function () {
                ele.stop();
                ele.css("background-position", "-200px 0");
                holderlight = true;
            }, function () {
                holderlight = false;
                am();
            });
        });

        function prepareRequest() {
            var content = null;
            if ($(".cls_search_text").length > 0){
                if ($(".cls_search_text").length == 1){
                    content = $(".cls_search_text").val();
                }else {
                    content = $(".cls_search_text:eq(1)").val();
                }
            }
            var range = $("#searchRange > li:not(.invisible) input:checked:not(#searchAll)").map(function () {
                return $(this).val();
            }).get().join(",");

            if ($("#searchAll").prop("checked")) {
                range = $("#searchRange > li:not(.invisible) input:not(#searchAll)").map(function () {
                    return $(this).val();
                }).get().join(",");
            }

            var type = $("#searchType > li:not(.invisible) input:checked:not(#atAll)").map(function () {
                return $(this).val();
            }).get().join(",");

            if ($("#atAll").prop("checked")) {
                type = $("#searchType > li:not(.invisible) input:not(#atAll)").map(function () {
                    return $(this).val();
                }).get().join(",");
            }
            return {
                author: typeof(authorCondition) == "undefined" ? null : authorCondition,
                searchContent: content,
                categories: range,
                types: type
            };
        }
    </script>
