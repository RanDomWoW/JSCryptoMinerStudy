// Only given a query
function fetchProducts($q){
	$.ajax({
		url: '/_v2/request/index.php',
		method: 'POST',
		dataType: 'text',
		data: {choice: 'pricesearch', action:'productSearch',q:$q},
		error: function (xhr, ajaxOptions, thrownError){
        		console.log(xhr.status);
        		console.log(thrownError);
      		},		
		success: function(data){
			$('#productContainer').html('');
			console.log("Product suggestions:");
			console.log(data);
			data = JSON.parse(data);
			j = 0;
			
			$(data.products).each(function(i,prod){
				if(prod.priceCount > 0){
					j++;
					if(j == 1){
						$('#productContainer').html('<span class="productContainerTitle">Produktforslag:</span>');
					}
					console.log(prod);
					temp = $('#productItemPrototype').clone();
					$(temp).attr('id','productItem-'+prod.id);
					//$(temp).children('a').html(prod.title+' - fra kr '+prod.price);
					$(temp).children('a').html(prod.title);
					$(temp).children('a').attr('onclick','fetchProduct('+prod.id+',\''+prod.title+'\');offerRefineSearch($(this).text());return false;');
					
					if(i > 10){
						$(temp).addClass('hidden');
						$(temp).hide();
					}
					if(i == 11){
						var html = "<a href='#' class='right' onclick='showAllProductSuggestions();$(this).remove();'>Vis flere forslag &raquo</a>"
						$('#productContainer').append(html);	
					}
					$('#productContainer').append(temp);
				}
			});
		}
	});
}

function showAllProductSuggestions(){
	$('.productContainerItem').removeClass('hidden');
}

function fetchProduct($id,$title){
	//$('#offerSearchText').val($title);
	$.ajax({
		url: '/_v2/request/index.php',
		method: 'POST',
		dataType: 'text',
		data: {choice: 'pricesearch', action:'getProduct',q:$id},
		error: function (xhr, ajaxOptions, thrownError){
        		console.log(xhr.status);
        		console.log(thrownError);
      		},
		success: function(data){
			$('#offerContainer').html('');
			console.log('Fetching product:')
			//console.log(data);
			console.log(JSON.parse(data));
			data = JSON.parse(data);
			//console.log(data.prices.length);
			$(data.prices).each(
				function(i,o){
		  			temp = $('#offerPrototype').clone();
		  			$(temp).attr('id','offer-'+o.id);
		  			$(temp).data('id',o.id);
		  			
		  			$(temp).attr('data-offer',JSON.stringify(o));
		  			$(temp).attr('data-query',$id+': '+$title);
		  			$(temp).attr('data-partner','prisguide');
		  			
		  			if(!$mouseDetected){
		  				$(temp).addClass('noMouse');
		  			}else{
		  				$(temp).addClass('mouse');
		  			}
		  			
		  			$(temp).find(".offerUrl").attr('href',o.shop.url);
		  			//$(temp).find(".offerTitle").html($title);
		  			$(temp).find(".offerTitle").html(o.title);
		  						
		  			$(temp).find(".offerPrice").html("kr "+o.price+",-");
		  			
		  			/*if(parseInt(o._source.price) < parseInt(o._source.price_before)){
		  				$(temp).find(".offerBeforePrice").html("kr "+o._source.price_before+",-");
		  			}*/

		  			var tempImg = String(data.image['300x300']);
		  			$(temp).find(".offerImage").css('background-image','url(' + tempImg + ')');
		  			
		  			$(temp).find(".offerMerchant").html(o.shop.name);
		  			
		  			//$(temp).find(".offerDescTitle").attr('title',offer.description);
		  			$('#offerContainer').append(temp);
				}
			);
		}
	});
}

function fetchOffers(){
	//console.log('start');
	$('.wayPoint').remove();
	Waypoint.destroyAll();
					
	if($searchReplace){
		$searchReplace = false;
		$('#offerContainer').html('');
	}
	
	if($('.offerLoading').length == 0){
		var temp = $('<span class="offerLoading"></span>');
		$('#offerContainer').append(temp);
		//console.log("Appended1");
		//throw new Error("Something went badly wrong!");
	}
	
	setUpEsClient2();
	
	if(!$offerSearchInProgress && !$offerSearchResultNoMore && $_offersActive){
		
		$offerSearchInProgress = true;
		
		var waypointElement = $('<div class="clearboth clearfix"><a id="wayPoint" class="wayPoint"></a></div>');
		//$('#offerContainer').after(waypointElement);					
		
		// Default sort
		var $dir = {order: $('#offerSortRules option:selected').attr('data-direction')};
		var $sort = {price_abs_reduction:$dir};
		
		switch($('#offerSortRules option:selected').val()){
			
			case 'price':
				$sort = {price:$dir};
			break;
			
			case 'beforePrice':
				$sort = {price_before:$dir};
			break;						
			
			case 'pctDiscount':
				$sort = {price_pct_reduction:$dir};
			break;
			
			case 'productName':
				$sort = {title:$dir};
			break;
		}
		
		$queryDateAdded = "now-350h/h";
		if($globalNew){
			$queryDateAdded = "now-24h/h";	
		}
								 
		var $offerSearchParams = {
			  index: 'offers',
					  body: {
					  	from: $resultOffset,
					  	size: $resultNum,
					    query:{
						    	bool:{
						    		must:[
