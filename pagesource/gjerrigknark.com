function offerOnlyDiscountsChangeLogic(el){
	if($(el).is(':checked')){
		$('#offerSortRulesDefault').attr('value','absDiscount');
		$('#offerSortRulesDefault').attr('data-direction','desc');
		$('#offerMinAbsDiscountDefault').attr('value','100');
		offersAdjustFilter('discountsOnly','true');
	}else{
		$('#offerSortRulesDefault').attr('value','price');
		$('#offerSortRulesDefault').attr('data-direction','asc');
		$('#offerMinAbsDiscountDefault').attr('value','0');
		offersAdjustFilter('discountsOnly','false');
	}
	resetSearch();
	fetchOffers();
}

function uncheckDiscountsOnly(){
	$('#offerOnlyDiscounts').prop('checked', false);
	offerOnlyDiscountsChangeLogic($('#offerOnlyDiscounts'));
}

