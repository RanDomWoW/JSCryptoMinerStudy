function offersToggleOnlyNew(){
	if($('#offersNewControlYes').is(":visible")){
		offersAdjustFilter('only_new','true');
		$globalNew = true;
		$('#offersNewControlYes').hide();
		$('#offersNewControlNo').fadeIn();
	}else{
		offersAdjustFilter('only_new','false');
		$globalNew = false;
		$('#offersNewControlNo').hide();
		$('#offersNewControlYes').fadeIn();
	}
	
	resetSearch();
	fetchOffers();
}

