<script>
var w;

function checksearch(){
	w = jQuery(window).width();
	navsearch = jQuery('#menu-main-navigation');
	navsearch.addClass('show');
	if (jQuery('#menu-main-navigation').css('display') == 'none' && parseInt(w) <= 768){
		navsearch.removeClass('show');
	}
	if (parseInt(w) <= 768){
		jQuery('.sub-menu').each(function(){
			jQuery(this).before('<i class="submenu_button fa fa-chevron-up"></i>');
		});
	}

	jQuery('.submenu_button').parent('li').off('click').on('click', function(){
		bt = jQuery(this).find('.submenu_button');
		if (bt.hasClass('fa-chevron-up')){
			bt.removeClass('fa-chevron-up').addClass('fa-chevron-down');
		}else{
			bt.removeClass('fa-chevron-down').addClass('fa-chevron-up');
		}
	}).on('click','a',function(e) {
		e.stopPropagation();
	});
}

jQuery(window).bind('resizeEnd', function() {
	checksearch();
});

 jQuery(window).resize(function() {
		if(this.resizeTO) clearTimeout(this.resizeTO);
		this.resizeTO = setTimeout(function() {
			jQuery(this).trigger('resizeEnd');
		}, 500);
	});


jQuery(document).ready(function($) {
	checksearch();
	jQuery('.menu-link').off('click').on('click', function(){
		jQuery(this).parent().find('.nav').removeClass('show').slideToggle(400,function(){
			checksearch();
		});
	});
});
</script>

<script>
    (function($){

        $(document).ready(function(){
            var $form = $('.signup-form');

            if ( $form.length > 0 ) {
                $form.find('form').on('submit', function ( event ) {
                    event.preventDefault();
                    register( $(this) );
                });
            };
        });
        function register($form) {
            $form.find('.submit-btn').blur();

            $.ajax({
                type        : $form.attr('method'),
                url         : $form.attr('action'),
                data        : $form.serialize(),
                cache       : false,
                dataType    : 'jsonp',
                contentType : "application/json; charset=utf-8",
                error       : function(err) {
                    console.log(err.responseText)
                },
                success     : function(data) {
                    console.log(data);
                    var $response;
                    if (data.result != "success") {
                        $response = $form.find('#mce-responses .response');
                        // Something went wrong, do something to notify the user. maybe alert(data.msg);
                        $response.html(data.msg.replace("0 - ", ""));

                        setTimeout(function() {
                            $response.parent().fadeOut();
                        }, 5000);
                    }
                    else {
                        $response = $form.find('#mce-thankyou .response');
                        // It worked, carry on...
                        $response.html(data.msg);
                    }
                    $response.parent().fadeIn(400);
                }
            });
        }
    })(jQuery);
</script>

