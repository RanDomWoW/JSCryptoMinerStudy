    (function ($) {
        $.fn.extend({
            donetyping: function (callback, timeout) {
                timeout = timeout || 500;
                var timeoutReference,
                    doneTyping = function (el) {
                        if (!timeoutReference) return;
                        timeoutReference = null;
                        callback.call(el);
                    };
                return this.each(function (i, el) {
                    var $el = $(el);
                    $el.is(':input') && $el.on('keyup keypress paste', function (e) {
                        if (e.type == 'keyup' && e.keyCode != 8) return;
                        if (timeoutReference) clearTimeout(timeoutReference);
                        timeoutReference = setTimeout(function () {
                            doneTyping(el);
                        }, timeout);
                    });
                });
            }
        });
    })(jQuery);
    $(document).click(function (event) {
        var target = $(event.target);
        if (target.parents('.SRL_Box').length == 0) {
            if (target.parents('#txtSearch').length == 0 && target.parents('.search-block').length == 0) { $('.SRL_Box').html(''); }
        }
    });
    $(document).ready(function () {
        $("#txtSearch").keypress(function (e) {
            if (e.which == 13) {
                DoSearch();
            }
        });
        $('#txtSearch').donetyping(function () {
            if ($("#txtSearch").val().length > 1) {
                lookUpSearch();
            }
        });
        $("#lookup_close_btn").click(function () {
            $('#txtSearch').removeClass("lookup_txt").val("");
            $("#lookup_frame").removeClass("lookup_frame").addClass("disNone");
            $("#lookup_close_btn").removeClass("lookup_close_btn").addClass("disNone");
            $(".search-block").removeClass("lookup_search_block");
        })
        $('#txtSearch').click(function () {
            if (isMobile || $(window).width() <= 768) {
                $("#hf_search_lookup_view").val("2");
                $(this).addClass("lookup_txt");
                $("#lookup_frame").removeClass("disNone").addClass("lookup_frame");
                $("#lookup_close_btn").removeClass("disNone").addClass("lookup_close_btn");

                if (navigator.userAgent.match(/(iPod|iPhone|iPad)/))
                {
                    $(".search-block").addClass("lookup_search_block_iphone");
                }
                else
                {
                    $(".search-block").addClass("lookup_search_block");
                }
            }
            else { $("#hf_search_lookup_view").val("1"); }
        });
        $('#txtSearch').on('keyup', function () {
            if (isMobile || $(window).width() <= 768) {
                if ($("#txtSearch").val().length > 1) {
                    lookUpSearch();
                }
            }
        });
    })
    function lookUpSearch() {
        var _S = $("#txtSearch").val()
            .replace('ي', 'ی')
            .replace('ك', 'ک')

        ;
        //.replace('ابز', 'ابز')
//        .replace('ﺯ', 'ز')
//.replace('ا', 'ا')
//.replace('ب', 'ب')
//.replace('ز', 'ز')
//.replace('ﺭ', 'ر')
        $.ajax({
            url: "/Pages/AJX_SearchMenu_LookUp.aspx?S=" + _S + "&view=" + $("#hf_search_lookup_view").val()
        }).done(function (data) {
            var view=$("#hf_search_lookup_view").val(); 
            if (view == "1")
            {
                $('.SRL_Box').html('');
                $('.SRL_Box').html($(data).find("#lookup_desktop").html());
                $('.SRL_Box').show();
            }
            else if (view == "2")
            {
                $('.SRL_Box_M').html('');
                $('.SRL_Box_M').html($(data).find("#lookup_mobile").html());
                $('.SRL_Box_M').show();
            }

        });
    }
    function DoSearch() {
        var txtSearch = $("#txtSearch");
        if (txtSearch.val())
        { window.location.href = "/Search?S=" + txtSearch.val(); }

    }
