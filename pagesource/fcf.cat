					<script>
						$(document).ready(function(){	
							$(".lightsoff").click(function() {
								$('.lightsoff').removeClass('lightson');
								$('.box-options').removeClass('box-options-on');
							});
							$('input').click(function() {
								var name = $(this).attr('name');
								switch(name) {
									case 'categoria':
										if($('input[name="disciplina"]').val() === ''){
											name = 'disciplina';
										}
										break;
									case 'grup':
										if($('input[name="categoria"]').val() === ''){
											name = 'categoria';
										}
										if($('input[name="disciplina"]').val() === ''){
											name = 'disciplina';
										}
										break;
								}
										
								if($('.box-'+name).hasClass('box-options-on')){
									$('.box-'+name).removeClass('box-options-on');
									$(".lightsoff").removeClass("lightson");
									$(this).blur();
								} else{
									$('input[name="'+name+'"]').focus();
									$('.box-options').removeClass('box-options-on');
									$('.box-'+name).addClass('box-options-on');
									$('input[name="'+name+'"]').focus();
									//$(this).nextAll().eq(4).addClass('box-options-on');
									$(".lightsoff").addClass("lightson");
								}
							});
							$(document).on('click', '.box-options > div', function(){
								var valor = $(this).attr('target');
								var url = $(this).attr('data-url');
								var name = $(this).parent().attr('class');
								name = name.replace('box-options', '').replace('box-options-on', '').replace('box-', '').trim();
								if(valor !== '' || typeof valor !== "undefined" || name !== ''){
									$('input[name="'+name+'"]').attr('data-value', valor);
									$('input[name="'+name+'"]').attr('value', $('span', this).text());
									$('input[name="'+name+'"]').attr('data-url', url);
									$('.lightsoff').removeClass('lightson');
									$('.box-options').removeClass('box-options-on');
									switch(name) {
										case 'disciplina':
											disciplina = $(this).attr('target');
											if(disciplina !== ''){
												$.ajax({
													type: "POST",
													url: "portada/categories",
													data: { disciplina: disciplina }
												})
												.done(function( msg ) {
													if(msg !== ''){
														$('.box-categoria').html(msg)
													} else{
														$('input[name="disciplina"]').click();
													}
												});
											}
											break;
										case 'categoria':
											$('.box-grup').html('');
											var idioma = "ca";;
											var disciplina_url = $('input[name="disciplina"]').attr('data-url');
											var categoria_url = $('input[name="categoria"]').attr('data-url');
											var tipus = $(this).attr('data-tipus');
											var categoria = $(this).attr('target');
											$.ajax({
												type: "POST",
												url: "portada/grups",
												data: { idioma: idioma, disciplina_url: disciplina_url, categoria_url: categoria_url, tipus: tipus, categoria: categoria }
											})
											.done(function( msg ) {
												if(msg !== ''){
													$('.box-grup').html(msg);
												} else{
													$('input[name="categoria"]').click();
												}
											});
											
											break;
									}
								}
							});
							$('#borrar-inputs-portada').click(function(){
								$('input').each(function(){
									$(this).attr('value', '');
									$(this).attr('data-url', '');
									$(this).attr('data-value', '');
								});
								$('.box-categoria').html('');
								$('.box-grup').html('');
							});
						});
					</script>







