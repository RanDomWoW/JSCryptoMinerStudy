<script type="text/javascript">
	function joinMailingList() {
		if (!(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,4})+$/.test($('#lightbox #email_address').val()))){
			alert("Please enter a valid email address");
        		return false;
        	}
        	
        	$('#login-box .state').hide();
        	$('#login-box #state2').show();
        	
		$.ajax({
			type: "POST",
			url: "/index.pl",
			data: {
				isa: "Metadot::SystemApp::ManageMailingLists",
				op: "subscribe",
				id: "272",
				name: "",
				single: $('#lightbox #email_address').val()
			},
			success: function(content) {
				if(content == '0') {
			        	$('#login-box .state').hide();
			        	$('#login-box #state4').show();
			        } else {
			        	$('#login-box .state').hide();
			        	$('#login-box #state3').show();
			        }
			}
		});
	}
</script>

