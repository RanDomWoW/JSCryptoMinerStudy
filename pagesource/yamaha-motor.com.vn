		$("#carouselBike").swipe( {
			swipeStatus:function(event, phase, direction, distance, duration, fingers, fingerData, currentDirection) {
				if($(window).width() < 768) {
					if(phase == 'cancel') {
						resetPosition()
						return false;
					}
					if(direction == 'right' || direction == 'left') {
						var x = 0;
						if(direction == 'right') x = distance;
						else x = -distance;

						if(distance < 450) {
							$(".eachMotobike[data-position=2]").css({
								'transform': 'translate('+x+'px, 0)',
							})
							if(direction == 'right') {
								$(".eachMotobike[data-position=3]").css({
									'transform': 'translate('+(-x * 2)+'px, 0)',
								})
								$(".eachMotobike[data-position=1]").css({
									'transform': 'translate('+x+'px, 0)',
								})
							} else {
								$(".eachMotobike[data-position=3]").css({
									'transform': 'translate('+x+'px, 0)',
								})
								$(".eachMotobike[data-position=1]").css({
									'transform': 'translate('+(-x * 2)+'px, 0)',
								})
							}
						}

						if(event.type == 'mousemove' || event.type == 'touchmove') {
							$(".eachMotobike").removeClass('center');
						}

						if(event.type == "mouseup" || event.type == "touchend" || phase == 'end') {
							if(direction == 'right') prevBike();
							else nextBike();
						}
					}
				}
			},
			threshold: 10,
			triggerOnTouchEnd: false,
			allowPageScroll: "vertical",
		});
		function resetPosition() {
