<script type="text/javascript">
$(window).scroll(function(){
	var sT = $(window).scrollTop();
	var vW = $(window).width();
	if(vW > 767) {
		if(sT >= 50) {
			$("body").addClass("fixedHeader");
		} else {
			$("body").removeClass("fixedHeader");
		}
	} else {
		if(sT >= 65) {
			$("body").addClass('hideTop');
			$(".header .top").slideUp(150);
			$(".logoArea .logo").css({padding: '10px 0'})
			$(".logoArea .container").css({height: '70px'})
			// $(".logoArea form").css({padding: '20px 0'})
			$(".btnShowgNavi").css({top: '5px'})
			$("#clickToShowSearch").css({padding: '17px 0'})
			$(".mainMenu.gNavi").css({top: '69px'})
		} else {
			$("body").removeClass('hideTop');
			$(".header .top").slideDown(150);
			$(".logoArea .logo").removeAttr('style')
			$(".logoArea .container").removeAttr('style')
			// $(".logoArea form").removeAttr('style')
			$(".btnShowgNavi").removeAttr('style')
			$("#clickToShowSearch").removeAttr('style');
			$(".mainMenu.gNavi").css({top: '139px'})
		}
	}

	var footerHeight = $("#section-footer").outerHeight();
	if(sT >= $(document).height() - (footerHeight + $(window).height()) + 180) {
		bottom = 260;
		if(vW <= 1080) bottom = 305;
		$(".iconFixFooter").css({
			'position': 'absolute',
			'bottom': bottom,
		});
	} else {
		$(".iconFixFooter").attr('style', '');
	}
})
$(".goToTop").click(function(){
	$("html, body").animate({scrollTop: 0}, 300);
	return false;
})
$(".disable").click(function(){
	return false;
})
$(".logoArea form button").click(function(){
	if($(window).width() < 1025) {
		if(!$(".logoArea form").hasClass('show')) {
			$(".logoArea form").addClass('show')
			return false;
		}
	}
})
$(".btnShowgNavi").click(function(){
	$("body").toggleClass('showGNavi');
	$(".mainMenu.gNavi").slideToggle();
})
$(".icoPlus").click(function(){
	$(this).parent().toggleClass('showChild');
	$(this).next().slideToggle();
})
$("span.h2 .clickShowChild").click(function(){
	$(this).parent().parent().toggleClass('showChild');
	$(this).parent().next().next().slideToggle();
	return false;
})


$("#clickToShowSearch, .logoArea form button").click(function(){
	if($(window).width() < 1281) {
		if(!$("body").hasClass('showSearch')) {
			$("body").addClass('showSearch');
			setTimeout(function(){
				$(".logoArea form input").focus()
			},100)
			return false;
		} else {
			$("body").removeClass('showSearch');
		}
	}
	$("#formSearchTop").submit();
})
</script>

<script>
		function createCookie(name, value, hours) {
			if (hours) {
				var date = new Date();
				date.setTime(date.getTime() + (hours * 60 * 60 * 1000));
				var expires = "; expires=" + date.toGMTString();
			} else var expires = "";
			document.cookie = escape(name) + "=" + escape(value) + expires + "; path=/";
		}
		
		
		function readCookie(name) {
			var nameEQ = escape(name) + "=";
			var ca = document.cookie.split(';');
			for (var i = 0; i < ca.length; i++) {
				var c = ca[i];
				while (c.charAt(0) == ' ') c = c.substring(1, c.length);
				if (c.indexOf(nameEQ) == 0) return unescape(c.substring(nameEQ.length, c.length));
			}
			return null;
		}
		
		
		function eraseCookie(name) {createCookie(name, "", -1);}
		
		function listCookies() {
			var theCookies = document.cookie.indexOf('compare').split(';');
			var aString = '';
			for (var i = 1 ; i <= theCookies.length; i++) {
				aString += i + ' ' + theCookies[i-1] + "\n";
			}
			return aString;
		}
		
		
		
		$(function(){
			for(var i = 1; i < 100; i++) {
				if (document.cookie.indexOf('compare_xe'+ i + '_') >= 0) {
					$('#moto_com_' + i).toggleClass('disable');
					$('#moto_com_' + i).text('ĐÃ CHỌN');
				}
			}
					
			$(".btnCompare").click(function(){
				$('#id_moto').text('');
				$('.compareBar').animate({bottom: "-100px"},300);
				var count = 0;
				
				/* effect after click */
				$(this).toggleClass('disable');
				$(this).text('ĐÃ CHỌN');
				$('.compareBar').animate({bottom: "0"},300);
				
				/* Get attribute element */   
				var id_moto = $(this).attr('data-numb');
				var name_moto = $(this).attr('data-title');
					
					if (document.cookie.indexOf('compare_'+ id_moto) >= 0) {
							$('#id_moto').append('<span>' + name_moto + '</span>' + ' đã được xoá khỏi danh sách so sánh');
							$(this).removeClass('disable');
							$(this).text('SO SÁNH XE');
							eraseCookie('compare_'+ id_moto, "", -1);
							
					} else {
						var test = Number(readCookie('count_moto'));
																		var limit = 3;
												if(test < limit) {
							createCookie('compare_'+ id_moto ,id_moto , 2);
							$('#id_moto').append('<span>' + name_moto + '</span>' + ' vừa được thêm vào danh sách so sánh');	
						} else {
							$('#id_moto').append('Chỉ được tối đa '+ limit +' xe');
						}
					}
					for(var i = 1; i < 10000; i++) {
						if (document.cookie.indexOf('compare_xe'+ i + '_') >= 0) {
							count ++;
							console.log(i)
						}
					}
					createCookie('count_moto',count, 24);
					
				/* after click */
				/* setTimeout(function() {
					$('.compareBar').animate({top: "-100px"},300);
				}, 2000);
				*/
				// setTimeout(function() {
				// 	$('#id_moto').text('');
				// }, 800);
				return false;
			});
			
			$(".closeBar").click(function(){
				$('.compareBar').animate({bottom: "-200px"},300);
			});	
			
		});
</script>
