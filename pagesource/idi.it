/*-------Typing effect MARQUEE---------- */
/*---------http://www.hongkiat.com/blog/css3-animation-advanced-marquee/------------------------------*/

.marquee {
  width: 80%;
  height: 50px;
  margin: 15px auto;
  overflow: hidden;
  position: relative;
  border: 1px solid #000;
  background-color: #222;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  -webkit-box-shadow: inset 0px 2px 2px rgba(0, 0, 0, .5), 0px 1px 0px rgba(250, 250, 250, .2);
  box-shadow: inset 0px 2px 2px rgba(0, 0, 0, .5), 0px 1px 0px rgba(250, 250, 250, .2);
}

.marquee p {
  position: absolute;
  font-family: Tahoma, Arial, sans-serif;
  font-size: bold;
  width: 100%;
  height: 100%;
  margin: 0;
  line-height: 50px;
  text-align: center;
  color: #fff;
  text-shadow: 1px 1px 0px #000000;
  filter: dropshadow(color=#000000, offx=1, offy=1);
}

.marquee p {transform:translateX(100%);}
.marquee p:nth-child(1) {animation: left-one 60s ease  infinite;}
.marquee p:nth-child(2) {animation: left-two 60s ease   infinite;}
.marquee p:nth-child(3) {animation: left-three 60s ease   infinite;}
.marquee p:nth-child(4) {animation: left-four 60s ease   infinite;}
.marquee p:nth-child(5) {animation: left-five 60s ease   infinite;}


@keyframes left-one { 
  0%  {transform:translateX(100%); }
 5% {transform:translateX(0); }
 15% {transform:translateX(0); }
 16%{transform:translateX(-100%);  }
 100% {transform:translateX(-100%);  }

}
@keyframes left-two {
    0% {transform:translateX(100%); }
  25% {transform:translateX(100%);  }
  25% {transform:translateX(0); }
 35% {transform:translateX(0); }
 100%{transform:translateX(-100%); }
}
@keyframes left-three {
    0% {transform:translateX(100%); }
  40% {transform:translateX(100%);  }
  40% {transform:translateX(0); }
  50% {transform:translateX(0); }
 100%{transform:translateX(-100%); }
}
@keyframes left-four {
    0% {transform:translateX(100%); }
  55% {transform:translateX(100%);  }
  55% {transform:translateX(0); }
  65% {transform:translateX(0); }
 100%{transform:translateX(-100%); }
}
@keyframes left-five {
    0% {transform:translateX(100%); }
  70% {transform:translateX(100%);  }
  70% {transform:translateX(0); }
  80% {transform:translateX(0); }
 100%{transform:translateX(-100%); }
}

