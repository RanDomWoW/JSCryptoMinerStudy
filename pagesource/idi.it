    .menurapidotop {font-size: 90%;}
    #menurapido {font-size: 90%;}
    /*----- slogan */
    #homeslogan, #homeflag, #homeidifarm {display:none;}
    /*----- titoli pagina */  
    h1, .h1 {font-size: 70%;}
    h2, .h2 {font-size: 70%;}
    /*----- blockquote */     
    .blockquote, blockquote {font-size: 130%;}
    /*----- articoli home */     
    .contentPopup, contentPopup {font-size: 70%;}
    /* ========== area slider */
    .sfondo-slider {display: none;}
    .cycle-slideshow{display: none;}
    /* ========== menu */    
    .ccm-responsive-overlay {
      background: #ffffcc;
      display: none;
      position: absolute;
      border: 2px solid rgb(217, 218, 220);
      width: 100%;
      z-index: 99;
      top: 270px;
      left: 0px;
      padding-top: 15px;
      padding-left: 20px;  
    }
    /* --- news ed eventi --- */
    .dsListEvents > ul > li {font-size: 80%;}
    
    /* --- tabella icone home page ---  */   
    table.table_icone {
       	width: 100%;
	    font-size: 50%;
	    img-resize: 50%;
    }
    /* --- testo in basso all home per progetti regionali ---  */       
    .testoresize {
    font-size: 70%;    
    }
    

