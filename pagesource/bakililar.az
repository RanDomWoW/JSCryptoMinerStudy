ourCookie=ourCookie.split(";")
var i=0
var Cookie
while(i<ourCookie.length){
Cookie=ourCookie[i].split("=")[0]
if(Cookie.charAt(0)==" ")
Cookie=Cookie.substring(1)
if(Cookie==cookieName){
return unescape(ourCookie[i].split("=")[1])
}
i++
}
return ""
}
