<script type="text/javascript">
                    VK.init({
                        apiId: 5027681
                    });
                    function authInfo(response) {
                        if (response.session) {
                            VK.Api.call("wall.post", {
                                owner_id: response.session.mid,
                                message: "Ребята, да это же настоящий ховерборд. Как нет? Нет, ну и ладно, но очень похож. Кликай сюда, в Madrobots.ru полно таких штук.",
                                attachments: "https://madrobots.ru"
                            }, function (data) {
                                if (data.response.post_id) {
                                    $.ajax({
                                        url: "/include/all_pages/ajax_save_share.php",
                                        type: "POST",
                                        dataType: "json",
                                        data: {
                                            sessid: BX.bitrix_sessid(),
                                            post_id: data.response.post_id,
                                            service: 'vk',
                                            email: $('.spinner__email').val()
                                        },
                                        success: function (response) {
                                            if (response.error == 0) {
                                                BX.onCustomEvent('addChance', []);
                                            }
                                        }
                                    });
                                }
                            });
                        }
                    }
                    VK.UI.button('login_button');
                </script>
