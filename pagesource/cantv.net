function checkFormCabecera(form){
checkResult = true;
if ((form.Username.value.length==0) || (form.Password.value.length==0)) {
alert('Todos los campos deben contener algun valor. Por favor complete los datos.');
checkResult = false;
}else {
if ((form.Username.value.length>25) || (form.Password.value.length>25)) {
alert('Los campos Login o Password exceden el número de caracteres permitidos. Por favor corríjalo e intente nuevamente.');
checkResult = false;
}else {
//Formato de la expresion regular: /EXPRESION-REGULAR/OPCIONES
//Las opciones pueden ser g(global match), i(case insensitive) o 'gi' case-insensitive global match
//La expresion regular de abajo hará match con todo lo que no sea letra, número,-,_,.
var badFormat = form.Username.value.match("[^-\.a-zA-Z0-9_]", "i");
if (badFormat){alert('El campo Login tiene caracteres inválidos. Por favor corríjalo e intente nuevamente.');
checkResult = false;
}}}
if (checkResult ) form.aceptar.disabled = true
return checkResult;}
