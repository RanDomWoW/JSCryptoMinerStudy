    $('.btnConfigHapilHide').on('click', function() {
        if($(this).find(".on_off").text() == "OFF")
        {
            swal('성공', '합필갤 BGM이 검색결과에서 나타나도록 설정하였습니다.', 'success');
            $(this).find(".on_off").text("ON");
            $.removeCookie('config_hapil_hide', {path: '/'});
            $.cookie('config_hapil_hide', 'N', {expires: 365, path: '/', domain: '.bgmstore.net'});
            $('.categories').each(function() {
                if($(this).text() == '합필갤')
                    $(this).show();
            });
        } else {
            swal('성공', '합필갤 BGM이 검색결과에서 제외되도록 설정하였습니다.', 'success');
            $(this).find(".on_off").text("OFF");
            $.removeCookie('config_hapil_hide', {path: '/'});
            $.cookie('config_hapil_hide', 'Y', {expires: 365, path: '/', domain: '.bgmstore.net'});
            $('.categories').each(function() {
                if($(this).text() == '합필갤')
                    $(this).hide();
            });
        }
    });



$('.deleteReasonList').change(function() {
