		$('#promptModal #promptText2').val(bgmEmbedTag2);
		$('#promptModal #promptText2').parent().show();
	}
	$('#promptModal').modal();
});
$('#promptText0,#promptText1,#promptText2').on('click', function (e) {
	$(this).focus().select();
});





$limit_count = 1;
$q_val = $('#q_val').text();
$q_type_val = $('#q_type_val').text();
$q_mode_val = $('#q_mode_val').text();
$sort_by = $('#sort_by').text();
$sort_type = $('#sort_type').text();
$member_num = $('#member_num').text();

$('.btnMoreBgm').on('click', function(e) {
	e.preventDefault();

	var l = Ladda.create(this);
 	l.start();

	$.ajax({
		url     : "/servlet/more_bgm",
		type    : "POST",
		data    : "limit_count="+$limit_count+"&q="+$q_val+"&q_type="+$q_type_val + "&sort_by=" + $sort_by + "&sort_type=" + $sort_type + "&member_num=" + $member_num + "&q_mode=" + $q_mode_val + "&rit="+ $('#rit_common').val(),
		global  : false,
		success : function(result){
			$limit_count = $limit_count + 1;
			var $returned_val = jQuery.parseJSON(result);
			$('.search_result').last().after($returned_val['html_data']);
			if($returned_val['result_count'] < 30) {
				$('.btnMoreBgm').parent().hide();
			}
		},
		error: function(XMLHttpRequest, textStatus, errorThrown) {
	        console.log("Status: " + textStatus + ", Error: " + errorThrown);
	    },
		complete : function() {
			l.stop();
		}
	});
	return false;
});

isPlaying = false;
audioError = false;
$(document).on('click', '.btnPlay:not(.mp3Play)', function(e) {
	audioError = false;
	e.preventDefault();
	curBtnObj = $(this);
	curBgmEmbedAreaObj = curBtnObj.parents('.bgmInfo').find('.bgmEmbedArea');

	if($('.random_autonext_player').length)
	{
		$('.random_autonext_player')[0].pause();
	}

	if($('#audio1').length)
	{
		$('#audio1')[0].pause();
	}

	playHtml = '<span class="glyphicon glyphicon-play"></span>';
	stopHtml = '<span class="glyphicon glyphicon-stop"></span>';
	bgmKey = curBtnObj.parents('.bgmInfo').attr('data-key');
	bgmUrl = "https://player.bgmstore.net/" + bgmKey + "/mp4";
	bgmEmbedTag = '<iframe src="' + bgmUrl + '" width="100%" height="240" frameborder="0"></iframe>';

	if(typeof(exBtnObj) == 'undefined')
		exBtnObj = curBtnObj;

	$('.bgmEmbedArea').not(curBgmEmbedAreaObj).html('').height(0);
	$('.btnPlay').not(curBtnObj).html(playHtml);

	if(curBtnObj.is(exBtnObj)) {
		//처음 재생 버튼을 누를때
		if(curBgmEmbedAreaObj.height() == 0) {
			curBgmEmbedAreaObj.height(240).html(bgmEmbedTag);
			curBtnObj.html(stopHtml);
		//현재 재생되고 있는 bgm을 다시 누를때, 즉 끌때
		} else {
			curBgmEmbedAreaObj.html('').height(0);
			curBtnObj.html(playHtml);

			if($('.random_autonext_player').length)
			{
				$('.random_autonext_player')[0].play();
			}
		}
	} else {
		//처음버튼 누르고 다른 버튼 재생 누를때
		curBgmEmbedAreaObj.height(240).html(bgmEmbedTag);
		curBtnObj.html(stopHtml);
	}

	exBtnObj = curBtnObj;
});


});
</script>











