$('body').on('click', '.btnDeleteRequestTrigger', function(e) {
	e.preventDefault();
	$('#deleteRequestModal').modal();
	var documentKey = $(this).parents('.bgmInfo').attr('data-key');

	$('#deleteRequestModal').attr('data-document-key', documentKey);
	$('#deleteRequestModal .deleteReasonList').val('');
	$('#deleteRequestModal #deleteRequestContent').val('');
});

$('body').on('click', '.btnDeleteRequest', function(e) {
	e.preventDefault();

	var documentKey = $('#deleteRequestModal').attr('data-document-key');
	var deleteReason = $('.deleteReasonList').val();
	var deleteRequestContent = $('#deleteRequestContent').val();

	if(deleteReason == '')
	{
		swal('오류', '삭제요청 사유를 선택해주세요.', 'warning');
		return;
	}
	/*alert(documentKey);
	alert(deleteReason);*/
	$.ajax({
		url     : "/servlet/delete_request",
		type    : "post",
		data    : "document_key=" + documentKey + "&delete_reason=" + deleteReason + "&content=" + deleteRequestContent + "&rit="+ $('#rit_common').val(),
		global  : false,
		success : function(result){
			var $returned_val = jQuery.parseJSON(result);
			swal('메시지', $returned_val['msg'], 'info');
			$('#deleteRequestModal').modal('hide');
		}
	});
});




$(document).on("click", '.btnEmbedShareText', function(e) {
	e.preventDefault();
	var bgmNum = $(this).parents('.bgmInfo').attr('data-num');
	var bgmKey = $(this).parents('.bgmInfo').attr('data-key');
	var bgmUrl0 = "https://player.bgmstore.net/" + bgmKey + "/mp4";
	var bgmUrl1 = "https://player.bgmstore.net/" + bgmKey;
	var bgmUrl2 = "https://player.bgmstore.net/" + bgmKey;
	var bgmEmbedTag0 = '<embed src="' + bgmUrl0 + '" autostart="true" allowscriptaccess="always" enablehtmlaccess="true" allowfullscreen="true" width="422" height="240" type="video/mp4"></embed><br><a href="http://bgmstore.net/view/' + bgmKey + '" target="_blank">BGM정보 : 브금저장소 - http://bgmstore.net/view/' + bgmKey + '</a>';
	var bgmEmbedTag1 = '<iframe src="' + bgmUrl0 + '" width="422" height="240" frameborder="0"></iframe><br><a href="http://bgmstore.net/view/' + bgmKey + '" target="_blank">BGM정보 : 브금저장소 - http://bgmstore.net/view/' + bgmKey + '</a>';
	var bgmEmbedTag2 = '<embed src="' + bgmUrl2 + '" autostart="true" allowscriptaccess="always" enablehtmlaccess="true" allowfullscreen="true" width="422" height="240" type="video/mp4"></embed><br><a href="http://bgmstore.net/view/' + bgmKey + '" target="_blank">BGM정보 : 브금저장소 - http://bgmstore.net/view/' + bgmKey + '</a>';

	$('#promptModal #promptText0').val(bgmEmbedTag0);
	$('#promptModal #promptText1').val(bgmEmbedTag1);
	if(bgmNum > 273072) {
		$('#promptModal #promptText2').parent().hide();
