<script>
/**	Gestion du form de recherche: redirection vers location ou achat **/
	jQuery(document).ready(function() {
		generatePricesRange();

	setTimeout(
		function() 
		{

			//location
			jQuery("select[name='filter_location']").val("").trigger("liszt:updated");
			//typeBien
			jQuery("select[name='filter_type']").val("").trigger("liszt:updated");
			//Pièces
			jQuery("select[name='filter_rooms']").val("").trigger("liszt:updated");
			//surface min
			jQuery("select[name='filter_surface_from']").val("").trigger("liszt:updated");
			//surface max
			jQuery("select[name='filter_surface_to']").val("").trigger("liszt:updated");
			//prix min
			jQuery("select[name='filter_price_from']").val("").trigger("liszt:updated");
			//prix max
			jQuery("select[name='filter_price_to']").val("").trigger("liszt:updated");
			jQuery("input[name='filter_ref']").val("");
		}, 100);

		if(window.location.pathname.indexOf("locations") >= 0) {
			jQuery('input[value=rent]').attr('checked', true);
			jQuery('input[value=sale]').removeAttr('checked');
			jQuery('#advanced_research').attr('action', '/locations');
			generatePricesRange();
		}
		else {
			jQuery('input[value=sale]').attr('checked', true);
			jQuery('input[value=rent]').removeAttr('checked');
			jQuery('#advanced_research').attr('action', '/achats');
			generatePricesRange();
		}

		jQuery('input[value=rent]').click(function() {
			jQuery('input[value=sale]').removeAttr('checked');
			jQuery('input[value=sale]').parent().removeClass('ez-checked');
			jQuery('input[value=rent]').attr('checked', true);

			jQuery('#advanced_research').attr('action', '/locations');
			generatePricesRange();
		});
		jQuery('input[value=sale]').click(function() {
			jQuery('input[value=sale]').attr('checked', true);
			jQuery('input[value=rent]').removeAttr('checked');
			jQuery('input[value=rent]').parent().removeClass('ez-checked');

			jQuery('#advanced_research').attr('action', '/achats');
			generatePricesRange();
		});
	});

	function generatePricesRange() {
		//LOCATION
		if(jQuery('input[value=rent]').is(':checked')) {
			html = ' \
                        <option value="">Prix minimum</option> \
						<option value="200">200 €</option> \
                        <option value="300">300 €</option> \
                        <option value="500">500 €</option> \
                        <option value="1000">1,000 €</option> \
                        <option value="1500">1,500 €</option> \
                        <option value="2000">2,000 €</option> \
                        ';
			jQuery('select[name="filter_price_from"]').html(html).trigger("liszt:updated");

			html = ' \
                        <option value="">Prix maximum</option> \
                        <option value="400">400 €</option> \
                        <option value="600">600 €</option> \
                        <option value="800">800 €</option> \
                        <option value="1000">1,000 €</option> \
                        <option value="2500">2,500 €</option> \
                        <option value="3500">3,500 €</option> \
                        ';
			jQuery('select[name="filter_price_to"]').html(html).trigger("liszt:updated");
		}

		//ACHAT
		if(jQuery('input[value=sale]').is(':checked')) {
			html = ' \
                        <option value="">Prix minimum</option> \
						<option value="10000">10,000 €</option> \
                        <option value="20000">20,000 €</option> \
                        <option value="50000">50,000 €</option> \
                        <option value="100000">100,000 €</option> \
						<option value="500000">500,000 €</option> \
                        ';
			jQuery('select[name="filter_price_from"]').html(html).trigger("liszt:updated");

			html = ' \
                        <option value="">Prix maximum</option> \
                        <option value="20000">20,000 €</option> \
                        <option value="50000">50,000 €</option> \
                        <option value="100000">100,000 €</option> \
                        <option value="200000">200,000 €</option> \
                        <option value="500000">500,000 €</option> \
                        <option value="1000000">1,000,000 €</option> \
						<option value="2000000">2,000,000 €</option> \
                        ';
			jQuery('select[name="filter_price_to"]').html(html).trigger("liszt:updated");
		}
		
	}
</script>

