    <script type="text/javascript">
        $(window).load(function () { $("#divPreloader").hide(); });
        $(document).ready(function () {
            timeNow();
            setInterval(function () { timeNow(); }, 2000);
        });

        function timeNow() {
            var sign = 'AM';
            var date = new Date();
            var hour = parseInt(date.getHours());
            var minute = parseInt(date.getMinutes());
            if (hour == 0) {
                hour = 12;
            }
            if (hour > 12) {
                sign = 'PM';
                hour = hour - 12;
            }
            if (hour <= 9) {
                hour = '0' + hour.toString();
            }
            if (minute <= 9) {
                minute = '0' + minute.toString();
            }
            $('#divDateTime').text(hour + ':' + minute + ' ' + sign);
        }
    </script>
    
