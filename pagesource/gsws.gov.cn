	<script>
	//检测输入的文本是否含有非法字符
	
	//规则
	var specialCharArr = new Array();
	$(function(){
		specialCharArr = new Array("%22","&#x","@", "%27","+","\\","$", "%28", "%2B", "%2F", "%3C", "%3D", "%3E","ß'","wangfo(", 
				"\\'", "script", "alert","prompt", "javascript", "__.", "/..", "\\..", "/**", "\\*", "DECLARE", "<iframe", "http://",
				"Content-Type:", "window.open", "onload=","xwork2","ProcessBuilder","window[","autofocus","><IMG","><A","():;",
				"onreadystatechange=","select","delete","update","count","*","insert","|","||","and","like","where","from","=","or",
				"onmouseover","eval(","onclick","new Function");
	})
	
	function checkStr(str){
		var res = false;
		for(var i = 0;i < specialCharArr.length; i++){
			if(contains(str.toUpperCase(),specialCharArr[i].toUpperCase())){
				res = true;
				break;
			}
		}
		return res;
		
	}
	
	function contains(str,param){
		
		if(str == null || param == null){
			return false;
		}
		var result = str.indexOf(param) >= 0;
        return result;
	}

	function search(){
			var v = document.getElementById("key").value;
			var q = document.getElementById("searchType").value;
			var searchKey = encodeData(v); 
			searchKey= 	encodeURI(encodeURIComponent(searchKey));
		
			var searchType=encodeData(q);
			searchType=encodeURI(encodeURIComponent(searchType));

			if (/^\s*$/.test(v)) {
				return false;
			}else{
				var result = checkStr(v);
				if(result){
					alert('请勿输入非法字符');
				}else{
					window.location = "http://www.gsws.gov.cn/searchSite.jsp?key=" +searchKey+"&type="+searchType;
				}
			}
		}

			
</script>
