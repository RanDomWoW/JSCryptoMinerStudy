    <script>
        img_slide_width = 2880;
        slide_num = 1;
        slide_amount = 5;
        slider_busy_flag = false;
        function resize_main_info_block() {
            window_width =  $(document).width();
            window_width_for_texts = window_width<1400?window_width:1440;
//            if($(window).width()<1400&&$(window).width()>960) {
//                koef = 1400-$(window).width();
//                $(".banners").width(700-koef+"px");
//                $(".banners .photo").width(350-koef+"px");
//                if((380-koef)>15)
//                    margin_left = 380-koef;
//                else
//                    margin_left = 15;
//                $(".banners .right .small").css("marginLeft",margin_left+"px");
//                $(".banners .right .info").css("marginLeft",margin_left+"px");
//            }
            $(".main_slider .bottom_part").width(window_width);
            $(".main_slider .top_part").width(window_width);
            $(".main_slider>div").css("left",-img_slide_width+"px");
            $(".main_slider .slide1").css("left","0px");
            $(".main_slider .slide_buttons").css("left",(window_width-window_width_for_texts)/2+window_width_for_texts*0.75+"px");
            $(".main_slider .slide_buttons_sections").css("left","50%");
            $(".main_slider_texts").css("marginLeft",-window_width_for_texts/2+"px");
            $(".main_slider_texts>div").width(window_width_for_texts);
            $(".main_slider_texts>div>div").css("left",window_width+"px");
            $(".main_slider_texts .slide_text1 div").css("left","0px");
        }

        $(document).ready(function() {
            $(".slide_right").click(function() {
                if(!slider_busy_flag)
                    next_slide();
            });
            $(".slide_left").click(function() {
                if(!slider_busy_flag)
                    prev_slide();
            });
            resize_main_info_block();
            $(window).resize(function() {
                resize_main_info_block();
            });
            setInterval("next_slide()", 7000);
        });
        function next_slide() {
            slide_num++;
            if(slide_num>slide_amount)
                slide_num = 1;
            next_slide_num = slide_num+1;
            if(next_slide_num>slide_amount)
                next_slide_num = 1;
            prev_slide_num = slide_num-1;
            if(prev_slide_num==0)
                prev_slide_num = slide_amount;
            chabge_slide();
        }
        function prev_slide() {
            slide_num++;
            if(slide_num>slide_amount)
                slide_num = 1;
            next_slide_num = slide_num+1;
            if(next_slide_num>slide_amount)
                next_slide_num = 1;
            prev_slide_num = slide_num-1;
            if(prev_slide_num==0)
                prev_slide_num = slide_amount;
            chabge_slide();
        }
        function chabge_slide() {
            slider_busy_flag = true;
            $(".slide_text"+prev_slide_num).find(".first").animate({"left":window_width+"px"},500);

            $(".slide_text"+slide_num).find(".first").animate({"left":"0px"},500,function() {
                $(".slide_text"+slide_num).find(".first").css("zIndex","5");
                $(".slide_text"+next_slide_num).find(".first").css("left",window_width+"px").css("zIndex","1");
            });
            $(".slide_text"+prev_slide_num).find(".second").animate({"left":window_width+"px"},100);
            $(".slide_text"+slide_num).find(".second").animate({"left":"0px"},700,function() {
                $(".slide_text"+slide_num).find(".second").css("zIndex","5");
                $(".slide_text"+next_slide_num).find(".second").css("left",window_width+"px").css("zIndex","1");
                //console.log(prev_slide_num+" - "+slide_num+" - "+next_slide_num);
                $(".slide"+slide_num).animate({"left":"0px"},200,function() {
                    /* 				 				alert(slide_num); */
                    slider_busy_flag = false;
                    $(".slide"+slide_num).css("zIndex","0");
                    /*$(".slide"+next_slide_num).css("left",-img_slide_width+"px").css("zIndex","1"); */
                    $(".slide").each(function() {
                        if(!$(this).hasClass("slide"+slide_num))
                            $(this).css("left",-img_slide_width+"px").css("zIndex","1");
                    })
                });
            });
        }
    </script>
