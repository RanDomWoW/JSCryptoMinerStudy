			if (nttId != null && nttId != '') {
				try {
					$.ajax({
						dataType:"json",
						type: "POST",
						url: "user/cop/bbs/selectBoardArticleGallary.do",
						data: { "nttId" : nttId, "bbsId" : bbsId},
						async: false,
						cache: false, 
						success : function(data, status, request) {
							$(".cCard1ImgList").css("opacity",0);
							if(data != null) {
								var html = "";
								
								html += "<h2 class=\"cBoxtitle\">";
