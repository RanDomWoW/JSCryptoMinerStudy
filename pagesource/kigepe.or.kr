	<script>
	 //집합교육,사이버교육 탭 
	  function edu_tab(num) {
			if(num == "1") {
				//집합교육 선택시
				$('.mainRContentTop ul.cyber_edu').hide();
				$('.mainRContentTop ul.set_edu').show();
				$('.mainRContentBottom ul#set').show();
				$('.mainRContentBottom ul#cyber').hide();
			} else if(num == "2") {
				//사이버교육 선택시
				$('.mainRContentTop ul.cyber_edu').show();
				$('.mainRContentTop ul.set_edu').hide();
				$('.mainRContentBottom ul#set').hide();
				$('.mainRContentBottom ul#cyber').show();
			}
		}
	 
	 //모바일 교육일정 탭	 
	$(document).ready(function(){
		$('.cBoxtitle.white').click(function(){
			$('.mainRContentTop').slideToggle(500);
			$('.mainRContentTop').toggleClass('on');
			$(this).children('span').toggleClass('on');
	    });		
		
		//태블릿부터 mainRcontentTop 안보이는 현상 수정 
	    $(window).resize(function(){
	    	if ($(window).width() >= 641) {
	        	$(".mainRContentTop").css('display','block');
	        }
	    	
	    	if ($(window).width() <= 640) {
	    		$(".mainRContentTop").css('display','none');
	    		$('.cBoxtitle.white').children('span').removeClass('on');
	    		$('.mainRContentTop').removeClass('on');
	        }
	    });
	});

	  
	</script>
