								html += "<ul>";
								html += "<li>";
								html += "<div class=\"cCard1ImgBox\">";
								html += "<img src=\'/cmm/fms/getImage.do?atchFileId="+data.thumbFileId+"\' alt=\"\" />";
								html += "</div>";
								html += "<div class=\"cCard1TextBox\">";
								html += "<span class=\"cLine\"></span>";
								html += "<a class=\"ellipsis\" href=\"user/cop/bbs/selectBoardArticle.do?nttId="+data.nttId+"&bbsId="+data.bbsId+"&menuNo=16900\">";
								html += data.nttSj;
								html += "</a>";
								html += "<span>";
								var frstRegistPnttm = new Date(data.frstRegisterPnttm);
								html += frstRegistPnttm.getFullYear()+"."+(frstRegistPnttm.getMonth()+1)+"."+frstRegistPnttm.getDate();
								html += "</span>";
								html += "</div>";
								html += "</li>";
								html += "</ul>";
								html += "</div>";
								html += "<div class=\"cCard1BuBox\">";
								html += "<ul>";
								html += "<li><a href=\"#\" onclick=\"fn_egov_gallary_change(\'"+data.preNttId+"\', \'"+data.bbsId+"\', \'pre\');\"><img src=\"/assets/kor/images/wvUser/bu_prev01.png\" alt=\"\" /> 이전글 보기 </a></li>";
								html += "<li><a href=\"#\" onclick=\"fn_egov_gallary_change(\'"+data.nextNttId+"\', \'"+data.bbsId+"\', \'next\');\"> 다음글 보기 <img src=\"/assets/kor/images/wvUser/bu_next01.png\" alt=\"\" /></a></li>";
								html += "</ul>";
								html += "</div>";

								$("#gallatyBox").html(html);
								$("#gallatyBox").find(".cCard1ImgList").animate({"opacity":1},300);
							}
						},
					    error: function(data, status, error) {
							window.error = error;
							
							alert(error);
						}
					});	
				} catch(e) {
					alert(e.message);
				}
			} else {
				if (action == 'pre') {
