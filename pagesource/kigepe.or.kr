    // 쿠키에 등록
    function setCookie( name, value, expiredays ){
        var todayDate = new Date();
            todayDate.setDate( todayDate.getDate() + expiredays );
            document.cookie = name + '=' + escape( value ) + '; path=/; expires=' + todayDate.toGMTString() + ';'
    }
    
    
    // 브라우저 확대 & 축소 기능 
    
	var nowZoom = 100;

	function zoomOut() {
		nowZoom = nowZoom - 10;
		if (nowZoom <= 70)
			nowZoom = 70;
		zooms();
	}

	function zoomIn() {
		nowZoom = nowZoom + 10;
		if (nowZoom >= 130)
			nowZoom = 130;
		zooms();
	}

	function zooms() {
		document.body.style.zoom = nowZoom + "%";

	}

    
    
    
    // 팝업창 닫기
    function closeWin(flag, layer){
        var obj  = window.event.srcElement;
        if ( flag ){
            setCookie( layer, 'done' , 1 );
        }
        //$(layer).hide();
        $(".layer_popup").removeClass("topLayoutPopup");
    }
    
    function hideDayLayer(layer){
        setCookie( layer, 'done' , 1 );
        $("#"+layer).hide();
    }

    function hideLayer(layerNum){     
        $("#popup_layer_"+layerNum).hide();
    }

    window.onload = function(){
        // 쿠키 정보 가져오기
        cookiedata = document.cookie;
        $(".layer_popup").each(function () {
            if (cookiedata.indexOf($(this).attr("id") + '=done') >= 0) {
                $(".layer_popup").removeClass("topLayoutPopup");

            } else {
                $(".layer_popup").addClass("topLayoutPopup");
            }
        });
        
        $(".popup_layer").each(function () {
            if (cookiedata.indexOf($(this).attr("id") + '=done') >= 0) {
            	
            } else {
            	$(this).show();
            }
        });    
    }
