    <script async type="text/javascript">
    function gTrailsPositionError(err) {
        if (err.code == err.PERMISSION_DENIED) {
            ShowAppMessage("Error", "Your browser supports geolocation, but permission for your location information was denied.<br><br>If you're using the Platform Preview, you can enable geolocation by clicking Page->Allow websites to track your physical location.");
        } else if (err.code == err.POSITION_UNAVAILABLE) {
            ShowAppMessage("Error", "Unable to locate your position.");
        } else {
            // err.TIMEOUT
            ShowAppMessage("Error", "Unable to locate your position; the call to Location Services timed out.");
        }
    }

    function gTrailsShowMap(position) {
        document.location.href = "/trailfinder/browsebymap/?z=13&lat=" + position.coords.latitude + "&lon=" + position.coords.longitude;
    }

    function gTrailsGetLocation() {
        navigator.geolocation.getCurrentPosition(gTrailsShowMap, gTrailsPositionError);
    }
    </script>

