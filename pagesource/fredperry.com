<script type="text/javascript">
(function($){
    $(document).ready(function(){
        window.footerSub = function() {
            var email = $("#newsletter").val();
            var url = $('#newsletter-validate-detail').attr('action').replace(/^https?:/,'');
            var _data = url.replace('newsletter/subscriber/new', 'newsletter/subscriber/newAjax');
            var $msg = $('#show-message').hide(), $loading = $('#loading-message');
            var recaptchaResp = $('#newsletter-validate-detail .g-recaptcha-response').val();

            if(newsletterSubscriberFormDetail.validator.validate()){
                try{
                    $msg.html('').removeClass('error success');
                    $.ajax({
                        url: _data,
                        dataType: 'json',
                        type: 'POST',
                        data: {email: email, 'g-recaptcha-response': recaptchaResp},
                        timeout: 115000,
                        success: function (data){
                            $('#loading-message').hide();
                            if(!data.error){
                                $('#newsletter').val('');
                                $msg.addClass('success').show();
                                $('.block-subscribe').html($msg);
                            } else {
                                $msg.addClass('error').show()
                            }
                            $msg.html(data.message).show()
                        },
                        error: function(){
                            $loading.hide();
                            $msg.html('An error has occurred. Please try again.').addClass('error').show()
                        }
                    });
                }catch (e){
                }
            }
        }
    });
})(jQuery);
</script>
