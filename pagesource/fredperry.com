<script type="text/javascript">
    (function($){
        $(document).ready(function(){


            var suppressPopup = function() {
                var expirationDays = parseInt(30);
                var expireDate = new Date();
                expireDate.setDate(expireDate.getDate() + expirationDays);
                Mage.Cookies.set('suppress-popup', 1, expireDate);
            }

            $(document).on('click touchstart', '#signup-overlay .panel-close', function (e) {
                e.preventDefault();
                $(this).parent().hide();
                if ( ! Mage.Cookies.get('suppress-popup')) {
                    suppressPopup();
                }
            });

            var params = ["dm_i"];
            for (index = 0; index < params.length; ++index) {
                var url  = window.location.href;
                var regex = new RegExp("[?&]" + params[index] + "(=|&|#|$)"),
                    results = regex.exec(url);
                if (results) {
                    suppressPopup();
                    break;
                }
            }

            if ( ! Mage.Cookies.get('suppress-popup') && $('body').is('.catalog-category-view, .cms-index-index')) {
                if (perryMediator.determineActiveScope() === 'mobile') {
                    setTimeout(function () {
                        $('#signup-overlay').css({
                            'display': 'block',
                            'opacity': '0',
                            'transition': 'opacity 500ms'
                        });
                    }, 4800);
                    setTimeout(function () {
                        $('#signup-overlay').css({
                            'opacity': '1'
                        });
                    }, 5000);
                } else {
                    $('#signup-overlay').css('display', 'block');
                }
            }
        });
    })(jQuery);
</script></div><!--f02dea373e1e9b465c5b59e1009315f5-->

