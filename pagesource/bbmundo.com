}

/* hide input */
input.radio:empty {
    margin-left: -846px;
}

/* style label */
input.radio:empty ~ label {
    position: relative;
    float: none;
    line-height: 2.5em;
    text-indent: 0;
    cursor: pointer;
    -webkit-user-select: none;
