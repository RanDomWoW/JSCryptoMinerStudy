		function validEmail($email) {
			var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
			return emailReg.test($email);
		}
		function fast_subscribe() {
			if($("#fast_email").val() != '' && validEmail($("#fast_email").val()) ){
			$.post( "/fast_subscribe.php", { sub_mail:$("#fast_email").val() }, function( data ){
				if ( data == 'true' ) {
					$('.js-result p').remove();
					$('.js-result').append('<p>Ваша заявка принята!</p>');
