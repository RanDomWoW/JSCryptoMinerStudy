<script type="text/javascript">
    document.onreadystatechange = function() {
        if (document.readyState === "complete") {
            Pap.executeAction('ekomi_rating', {}, function(data) {
                if (data.success) {
                    var dec = parseInt(data.rating.toString().split('.')[0]),
                        fl = parseInt(data.rating.toString().split('.')[1]) || 0;

                    if (dec === 0) {
                        AUI().all('.eKomi-widget ul li span').setStyles({width: 0});
                    } else {
                        AUI().all('.eKomi-widget ul li:nth-child(' + dec++ + ')~li span').setStyles({width: 0});
                    }
                    AUI().all('.eKomi-widget ul li:nth-child(' + dec++ + ') span').setStyles({
                        width: fl * 10 + '%'
                    });

                    AUI().all('.eKomi-widget .num').setContent(data.feedbacksNumber);
                    AUI().all('.eKomi-widget .mark').setContent(data.ratingName);
                    AUI().all('.eKomi-widget').setStyles({visibility: null, cursor: 'pointer'});
                    AUI().all('.eKomi-widget').setAttribute('onclick', 'window.open("https://www.ekomi.de/bewertungen-drivelogde.html","_blankekomi"); _gaq.push(["_trackEvent", "External-Link-Click", "ekomi_widget", "Click_ekomi_widget_GOV"]);');
                }

                //set structured data metatags
                var parentEkomiContainer = AUI().all('.eKomi-widget');
                var ratingValueMeta = AUI().Node.create('<meta/>').setAttribute('itemprop', 'ratingValue').setAttribute('content', data.rating);
                var bestRatingMeta = AUI().Node.create('<meta/>').setAttribute('itemprop', 'bestRating').setAttribute('content', 5);
                var worstRatingMeta = AUI().Node.create('<meta/>').setAttribute('itemprop', 'worstRating').setAttribute('content', 1);
                var itemReviewedMeta = AUI().Node.create('<meta/>').setAttribute('itemprop', 'itemReviewed').setAttribute('content', 'Drivelog.de');
                var ratingCountMeta = AUI().Node.create('<meta/>').setAttribute('itemprop', 'ratingCount').setAttribute('content', data.feedbacksNumber);

                parentEkomiContainer.append(ratingValueMeta);
                parentEkomiContainer.append(bestRatingMeta);
                parentEkomiContainer.append(worstRatingMeta);
                parentEkomiContainer.append(itemReviewedMeta);
                parentEkomiContainer.append(ratingCountMeta);

                console.log(data);
            }, null, "POST", null, true, null, false, '.eKomi-widget');
        }
    };

</script>
