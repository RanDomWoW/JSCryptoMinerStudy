<script>
    var findServices = function(elem){

        var searchBox = jQuery(elem);
        var selectedOptionBox = searchBox.find('.select-custom');
        var selectedOptionValue = selectedOptionBox.text();
        var serviceOptionsElems = searchBox.find('li a');
        var actionButton = jQuery("#action-button");
        var clickedUrl = "";

        serviceOptionsElems.bind("mousedown", function(e){
            clickedUrl = jQuery(e.target).attr('href');
            setRedirectUrl(clickedUrl);
            setListenerForSearch(clickedUrl);
        });

        serviceOptionsElems.bind("keydown", function(e){
            if (e.which === 13) {
                clickedUrl = jQuery(e.target).attr('href');
                setRedirectUrl(clickedUrl);
                doRedirect();
            }
        });
        var setListenerForSearch = function(actionUrl){
            jQuery(document).bind('keydown',function(event,actionUrl) {
                if(event.which === 13) {
                    jQuery(document).unbind('keydown');
                    doRedirect();
                }
            });
        };
        var setRedirectUrl = function(actionUrl){
            actionButton.attr('href',actionUrl);
        };
        var getRandomInt = function(min, max) {
            return Math.floor(Math.random() * (max - min) + min);
        };
        var doRedirect = function(){
            ranColor = "0E8A16";
            actionButton.css({'background-color':'#' + ranColor});
            actionButton.click();
        };
        actionButton.click(function(){
            window.location.href = actionButton.attr("href");
        });

    };

    findServices('#svc-search');
</script>
