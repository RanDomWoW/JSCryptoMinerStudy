	function showInfoAnswersPanel()
	{
		var pInfoAnswers = $("#pInfoAnswers");
		pInfoAnswers.css("visibility", "visible");
		pInfoAnswers.show();

		resizeInfoAnswersPanel();
		$(window).resize( resizeInfoAnswersPanel );
	}

	function resizeInfoAnswersPanel()
	{
		resizePanel($(".infoAnswers"), $("#pInfoAnswers"), 700, 280);
	}

	function initializeGlobalStatistics()
	{
		$(".infoAnswers").click(
			function()
			{
				showInfoAnswersPanel();
			}
		);

		$("#bCloseInfoAnswers").click( 
			function()
			{
				$(window).off("resize", resizeInfoAnswersPanel);

				$("#pInfoAnswers").hide();
				event.preventDefault();
			}
		);
	};

</script>

		
			


