<script type="text/javascript">

function vidRescale(){	
	var w = $(window).width(); 
	var h = $(window).height();
	
	if (w/h > 16/9){
		$('.home-video').css({width: w+'px', height: (w/16*9) + 'px', left: '0px'});
	} else {
    	$('.home-video').css({width: (h/9*16)+'px', height: h + 'px'});
    	$('.home-video').css({left: -($('.home-video').outerWidth()-w)/2});
	}
}

$(window).on('load resize', function(){
  vidRescale();
});


</script>


