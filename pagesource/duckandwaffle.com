});
</script>

<script type="text/javascript">

$(document).ready(function(){

    $('.navbar-page-subnav a[href*="#!"]').click(function(event){
        var href = $(this).attr("href");
        var rexp = /.*#!(.*)/i;
        var ar = rexp.exec( href );

        rexp = /(.*)\/(.*)/i;
        var ar1 = rexp.exec( ar[1] );
        var dat = 0;
        if( ar1 && ar1.length > 1){
            ar[1] = ar1[1];
            dat = $(this).data("id");
        }
        $.smoothScroll({
           scrollTarget: '#'+ar[1],
           speed: 1000,
           afterScroll: function(){
               location.hash = ar[1];
               //$(window).trigger('resize');
               if(dat){
                   //sub-page-2902
                   $('a[rel=sub-page-'+dat+']').trigger('click');
               }
           }
        });
        event.preventDefault();
    });

    $('.back-to-top, .back-to-top-1').click(function(event){
        var rexp = /.*#!(.*)/i;
        var ar = rexp.exec( $(this).attr("href") );
        if( $(window).width() < 768){
            $('body').animate(
                {scrollTop: 0},
                300,
                function(){
                    //location.hash = ar[1];
                    location.hash = '';
                }
            );
        }
        else{
            $.smoothScroll({
               scrollTarget: '#'+ar[1],
               speed: 300,
               afterScroll: function(){
                   location.hash = ar[1];
               }
            });
        }
        event.preventDefault();
    });

    $('.scroll-bottom').click(function(event){
    	$('body').animate(
                {scrollTop: $('body').scrollTop() + 400},
                1000,
                function(){
                    //location.hash = ar[1];
                    //location.hash = '';
                }
            );
    	event.preventDefault();
    });


    var is_animation1 = false;


    $(".sub-navigation a").click(function(){
        if( $(this).attr("href") == "#" )
        {
            var $wrapper = $(this).closest(".content-wrapper");
            var $ul = $(">.sub-pages", $wrapper);
            var $li = $(">li."+$(this).attr("rel"), $ul);
            if( ! $li.hasClass("selected") )
            {
                var idx = $(">li", $ul).index( $li );
                var l = -100*idx;
                $ul.animate({left: l + "%"}, 400, function(){
                    $(">li", $ul).removeClass("selected");
                    $li.addClass("selected");
                });
            }
            return false;
        }
    });

    Hash.pushState(true);
	Hash.on('#(.*)\/(.*)', {

		yep: function(path, parts){

			location.hash = parts[1];

			var $li = $("#"+parts[1]+'-'+parts[2]).closest(".sub-page-li");
			var $ul = $($li).closest(".sub-pages");

            if( ! $li.hasClass("selected") )
            {
                var idx = $(">li", $ul).index( $li );
                var l = -100*idx;
                $ul.css({left: l + "%"});
                $(">li", $ul).removeClass("selected");
                $li.addClass("selected");
            }


		},

		nop: function(){
			//setTimeout(function(){ load_page( "action=index" ); }, 10);

		}

	});

    $(window).scroll(function(){
          if( $(this).scrollTop() )
          {
              $(".back-to-top, .scroll-bottom").fadeIn();
          }
          else
          {
              $(".back-to-top").fadeOut();
          }
    });


    if( $(this).scrollTop() )
    {
        $(".back-to-top, .scroll-bottom").fadeIn();
    }


    var taglineFunc = function(){
    	var $wr = $(".tagline");
    	var $cur = $(".tagline>li.active");
    	var $next = $cur.next().length ? $cur.next() : $(".tagline>li:first");

    	$next.removeAttr("style");
    	var h = $next.height();
    	var h1 = $wr.height();
    	if(h > h1 && h1){
    		$next.css({fontSize: (h1/h*100) + "%" });
        } 
    	
    	$next.fadeIn(2000, function(){$(this).addClass("active");});
    	if( $(".tagline>li").length > 1 ){
        	$cur.fadeOut(2000, function(){$(this).removeClass("active");});
   			setTimeout(function(){ taglineFunc(); }, 7000);
    	}
    };


    taglineFunc();
   

    if($(".home-duckpost-wrapper").length){
    	var $wrapper = $(".home-duckpost-wrapper");
    	//$(".page-uid-home_duck_post").ajax_loading("show");
        $.ajax({
            url: $wrapper.data("url"),
            //cache: false,
            //dataType: "json",
            type: "GET",              
            success: function(data) {
                //$(".page-uid-home_duck_post").ajax_loading("hide");
                //$(".eb-slide-show", $wrapper.html(data)).eb_slideshow();
                $(".home-duckpost-thumbnails", $wrapper.html(data)).iscroller({rows: 1, circular: true, plainModeWidth: 0});
            },
            error: function(jqXHR, textStatus, errorThrown){
            }
        });
    }    

});




</script>



