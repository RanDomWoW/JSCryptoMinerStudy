<script type="text/javascript">
var is_ajax_loading = false;
var data_changed = false;
var last_params = {get: false};

function load_social_feed(cont, twitter_id, facebook_id, instagram_id)
{
	$(".page-uid-social_media_feeds .page-text").addClass("awaiting");

  	$.ajax({
        url: "https://duckandwaffle.com/index.php",
        type: "GET",
        dataType: "json",
        data: { action: "social_ajax", twitter_id: twitter_id, facebook_id: facebook_id, instagram_id: instagram_id, start: $(cont).data('start') },
        async: true,
        success:function(items){

          var statusHTML = [];
          for (var i=0; i<items.length; i++){
              var orign 			= items[i].orign;
              var feed_datetime_uts = items[i].feed_datetime_uts;
              var feed_user			= items[i].feed_user;
              var feed_text			= items[i].feed_text;
              var feed_image		= items[i].feed_image;
              var feed_thumbnail	= items[i].feed_thumbnail;
              var feed_image		= items[i].feed_image;
              var feed_id			= items[i].feed_id;
              var feed_link			= items[i].feed_link;


              var c1 = 0 && i > 3 ? " hidden-mobile-social-media " : "";

             if( "tw" == orign )
             {
                 var status = feed_text.replace(/((https?|s?ftp|ssh)\:\/\/[^\"\s\<\>]*[^.,;\'\">\:\s\<\>\)\]\!])/g, function(url) {
                    return '<a target="_blank" href="'+url+'">'+url+'</a>';
                 }).replace(/\B@([_a-z0-9]+)/ig, function(reply) {
                    return  reply.charAt(0)+'<a target="_blank" href="http://twitter.com/'+reply.substring(1)+'">'+reply.substring(1)+'</a>';
                 });
                 statusHTML.push('<div class="iscroller-item thumbnail text-center tw '+c1+'" id="feed-'+feed_id+'"><div class="wrapper"><div class="social-feed-title"></div><div class="social-feeds-item-content"><span>'+status+'</span> <br/> <a target="_blank" style="font-size:85%" href="http://twitter.com/'+feed_user+'/statuses/'+feed_id+'">'+relative_time(feed_datetime_uts)+'</a></div></div></div>');

             }
             else if( "in" == orign )
             {
                if( "" != feed_image )
                {
                    statusHTML.push('<div class="iscroller-item thumbnail text-center in '+c1+'" id="feed-'+feed_id+'"><div class="social-feed-title"></div><a class="event-overlay" target="_blank" href="'+feed_link+'"></a><img src="'+feed_image+'"/></div>');
                }
             }
             else if( "fb" == orign )
             {
            	 if( feed_image )
                 {
                     statusHTML.push('<div class="iscroller-item thumbnail text-center fb '+c1+'" id="feed-'+feed_id+'"><div class="social-feed-title"></div><a class="event-overlay" target="_blank" href="'+feed_link+'"></a><div class="fb-image" style="background-image: url('+feed_image+');"></div></div>');
                 }
            	 else
            	 {
	                 var status = feed_text.replace(/((https?|s?ftp|ssh)\:\/\/[^\"\s\<\>]*[^.,;\'\">\:\s\<\>\)\]\!])/g, function(url) {
	                    return '<a target="_blank" href="'+url+'">'+url+'</a>';
	                 });
	                 var img = '';
	                 var tit = '<div class="social-feed-title"></div>';
	                 if( feed_image ){
	                     img = '<img src="'+feed_image+'"/>';
	                     tit = '';
	                 }
	                 statusHTML.push('<div class="iscroller-item thumbnail text-center fb '+c1+'" id="feed-'+feed_id+'"><div class="wrapper">'+tit+'<div class="social-feeds-item-content">'+img+'<span>'+status+'</span> <br/> <a target="_blank" style="font-size:85%" href="'+feed_link+'">'+relative_time(feed_datetime_uts)+'</a></div></div></div>');
            	 }
             }
             $(cont).data('start',feed_datetime_uts);
          }
          $(cont).html( statusHTML.join("") );


          $(".iscroller.social-media-thumbnails").iscroller({plainModeWidth: 0, rows: 4}).removeClass("awaiting");

          $(".page-uid-social_media_feeds .page-text").removeClass("awaiting");

        }
    });


}

function relative_time(time_value)
{
      var parsed_date = new Date(parseInt( time_value )*1000);
      var relative_to = new Date();
      var delta = parseInt( ( relative_to.getTime() - parsed_date.getTime() ) / 1000 );
      delta = delta + (relative_to.getTimezoneOffset() * 60);

      if (delta < 60) {
        return "less than a minute ago";
      } else if(delta < 120) {
        return "about a minute ago";
      } else if(delta < (60*60)) {
        return (parseInt(delta / 60)).toString() + " minutes ago";
      } else if(delta < (120*60)) {
        return "about an hour ago";
      } else if(delta < (24*60*60)) {
        return "about " + (parseInt(delta / 3600)).toString() + " hours ago";
      } else if(delta < (48*60*60)) {
        return "1 day ago";
      } else {
        return (parseInt(delta / 86400)).toString() + " days ago";
      }
}

function load_page( $container, qs )
{
    if( is_ajax_loading )
    {
        return;
    }

    is_ajax_loading = true;
    data_changed = false;

    if( $container.hasClass("about-wrapper") )
    {
    	$(".top-wrapper, .bottom-wrapper, .content-wrapper, .location-left-wrapper").hide();
    }


    $container.ajax_loading("show");

    $.ajax({
        type: "GET",
        dataType: "json",
        url: "https://duckandwaffle.com/index_ajax.php?" + qs,
        error: function(jqXHR, textStatus, errorThrown){
            data_changed = true;
            alert("Unknown error, please reload page" +  jqXHR.responseText);
            is_ajax_loading = false;
        },
        success: function(data){
            data_changed = false;

            last_params['get'] = data['get'];
            last_params['params'] = data['params'];

            var $contentWrapper = $(data["content"]);

            $container.attr("class", $contentWrapper.attr("class"));
            $container.html($contentWrapper.html());

            //$(window).trigger("resize");

            //setTimeout(function(){ $("#flipbook").turn('next'); }, 200);


            if( "" != data['css'] )
            {
                $("#dynamic-style").remove();

                if( $.browser.msie ) // IE
                {
                    var ss1 = document.createElement("style");
                    ss1.setAttribute("id", "dynamic-style");
                    ss1.setAttribute("type", "text/css");
                    var hh1 = document.getElementsByTagName("head")[0];
                    hh1.appendChild(ss1);
                    ss1.styleSheet.cssText = data["css"];
                }
                else
                {
                    $("<style/>").attr({id: "dynamic-style", type: "text/css"}).html(data["css"]).appendTo("head");
                }
            }


            init_loaded_page($container);


            $container.ajax_loading("hide");
            is_ajax_loading = false;
        }
    });

}



function init_loaded_page($container)
{

	$(".menu-column", $container).each(function(){
		if( $(this).data("manual") )
		{
			if( $(".columnbreak", this).length )
			{
				$(this).columnize({columns: 2, buildOnce: false, lastNeverTallest: true, manualBreaks: true });
			}
		}
		else
		{
			$(this).columnize({columns: 2, buildOnce: false, lastNeverTallest: true });
		}
	});


    $(".home-duckpost-thumbnails", $container).iscroller({rows: 1, circular: true, plainModeWidth: 0});
    $(".home-instagram-thumbnails", $container).iscroller({rows: 2, circular: true, plainModeWidth: 0});
    $(".featured-events-thumbnails", $container).iscroller_full_width({rows: 1, circular: true});
    $(".home-menu-thumbnails", $container).iscroller({rows: 1, circular: true, plainModeWidth: 0});
    $(".home-press-thumbnails", $container).iscroller({rows: 1, circular: true, plainModeWidth: 0});
    $(".events-thumbnails", $container).iscroller_full_width({rows: 1, circular: true, plainModeWidth: 0, 
    	responsiveItemWidth: function(){	    	
	    	var ww = $(".events-thumbnails").width();
	    	var isz = $(this).data("itemsSize"); 	    	    		    	
	    	var ret = isz < 4 ? ww/$(this).data("itemsSize") : ww/3;
	    	
	    	if( ww < 800 )
	    	{
	    		ret = ww/2;
	    	}

	    	if( ww < 500 )
	    	{
	    		ret = ww;
	    	}

	    	if(1 == isz)
	    	{
	    		ret = ww;
	    	}
	    		    	
	    	
	    	return ret;	    	
	      }
      });

    $(".social-media-thumbnails", $container).iscroller({rows: 2, plainModeWidth: 768, circular: true});
    $(".social-media-facebook-thumbnails", $container).fbcarousel({rows: 1, circular: true, plainModeWidth: 0});
    $(".featured-event-carousel", $container).triplecarousel();

    $("a[data-url]", $container).click(function(event){
        load_page( $( $(this).closest(".content-wrapper") ), $(this).data("url") );
        event.preventDefault();
    });

    $("a[data-about_url]", $container).click(function(event){
        if( ! $( ".about-wrapper" ).length )
        {
            $("<div/>").addClass("about-wrapper").appendTo("body");
        }
        load_page( $( ".about-wrapper" ), $(this).data("about_url") );
        event.preventDefault();
    });

    $(".about-close", $container).click(function(event){
        $( ".about-wrapper" ).remove();
        $(".top-wrapper, .bottom-wrapper, .content-wrapper, .location-left-wrapper").show();
        $(window).trigger("resize");
        event.preventDefault();
    });
    $('.date-pick', $container).datepicker({autoclose: true});

    $(".event-overlay", $container)
    .css({opacity: 0, display: "block"}).addClass("event-overlay-hover")
    .hover(
        function(){
            $(this).css({opacity: 0.3});
        },
        function(){
            $(this).css({opacity: 0});
        }
    );

    $(".mailing-form-wrapper-ajax", $container).each(function(){
        var id = $(".mailing-form-id", this).val();
        var $o = $("#mailing-form-"+id);
        var $wrapper = $(this);
        $("form", $o).submit(function(event){

            $wrapper.ajax_loading("show");
            $.ajax({
                    url: "https://duckandwaffle.com/index.php?action=mailing_form_ajax&id="+id,
                    cache: false,
                    dataType: "json",
                    type: "POST",
                    data: $("form", $o).serialize(),

                    success: function(data) {
                        $wrapper.ajax_loading("hide");
                        if( undefined == data["status"] )
                        {
                                alert("Unknown error - please reload page and try again.");
                        }
                        else
                        {
                            $(".mailing-form-message-wrapper", $o).html( data["status_message"] );
                                $(".mailing-form-field-border", $o).removeClass("mailing-form-field-error");
                                if( "error" == data["status"] )
                                {
                                    $(data["ids"]).addClass("mailing-form-field-error");
                                }
                                //location.hash = $(".dummy-top", $o.closest(".content-wrapper")).attr("id");
                                $o.closest(".subpage-text").scrollTop(0);
                        }
                },
                error: function(jqXHR, textStatus, errorThrown){
                      alert(errorThrown);
                }
            });
            event.preventDefault();
        });
    });
    $(".mailing-form-wrapper", $container).each(function(){
        var $this = $(this);
        var $wr = $(".dummy-top", $(this).closest(".content-wrapper"));
        if( ! $this.hasClass("hash-processed") )
        {
            var $form = $("form", $this);
            var id = $wr.attr("id");
            id = id.replace(/\-/,"\/");
            $form.attr("action", $form.attr("action") + "#" + id );
        }
    });

    $(".colorboxIframePage", $container).colorbox({width: "400px", minHeight: "400px", /*iframe: true, */scrolling: false});
    $(".colorboxPDF", $container).colorbox({width: "80%", height: "80%", iframe: true});
    $(".colorboxMap", $container).colorbox({width: "80%", height: "80%", iframe: true});
    var c = 1300/866;
    if( $(window).width() / $(window).height()  > c )
    {
        $(".colorbox", $container).colorbox({maxWidth: "95%", maxHeigh: "95%", height: "95%"});
    }
    else
    {
        $(".colorbox", $container).colorbox({maxWidth: "95%", maxHeigh: "95%"});
    }

    $(".opentable", $container).colorbox({width: "1100px", height: "700px", iframe: true});
    $(".amenities-colorbox", $container).colorbox({width: "500px", height: "400px",  inline: true});
    $(".eb-slide-show", $container).eb_slideshow();

    $.swapImage(".swapImage", $container);

    
    

    $("<div/>").addClass("secClose").hover(function(){$(this).addClass("secClose-hover")},function(){$(this).removeClass("secClose-hover")})
    .click(function(){
        var $wr = $(this).closest(".room-details-box");
        $wr.fadeOut();
    }).appendTo( $(".room-details-box", $container) );


    $(".open-room-details-box", $container).click(function(event){
        var rel = $(this).attr("rel");
        var $wr = $(this).closest(".content-wrapper");
        $(".room-details-box", $wr).fadeOut();
        $(rel, $wr).fadeIn();

        $('.room-details-next, .room-details-prev').fadeOut();
        event.preventDefault();
    });
    $('.room-details-box .secClose').click(function(){
        $('.room-details-next, .room-details-prev').fadeIn();
    });

    
    
}
</script>



<script type="text/javascript">
$(document).ready(function(){
    init_loaded_page("body");

    $(document).on('cbox_open', function(){
        Hammer(document.body).on("swipeleft", $.colorbox.next);
        Hammer(document.body).on("swiperight", $.colorbox.prev);
    });
    
    $(document).on('cbox_closed', function(){
        Hammer(document.body).off("swipeleft", $.colorbox.next);
        Hammer(document.body).off("swiperight", $.colorbox.prev);
    });

    
    $(document).on('cbox_complete', function(){
        $(".eb-slide-show", $("#colorbox")).eb_slideshow();
    });

    $(".carousel").each(function(){
    	$(this).hammer().on("swipeleft", function(){$(this).carousel('next');});
    	$(this).hammer().on("swiperight", function(){$(this).carousel('prev');});
    }); 

