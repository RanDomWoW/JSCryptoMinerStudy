	<script>
  $(document).ready(function() {
    var uploader = $("#bootstrapped-fine-uploader").fineUploader({
       
     autoUpload: true,
     multiple: true,
      failedUploadTextDisplay: {
        mode: 'custom',
        maxChars: 40,
        responseProperty: 'error',
        enableTooltip: true
      },
       
      debug: true,
      template: 'qq-template-bootstrap',
      classes: {
          success: 'alert alert-success',
          fail: 'alert alert-error'
      },
      
    }).on('complete', function(event, id, fileName, responseJSON) {
       if (responseJSON.success) {
          $('#bootstrapped-fine-uploader').append('<div style="margin-top: 5px; text-align: center" class="alert alert-success"><b>Embed:' + fileName+ ':</b> ' + responseJSON.link + '</div>');
          $('input[name=aktorki]').val('');
          $('input[name=kategorie]').val('');
          $('input[name=email]').val('');
          $('input[name=rules]').val('');
          $('input[name=filename]').val('');

       }
       if (responseJSON.errorr) {
            $('#info').removeClass('invisible');
          for(var x in responseJSON.errorr) 
            { 
              var tmp = responseJSON.errorr[x]; 
               $('#blad').append('<li style="margin-left: 40px">' + tmp + '</li>');
         
            }
       }
   }); 
  });
</script>

