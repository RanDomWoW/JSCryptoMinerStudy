SlideShow4239147.updateIndexes = function () {
    var currentIndex = SlideShow4239147.currentContentIndex;
    var photoCount = SlideShow4239147.container.parent().find('#SlideShow4239147IndexNavigation').data('photo-count');

    SlideShow4239147.container.parent().find('.sldw_photo_index.selected').removeClass('selected');
    SlideShow4239147.container.parent().find('.sldw_photo_index[data-index=' + currentIndex + ']').addClass('selected');
   
    if (photoCount > 7) {
        var maxIndex = SlideShow4239147.CalcMaxIndex(photoCount);
        var minIndex = SlideShow4239147.CalcMinIndex(photoCount);
        SlideShow4239147.container.parent().find('.sldw_photo_index').each(function() {
