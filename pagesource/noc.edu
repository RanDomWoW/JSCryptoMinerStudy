SlideShow6377870.setContainers = function(current_container) {
	//set up the containers for the next transition
	if (current_container === undefined) current_container = 'index';
	
	switch (current_container) {
		case 'next':
			SlideShow6377870.currentContentContainer.html(SlideShow6377870.nextContentContainer.html());
