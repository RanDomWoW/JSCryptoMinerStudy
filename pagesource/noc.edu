			current_caption_container.children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow4239147.resizeCaptionContainer);
        } else {
        	current_caption_container.children().children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow4239147.resizeCaptionContainer);
        }
}

// Perform transition is run after the timer expires or when the next/prev arrows are clicke
SlideShow4239147.PerformTransition = function(trans_dir) 
{
	if (SlideShow4239147.transitioning == true) return;    
    var is_fwd = (trans_dir === 'next');
    var is_selected_index = (trans_dir === 'selected');
    var next_img = '';

    if (!is_selected_index) {
        // next_img only references the correct element before setContainers is called!
        next_img = is_fwd ? SlideShow4239147.nextContentContainer : SlideShow4239147.previousContentContainer;
    } else {
        SlideShow4239147.selectedContentContainer.html(SlideShow4239147.contents[SlideShow4239147.currentContentIndex]);
        next_img = SlideShow4239147.selectedContentContainer;
    }

    var next_caption_container = next_img.find('.captionContainer');
	if (next_caption_container.children('img').length > 0) {
		next_caption_container.children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow4239147.resizeCaptionContainer);
	} else {
		next_caption_container.children().children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow4239147.resizeCaptionContainer);
	}

    SlideShow4239147.updateIndexes();
    
    
    
    
        next_img.css('display', 'block');
        if (next_img.children('.SlideShowImage').eq(0).children().children('img').length > 0) {
			next_img.children('.SlideShowImage').eq(0).children().children('img').first().each(SlideShow4239147.resizeCaptionContainer);
        } else {
        	next_img.children('.SlideShowImage').eq(0).children().children().children('img').first().each(SlideShow4239147.resizeCaptionContainer); 
        }


        
            
            var start = {};
            var end = {};
            

            //hide the current container
	        next_img.css('display', 'none');
	        var animateDiv = SlideShow4239147.container.children('.SlideShowImage').eq(0);
            animateDiv.hide('fade', start, 1 * 500, function() {                
                animateDiv.html(next_img.children(":first").clone(true));
	            next_img.css('display', 'block');
	            SlideShow4239147.container.parent().parent().height(next_img.height());
	            next_img.css('display', 'none');
	            SlideShow4239147.setContainers(trans_dir);
                animateDiv.show('fade', end, 1 * 500);
            });

        
}

