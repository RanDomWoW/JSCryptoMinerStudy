    if (photoCount > 7) {
        var maxIndex = SlideShow6377870.CalcMaxIndex(photoCount);
        var minIndex = SlideShow6377870.CalcMinIndex(photoCount);
        SlideShow6377870.container.parent().find('.sldw_photo_index').each(function() {
            var $li = LCMS.jq(this);
            var photoIndex = parseInt($li.data('index'));
            if ((photoIndex < minIndex) || (photoIndex > maxIndex)) {
                $li.hide();
            }
            else {
                $li.show();
            }
            
            if (photoIndex === maxIndex && maxIndex !== (photoCount - 1)) {
                $li.find('a').text('...');
            }
            else if (photoIndex === minIndex && minIndex !== 0) {
                $li.find('a').text('...');
            }
            else {
                $li.find('a').text((photoIndex + 1));
            }
        });
    }
