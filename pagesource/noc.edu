// Perform transition is run after the timer expires or when the next/prev arrows are clicke
SlideShow6377870.PerformTransition = function(trans_dir) 
{
	if (SlideShow6377870.transitioning == true) return;    
    var is_fwd = (trans_dir === 'next');
    var is_selected_index = (trans_dir === 'selected');
    var next_img = '';

    if (!is_selected_index) {
        // next_img only references the correct element before setContainers is called!
        next_img = is_fwd ? SlideShow6377870.nextContentContainer : SlideShow6377870.previousContentContainer;
    } else {
        SlideShow6377870.selectedContentContainer.html(SlideShow6377870.contents[SlideShow6377870.currentContentIndex]);
        next_img = SlideShow6377870.selectedContentContainer;
    }

    var next_caption_container = next_img.find('.captionContainer');
	if (next_caption_container.children('img').length > 0) {
		next_caption_container.children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow6377870.resizeCaptionContainer);
	} else {
		next_caption_container.children().children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow6377870.resizeCaptionContainer);
	}

    SlideShow6377870.updateIndexes();
    
    
    
    
        next_img.css('display', 'block');
        if (next_img.children('.SlideShowImage').eq(0).children().children('img').length > 0) {
			next_img.children('.SlideShowImage').eq(0).children().children('img').first().each(SlideShow6377870.resizeCaptionContainer);
        } else {
        	next_img.children('.SlideShowImage').eq(0).children().children().children('img').first().each(SlideShow6377870.resizeCaptionContainer); 
        }


          
	        
	        
	        SlideShow6377870.container.parent().parent().height(next_img.height());
			//hide the current container
	        next_img.css('display', 'none');    
	        var pictDiv = SlideShow6377870.container.children('.SlideShowImage').eq(0);
	        pictDiv.html(next_img.children(":first").clone(true));       
	        SlideShow6377870.setContainers(trans_dir);
	    
	    
        
}

