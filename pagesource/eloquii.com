<script type="text/javascript">
	(function(jQuery) {
		jQuery(document).ready(function() {
			var ltoViewPort = window.innerWidth;
			var productileWidth = 200;
			var view = 6;
			lto_init();
			
			function lto_init() {
				jQuery('.LTOjs').each(function(i, el) {
					var items = jQuery('.lto-nav a', el).length;
					if(ltoViewPort >= 1200){
						ltoViewPort = 1200;
					} else if (ltoViewPort < 1200 && ltoViewPort >= 1024){
						ltoViewPort = 1000;
						view = 5;
					} else if (ltoViewPort < 1024 && ltoViewPort >= 768){
						ltoViewPort = 600;
						view = 3;
					} else if (ltoViewPort < 768){
						ltoViewPort = 400;
						productileWidth = 160;
						view = 2;
					}

					jQuery('.lto-nav a', el).css({'width': (ltoViewPort-items)/items});
					
					jQuery('.lto-nav a', el).click(function(e) {
						if (jQuery(this).hasClass('selected') == false) {
							e.preventDefault();
							var LTOjs = jQuery(this).parents('.LTOjs');
							jQuery('.lto-nav a', LTOjs).removeClass('selected');
							jQuery(this).addClass('selected');
							ltd_loadCategory(jQuery(this).attr('rel'), LTOjs);
						}
					});
				
					var selected = jQuery('.lto-nav a', el)[0];
					jQuery(selected).addClass('selected');
					
					// Hide container, show loader, hide navigation
					jQuery('.lto-container', el).empty().hide();
					jQuery('.lto-loader', el).show();
					jQuery('.lto-next', el).hide();
					jQuery('.lto-back', el).hide();
					
					// Delay loading top looks for 5 seconds
					window.setTimeout(function() {
						ltd_loadCategory(jQuery(selected).attr('rel'), el);
					}, 100);
				});
			}
		
			function ltd_loadCategory(loadUrl, LTOjs) {
				// Hide container, show loader, hide navigation
				jQuery('.lto-container', LTOjs).empty().hide();
				jQuery('.lto-loader', LTOjs).show();
				jQuery('.lto-next', LTOjs).hide();
				jQuery('.lto-back', LTOjs).hide();
				
				// Build URL
				loadUrl = app.util.appendParamToURL(loadUrl, 'format', 'ajax');
				loadUrl = app.util.appendParamToURL(loadUrl, 'loadsale', 'true');
				loadUrl = app.util.appendParamToURL(loadUrl, 'start', '0');
				loadUrl = app.util.appendParamToURL(loadUrl, 'sz', '24');
				//loadUrl = app.util.appendParamToURL(loadUrl, 'prefn1', 'colorFamily');
				//loadUrl = app.util.appendParamToURL(loadUrl, 'prefv1', 'Green|Blue');

				// Load AJAX
				jQuery('.lto-container', LTOjs).load(loadUrl, function() {
					
					if (jQuery('.product.producttile', this).length > 0) {
						var LTOjs = jQuery(this).parents('.LTOjs');
						jQuery('.lto-loader', LTOjs).hide();
					
						var items = jQuery('.product.producttile', this).length;
						jQuery(this).css({'left': 0});
						jQuery(this).css({'width': productileWidth*items});
					
						var index = 0;
						jQuery('.product.producttile', this).removeClass('first');
						var first = jQuery('.product.producttile', this)[index];
						jQuery(first).addClass('first');
					
						jQuery('.lto-next', LTOjs).hide();
						jQuery('.lto-back', LTOjs).hide();
						if (items > (ltoViewPort/productileWidth)) {
							jQuery('.lto-next:hidden', LTOjs).delay(1000).fadeIn();
							jQuery('.lto-back', LTOjs).hide();
						}
					
						jQuery('.lto-next', LTOjs).unbind().click(function(e) {
							trackGA("HPGoingFast", 100, "Next");
							var parent = jQuery(this).parent('.lto-wrapper');
							var index = jQuery('.product.producttile.first', parent).index();
							var count = jQuery('.product.producttile', parent).length;
							index = Math.max(0, Math.min(count-(view-1), index+(view-1), count-view));
							jQuery('.product.producttile', parent).removeClass('first');
							var first = jQuery('.product.producttile', parent)[index];
							jQuery(first).addClass('first');
							var left = index*productileWidth*-1;

							jQuery('.lto-container', parent).animate({'left': left}, 1000, 'easeInOutCubic');
							if (index > 0) {
								jQuery('.lto-back:hidden', parent).fadeIn();
							} else {
								jQuery('.lto-back:visible', parent).fadeOut();
							}
							next = Math.max(0, Math.min(count-(view-1), index+(view-1), count-view));
							if (next > index) {
								jQuery('.lto-next:hidden', parent).fadeIn();
							} else {
								jQuery('.lto-next:visible', parent).fadeOut();
							}
						});
						jQuery('.lto-back', LTOjs).unbind().click(function(e) {
							trackGA("HPGoingFast", -1, "Back");
							var parent = jQuery(this).parent('.lto-wrapper');
							var index = jQuery('.product.producttile.first', parent).index();
							var count = jQuery('.product.producttile', parent).length;
							index -= view+1;
							index = Math.max(0, index);
							jQuery('.product.producttile', parent).removeClass('first');
							var first = jQuery('.product.producttile', parent)[index];
							jQuery(first).addClass('first');
							var left = index*productileWidth*-1;
							jQuery('.lto-container', parent).animate({'left': left}, 1000, 'easeInOutCubic');
							if (index > 0) {
								jQuery('.lto-back:hidden', parent).fadeIn();
							} else {
								jQuery('.lto-back:visible', parent).fadeOut();
							}
							next = Math.max(0, Math.min(count-(view), index+(view)));
							if (next > index) {
								jQuery('.lto-next:hidden', parent).fadeIn();
							} else {
								jQuery('.lto-next:visible', parent).fadeOut();
							}
						});
						
						// Remove some HTML and classes and response. Don't want to show price, name, etc.
						jQuery('.product.producttile.last', this).removeClass('last');
						jQuery('.product.producttile', this).next('div.clear').remove();
						jQuery('.product.producttile .pricing', this).remove();
						jQuery('.product.producttile .promo', this).remove();
						jQuery('.product.producttile .saleonselectcolors', this).remove();
						jQuery('.product.producttile .morecolors', this).remove();
						jQuery('.product.producttile .newbadge', this).remove();
						jQuery('.product.producttile .salebadge', this).remove();
						jQuery('.product.producttile .finalbadge', this).remove();
						jQuery('.product.producttile .as-shown', this).remove();
						jQuery('.product.producttile .set-name', this).remove();
						jQuery('.product.producttile .name', this).remove();
						// jQuery('.product.producttile', this)
					
						$('.producttile').click(function(){
							trackGA("HPGoingFast", ($(this).attr('rel')), $(this).find('img').attr('src').substring($(this).find('img').attr('src').lastIndexOf('/') + 1));
						});

						// Bind Quickview
						var quickViewOptions = {
							buttonSelector: 'div.quickviewbutton, div.toplooksbutton',
							imageSelector: 'div.producttile div.image',
							buttonLinkSelector: 'div.quickviewbutton a',
							productNameLinkSelector: 'div.producttile div.name a'
						};
						jQuery(quickViewOptions.imageSelector).unbind('mouseenter mouseleave');
						jQuery(quickViewOptions.buttonLinkSelector).unbind('click');
						app.quickView.bindEvents(quickViewOptions);
					
						app.execUjs();
						
						// Add a delay so images have time to load
						jQuery(this).delay(500).fadeIn(1000);
					}
				});
			}
		});
	})(jQuery);
</script>

