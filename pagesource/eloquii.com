	}
	if (subtotalVal) {
		subtotalVal = Number(subtotalVal.toFixed(2));
	}
	if (totalValue) {
		totalValue = Number(totalValue.toFixed(2));
	}
	var basketSummary = {
		amttofreeship: {
			currency: currency,
			value: amountToFreeShip
		},
		subtotal: {
			currency: currency,
			value: subtotalVal
		},
		total: {
			currency: currency,
			value: totalValue
		},
		id: 'null',
		quantity: 0.0
	};
	
	Object.keys(basketSummary.amttofreeship).forEach(function(key) {
	    var value = basketSummary.amttofreeship[key];
	    if (value === "null" || value === null || value === "undefined") {
	        delete basketSummary.amttofreeship;
	    }
	});
	Object.keys(basketSummary.total).forEach(function(key) {
	    var value = basketSummary.total[key];
	    if (value === "null" || value === null || value === "undefined") {
	        delete basketSummary.total[key];
	    }
	});
	Object.keys(basketSummary.subtotal).forEach(function(key) {
	    var value = basketSummary.subtotal[key];
	    if (value === "null" || value === null || value === "undefined") {
	        delete basketSummary.subtotal[key];
	    }
	});
    var value = basketSummary.id;
    if (value === "null" || value === null || value === "undefined") {
        delete basketSummary.id;
    }
	
	// the item level basketSummary requires a quantity. I use jQuery.extend because I want a copy of basketSummary that does not reference the original object
	var itemBasketSummary = jQuery.extend(true, {}, basketSummary);
	itemBasketSummary.quantity = 0.0;

	for(var key in basketItems){
		uv.emit('eloquii.ecBasketItem', {
			basket: itemBasketSummary,
			product: basketItems[key],
			quantity: itemQuantity[key],
			subtotal: itemSubtotal[key]
		})

	}
	uv.emit('eloquii.ecBasketSummary', {
			basket: basketSummary
	})
	
	uv.emit('eloquii.ecUser', {
		"user": user
	})
	
});
</script>


