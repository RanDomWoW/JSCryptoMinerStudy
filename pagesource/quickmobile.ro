        <script>
            function getId(url) {
                var regExp = /^.*(youtu.be\/|v\/|u\/\w\/|embed\/|watch\?v=|\&v=)([^#\&\?]*).*/;
                var match = url.match(regExp);

                if (match && match[2].length == 11) {
                    return match[2];
                } else {
                    return 'error';
                }
            }
            $('#videoModal').on('show.bs.modal', function(e) {
                console.log(e.relatedTarget);
                var myId = getId($(e.relatedTarget).data('movie'));

                var myTitle = $(e.relatedTarget).data('title-video');

                var myCode = '<iframe width="560" height="315" src="https://www.youtube.com/embed/'
                    + myId + '" frameborder="0" allowfullscreen id="VideoHome"></iframe>';

                document.getElementById('myCode').innerHTML = myCode;
                document.getElementById('myModalLabelTitle').innerHTML = myTitle;
            });

            //stop watching video close modal
            $(document).ready(function() {
                $('#BtnCloseModalVideo').on('click', function(ev) {

                    $("#VideoHome")[0].src += "&autoplay=0";
                    ev.preventDefault();
                });
            });

        </script>
