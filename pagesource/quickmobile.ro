<script>
    
        $("#query-mobile").autoSuggest(siteurl + "search_suggestion", {
            minChars: 3, 
            matchCase: false, 
            queryParam: 'query',
            extraParams: "&as=true",
            selectedItemProp: 'name', 
            selectedValuesProp: 'name', 
            emptyText: 'Nu a fost gasit nici un produs', 
            searchObjProps: 'name',
            resultsHighlight: false, 
            resultClick: function (data) {
                $.ajax({
                    url: siteurl + 'cauta' + '/' + data.attributes.query + "?as=true",
                    success: function () {
                        window.location.href = data.attributes.url;
                    }
                });
            }
        });
        $('#search-box-mobile').on("keydown", function (e) {
            var code = e.keyCode || e.which;
            if (code == 13) {
                $("#query-mobile").autocomplete( "close" );
                $(this).submit();
            }
        });
        $('#search-box-mobile form').on('submit', function(e) {
            e.preventDefault();
            var url = siteurl + 'cauta' + '/' + $(this).find('.query-input-mobile').val();
            $.ajax({
                url: url + "?as=true",
                success: function () {
                    window.location = url;
                }
            });
            return false;
        });
    
</script>

