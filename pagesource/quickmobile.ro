<script>
    function serverTime(){
        return new Date(2018, 03, 18, 21, 31, 13);
    }
    
    $('[data-countdown]').each(function () {
        var settings =  {
            until: new Date($(this).data('countdown') * 1000),
            serverSync: serverTime()
        };
        if ($(this).data('format')){
            settings.format = $(this).data('format');
        }else if ($(this).data('layout')){
            settings.layout = $(this).data('layout');
        }else{
            settings.layout = $(this).html();
        }
        $(this).show();
        $(this).countdown(settings);
    });
    
</script>

