<script type="text/javascript">
$(document).ready(function(){
	$(".close1").click(function(){
		$("#takaro").hide()
		return false;
	})	
	
	$("input[name=commit]").click(function(){
		var loginnev=$("input[name=loginnev]").val();
		var loginemail=$("input[name=loginemail]").val();
		var loginpass=$("input[name=loginpass]").val();	
		var reg=$("input[name=reg]:checked").val();

		$.post("hpc/ajax.php",{muvelet:"login",loginnev:loginnev,loginpass:loginpass,loginemail:loginemail,reg:reg},function(b){
					
		   if(b.trim()=="ok"){
		   $(".errorwindow").addClass("green").show().html("OK");
            if (reg=="2")
			location.href="http://www.shp.hu/hpc/ugyfelkapu/index1.php";			
	    if (reg=="1")			
			location.href="http://www.shp.hu/hpc/start.php";	
			setTimeout(function(){
				$(".fix").load("hpc/start_oldal/start_fix.php");
				$(".errorwindow").removeClass("green").hide();
			},300)          
		  
		   $("#takaro").hide()
		   }else{alert(b);}	

		})	

		return false;
	})
	
	$("input[name=reg]").change(function(){
		ert=$("input[name=reg]:checked").val();
		if(ert==1){
			$("input[name=loginnev]").show();
			$("input[name=loginpass]").hide();
		}else{
			$("input[name=loginnev]").hide();
			$("input[name=loginpass]").show();
		}
	})
})


$('#regelek').keydown(function() {
var key = e.which;
if (key == 13) {
// As ASCII code for ENTER key is "13"
$('#regelek').submit(); // Submit form code
}
});

</script>
