var submitting = false;
function FrontPage_Form2_Validator(theForm)
{
	if (ValidateText(theForm.first_name.value, "Nombre")==false)
	{		
		theForm.first_name.focus();
		location.href='#first_name';
		return (false);
    }		
	//last name
	if (ValidateText(theForm.last_name.value, "Apellido")==false)
	{		
		theForm.last_name.focus();
		location.href='#last_name';
		return (false);
    }	

	//email	 
	if (ValidateEmail(theForm.email.value, "Email")==false)
	{		
		theForm.email.focus();
		location.href='#email';
		return (false);
    }
	
	//phone1_area
	if (ValidateNumber(theForm.w_area.value, "Codigo de Area del Telefono")==false)
	{		
		theForm.w_area.focus();
		location.href='#w_area';
		return (false);
    }
 
	//phone1	 
	if (ValidateNumber(theForm.w_phone.value, "Telefono")==false)
	{		
		theForm.w_phone.focus();
		location.href='#w_phone';
		return (false);
    }
	
	//phone2_area
	if (ValidateNumber(theForm.h_area.value, "Codigo de Area del Celular")==false)
	{		
		theForm.h_area.focus();
		location.href='#h_area';
		return (false);
    }
 
	//phone2
	if (ValidateNumber(theForm.h_phone.value, "Celular")==false)
	{		
		theForm.h_phone.focus();
		location.href='#h_phone';
		return (false);
    }
	
	//State	 
	if (ValidateText(theForm.state.value, "Estado")==false)
	{		
		theForm.state.focus();
		location.href='#state';
		return (false);
    }
	
	if (theForm.degree.selectedIndex == 0)
	{
		alert("Seleccione un Programa");
		theForm.degree.focus();
		location.href='#degree';
		return (false);
	}
  
	return (true);
}
//--></script>
