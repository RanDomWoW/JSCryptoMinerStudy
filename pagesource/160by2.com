		function validLogin(){
			var username=document.getElementById("username").value;
			var password=document.getElementById("password").value;
			var mobileRegExp = /^([0-9]+)$/;
			if(username=="" || username=="Enter Mobile Number"){
				alert("Enter your mobile number");
				document.getElementById("username").focus();
				return false;
			}
			else if(username.length!=10){
				alert("Enter valid mobile number");
				document.getElementById("username").focus();
				return false;
			}
			else if(username.substring(0,1)!=9 && username.substring(0,1)!=8 && username.substring(0,1)!=7 && username.substring(0,1)!=6){
		    	alert("Enter valid mobile number");	
		    	document.getElementById("username").focus();
			    return false;
		   	}
			else if(!mobileRegExp.test(username)){
				alert("Enter valid mobile number");
				document.getElementById("username").focus();
				return false;
			}
			else if(password=="" || password=="Password"){
				alert("Enter password");
				document.getElementById("password").focus();
				return false;
			}
			
			return true;
		}	
