    <script type="text/javascript">
        $('.filterbtn').click(function(e) {
            e.preventDefault();
            $('.filterbtn').removeClass('on');
            $(this).addClass('on');
            $('.filtercontainer').hide();
            $($(this).attr('href') + 'container').show();
        });
        $('.filtercontainer').each(function () {
            if ($(this).find('article').length < 9) $(this).find('.show-more-link').remove();
        });
        $('.filtercontainer .show-more-link').click(function(e) {
            e.preventDefault();
            $('#endtimeline').attr('rel', 'processing');
            var container = '#' + $(this).attr('rel') + 'container';
            $.ajax({
                url: '/ajax/home.aspx?type=' + $(this).attr('rel') + "&page=" + $(this).attr('page')
            }).done(function (msg) {
                var data = $('<div>' + msg + '</div>');
                if (data.find('article').length < 9) { $(container).find('.show-more-link').remove(); }
                //data.find('article').each(function() {
                //    if ($(container).find('article[rel="' + $(this).attr('rel') + '"]').length > 0) {
                //        $(this).remove();
                //    }
                //});
                $(container + ' > section').append(data.html());
                $('#endtimeline').attr('rel', '');
            });
            $(this).attr('page', parseInt($(this).attr('page')) + 1);
        });
        
        $(window).scroll(function () {
            var mytop = $(window).scrollTop();
            if ($('#endtimeline').attr('rel') === '') {
                var endtimeline = $('#endtimeline').offset().top - ($(window).height() - 100);
                
                if (mytop >= endtimeline && parseInt($('.filtercontainer:visible .show-more-link').attr('page')) < 5) {
                    $('.filtercontainer:visible .show-more-link').click();
                }
            }
        });
            </script>
