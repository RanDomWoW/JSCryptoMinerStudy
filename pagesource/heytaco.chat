<script type="text/javascript"> 

    // mobile menu
    if ( $('#menu').length ) {
        var slideout = new Slideout({
            'panel': document.getElementById('panel'),
            'menu': document.getElementById('menu'),
            'padding': 256,
            'tolerance': 70
        });

        // toggle mobile menu
        document.querySelector('.toggle-button').addEventListener('click', function() {
            slideout.toggle();

            if ( $('.menu-open').length) {
                $('.preview-banner').animate({
                    left: "0",
                }, 200);
                $('.preview-banner').removeClass('menu-open');
            } else {
                $('.preview-banner').animate({
                    left: "256",
                }, 200);      
                $('.preview-banner').addClass('menu-open');     
            }
                
        });
    }    

    // legacy mobile menu 
    $('.mobile-menu').click(function() {
        $('nav').toggle( "fast", function() {});
    });

    // dom ready
	$(document).ready(function (){

   	    // init moment
   		moment().format();

        // loading screen
        $('.global-nav .global-nav-item a, .live-mode-toggle').click(function(){
            $('#loading').show();

            // if longer than 3s show spinner
            setTimeout(function() {
                $('#loading').addClass('show-spinner');
            }, 1000);    
        }); 

   		// show tooltips
   		$('[data-toggle="tooltip"]').tooltip({html: true}); 

        $(".view-leaderboard").click(function(e){
            $('html, body').animate({
                scrollTop: $(".leaderboard").offset().top
            }, 800);
        });

        // event tracking
        $('.event-homepage-modal-top-btn').click(function() {
            ga('send','event','Click','Homepage Try HeyTaco! modal top button');
        });
        $('.event-homepage-modal-bottom-btn').click(function() {
            ga('send','event','Click','Homepage Try HeyTaco! modal bottom button');
        });
        $('.event-homepage-modal-addtoslack-btn').click(function() {
            ga('send','event','Click','Homepage modal Add to Slack button');
        });
        $('.create-account-link').click(function() {
            ga('send','event','Click','Create Account Modal');
        });
        $('.get-sticker').click(function() {
            ga('send','event','Click','Went to Sticker Survey');
        });
        $('.get-started-btn-pricing-page').click(function() {
            ga('send','event','Click','Get Started Button (Pricing Page)');
        });
        $('.add-to-slack-pricing-page').click(function() {
            ga('send','event','Click','Add to Slack Button (Pricing Page)');
        });  

        // event tracking v2
        $('.billboard .btn-add').click(function() {
            ga('send','event','Click','Homepage Billboard Add to Slack button');
        });  
        $('.activity .btn-add').click(function() {
            ga('send','event','Click','Homepage Activity Add to Slack button');
        });  
        $('.track-homepage-toggle').click(function() {
            ga('send','event','Click','Homepage Activity/Leaderboard Toggle');
        });  

        // track data downloads
        $('.download-taco-data').click(function() {
            ga('send','event','Click','Downloaded Taco Data');
        });  
        $('.download-redemptions-data').click(function() {
            ga('send','event','Click','Downloaded Redemptions Data');
        });  
        $('.download-user-data').click(function() {
            ga('send','event','Click','Downloaded Users Data');
        });  



         
        // contact form
        $('#contact-init').click(function() {
            $('#contact').velocity('transition.bounceUpIn', {
                duration: 500
            });
        });
    });
</script>

