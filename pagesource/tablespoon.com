    <script id="_registration.checkbox.tmpl" type="text/html">
        <div data-bind="css: CssClass, attr: { 'data-field-id': Id }">
            <span class="hint-text" data-bind="text: HintText"></span>
            <label class="optInContainer">
                <span>
                    <input type="checkbox" data-bind="checked: Value, attr: { 'id': FieldLabelLinkName, 'name': FieldLabelLinkName }" /></span>&nbsp;<span data-bind="    text: Label"></span>
            </label>
            <span class="previewLink" data-bind="visible: NewsletterLink"><a data-bind="    attr: { href: NewsletterLink }, text: $root.GlobalConfiguration.NewsletterLinkText" target="_blank"></a></span>
        </div>
    </script>

    <script id="_registration.question.tmpl" type="text/html">
        <div data-bind="css: CssClass, attr: { 'data-field-id': Id }">
            <label data-bind="text: Label, attr: { 'for': FieldLabelLinkName }" />
            <div class="question-answer" data-bind="foreach: SelectOptions">
                <label class="optInContainer">
                    <span>
                        <input type="checkbox" data-bind="checked: Selected, attr: { 'id': DataValue, 'name': DataValue }" /></span>&nbsp;<span data-bind="    text: DisplayText"></span>
                </label>
            </div>
        </div>
    </script>

    <script id="_registration.3zipcode.tmpl" type="text/html">
        <div data-bind="css: CssClass, attr: { 'data-field-id': Id }">
