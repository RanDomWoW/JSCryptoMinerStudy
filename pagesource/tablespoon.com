            <span class="hint-text" data-bind="text: HintText"></span>            
            <input type="text" min="5" max="9" data-bind="value: Value, validationElement: Value, attr: { 'id': FieldLabelLinkName, 'name': FieldLabelLinkName, 'required': IsMandatory ? '' : null }" />
            <span class="alert alert-error" data-bind="text: Error, visible: Error"></span>
        </div>
    </script>

    <script id="_registration.dateofbirthtextbox.month.tmpl" type="text/html">        
        <input class="monthPart" type="number" pattern="\d*" min="1" max="12" maxlength="2" data-bind="numericAutoTab: 2, value: MonthPart, attr: { 'id': 'month' + FieldLabelLinkName, 'name': 'month' + FieldLabelLinkName, 'placeholder': $root.GlobalConfiguration.MonthWatermarkText }" />
    </script>
    <script id="_registration.dateofbirthtextbox.day.tmpl" type="text/html">        
        <input class="dayPart" type="number" pattern="\d*" min="1" max="31" maxlength="2" data-bind="numericAutoTab: 2, value: DayPart, attr: { 'id': 'day' + FieldLabelLinkName, 'name': 'day' + FieldLabelLinkName, 'placeholder': $root.GlobalConfiguration.DayWatermarkText }" />
    </script>
    <script id="_registration.dateofbirthtextbox.year.tmpl" type="text/html">        
        <input class="yearPart" type="number" pattern="\d*" min="1900" max="2018" maxlength="4" data-bind="numericAutoTab: 4, value: YearPart, attr: { 'id': 'year' + FieldLabelLinkName, 'name': 'year' + FieldLabelLinkName, 'required': IsMandatory ? '' : null, 'placeholder': $root.GlobalConfiguration.YearWatermarkText }" />
    </script>
    <script id="_registration.dateofbirthtextbox.tmpl" type="text/html">
        <div data-bind="css: CssClass, attr: { 'data-field-id': Id }">
            <label data-bind="text: Label, attr: { 'for': 'month' + FieldLabelLinkName }"></label>
            <span class="hint-text" data-bind="text: HintText"></span>

            

            <!-- ko if: $root.DatePattern.HasYearBeforeMonthAndDay() -->
            <span data-bind="template: '_registration.dateofbirthtextbox.year.tmpl'"></span>
            <!-- /ko -->

            <!-- ko if: $root.DatePattern.HasMonthBeforeDay() -->
            <span data-bind="template: '_registration.dateofbirthtextbox.month.tmpl'"></span>
            <span data-bind="template: '_registration.dateofbirthtextbox.day.tmpl'"></span>
            <!-- /ko -->

            <!-- ko if: !$root.DatePattern.HasMonthBeforeDay() -->
            <span data-bind="template: '_registration.dateofbirthtextbox.day.tmpl'"></span>
            <span data-bind="template: '_registration.dateofbirthtextbox.month.tmpl'"></span>
            <!-- /ko -->

            <!-- ko if: !$root.DatePattern.HasYearBeforeMonthAndDay() -->
            <span data-bind="template: '_registration.dateofbirthtextbox.year.tmpl'"></span>
            <!-- /ko -->

            <!-- No simple way to validate three fields with one message, so we only require the year.  Server-side validation will handle the other fields.  -->
            <span class="dob-error alert alert-error" data-bind="text: Error, visible: Error"></span>
        </div>
    </script>

    <script id="_registration.dateofbirthdropdown.month.tmpl" type="text/html">
        <select class="monthPart" data-bind="value: MonthPart, options: MonthSelectOptions, optionsCaption: $root.GlobalConfiguration.MonthSelectionText, attr: { 'id': 'month' + FieldLabelLinkName, 'name': 'month' + FieldLabelLinkName }"></select>
    </script>
    <script id="_registration.dateofbirthdropdown.day.tmpl" type="text/html">
        <select class="dayPart" data-bind="value: DayPart, options: DaySelectOptions, optionsCaption: $root.GlobalConfiguration.DaySelectionText, attr: { 'id': 'day' + FieldLabelLinkName, 'name': 'day' + FieldLabelLinkName }"></select>
    </script>
    <script id="_registration.dateofbirthdropdown.year.tmpl" type="text/html">
        <select class="yearPart" data-bind="value: YearPart, options: YearSelectOptions, optionsCaption: $root.GlobalConfiguration.YearSelectionText, attr: { 'id': 'year' + FieldLabelLinkName, 'name': 'year' + FieldLabelLinkName }"></select>
    </script>
    <script id="_registration.dateofbirthdropdown.tmpl" type="text/html">
        <div data-bind="css: CssClass, attr: { 'data-field-id': Id }">
            <label data-bind="text: Label, attr: { 'for': 'month' + FieldLabelLinkName }"></label>
            <span class="hint-text" data-bind="text: HintText"></span>

            

            <!-- ko if: $root.DatePattern.HasYearBeforeMonthAndDay() -->
            <span data-bind="template: '_registration.dateofbirthdropdown.year.tmpl'"></span>
            <!-- /ko -->

            <!-- ko if: $root.DatePattern.HasMonthBeforeDay() -->
            <span data-bind="template: '_registration.dateofbirthdropdown.month.tmpl'"></span>
            <span data-bind="template: '_registration.dateofbirthdropdown.day.tmpl'"></span>
            <!-- /ko -->

            <!-- ko if: !$root.DatePattern.HasMonthBeforeDay() -->
            <span data-bind="template: '_registration.dateofbirthdropdown.day.tmpl'"></span>
            <span data-bind="template: '_registration.dateofbirthdropdown.month.tmpl'"></span>
            <!-- /ko -->

            <!-- ko if: !$root.DatePattern.HasYearBeforeMonthAndDay() -->
            <span data-bind="template: '_registration.dateofbirthdropdown.year.tmpl'"></span>
            <!-- /ko -->

            <!-- No simple way to validate three fields with one message, so we only require the year.  Server-side validation will handle the other fields.  -->
            <span class="dob-error alert alert-error" data-bind="text: Error, visible: Error"></span>
        </div>
    </script>

    <script id="customMessageTemplate" type="text/html">
        <!-- Used by the KO client-side validation plugin -->
        <span class="alert alert-error" style="display: none;" data-bind="validationMessage: field"></span>
    </script>

    <script id="progressBarTemplate" type="text/html">
        <div class="progressBar clearfix" data-bind="visible: IsProgressBarVisible && RegistrationSteps.length > 1">
            <!-- ko foreach: RegistrationSteps -->
            <div data-bind="attr: {'data-step-id': StepNumber, 'class': progressBarClass()}, style: { 'width': (100 / $root.RegistrationSteps.length) +'%' }">&nbsp;</div>
