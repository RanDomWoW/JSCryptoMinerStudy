            <span class="alert alert-error" data-bind="text: Error, visible: Error"></span>
        </div>
        <div data-bind="css: CssClass">
            <label data-bind="text: ConfirmFieldText, attr: { 'for': 'confirm' + FieldLabelLinkName }"></label>
            <span class="hint-text" data-bind="text: HintText"></span>            
            <input type="password" data-bind="hideShowPassword: {}, value: ConfirmPassword, attr: { 'id': 'confirm' + FieldLabelLinkName, 'name': 'confirm' + FieldLabelLinkName, 'required': IsMandatory ? '' : null }" />
            <span class="alert alert-error" data-bind="text: Error, visible: Error"></span>
