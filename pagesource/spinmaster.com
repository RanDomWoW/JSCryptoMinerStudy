            $(function () {
                //init
                $('.homebg').eq(0).css('background','url(' + $('.slide').eq(0).attr('data-bg') + ')');

                var carosel = $("#slideshow_home").find(".slide").carousel({
                    pagination: true,
                    width: '100%',
                    height: '100%',
                    navButton:true,
                    handleResize: true,
                    slideShow: 7000,
                    slideStopOnClick:true,
                    navColor:'cNavWhite',
                    id: "slidenav",
                    itemSelected: function (i) {
                        $('body').prepend('<div class=\'homebg\'><div>');
                        $('.homebg').eq(0).stop()
                            .css({
                                'opacity':'0',
                                'z-index':'2',
                                'background':'url(' + $(this)
                                    .find(".slide:eq(" + i + ")")
                                    .attr('data-bg') + ')'})
                                    .animate({ opacity: 1}, 600, function() {
                                        $(this).css({'z-index':'1'});
                                        $('.homebg').eq(1).remove();
                                    });
                    }
                });

                function revealWarning(){
                    $('#warning').css('display','block');
                    $('#warning').animate({opacity:'1',bottom:'20px'});
                }

                function hideWarning(){
                    $('#warning').animate({opacity:'0',bottom:'-100px'}, 300, function(){
                        $('#warning').css('display','none');
                    });
                }

                $('#warning #close').click(function(){
                    hideWarning();
                })

                //setTimeout(revealWarning, 1500);
                //setTimeout(hideWarning, 10000);
            });
        </script>

