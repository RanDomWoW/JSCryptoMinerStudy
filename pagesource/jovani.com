  recognition.continuous = true;
  recognition.interimResults = true;

  recognition.onstart = function() {
    recognizing = true;
    $('#start_button').addClass('blinkingrecord');
	$('#start_button i').addClass('fa-microphone');
	$('#start_button i').removeClass('fa-microphone-slash');
  };

  recognition.onerror = function(event) {
    if (event.error == 'no-speech') {
      $('#start_button').removeClass('blinkingrecord');
	  $(".blinkingrecord").css('color', "#ffffff");
	  $('#start_button i').addClass('fa-microphone-slash');
		$('#start_button i').removeClass('fa-microphone');
      ignore_onend = true;
    }
    if (event.error == 'audio-capture') {
      $('#start_button').removeClass('blinkingrecord');
	  $(".blinkingrecord").css('color', "#ffffff");
	  $('#start_button i').addClass('fa-microphone-slash');
	$('#start_button i').removeClass('fa-microphone');
      ignore_onend = true;
    }
    if (event.error == 'not-allowed') {
      ignore_onend = true;
	  $('#start_button i').addClass('fa-microphone-slash');
	$('#start_button i').removeClass('fa-microphone');
    }
  };

  recognition.onend = function() {
    recognizing = false;
	$('#start_button').removeClass('blinkingrecord');
	$(".blinkingrecord").css('color', "#ffffff");
    if (ignore_onend) {
      return;
    }
	$('#start_button i').addClass('fa-microphone');
	$('#start_button i').removeClass('fa-microphone-slash');
	
    if (!final_transcript) {
      return;
    }
  };

  recognition.onresult = function(event) {
    var interim_transcript = '';
    if (typeof(event.results) == 'undefined') {
      recognition.onend = null;
      recognition.stop();
      upgrade();
      return;
    }
    for (var i = event.resultIndex; i < event.results.length; ++i) {
      if (event.results[i].isFinal) {
        final_transcript += event.results[i][0].transcript;
      } else {
        final_transcript = event.results[i][0].transcript;
        interim_transcript = event.results[i][0].transcript;
      }
    }
    //final_transcript = capitalize(final_transcript);
    if (interim_transcript) {
	  
	  if (interim_transcript.trim() == 'clear' || interim_transcript.trim() == 'delete' || interim_transcript.trim() == 'start again') {
			  $('#search input').val('');
			  return;
		}
	  
	  if ($('#search input').val() != '') {
		  if (interim_transcript.trim() == 'go' || interim_transcript.trim() == 'search' || interim_transcript.trim() == 'submit' || interim_transcript.trim() == 'find') {
			  $('header input[name=\'search\']').parent().find('#submitsearch').trigger('click');
			  return;
		  }
	  }
	  $('#search input').val(final_transcript);
    }
	//if (final_transcript) {
		//$('#search input').val(final_transcript);
	//}
  };
}

function upgrade() {
  $('#start_button').hide();
}



function startButton(event) {
  if (recognizing) {
    recognition.stop();
    return;
  }
  final_transcript = '';
  //recognition.lang = select_dialect.value;
  recognition.start();
  ignore_onend = false;
  $('#start_button i').addClass('fa-microphone');
	$('#start_button i').removeClass('fa-microphone-slash');

  start_timestamp = event.timeStamp;
}

</script>
