<script>
    $(document).ready(function () {
        $('.tel').mask('(99) 9999-99999');

        $("input[type=tel]").mask("(99) 9999-99999");

        var c = new CookieDataForm('btn-content');
        var p = new PreMatricula();

        $('#form_posgraduacao').find('*').hide().attr('disabled', true);
        $('#form_graduacao').find('*').attr('disabled', true);

        $('.tipocurso').attr('disabled', false);
        $('.tipocurso>option').attr('disabled', false);

        $('.tipocurso').change(function () {
            var tipo_atual = $(this).val();
            if (tipo_atual == 'GRA' || tipo_atual == '') {
                $('#form_posgraduacao').find('*').hide('slow').attr('disabled', true);
                $('#form_graduacao').find('*').show('slow').attr('disabled', false);
            } else {
                $('#form_posgraduacao').find('*').show('slow').attr('disabled', false);
                $('#form_graduacao').find('*').hide('slow').attr('disabled', true);
            }
            $('.tipocurso').attr('disabled', false);
            $('.tipocurso>option').attr('disabled', false);
            $('.tipocurso').val(tipo_atual);
        });

        $('.setnomecurso').change(function () {
            $('.nomecurso').val($(this).find(' option[value=' + $(this).val() + ']').text());
        });


    });


    $(window).scroll(function () {
        if ($(this).scrollTop() > 80) {
            $('.barra_topo_social').addClass("some_top_icons");
            $('.menu_topo .navbar-nav>li>a').addClass("dimnui_menu");
            $('.menu_topo .navbar-brand').addClass("muda_logo");
        } else {
            $('.barra_topo_social').removeClass("some_top_icons");
            $('.menu_topo .navbar-nav>li>a').removeClass("dimnui_menu");
            $('.menu_topo .navbar-brand').removeClass("muda_logo");
        }
    });


</script>

