				  var days = Math.floor(distance / (1000 * 60 * 60 * 24));
				  var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
				  var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
				  var seconds = Math.floor((distance % (1000 * 60)) / 1000);
				  
				  // Variablen "nullen", wenn sie kleiner als Null sind
				  if (days < 0) {
					var days = 0
				  }
				  if (hours < 0) {
					var hours = 0
				  }
				  if (minutes < 0) {
					var minutes = 0
				  }
				  if (seconds < 0) {
					var seconds = 0
				  }
				  
				  // Tage-Variable je nach Stand setzen
				  if (days == 1) {
					var daystage = " Tag, "
				  } else {
					var daystage = " Tage, "
				  }

				  // Ausgabe der Zeit zusammensetzen, die im Ausgabe-Element angezeigt werden soll
				  document.getElementById("demo2").innerHTML = "Folgende Angebote gelten noch " + days + daystage + hours + " Std., "
