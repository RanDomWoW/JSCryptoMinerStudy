<script type="text/javascript">
	function col2Arestyle() {

		var $stories = $wn("#WNCol2 #DisplaySizeId7 .wnGroup.contentGroup");
		var i = 0;
		$stories.find('.wnItem.feature').each(function() {
			$wn(this).find('.summaryImage.abridged').prependTo(this);
			$wn(this).find('.summaryImage.abridged').append("<a href='#' class='play'><img src='http://ftpcontent.worldnow.com/wncustom/kitv/custom/images/video-Icon-white.png'></a>"); 
		}); 

		var leftArrow = "<a href='#' class='left_arrow'><img src='http://ftpcontent.worldnow.com/wncustom/kitv/custom/images/left_arrow.png'></a>";     
		var rightArrow = "<a href='#' class='right_arrow'><img src='http://ftpcontent.worldnow.com/wncustom/kitv/custom/images/right_arrow.png'></a>"; 

		$wn("#WNCol2 #DisplaySizeId7 .wnGroup.contentGroup").each(function() {
			$wn(this).addClass('slider');
			$wn(this).find(".feature").wrapAll("<ul id='csWrapper'></ul>");
			$wn(this).find("#csWrapper").before(leftArrow);
			$wn(this).find("#csWrapper").after(rightArrow); 

			/*start of next previous functionality of carousal*/
			var storyCount = $wn(this).find(".feature:visible").length, visibleStories = 4, count = 0;
			$wn(this).find("a.left_arrow").hide();
			$wn(this).find("#csWrapper").parent().css('min-width', '650px');
			$wn(this).find("#csWrapper").parent().css('width', (storyCount * 160).toString() + 'px');
			$wn(this).find("a.left_arrow").on("click", function(event) {
				event.preventDefault();
				if (count > 0) {
					$wn(this).parent().find('#csWrapper').animate({
						'left' : '+=155px'
					});
					count--;
					if (count <= 0)
						$wn(this).parent().find("a.left_arrow").hide();
				} else {
					$wn(this).parent().find("a.left_arrow").hide();
				}
				if (count < storyCount - visibleStories) {
					$wn(this).parent().find("a.right_arrow").show();
				}

			});

			$wn(this).find("a.right_arrow").on("click", function(event) {
				event.preventDefault();
				if (count < storyCount - visibleStories) {
					$wn(this).parent().find('#csWrapper').animate({
						'left' : '-=155px'
					});
					count++;
					if (count >= storyCount - visibleStories)
						$wn(this).parent().find("a.right_arrow").hide();
				} else {
					$wn(this).parent().find("a.right_arrow").hide();
				}
				if (count > 0) {
					$wn(this).parent().find("a.left_arrow").show();
				}
			});
			/*end of next previous functionlity of carousal*/

		});

		// append stories to video icon
		$('#csWrapper > li > div').each(function() {
			var link = $(this).find('a').attr('href');
			$('.play', this).attr('href', link);
		});

	}



