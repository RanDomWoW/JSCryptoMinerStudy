			$('.nav__menu .skip a').click(function(e) {
				e.preventDefault();
				var target = $(this).attr('href');
				$('.nav__menu').removeClass('focused');
				$(target + ' a:first').focus();
			});

			$('.nav__link--menu').click(function(e) {
				if ($('.nav__link--burger').css('display') == 'none' && !!('ontouchstart' in window) || !!('onmsgesturechange' in window)) {
