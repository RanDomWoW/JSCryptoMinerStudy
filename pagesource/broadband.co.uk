			$('.nav__link--menu, .nav__menu a, .nav__menu--checker input').blur(function() {
				if ($(this).hasClass('nav__link--menu')) {
					var parentid = $(this).parent().attr('id');
					$('#' + parentid + ' > .nav__menu').removeClass('focused');
