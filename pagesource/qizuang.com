        $('.pub-nav-hover').show().attr('state','zxmt-nav').find('.zxmt-nav').show().siblings('.zxgl-nav').hide();
    });
    $('.nav-list-meitu').mouseleave(function() {
        clearTimeout(timer1)
        $(this).removeClass('on').find('i').removeClass('fa-sort-asc').addClass('fa-sort-desc').css('line-height','77px');
        $(this).siblings().removeClass('on').find('i').removeClass('fa-sort-asc').addClass('fa-sort-desc').css('line-height','77px');
