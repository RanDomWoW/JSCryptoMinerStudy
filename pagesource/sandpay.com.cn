<script type="text/javascript">
	$(function () {
	    $(".mask").click(function(e) {
        if (e.target.tagName != "IMG") {
            $(document).scrollTop(s_t);
            $(".mask").fadeOut("fast");
            $("body").css({
                "position" : "static"
            });
        }
    });
    $.ajax({
          type: "POST",
          url: "https://www.sandpay.com.cn:443/getFooterResource",
          dataType: "json",
          success: function (data) {
            var resourceList = data.resourceList;
            var serviceList = data.serviceList;
			if(resourceList.length>0){
			    var html= "";
                      for(var i = 0 ; i<resourceList.length ; i++){
                          var r=resourceList[i];
                          var imgArr = r.resourceUrl.split(',');
                          if(imgArr.length > 1){
                              html+='<span><a href="javaScript:;" onclick="getCertificate(\'http://ceshi.sandpay.com.cn//editor_img/'+imgArr[0]+'\')">'+
                                  '<img src="http://ceshi.sandpay.com.cn//editor_img/'+imgArr[1]+'" style="width: auto;height: 28px;"/></a></span>';
                          }else{
                              html+='<span><a href="javaScript:;" onclick="toHref(\''+r.locationUrl+'\')">'+
                                  '<img src="http://ceshi.sandpay.com.cn//editor_img/'+imgArr[0]+'" style="width: auto;height: 28px;"/></a></span>';
                          }
                      }
				$('#footer-certificate').append(html);
				$('#footer-certificate').find('img').css('margin','0 4px 8px 0');
           }
           if(serviceList.length>0){
               var html= "";
               var ul=$('#productService-ul');
               for(var i = 0 ; i<serviceList.length ; i++){
                   var s=serviceList[i];
                   html+='<li><a href="javaScript:;" onclick="goHref(\''+s.newsId+'\',\''+s.contentType+'\')">'+s.newsTitle+'</a>' +
					   '<span id="'+s.newsId+'" style="display: none">'+s.newContent+'</span></li>';
               }
               ul.append(html);
           }
          },
			error: function (data) {
                layer.msg("系统繁忙，请稍后重试",{icon:2,offset: '400px'})
			}
        });
    });

	function toHref(url){
        if(url !='' && url != 'null'){
            window.open(url);
        }
	}

    function postToNewsDetail(id,type){
        var data = {"id":id,"nType":type};
        postForm("https://www.sandpay.com.cn:443/news/detail.html",data);
    }
    function postToNewsIndex(type){
        var data = {"nType":type};
        postForm("https://www.sandpay.com.cn:443/news/index.html",data);
    }
</script>

