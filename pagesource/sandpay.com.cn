<script type="text/javascript">
    $(function () {
        
        $('li[name="headOption"]').each(function(){
            var v = $(this).attr('value');
            if(v == '1'){
                $(this).addClass('active');
            }else{
                $(this).removeClass('active');
            }
        })
        $.ajax({
            type: "POST",
            url: "https://www.sandpay.com.cn:443/geHeadResource",
            dataType: "json",
            success:function (data){
                            var html = "";
                            var productList=data.productList;
                            var serviceList=data.serviceList;
                            for(var i =productList.length-1;i>= 0;i--){
                                var product = productList[i];
                                html+='<ul class=""><li>'+product.codeDesc+'</li>'+getHtml(serviceList,product.id)+'</ul>'
                            }
                           $('#productService').append(html);
                },
                error: function (data) {
                    layer.msg("系统繁忙，请稍后重试",{icon:2,offset: '400px'})
                }
            })
        function getHtml(data,parentId) {
            var html = "";
            if (data.length >0) {
                for (var i = 0; i < data.length;i++) {
                    var d = data[i];
                    if(d.businessType == parentId) {
                        html += '<li><a href="javaScript:;" onclick="goHref(\''+d.newsId+'\',\''+d.contentType+'\')" >' +
                            '<span class="pro-nav-width">' + d.newsTitle + '</span>' +
                            '<span style="display: none" id="'+d.newsId+'">' + d.newContent + '</span>' +
                            '<i class="pro-nav-arrow"></i></a></li>';
                    }
                }
            }
            return html;
        }
    })
    function goHref(newsId,type){
            if(type == '3'){
              var content= $('#'+newsId).text();
                window.open(''+content+'');
            }else{
                var data = {"id":newsId};
                postForm("https://www.sandpay.com.cn:443/product/toUIView",data);
            }ShopManagerController
    }



</script>

