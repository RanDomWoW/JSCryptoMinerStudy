                    swal('Lỗi',data.message,'error');
                }
            }
        });
            
    });          

    $('.request_price_send').on('click',function()
	{
        var product_id = $('.request_price_iditem').val().trim();
        var company = $('.request_price_company').val().trim();
        var customer = $('.request_price_customer').val().trim();
        var phone = $('.request_price_phone').val().trim();
        var email = $('.request_price_mail').val().trim();
        var address = $('.request_price_address').val().trim();
        var note = $('.request_price_note').val().trim();
        var quantity = $('.request_price_number').val().trim();

        var regex = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
        var rephone=/^\d{10,11}$/;

        var num = parseInt(quantity);            
        if(isNaN(num) || num<1)
		{
            swal('Chú ý','Số lượng sản phẩm không hợp lệ, phải là số và phải lớn hơn 1','warning');
            return false;
        }
		
        if((company=='') || (customer=='') || (phone=='')||(email=='') || (address==''))
		{
              swal('Chú ý','Cần phải nhập đủ tên công ty, người liên hệ, điện thoại, email, địa chỉ','warning');
              return false;
        }
            if(!rephone.test(phone)){
              swal('Chú ý','Số điện thoại phải có 10,11 số','warning');              
              return false;
            }

            if(!regex.test(email)){
              swal('Chú ý','Xin vui lòng nhập email hợp lệ','warning');               
              return false;
            }

            $.ajax({
                type: 'POST',
                cache: false,
                url: 'https://thegioicongnghiep.com/send-request-price',
                dataType: 'json',
                data: {
                        'product_id':product_id,
                        'company':company,
                        'customer':customer,
                        'phone':phone,
                        'email':email,
                        'address':address,
                        'note':note,
                        'quantity':quantity
                      },
                success: function(data) {    
                  if(data.isOk==1){             
                    $('#form_request_price').find('.modal-body').children().remove();
                    $('#form_request_price').find('.close').click();
                    swal('Thành công',data.message,'success'); 
                    
                  }else{
                    swal('Lỗi',data.message,'error');
                  }
                }
        });
    })        
})
</script>
