                    $(".LanguageListContainer .UpDownArrow").css("border-bottom", "thin solid dimgray");

                    $(".CountryFlagBackground").css("border-bottom", "thin solid dimgray");



                    var LanguageClickedInfo = $(this).attr('class');

                    Debug("In SecondaryNavigation.ascx, LanguageClickedInfo: " + LanguageClickedInfo);

                    LanguageClickedInfoGlobal = LanguageClickedInfo;

                    if (SetCookiesServerSide) {

                        try {

                            // **** JFD: Put this in - wasn't necessary originally

                            SetLanguagePersistenceCookieClientside2(LanguageClickedInfo);
                            DeleteAllCookiesWithSignature(LanguageCookieFormatStringPart1);

                            // Set LanguagePersistence Cookie server-side

                            TransferLanguageDropdownClickInfoBackToServer('/ehAjax/ProcessLanguageBarClickInfoFromClient.ashx', LanguageClickedInfo, RequestPath, AreCookiesEnabledInBrowser);

                        }

                        catch (e) {

                            Debug("Exception occurred in when calling SecondaryNavigation.ascx/TransferLanguageBarClickInfoBackToServer(): " + e.message);

                            SetLanguagePersistenceCookieClientside(LanguageClickedInfo);

                        }

                    }

                    else {

                        SetLanguagePersistenceCookieClientside(LanguageClickedInfo);

                    }

                });

            }

            catch (e) {

            }

        });

        var EscapeKeycode = 27;

        $(window).keyup(function (evt) {

            if (evt.keyCode == EscapeKeycode) {

                HideElement($(".CountryFlagMenu"));

                evt.stopPropagation();

                evt.preventDefault();

            }

        });


        function Debug(msg, ShowAlert) {

            ShowAlert = false;

            try {

                if (window.console)

                    console.log(msg);

                if (ShowAlert === true)

                    alert(msg);

            }

            catch (e) {

                // if fails - give up

            }

        }
        
        function HideElement(element) {

            element.css('display', 'none');

            element.hide();

        }

        function ShowElement(element) {

            element.css('display', 'inline-block');

            element.show();

        }


        function SetLanguagePersistenceCookieClientside(LanguageClickedInfo) {

            try {

                // LanguageClickedInfo is typically of the form: trans-xx [active]

                if (LanguageClickedInfo != null) {

                    var LanguageCode = LanguageClickedInfo.replace("active", "").trim().split("-")[1];

                    if (LanguageCode != "") {

                        SetValueForAllCookiesWithSignature(LanguageCookieFormatStringPart1, "false", null);

                        // ***** JFD: Put this in - wasn't necessary originally

                        DeleteAllCookiesWithSignature(LanguageCookieFormatStringPart1);

                        var CookieName = LanguageCookieFormatStringPart1 + LanguageCode;

                        SetSessionCookieValue(CookieName, "true", "/", true);

                    }

                }

            }

            catch (e) {

                Debug("Exception occurred in SecondaryNavigation.ascx/SetLanguagePersistenceCookieClientside(): " + e.message);

            }

        }


        function SetLanguagePersistenceCookieClientside2(LanguageClickedInfo) {

            try {

                // LanguageClickedInfo is typically of the form: trans-xx [active]

                if (LanguageClickedInfo != null) {

                    var LanguageCode = LanguageClickedInfo.replace("active", "").trim().split("-")[1];

                    if (LanguageCode != "") {

                        SetValueForAllCookiesWithSignature(LanguageCookieFormatStringPart1, "false", null);

                        // **** JFD: Put this in - wasn't necessary originally

                        DeleteAllCookiesWithSignature(LanguageCookieFormatStringPart1);

                        var CookieName = LanguageCookieFormatStringPart1 + LanguageCode;

                        //SetSessionCookieValue(CookieName, "true", "/", true);

                    }

                }

            }

            catch (e) {

                Debug("Exception occurred in TransferLanguageBarClickInfoBackToServer(): " + e.message);

            }

        }


        function TransferLanguageDropdownClickInfoBackToServer(BaseUrlOfAjaxHandler, LanguageBarClickInfo, RequestPath, AreCookiesEnabledInBrowser) {

            var UrlOfAjaxHandler = BaseUrlOfAjaxHandler + "?LanguageBarClickInfo=" + LanguageBarClickInfo + "&RequestItemPath=" + RequestPath + "&AreCookiesEnabledInBrowser=" + AreCookiesEnabledInBrowser;

            $.ajax({

                type: "GET",
                url: UrlOfAjaxHandler,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: false,
                cache: false,
                success: function (result) {
                    // alert(result);

                },

                error: function (xhr, ajaxOptions, thrownError) {

                    SetLanguagePersistenceCookieClientside(LanguageClickedInfoGlobal);

                    Debug("An error occurred in SecondaryNavigation.ascx/TransferLanguageDropdownClickInfoBackToServer() with xhr.status=" + xhr.status + ", thrownError=" + thrownError + ", ajaxOptions=" + ajaxOptions);

                }

            });

        }

        function AreCookiesEnabled() {

            function CreateCookie(name, value, days) {

                var expires = "";

                if (days) {

                    var date = new Date();

                    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));    // milliseconds

                    expires = "; expires=" + date.toGMTString();

                }

                else
                    expires = "";

                document.cookie = name + "=" + value + expires + "; path=/";
            }
            
            function ReadCookie(name) {

                var nameEQ = (name + "=");

                var CookieParts = document.cookie.split(';');

                for (var i = 0; i < CookieParts.length; i++) {

                    var CookiePart = CookieParts[i];
                                        
                    while (CookiePart.charAt(0) === ' ')

                        CookiePart = CookiePart.substring(1, CookiePart.length);
                    
                    if (CookiePart.indexOf(nameEQ) === 0)

                        return CookiePart.substring(nameEQ.length, CookiePart.length);
                }

                return null;
            }

            function EraseCookie(name) {

                CreateCookie(name, "", -1);     // 1 day ago

            }

            var AreCookiesEnabledResult = false;

            var TestCookieName = "TestCookieName";

            var TestCookieValue = "TestCookieValue";

            var ErrorMessage = "Your browser is not setup to allow cookies";

            var ReadTestCookieValue = "";



            try {

                CreateCookie(TestCookieName, TestCookieValue, 1);   // 1 day

                ReadTestCookieValue = (ReadCookie(TestCookieName) !== null) ? ReadCookie(TestCookieName) : "";

                if (ReadTestCookieValue === TestCookieValue) {

                    AreCookiesEnabledResult = true;

                    EraseCookie(TestCookieName);

                }

            }

            catch (e) {

                Debug("Exception occurred in SecondaryNavigation.ascx/AreCookiesEnabled(): " + e.message);

            }

            return AreCookiesEnabledResult;

        }

        function getWidthOfText(txt, fontname, fontsize) {

            // Create a dummy canvas (render invisible with css)

            var c = document.createElement('canvas');

            var ctx = c.getContext('2d');

            ctx.font = fontsize + 'px' + fontname;

            var length = ctx.measureText(txt).width;

            return length;

        }

    }();

</script>



