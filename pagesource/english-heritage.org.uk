                    $('.LanguageListContainer .UpDownArrow').css("border-bottom", "thin solid dimgray");

                    $(".CountryFlagBackground").css("border-bottom", "thin solid dimgray");
                }

            });    

            try {

                // if click on a language within the language dropdown, then transfer relevant click info to server so that a 'selected language' cookie may be set

                $('.CountryFlagMenu ul li').click(function () {



                    HideElement($(".CountryFlagMenu"));

                    $(".CountryFlagBackground").css("background-color", "#f1eee7");

                    $(".LanguageListContainer .UpDownArrow").css({

