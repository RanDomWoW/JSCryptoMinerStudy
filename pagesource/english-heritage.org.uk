    EH_REGIONAL_MAPS.RegionMaps = function () {

        var gIsAlert = false;

        var gDefaultLat = "0";            //  '51.178899700000000000';    // stonehenge - in 'South West' region
        var gDefaultLong = "0";          //  '-1.826117100000033100';    // stonehenge - in 'South West' region

        function Debug(msg, ShowAlert) {
            //ShowAlert = false;
            try {
                if (window.console)
                    console.log(msg);

                if (ShowAlert === true)
                    alert(msg);
            }
            catch (e) {
                // if fails - give up
            }
        }

        $(document).ready(function () {

            var DefaultJsonOutput = { "PathOfCurrentMapImage" : "http://www.english-heritage.org.uk/remote/www.english-heritage.org.uk/content/images/England?quality=60&anchor=middlecenter", 
                                             "PathOfRelatedHighlightPropertyToBeViewableOnHomePage" : "/visit/places/stonehenge/" ,
