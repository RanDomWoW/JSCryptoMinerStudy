        // success callback for getCurrentPosition() function
        function GetCurrentPositionSuccess(Position) {

            var GeoInfo = {};

            try {
                GeoInfo.Lat = Position.coords.latitude;
                GeoInfo.Long = Position.coords.longitude;
            }
            catch (e)
            {
                GeoInfo.Lat = gDefaultLat;
                GeoInfo.Long = gDefaultLong;
                Debug("Exception error occurred in RegionalMaps.GetCurrentPositionSuccess callback - " + e.message, gIsAlert);
            }

            GetRegionImageAndHighlightPropertyInfoAsJson(GeoInfo.Lat, GeoInfo.Long);
        }

        // error callback for getCurrentPosition() function
        function GetCurrentPositionError(PositionError) {
            Debug('RegionalMaps.GetCurrentPositionError() callback called resulting from getCurrentPosition() with error code: (' + PositionError.code + '): ' + PositionError.message, gIsAlert);

            var GeoInfo = {};
            GeoInfo.Lat = gDefaultLat;
            GeoInfo.Long = gDefaultLong;

            GetRegionImageAndHighlightPropertyInfoAsJson(GeoInfo.Lat, GeoInfo.Long);
        }

        function GetRegionImageAndHighlightPropertyInfoAsJson(Lat, Long) {

            // Passing the PageId is vital for recovering the RequestItem object in the ajax handler on the server
            var PageId = "487";

            var url = "/ehajax/NM_Ajax/GetRegionImageAndHighlightPropertyInfo.ashx?Lat=" + Lat + "&Long=" + Long + "&PageId=" + PageId;

            $.ajax({
                type: "GET",
                url: url,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: false,
                cache: false,
                success: function (msg) { GetJsonResultsSuccess(msg); },
                error: function (msg) { GetJsonResultsError(msg); }
            });
        }

        function GetMapAndHighlightPlaceInfo(json)
        {
            try
            {
                // attach region image
                $('.property-finder-map-box').css('background', "url('" + json.PathOfCurrentMapImage + "') no-repeat scroll center center / cover rgba(0, 0, 0, 0)");
                // $('.property-finder-map-box').css('width', '100%');
                $('.property-finder-map-box').get(0).setAttribute('title', json.CurrentRegion);

                // attach highlight property image, link and title (hint text)
                var HpBannerBlockPropertyFeatureElement = $('#hp-banner-block-property-feature').get(0);            // document.getElementById("hp-banner-block-property-feature"); 
                HpBannerBlockPropertyFeatureElement.setAttribute("title", json.TitleOfRelatedHighlightPropertyToBeViewableOnHomePage);
                HpBannerBlockPropertyFeatureElement.setAttribute("href", json.PathOfRelatedHighlightPropertyToBeViewableOnHomePage);
                //HpBannerBlockPropertyFeatureElement.style.background = "url('" + json.PathOfRelatedHighlightPropertyToBeViewableOnHomePageImageItem + "') no-repeat scroll center center / cover rgba(0, 0, 0, 0)";

                //The banner div to put the image in HpBannerBlockPropertyFeatureElementDiv.
                $('#hp-banner-block-property-feature > div').attr('data-interchange', "[" + json.PathOfRelatedHighlightPropertyToBeViewableOnHomePageImageItem + ", (default)], [" + json.PathOfRelatedHighlightPropertyToBeViewableOnHomePageImageItemSmall + ", (small)], [" + json.PathOfRelatedHighlightPropertyToBeViewableOnHomePageImageItemMedium + ", (medium)], [" + json.PathOfRelatedHighlightPropertyToBeViewableOnHomePageImageItem + ", (large)]");
                //$('#hp-banner-block-property-feature > div').css('background', "url('" + json.PathOfRelatedHighlightPropertyToBeViewableOnHomePageImageItem + "') no-repeat scroll center center / cover rgba(0, 0, 0, 0)");
                //These both need to be called otherwise it doesn't know what to do.
                $(document).foundation('interchange', 'reflow');
                $(document).foundation('interchange', 'resize');
                
                // attach content title
                var ContentItemTitleElement = $('#content-item-title').get(0);
                ContentItemTitleElement.textContent = json.TitleOfRelatedHighlightPropertyToBeViewableOnHomePage;   // document.getElementById("content-item-title")

                // attach event associated with highlight property
                if (json.HighlightPropertyNextEventTitle) {
                    $('.property-event-highlight a').text(json.HighlightPropertyNextEventTitle);
                    $('.property-event-highlight a').attr('href', json.HighlightPropertyNextEventPath);
                }
                else {
                    $('.property-event-highlight').hide();
                };
                if (json.CurrentRegion == "")
                    $('.hp-banner-block.property-feature span.content-item-prefix').text('');
                else
                    $('.hp-banner-block.property-feature span.content-item-prefix').text('Near you');
            }
            catch (e) {
                var CurrentLat = Position.coords.latitude;
                var CurrentLong = Position.coords.longitude;
            }
        }

        function GetJsonResultsSuccess(json) {

            try
            {
                GetMapAndHighlightPlaceInfo(json);
                
            }
            catch (e) {
                Debug("Exception error occurred in RegionalMaps.GetJsonResultsSuccess callback - " + e.message, gIsAlert);
            }
        }

        function GetJsonResultsError(json) {
            Debug("Error occurred in RegionalMaps.GetJsonResultsError callback  ", gIsAlert);
        }

    }();

</script>


