	function getColors(thisColor,set,total){
	
		var alpha;
		if (thisColor > 10){
			alpha = 1.0;
			thisColor = thisColor - 10;
		}else{
			alpha = 0.9;
		}
		var defColors = [
			["rgba(255,255,255,1.0)",	// default
			
			"rgba(0,64,135,"+alpha+")",
			"rgba(10,118,140,"+alpha+")",
