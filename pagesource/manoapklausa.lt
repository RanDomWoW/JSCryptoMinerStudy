			}else if (thisColor == 3){
				thisColor = 5;
			}
		}else if (total == 4){
			if (thisColor == 2){
				thisColor = 3;
			}else if (thisColor == 3){
				thisColor = 5;
			}else if (thisColor == 4){
				thisColor = 7;
			}
		}
		var color = defColors[set-1][thisColor];
		return color;
	}

	
	function show_main(show,hide) {
		var i=0;
		while (i < 6) {
			i++;
			var hide_a = hide+i;
			var elem = document.getElementById(hide_a);
			if (elem){
				document.getElementById(hide_a).style.display = 'none';
			}
		}
		document.getElementById(show).style.display = 'block';	
	}
	
	function chColorsSet(cc,set,type){
		if (type == 3 || type == 4){
			var clra;
			clra = getColors(1,set,1);
			return clra;
		}else{
			clrb = new Array; 
			var i=0;
			while (i < cc) {		
				i++;
				clrb[i-1] = getColors(i,set,cc);
			}		
			return clrb;
		}
	}	
	
