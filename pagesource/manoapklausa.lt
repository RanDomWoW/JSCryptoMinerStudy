		},			
		afterDatasetsDraw: function(chart) {
			
			var ctx = chart.chart.ctx;
			ctx.restore();
			// title
			var i=0;
			while (i < 4) {
				if (chart.config.options.elements.posX){
					if (chart.config.options.elements.posX[i]){
					
						if (chart.config.options.elements.smSize > 15){
							ctx.font = '16px Arial';
						}else{
							ctx.font = '14px Arial';
						}
