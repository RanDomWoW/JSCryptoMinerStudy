<script async src="https://cdn.klarna.com/1.0/code/client/all.js"></script><script id="veplatform-masterdata">
var veData = {"currency":{"isoCode":"NOK","isoCodeNum":"578","name":"Norsk krone","sign":"kr "},"language":{"isoCode":"no","languageCode":"nb-no","name":"Norsk"},"culture":{"dateFormatFull":"Y\/m\/d H:i:s"},"user":{"email":null,"firstName":null,"lastName":null},"currentPage":{"currentUrl":"https:\/\/e-wheels.no\/","orderId":null,"currentPageType":"home","product":{"category":null,"description":null,"descriptionShort":null,"images":{"partialImagePath":null,"fullImagePath":null},"name":null,"productLink":null,"manufacturerName":null,"priceCurrent":null,"priceWithoutDiscount":null,"priceDiscount":null,"productId":null}},"history":{"productHistory":[],"lastVisitedCategory":{"name":null,"link":null}},"cart":{"dateUpd":null,"promocode":{"code":null,"name":null,"type":null,"value":null},"totalPromocodeDiscount":null,"totalPrice":null,"totalProducts":null,"products":[],"taxes":[]}}; // Ve Interactive cannot detect it if it's inside the IIFE
(function(){
    if ('sessionStorage' in window) {
        veData.history = getHistory()
        var type = $('meta[property="og:type"]').prop('content')

        if (type === 'product.group') {
            veData.history.lastVisitedCategory = {
                name: $('meta[property="og:title"]').prop('content'),
                link: $('meta[property="og:url"]').prop('content'),
            };
        }

        if (type === 'product') {
            appendProductHistory()
            veData.history.lastVisitedCategory = {
                name: $('meta[property="product:category"]').prop('content'),
                link: $('meta[property="product-category-link"]').prop('content'),
            };
        }

        setHistory(veData.history)
    }

    function setHistory(data) {
        try {
            sessionStorage.setItem('veData.history', JSON.stringify(data))
        } catch(err) {
            'console' in window && console.error(err)
        }
    }

    function getHistory() {
        try {
            return JSON.parse(sessionStorage.getItem('veData.history')) || veData.history
        } catch(err) {
            return veData.history
        }
    }

    function appendProductHistory() {
        if (veData.history.productHistory.length > 0) {
            var shouldAppendToHistory = true;
            veData.history.productHistory.forEach(function(product){
                if (product.productLink === veData.currentPage.product.productLink) {
                    shouldAppendToHistory = false;
                }
            });

            if (!shouldAppendToHistory) {
                return;
            }
        }

        veData.history.productHistory.push(veData.currentPage.product);
    }

    if ('KcoCheckoutStore' in window) {
        function pad(number) {
          if (number < 10) {
            return '0' + number;
          }
          return number;
        }
        var base_url = "https:\/\/e-wheels.no\/", currency_sign = "kr ";

        function dataMapper(products) {
            return products.map(function(product){
                var image = product.getIn(['xx', 'image']);
                return {
                    category: product.getIn(['xx', 'category'], null),
                    description: null,
                    descriptionShort: product.getIn(['xx', 'ingress'], null),
                    images: {
                        partialImagePath: image,
                        fullImagePath: base_url + image
                    },
                    name: product.getIn(['xx', 'title']),
                    productLink: base_url + product.getIn(['xx', 'product_path']),
                    manufacturerName: product.getIn(['xx', 'manufacturer'], null),
                    priceCurrent: currency_sign + product.getIn(['xy', 'price']),
                    priceWithoutDiscount: currency_sign + product.getIn(['xy', 'org_price']),
                    priceDiscount: currency_sign + product.getIn(['xy', 'discount']),
                    productId: product.getIn(['xx', 'id']),
                    quantity: product.get('qty'),
                    productSubTotal: currency_sign + product.getIn(['xy', 'subtotal']),
                }
            }).toArray();
        }

        // Update veData with new data from the cart state
        var unsubscribe = KcoCheckoutStore.subscribe(function(){
            veData.user.email = KcoCheckoutStore.getIn(['data', 'post','data','levadr','epost']);
            veData.user.firstName = KcoCheckoutStore.getIn(['data', 'post','data','levadr','fornavn']);
            veData.user.lastName = KcoCheckoutStore.getIn(['data', 'post','data','levadr','etternavn']);

            var now = new Date;
            veData.cart.dateUpd = now.getUTCFullYear() +
                                '-' + pad(now.getUTCMonth() + 1) +
                                '-' + pad(now.getUTCDate()) +
                                ' ' + pad(now.getUTCHours()) +
                                ':' + pad(now.getUTCMinutes()) +
                                ':' + pad(now.getUTCSeconds());

            veData.cart.products = dataMapper(KcoCheckoutStore.getIn(['data', 'contents']));

            veData.cart.totalPrice = currency_sign + KcoCheckoutStore.getIn(['data', 'total_pricified', 'sum_total']);
            veData.cart.totalProducts = currency_sign + KcoCheckoutStore.getIn(['data', 'total_pricified', 'total']);
            veData.cart.taxes.taxValue = currency_sign + KcoCheckoutStore.getIn(['data', 'total_pricified', 'sum_taxes']);

            var discountTotal = KcoCheckoutStore.getIn(['data', 'discountTotal']);
            if(discountTotal !== false) {
                var discountCode = KcoCheckoutStore.getIn(['data', 'discount', 'code']);
                veData.cart.promocode = {
                    code: discountCode,
                    name: discountCode,
                    type: 'percent',
                    value: KcoCheckoutStore.getIn(['data', 'discount', 'discount'])
                };
                veData.cart.totalPromocodeDiscount = discountTotal;
            } else {
                veData.cart.promocode = {code: null, name: null, type: null, value: null}
                veData.cart.totalPromocodeDiscount = null;
            }
        })
    }
})();
</script>
