        $.ajax({
            url: "http://www.phonero.no/wp-content/themes/Phonero/load_articles.php",
            data: { userType : userResult, catType: category },
            method: "post",
            dataType: 'html',
            success: function(result) {
				$('#circleG').hide();  //<--- hide again
                $(".loadArticles").html(result).promise().done(function(){
				load_autocomplete(userResult);
			    });
				$(".categ_boxes").css('display', 'block');
