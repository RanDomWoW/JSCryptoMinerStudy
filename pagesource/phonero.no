		 $('.joomag_data').removeClass('remove');
	 });
	
	//Check if cookie is already set
	var user = $.cookie('userType');
	if(user){
		var userResult = $.cookie('userType');
        var category = "ofte-stilte-sporsmal";

        //Check if Cookie set to Privat
        if(userResult == "PRIVAT"){
            $(".fsearch-fd").css('display', 'none');
