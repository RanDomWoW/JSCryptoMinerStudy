            $(".checkSubsciption").css('border-color','red');
        }

        //If Admin entered PRIVAT
        else if(sub_type == "PRIVAT"){
            $.cookie('userType', sub_type, { expires: 30, path: '/' });
            var typeUser = sub_type;
            $(".fsearch-fd").css('display', 'none');
            $(".alert_box").css('display', 'none');
            $(".velkommen").css('display', 'block');
            $(".logg_out").show();
        }

        //For Admin to check results Quickly
		else if((sub_type == "BEDRIFT") || (sub_type == "BEDRIFTZTE") || (sub_type == "SP") || (sub_type == "UNKNOWN")){
			$.cookie('userType', sub_type, { expires: 30, path: '/' });
			var typeUser = sub_type;
            var category = "ofte-stilte-sporsmal";
            $('#circleG').show();  //<--- Show Gif
            $.ajax({
                url: "http://www.phonero.no/wp-content/themes/Phonero/load_articles.php",
                data: { userType : sub_type, catType: category },
                method: "post",
                dataType: 'html',
                success: function(result) {
								$('#circleG').hide();  //<--- Hide Gif
                    $(".alert_box").css('display', 'none');
								$(".logg_out").show();
                $(".fsearch-fd").css('display', 'none');
                $(".show_srch_cont").css('display', 'block');
                $(".loadArticles").html(result).promise().done(function(){
								load_autocomplete(sub_type);
		    			});
                    $.ajax({
                        url: "http://www.phonero.no/wp-content/themes/Phonero/load_logins.php",
                        data: { userType : typeUser, catType: category },
                        method: "post",
                        dataType: 'html',
                        success: function(result) {
                            $(".logginn_content").html(result);
														$(".logginn_tab").css('display','block');
                        }
                    });
                }
            });
			//Display Articles on Mobile
            $.ajax({
                url: "http://www.phonero.no/wp-content/themes/Phonero/load_articles_mb.php",
                data: { userType : sub_type, catType: category },
                method: "post",
                dataType: 'html',
                success: function(result) {
								$(".loadArticlesMB").html(result).promise().done(function(){
									load_mobile_autocomplete(sub_type);
					    	});
				}
			});
		}

        //For Users to return result for entered Number
        else{
            $(".checkSubsciption").css('border-color','#cccccc');
            $('#circleG').show(); //<----here
            $.ajax({
                url: "http://www.phonero.no/wp-content/themes/Phonero/check-membership.php",
                data: { subType : sub_type, catType: category },
                method: "post",
                dataType: 'html',
                success: function(result) {
					if(result == '' || result=='UNKNOWN'){
                        $(".alert_box").css('display', 'block');
						/*alert("You've Entered a wrong value!");*/
						$('#circleG').hide();  //<--- hide again
						return false;
					}
					$.cookie('userType', result, { expires: 30, path: '/' });
                    if(result=="PRIVAT"){
                        $('#circleG').hide();
                        $(".alert_box").css('display', 'none');
                        $(".fsearch-fd").css('display', 'none');
                        $(".logg_out").show();
                        $(".velkommen").css('display', 'block');
                        return false;
                    }
					var typeUser = result;
                    var category = "ofte-stilte-sporsmal";
                    $('#circleG').hide();  //<--- hide again
                    $.ajax({
                        url: "http://www.phonero.no/wp-content/themes/Phonero/load_articles.php",
                        data: { userType : result, catType: category },
                        method: "post",
                        dataType: 'html',
                        success: function(result) {
                            $(".alert_box").css('display', 'none');
							$(".logg_out").show();
                            $(".fsearch-fd").css('display', 'none');
                            $(".show_srch_cont").css('display', 'block');
                            $(".loadArticles").html(result).promise().done(function(){
				load_autocomplete(typeUser);
			    });
		                    $.ajax({
		                        url: "http://www.phonero.no/wp-content/themes/Phonero/load_logins.php",
		                        data: { userType : typeUser },
		                        method: "post",
		                        dataType: 'html',
		                        success: function(result) {
		                            $(".logginn_content").html(result);
									$(".logginn_tab").css('display','block');
		                        }
		                    });
                        }
                    });
					//Display Articles on Mobile
		            $.ajax({
		                url: "http://www.phonero.no/wp-content/themes/Phonero/load_articles_mb.php",
		                data: { userType : typeUser, catType: category },
		                method: "post",
		                dataType: 'html',
		                success: function(result) {
							$(".loadArticlesMB").html(result).promise().done(function(){
						load_mobile_autocomplete(typeUser);
					    });
						}
					});
                }
            });
        }
    });
	
