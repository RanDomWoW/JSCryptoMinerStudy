$(document).ready(function() {

    var url = window.location.hash;

    if(url=="#abonnementsvilkar"){
        $("#tab_nav").tabs({
            active: 2
        })
        $("#528").prev().children('a').removeClass('collapsed');
        $("#528").addClass('in');
        $("#528").css('height','auto');
    }
	
	var decodeEntities = (function() {
	// this prevents any overhead from creating the object each time
	var element = document.createElement('div');
      
	function decodeHTMLEntities (str) {
	  if(str && typeof str === 'string') {
	    // strip script/html tags
	    str = str.replace(/<script[^>]*>([\S\s]*?)<\/script>/gmi, '');
	    str = str.replace(/<\/?\w(?:[^"'>]|"[^"]*"|'[^']*')*>/gmi, '');
	    element.innerHTML = str;
	    str = element.textContent;
	    element.textContent = '';
	  }
      
	  return str;
	}
      
	return decodeHTMLEntities;
      })();

	
	
	var load_autocomplete = function(sub_type){
		$( "#autosearch" ).val("");
		$( "#autosearch" ).autocomplete({
			    source: 'http://www.phonero.no/wp-admin/admin-ajax.php?action=search_posts&userType='+sub_type,
			    focus: function(event, ui) {
					// prevent autocomplete from updating the textbox
					event.preventDefault();
					// manually update the textbox
					$(this).val(decodeEntities(ui.item.label.replace("&amp;","&" )));
			    },
			    select: function( event, ui ) {
				event.preventDefault();
				$(this).val(decodeEntities(ui.item.label.replace("&amp;","&" )));
				load_article_page(ui.item.value,decodeEntities(ui.item.label),'desktop');
			    },
			    create: function () {
				$(this).data('ui-autocomplete')._renderItem = function (ul, item) {
				    return $("<li></li>")
					.data("item.autocomplete", item)
					.append("<a>" + decodeEntities(item.label) + "</a>")
					.appendTo(ul);
				};
			    }
			}).autocomplete("widget").addClass("article_search");
	}
	
	var load_mobile_autocomplete = function(sub_type){
		$( "#autosearchmb" ).val("");
		$( "#autosearchmb" ).autocomplete({
			    source: 'http://www.phonero.no/wp-admin/admin-ajax.php?action=search_posts&userType='+sub_type,
			    focus: function(event, ui) {
					// prevent autocomplete from updating the textbox
					event.preventDefault();
					// manually update the textbox
					$(this).val(decodeEntities(ui.item.label.replace("&amp;","&" )));
				},
			    select: function( event, ui ) {
				$(this).val(decodeEntities(ui.item.label.replace("&amp;","&" )));
				event.preventDefault();
				load_article_page(ui.item.value,decodeEntities(ui.item.label),'mobile');
			    },
			    create: function () {
				$(this).data('ui-autocomplete')._renderItem = function (ul, item) {
				    return $('<li>')
					.append(decodeEntities(item.label))
					.appendTo(ul);
				};
			    }
			}).autocomplete("widget").addClass("article_search");
	}
	
	var load_article_page = function(a_id,t,f){
		$(".loadArticles").css('display', 'none');
		$(".loadArticlesMB").css('display', 'none');
		$(".categ_boxes").css('display', 'none');
		$(".off_hide").css('display', 'none');
		$(".cat_name").html(t);
		$(".brd_crumbs").css('display', 'block');
		if (f=='desktop') {
				$.ajax({
				url: "http://www.phonero.no/wp-content/themes/Phonero/load_article_single.php",
				data: { post_id : a_id},
				method: "post",
				dataType: 'html',
				success: function(result) {
				
					$(".loadArticles").html(result).promise().done(function(){
					    //load_autocomplete(sub_type);
					});
					$(".loadArticles").css('display', 'block');
				}
				});
			}else{
				$.ajax({
				url: "http://www.phonero.no/wp-content/themes/Phonero/load_article_single_mobile.php",
				data: { post_id : a_id},
				method: "post",
				dataType: 'html',
				success: function(result) {
						$(".loadArticlesMB").html(result).promise().done(function(){
						    //load_autocomplete(sub_type);
						});
						$(".loadArticlesMB").css('display', 'block');	
				}
			    });	
			}
	}
	
    //Joomag Show Script
	$('#show_joomag').click(function() {
		$('.small_mag').hide();
		$('.joomag_data').toggle( 'blind', 500 );
		$('#hide_joomag').addClass('ShowCross');
		$.ajax({
		   type: "POST",
		   url: "http://www.phonero.no/wp-content/themes/Phonero/ajexdata.php",
