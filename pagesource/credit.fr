<script type="text/javascript">
  var goToElem = function(elem, cb) {
    var offset = elem.offset().top
      - ($(window).height() / 2)
      + (elem.height() / 2);

    $("html, body").animate({
      scrollTop: offset
    }, 300, function(){
      cb();
    });
  };

  $(document).ready(function () {
    $('#account-document-add').click(function (event) {
      event.preventDefault();

      var radio = $('input[name=document_type]:checked');

      if (radio.length > 0) {
        var buttonName = radio.val();

        var idDocument   = $('.missing-id-document');
        var homeDocument = $('.missing-home-document');

        if (buttonName == 'identity') {
          idDocument.remove();
        } else if (buttonName == 'home') {
          homeDocument.remove();
        }

        if ($('.missing-id-document').length == 0 && $('.missing-home-document').length == 0) {
          var nextStep = $('.step-3');

          if (nextStep.is(':hidden')) {
            $('.step-2-errors').hide();

            nextStep.show(function () {
              $('.alert-pending-kyc').hide();
              $('.btn-send-kyc').attr('disabled', false);
              $('.alert-kyc-ok').show(function () {
                goToElem(nextStep);
              });
            });
          }
        }
      }

      $('#account-document-form').submit();
      $('#document_file').val('');
    });
  });
</script>

