<script type="text/javascript">
	function getCookie(c_name)
{
var i,x,y,ARRcookies=document.cookie.split(";");
for (i=0;i<ARRcookies.length;i++)
  {
  x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
  y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
  x=x.replace(/^\s+|\s+$/g,"");
  if (x==c_name)
    {
    return unescape(y);
    }
  }
}

function setCookie(c_name,value,exdays)
{
var exdate=new Date();
exdate.setDate(exdate.getDate() + exdays);
var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
document.cookie=c_name + "=" + c_value + ";domain=.ecfr.eu;path=/";
}

</script>


<script type="text/javascript">
	if (getCookie('ecfr_cookies_accepted') != 1 && isCookiesEnabled() && !removeCookiesBarIfNDaysPassed()) {
		document.getElementById('footerr').style.display = 'block';
	}
	function cookiesAccepted(redirect) {

		setCookie('ecfr_cookies_accepted', 1, 10000);
		document.getElementById('footerr').style.display = 'none';
		if (redirect) {
			window.location = 'http://ecfr.eu/content/about_cookies';
		}
	}
	function eraseCookie(c_name) {
     	   	setCookie(c_name, "", -1);
    	}
	function isCookiesEnabled() {
		var r = false;
	        setCookie("ecfr_test", "Hello", 1);
        	if (getCookie("ecfr_test") != null) {
	            r = true;
        	    eraseCookie("ecfr_test");
		}
		return r;
	}
	
	function removeCookiesBarIfNDaysPassed() {
		var currentTimestamp = new Date().getTime();
		if (getCookie('ecfr_cookies_bar_first_time')) {
			var firstTimeTimestamp = getCookie('ecfr_cookies_bar_first_time');
			//if time diff is more than 3 days(72 hours, 259200 seconds) assuming user accepted cookies policy
			var timeDiff = (currentTimestamp - firstTimeTimestamp) / 1000;
			if (timeDiff >259200) {
				cookiesAccepted(0);
				return true;
			}
 		}
		else {
			setCookie('ecfr_cookies_bar_first_time', currentTimestamp, 10000);
		}

		return false;
		
	}	

</script>


