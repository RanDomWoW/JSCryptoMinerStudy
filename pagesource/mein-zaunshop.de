//<![CDATA[
Event.observe(document, "dom:loaded", function (event) {
    var relatedItem = $('ajaxpro-related-products').select('ul li').first();
    var crosssellItem = $('ajaxpro-crosssell-products').select('ul li').first();
});
var relatedItemFormsBuffer = [];

AjaxPro.observe('addObservers', function () {

    setLocation = setLocation.interceptor(function (url) {
        if (url.search('checkout/cart/add') != -1) {

            AjaxPro.request({
                'url': url,
                'method': 'get'
            });
            //Event.stop();
            return false;
        }
    });

    /**
     * redeclare submit form function on product page
     *  add product action checkout/cart/add
     */
    var submit = function () {
        var url = this.form.action;
        var isUploadFile = $(this.form).select('input[type=file]').length;
        if (isUploadFile) {
            var template = '<iframe id="product_addtocart_form_frame" name="product_addtocart_form_frame" style="width:0; height:0; border:0;display:none;"><\/iframe>';
            Element.insert($('product_addtocart_form'), {after: template});
            this.form.setAttribute('target', 'product_addtocart_form_frame');
            //add product
            this.form.submit();

            var interval = window.setInterval(function () {

                var iframe = $('product_addtocart_form_frame');
                if ('undefined' == typeof iframe) {
                    return;
                }

                var isAfterSubmit = false;
                try {

                    if (iframe.contentDocument) {
                        isAfterSubmit = iframe.contentDocument.body.innerHTML.length > 1 ? true : false;
                    } else if (iframe.contentWindow) {
                        isAfterSubmit = iframe.contentWindow.document.body.innerHTML.length > 1 ? true : false;
                    } else if (iframe.document) {
                        isAfterSubmit = iframe.document.body.innerHTML.length > 1 ? true : false;
                    }

                } catch (exception) {
                    console.log(exception);
                }

                if (!isAfterSubmit) {
                    return;
                }
                AjaxPro.request({
                    'url': url,
                    parameters: {'onlyblocks': 1}
                });
                Element.remove(iframe);
                window.clearInterval(interval);
            }, 1000);
            this.form.removeAttribute('target');
            return false;
        }

        // check quantity-unit-field
        $$('.product-main-info .qtywithunit').each(function (element) {
            if (element.value == element.getAttribute('data-placeholder')) {
                element.value = 1;
                updateQuantityUnit(element);
            }
        });

        var params = this.form.serialize(true);
        if (this.validator && this.validator.validate()) {
            AjaxPro.request({
                'url': url,
                parameters: params
            });
        }

        return false;
    };

    if (typeof productAddToCartFormOld != 'undefined') {

        productAddToCartForm.submit = submit.bind(productAddToCartFormOld);

    } else if (typeof productAddToCartForm != 'undefined') {

        productAddToCartForm.submit = submit;

        if ($('main-product-container').down('qty')) {
            $('main-product-container').down('qty').observe('keypress', function (e) {
                if (13 === e.keyCode) {
                    Event.stop(e);
                    productAddToCartForm.submit();
                }
            });
        }
    }

    if (typeof window.quantityUnit !== 'undefined') {
        if (typeof window.quantityUnit.relatedForms != 'undefined') {
            for (var itemId in window.quantityUnit.relatedForms) {
                window.quantityUnit.relatedForms[itemId].submit = submit;
                if($('item-related-' + itemId).down('.qty') && typeof relatedItemFormsBuffer[itemId] === 'undefined'){
                    relatedItemFormsBuffer[itemId] = true;
                    $('item-related-' + itemId).down('.qty').observe('keypress', function (e) {
                        if (13 === e.keyCode) {
                            Event.stop(e);
                            window.quantityUnit.relatedForms[this.offsetParent.id.substring(13)].submit();
                        }
                    });
                }
            }
        }
    }

    if (typeof productPatternForm != 'undefined') {
        productPatternForm.submit = submit;
    }

    /**
     * redeclare submit form function on shopping cart page
     * update action checkout/cart/updatePost
     */
    var shoppingCartTable = $('shopping-cart-table');
    if (shoppingCartTable) {
        var shoppingCartForm = shoppingCartTable.up('form');
        if (typeof shoppingCartForm != 'undefined') {

            //prototype  multiple submit bugfix
            //http://www.developpez.net/forums/d577369/webmasters-developpement-web/javascript/bibliotheques-frameworks/prototype-script-aculo-us/prototype-serialize-multiple-submit/
            shoppingCartForm.select(':submit').each(function (submitInput) {
                submitInput._submitted = false;
                submitInput.onclick = function () {
                    submitInput._submitted = true;
                }.bind(this);
            }.bind(this));

            shoppingCartForm.observe('submit', function (e) {
                var el = Event.element(e);
                el.stopObserving('submit');
                Event.stop(e);
                var params = el.serialize(true);

                shoppingCartForm.select(':submit').each(function (_el) {
                    if (_el._submitted === true) {
                        params[_el.name] = _el.value;
                    }
                    _el._submitted = false;
                }.bind(this));

                var url = el.action;

                AjaxPro.request({
                    'url': url,
                    parameters: params
                });

                return false;
            });
        }
    }

    $$('a').each(function (element) {
        var url = element.getAttribute('href');

        if (url == '#') {
            var onclickHandler = element.getAttribute('onclick');
            if (
                onclickHandler
                    && typeof onclickHandler == 'string'
                    && onclickHandler != ''
                    && onclickHandler.search('setLocation') != -1
                    && onclickHandler.search('checkout/cart/add') != -1
                ) {

                element.stopObserving('click');
                element.observe('click', function (e) {
                    Event.stop(e);
                });
            }
        }
        if (url && url.search('checkout/cart/delete') != -1) {
            element.stopObserving('click');
            element.setAttribute('onclick', '');
            element.observe('click', function (e) {
                Event.stop(e);
                                if (!confirm('Soll dieser Artikel wirklich aus Ihrem Warenkorb entfernt werden?')) {
                    return false;
                }
                                AjaxPro.request({
                    'url': url
                });
                return false;
            });
            return false;
        }
    });
});
AjaxPro.observe('onLoading:checkout', function () {

    AjaxPro.effect.opacity
        .setSelector('.block-cart')
        .show(0.5)
    ;
});

AjaxPro.observe('onFailure:checkout', function () {
    AjaxPro.effect.opacity
        .setSelector('.block-cart')
        .hide();
});

AjaxPro.observe('onComplete:checkout', function (e) {
    var r = e.memo.response;

    // Update cartheader
    $$('.top-bar .top-container').invoke('update', r.layout.cart_sidebar);

    // added by flagbit
    if (r.productImage) {
        var image = $('ajaxpro-addtocart-form').select('.ajaxpro-product .product-image img').first();
        image.src = r.productImage;

        if (r.productName) {
            image.alt = r.productName;
            image.title = r.productName;
        }
    }
    if (r.productRelatedProducts && r.productUrl) {
        var categoryView = $$('body')[0].classList.contains('catalog-category-view');
        var dcRelated = $('ajaxpro-addtocart-form').select('.link-button.btn-related-products').first();
        var productUrl = r.productUrl + '#dcRelated';
        if(categoryView == true) {
            dcRelated.href = productUrl;
            dcRelated.show();
        }
    }

    fillProductsData(r.products);

    if (r.crosssell && r.crosssell.length > 0) {
        $('ajaxpro-addtocart-form').addClassName('with-crosssell');
        buildList(r.crosssell, 'ajaxpro-crosssell-products');
    } else {
        $('ajaxpro-crosssell-products').innerHTML = '';
    }
    if (r.related && r.related.length > 0) {
        $('ajaxpro-addtocart-form').addClassName('with-related');
        buildList(r.related, 'ajaxpro-related-products');
    } else {
        $('ajaxpro-related-products').innerHTML = '';
    }
    if(r.products && r.products.length > 0 && r.products[0].muster_option && r.products[0].muster_option.length > 0){
        newMusterName = r.products[0].muster_option[0].label + ' : ' + r.products[0].muster_option[0].value;
        $$('.product-info-container .product-name').first().innerHTML = newMusterName;
    }

    if (r.messages) {
        AjaxPro.effect.message
            .setElementId('ajaxpro-addtocart-form')
            .setMessages(r.messages)
            .show()
        ;

        $('ajaxpro-cart-button').show();
        $$('.ajaxpro-btn-with-notice').first().removeClassName('btn-center');

        for (var code in r.messages) {
            var temp = r.messages[code], length = temp.length;
            for (i = length; i--;) {
                if (temp[i] == 'Please specify the product\'s option(s).') {
                    $('ajaxpro-cart-button').hide();
                    $$('.ajaxpro-btn-with-notice').first().addClassName('btn-center');
                }
            }
        }
    }

    if (typeof truncateOptions === 'function') {
        truncateOptions();
    }
});

// show currently added product data
function fillProductsData(products) {
    var infoContainer = $('ajaxpro-addtocart-form').select('.product-info-container').first();
    var dataContainer = infoContainer.select('.product-info').first();
    var dataContainerClone = Element.clone(dataContainer, true);
    infoContainer.innerHTML = '';

    products.each(function (product) {
        for (var prop in product) {
            if (product.hasOwnProperty(prop)) {
                dataContainerClone.select('.product-' + prop).invoke('update', product[prop]);
            }
        }
        dataContainerClone.select('.product-oldPrice').invoke('hide');
        if (product.hasOwnProperty('showOldPrice')) {
            if (product['showOldPrice'])  dataContainerClone.select('.product-oldPrice').invoke('show');
        }
        infoContainer.appendChild(dataContainerClone);
    });
}

// build related and upsell list
function buildList(data, target) {
    var maxItems = 4;
    var container = $(target).select('ul').first();
    var listElement;
    var counter = 0;
    var listElementTemp;

    if (typeof container != 'undefined') {

        // TODO: change init of crosssell and related items
        if (target == 'ajaxpro-crosssell-products' && typeof crosssellItem != 'undefined') {
            listElementTemp = crosssellItem;
        } else if (typeof relatedItem != 'undefined') {
            listElementTemp = relatedItem;
        } else {
            listElementTemp = container.select('li').first();
        }

//        var listElementTemp = container.select('li').first();
        listElement = Element.clone(listElementTemp, true); // need cloning for ie
        container.innerHTML = '';

        data.each(function (item) {

            if (counter >= maxItems) {
                return;
            }

            // image
            var image = listElement.select('img').first();
            image.src = item['image_url'];
            image.alt = item['name'];
            image.title = item['name'];

            // urls
            listElement.select('.product-image').first().href = item['url'];
            listElement.select('.product-name a').first().href = item['url'];

            // name
            listElement.select('.product-name a').first().innerHTML = item['name'];

            // price information
            listElement.select('.price-container').first().innerHTML = item['price'];
            listElement.select('.baseprice-box').first().remove();

            var copy = Element.clone(listElement, true);
            container.appendChild(copy);

            counter++;
        });
    }
}

String.prototype.toDOM = function () {
    var d = document
        , i
        , a = d.createElement("div")
        , b = d.createDocumentFragment();
    a.innerHTML = this;
    while (i = a.firstChild)b.appendChild(i);
    return b;
};

