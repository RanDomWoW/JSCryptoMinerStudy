    function closeNavSearchForm() {
      if (!nav_search_form_opened) {
        return;
      }
      nav_search_form_opened = false;
      
      $('#searchquery').blur();
      $('#sva_fixed_nav').animate({paddingBottom: 0}, 'slow');
      $('#sva_phone_search').removeClass('opened');
      $('body').removeClass('nav_search_form_opened');
      closeDrawer();
    }

    function openNavSearchForm() {
      if (nav_search_form_opened) {
        return;
      }
      nav_search_form_opened = true;

      // added to close drawer if opened
      if (isIos && myDevice == 'phone' && $('#sva_left_nav').hasClass('drawer_opened')) {
//        closeDrawer();
        hideDrawer();
      }

      // for ios the navigation needs to be closed when the virtual keyboard is present
      if (isIos && myDevice == 'phone' && $('#mobile_nav_lnk').hasClass('opened')) {
        $('body').css({'overflow': 'visible'});
        $('#mobile_nav_lnk').removeClass('opened');
        $('#sva_mobile_menu').css({bottom: -($('#sva_mobile_menu').height())});
        arrangeMobileMenu();
      }

      if (myDevice == 'phone') {
        $('#sva_fixed_nav').animate({paddingBottom: 30}, function() {
          if (isIos) {
            $('html, body').animate({scrollTop: $('body').height()});
          }
        });
      }

      $('#sva_phone_search').addClass('opened');
      $('body').addClass('nav_search_form_opened');
      $('#searchquery').focus();
    }

    //
    $(function() {

      $('#sva_logo').click(function() {
        if($('#mobile_prompt').length > 0) {
          //setMobilePromptCookie();
        }
      });

      $('#sva_phone_search').click(function() {
        if($('#mobile_prompt').length > 0) {
          //setMobilePromptCookie();
        }
        nav_search_form_opened ? closeNavSearchForm() : openNavSearchForm();
      });

      $('.phone #search_btn').click(function() {
        closeNavSearchForm();
      });

      $('.phone #sva_search form').submit(function() {
        if (($('#searchquery').val() ==  "")) {
          $('#sva_search form').animate({ width: '0' }, 'slow');
          return false;
        }
      });

      $('#searchquery').focus(function() {
        if (isIos && myDevice != 'phone') {
          skip_next_window_resize = true;
          $('body').addClass('nav_search_form_opened');
          nav_search_form_opened = true;
        }
      });

      $('#searchquery').blur(function() {
        closeNavSearchForm();
        if (isIos && myDevice != 'phone') {
          skip_next_window_resize = true;
          $('body').removeClass('nav_search_form_opened');
          nav_search_form_opened = false;
        }
      });

//      $('body').on('swipeleft', '#nav_drawer', function(e) {console.log('closing');}).on('swiperight', '#nav_drawer', function(e) {console.log('opening');});

      $('#sva_left_nav #nav_drawer, #sva_left_nav').swipe({
        swipeLeft:function(event, direction, distance, duration, fingerCount) {
          hideDrawer();
        },
        swipeRight:function(event, direction, distance, duration, fingerCount) {
          hideDrawer();
        },
        threshold:0
      });

      // show or hide the hamburger menu on tap / click; available only for phone and desktop
      $('#mobile_nav_lnk').click(function() {
        closeNavSearchForm();
        // if menu is already opened, we close it
        if ($(this).hasClass('opened')) {
          $('body').css({'overflow': 'visible'});
          $(this).removeClass('opened');
          $('body').get(0).offsetHeight;
          // check for which device we are closing the menu
          if (myDevice == 'phone') {
            hideMobileMenu();
          } else {
            hideTabletMenu();
          }
        } else {
          // if any of the bubbles is opened, we close it
          if (sva_over_box !== false) {
            $('#sva_desktop_subnavigation .sva_box.' + sva_over_box).animate({
              bottom: '-' + ($('#sva_desktop_subnavigation .sva_box.' + sva_over_box).height() + $('#sva_wrapper').height() + 20) + 'px'
            }, 100);
            sva_over_box = false;
          }
          // we make it active/opened
          $(this).addClass('opened');
          $('body').css({'overflow': 'hidden'});
          // check for which device we open the menu
          if (myDevice == 'phone') {
            showMobileMenu();
          } else {
            showTabletMenu();
          }
        }
      });

      // remove default behavior for the mobile menu links
      $('body').on("click", '.phone #sva_mobile_menu #sva_mobile_navigation > ul#sva_nav > li > a, .phone #sva_mobile_menu #sva_mobile_navigation > ul#sva_snav > li > a, .tablet #sva_mobile_menu #sva_mobile_navigation > ul#sva_snav > li > a', function(e) {
        if ($(this).attr('class') != 'faculty') {
          $('.sva_box_wrapper>div').scrollTop(0);
          (e.preventDefault) ? e.preventDefault() : e.returnValue = false;
          $('#sva_subnavigation .sva_box').css('display', 'none');
          $('#sva_subnavigation .go_back_title').css('display', 'none');
          $('#sva_subnavigation .sva_box.' + $(this).attr('class')).css({'display':'block'});
          $('.phone #sva_subnavigation .go_back_title.' + $(this).attr('class')).css({'display':'block'});
          // reposition mobile menu divot
          if ($('#sva_wrapper').width() - $('#sva_content').width() > 18) {
//            $('#sva_subnavigation').css({'background-position': ($('#sva_subnavigation').width() - 18 - ($('#sva_wrapper').width() - $('#sva_content').width())/2) + 'px 385px'});
          } else {
            $('#sva_subnavigation').css({'background-position': 'bottom right'});
          }
          if (myDevice == 'phone') {
            $('#sva_mobile_menu').animate({
              left: 'auto',
              right: ($(window).width() - $('#sva_subnavigation').width()) + 'px'
            }, 250);
          } else {
            $('#sva_mobile_navigation #sva_snav li').removeClass('active_submenu');
            $('#sva_mobile_menu').animate({
              right: '5px',
              bottom: '95px'
            }, 250);
            $(this).parent().addClass('active_submenu');
          }
          return false;
        }
      });

      $('body').on("mouseenter mouseleave", '.desktop #sva_navigation li', function(e) {
        if ($('body').is('.nav_search_form_opened.with_ios_keyboard')) {
          skip_next_window_resize = true;
          return;
        }
        if (e.type === "mouseenter") {
          sva_over_box = $('a', this).attr('class');
          var current_menu = $('#sva_desktop_subnavigation .sva_box.' + $('a', this).attr('class'));
          current_menu.css({right: 'auto'}).show().animate({bottom: '-1'}, 100);
          if (($('a', this).attr('class') == 'edu') && (myDevice == 'tablet')) {
            current_menu.css({left: 'auto', right: 33 + ($('body').width() - $('#sva_content').width())});
           
            $('.tablet #sva_desktop_subnavigation .sva_box.' + $('a', this).attr('class') + ' .sva_box_wrapper').css('background-position', '85% 100%');
          }
          //EC: 2/10/16 : now that STUDENTS is all the way on the right, need to explicitly set its min-width:400px to make sure it always displays in 2 cols; this also necessitates positioning it manually
          else if (($('a', this).attr('class') == 'students') && (myDevice == 'desktop')) {
            current_menu.css({left: 'auto', minWidth: '400px', right: ($('body').width() - $('#main-wrapper').width())/2 });
          }
          else {
            if ($(this).parent().attr('id') == 'sva_nav') {
              current_menu.css({left: $(this).offset().left + $(this).width()/2 - current_menu.width()/2});
            }
            else {
              current_menu.css({left: $(this).offset().left - 5 + $(this).width()/2 - current_menu.width()/2});
            }
          }
          // we try to close the side menu
          $('body').css({'overflow': 'visible'});

          $('#sva_mobile_navigation').scrollTop(0);
          $('.sva_box_wrapper>div').scrollTop(0);

          $('#sva_mobile_menu').animate({
            right: -($('#sva_mobile_menu').width() + 5)
          }, 250);
          $('#sva_mobile_navigation #sva_snav li').removeClass('active_submenu');
          $('#mobile_nav_lnk').removeClass('opened');
        } else if (e.type === "mouseleave") {
          sva_over_box = false;
          var whichOne = $('a', this).attr('class');
          window.setTimeout(function() {
            hideNavSubmenu(whichOne);
          }, 100);
        }
      });

      $('body').on("mouseenter mouseleave", '.desktop #sva_desktop_subnavigation .sva_box, .tablet #sva_desktop_subnavigation .sva_box', function(e) {
        if (e.type === "mouseenter") {
          sva_over_box = $(this).attr('class').replace('sva_box ', '');
        } else if (e.type === "mouseleave") {
          var thisOne = sva_over_box;
          sva_over_box = false;
          window.setTimeout(function() {
            hideNavSubmenu(thisOne);
          }, 100);
        }
      });

      $('body').on("click", '.tablet #sva_navigation li', function(e) {
        (e.preventDefault) ? e.preventDefault() : e.returnValue = false;
        $('body').css({'overflow': 'visible'});

        $('#sva_mobile_navigation').scrollTop(0);
        $('.sva_box_wrapper>div').scrollTop(0);

        $('#sva_mobile_menu').animate({
          right: -($('#sva_mobile_menu').width() + 5)
        }, 250);
        $('#sva_mobile_navigation #sva_snav li').removeClass('active_submenu');
        $('#mobile_nav_lnk').removeClass('opened');
        $('#mobile_nav_lnk').blur();
        if (sva_over_box !== false) {
          $('#sva_desktop_subnavigation .sva_box.' + sva_over_box).animate({
            bottom: '-' + ($('#sva_desktop_subnavigation .sva_box.' + sva_over_box).height() + $('#sva_wrapper').height() + 20) + 'px'
          }, 100);
          if (sva_over_box != $('a', this).attr('class')) {
            sva_over_box = $('a', this).attr('class');
            $('#sva_desktop_subnavigation .sva_box.' + $('a', this).attr('class')).css({'right':'auto', 'display':'block'}).animate({bottom: '-1'},100);
            if (($('a', this).attr('class') == 'edu') && (myDevice == 'tablet')) {
              $('#sva_desktop_subnavigation .sva_box.' + $('a', this).attr('class')).css({'left': 'auto', 'right': 33 + ($('body').width() - $('#sva_content').width()) + 'px'});
              $('.tablet #sva_desktop_subnavigation .sva_box.' + $('a', this).attr('class') + ' .sva_box_wrapper').css('background-position','85% 100%');
            } else {
              $('#sva_desktop_subnavigation .sva_box.' + $('a', this).attr('class')).css({'left':$(this).offset().left + $(this).width()/2 - $('#sva_desktop_subnavigation .sva_box.' + $('a', this).attr('class')).width()/2 - 10 + 'px'});
            }
          } else {
            sva_over_box = false;
          }
        } else {
          sva_over_box = $('a', this).attr('class');
          $('#sva_desktop_subnavigation .sva_box.' + $('a', this).attr('class')).css({'right':'auto', 'display':'block'}).animate({bottom: '-1'},100);
          if (($('a', this).attr('class') == 'edu') && (myDevice == 'tablet')) {
            $('#sva_desktop_subnavigation .sva_box.' + $('a', this).attr('class')).css({'left': 'auto', 'right': 33 + ($('body').width() - $('#sva_content').width()) + 'px'});
            $('.tablet #sva_desktop_subnavigation .sva_box.' + $('a', this).attr('class') + ' .sva_box_wrapper').css('background-position','83% 100%');
          } else {
            $('#sva_desktop_subnavigation .sva_box.' + $('a', this).attr('class')).css({'left':$(this).offset().left + $(this).width()/2 - $('#sva_desktop_subnavigation .sva_box.' + $('a', this).attr('class')).width()/2 - 15 + 'px'});
          }
        }
      });

      function hideNavSubmenu(subm) {
        if (subm != sva_over_box) {
          $('#sva_desktop_subnavigation .sva_box.' + subm).animate({
            bottom: '-' + ($('#sva_desktop_subnavigation .sva_box.' + subm).height() + $('#sva_wrapper').height() + 20) + 'px'
          }, 100);
          $('#sva_content #sva_navigation #sva_nav .' + subm).blur();
          $('body').click();
        }
      }

      function showNavSubmenu() {

      }

    });



