  <script type="text/javascript">

    var nav_search_form_opened = false;
    var menu_arranged_for = null;

    // according to the device type, runs the appropriate function to sort navigation elements
    function groupNavElements() {
      $('body').css({'overflow': 'visible'});
      if (myDevice == 'phone') {
        // hide and dimension the phone menu
        arrangeMobileMenu();
      } else if (myDevice == 'tablet') {
        // hide and dimension the tablet menu
        arrangeTabletMenu();
      } else {
        // hide and dimension the desktop menu
        arrangeDesktopMenu();
      }
    }

    // phone show & hide menu
    function showMobileMenu() {
      if($('#mobile_prompt').length > 0) {
        //setMobilePromptCookie();
      }
      $('#sva_subnavigation').css({'width': $('#sva_fixed_nav').width() + 1 + 'px', 'max-width': $('#sva_fixed_nav').width() + 1 + 'px'});
      $('#sva_nav_overlay').css({'display':'block'});
//      $('#sva_mobile_navigation').each(function() {
//        $(this).scrollTop(0);
//      });
//      $('.sva_box_wrapper').each(function() {
//        $(this).scrollTop(0);
//      });
      $('#sva_mobile_navigation').scrollTop(0);
      $('#sva_mobile_menu').animate({
        bottom: $('#sva_wrapper').height()
      }, 250);
    }
    function hideMobileMenu() {
//      $('#sva_mobile_navigation').each(function() {
//        $(this).scrollTop(0);
//      });
//      $('.sva_box_wrapper').each(function() {
//        $(this).scrollTop(0);
//      });
      $('#sva_nav_overlay').css({'display':'none'});
      $('#sva_mobile_menu').animate({
        bottom: -($('#sva_mobile_menu').height())
      }, 250, function() {
        arrangeMobileMenu();
      });
    }

    // tablet show & hide menu
    function showTabletMenu() {

//      $('#sva_mobile_navigation').each(function(){
//        $(this).scrollTop(0);
//      });
//      $('.sva_box_wrapper').each(function() {
//        $(this).scrollTop(0);
//      });

      $('#sva_mobile_menu').animate({
        right: -($('#sva_subnavigation').width() + 21)
      }, 250);

    }
    function hideTabletMenu() {
      $('#sva_mobile_menu').animate({
        right: '-720px'
      }, 250, function() {
        arrangeTabletMenu();
      });

    }

    // for the phones, to go back to main navigation
    function navBack() {
      $('#sva_mobile_menu').animate({
        left: 'auto',
        'right': '-' + ($('#sva_subnavigation').width() + 25) + 'px'
      }, 250);
      $('.sva_box_wrapper').each(function() {
        $(this).scrollTop(0);
      });
    }

    // brings back all submenus to the original folder
    function resetSubNavigation() {
      $('.sva_box').each(function() {
        $(this).detach().appendTo('#sva_subnavigation');
      });
      // removing the extra elements used to correctly auto dimension navigation bubbles
      $('.sva_subnav_wrapper').each(function() {
        $(this).remove();
      });
    }

    //
    // prepare containers for mobiles
    //
    function arrangeMobileMenu() {
      $('body').css({'overflow':'hidden'});
      // appending necessary elements
      // make sure the elements are not already in the right container
      if (menu_arranged_for !== 'mobile') {
        $('#sva_nav').detach().appendTo('#sva_mobile_navigation');
        $('#sva_snav').detach().appendTo('#sva_mobile_navigation');
        $('#sva_actions').detach().appendTo('#sva_mobile_navigation');
        $('#sva_search').detach().insertAfter('#mobile_nav_lnk');
        $('#sva_subnavigation').detach().appendTo('#sva_mobile_menu > div');
        
        // reset subnavigation items arrangement
        resetSubNavigation();
      }
      menu_arranged_for = 'mobile';

      //remove max-height from tablet max-height fix
      $('#sva_mobile_menu').css({'max-height':''});

      // menu container size
//      $('#sva_mobile_navigation, #sva_subnavigation').css({'max-height': ($(window).height() - $('#sva_wrapper').height() - 25) + 'px'});
//      $('#sva_mobile_navigation').css({'max-height': ($(window).height() - $('#sva_wrapper').height() - 45) + 'px', 'margin-bottom':'20px'});
//      $('.sva_box_wrapper').css({'max-height': ($(window).height() - $('#sva_wrapper').height() - 75) + 'px'});
      $('#sva_mobile_navigation, #sva_subnavigation').css({'max-height': ($(window).height() - $('#sva_wrapper').height() - 5) + 'px'});
      $('#sva_mobile_navigation').css({'max-height': ($(window).height() - $('#sva_wrapper').height() - 25) + 'px', 'height': ($(window).height() - $('#sva_wrapper').height() - 25) + 'px', 'margin-bottom':'20px'});
      $('.sva_box_wrapper').css({'max-height': ($(window).height() - $('#sva_wrapper').height() - 55) + 'px'});

      // make menu link not opened
      $('#mobile_nav_lnk').removeClass('opened');
      $('body').get(0).offsetHeight;

      $('#sva_mobile_navigation #sva_nav li a, #sva_mobile_navigation #sva_snav li a').each(function() {
        $(this).prop('onclick', 'return false');
      });

      $('.sva_box_wrapper div').css({'height': '', 'max-height': '', 'overflow':'auto'});

      // expand the container more
      $('#sva_mobile_menu').css({'width':($(window).width()*2 + 30) + 'px', 'border-radius':'0 0 0 0'});

      $('#sva_mobile_navigation').css({'width':$(window).width() - 20 + 'px', 'right':'0'});
      $('#sva_subnavigation').css({'width':$(window).width() - 20 + 'px', 'max-width':$(window).width() -20 + 'px'});

      // hide menu from view
      $('#sva_mobile_menu').css({'bottom':-($('#sva_mobile_menu').height()) + 'px', 'right':'-' + ($(window).width() + 30) + 'px', 'left': 'auto'});

      $('#sva_mobile_navigation').scrollTop(0);
      $('.sva_box_wrapper').scrollTop(0);

      $('body').css({'overflow':'visible'});

      var mobileViewPrompt = '<div id="mobile_prompt"><div id="view_sections"></div><div id="view_all"></div><a href="javascript:;" onclick="setMobilePromptCookie()" id="got_it"></a></div>';
      /*
      EC: 10/30/14 - removing mobile prompt; also renaming getSVAMobileCookie --> getSVACookie for more general use
      // SVA Mobile Prompt Cookie
      if((getSVAMobileCookie('SVAMobilePrompt') == null) &&($('#mobile_prompt').length == 0)) {
        $('body').css('overflow', 'hidden');
        $(mobileViewPrompt).insertAfter('#sva_left_nav');
        setSVAMobileCookie('SVAMobilePrompt', 1);
        $('#view_sections').css('top', parseInt($(window).height()/2 - 45) + 'px');
      };
    */
    }

    //
    // prepare containers for tablets
    //
    
    function arrangeTabletMenu() {
      closeNavSearchForm();

      // append necessary elements
      // make sure the elements are not already in the right container
      if (menu_arranged_for !== 'tablet') {
        $('#sva_actions').detach().appendTo('#sva_content');
        $('#sva_search').detach().appendTo('#sva_content');
        $('#sva_nav').detach().appendTo('#sva_navigation');
        $('#sva_snav').detach().appendTo('#sva_mobile_navigation');

        // reset subnavigation items arrangement
        resetSubNavigation();
      }
      menu_arranged_for = 'tablet';

//      $('.sva_box_wrapper div').css({'height': ($(window).height() - 175) +'px', 'max-height': ($(window).height() - 175) +'px', 'overflow':'hidden', 'overflow-y':'scroll'});
      $('.sva_box_wrapper div').css({'height': '', 'max-height': ($(window).height() - 175) +'px', 'overflow':'hidden', 'overflow-y':'scroll'});

      // arrange subnavigation items
      $('#sva_nav li a').each(function() {
        var menuElem = $(this);
        var menuElemClass = menuElem.attr('class').replace('img_replace', '').replace(' ', '');
        $('.sva_box.' + menuElemClass).detach().appendTo('#sva_desktop_subnavigation');
        $('.sva_box.' + menuElemClass).css({'bottom': '-2000px', 'right': -($('.sva_box.' + menuElemClass).width()/2 - $('a.' + menuElemClass).width()/2 + 20) + 'px'});
      });

      $('#sva_subnavigation').detach().appendTo('#sva_mobile_menu > div');

      var max_submenu = 0;

      $('#sva_subnavigation .sva_box').each(function() {
        if ($(this).height() > max_submenu) {
          max_submenu = $(this).height();
        }
        $(this).css({'display':'none'});
      });

      // fix for the hamburger menu

      $('#sva_mobile_navigation').css({'margin-bottom': '0', 'max-height':'', 'width':'auto'});

      // menu container size
      $('#sva_subnavigation').css({'max-width': $(window).width() - $('#sva_mobile_navigation').width() - 5 + 'px'});

      // make menu link not opened
      $('#mobile_nav_lnk').removeClass('opened');
      $('body').get(0).offsetHeight;

      // hide menu from view
      $('#sva_mobile_menu').css({'bottom': '95px', 'right': '-720px', 'left':'auto', 'border-radius':'10px 10px 0 0', 'max-height': ($(window).height() - 105) +'px'});
      // visible main menu size
      //EC: 2/16/16 : causing navigation display errors when window resized from desktop > mobile > desktop
      // $('#sva_navigation').css({'width': ($('#sva_content').width() - 221 - 27 - 15) + 'px', 'max-width': ($('#sva_content').width() - 221 - 27 - 15) + 'px'});

      // actions menu sizing
      $('#sva_actions').css({'width': ($('#sva_content').width() - $('#sva_search').width() - 25) + 'px'});

