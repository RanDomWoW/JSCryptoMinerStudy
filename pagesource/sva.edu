    $(function(){

      var patchOffset = 3;
      var pageOffset = 2;
      var firstPageRevealed = false;
      // The slideshow images will always be in the ratio of 980 x 380, height being equal to about 38% of the width
      var slideImageRatio = 0.387;
      adjust_video_size('#hp_video_content');
      moveLeftNav();
      resizeCarousel();
      adjustQuiltThumb();
      adjustAlertThumb();
//      $('#bigPic').css('height', $('#bigPic img').height());
      $('#bigPic').css('height', Math.round($('#bigPicContainer').width() * slideImageRatio));

      $(window).resize(function() {
        adjust_video_size('#hp_video_content');
        resizeCarousel();
        adjustQuiltThumb();
        adjustAlertThumb();
        resetQuilt();
        $('#bigPic').css('height', $('#bigPic img').height());
      });

      $('#show_more').on('click', function(e){

        if (pageOffset <= $('#quilt_pages .quilt_page').length) {

            $('#quilt_pages .quilt_page:nth-child(' + pageOffset + ')').slideDown();

            if (pageOffset == $('#quilt_pages .quilt_page').length) {
                $(this).fadeOut();
            }

            pageOffset++;
        } else {
            return;
        }

      });

      if(myDevice == 'phone') {
        $('#bigPicContainer').swipe({
          swipeLeft:function(event, direction, distance, duration, fingerCount) {
            showPrevSlide();
          },
          swipeRight:function(event, direction, distance, duration, fingerCount) {
            showNextSlide();
          },
          //Default is 75px, set to 0 for demo so any distance triggers swipe
          threshold:0
        });
      }

    });
    
    function adjust_video_size(e) {
      $(e).css('height', 'auto');
      newWidth = $(e).width() + 'px';
      newHeight = ( $(e).width() * $(e + ' iframe').prop('height') / $(e + ' iframe').prop('width') ) + 'px';
      $(e + ' iframe').css({'width': newWidth, 'height': newHeight});
      $('.quilt_page.active').css('width', $('#quilt_pages').width() + 'px');
    }
    function resetQuilt() {
      $('.scrollableStrip').css('margin-left', '0');
      $('.scrollableStrip .quilt_page').removeClass('active');
      $('.scrollableStrip .quilt_page:first-child').addClass('active');
      $('#quilt_page_controls .pageIndex .page').removeClass('sel')
      $('#quilt_page_controls .pageIndex .page:first-child').addClass('sel')
    }

    function resizeCarousel() {
      newWidth = $('#quilt_pages').width();
      $('.quilt_page').css('width', newWidth + 'px');
      pageQuiltWidth = newWidth;
      pageQuiltWidth = newWidth;
    }
    
    function adjustQuiltThumb() {
      if(myDevice == 'phone' && $(window).width() < 464) {
        $('ul.quilt div.quilt_patch .thumb').css('height', $('ul.quilt div.quilt_patch .thumb').width() + 'px');
      } else {
        $('ul.quilt div.quilt_patch .thumb').removeAttr('style');
      }
    }
    
    function adjustAlertThumb() {
      if(myDevice == 'phone' && $(window).width() < 464) {
        $('#hp_alert img').css('height', $('#hp_alert .alertImage').width() + 'px');
      } else {
        $('#hp_alert img').css('height', 'auto');
      }
    }
    
    function adjustSocialMediaIcons() {
      if(myDevice == 'tablet') {
        $('ul.sva_social_channels li').css('width');
      }
    }
  </script>

