    <script type="text/javascript">
    $(function($) {
        // 当日取得
        var thisdate = new Date();
        var thisyear = thisdate.getFullYear();
        var thismonth = thisdate.getMonth() + 1;
        var today = thisdate.getDate();
        var initlastday = new Date(thisyear, thismonth, 0).getDate();
        $("#d").empty();
        for (var i=1; i <= initlastday; i++) {
            var initday = new Date(thisyear,thismonth - 1,i).getDay();
            var initdayStr = [ "日", "月", "火", "水", "木", "金", "土" ][initday];
            if (i == today) {
                $("#d").append($("<option selected>").val(i).text(i+'日('+ initdayStr +')'));
            }else{
                $("#d").append($("<option>").val(i).text(i+'日('+ initdayStr +')'));
            }
        }
        $("#m").val(thismonth);
        // 月選択
        $("#m").change(function() {
            var selectmonth = $("#m").val();
            var selectday = $("#d").val();
            if(selectmonth >= thismonth){
                var selectyear =  thisyear;
            }else{
                var selectyear =  thisyear + 1;
            }
            var lastday = new Date(selectyear, selectmonth, 0).getDate();
            //一旦要素削除
            $("#d").empty();
            for (var i=1; i <= lastday; i++) {
                var dayOfWeek = new Date(selectyear,selectmonth - 1,i).getDay();
                var dayOfWeekStr = [ "日", "月", "火", "水", "木", "金", "土" ][dayOfWeek];
                if (i == selectday) {
                    $("#d").append($("<option selected>").val(i).text(i+'日('+ dayOfWeekStr +')'));
                }else{
                    $("#d").append($("<option>").val(i).text(i+'日('+ dayOfWeekStr +')'));
                }
            }
        });
        // カレンダー画面からのデータ
        $("#datepicker").bind("change", function() {
            var data = $(this).val();
            var caldata = data.split("/");
            var calyear = caldata[0];
            var calmonth = caldata[1];
            var calday = caldata[2];
            // 月設定
            $("#m").val(calmonth);
            var callastday = new Date(calyear, calmonth, 0).getDate();
            //一旦要素削除
            $("#d").empty();
            for (var i=1; i <= callastday; i++) {
                var caldayOfWeek = new Date(calyear,calmonth - 1,i).getDay();
                var caldayOfWeekStr = [ "日", "月", "火", "水", "木", "金", "土" ][caldayOfWeek];
                if (i == calday) {
                    $("#d").append($("<option selected>").val(i).text(i+'日('+ caldayOfWeekStr +')'));
                }else{
                    $("#d").append($("<option>").val(i).text(i+'日('+ caldayOfWeekStr +')'));
                }
            }
        })
    });
    </script>
