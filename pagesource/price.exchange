    <script type="text/javascript">

            $(function() {





				currency = 'USD';



                function getPrice() {





                    $.ajax({
                        url: '/update',
                        dataType: 'json',
                        success: function(data) {



                            json = data;


							
                            price = data.price;
                            lasttrade = data.lasttradetime;
                            $('#lasttrade').text(lasttrade);



                            if (!currency) {

                                fiat = data.USD["last"];

                            } else {



                                switch (currency)

                                {

                                    case "USD" : fiat = json.USD["last"]; break;

                                    case "AUD":  fiat = json.AUD["last"];   break;

                                    case "JPY":  fiat = json.JPY["last"];      break;

                                    case "CAD":     fiat = json.CAD["last"];  break;

                                    case "CNY": fiat = json.CNY["last"];  break;

                                    case "EUR":  fiat = json.EUR["last"];  break;

                                    case "GBP":  fiat = json.GBP["last"]; break;
                                    case "BRL": fiat = json.BRL["last"];  break;
                                                   case "CHF": fiat = json.CHF["last"]; break;
                                        case "CLP":fiat = json.CLP["last"];  break;
                                        case "DKK": fiat = json.DKK["last"];   break;
                                        case "HKD": fiat = json.HKD["last"];  break;
                                        case "ISK": fiat = json.ISK["last"];  break;
                                        case "KRW":fiat = json.KRW["last"]; break;
                                        case "NZD":   fiat = json.NZD["last"];  break;
                                        case "PLN": fiat = json.PLN["last"];  break;
                                        case "RUB":  fiat = json.RUB["last"];     break;
                                        case "SEK": fiat = json.SEK["last"]; break;
                                        case "SGD": fiat = json.SGD["last"];  break;
                                        case "THB": fiat = json.THB["last"];   break;
                                        case "TWD": fiat = json.TWD["last"];  break;
                                    default:

                                        fiat = json.USD["last"];



                                }



                            }



                            coin = $('#amount').val();
                        
                          ;
                                                        mprice = coin;
                                                        fiatprice = (fiat * mprice).toFixed(2);
                                                  ;
                            $('#value').val(mprice);
                             
                                                        

                            $('#USD').val(fiatprice);
                            
                            
                            
                            $(document).attr('title', price + ' USD = 1 BTC; | Bitcoin Price - Bitcoin value converter');

                                                        
                            
                            $('#tool').attr('title', '1 BTC = ' + fiat + ' USD');

                            $('#tool').tooltip();







                        },
                        error: function(data) {

                            $('#value').val("error");

                        }

                    });
					
					restartCountdown();

                }

                
function restartCountdown() {
	
	count = 100;
	
}
					




                getPrice();




                setInterval(function() {



                    getPrice();
					
					



                }, 1000 * 40);
				

				setInterval(function() {

  
			count = count - 3; 
			
			width = count + "%"; 
			
			$("#countdown").css("width", width);
                    
			


                }, 1000);



                function updatec(currency) {



                    switch (currency)

                                {

                                    case "USD": fiat = json.USD["last"]; break;
                                    case "AUD": fiat = json.AUD["last"];   break;
                                    case "JPY": fiat = json.JPY["last"];      break;
                                    case "CAD": fiat = json.CAD["last"];  break;
                                    case "CNY": fiat = json.CNY["last"];  break;
                                    case "EUR": fiat = json.EUR["last"];  break;
                                    case "GBP": fiat = json.GBP["last"]; break;
                                    case "BRL": fiat = json.BRL["last"];  break;
                                                                        case "CHF": fiat = json.CHF["last"]; break;
                                                                        case "CLP": fiat = json.CLP["last"];  break;
                                                                        case "DKK": fiat = json.DKK["last"];   break;
                                                                        case "HKD": fiat = json.HKD["last"];  break;
                                                                        case "ISK": fiat = json.ISK["last"];  break;
                                                                        case "KRW": fiat = json.KRW["last"]; break;
                                                                        case "NZD": fiat = json.NZD["last"];  break;
                                                                        case "PLN": fiat = json.PLN["last"];  break;
                                                                        case "RUB": fiat = json.RUB["last"];     break;
                                                                        case "SEK": fiat = json.SEK["last"]; break;
                                                                        case "SGD": fiat = json.SGD["last"];  break;
                                                                        case "THB": fiat = json.THB["last"];   break;
                                                                        case "TWD": fiat = json.TWD["last"];  break;
                                    default:    fiat = json.USD["last"];



                                }





                    price = json.price;

                    coin = $('#amount').val();
                                        
                    fiatprice = fiat * price * coin;


                   

                                        ;
                                         
                                         mprice = (fiat * coin).toFixed(2);
                                        $('#USD').val(mprice);
                                        ;
        
                                         
                    $('#tool').attr('title', '1 BTC = ' + fiat + ' ' + currency).tooltip('fixTitle').tooltip('show');

                    $('#currency').text(currency);
					$('#fiat').val(currency);


                }









                $('#amount').bind('keydown keyup keypress', calcPrice);



                function calcPrice() {

                    var coins = parseFloat($('#amount').val());

                                                                                var value = coins;
                                                                                
                    $('#value').val(value);

                    usdprice = fiat * value;

                    $('#USD').val(usdprice.toFixed(4));



                }



                $(".cchange").click(function() {

                    currency = this.id;

                    updatec(currency);



                });



            });





        
</script>
