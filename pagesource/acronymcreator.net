<script type="text/javascript">
                
    function StopEnter (event) 
    {
        var keyCode = event.keyCode ? event.keyCode : event.which ? event.which : event.charCode;
        if (keyCode == 13) {please_wait(); return false;} 
        else{return true;}
    }
        
    function del_hide(C,R) 
    {
        if (document.getElementById("tx_"+C+"_"+R).value=='') return;
        document.getElementById("del_"+C+"_"+R).style.backgroundImage = "url('pics/del_off.gif')";
    }

    function del_show(C,R) 
    {
        if (document.getElementById("tx_"+C+"_"+R).value=='') return;
        document.getElementById("del_"+C+"_"+R).style.backgroundImage = "url('pics/del_on.gif')";
    }
    
    function del_entry(C,R)
    {
        tx = document.getElementById("tx_"+C+"_"+R);
        tx.value = "";
        change_table(C,R);
    }

    function col_del_hide(el) 
    {
        el.style.backgroundImage = "url('pics/top-left.gif')";
    }

    function col_del_show(el) 
    {
        el.style.backgroundImage = "url('pics/top-left_on.gif')";
    }
    
    function col_del(C)
    {
        for (c=C; c<9; c++)
        {
            for(r = 0;  r <= 9;  r++)
            {
                tx_this_col = document.getElementById("tx_"+c+"_"+r);   
                if (c<8)
                    {tx_this_col.value =  document.getElementById("tx_"+(c+1)+"_"+r).value;}
                else
                    {tx_this_col.value='';}
            }
            if (c<8)
                {document.getElementById("enforce_"+c).checked = document.getElementById("enforce_"+(c+1)).checked;}
        }
        show_table()
    }
    
    
    function col_add(C)
    {
        for (c=7; c>=C; c--)
        {
            for(r = 0;  r <= 9;  r++)
            {
                tx_next_col = document.getElementById("tx_"+(c+1)+"_"+r);   
                if (c==C) tx_next_col.value='';
                else  tx_next_col.value = document.getElementById("tx_"+c+"_"+r).value;
            }
            if (c==C ) document.getElementById("enforce_"+(c+1)).checked = false; 
            else document.getElementById("enforce_"+(c+1)).checked = document.getElementById("enforce_"+(c)).checked;
        }
        
        show_table()
    }
    
    
    function please_wait(form)
    {	        
        i = 0;
        for (c=0; c<9; c++)
        {            
                if (document.getElementById("tx_"+c+"_0").value != "") {i++;};
        }
        if (i<=1)
            alert("Please give at least two words in two different columns.");        
        else
        {
            el = document.getElementById("WaitMSG");
            el.innerHTML = 'Calculating. Please wait.<br>';
            el.className = "PleaseWait open";
            form.submit();
        }
    }	
    	
      
    var lastEnter = true;
        
    function catchEnter(event)
    {
        if (event.keyCode == 13 || event.keyCode == 9)   
        {
            if (event.keyCode == 13)
                lastEnter = true;
            else
                lastEnter = false;
            document.F.F_Submit.focus();  // to always trigger onchange - otherwise fails in IE
            return false;
        }
        return true;
    }
            
    function key_check(C,R)
    {            
        var checkOK = "qwertzuiopasdfghjklyxcvbnm QWERTZUIOPASDFGHJKLYXCVBNM";
            
        tx = document.getElementById("tx_"+C+"_"+R);
        s = tx.value;
            
        for (i=0; i<s.length; i++)
        {
            ch = s.charAt(i);
            if (checkOK.indexOf(ch)==-1 || (ch==" " && i==0))
            {
                tx.value = s.substring(0, i);
                return false;
            }
        }
             
        if (s.charAt(s.length-1)==" " && R==0)
        {
            tx_next = document.getElementById("tx_"+(C+1)+"_"+0);
            if (tx_next)
            {
                if (tx_next.value=="") 
                { 
                    tx.value = s.substring(0, s.length-1);
                    change_table(C,R);
                    tx_next.focus();
                }
            }
        }
        return false;
    }
      
      
    function show_table()
    {   
        var empty = 0;
        
        // open all columns
        for (C=0; C<9; C++)
        {
            document.getElementById("col_"+C).className = "open";
            document.getElementById("plus_"+C).className = "open";
        }
        document.getElementById("plus_left").className = "open";
    
        // close unnecesary columns at the end        
        for (C=9-1; C>0; C--)
        {
            if (document.getElementById("tx_"+(C)+"_"+0).value != "" || document.getElementById("tx_"+(C-1)+"_"+0).value != "")  
             {             
             break;
             }
            
            document.getElementById("col_"+C).className = "closed";
            document.getElementById("plus_"+C).className = "closed";
            document.getElementById("plus_"+(C-1)).className = "closed";
            
            if (C==1) 
            {
            document.getElementById("plus_left").className = "closed";
            empty=1;            
            }
            
        }
        
        document.getElementById("plus_"+(9-1)).className = "closed";
        
                    
        // check entries    
        for (C=0; C<9; C++)
        {
            document.getElementById("row_"+C+"_"+0).className = "open";
            
            for(i = 0;  i <= 9;  i++)
            {
                tx_el_this = document.getElementById("tx_"+C+"_"+i);
        
                if (tx_el_this.value != "")
                {
                    if (i<9) 
                    {
                        document.getElementById("row_"+C+"_"+(i+1)).className = "open";
                        document.getElementById("or_"+C+"_"+i).className = "open";
                    }
                    document.getElementById("del_"+C+"_"+i).className = "del_entry";
                    document.getElementById("del_"+C+"_"+i).style.backgroundImage = "url('pics/del_off.gif')";
                }	
                else
                {
                    if (i<9) 
                        {document.getElementById("row_"+C+"_"+(i+1)).className = "closed";}
                    document.getElementById("or_"+C+"_"+i).className = "closed";
                    document.getElementById("del_"+C+"_"+i).style.backgroundImage = "url('pics/right.gif')";
                }
            }
        }
    //if (empty==1) document.getElementById("tx_0_0").value = "type here";
    }
                     
            
    function change_table(C,R)
    {
        key_check(C,R);
        
        tx_el_this = document.getElementById("tx_"+C+"_"+R);
        tx_el_next = document.getElementById("tx_"+C+"_"+(R+1));
        td_el_next = document.getElementById("td_"+C+"_"+(R+1));
        
        if (tx_el_this.value != "")   // new entry
        {
            1;
        }

        else     //deleted entry
        {
            var i;
            for(i = R;  i < 9;  i++)
            {
                tx_el_next = document.getElementById("tx_"+C+"_"+(i+1));
                if (tx_el_next.value == "") 
                     break;
                tx_el_this.value = tx_el_next.value;
                tx_el_next.value = "";
                tx_el_this = tx_el_next;
            }
            to_focus = tx_el_this;
        }
            
        show_table();
        
        // find guy to focus
        if (lastEnter == false)  // it was a tab, so try next column
            C=C+1;
            
        for (c = C; c<9; c++)
        {
            for (r=0; r<=9; r++)
            {
                tx = document.getElementById("tx_"+c+"_"+r);
                if (tx.value == "")
                {
                    tx.focus();
                    return;
                }
            }
        }
    }
    
    function more(el, R)
    {
        block = document.getElementById('more_'+R);
        if (block.className == "closed")
        {
            block.className = "open";
            el.src="pics/opened.gif";
        }
        else
        {
            block.className = "closed";
            el.src="pics/closed.gif";
        }
    }
    
    
    function my_reset()
    {                 
        //alert('test');
        for (c=0; c<9; c++)
        {            
            document.getElementById("enforce_"+c).checked = false;
            for(r = 0;  r <= 9;  r++)
            {
                document.getElementById("tx_"+c+"_"+r).value = "";
            }
        }
        show_table();
        //document.getElementById("tx_0_0").value = "type here";
        document.getElementById("tx_0_0").focus();
    }

    function show_example()
    {
    my_reset();
    
    document.getElementById("tx_0_0").value = "ideal";
    document.getElementById("tx_0_1").value = "catchy";
    document.getElementById("tx_0_2").value = "the best";
