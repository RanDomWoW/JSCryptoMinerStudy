    document.getElementById("tx_2_0").value = "of";
    
    document.getElementById("tx_3_0").value = "my";
    document.getElementById("tx_3_1").value = "our";
        
    document.getElementById("tx_4_0").value = "enterprise";
    document.getElementById("tx_4_1").value = "algorithm";
    document.getElementById("tx_4_2").value = "company";
    document.getElementById("tx_4_3").value = "business";
    document.getElementById("tx_4_4").value = "product";
    document.getElementById("enforce_4").checked = true;
    
    show_table();
    }
    
    function toggle_phrases()
    {
    el = document.getElementById('phrases');
    if (el.className == 'comment open')   
        el.className = 'comment closed';  
    else
        el.className = 'comment open';
    }
    
    function toggle_dictionaries()
    {
    el = document.getElementById('dictionaries');
    ShowOptions = document.getElementById('ShowOptions');
    if (el.className == 'dictionaries open')   
        {
        el.className = 'dictionaries closed';  
        ShowOptions.value = 'closed';
        }
    else
        {
        el.className = 'dictionaries open';
        ShowOptions.value = 'open';
        }
    }
    
