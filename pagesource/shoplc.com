<script>
function initAddToCart() {
	$('.add_to_cart_form').each(function () {
		var productCode = this.productCodePost.value;
		$(this).find('button.submitAjaxButton').attr('type','submit');
		var typeOfaddtocart = $(this).find('button.submitAjaxButton').attr('typeOfaddtocart');
		
		var options = {
				success: function(cartResult, statusText, xhr){
					
					if(!LC.login.checkLoged(xhr)) return;
					pushCartToFacebook(productCode);
					if (typeof ACC.product.displayAddToCartPopup == 'function')
					{
						ACC.product.displayAddToCartPopup(cartResult,statusText,xhr,null);
						
					}

				},
				error: function(){
					//omit error
				}
		};
		
		$(this).ajaxForm(options);
	});
}

function pushCartToFacebook (productCode) {
	window._fbq = window._fbq || [];
	window._fbq.push(['track', 'AddToCart', { content_ids: ["'"+productCode+"'"], content_type: 'product' }]);		
}

// $(document).ready(function() {
