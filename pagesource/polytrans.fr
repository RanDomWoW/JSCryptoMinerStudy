<script>
	$(document).ready(function(){
    /*=====================================================*\
====#    Scripts pour la version #diapoHome du diaporama    #================================================================
    \*=====================================================*/
		// On génère le menu de navigation
		$('#diapoHome .item').each(function(index,element) { // Pour chaque item...
			$('#diapoHome .navDiapo').append('<li data-numero="'+index+'">&nbsp;</li>'); //... on crée un li dans .navDiapo et on lui ajoute un attribut data-numero
		});
		$('#diapoHome .navDiapo li:first-child').addClass('active'); //... et la classe "active" si le premier

		// On modifie le code html des <div class="item">
		$('#diapoHome .item').each(function(index,element){ // Pour chaque élément du diapo...
			$(this).attr('data-numero',index); //... on lui ajoute un attribut data-numero
			if(index==0){$(this).addClass('active');} //... et la classe "active" si le premier
			if($(this).find('.text').length){$(this).addClass('hasText');} //... si il y a un texte qui accompagne, on ajoute une classe hasText
			$(this).css('background-image','url('+$(this).find('.mainImage img').attr('src').replace(/\/thumbnails\//, '/')+')');
			$(this).find('.mainImage').remove();
		});

		// Moteur :
		loot = true; // On active la boucle (si true, le diaporama change régulièrement d'élément)
		vitesse = 6; // Vitesse de défilement en secondes

		// Si on quitte un élément de la liste
		$('#diapoHome').mouseleave(function(){
			loot = true; // On réactive la boucle
		});
		// Si on clique un élément de la liste
		$('#diapoHome .navDiapo li').click(function(){
			$('#diapoHome .navDiapo li.active').removeClass('active');
			$(this).addClass('active');
			updateRight($(this).attr('data-numero'));
			loot = false; // On désactive la boucle
		});
		// Si on clique sur une flèche
		$('#diapoHome a.flecheDiapo').click(function(){
			changeListe($(this).attr('data-dir'));
			loot = false; // On désactive la boucle
		});
		// Si on survole le diapo
		$('#diapoHome').mouseenter(function(){
			loot = false; // On désactive la boucle
		});
		setInterval(changeListe, vitesse*1000);

    /*======================================================*\
====#    Scripts pour la version #blocsDiapo du diaporama    #================================================================
    \*======================================================*/
		$('#blocsPromo .navDiapo, #blocsPromo a.flecheDiapo, #blocsPromo .item .title').remove();
		// On génère l'incrémentation des <div class="item">
		$('#blocsPromo .item').each(function(index,element){ // Pour chaque élément du diapo...
			$(this).css('background-image','url('+$(this).find('.mainImage img').attr('src').replace(/\/thumbnails\//, '/')+')');
			$(this).find('.mainImage').remove();
		});

    /*===================================*\
====#    Traitement des fiches du Mag'    #================================================================
    \*===================================*/
		$('#zoneMag .actuMag').each(function(index,element){
			if($(this).find('.image img').length){
				$(this).find('.image').css('background-image','url('+$(this).find('.image img').attr('src').replace(/\/thumbnails\//, '/')+')');
				$(this).find('.image img').remove();
			}
		});


	});


    /*=================*\
====#    Fonctions !    #================================================================
    \*=================*/

	// Fonction qui affiche la partie droite en fonction du lien actif
	function updateRight(elId) {
		$('#diapoHome .item').removeClass('active');
		$('#diapoHome .item[data-numero='+elId+']').addClass('active');
	}

	// Fonction appelée à chaque changement de liste automatique
	function changeListe(direction){
		if(loot==true || direction=='suiv'){ // Si le défilement automatique est activé ou qu'on clique sur la flèche de droite
			// Si l'élément actif n'est pas le dernier, on active le suivant
			if($('#diapoHome .navDiapo li.active').next().text()!=''){
				$('#diapoHome .navDiapo li.active').removeClass('active').next().addClass('active'); }
			// Sinon, on active le premier !
			else{
				$('#diapoHome .navDiapo li.active').removeClass('active');
				$('#diapoHome .navDiapo li').first().addClass('active'); }
			// Et on change !
			updateRight($('#diapoHome .navDiapo li.active').attr('data-numero'));
		}
		else if(direction=='prec'){ // Si on clique sur la flèche de gauche
			// Si l'élément actif n'est pas le premier, on active le précédent
			if($('#diapoHome .navDiapo li.active').prev().text()!=''){
				$('#diapoHome .navDiapo li.active').removeClass('active').prev().addClass('active'); }
			// Sinon, on active le dernier !
			else{
				$('#diapoHome .navDiapo li.active').removeClass('active');
				$('#diapoHome .navDiapo li').last().addClass('active'); }
			// Et on change !
			updateRight($('#diapoHome .navDiapo li.active').attr('data-numero'));
		}
	}
</script>

