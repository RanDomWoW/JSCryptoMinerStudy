<script type="text/javascript">
    function up() {  
        var top = Math.max(document.body.scrollTop,document.documentElement.scrollTop);  
        if(top > 0) {  
          window.scrollBy(0,((top+200)/-10));  
          t = setTimeout('up()',40);  
        } else clearTimeout(t);  
        return false;  
    } 
</script>


  

																							

