    <style type="text/css">
	#stellenmarkt {
		width: 310px;
	}
	.index-toplist- #stellenmarkt {
		float: left;
	}
	#stellenmarkt .content-liste .anzeige {
		font-size: 13px;
		line-height: 18px;
		margin-bottom: 15px;
	}
	#stellenmarkt .content-liste .anzeige a {
		font-weight: bold;
	}

    #schnaeppchen li a, #schnaeppchen .content-liste a {
        display: inline;
        font-size: 13px;
        line-height: 18px;
        margin-bottom: 15px;
        font-weight: bold;
    }

    #schnaeppchen li, #schnaeppchen .content-liste {
        font-size: 13px;
        line-height: 18px;
        margin-bottom: 15px;
    }
    .new_header span span.small {
        font-size: 12px;
        font-weight: normal;
        text-transform: none;
        padding: 0 3px 0 3px;
    }


    #news-uebersicht-videos-liste .systeme{
        margin-top:16px;
    }


    #game_content {
    position: relative;
    padding: 0;
    margin-left: 50px;
    margin-top:0px;
    width: 960px;
    min-height: 627px;
    max-height: 627px;
    overflow: hidden;
    }
    #game_header {
    font-weight: normal;
    height: 76px;
    background: url(http://static3.fore.4pcdn.de/sourcen/portal/4players/games/mini_nsq/mini_nsq_header.jpg);
    border-bottom: white 5px solid;
    }
    #clock_min {
    position: absolute;
    font-size: 30px;
    top: 23px;
    right: 77px;
    }
    #game_upper_header {
    position: absolute; left: 10px; top: 12px; font-size: 19px; font-weight: bold; color: #f59700;
    }
    #game_under_header {
    position: absolute; left: 10px; bottom: 12px; font-size: 19px; font-weight: bold; color: white;
    }

    #clock_sec {
    position: absolute;
    font-size: 30px;
    top: 23px;
    right: 10px;
    }
    #clock_dp {
    position: absolute;
    font-size: 30px;
    top: 21px;
    right: 56px;
    }
    #game_content h2 {
    color:#f59700;
    margin-left: 10px;
    }
    #game_highscores {
    color: white;
    position: absolute;
    top: 500px;
    left: 0px;
    font-size: 13px;
    height: 117px;
    }
    #game_content #game_highscores div {
    color: white;
    float: left;
    margin-left: 10px;
    font-size: 13px;
    width: 250px;
    margin-top: 5px;
    margin-right: 10px;
    }
    #game_content #game_highscores div a {
    font-size: 13px;
    color:#f59700;
    float: left;
    max-width: 180px;
    overflow: hidden;
    }
    #game_content #game_highscores div .counter {
    font-size: 13px;
    width: 17px;
    float: left;
    margin-right: 5px;
    }
    #game_content #game_highscores div .score {
    font-size: 13px;
    float: right;
    }
    mark {
    font-weight: bold;
    color:#f59700;
    }


.fb-like-iframe{
	border:none;
	overflow:hidden;
	width:120px;
	height:21px;
}
.facebook-likebutton
{
	width:120px;
	height:21px;

    .social-button {
        width: 100%
        height: 100%;
        display: inline-block;
        cursor: pointer;
    }
}
    .social-counter {
        font-family: "Open Sans", Arial, snas-serif;
        color: white;
        line-height: 23px;
        vertical-align: bottom;
        float: right;
        margin-right: 5px;
        display: inline-block;
        font-size: 13px;
    }
    .social-image {
        min-width: 24px;
        max-width: 24px;
        min-height: 24px;
        max-height: 24px;
        margin: 0;
        margin-bottom: -1px;
        background: transparent;
        float: left;
        display: inline-block;
    }
    .social-facebook {
        background-color: #3b5998;
        margin: 0px;
        margin-right: 1px;
        width: 70px;
        float: left;
        display: inline-block;
        cursor: pointer;
    }
    .social-facebook .social-image {
        background-image: url(http://static1.fore.4pcdn.de/sourcen/portal/4players/icons.png);
        background-position: -99px -128px;
    }
    .social-twitter {
        background-color: #2caae1;
        margin: 0px;
        margin-right: 1px;
        width: 70px;
        float: left;
        display: inline-block;
        cursor: pointer;
    }
    .social-twitter .social-image {
        background-image: url(http://static1.fore.4pcdn.de/sourcen/portal/4players/icons.png);
        background-position: -150px -128px;
    }
    .social-gplus {
        background-color: #da503b;
        margin: 0px;
        width: 70px;
        margin-right: 1px;
        float: left;
        display: inline-block;
        cursor: pointer;
    }
    .social-gplus .social-image {
        background-image: url(http://static1.fore.4pcdn.de/sourcen/portal/4players/icons.png);
        background-position: -125px -128px;
    }
    .social-pur {
    /*background-color: #090;
    width: 70px;
    cursor: pointer;*/
    margin: 0px;
    display: inline-block;
    float: left;
    }
    .social-pur .social-image {
        min-width: 0px;
    }



    #bericht-social div a .social-image, .bericht-social div a .social-image {
        float: left;
    }

