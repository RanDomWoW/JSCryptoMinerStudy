</form>
<script>
var URL = "http://s3.amazonaws.com/grayfilestore-kwtx/gpollData/KWTX_poll.json?"+Math.random();

function populateForm(e){
	document.getElementById("poll_id").value = e[0].id;
	document.getElementById("pollQuestion").innerHTML = e[0].question;
	if(e[0].sponsor){
	    document.getElementById("pollSponsor").innerHTML = 'Brought to you by:<br/><img src="'+e[0].sponsor+'" style="max-width:100%;">';
	}
	if(!getCookie('gPoll[' + jQuery('#poll_id').val() + ']')) {
		var formHTML = '';
		for(var i=0; i<Object.keys(e[0].responses).length; i++){
			key = e[0].responses;
			formHTML+='<li class="list-group-item">';
			formHTML+='<div class="radio">';
			formHTML+='<label>';
			formHTML+='<input type="radio" name="response" value="'+Object.keys(key)[i]+'">'+key[Object.keys(key)[i]];
			formHTML+='</label>';
			formHTML+='</div>';
			formHTML+='</li>';
		}
		document.getElementById("pollAnswer").innerHTML = formHTML;
	}
	else {
		//jQuery("#poll_answers").html("<p>You have already voted for this poll, you may not vote again.</p>");
		setLoading(0);
                displayResults(e);
		jQuery("#vote_btn").prop("disabled", true);
		jQuery("#vote_btn").prop("value", "Voted");
	}
	document.getElementById("lp_form_id").style.display = 'block';
}

function inactiveForm(){
  document.getElementById("lp_form_id").style.display = 'none';
  document.getElementById("pollQuestion").innerHTML = 'There are no polls currently active. Please try again later.';
}

function getPollData(URL, t){
console.log(URL);
  jQuery.ajax({
      url: URL,
      dataType: 'json',
      success: function(e){
        if(e[0].active == 1){
            populateForm(e);
        }else if(e[0].active == 0){
          inactiveForm();
        }else{
          alert('There is something wrong');
          console.log(e);
        }
      }
    });
}

function setLoading(e){
  if(e == 1){
    document.getElementById("pollAnswer").style.display = "none"; 
    document.getElementById("submitControls").style.display = "none";
    document.getElementById("lp_loader").style.display = "block"; 
  }else{
    document.getElementById("lp_loader").style.display = "none"; 
  }
}

function displayResults(e){
  document.getElementById("back").innerHTML = '';
  var formHTML = '<div class="lp_thankyou">Thank you for voting!</div>';
  for(var i=0; i<Object.keys(e[0].responses).length; i++){
      counts = e[0].counts;
      perc = e[0].percents;
      key = e[0].responses;
      formHTML+='<li class="list-group-item">'+key[Object.keys(key)[i]];
      formHTML+='<div class="progress">';
      formHTML+='<div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width:'+perc[Object.keys(perc)[i]]+'">';
      formHTML+='<span class="sr-only">'+perc[Object.keys(perc)[i]]+' Complete</span>'+counts[Object.keys(counts)[i]];
      formHTML+='</div>';
      formHTML+='</div>';
      formHTML+='</li>';
  }
  document.getElementById("pollAnswer").style.display = "none"; 
  document.getElementById("submitControls").style.display = "none"; 
  document.getElementById("back").innerHTML = formHTML;
}

jQuery(document).ready(function () { 
	getPollData(URL, 0);   
	//post to DB
	jQuery("#lp_form_id").submit(function (e) {
		e.preventDefault();
		console.log(getCookie('gPoll[' + jQuery('#poll_id').val() + ']'));
		if(!getCookie('gPoll[' + jQuery('#poll_id').val() + ']')) {
			jQuery.ajax({
				type:"GET",
				url: 'http://awspublic-loadbalancer-1236266993.us-east-1.elb.amazonaws.com/gPoll/poll_ajax.php',
				cache:false,
				data:jQuery("#lp_form_id").serialize(),
				crossDomain: true,
				dataType:'jsonp',
				jsonp:'jsonp_callback',
				success:function(data){
					if(data.response=="success"){
						setCookie('gPoll[' + jQuery('#poll_id').val() + ']', true, 60*60*24); 
						setLoading(1);
		                setTimeout(function () {
		                  getPollData(URL, 1);
		                }, 2000);
						jQuery("#vote_btn").prop("disabled", true);
						jQuery("#vote_btn").prop("value", "Voted");
					}else{
						jQuery("#pollAnswer").html(data.response);
						jQuery("#vote_btn").prop("disabled", true);
						jQuery("#vote_btn").prop("value", "Voted");
						//alert(data.response);
					}
				}
			});
		}
		else {
			jQuery("#poll_answers").html("<p>You have already voted for this poll, you may not vote again.</p>");
			jQuery("#vote_btn").prop("disabled", true);
			jQuery("#vote_btn").prop("value", "Voted");
		}
	});
});

function setCookie(cname, cvalue, expires) {
    var d = new Date();
    d.setTime(d.getTime() + (expires * 1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}
</script>




