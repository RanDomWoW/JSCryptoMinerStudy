                // get the nearest store //
                function setCookie(cname, cvalue, exdays) {
                    var d = new Date();
                    d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
                    var expires = "expires=" + d.toUTCString();
                    document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
                }

                function getCookie(cname) {
                    var name = cname + "=";
                    var ca = document.cookie.split(';');
                    for (var i = 0; i < ca.length; i++) {
                        var c = ca[i];
                        while (c.charAt(0) == ' ') c = c.substring(1);
                        if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
                    }
                    return "";
                }

                function showNearestLocationDetails() {
                    var responseStr = getCookie("storeDetails");
                    var storeDetailsArr = responseStr.split("|");
                    var country = (storeDetailsArr[6] == 'null' || storeDetailsArr[6] == '') ? '' : "| " + storeDetailsArr[6];
                    var storeDet = storeDetailsArr[1] + " | " + storeDetailsArr[3] + ", " + storeDetailsArr[4] + " " + storeDetailsArr[5] + " " + country;
                    $('#store-address').empty();
                    $('#store-address').html(storeDet);
                    $('#locator_name').html(storeDetailsArr[2] != '' ? "<b>" + storeDetailsArr[2] + "</b> | " : storeDetailsArr[0] + " | ");
                    var positionStr = getCookie('position');
                    var position = positionStr.split("|")
                    var queryStr = "saddr=" + position[0] + "," + position[1] + "&daddr=TCC, " + storeDetailsArr[1] + ", " + storeDetailsArr[3] + ", " + storeDetailsArr[4] + ", " + storeDetailsArr[5]
                    var href = "https://maps.google.com/maps?" + queryStr;
                    $('#direction_lnk').attr("href", encodeURI(href));
                    $('#title').html('Nearest Store');
                }


                function checkNearestLocationCookieExists() {
                    var storeDetails = getCookie("storeDetails");
                    if (storeDetails != "") {
                        showNearestLocationDetails();
                    } else {
                        if (navigator.geolocation) {
                            navigator.geolocation.getCurrentPosition(showPosition);
                        }
                    }
                }

                function showPosition(position) {
                    console.log("Latitude: " + position.coords.latitude +
                        "<br>Longitude: " + position.coords.longitude);
                    var data = {
                        'action': 'get_nearest_store',
