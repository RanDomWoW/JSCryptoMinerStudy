		// set the src of the image to either the default or the nearest breakpoint
		var setSrc = function(nodes) {
			nodes.each(function(val, i) {
				var image = $(this);
				var sizes = JSON.parse(image.attr("data-sizes"));
				var src = sizes.default;

				// look over our size breakpoints and find the smallest breakpoint that our screen width is less than, utilize that src
				var width = $(window).width();
				var breakpoints = Object.keys(sizes.breakpoints).map(function(val) { return Number(val) }).sort(function(a, b) { return b < a });

				breakpoints.some(function(val) {
					if (width <= Number(val)) {
						src = sizes.breakpoints[val];
						return true;
					}
				});

				image.attr("src", src);
			});
		}

