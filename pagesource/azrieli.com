        function getRightPos() 
        {
            var leftElmPos = $j('#content').position().left;
            var rightPos = leftElmPos - 80;

            return rightPos;
        }


        $j(window).resize(function () 
        {
            var rightPos = getRightPos();
            $j('a.back-to-top').css("left", rightPos + "px");
        });


        $j(window).ready(function () 
        {
            var rightPos = getRightPos();
