    <script type="text/javascript">
  
                    //For ie7, AJAXCallGet was undefined
                    function AJAXCallGet(webMethod, onSuccess) {
                        $j.ajax({
                            url: webMethod,
                            success: onSuccess,
                            type: "GET"
                        });
                    }

                    var newLink = jQuery("#secureThickbox").attr('link');
                    jQuery("#secureThickbox").attr('href', newLink);


                    function UpdateIfLoggedInAndCard() {
                        var webMethod = "/My_Account/IsLoggedIn/";
                        var x="";
                        var parameters = "{}";
                        try {
                            AJAXCallGet(webMethod, function (x) {
                                OnUpdateIfLoggedInSuccess(x);
                                UpdateCartCount();
                            });
                        } catch (e) {
    
                        }
                    }

                    function OnUpdateIfLoggedInSuccess(retVal) {
                        if (retVal.d != undefined && retVal.d != "") {
                            var result = retVal.d;
                            if (result.success && result.LoggedInUser!="") {
                                var account=$j('#menuMyAccount');
                                $j('#HelloId').text(result.LoggedInUser);
                                $j("#my_account_link").show();

                                if($j('.fConnect')[0].style.display!="none"){
                                    $j('.fConnect').hide();
                                }
                            }else{
                                //jQuery("#topMainMenu").width(jQuery("#topMainMenu").width()+100);
                                document.getElementById('fConnect').style.display="block";
                                $j("#my_account_link").hide();
                            }
                        }
                    }


                    function UpdateCartCount() {
                        //Change to: "/AllDeals/GetUserDetailes/"
                        var webMethod = "/Cart/ItemCount/";
                        var x="";
                        var parameters = "{}";
                        AJAXCallGet(webMethod, function (x) {
                            OnUpdateCartCountSuccess(x);
                        });
                    }
                    function OnUpdateCartCountSuccess(retVal) {
                        if (retVal.d != undefined) {
                            var result = retVal.d;
                            if (result.success) {
                                jQuery("#cartCount").html(result.count);
                                if(result.count > 0) {
                                    if(document.getElementById('buy2blogo') != null && typeof(document.getElementById('buy2blogo')) != 'undefined'){
                                          //  jQuery("#topMainMenu").width(376);
                                            if (screen.width >= 1280 && screen.width < 1600) {
                                                document.getElementById('joinLink').style.display = "none";
                                            }
                                    }
                                    else if(document.getElementById('fConnect') != null && typeof (document.getElementById('fConnect')) != 'undefined' &&
                                                jQuery("#fConnect")[0].style.display == "block"){
                                           // jQuery("#topMainMenu").width(376);
                                            if (screen.width >= 1280 && screen.width < 1600) {
                                                document.getElementById('logotype-bgId').style.paddingRight = "8px";
                                            }
                                    }
                                    else if(jQuery("#topCartButton")[0].style.display!="block" && jQuery("#topMainMenu").width()<256)
                                        jQuery("#topMainMenu").width(jQuery("#topMainMenu").width()+100);

                                    //jQuery("#logotype-bgId")[0].style.paddingRight="65px";
                                    //jQuery("#topCartButton").show();
                                    jQuery("#topCartButton")[0].style.display="block";
                                }
                            }
                        }
                    }

                 

                    function ShowCartDetails() {
                        $j('#modal-cart').modal({ close: false });
                        LoadCart();
                        return false;
                    }
                    function LoadCart() {
                        var webMethod = "/Cart/ItemDetails/";
                        var parameters = "{}";

                        AJAXCall(webMethod, parameters, function (x) {
                            OnLoadCartSuccess(x);
                        });
                    }
                    function OnLoadCartSuccess(retVal) {
                        if (retVal.d != undefined) {
                            var result = retVal.d;
                            if (result.success) {

                                var html = '';
                                for (var i = 0; i < result.items.length; i++) {
                                    //html += "<div><img src='" + result.items[i].ImageUrl + "' width=45 />" + result.items[i].ProductName + "<br /> Quantity : " + result.items[i].Quantity + "</div> <hr />";
                                    html += "<div><img src='" + result.items[i].ImageUrl + "' width=45 />" + result.items[i].ShortName + "<br /> Quantity : " + result.items[i].Quantity + "</div> <hr />";
                                }

                                html += "<a href='/Cart/' style='color:#000000'>View My Cart</a>";

                                html = "<div style='padding : 4px; width : 500px; height : 450px; overflow : auto; '>" + html + "</div>";
                                $j("#cartListing").html(html);
                            }
                        }
                    }
    </script>
