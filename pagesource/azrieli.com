    <script type="text/javascript">

        var timer;
        $j("a.linkToThirdCatMenu").hover(function () {
            if (!timer) {
                timer = window.setTimeout(function () {
                    timer = null;
                    openThirdCatMenu();
                }, 100);
            }
        },
        function () {
            if (timer) {
                window.clearTimeout(timer);
                timer = null;
            }
        });

        function showSubMenu(event) {

            if ($j(this).find('.hasSubCategoryHeader').length) 
            {
                $j(".hasSubCategoryHeader").show();
            }
            else {
                $j(".hasSubCategoryHeader").hide();
                $j('ul', this).show();
            }
        }
        function hideSubMenu(event) {
            $j(".hasSubCategoryHeader").hide();
            $j('ul', this).not(".TopMenuThirdCategories").hide();
        }

        $j("#mainCategoriesContainer").find('li').not('li.SubCategoryImg').not('li.categoriesContainer').hoverIntent({
            over: showSubMenu,
            timeout: 100,
            interval: 100,
            out: hideSubMenu
        });

        $j('li.TopMenuSubCategoryHeader').not('.topMenuLink').hoverIntent({
            over: showThirdCatMenu,
            out: function () { return; },
            interval: 100,
            timeout: 100
        });

         function showThirdCatMenu(event) 
        {
            $j("ul.TopMenuThirdCategories").hide();
            $j('ul.TopMenuThirdCategories', this).show();
        }

        function openThirdCatMenu(cat) {
          
            $j("ul.TopMenuThirdCategories").hide();
            if (cat == undefined) {
                $j(".hasSubCategoryHeader").show();
                $j(".hasSubCategoryHeader").find(".TopMenuThirdCategories").first().show();
            }
            else 
            {
                $j("li.activefashion").find("ul.hasSubCategoryHeader ").show();
                $j(".hasSubCategoryHeader").find('a.TopMenuCategoryLink').filter(function () {
                    return $j(this).text() === cat;
                }).parent().find('ul').show();
            }

        }

        function openThirdCatMenuCurrent(event) 
        {
            openThirdCatMenu($j(this).find('span').text());
        }

        function hideThirCatMenuCurrent(event) 
        {
            $j(".hasSubCategoryHeader").find('a.topSubCategoryInnerLinkHeader').filter(function () {
                return $j(this).text() === $j(this).find('span').text();
            }).parent().find('ul').hide();
        }

        $j("#fashionTopMenu").find('a').not('.topMenuLink').hoverIntent({
            over: openThirdCatMenuCurrent,
            out: function () { return; },
            interval: 100,
            timeout: 100
        });

        $j(".TopMenuCategories").mouseout(function () {
            $j(".hasSubCategoryHeader").hide();
        });

        
       
    </script>

