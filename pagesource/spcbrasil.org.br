    <script type="text/javascript">
        $(function($){
            $('#slides').cycle({
                after: function(currSlideElement, nextSlideElement, options, forwardFlag){
                },
                before: function(currSlideElement, nextSlideElement, options, forwardFlag){
                    var index = $(this).parent().children().index(nextSlideElement),
                        pager = $('#controller').find('.jFlowControl').eq(index);

                    handlePagerBehavior(pager);
                },
                skipInitializationCallbacks: true,
                pauseOnPagerHover: true,
                pause: true,
                delay: 7000
            });

            $('#controller').find('.jFlowControl').click(function(){
                var index  = handlePagerBehavior(this);
                $('#slides').cycle(index);
            });

            function handlePagerBehavior(ele){
                var index = $(ele)
                    .siblings()
                    .removeClass('jFlowSelected')
                    .end()
                    .addClass('jFlowSelected')
                    .parent()
                    .children()
                    .index(ele);

                return index;
            }
         });


