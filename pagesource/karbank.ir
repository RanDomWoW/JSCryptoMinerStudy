<script>
function changeDefaultTab(tab_id)
{
	content = document.getElementById(tab_id).innerHTML;
	document.getElementById("Tab_default").innerHTML = content;

	if (tab_id == 'Tab_1')
	{
		document.getElementById("Head_Tab_1").className = "active";
		document.getElementById("Head_Tab_2").className = "";
		document.getElementById("Head_Tab_3").className = "";
	}
	else if (tab_id == 'Tab_2')
	{
		document.getElementById("Head_Tab_1").className = "";
		document.getElementById("Head_Tab_2").className = "active";
		document.getElementById("Head_Tab_3").className = "";
	}
	else if (tab_id == 'Tab_3')
	{
		document.getElementById("Head_Tab_1").className = "";
		document.getElementById("Head_Tab_2").className = "";
		document.getElementById("Head_Tab_3").className = "active";
	}
}
</script>

