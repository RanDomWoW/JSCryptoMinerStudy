    <script>
        $(document).ready(function(){
            $(".contenedor_acordeon span").addClass("boton-mas");
            
            $('.bxslider').bxSlider({
                auto: false,
                autoStart: false
            });
            
            $('.bxslider-logos').bxSlider({
                minSlides: 1,
              maxSlides: 3,
              auto: false,
              moveSlides: 1,
              slideWidth: 160,
              slideMargin: 70,
              pager: false,
              controls: true,
              nextText: '>',
              prevText: '<',
              responsive: true
            });
            
            $(".bxslider-alianzas").bxSlider({
                auto: true,
                autoStart: true,
                responsive: true,
                minSlides: 1,
                maxSlides: 4,
                moveSlides: 1,
                slideWidth: 230,
                slideMargin: 30,
                pager: false,
                controls: false
            });
            
            $(".fancybox").fancybox();
            $(".various").fancybox({
                maxWidth	: 800,
                maxHeight	: 450,
                fitToView	: false,
                width		: '70%',
                minHeight	: 300,
                autoSize	: false,
                closeClick	: false,
                openEffect	: 'none',
                closeEffect	: 'none'
            });

            $(".contenedor_acordeon").on("click", function(){
                if ($(this).hasClass("fondo_azul")) {
                    $(this).next(".contenedor_contenido_acordeon").slideUp();
                    $(this).removeClass("fondo_azul");
                    $(this).find("span").removeClass("boton-menos").addClass("boton-mas");
                    $(".contenedor_acordeon").removeClass("fondo_azul");
                    $(".contenedor_acordeon").find("span").removeClass("boton-menos").addClass("boton-mas");
                } else {
                    $(".contenedor_acordeon").removeClass("fondo_azul");
                    $(".contenedor_contenido_acordeon").slideUp();
                    $(this).addClass("fondo_azul");
                    $(".contenedor_acordeon").find("span").removeClass("boton-menos").addClass("boton-mas");
                    $(this).find("span").removeClass("boton-mas").addClass("boton-menos");
                    $(this).next(".contenedor_contenido_acordeon").slideDown();
                }
            });
            
            $( "article.centro-de-documentacion a:contains('Descarga')" ).addClass( "btn btn-success" );
            $( "article.documentos a:contains('Descarga')" ).addClass( "btn btn-success" );
            $( "article.boletin-chilecompra a:contains('Descarga')" ).addClass( "btn btn-success" );
            
            // Organizar y mover Eventos en Calendario
            var mes = $("#mes-actual").val();
            var mover_evento_anterior = "";
            var ev = 0, ad = 0;
            $(".container_event").each(function(){
                var categoria = "";
                var mes_aux = $(this).find(".mes-evento").val();
                if (mes_aux === mes) {
                    $(this).removeClass("ocultar_evento").addClass("mostrar_evento");
                    mover_evento = $(this).find(".id-evento").val();
                    /*alert(mes+" "+mes_aux+" "+mover_evento);*/
                    if (mover_evento_anterior != mover_evento) {
                        ev = 0;
                        ad = 0;
                        $('<div class="marcador_eventos"></div>').appendTo("#"+mover_evento);
                        var alerta = $("#"+mover_evento);
                        for (var i = 1; i < 8; i++) {
                            if (alerta.hasClass("end")) {
                                i = 8;
                            } else {
                                alerta = alerta.next();
                            }
                        }
                        $('<li id="'+mover_evento+'-id" class="number_events"></li>').insertAfter(alerta);
                    } else {
                        if ($(this).hasClass("eventos") && ev == 0) {
                            $('<span class="eventos"></span>').appendTo("#"+mover_evento+" .marcador_eventos");
                            ev = 1;
                        }
                        if ($(this).hasClass("agenda-directora") && ad == 0) {
                            $('<span class="agenda-directora"></span>').appendTo("#"+mover_evento+" .marcador_eventos");
                            ad = 1;
                        }
                    }
                    if ($(this).hasClass("eventos") && ev == 0) {
                        $('<span class="eventos"></span>').appendTo("#"+mover_evento+" .marcador_eventos");
                        ev = 1;
                    }
                    if ($(this).hasClass("agenda-directora") && ad == 0) {
                        $('<span class="agenda-directora"></span>').appendTo("#"+mover_evento+" .marcador_eventos");
                        ad = 1;
                    }
                    $(this).appendTo("#"+mover_evento+"-id");
                    mover_evento_anterior = mover_evento;
                } else {
                    $(this).remove();
                }
            });
            
            $("#calendar ul.dates li").on("click", function(e){
                e.preventDefault();
                $("#calendar ul.dates li").removeClass("evento_select");
                var id_aux = $(this).attr("id");
                $(".number_events").removeClass("mostrar_evento_select");
                $("#"+id_aux+"-id").addClass("mostrar_evento_select");
                $(this).addClass("evento_select");
            });
            
            $("#control_tabulador li").on("click", function(e){
                e.preventDefault;
                var id_aux = $(this).attr("id");
                if (!$(this).hasClass("tab_seleccionado")) {
                    $("#control_tabulador li").removeClass("tab_seleccionado");
                    $(this).addClass("tab_seleccionado");
                    $("#tabulador div.tab_content").removeClass("mostrar_content_tab").addClass("ocultar_content_tab");
                    $("#"+id_aux+"_tab").removeClass("ocultar_content_tab").addClass("mostrar_content_tab");
                }
            });
            
            //Show/hide events on home page
            /* var mes_actual_home = $(".mes-actual").val();
            var dia_actual_home = $(".dia-actual").val();
            $("#eventos_inicio article").each(function(){
                var mes_actual_articulo = $(this).find(".mes-evento-home").val();
