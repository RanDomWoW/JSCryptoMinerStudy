/*** GOOGLE LAYER HOMEPAGE  **/
function googleLayerHomeCerca() {			
	var linAnd = "";
	var mmA = "";
	var ggA = "";
	var andata = "";
	var linRit = "";
	var mmR = "";
	var ggR = "";
	var ritorno = "";
	
	
	try{
		if(document.getElementById('lina_c').value){
			linAnd = document.getElementById('lina_c').value.substring(3);
			mmA = document.getElementById('mma').value;
			ggA = document.getElementById('gga').value;
			if(mmA != "" && ggA != ""){
				andata = mmA + "-"+ ggA;
			}
		}	
		
		if(document.getElementById('linr_c').value){
			linRit = document.getElementById('linr_c').value.substring(3);
			mmR = document.getElementById('mmr').value;
			ggR = document.getElementById('ggr').value;
			if(mmR != "" && ggR != ""){
				ritorno = "-"+ mmR + "-"+ ggR;
			}
			
		}else{
			linRit = "solo-andata"
	 	}
	
		dataLayer.push({ 
			'event':'GAevent', 
			'eventCategory':'widget',
			'eventLabel':andata+ritorno,
			'eventAction':'cerca', 
			'trattaAndata':linAnd,
			'trattaRitorno':linRit 
		});
									
	}catch(e){}
}

