                    )
                $curobj.hover(
                    function(e) {
                        var $targetul = $(this).children("dl:eq(0)")
                        this._offsets = {
                            left: $(this).offset().left,
                            top: $(this).offset().top
                        }
                        var menuleft = this.istopheader ? 0 : this._dimensions.w
