<script type="text/javascript">
$(function(){  
function shuffle(array) {
  var n = array.length, t, i;
  while (n) {
    i = Math.floor(Math.random() * n--);
    t = array[n];
    array[n] = array[i];
    array[i] = t;
  }
  return array;
}
var charcount = function (str) {
  len = 0;
  str = escape(str);
  var i;
  for (i=0;i<str.length;i++,len++) {
    if (str.charAt(i) == "%") {
      if (str.charAt(++i) == "u") {
        i += 3;
        len++;
      }
      i++;
    }
  }
  return len;
}
var trimlength = 24;
var data = document.getElementById('dataJSON').textContent;
var data = JSON.parse(data);
var sdata = shuffle(data);
for (var i=0 ; i<=7 ; i++){
    if ( i == 0 ){
        var photoarticleTitle = sdata[i]['title'];
        if( sdata[i]['title'].length >= trimlength){
            if(trimlength*2 > charcount(photoarticleTitle.substr(0,trimlength))){
                trimlength = (trimlength*2 - charcount(photoarticleTitle.substr(0,trimlength)))/2 + trimlength;
                trimlength = Math.floor(trimlength);
            }else{
                trimlength = trimlength;
            }
            photoarticleTitle = photoarticleTitle.substr(0,trimlength)+"...";
        }
        $('#editorTopics .largeArea > a').attr('href', sdata[i]['link']).attr('title', sdata[i]['title']);
        $('#editorTopics figure').html(sdata[i]['img']);
        $('#editorTopics h2.articleTitle').html(photoarticleTitle);
    }else{
        ii = i-1;
        $('#editorTopics ul.titleList li:eq('+ii+') a').attr('href', sdata[i]['link']).attr('title', sdata[i]['title']).html(sdata[i]['title']);
    }
}

$('#editorTopics largeArea figure').show();
$('#editorTopics h2').show();
});
</script>        	
