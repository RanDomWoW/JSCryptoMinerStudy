<script type="text/javascript">
    function validateLogin() {
        
        //remove waterwarksa first.
        HideAllWatermarks();

        var email = $get('ctl00_cphContent_UserMenu1_LoginBox1_txtEmail');
        var password = $get('ctl00_cphContent_UserMenu1_LoginBox1_txtPassword');
        var status = $get('ctl00_cphContent_UserMenu1_LoginBox1_lblStatus');

        if (email.length == 0 || email.value == "") {
            status.innerHTML = "אנא הקלד דואר אלקטרוני";

            return false;

        }

        var emailReg = /^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/;
        var regex = new RegExp(emailReg);
        if (!regex.test(email.value)) {
            status.innerHTML = "אנא הקלד דואר אלקטרוני תקין";

            return false;

        }

        if (password.length == 0 || password.value == "") {
            status.innerHTML = "אנא הקלד סיסמא";

            return false;
        }

        return true;

    }
</script>

