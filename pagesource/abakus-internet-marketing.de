<script>
function validateEmail(email) {
  var reg = new RegExp(/^[A-Z0-9._%+-]+@([A-Z0-9-]+\.)+[A-Z]{2,4}$/i);
  return email.match(reg);
}

jQuery(document).ready(function() {

  jQuery('#w-preview-fields-content-cell-field-email').keypress(function(e) {
    var keycode = (e.keyCode ? e.keyCode : e.which);
    if (keycode == '13') {
      e.preventDefault();
      jQuery("#form-button").click();
    }
  });

  jQuery("#form-button").click(function(e) {
    e.preventDefault();
    if (!validateEmail(jQuery("#w-preview-fields-content-cell-field-email").val())) {
      swal({
        title: "Hinweis!",
        text: "Bitte überprüfen Sie Ihre E-Mail.",
        type: "warning",
        confirmButtonColor: "#b51434",
        confirmButtonText: "OK",
        closeOnConfirm: true
      });
    } else {
      ajaxNewsletter();
      jQuery(this).prop('disabled', true);
    }
  });

  function ajaxNewsletter() {
    jQuery.ajax({
      beforeSend: function() {
        jQuery("#newsletterResultText").html('<i class="fa fa-circle-o-notch fa-spin fa-fw"></i> Bitte warten...');
      },
      type: "POST",
      url: "https://www.abakus-internet-marketing.de/wp-includes/newsletterMailjet/newsletterPHPMailjet.php",
      dataType: "html",
      data: "newsletterEmail=" + jQuery('#w-preview-fields-content-cell-field-email').val(),
      //async: false,
      cache: false,
      success: function (response) {

        //clean the fields
        jQuery("#w-preview-fields-content-cell-field-email").val("");//empty input field
        jQuery("#newsletterResultText").html("");//empty the preloader container

        var parsed = jQuery.parseHTML(response);//parse HTML
        var resultText = "";//string for user info
        //alert(response);//trace

        //first check if .error doesn't exists (in order to check for success first)
        if(jQuery(parsed).find(".error").length == 0) {
          //console.log("Element .error it doesn't exist");

          //then look for success
          if(jQuery(parsed).find(".success").length == 0) {
            //console.log("Element .success it doesn't exist");
            resultText = "Bitte versuchen Sie es später noch einmal.";//error return custom message
          } else {
            //console.log("Element .success exists.");
            resultText = jQuery(parsed).find(".success").first().text();//parse success message
          }

        } else {
          //console.log("Element .error exists.");
          resultText = jQuery(parsed).find(".error").first().text();//parse error message
        }

        //check if resultText var is empty
        if (response == 0) {
          resultText = "Bitte versuchen Sie es später noch einmal";//error return custom message
        } else if (resultText == "") {
          resultText = "Bitte versuchen Sie  es später noch einmal";//error return custom message
        }

        //console.log(resultText);//trace
        swal({
          title: "ABAKUS Newsletter",
          text: resultText,//string variable, info for the user
          confirmButtonColor: "#b51434",//button color
          confirmButtonText: "OK",//button string
          closeOnConfirm: true//if the popup will close on click OK
        });

      },
      error: function (request, status, error) {
          //console.log(request.responseText);
      }
    });
    return false;
  }

});
</script></div>
