			$(this).val('');
		}	
	});
	//onblur
	$('#newsLetterEmail').blur(function(){
		var getName = $('#newsLetterEmail').val();
			if(getName==''){
				$(this).val('آدرس ایمیل');
			}
			else{
				$('#newsLetterEmail').val(getName);
