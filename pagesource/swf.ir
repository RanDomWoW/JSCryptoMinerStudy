function newsLetterSubmit ( elementId ) {
	$("#SubmitRegisterNewsletter").css('background','url(img/newsletter/back.png) center left no-repeat');
	$("#SubmitUnRegisterNewsletter").css('background','url(img/newsletter/back.png) center left no-repeat');
	$("#"+elementId).css('background','url(img/newsletter/back_hover.png) center left no-repeat');
}
function checkmail(FormSingUp)
{
	var flag=true;
	var Mail = FormSingUp.TMail.value;
	var Mobile = FormSingUp.TMobile.value;
	if(Mail == "" | Mobile == "" | Mail == "آدرس ایمیل" | Mobile == "تلفن همراه")
	{
		alert("شماره موبایل و یا ایمیل وارد نشده است .");
		flag=false;
	}
	if(flag!=false)
	{ 
		function email_validator(email) 
		{
			reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
			if(reg.test(email) == false) 
			{
				alert("آدرس ایمیل خود را صحیح وارد کنید");
				flag=false;
			}	
		}
		email_validator(Mail);
	}
	return flag;
}
