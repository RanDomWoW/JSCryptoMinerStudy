<script type="text/javascript">
var t = n =0, count;
$(document).ready(function(){ 
count=$("#Focus_Images_list a").length;
$("#Focus_Images_list a:not(:first-child)").hide();
$("#Focus_Images_info").html($("#Focus_Images_list a:first-child").find("img").attr('alt'));
$("#Focus_Images_info").click(function(){window.open($("#Focus_Images_list a:first-child").attr('href'), "_blank")});
$("#Focus_Images li").click(function() {
var i = $(this).text() -1;//获取Li元素内的值，即1，2，3，4
n = i;
if (i >= count) return;
$("#Focus_Images_info").html($("#Focus_Images_list a").eq(i).find("img").attr('alt'));
$("#Focus_Images_info").unbind().click(function(){window.open($("#Focus_Images_list a").eq(i).attr('href'), "_blank")})
$("#Focus_Images_list a").filter(":visible").fadeOut(500).parent().children().eq(i).fadeIn(1000);
document.getElementById("Focus_Images").style.background="";
$(this).toggleClass("on");
$(this).siblings().removeAttr("class");
});
t = setInterval("showAuto()", 4000);
$("#Focus_Images").hover(function(){clearInterval(t)}, function(){t = setInterval("showAuto()", 4000);});
})

function showAuto()
{
n = n >=(count -1) ?0 : ++n;
$("#Focus_Images li").eq(n).trigger('click');
}
</script>
