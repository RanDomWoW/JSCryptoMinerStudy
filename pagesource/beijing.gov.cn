<script>
$(function(){
    //需要jquery框架
    jQuery.poplayer = function(btn,layer,evt){
        $(btn).find(".arc").hide();
        if(evt == "hover"){
            $(layer).hover(function(){
                $(this).show();
                $(this).prevAll(btn).find(".arc").show();
            },function(){
                $(this).hide();
                $(this).prevAll(btn).find(".arc").hide();
            });
            $(btn).hover(function(){
                $(this).find(".arc").show();
                $(this).nextAll(layer).show();
            },function(){
                $(this).find(".arc").hide();
                $(this).nextAll(layer).hide();
            });
        }
        if(evt == "click"){
            $(btn).click(function(){
                if($(this).nextAll(layer).is(":visible")==false){
                    $(this).find(".arc").show();
                    $(this).nextAll(layer).show();
                }else{
                    $(this).find(".arc").hide();
                    $(this).nextAll(layer).hide();
                }
            })
        }
    }
    $.poplayer(".btn_pop",".layer","hover");
    $(".right_bar .btn_close").click(function(){
        $(this).parents(".right_bar").hide();   
    });
});
</script>
