	<script type="text/javascript">
	var $siteback = null;
	var $sitedomain = null;
	function FBLogin(login, goBackAfterLoginToSiteNewWindow, fb_login_domain) {
		goBackAfterLoginToSiteNewWindow = goBackAfterLoginToSiteNewWindow || 0;
		$siteback = goBackAfterLoginToSiteNewWindow;
		$sitedomain = fb_login_domain;
		
		var loginOFMFB = function() {
			FB.api('/v2.8/me', function(response) {
				var para = '';
				if (login) {
					para = "&login=1";
				}

				if (login == 'RELOAD') {
					self.reload();
				} else {
					if (!$sitedomain)
					{
						window.location = '/public3/includes/facebook/connect/fbstation.php?fb=AUTHED' + para + '&goBackAfterLoginToSiteNewWindow=' + $siteback;
					}
					else
					{
						window.location = 'http://' + $sitedomain + '/public3/includes/facebook/connect/fbstation.php?fb=AUTHED' + para + '&goBackAfterLoginToSiteNewWindow=' + $siteback;
					}
				}
			});
		};

        		var login = function() {
			FB.getLoginStatus(function(response) {
				if (response.status === 'connected') {
					loginOFMFB();
				} else {
					FB.login(function(response) {
						if (response.authResponse) {
							loginOFMFB();
						}
					}, {scope: 'email,publish_actions'});
				}
			});
		};

		// Verarbeitung
		if (!document.getElementById('fb-root')) {
			fb_div = document.createElement("div");
			fb_div.setAttribute("style", "position:absolute;z-index:99999;", 0);
			fb_div.setAttribute("id", "fb-root");
			document.body.appendChild(fb_div);

			(function(d, s, id) {
				var js, fjs = d.getElementsByTagName(s)[0];
				if (d.getElementById(id)) {
					return;
				}
				js = d.createElement(s);
				js.id = id;
				js.src = "//connect.facebook.com/de_DE/sdk.js";
				fjs.parentNode.insertBefore(js, fjs);
			}(document, 'script', 'facebook-jssdk'));

			window.fbAsyncInit = function() {
				FB.init({
					appId: '161772757225502', // Anwendungs ID
					version: 'v2.8',
					status: true, // Login Status prüfen
					cookie: true, // Cookies nutzen
					xfbml: true  // XFBML parsen
				});
				login();
			};
		} else {
			login();
		}
	}
</script>

