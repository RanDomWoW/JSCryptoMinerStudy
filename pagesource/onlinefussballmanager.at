	function register_check_email_used()
	{
		
		var akt_email = document.registrierung.email.value;
		if ((akt_email.length > 0) && (email_check_last_value != akt_email))
		{
			email_check_last_value = akt_email;
			
			if (email_already_checked[akt_email] == 'ok')
			{
				email_set_ok('Deine eMail', 'Deine angegebene eMail-Adresse ist korrekt');
			}
			else if (email_already_checked[akt_email])
			{
				//email_already_used = akt_email;
				email_set_error('Deine eMail',email_already_checked[akt_email]);
			}
			else
			{
				email_check_version++;
				$.get('/js/request.email_used.php?email=' + encodeURI(akt_email + '&ev=' + email_check_version + '&s=' + server), function(data)
				{
					dummy = data.split('#');
					akt_check_version = dummy[0];
					data = dummy[1];
					if (akt_check_version == email_check_version)
					{
						email_check_version = akt_check_version;
						if (data)
						{
															email_already_checked[akt_email] = data;
															email_set_error('Deine eMail',data);
						}
						else
						{
							email_already_checked[akt_email] = 'ok';
							email_set_ok('Deine eMail', 'Deine angegebene eMail-Adresse ist korrekt');
						}
					}
					email_timer_active = false;
				});
			}
		}
	}

