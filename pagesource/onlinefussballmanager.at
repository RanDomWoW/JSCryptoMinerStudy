	function fokus_check()
	{
		if (!reg_over && reg_focus)
		{
			reg_over = reg_focus;
		}
		if (reg_over == 'password')
		{
						$('#reg_email_box').hide();
			$('#reg_password_box').show();
			$('#reg_password2_box').hide();
			return true;
		}
		else if (reg_over == 'email')
		{
						$('#reg_email_box').show();
			$('#reg_password_box').hide();
			$('#reg_password2_box').hide();
			return true;
		}
		else if (reg_over == 'password2')
		{
						$('#reg_email_box').hide();
			$('#reg_password_box').hide();
			$('#reg_password2_box').show();
			return true;
		}
		else if (reg_over == 'serverBox')
		{
						$('#reg_email_box').hide();
			$('#reg_password_box').hide();
			$('#reg_password2_box').hide();
			return true;
		}
				return false;
	}

