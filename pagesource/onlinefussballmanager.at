	function email_has_forbidden_characters()
	{
		if (document.registrierung.email.value.length > 0)
		{
			var hit = document.registrierung.email.value.match(/[^@a-z0-9-_.+]/i);
			if (hit)
			{
				return hit[0];
			}
		}
		return false;
	}

	function email_correct_sytax()
	{
		if (document.registrierung.email.value.length > 0)
		{
			if (document.registrierung.email.value.match(/^([-_.0-9a-z+]+@[0-9a-z]+[-._0-9a-z]*\.[a-z]{2,15})$/i))
			{
				return true;
			}
		}
		return false;
	}

