				var name = $('#name').val();
				var text = $('#text').val();
				var betreff = $('#betreff').val();
				var mail = $('#mail').val();
				var code = $('#code').val();
				var server = $('#imprintSelectServer p').html().replace(/Server/, '');

				$.ajax({
					url: "/ajax/kontakt_formular.php",
					data: {'name' : name, 'text':text, 'betreff': betreff, 'mail':mail, 'code':code, 'server':server},
					dataType: "json",
					type: 'POST'
				}).success(function(data) {
					if (data['error']){
						$('#imprintMailMessage div').removeClass('icon-info');
						$('#imprintMailMessage div').removeClass('icon-warning');
						$('#imprintMailMessage div').addClass('icon-warning');

						$('#imprintMailMessage').removeClass('mailMessageBoxError');
						$('#imprintMailMessage').removeClass('mailMessageBox');
						$('#imprintMailMessage').addClass('mailMessageBoxError');

						$('#formularErrorText').css('color', '#da470c');

						$('#formularErrorText').html(data['error']);

						var d = new Date();

						$('#captchaImg').attr('src', '/pss/captcha_pic.php?t='+d.getTime());
					}else{
						$('#imprintMailMessage div').removeClass('icon-info');
						$('#imprintMailMessage div').removeClass('icon-warning');
						$('#imprintMailMessage div').addClass('icon-info');

						$('#imprintMailMessage').removeClass('mailMessageBoxError');
						$('#imprintMailMessage').removeClass('mailMessageBox');
						$('#imprintMailMessage').addClass('mailMessageBox');

						$('#formularErrorText').css('color', '#0c3976');
						$('#formularErrorText').html(data['ok']);

					}

					$('#imprintMailMessage').show();
					$('#imprintContact').hide();

					setTimeout('$(\'#imprintMailMessage\').fadeOut();',1500 );
				});
				return false;
			});
		});

