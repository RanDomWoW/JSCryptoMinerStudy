	function register_check_password()
	{
		password_ok = false;
		if ((reg_focus != 'password') && (document.registrierung.password.value.length < 5) && (document.registrierung.password.value.length > 0))
		{
			// do nothing while focused
		}
		else if (pwBlacklist[document.registrierung.password.value])
		{
			password_set_error('Passwort', 'Dein Passwort ist zu einfach zu erraten')
			password_ok = false;
		}
		else if (specialchar = password_has_forbidden_characters())
		{
			password_set_error('Passwort', 'Dein Passwort enthält unerlaubte Zeichen ("' + specialchar + '")');
		}
		else if (document.registrierung.password.value.length > 26)
		{
			password_set_error('Passwort', 'Dein Passwort ist zu lang (max. 26 Zeichen)')
		}
		else if (document.registrierung.password.value.length >= 5)
		{
			password_set_ok('Passwort', 'Dein Passwort ist in Ordnung')
			password_ok = true;
		}
		else
		{
			password_set_info('Passwort', 'Wähle dein Passwort (mind. 5 Zeichen)')
		}

		if (!fokus_check())
		{
			$('#reg_password_box').hide();
		}

		if (!password_ok && (document.registrierung.password.value) && (reg_focus != 'password'))
