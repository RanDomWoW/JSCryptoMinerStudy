<script>
			$(document).ready(function() {
				$('.loading').click(function() {
					$('body').append('<div id="loading"><img src="/application/front/views/templates/main/images/loading.gif" alt="olele.bg" /></div>');
				});
				
								
								
				$('a').click(function() {
					if ($(this).attr('href') == '') {
						return false;
					}
				});
				
				$("#menu").mouseleave(function() {
					$('#menu > ul > li > a.category').removeClass('active');
					$("#menu #drop-down .menu-content").hide();
				});
				$("#menu > ul > li > a.category").mouseenter(function(event) {
					var thisTab = $("#menu > ul > li > a").index($(this));
					
					if ($("#menu #drop-down .menu-content:eq("+thisTab+")").is(':visible') == false) {
						$("#menu #drop-down .menu-content").hide();
						if ($('#cart-content').is(':visible')) {
							$('#cart-content').hide();
						}
						
						var subMenu = $("#menu #drop-down .menu-content:eq("+thisTab+")");
						if (subMenu.find('.sub-category').length > 0) {
							subMenu.show();
							subMenu.css('width', $('#wrap').width() - 20);
						}
					} else {
						$("#menu #drop-down .menu-content").hide();
					}
				});
				
				$('#cart').mouseenter(function() {
					if ($('#cart-content').is(':visible') == false) {
						$.get('https://olele.bg/cart/content/', function(data) {
							$('#cart-content').html(data);
							$('#cart-content').css('display', 'block');
						});
					}
				});				
				$('.product').mouseenter(function() {
					$(this).children(".add-to-cart").slideToggle(200);
				});
				
				$('.product').mouseleave(function() {
					$(this).children(".add-to-cart").slideToggle(200);
				});
				
				$('#button-recently-viewed').click(function() {
					if ($('#recently-viewed-products').is(":visible"))
						$('#recently-viewed-products').hide(function() {
							$('#recently-viewed-products').slick('removeSlide', null, null, true);
							$('#recently-viewed-products').slick('unslick');
						});
					else {
						$('#recently-viewed-loading').show();
						$('#recently-viewed-products').show(function() {
							$.get('https://olele.bg/products/recently-viewed/', function(data) {
								dataJSON = JSON.parse(data)
								if (dataJSON.length > 0) {
									$('#recently-viewed-products').slick({
										infinite: false,
										slidesToShow: 9,
										slidesToScroll: 9,
										nextArrow: '<span class="slick-arrow-right white"></span>',
										prevArrow: '<span class="slick-arrow-left white"></span>',
									});
									
									$.each(dataJSON, function(i, val) {
										$('#recently-viewed-products').slick('slickAdd', val);
									});
									
									$('#recently-viewed-loading').hide();
								}
							});
						});
					}
				});
				
				$(document).on('click', '.wishlist', function () {
					var product = $(this).attr('data-id');
					var obj = $(this);
					
					$.post('https://olele.bg/wishlist/toggle/', {product: product}, function(data) {
						var dataJSON = JSON.parse(data);
						if (dataJSON.success == false) {
							if (dataJSON.type == 'not-logged') {
								if (confirm(dataJSON.message))
									window.location = 'https://olele.bg/account/create/';
							}
							else
								alert(dataJSON.message);
						} else {
							obj.removeClass('inactive').removeClass('active');
							obj.addClass(dataJSON.class);
						}
					});
				});
				
				$(document).on('click', '.add-to-cart', function () {
					var productID = $(this).attr('data-product');
					var cart = $('#cart');
					var imgtodrag = $('#product-image-' + productID);
					var imgclone = imgtodrag.clone()
						.offset({
						top: imgtodrag.offset().top,
						left: imgtodrag.offset().left
					})
					.css({
						'opacity': '0.8',
						'position': 'absolute',
						'width': imgtodrag.css('width'),
						'z-index': '100'
					})
					.appendTo($('body'))
					.animate({
						'top': cart.offset().top + 10,
						'left': cart.offset().left + 10,
						'width': 75
					}, 500);
					
					imgclone.animate({'width': 0, 'height': 0}, function() {
						$(this).detach()
						
						$.post('https://olele.bg/cart/add/', {product: productID, quantity: 1}, function(data) {
							var dataJSON = JSON.parse(data);
							
							fbq('track', 'AddToCart', {
								content_name: dataJSON.product,
								content_ids: [productID],
								content_type: 'product',
								value: dataJSON.price / 2,
								currency: 'EUR'
							});
							
							window.criteo_q.push(
								{ event: "setAccount", account: 46709 },
								{ event: "setSiteType", type: "d" },
								{ event: "viewBasket", item: [
								 { id: productID, price: dataJSON.price, quantity: 1 }
								]}
							);
							
							
							if ($('#cart-content').is(':visible') == false) {
								$('#cart').mouseenter();
								setTimeout(function(){ $('#cart-content').hide(500); }, 2000);
							} else {
								$.get('https://olele.bg/cart/content/', function(data) {
									$('#cart-content').html(data);
								});
							}
							
							$('#subtotal').html(dataJSON.subtotal); 
								$('#items-count').html(dataJSON.itemsCount); 
								if (dataJSON.message) 
									alert(dataJSON.message);
						});
					});
				});
				
				$('.products-order').change(function() {
					var url = '?order=_ORDER_';
					window.location.href = url.replace('_ORDER_', $(this).val());
				});
				
				$('.attribute').click(function() {
					var url = '?attribute=__ATTRIBUTE_';
					window.location.href = url.replace('__ATTRIBUTE_', $(this).val());
				});
				
				var searchHintsEnabled = false;
								
				$('#search input[type="text"]').mousedown(function() {
					if (searchHintsEnabled == true) {
						$('#search input[type="text"]').val('');
						searchHintsEnabled = false;
					}
				});
			});
		</script>
