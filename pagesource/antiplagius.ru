                size = size.toFixed(2) + ' КБ';
            } else {
                size = size / 1024;
                size = size.toFixed(2) + ' МБ';
            }
            
            return size;
        },
        addFile: function(elem) {
            var $file = $(elem);
            var file = $(elem)[0].files[0];
            var size = file.size;
            
            size = this.getSize(size);
            
            console.log('test&')
            
            this.totalSize += file.size;
            
            $list = this.getFileList(this.formSelector);
            
            $item = $(this.tpl);
            $item.data('name', $file.prop('name'));
            $item.find('.file-title').html(file.name);
            $item.find('.file-size').html(size);
            
            $list.append($item);
            
            
        },
        checkFiles: function() {
            
            this.totalSize = 0;
            
            $(this.formSelector).find('[type=file]').each(function(){
                var elem = $(this);
                //$(files[1]).prop('files')[0].size
                var files = elem.prop('files');
                if (!files.length) {
                    return true;
                }
                console.log(feedbackForm.getSize(files[0].size));
                feedbackForm.totalSize += files[0].size;
                console.log(feedbackForm.getSize(feedbackForm.totalSize));
            });
            
            var size = this.getSize(this.totalSize);
            
            if (this.totalCount > 0) {
                $(this.formSelector).find('.file-list').show();
                
                $(this.formSelector).find('.total-file-size').html(size);
                
            } else {
                $(this.formSelector).find('.file-list').hide();
                $(this.formSelector).find('.total-file-size').html(size);
                //$(this.formSelector).find('.file-total').hide();
            }
        }
    };
    
    function addFileToFeedback (form) {
        feedbackForm.fileIndex++;
        $form = $(form);
        $inputList = feedbackForm.getInputList($form);
        
        var name = 'file_' + feedbackForm.fileIndex;
        
        $inputList.append('<input type="file" name="' + name + '">');
        $form.find('[name="' + name + '"]').click();
    }
    
    $(document).on('change', feedbackForm.formSelector + ' input[type=file]', function(e){
        console.log(e);
        console.log($(this)[0].files);
        
        var file = $(this)[0].files[0];
        var maxSize = 15 * 1024 * 1024;
        
        if ((file.size + feedbackForm.totalSize) >= maxSize) {
            console.error('Превышен допустимый размер файлов');
            
            var size = feedbackForm.getSize(file.size);
            
            $(feedbackForm.formSelector).find('.file-error').html('Превышен допустимй размер файлов.<br> Загрузка файла <em><b>"' + file.name + '" (' + size + ')</b></em> невозможна.').show();
            setTimeout(function(){
                $(feedbackForm.formSelector).find('.file-error').hide();
            }, 3500);
            
            e.preventDefault();
            $(this).remove();
            return false;
        }
        
        console.log(file);
        feedbackForm.totalCount++;
        feedbackForm.addFile($(this));
        feedbackForm.checkFiles();
        
        
    });
    
    $(document).on('click', feedbackForm.formSelector + ' .file-remove', function(e){
        e.preventDefault();
        
        var $item = $(this).parents('.file-row');
        var name = $item.data('name');
        
        $file = $(feedbackForm.formSelector).find('[name="' + name + '"]');
        console.log(name);
        console.log($file);
        if ($file.length) {
            $file.remove();
        }
        
        $item.remove();
        
        feedbackForm.totalCount--;
        feedbackForm.checkFiles();
        
        $(this).blur()
        return false;
    });


