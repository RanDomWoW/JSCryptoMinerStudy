        if (typeof LiveTex === 'undefined' || typeof LiveTex.openWelcomeWindow !== 'function') {
            setTimeout(function(){
                activateWindowHello();
            }, 500);
            return false;
        }
        LiveTex.openWelcomeWindow();
        return true;
    }
    
    function listenHrefToActivateWindowHello() {
        var str = window.location.search;
        if (str.indexOf('?') !== 0) return false;
        var arStr = str.split('?')[1].split('&');
        if (arStr.length < 1) return false;
        var temp;
        for (var k in arStr) {
            temp = arStr[k].split('=');
            if (temp[0] === 'activechat' && typeof temp[1] !== 'undefined' && temp[1] === '1') {
                return activateWindowHello();
            }
        }
    }
    
