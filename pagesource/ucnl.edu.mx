        /// <reference path="angular.js" />
'use strict';
/**
 * @ngdoc overview
 * @name UCNL2016
 * @description
 * # UCNL2016
 *
 * Módulo principal.
 */
//var UCNL2016 = angular.module('UCNL2016', [
angular.module('UCNL2016', [
    //'UCNL.Tickets',
    'oc.lazyLoad',
    'ui.router',
    //'ui.bootstrap',
    'angular-loading-bar',
    'LocalStorageModule',
    'ngAlertify',
    'ngFileSaver'
    //'ngCookies',
    //'vcRecaptcha'
    //'angular-confirm'
]).config(['$stateProvider','$urlMatcherFactoryProvider', '$urlRouterProvider', '$ocLazyLoadProvider', '$locationProvider', '$httpProvider', 'localStorageServiceProvider', '$provide',
    function ($stateProvider, $urlMatcherFactory,  $urlRouterProvider, $ocLazyLoadProvider, $locationProvider, $httpProvider, localStorageServiceProvider, $provide) {

        $locationProvider.html5Mode(true);

        $urlMatcherFactory.caseInsensitive(true);


        $provide.decorator('$browser', ['$delegate',
            function ($delegate) {
                var _baseHref = $delegate.baseHref,
                    _url = $delegate.url;

                $delegate.baseHref = function () {
                    return angular.lowercase(_baseHref());
                };
                $delegate.url = function (url, replace) {
                    // no need to modify the setter
                    if (url) {
                        return _url(url, replace);
                    }
                    // just lowercase the getter
                    return angular.lowercase(_url());
                };
                return $delegate;
            }
        ]);



    $urlRouterProvider.otherwise('/');

    $urlRouterProvider.rule(function ($injector, $location) {
        var path = $location.path();
        var hasTrailingSlash = path[path.length - 1] === '/';

        if (hasTrailingSlash) {

            //if last charcter is a slash, return the same url without the slash  
            var newPath = path.substr(0, path.length - 1);
            return newPath;
        }

    });



    $httpProvider.interceptors.push(function () {
        return {
            "request": function (config) {
                if (config.url && config.url.endsWith && config.url.endsWith(".html")) {
                    config.headers["Accept"] = "*/*";
                }
                else if (config.url && !config.url.endsWith && config.url.indexOf(".html") === config.url.length - 5) {
                    config.headers["Accept"] = "*/*";
                }
                /*if (sessionStorage.logdata)
                    config.headers["Auth"] = JSON.parse(sessionStorage.logdata).jwt;*/
                return config;
            }
        };
    });

    $ocLazyLoadProvider.config({
        debug: false,
        events: false
    });

    $stateProvider
        .state('portada', {
            url: '/portada',
            templateUrl: 'fragments/portada.min.html'
        })
        .state('inicio', {
            url: '/',
            templateUrl: 'fragments/inicio.min.html',
            cache: !localStorage.clearCache
        })
        .state('registro', {
            url: '/registro',
            templateUrl: 'fragments/registro.min.html'
            //templateUrl: 'fragments/portada.min.html'
        })
        .state('estadisticas-registro', {
            url: '/estadisticas',
            templateUrl: 'fragments/estadisticas-registro.min.html'
            //templateUrl: 'fragments/portada.min.html'
        })
        .state('admin', {
            url: '/admin',
            templateUrl: 'fragments/admin.min.html'
        })
        .state('confirmacionCorreo', {
            url: "/confirmacion-correo/{IdAlumno:int}/{CadenaConfirmacion}",
            templateUrl: 'fragments/confirmacionCorreo.min.html',
            resolve: {
                loadMyFiles: ['$ocLazyLoad', function ($ocLazyLoad) {
                    return $ocLazyLoad.load({
                        name: 'UCNL2016',
                        files: [
                            'controllers/confirmacionCorreoController.min.js'
                        ]
                    })/*, $ocLazyLoad.load({
                    name: 'ui.bootstrap',
                    files: [
                        'js/angular-ui/ui-bootstrap-tpls.min.js'
                    ]
                })*/;
                }]
            }

        });
    }])
.run(["$rootScope", "$cacheFactory", "localStorageService", "$http", function ($rootScope, $cacheFactory, localStorageService, $http) {
    window.$cacheFactory = $cacheFactory;

    $http.defaults.headers.common.Auth = localStorageService.get('jwt') ? localStorageService.get('jwt').jwt : null;

    if ($http.defaults.headers.common.Auth !== null) {
        $http.post('api/refresh').then(
            function (r) {
                localStorageService.set('jwt', r.data);
                $http.defaults.headers.common.Auth = localStorageService.get('jwt').jwt;
            }, function (r) {
            });
    }


    $rootScope.$on('$stateChangeSuccess', function () {
        document.body.scrollTop = 0;
        if (localStorage.clearCache)
            localStorage.removeItem('clearCache');
    });
}]);





window.nullProp = function (object, prop) {
    if (object === null)
        return null;
    else
        return object[prop];
};
        
    </script>
    
    
    
        
