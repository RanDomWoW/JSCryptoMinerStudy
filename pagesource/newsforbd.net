$(document).ready(function(){	
	
	$('#Send').click(function(){
		
        var vote = $("input[@name='radio_name']:checked").val();
		var id1 = $('[name=id]').val();
		
		var recorddate = $('[name=recorddate]').val();
		
		//var id  = $('#id').attr('value');
		// alert("Data Loaded: " + recorddate );
		if( vote == null)
        {
                alert("Select your choice.");
				return false;
        }
		
		
		showLoader();
		$.post("http://www.newsforbd.net/voting/result/"+vote+"/"+id1+"/"+recorddate,{
		}, function(response){
			hideLoader();
			$('#wrap').html(unescape(response));				
		});
	});	
	
	//show loading bar
	function showLoader(){
		$('.search-background').fadeIn(200);
	}
	//hide loading bar
	function hideLoader(){
		$('.search-background').fadeOut(200);
	};
	
});	

</script>
