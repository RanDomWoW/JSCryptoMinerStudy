					.data(topojson.feature(topodata, topodata.objects.states).features)
					.enter().append('path')
						.attr('class', 'Map_filled')
						.style('fill', fill_function)
						.attr('d', path)
						.on('click', click_filled)
						.on('mouseover', mouseover_filled)
						.on('mouseout', mouseout_filled);
						
				// outlines
				g.append('path')
					.datum(topojson.mesh(topodata, topodata.objects.states))
					.attr('class', 'Map_outline')
					.attr('d', path);
				
				add_labels(topojson.feature(topodata, topodata.objects.states).features);
			});
		</script>
