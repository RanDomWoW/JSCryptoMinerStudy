    <script type="text/javascript">
        $(function() {
            var $images = $('img[data-fallback]');
            if (window.attachEvent) {
                $.each($images, function(i, element) {
                    element.attachEvent('onerror', function (event) {
                        var $target = $(event.currentTarget);
                        var fallback = $target.data('fallback');
                        if (fallback) {
                            $target.prop('src', fallback);
                        }
                    });
                });
            } else {
                $.each($images, function (i, element) {
                    element.addEventListener('error', function (event) {
                        var $target = $(event.currentTarget);
                        var fallback = $target.data('fallback');
                        if (fallback) {
                            setTimeout(function () { 
                                $target.prop('src', fallback);
                            }, 100);
                        }
                    });
                });
            }

            var $lazyLoadImages = $('img._lazy-load');
            if ($lazyLoadImages.length) {
                $.each($lazyLoadImages, function(i, element) {
                    var $image = $(element);
                    var imageSource = $image.data('src');
                    if (imageSource) {
                        var imagePreloader = new Image();
                        imagePreloader.onload = function() {
                            $image.prop('src', imageSource);
                        }
                        imagePreloader.src = imageSource;
                    }
                });
            }
        });
        
        !function($, document, window) {
            
            function socialShare (event) {
                var $target = $(event.currentTarget);
                var criteria = $target.data('criteria');

                var title = '빙그레';
                var sharedUrl = location.href;
                
                if (criteria) {
                    switch (criteria) {
                    case 'facebook':
                        window.open('https://www.facebook.com/sharer/sharer.php?u=' + sharedUrl, 'facebookWindow', 'height=380,width=660,resizable=0,toolbar=0,menubar=0,status=0,location=0,scrollbars=0');
                        return;
                    case 'twitter':
                        window.open(
                            'http://twitter.com/intent/tweet?text=' + encodeURIComponent(title) + ' ' + encodeURIComponent(sharedUrl), 'twitterWindow', 'height=380,width=660,resizable=0,toolbar=0,menubar=0,status=0,location=0,scrollbars=0');
                        return;
                    }
                }
            }
            
            var defaultInitializer = function() {
                var $document = $(document);
                if ($document.length) {
                    $document.on('click', 'a._social-share', function(event) {
                        socialShare(event);
                    });
                }
            }
            
            $(document).ready(defaultInitializer);
        }(jQuery, document, window);
    </script>
	
    <script type="text/javascript">
    (function($) {
        $(document).ready(function() {
            var $videoPopup = $('#mainSlidepop');
            if ($videoPopup.length) {
                var $playerFrame = $videoPopup.find('.yut_player iframe');
                var $popupClose = $videoPopup.find('.btn_close');
                $('.yut_play').on('click', function(event) {
                    var $target = $(event.currentTarget);
                    var $chooseWrap = $target.closest('._choose-wrap');
                    var youtubeSource = $target.prop('href');
                    
                    $playerFrame.prop('src', youtubeSource);
                    $('.tit').text($chooseWrap.find('.card_txt').text());
                });
                $popupClose.on('click', function() {
                    $playerFrame.prop('src', '');
                });
            }

            var $popupName = $(".main_notice");
            var popupCount = $popupName.length;

            for (var i = 0; popupCount > i; i++) {
                var cookieData = document.cookie;
                var start = cookieData.indexOf("binggrae_popup_" + $popupName.eq(i).data("no"));
                if (start == -1) {
                    $popupName.eq(i).css("display", "block");
                } else {
                    $popupName.eq(i).remove();
                    var $dimm = $('.mask');
                    if ($dimm.length) {
                        $dimm.hide();
                    }
                }
            } 

            var $noticeClose = $popupName.find("button");
            $noticeClose.on('click', function (event) {
                var $target = $(event.currentTarget);
                var $chooseWrap = $target.closest('.wrapper');

                $chooseWrap.remove();
            });

            // Main Visual Image Cache
            var $visualImageSource = $('.main_visual .indigator button');
            if ($visualImageSource.length) {
                for (var i = 0, size = $visualImageSource.length; i < size; i++) {
                    var imageSource = $visualImageSource.eq(i).data('url');
                    if (imageSource) {
                        var imageCache = new Image();
                        imageCache.src = imageSource;
                    }
                }
            }
        });
    })(jQuery);

    function setCookie(event, no) {
        var $target = $(event.currentTarget);
        var $chooseWrap = $target.closest('.wrapper');
        var $dimm = $('.mask');
    
        if (!$chooseWrap.length) {
            return;
        }
        if (!$dimm.length) {
            return;
        }

        var expire = new Date();
        expire.setDate(expire.getDate() + 1);
        cookies = 'binggrae_popup_'+no + '=binggrae; path=/ ';
        cookies += ';expires=' + expire.toGMTString() + ';';
        document.cookie = cookies;

        $chooseWrap.remove();
        $dimm.hide();
    }
    </script>

