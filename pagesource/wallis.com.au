					jQuery(elems[i]).removeAttr('href');
					jQuery(elems[i]).click(function() {
						jQuery('#search').fadeToggle();
						jQuery('#search .text').focus();
						jQuery('#search .text').select();
					});
			}
		}

		if (!jQuery.browser.opera) {
			jQuery('select').each(function() {
				var title = jQuery(this).attr('title');
				jQuery(this)
					.css({
						'z-index': 10,
						'opacity': 0,
						'-khtml-appearance': 'none'
					})
					.after('<span class="select">' + title + '</span>')
					.change(function() {
						val = jQuery('option:selected', this).text();
						jQuery(this).next().text(val);
					});
			});
		}

		/* jQuery('#block-views-front-now-showing-block h2').bind('click', function() {
			location.href = '/movies/now-showing';
		});
		jQuery('#block-views-front-promotions-block h2').bind('click', function() {
			location.href = '/movies/coming-soon';
		}); */

		jQuery('.quick-find-big').bind('click', function() {
			if (!jQuery.browser.msie || jQuery.browser.version != '6.0') {
				/* $this = jQuery(this);
				href = $this.children('a').attr('href').substr(1);
				if (href.indexOf('/', 0) != -1) {
					href = href.substr(href.indexOf('/') + 1);
				} */
				href = 'buy-online';

				$overlay = jQuery('#quick-find-overlay');

				if (!$overlay.length) {
					// Create overlay div
					$body = jQuery('body');
					$body.append('<div id=\'quick-find-overlay\'></div>');
					$body.append('<div id=\'quick-find-info\' class=\'box\'></div>');
					$overlay = jQuery('#quick-find-overlay');
					$overlay.bind('click', function() {
						jQuery('#quick-find-overlay').hide();
						jQuery('#quick-find-info').hide();
					});
				}

				$info = jQuery('#quick-find-info');

				$overlay.css('display', 'block');
				$info.css('display', 'block');
				$info.css('height', '335px');
				$info.css('width', '630px');
				$info.css('margin-top', '-168px');
				$info.css('margin-left', '-315px');
				$info.html('');
				$info.append('<div class="close"></div>');
				$close = jQuery('#quick-find-info .close');
				$close.bind('click', function() {
						jQuery('#quick-find-overlay').hide();
						jQuery('#quick-find-info').hide();
				});

				jQuery.ajax({
					url: '/ajax/quick-find-page/' + href,
					success: function(json) {
						json = jQuery.parseJSON(json);
						jQuery('#quick-find-info').append('<div class="quick-buy-now-selector"><h3 style="color:#ffff00;font-weight:bold;">Tickets</h3><p>Select a movie and a venue below and click <em style="font-style:italic">Buy Tickets</em>.</p>' + json.quick_buy_now_selector + '<br><br><h3 style="color:#ffff00;font-weight:bold;">Vouchers</h3><p>Click on <em style="font-style:italic">Buy Vouchers</em> to select your vouchers.</p><form><input type="button" onclick="parent.location=\'http://www.wallis.com.au/gift-store\'" value="Buy Vouchers"></form>');
						jQuery('#quick-find-info').append(json.node);
						jQuery('#quick-find-info .node-page')
							.removeClass('node-page')
							.addClass('quick-buy-now-content');
					}
				});

				return false;
			}
		});

			});
</script>

