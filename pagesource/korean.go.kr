//통합 검색
function searchAllSubmit(searchGubun){
    if($('#searchKeyword','#searchAllForm').val()==""){
        alert("검색어를 입력하세요.");
        $('#searchKeyword','#searchAllForm').focus();
        return;
    }
    if($("#searchOrderGubun", "#searchAllForm").val()==""){
        $("#searchOrderGubun", "#searchAllForm").val("accuracy");
    }
    $("#searchGubun", "#searchAllForm").val(searchGubun)
    $("#searchAllForm").attr("action","/front/search/searchAllList.do;front=696524D869CB3A45C9A2EADEDD18B163").submit();
}
</script>
