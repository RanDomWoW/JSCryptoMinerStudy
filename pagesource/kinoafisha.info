		iframe.style.visibility = 'hidden';
		iframe.style.display = 'none';
		document.getElementsByTagName("body")[0].appendChild(iframe);
		var doc = iframe.contentDocument;
		doc.open();
		doc.writeln(iframeContent);
		doc.close();

		setCookie("timeout_set", "1", {
			"expires" : 3600 * 24,
			"path" : "/"
		});
	}
</script>
