<script type="text/javascript" src="scripts/geolocalizacao.js?v=7"></script>
<script type="text/javascript">
    var cookieCidade = "";
    var stopGeolocalizacao = false;

    geolocalizacao = new geolocalizacao();

    $(document).ready(function () {

        if (cookieCidade == "" && page == 'index') {
            geolocalizacao.geolocalizar(function () {
                if (stopGeolocalizacao == true)
                    return;

                $("#cidade").val(geolocalizacao.cidade);
                atualizarCidade(geolocalizacao.cidade);
                //montaLinks(cidade);
            });
        }

        $("#cidade").keydown(function () {
            stopGeolocalizacao = true;
        });

        $("#desc-cidade2, #alterar-local-pesquisa").click(function () {
            $("#div-barra-busca-conteudo").toggleClass("modoCidade");
            $("#cidade").val('').focus();
        });

        $("#filtro_topo_mobile select").change(function () {
            var tipoSelecionado = $(this).val();

            if (tipoSelecionado == 'Endereços')
                tipoSelecionado = 'endereco';

            if (tipoSelecionado == 'Telefones')
                tipoSelecionado = 'telefone';

            tipoSelecionado = tipoSelecionado.toLowerCase();

            $("li[data-tipo=" + tipoSelecionado + "]").trigger("click");
        });

        if (tipoBusca == '') {
            tipoBusca = 'todos';
        }

        tipoBuscaSelecionado = tipoBusca;
        if (tipoBusca == 'telefone') {
            if ($('#termo').val().length <= 2) {
                tipoBuscaSelecionado = 'ddd';
            }
        }

        if (tipoBusca == 'endereco') {
            if ($("#termo").val().match(/[\d]{5}\-?[\d]{3}/)) {
                tipoBuscaSelecionado = 'cep';
            }
        }

        $("option[data-tipo=" + tipoBuscaSelecionado + "]").attr("selected", "selected");

        $("#termo").data('tipo', tipoBusca);

        if (tipoBusca == 'empresas') {
            $("#filtro_topo").find('li[data-tipo="empresas"]').addClass("selected");
        } else if (tipoBusca == 'pessoa' || tipoBusca == 'pessoas') {
            $("#filtro_topo").find('li[data-tipo="pessoas"]').addClass("selected");
        } else if (tipoBusca == 'endereco') {
            if ($("#termo").val().match(/[\d]{5}\-?[\d]{3}/)) {
                /*$("#filtro_topo").find('li[data-tipo="endereco"]').addClass("selected");*/
                $("#maisTexto").html('cep').parent().addClass("selected").data('tipo', 'cep');
                $("#cidade").val('');

            } else {
                $("#filtro_topo").find('li[data-tipo="endereco"]').addClass("selected");
            }
        } else if (tipoBusca == 'telefone') {
            if ($("#termo").val().match(/[\d]{2}/)) {
                $("#cidade").val("");
                $("#maisTexto").parent().addClass("selected");
                termo = $("#termo").val();

                if (termo.length == 2) {
                    $("#maisTexto").html('ddd').parent().data('tipo', 'ddd');
                } else if (termo.length >= 3) {
                    $("#maisTexto").html('telefones').parent().data('tipo', 'telefones');
                }

                /*$("#filtro_topo").find('li[data-tipo="telefone"]').addClass("selected");*/
            } else {
                $("#filtro_topo").find('li[data-tipo="telefone"]').addClass("selected");
            }
        } else {
            $("#filtro_topo").find('li[data-tipo="todos"]').addClass("selected");
            //$('#outrosTermos').css("border-top-left-radius", "0px");
            $('#outrosTermos').removeClass("borda-total");
            $('#outrosTermos').addClass("borda-parcial");
        }
        $("#filtro_topo").on('click', '.ultimo', function () {
            if ($(this).hasClass('aberto')) {
                $(this).html('+ filtros').removeClass('aberto');
                $("#mais_filtros").hide();
            } else {
                $(this).html('fechar').addClass('aberto');
                $("#mais_filtros").show();
            }
        });

        atualizarCidade($("#cidade").val());

        // Ao sair do input (cidade).
        $('#cidade').on("change focusout", function () {
            // alert('alterando cidade!');
            // Monta a cidade e a sigla do estado.
            var varSiglaEstado = siglaEstado(retornaEstado($('#cidade').attr("value")));
            var cidadeString = $('#cidade').attr("value");
            if (varSiglaEstado != '') {
                cidadeString = cidadeString + ", " + varSiglaEstado;
            }
            $('#desc-cidade, #desc-cidade2').html(cidadeString);
            var cidade = $('#cidade').attr("value").toLowerCase();
            montaLinks(cidade);
        });
    });

    // Função que retorna a sigla do estado.
    /*function siglaEstado(estado) {
     var sigla = '';
     switch (estado) {
     case (estado = 'santa-catarina'):
     sigla = 'SC';
     break;
     case (estado = 'parana'):
     sigla = 'PR';
     break;
     case (estado = 'rio-grande-do-sul'):
     sigla = 'RS';
     break;
     default:
     sigla = 'SC';
     }
     return sigla;
     }*/

    function atualizarCidade(cidade) {
        var varSiglaEstado = siglaEstado(retornaEstado($('#cidade').attr("value")));
        if (varSiglaEstado != '') {
            varSiglaEstado = ", " + varSiglaEstado;
        }

        $('#desc-cidade, #desc-cidade2').html($('#cidade').attr("value") + varSiglaEstado);
        var cidade = $('#cidade').attr("value").toLowerCase();
        montaLinks(cidade);
    }

    // Função que monta os links para a busca rápida.
    function montaLinks(cidade) {
        if (cidade == "") {
            cidade = "todos estados";
        }
        if (cidade.indexOf(',') > -1) {
            var result = cidade.split(',');
            estado = estenderEstado(result[1].trim());
            if (estado === false)
                estado = 'todos estados';
            cidade = result[0].trim();
        } else {
            if (cidade === 'são paulo'){
                cidade = 'todas cidades';
                estado = 'sao paulo';
            }else if(cidade === 'rio de janeiro'){
                cidade = 'todas cidades';
                estado = 'rio de janeiro';
            }else{
                estado = retornaEstado(cidade);
            }
        }
        cidade = cidade.replace(/\-/g, '-').replace(/\s/g, '-');
        estado = estado.replace(/\-/g, '-').replace(/\s/g, '-');

        if (estado === cidade) {
            cidade = 'todas-cidades';
        }

        // Monta links da div Alimentação.
        $("#div-alimentacao .texto a").each(function (index) {
            var arrayAlimentacao = [];
            arrayAlimentacao[0] = "/titulo/pizzarias";
            arrayAlimentacao[1] = "/titulo/restaurantes";
            arrayAlimentacao[2] = "/busca/lanches";
            arrayAlimentacao[3] = "/busca/churrascaria";

            var link = arrayAlimentacao[index];
            $(this).attr('href', link + '/' + cidade + '/' + estado + "/");
        });

        // Monta links da div Automóveis.
        $("#div-automoveis .texto a").each(function (index) {
            var arrayAutomoveis = [];
            arrayAutomoveis[0] = "/busca/concessionarias";
            arrayAutomoveis[1] = "/busca/oficinas";
            arrayAutomoveis[2] = "/busca/autopecas";
            arrayAutomoveis[3] = "/busca/motos";

            var link = arrayAutomoveis[index];
            $(this).attr('href', link + '/' + cidade + '/' + estado + "/");
        });

        // Monta links da div Casa.
        $("#div-casa .texto a").each(function (index) {
            var arrayCasa = [];
            arrayCasa[0] = "/titulo/materiais-de-construcao";
            arrayCasa[1] = "/busca/moveis-sob-medida";
            arrayCasa[2] = "/titulo/arquitetos";
            arrayCasa[3] = "/titulo/marmores-e-granitos";

            var link = arrayCasa[index];
            $(this).attr('href', link + '/' + cidade + '/' + estado + "/");
        });

        // Monta links da div Saúde.
        $("#div-saude .texto a").each(function (index) {
            var arraySaude = [];
            arraySaude[0] = "/titulo/clinica-de-estetica";
            arraySaude[1] = "/busca/farmacias";
            arraySaude[2] = "/titulo/hospitais";
            arraySaude[3] = "/titulo/dentistas";

            var link = arraySaude[index];
            $(this).attr('href', link + '/' + cidade + '/' + estado + "/");
        });

        // Monta links da div Serviços Gerais.
        $("#div-servicos-gerais .texto a").each(function (index) {
            var arrayServicos = [];
            arrayServicos[0] = "/busca/eletricistas";
            arrayServicos[1] = "/titulo/serralherias-e-serralheiros";
            arrayServicos[2] = "/titulo/fretes";
            arrayServicos[3] = "/titulo/informatica";

            var link = arrayServicos[index];
            $(this).attr('href', link + '/' + cidade + '/' + estado + "/");
        });

        // Monta links da div Locais Diversos.
        $("#div-locais-diversos .texto a").each(function (index) {
            var arrayLocais = [];
            arrayLocais[0] = "/titulo/supermercados";
            arrayLocais[1] = "/titulo/pet-shop";
            arrayLocais[2] = "/titulo/papelarias";
            arrayLocais[3] = "/titulo/imobiliarias";

            var link = arrayLocais[index];
            $(this).attr('href', link + '/' + cidade + '/' + estado + "/");
        });
    }
</script>
