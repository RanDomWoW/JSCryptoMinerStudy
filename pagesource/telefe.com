irmak, Saygin Soysal, Burak Tamdogan, Tuvana Türkay y un gran elenco.","body":null,"airingDate":null,"publicationDate":null,"availableDate":null,"expirationDate":null,"seasonNumber":0,"episodeNumber":0,"duration":0.0,"extras":{},"parents":[],"images":[{"id":603258,"title":"master-thumb.jpg","url":"/media/18146948/master-thumb.jpg","type":["Logo 16:9"],"width":720,"height":406},{"id":611962,"title":"Hero-home-big (2).jpg","url":"/media/18151738/hero-home-big-2.jpg","type":["Fondo Desktop Home Hero"],"width":1920,"height":450},{"id":611963,"title":"NUEVO-Hero-home-small (2).jpg","url":"/media/18151740/nuevo-hero-home-small-2.jpg","type":["Fondo Mobile Home Hero"],"width":1920,"height":480},{"id":611961,"title":"poster-master-final (2).jpg","url":"/media/18151739/poster-master-final-2.jpg","type":["Poster"],"width":566,"height":720},{"id":611965,"title":"hero-master-micrositio-.jpg","url":"/media/18151742/hero-master-micrositio.jpg","type":["Fondo Landing Hero"],"width":1920,"height":240}],"videos":[]},{"id":597009,"permalink":"/primera-cita/","title":"Primera Cita","description":null,"body":null,"airingDate":null,"publicationDate":null,"availableDate":null,"expirationDate":null,"seasonNumber":0,"episodeNumber":0,"duration":0.0,"extras":{},"parents":[],"images":[{"id":598241,"title":"poster-master-final-Primera-Cita.jpg","url":"/media/18144234/poster-master-final-primera-cita.jpg","type":["Poster"],"width":566,"height":720},{"id":598240,"title":"NUEVO-Hero-home-small-Primera-Cita.jpg","url":"/media/18144233/nuevo-hero-home-small-primera-cita.jpg","type":["Fondo Mobile Home Hero"],"width":1920,"height":480},{"id":598226,"title":"Hero-home-big-Primera-Cita.jpg","url":"/media/18144224/hero-home-big-primera-cita.jpg","type":["Fondo Desktop Home Hero"],"width":1920,"height":450},{"id":598239,"title":"master-thumb-Primera-Cita.jpg","url":"/media/18144232/master-thumb-primera-cita.jpg","type":["Logo 16:9"],"width":720,"height":406},{"id":598238,"title":"hero-master-micrositio