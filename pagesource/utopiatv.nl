	<script type="text/html" id="video-timeline">
		<section id="timeline" class="site-frame section" data-bind="component: {component: 'timeline'}">
		
			<!-- ko if: dataManager.phase > 1 -->
			<form class="timeline-filter">
				<div class="filters">
					<span class="btn-tooltip" data-bind="css: {active: term() == 'clip'}, click: controller.switchToFilter.bind(controller, 'overview', 'clip', '')"><!-- ko localizedText: 'pages.video.filter-clips' -->&nbsp;<!-- /ko --><span class="arrow"></span></span>
					<span class="btn-tooltip" data-bind="css: {active: term() == 'catchup'}, click: controller.switchToFilter.bind(controller, 'overview', 'catchup', ''), visible: dataManager.config.video.catchup.enabled"><!-- ko localizedText: 'pages.video.filter-catchup' -->&nbsp;<!-- /ko --><span class="arrow"></span></span>
					<span class="btn-tooltip" data-bind="css: {active: term() == 'playlist'}, click: controller.switchToFilter.bind(controller, 'overview', 'playlist', '')"><!-- ko localizedText: 'pages.video.filter-playlist' -->&nbsp;<!-- /ko --><span class="arrow"></span></span>
		
					<!-- ko if: term() == 'clip' -->
						<!-- ko template: {name: 'calendar'} --> <!-- /ko -->
					<!-- /ko -->
				</div>
		
				<fieldset class="search-field">
					<div data-bind="component: {component: 'auto-complete', template: false}">
						<input type="text" autocomplete="off" data-bind="value: value, valueUpdate: 'afterkeydown', localizedText: {id: 'timeline.search-placeholder', attr: 'placeholder'}" name="search" placeholder="" />
						<ul class="auto-complete-list" data-bind="foreach: tagsFound">
							<li data-bind="
									click: $root.controller.onTagSelect.bind($root.controller),
									text: title
								"></li>
						</ul>
					</div>
					<span class="btn-tooltip" data-bind="css: {active: filter() == 'tag' || filter() == 'search', disabled: !canSearch()}, click: controller.onSearchClick.bind(controller)"><!-- ko localizedText: 'timeline.search-cta' --><!-- /ko --><span class="arrow"></span></span>
				</fieldset>
		
				<fieldset class="filter-field" style="display: none;">
					<span class="label" data-bind="localizedText: 'timeline.sort'"></span>
		
					<div class="drop-down" data-dropdown>
						<span class="value" data-bind="localizedText: 'timeline.time'"></span>
						<span class="icon-arrow-down"></span>
		
						<div class="content-holder">
							<ul class="list">
								<li data-value="time" data-bind="localizedText: 'timeline.sort-time'"></li>
								<li data-value="other" data-bind="localizedText: 'timeline.sort-other'"></li>
							</ul>
						</div>
					</div>
				</fieldset>
			</form>
			<!-- /ko -->
		
			<div class="timeline-feed">
		
				<!-- ko if: filter() == 'tag' || filter() == 'search' -->
				<div class="timeline-search-result">
					<!-- ko localizedText: 'timeline.search-result' --><!-- /ko --> <span class="search-key" data-bind="text: term()"></span>
					<span class="btn-close" data-bind="click: controller.switchToFilter.bind(controller, 'overview', 'clip', '')">x</span>
				</div>
				<!-- /ko -->
		
				<!-- ko if: dataManager.phase > 1 -->
				<div data-bind="visible: filter() == 'overview' && term() == 'clip'">
					<div class="timeline-days days" data-bind="component: {component: 'timeline-days'}">
						<span class="prev icon-arrow-left" data-bind="click: controller.onPrevClick.bind(controller), visible: canGoBack()"></span>
						<span class="next icon-arrow-right" data-bind="click: controller.onNextClick.bind(controller), visible: canGoForward()"></span>
						<div class="slider" data-carousel-wrapper data-bind="foreach: items">
							<div class="tab item" data-bind="css: {active: $root.selectedDay() == $index()}, attr: {'data-index': $index()}">
								<span class="day" data-bind="localizedText: 'general.day'"></span>
								<span class="index" data-bind="text: $root.getDay(date)"></span>
								<span class="date" data-bind="text: $root.getDate(date)"></span>
								<span class="icon" data-bind="css: 'icon-wheater-' + weather.code() + (weather.code() > 100 ? '-' + weather.period() : '')"></span>
							</div>
						</div>
					</div>
				</div>
		
				<div data-bind="visible: filter() == 'overview' && term() == 'catchup'">
					<div class="timeline-days months" data-bind="component: {component: 'timeline-days'}">
						<span class="prev icon-arrow-left" data-bind="click: controller.onPrevClick.bind(controller), visible: canGoBack()"></span>
						<span class="next icon-arrow-right" data-bind="click: controller.onNextClick.bind(controller), visible: canGoForward()"></span>
						<div class="slider" data-carousel-wrapper data-bind="foreach: items">
							<div class="tab item" data-bind="css: {active: $root.selectedDay() == $index()}, attr: {'data-index': $index()}">
								<span class="date" data-bind="text: moment(date).format('MMMM')"></span>
							</div>
						</div>
					</div>
				</div>
				<!-- /ko -->
		
				<!-- ko if: filteredItemsTemplate().length > 0 -->
		
					<!-- Repeatable row -->
					<!-- ko foreach: new Array(Math.ceil($root.filteredItems().length / 4)) -->
						<div class="grid-program-timeline-col1">
		
							<!-- ko with: $root.filteredItems()[$index() * 4 + 0] -->
							<div class="s1 x0 y0">
								<!-- ko template: {
								    name: $root.getMediaTemplate('media-clip', $data),
			                        data: $.extend({}, $data, {'lockedType': $root.itemAvailableFor($data)})
								} --><!-- /ko -->
							</div>
							<!-- /ko -->
		
							<!-- ko with: $root.filteredItems()[$index() * 4 + 1] -->
							<div class="s1 x1 y0">
								<!-- ko template: {
								    name: $root.getMediaTemplate('media-clip', $data),
			                        data: $.extend({}, $data, {'lockedType': $root.itemAvailableFor($data)})
								} --><!-- /ko -->
							</div>
							<!-- /ko -->
		
							<!-- ko with: $root.filteredItems()[$index() * 4 + 2] -->
							<div class="s1 x2 y0">
								<!-- ko template: {
								    name: $root.getMediaTemplate('media-clip', $data),
			                        data: $.extend({}, $data, {'lockedType': $root.itemAvailableFor($data)})
								} --><!-- /ko -->
							</div>
							<!-- /ko -->
		
							<!-- ko with: $root.filteredItems()[$index() * 4 + 3] -->
							<div class="s1 x3 y0">
								<!-- ko template: {
								    name: $root.getMediaTemplate('media-clip', $data),
			                        data: $.extend({}, $data, {'lockedType': $root.itemAvailableFor($data)})
								} --><!-- /ko -->
							</div>
							<!-- /ko -->
		
						</div>
					<!-- /ko -->
				<!-- /ko -->
		
				<!-- ko if: $root.filteredItems().length == 0 -->
					<div class="article-text first-child">
						<div class="article-text-inner">
							<!-- ko if: (filter() == 'tag' || filter() == 'search') -->
								<h2 class="heading-02" data-bind="localizedText: 'timeline.search-result-none'"></h2>
							<!-- /ko -->
							<!-- ko if: term() == 'playlist' -->
								<h2 class="heading-02" data-bind="localizedText: 'pages.video.no-playlists'"></h2>
							<!-- /ko -->
							<!-- ko if: (filter() != 'tag' && filter() != 'search' && term() != 'playlist') -->
								<h2 class="heading-02" data-bind="localizedText: 'pages.video.no-videos-for-day'"></h2>
							<!-- /ko -->
						</div>
					</div>
				<!-- /ko -->
		
				<span class="btn-loadmore" data-bind="visible: pagination() && pagination().hasNextPage, localizedText: 'timeline.load-more-videos', click: $root.controller.loadMore.bind($root.controller)"></span>
			</div>
		
		</section>
	</script>
	
	<script type="text/html" id="voting-verified-popup">
		<div class="utopia-popup voting-verified-popup" style="display: none;">
			<div class="blocker"></div>
			<div class="background"></div>
			<div class="header">
				<h2 data-bind="localizedText: 'pages.participant.voting.verify-popup.title'"></h2>
				<a class="close" href="#" data-bind="click: $data.onCancel"><span class="icon-close"></span></a>
			</div>
			<div class="content">
				<p data-bind="localizedText: {id: 'pages.participant.voting.verify-popup.body', html: true}"></p>
		
				<a class="button btn-maincolor" href="#" data-bind="click: $data.onAccept, localizedText: 'pages.participant.voting.verify-popup.accept-cta'"></a>
				<a class="button btn-grey" href="#" data-bind="click: $data.onCancel, localizedText: 'pages.participant.voting.verify-popup.cancel-cta'"></a>
			</div>
			<div class="content complete">
				<p data-bind="localizedText: {id: 'pages.passport.profile.verify.complete', html: true}"></p>
		
				<a class="button btn-grey" href="#" data-bind="click: $data.onCancel, localizedText: 'general.close'"></a>
			</div>
			<div class="content error">
				<p data-bind="localizedText: {id: 'pages.passport.profile.verify.error', html: true}"></p>
		
				<a class="button btn-grey" href="#" data-bind="click: $data.onCancel, localizedText: 'general.close'"></a>
			</div>
		</div>
	</script>
	
    
</body>
</html>