<script language="javascript">

//互动交流
function setTab(name,cursel,n){
for(i=1;i<=n;i++){
  var menu1=document.getElementById(name+i);
  var menu2=document.getElementById(name+i+i);
  var menu3=document.getElementById(name+i+i+i);
  var con=document.getElementById("con_"+name+"_"+i);
  var more=document.getElementById("more_"+name+"_"+i);
  var more1=document.getElementById("a_"+name+"_"+i);
  menu1.className=i==cursel?"css2":"css1";
  menu2.className=i==cursel?"css22":"css11";
  menu3.className=i==cursel?"css222":"css111";
 more.style.display=i==cursel?"block":"none";
  more1.style.display=i==cursel?"block":"none";
  con.style.display=i==cursel?"block":"none";
}
}
//要闻、信息公开切换
function setTOP(name,cursel,n){
for(i=1;i<=n;i++){
  var menu=document.getElementById(name+i);
  var con=document.getElementById("con_"+name+"_"+i);
  var more=document.getElementById("more_"+name+"_"+i);
  menu.className=i==cursel? name+"css1":name+"css2";
  con.style.display=i==cursel?"block":"none";
  more.style.display=i==cursel?"block":"none";
}
}

//------
var menu = new Array;
        menu[1] = "menu1";
        menu[2] = "menu2";
		menu[3] = "menu3";
		menu[4] = "menu4";
		menu[5] = "menu5";
		menu[6] = "menu6";
function closeMenu()
{
        for (i=1;i<=15;i++)                
        {                    
                mObj = eval(menu[i]);                                        
                mObj.style.visibility="hidden";                
        }
}
function showMenu(n)
{        
        var obj = eval(menu[n]);                
        if(obj.style.visibility=="visible")                 
        return;                
        obj.style.visibility="visible";                
        for (i=1;i<=15;i++)        
        {                    
                mObj = eval(menu[i]);                                        
                if (i!=n)                                         
                mObj.style.visibility="hidden";                                
        }
}
function hideMenu(n) 
{                
        var obj = eval(menu[n]);                        
        obj.style.visibility="hidden";
}
function setwhite(obj)
{        
        obj.style.backgroundColor="";
}
function setnull(obj)
{
        obj.style.backgroundColor="";
}
</script>

