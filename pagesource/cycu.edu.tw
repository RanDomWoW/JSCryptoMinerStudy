    <script>
        var carouselBackwardFocus = 0;
        var carouselIndex = 0;
        var carouselCount = 0;
        var carouselBackwardFocus_news = 0;
        var carouselIndex_news = 0;
        var carouselCount_news = 0;
        $(function () {
            carouselCount = $('#carousel .item').length;
            carouselCount_news = $('#carousel_news .item').length;
            if ($(window).width() < 768) {
                $("#carousel").attr("data-ride", "carousel").removeClass("visor-carousel").addClass("carousel");
                $("#carousel").carousel({
                    swipe: 30
                });

            } else {
                $("#tab5").on("focus", function () {
                    carouselBackwardFocus = 0;
                });
                $("#carousel .item").on("focus", function () {
                    //console.log("focus");
                    //setTimeout(function () {
                    //    $('#carousel').carousel('pause');
                    //}, 500);
                    $('#carousel').carousel('pause');
                    if (carouselBackwardFocus != 2) {
                        if (carouselBackwardFocus == 0) {
                            carouselBackwardFocus = 2;
                            $('#carousel').carousel(0);
                        } else if (carouselBackwardFocus == 1) {
                            carouselBackwardFocus = 2;
                            $('#carousel').carousel(carouselCount - 1);

                        }
                    }
                }).on("focusout", function () {
                    //setTimeout(function () {
                    //    $('#carousel').carousel('cycle');
                    //}, 500);
                    //$('#carousel').carousel('cycle');
                    }).on("keydown", function (e) {
                        var keyCode = e.keyCode || e.which;

                        if (keyCode == 9) {
                            if (e.shiftKey) {
                                //Focus previous input
                                //console.log("Focus previous input");
                                if (carouselIndex - 1 == -1) {
                                    $("#tab5").focus();
                                } else {
                                    $('#carousel').carousel(carouselIndex - 1);
                                }
                            }
                            else {
                                //Focus next input
                                //console.log("Focus next input");
                                if (carouselIndex + 1 >= carouselCount) {
                                    $("#newsaccesskey").focus();
                                } else {
                                    $('#carousel').carousel(carouselIndex + 1);
                                }
                            }
                            e.preventDefault();

                        }
                    });
                $("#news").on("focus click", function () {
                    carouselBackwardFocus = 1;
                });
                $("#newsaccesskey").on("focus click", function () {
                    carouselBackwardFocus = 1;
                }).on("keydown", function (e) {
                    var keyCode = e.keyCode || e.which;

                    if (keyCode == 9) {
                        if (e.shiftKey) {
                            //Focus previous input
                            //console.log("Focus previous input");
                            $('#carousel').carousel(carouselCount - 1);
                            $("#carousel .item:last-child").focus();
                            e.preventDefault();
                        }
                        

                    }
                });
                $('#carousel').carousel({
                    interval: 5000

                }).on('slid.bs.carousel', function (evt) {
                    carouselIndex = $(this).find('.active').index();
                    //console.log('current slide = ', $(this).find('.active').index());
                    if (carouselBackwardFocus == 2) {
                        $(this).find('.active').focus();
                    }
                    //console.log(direction);
                    });

                $("#news_more").on("focus", function () {
                    carouselBackwardFocus_news = 0;
                });
                $("#carousel_news_prev").on("focus", function (e) {
                    $('#carousel_news').carousel('pause');
                    //console.log("carousel_news_prev : " + carouselBackwardFocus_news);
                    $(this).blur();
                    if (carouselBackwardFocus_news == 2) {
                        carouselBackwardFocus_news = 1;
                        setTimeout(function () {
                            $("#board_more").focus();
                        }, 300);
                    } else {
                       // $('#carousel_news').carousel(carouselCount_news - 1);
                       // $("#carousel_news .listitem:last-child").focus();
                    }
                });
                $("#carousel_news_next").on("focus", function (e) {
                    $('#carousel_news').carousel('pause');
                    //console.log("carousel_news_next : " + carouselBackwardFocus_news);
                    $(this).blur();
                    if (carouselBackwardFocus_news == 2) {
                        carouselBackwardFocus_news = 1;
                        setTimeout(function () {
                            $("#board_more").focus();
                        }, 300);
                    } else {
                        //$('#carousel_news').carousel(carouselCount_news - 1);
                        //$("#carousel_news .listitem:last-child").focus();
                    }
                    
                });
                $("#board").on("focus click", function () {
                    carouselBackwardFocus_news = 1;

                });
                $("#board_more").on("focus click", function () {
                    carouselBackwardFocus_news = 1;
                }).on("keydown", function (e) {
                    var keyCode = e.keyCode || e.which;

                    if (keyCode == 9) {
                        if (e.shiftKey) {
                            //Focus previous input
                            //console.log("Focus previous input:" + carouselCount_news);
                            $('#carousel_news').carousel('pause');
                            $('#carousel_news').carousel(carouselCount_news - 1);
                            setTimeout(function () {
                                $('#carousel_news').carousel('pause');
                                $("#carousel_news .listitem:last").focus();
                            },500);
                            e.preventDefault();
                        }


                    }
                });;
                $('#carousel_news').carousel({
                    interval: 5000

                }).on('slid.bs.carousel', function (evt) {
                    carouselIndex_news = $(this).find('.active').index();
                    //console.log('news current slide = ', $(this).find('.active').index());
                    //$(this).find('.active').focus();
                    //console.log(direction);
                    });
                $("#carousel_news .listitem").on("focus", function () {

                    
                    //setTimeout(function () {
                    //    $('#carousel').carousel('pause');
                    //}, 500);
                    $('#carousel_news').carousel('pause');
                    if (carouselBackwardFocus_news != 2) {
                        if (carouselBackwardFocus_news == 0) {
                            carouselBackwardFocus_news = 2;
                            $('#carousel_news').carousel(0);
                            $("#carousel_news .listitem:first").focus();
                        } else if (carouselBackwardFocus_news == 1) {
                            carouselBackwardFocus_news = 2;
                            $('#carousel_news').carousel(carouselCount_news - 1);
                            $("#carousel_news .listitem:last").focus();

                        }
                    }
                }).on("focusout", function () {
                    //setTimeout(function () {
                    //    $('#carousel').carousel('cycle');
                    //}, 500);
                    //$('#carousel').carousel('cycle');
                }).on("keydown", function (e) {
                    var keyCode = e.keyCode || e.which;

                    if (keyCode == 9) {
                        if (e.shiftKey) {
                            //Focus previous input
                            //console.log("Focus previous input");
                            var index = $(this).index();
                            console.log("focus index :" + index);
                            if (index == 0) {
                                $('#carousel_news').carousel(carouselIndex_news - 1);
                            }
                        }
                        else {
                            //Focus next input
                            //console.log("Focus next input");
                            var index = $(this).index();
                            console.log("focus index :" + index);
                            console.log("focus :" + ((index + 1) % 4));
                            if ((index + 1) % 4 == 0) {
                                //switch next page
                                $('#carousel_news').carousel(carouselIndex_news + 1);
                            }
                        }
                        //e.preventDefault();

                    }
                });
            }
            if ($(window).width() > 768) {
                $(".scrollpane").jScrollPane();
            }
            $('#marquee').marquee({
                duration: 20000,
                gap: 50,
                duplicated: false,
                pauseOnHover: true
            });

        });

    </script>


