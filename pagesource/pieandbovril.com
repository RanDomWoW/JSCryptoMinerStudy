	<script type="text/javascript">

jQuery.noConflict();																	

jQuery(document).ready(function(){
	
	// Global variables
	var token = "281e84302fd49b980154578decfeb217";
	var site_path = "http://www.pieandbovril.com/";
	var moduleID = "1";
	var mwmod = "mwm276";
	var itemid = "101";
	var pageLimit = "12";
	var lastPage = "2";
	var endPage = parseInt(lastPage) + 2;
	var pagination = "3";
	var $container = jQuery("#mnwall_container_"+mwmod);
	var gridType = "6";
	gridType = parseInt(gridType);
	if (gridType == 99) {
		gridLayout = "vertical";
	} else {
		gridLayout = "packery";
	}                                
	var themeType = "1";
	var hoverBox = "0";
	var custom_scrollbar = "0";
	var loader = "images/mwloaders/loader70.gif";
	var fancybox = "1";
	// Fancybox
	if (fancybox == 1) {
		$container.find(".mnwall-item-fancy-icon").fancybox();
	}
	
	// Custom scrollbar
	if (custom_scrollbar == 1) {
		jQuery("#mnwall_container_"+mwmod+" .mnwall-item-inner").mCustomScrollbar();
	}
	
	// Colors
	var detailBoxBackground = "#1c2837";
	var detailBoxBackgroundOpacity = "0.50";
	
	// Hover effects
	var hoverBoxEffect = "2";
	var hoverBoxContentEffect = "1";
	if (hoverBoxContentEffect == 1) {
		var hoverContentEffectClass = 'hoverFadeIn';
	} else if (hoverBoxContentEffect == 2) {
		var hoverContentEffectClass = 'slideInLeft';
	} else if (hoverBoxContentEffect == 3) {
		var hoverContentEffectClass = 'slideInRight';
	} else if (hoverBoxContentEffect == 4) {
		var hoverContentEffectClass = 'slideInTop';
	} else if (hoverBoxContentEffect == 5) {
		var hoverContentEffectClass = 'slideInBottom';
	} else if (hoverBoxContentEffect == 6) {
		var hoverContentEffectClass = 'slideInMix1';	
	} else if (hoverBoxContentEffect == 7) {
		var hoverContentEffectClass = 'slideInMix2';	
	} else if (hoverBoxContentEffect == 8) {
		var hoverContentEffectClass = 'slideInMix3';	
	} else {
		var hoverContentEffectClass = 'no-effect';
	}
					
	// Initialize isotope	
	var $wall = jQuery("#mnwall_iso_container_"+mwmod).imagesLoaded( function() 
	{
		// Isotope
		$wall.isotope({
			// General
			itemSelector: ".mnwall-item",
			layoutMode: gridLayout,
			// Vertical list
			vertical: {
				horizontalAlignment: 0
			},
			getSortData: {
			  	title: "[data-title]",
				category: "[data-category]",
				author: "[data-author]",
				date: "[data-date]"
			},
			sortBy: "original-order"
		});
	});
	
	// Fix classes
	if (gridType != 99 && gridType != 98) 
	{
		// Add classes to items
		$container.find(".mnwall-item").each(function(index){
			mnw_mas_index = index + 1;
			if (mnw_mas_index > gridType) {
				mnw_mas_index = recurseMasItemIndex(mnw_mas_index, gridType);
			}
			jQuery(this).removeClass (function (class_index, css) {
				return (css.match (/(^|\s)mnwitem\S+/g) || []).join(" ");
			}).addClass("mnwitem"+mnw_mas_index);
		});
		// Re-initialize isotope layout
		//$wall.isotope();
	}
	
	// Hover box trigger
	if (hoverBox == "1") {
		triggerHoverBox(gridType, mwmod, hoverBoxEffect, hoverContentEffectClass);
		centerHoverContent($container, gridType);
	}
		
	// Add detail-box background-color
	backgroundColor($container, gridType, themeType, detailBoxBackground, detailBoxBackgroundOpacity);
		
	// Last page
	if (jQuery("#mnwall_container_"+mwmod+" .more-results.mnw-all").attr("data-page") == endPage) {
		jQuery("#mnwall_container_"+mwmod+" .more-results.mnw-all").addClass("disabled");
		jQuery("#mnwall_container_"+mwmod+" .more-results.mnw-all span.more-results").hide();
		jQuery("#mnwall_container_"+mwmod+" .more-results.mnw-all span.no-results").show();
		jQuery("#mnwall_container_"+mwmod+" .more-results.mnw-all img").hide();
	}
			
	// Filters
	var filters = {};
	jQuery("#mnwall_iso_filters_"+mwmod).on( "click", ".mnwall-filter", function(event) 
	{
		event.preventDefault();
		var $this = jQuery(this);
		// get group key
		var $buttonGroup = $this.parents(".button-group");
		var filterGroup = $buttonGroup.attr("data-filter-group");
		// set filter for group
		filters[ filterGroup ] = $this.attr("data-filter");
		// combine filters
		var filterValue = "";
		for ( var prop in filters ) {
			filterValue += filters[ prop ];
		}
		// set filter for Isotope
		$wall.isotope({ filter: filterValue });
		// Fix background image size
		if (gridType != 99 && gridType != 98) 
		{
			backgroundSize($container);
		}
		// Center hover content
		if (hoverBox == "1") {
			centerHoverContentDynamic($container, gridType);
		}
	});
	
	// Change active class on filter buttons
	active_Filters(mwmod);
	
	// Dropdown filter list
	dropdown_Filters(mwmod);
		
	// Bind sort button click
  	jQuery("#mnwall_container_"+mwmod+" .sorting-group-filters").on( "click", ".mnwall-filter", function(event) {
		event.preventDefault();
    	var sortValue = jQuery(this).attr("data-sort-value");
    	// set filter for Isotope
		$wall.isotope({ 
			sortBy: sortValue
		});
  	});
	
	// Change active class on sorting filters
	jQuery("#mnwall_container_"+mwmod+" .sorting-group-filters").each( function( i, sortingGroup ) {
		var $sortingGroup = jQuery( sortingGroup );
		$sortingGroup.on( "click", ".mnwall-filter", function() {
	  		$sortingGroup.find(".mnw_filter_active").removeClass("mnw_filter_active");
	  		jQuery( this ).addClass("mnw_filter_active");
		});
	});
	
	// Bind sorting direction button click
  	jQuery("#mnwall_container_"+mwmod+" .sorting-group-direction").on( "click", ".mnwall-filter", function(event) {
		event.preventDefault();
    	var sortDirection = jQuery(this).attr("data-sort-value");
		if (sortDirection == "asc") {
			sort_Direction = true;
		} else {
			sort_Direction = false;
		}
    	// set direction
		$wall.isotope({ 
			sortAscending: sort_Direction
		});
  	});
	
	// Change active class on sorting direction
	jQuery("#mnwall_container_"+mwmod+" .sorting-group-direction").each( function( i, sortingDirection ) {
		var $sortingDirection = jQuery( sortingDirection );
		$sortingDirection.on( "click", ".mnwall-filter", function() {
	  		$sortingDirection.find(".mnw_filter_active").removeClass("mnw_filter_active");
	  		jQuery( this ).addClass("mnw_filter_active");
		});
	});
	
	// Dropdown sorting list
	dropdown_Sortings(mwmod);
	
	// Fix background image size
	if (gridType != 99 && gridType != 98) 
	{
		backgroundSize($container);
	}
					  
	// Load more (Append) pagination
	$container.find(".more-results.mnw-all").on( "click", function(event)
	{
		event.preventDefault();
		
		if (jQuery(this).hasClass("disabled")) {
			return false;
		}
						
		// Find page
		var dataPage = jQuery(this).attr("data-page");
		page = parseInt(dataPage);
		new_page = page + 1;
		
		// Increment page in data-page
		jQuery(this).attr("data-page", new_page);
				
		appendWall(
			site_path,
			page,
			moduleID, 
			itemid, 
			token, 
			mwmod, 
			$wall, 
			gridType, 
			$container, 
			themeType, 
			detailBoxBackground, 
			detailBoxBackgroundOpacity, 
			hoverBox,
			hoverBoxEffect, 
			hoverContentEffectClass,
			pagination,
			endPage,
			fancybox
		);
		
	});
		
	// Previous arrow pagination
	$container.find(".mnwall_arrow_prev").on( "click", function(event)
	{
		event.preventDefault();
		
		var current = jQuery(this);
		
		if (jQuery(this).hasClass("disabled")) {
			return false;
		}
								
		// Find page
		var dataPage = jQuery(this).attr("data-page");
		page = parseInt(dataPage);
		new_page = page - 1;
		next_page = page + 1;
		
		// Decrease page in link id
		jQuery(this).attr("data-page", new_page);
		jQuery("#mnwall_container_"+mwmod+" .mnwall_arrow_next").attr("data-page", next_page);
		
		previousWall(
			site_path,
			page,
			moduleID, 
			itemid, 
			token, 
			current,
			loader, 
			$wall, 
			gridType, 
			$container, 
			themeType, 
			detailBoxBackground, 
			detailBoxBackgroundOpacity, 
			hoverBox,
			hoverBoxEffect, 
			hoverContentEffectClass,
			mwmod, 
			pagination,
			new_page,
			fancybox
		)
		
	});
	
	// Next arrow pagination
	$container.find(".mnwall_arrow_next").on( "click", function(event)
	{
		event.preventDefault();
		
		var current = jQuery(this);
		
		if (jQuery(this).hasClass("disabled")) {
			return false;
		}
								
		// Find page
		var dataPage = jQuery(this).attr("data-page");
		page = parseInt(dataPage);
		next_page = page + 1;
		prev_page = page - 1;
		end_page_next = next_page - 1;
		end_page_prev = next_page - 3;
		
		// Increment page in link id
		jQuery(this).attr("data-page", next_page);
		jQuery("#mnwall_container_"+mwmod+" .mnwall_arrow_prev").attr("data-page", prev_page);
						
		nextWall(
			site_path,
			page,
			moduleID, 
			itemid, 
			token, 
			current,
			loader, 
			$wall, 
			gridType, 
			$container, 
			themeType, 
			detailBoxBackground, 
			detailBoxBackgroundOpacity, 
			hoverBox,
			hoverBoxEffect, 
			hoverContentEffectClass,
			mwmod,
			pagination,
			endPage,
			end_page_prev,
			end_page_next,
			fancybox
		)
		
	});
	
	// Pages pagination
	$container.find(".mnwall_page").on( "click", function(event)
	{
		var current = jQuery(this);
		jQuery("#mnwall_container_"+mwmod+" .mnwall_page").removeClass("mnw_active");
		
		event.preventDefault();
								
		// Find page
		var dataPage = jQuery(this).attr("data-page");
		page = parseInt(dataPage);
						
		pageWall(
			site_path,
			page,
			moduleID, 
			itemid, 
			token, 
			current,
			loader, 
			$wall, 
			gridType, 
			$container, 
			themeType, 
			detailBoxBackground, 
			detailBoxBackgroundOpacity, 
			mwmod,
			hoverBox,
			hoverBoxEffect, 
			hoverContentEffectClass,
			pagination,
			fancybox
		)
		
	});
	
	jQuery(window).resize(function() {
		
		// Center hover content
		if (hoverBox == "1") {
			centerHoverContentDynamic($container, gridType);
		}
				
		// Fix background image size
		if (gridType != 99 && gridType != 98) 
		{
			backgroundSize($container);
		}
		
	});
			
});		

	</script>
