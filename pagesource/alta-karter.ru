    <script>
    var hideItems = function () {
        $('#menu .accordion').removeClass('expanded');
        $('#menu .accordion-collapse').hide();
    };
    $(document).on('click', '.accordion-header', function () {
        var $this = $(this);
        var expanded = $this.parent('.accordion').hasClass('expanded');
        hideItems();
        if (!expanded) {
            $('#overlay').addClass('active');
            $this.parent('.accordion').addClass('expanded');
            $this.next('.accordion-collapse').show();
            $('.nav-back').show();
        }
        return false;
    });

    $(document).on('click', '.nav-back', function () {
        $('#menu .accordion').removeClass('expanded');
        $('#menu .accordion-collapse').hide();
        $('.nav-back').hide();
    });
    $(document).on('click', '#menuLink', function () {
        if ($(this).hasClass('active')) {
            hideItems();
            $('#layout, #menu, #menuLink, #overlay').removeClass('active color');
        } else {
            $('#layout, #menu, #menuLink, #overlay').addClass('active');
            $('#overlay').addClass('color');
        }
        return false;
    });
    $(document).on('click', '#overlay', function () {
        hideItems();
        $('#layout, #menu, #menuLink, #overlay').removeClass('active color');
        return false;
    });
</script>
