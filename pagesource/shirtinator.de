					    localization:  {"BASKET_SIZE_LABEL":"Gr\u00f6\u00dfe","BASKET_COLOR_LABEL":"Farbe","BASKET_EDIT_BUTTON_TEXT":"Bearbeiten","BASKET_DISCOUNT_TEXT":"Originalpreis","BASKET_PRODUCT":"Artikel","BASKET_DELETE_BUTTON_TEXT":"L\u00f6schen","close":"close","PRICE_INCL_VAT_EXCL_SHIPPING":"* inkl. gesetzl. Mwst. und zzgl. <a>Service & Versandkosten<\/a>","CLOSE":"Schlie\u00dfen","BASKET_TITLE":"Warenkorb","BASKET_KASSE":"Zum Warenkorb","BASKET_ITEMS":"||||||Artikel","MAX_AMOUNT_PRODUCT":"Leider haben wir dieses Produkt nur noch %X mal auf Lager."},
					    priceFormat:  {"decimal_point":",","thousands_sep":".","int_curr_symbol":"EUR ","currency_symbol":"\u20ac","mon_decimal_point":",","mon_thousands_sep":".","positive_sign":"","negative_sign":"-","int_frac_digits":2,"frac_digits":2,"p_cs_precedes":0,"p_sep_by_space":1,"n_cs_precedes":0,"n_sep_by_space":1,"p_sign_posn":1,"n_sign_posn":1,"grouping":[3,3],"mon_grouping":[3,3]}
					});
					
					shrBasket.addEvent(shrBasket.EVENT.INIT, function(evt, data) {
						// first load
						
					});
					
					
					shrBasket.addEvent(shrBasket.EVENT.ADD_PRODUCT, function(evt, data, oldData, callFrom) {
						if (data.createdShopProducts instanceof Array){
							shrBasket.openBasket(true, true);
							setTimeout(function() {
								shrBasket.closeBasket();
							}, showDelay);
						}
						// no push to gtm if is add to cart form Ready to use product : RU or Design template : T because they have implemented self functions for addProduct
						if (callFrom != 'RU' && callFrom != 'T') {
							shrGtm.basketUpdated(oldData, data.shopOrderValue);
						}
						$('.ps-container', shrBasket).scrollTop($('.ps-container', shrBasket)[0].scrollHeight);
					});
					
					shrBasket.addEvent(shrBasket.EVENT.UPDATE_PRODUCT, function(evt, data, oldData) {
						if (data.createdShopProducts instanceof Array){
							shrBasket.openBasket(true, true);
							setTimeout(function() {
								shrBasket.closeBasket();
							}, showDelay);
						}
						shrGtm.basketUpdated(oldData, data.shopOrderValue);
						$('.ps-container', shrBasket).scrollTop($('.ps-container', shrBasket)[0].scrollHeight);
					});
					    
					shrBasket.addEvent(shrBasket.EVENT.REMOVE_PRODUCT, function(evt, data, oldData) {
						shrGtm.basketUpdated(oldData, data.shopOrderValue);
					});

					shrBasket.addEvent(shrBasket.EVENT.UPDATE_BASKET, function(evt, data, oldData) {
						if (typeof data == 'object'){
							shrBasket.openBasket(true, true);
							setTimeout(function() {
								shrBasket.closeBasket();
							}, showDelay);
						}
						
						// the list of added product from sis page or PC shop
						var productsAdded = sis_products ? sis_products.lastAddedProducts : [];

						shrGtm.basketUpdated(oldData, data.shopOrderValue);
						$('.ps-container', shrBasket).scrollTop($('.ps-container', shrBasket)[0].scrollHeight);
					});
					
					shrBasket.addEvent(shrBasket.EVENT.UPDATE_AMOUNT, function(evt, shopOrderValue, shopProductId, reservations, removedProducts, oldData) {
						shrGtm.basketUpdated(oldData, shopOrderValue);
						var prodId = $('#productId').val();
						var colId = $('#colorId').val();
						if(ShrCreator.product && ShrCreator.product.data){
							prodId = ShrCreator.product.data.baseProductId || ShrCreator.product.data.id;
							colId = ShrCreator.product.activeColor;
						}
						var res = reservations.filter(function(item){
							return item.productId == prodId && item.colorId == colId;
						});
						if(res.length){

							if(window.sis_products!==undefined){
								ShrCreator.DataModel.Reservation.get([{productId:prodId}],function(r){
									var result = r.result;
									$('.textBox').each(function(resIndex,resItem){
										var itemSizeId = resItem.id.split('_')[1];
										var reservedAmount = result.filter(function(item){
											return item.colorId == colId && item.sizeId == itemSizeId;
										});
										if(reservedAmount.length){
											resItem.value = +reservedAmount[0].amount;
											$(resItem).data({
											'baseId': reservedAmount[0].productionBaseId,
											'sizeId': reservedAmount[0].sizeId
											});
										}else{
											resItem.value = 0;
										}
									});
									sis_products.updateAmountPickerValues();
								});
							}else if(ShrCreator.product && ShrCreator.product.data){

								ShrCreator.DataModel.Reservation.get([{productId:(ShrCreator.product.data.baseProductId || ShrCreator.product.data.id)}], function(response){
									var reservedItems = response.result;
									ShrCreator.product.selectedSizes = [];

									$('.row input[sizeid]').each(function(sizeIndex, sizeItem){
										var sizeElm = $(sizeItem);
										var itemSizeId = sizeElm.attr('sizeid');
										var reservedAmount = reservedItems.filter(function(item){
											return item.colorId == colId && item.sizeId == itemSizeId;
										});
										if(reservedAmount.length > 0){
											ShrCreator.product.selectedSizes.push({
												sizeId: reservedAmount[0].sizeId,
												amount: reservedAmount[0].amount,
												discontinued: true
											});
											sizeElm.val(reservedAmount[0].amount);
										}

									});
									ShrCreator.creator.sizeAmountControl.update();
								});
							}
						}
					});
					
					$(shrBasket).data('getSelf', function() {
					    return shrBasket;
					});
//				    });
				
				</script>
