        var response = $('#ga_feedback_no').val() ? $('#ga_feedback_no').val() : $('#ga_feedback_yes').val();

        if (response.length > 2) {
            google({
                userFeedback: response,
                event: "userFeedback"
            });
            epi({
                positive: $('#ga_feedback_no').val() ? false : true,
                response: response,
                stage: 2
            });
        }
    }
    function google(form) {
        dataLayer.push(form);
    }
    function epi(form) {
        var data = {
            PagePath: '/',
            Positive: form.positive,
            Response: form.response,
            Stage: form.stage
        };

        $.ajax({
            type: "POST",
            url: "/Services/SiteAnalyticsService.ashx",
            data: JSON.stringify(data),
            contentType: "application/json; charset=utf-8"
        });
