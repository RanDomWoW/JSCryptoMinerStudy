    function CheckLangAndRedirect() {
        if (document.cookie.indexOf('navLang=') != -1) {
            var Navlang = getCookie("navLang");
            if (!window.location.href.includes(Navlang)) {
                window.location = "https://" + Navlang + ".sita.aero";
                console.log(Navlang);
            }
        }
