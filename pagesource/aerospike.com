    if (closestUL.hasClass('small-block-grid-1') && closestUL.hasClass('medium-block-grid-3') && closestUL.hasClass('large-block-grid-3')) {
      $('.success-story-content').css('min-height','450px');
      $('li.mix').css('clear','right');
    } else  {
      $('.success-story-content').css('min-height','initial');
      $('li.mix').css('clear','none');
    }
  } else {
    $('.success-story-content').css('min-height','initial');
