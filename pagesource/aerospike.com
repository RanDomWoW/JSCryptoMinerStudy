//If User clicks on a link in the menu that has a hash link.
if(window.location.hash) {
  // Fragment exists
  $('.dropdown-content .active').removeClass('active');
  var hash = window.location.hash;
  $( "a[href='/technologies/"+hash+"']" ).addClass('active');
  $( "a[href='/technologies/"+hash+"']" ).parent('li').addClass('active');
  $( "a[href='/products/"+hash+"']" ).addClass('active');
  $( "a[href='/products/"+hash+"']" ).parent('li').addClass('active');
  $( "a[href='/services/"+hash+"']" ).addClass('active');
  $( "a[href='/services/"+hash+"']" ).parent('li').addClass('active');
  $( "a[href='/company/"+hash+"']" ).addClass('active');
  $( "a[href='/company/"+hash+"']" ).parent('li').addClass('active');

  if($( ".tab-title a[href='"+hash+"']")){
    $( ".tab-title a[href='"+hash+"']").click();
    if($('#menu-products-products .active')){
      //$('.aerospike-services').scrollView();
      $( hash+" section" ).scrollView();
    }
    if($('#menu-services-support .active').length > 0){
      //$('.aerospike-support').scrollView();
      $( "a[href='"+hash+"']" ).closest('section').scrollView();
    }
  }
}

//If a user clicks on a link that has a hash while being on a hash page.
window.onhashchange = function() {
  // console.log('fired hash function');
  $('.dropdown-content .active').removeClass('active');
  var hash = window.location.hash;
  $( "a[href='/technologies/"+hash+"']" ).addClass('active');
  $( "a[href='/technologies/"+hash+"']" ).parent('li').addClass('active');
  $( "a[href='/products/"+hash+"']" ).addClass('active');
