    } else  {
      $('.success-story-content').css('min-height','initial');
      $('li.mix').css('clear','none');
    }
  } else {
    $('.success-story-content').css('min-height','initial');
    $('li.mix').css('clear','none');
  }
}

if(window.location.pathname == "/technologies/" || window.location.pathname == "/services/" || window.location.pathname == "/company/" || window.location.pathname == "/products/"){
