  $( "a[href='/company/"+hash+"']" ).addClass('active');
  $( "a[href='/company/"+hash+"']" ).parent('li').addClass('active');

  if($( ".tab-title a[href='"+hash+"']")){
    $( ".tab-title a[href='"+hash+"']").click();
    if($('#menu-products-products .active')){
      //$('.aerospike-services').scrollView();
      $( hash+" section" ).scrollView();
    }
    if($('#menu-services-support .active').length > 0){
      //$('.aerospike-support').scrollView();
      $( "a[href='"+hash+"']" ).closest('section').scrollView();
    }
  }

  //Fix for resource-center
  if(hash == '#reports' || hash == '#benchmarks' || hash == '#all' || hash == '#whitepapers'){
    // small-block-grid-1 medium-block-grid-3 large-block-grid-3
    var closestUL = $('.success-story-content').closest( "ul" );
