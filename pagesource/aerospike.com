$(document).ready(function(){

  //Active Top Level Links
  var current = window.location.pathname.slice(0,-1);
  $('a[href^="'+current+'"]').addClass('active');

  $('.search-launch').click(function(e){
      e.preventDefault();
      $(this).removeClass('white').css("color","#333");
      $('.search-container').fadeIn();
      if($('#top-search-input').hasClass('search-open')){
        $('.top-docs').css('visibility','initial');
        $('.top-community').css('visibility','initial');
        $('.search-container').fadeOut(function(){
          $('#top-search-input').removeClass('search-open');
        });
        $(this).css("color","#fff");
      } else {
        $('#top-search-input').addClass('search-open');
        $('.top-docs').css('visibility','hidden');
        $('.top-community').css('visibility','hidden');
      }

      // $('#top-search-input').bind("enterKey",function(e){
      //     var q = $(this).text();
      //     document.href= "/search?q="+q;
      // });

      $('#top-search-input').keyup(function(e){
        if(e.keyCode == 13)
        {
          var q = $(this).val();
          document.location= "/search?q="+q;
        }
      });

  });

});

$('.search-container').hide();

