  return this.each(function () {
    $('html, body').animate({
      scrollTop: $(this).offset().top
    }, 50);
  });
}

//Resoruce Center Hash
setTimeout(function(){
  var hash = window.location.hash;
  var linkx = hash.slice(1);
  $('.rc-'+ linkx).click();

  // if(hash == '#reports' || hash == '#benchmarks' || hash == '#all' || hash == '#whitepapers'){
