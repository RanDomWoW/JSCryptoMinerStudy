		// used this implementation to allow auto fill password by browser
		WatermarkPassword = function(input) {
			this.input = input;
			this.label = input.title;
			var watermarkPassword = this;
			if (this.input.value == ""){
				this.input.value = this.label;
				$(input).addClass("watermark");
			}else if (this.input.value == this.label) {
				$(input).addClass("watermark");
			}
			$(input).bind("focus", function(){watermarkPassword.onfocus()});
			$(input).bind("blur", function(){watermarkPassword.onblur()});
		}

		WatermarkPassword.prototype.onfocus = function() {
			if (this.input.value == this.label) {
				this.input.value = "";
				$(this.input).removeClass("watermark");
			}
		}

		WatermarkPassword.prototype.onblur = function() {
			if (this.input.value == "") {
				this.input.value = this.label;
				$(this.input).addClass("watermark");
			}
		}
		
		function asyncLogin(){
