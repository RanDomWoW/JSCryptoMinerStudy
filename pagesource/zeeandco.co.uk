<script type="text/javascript">
    jQuery(document).ready(function() {

        var blockPopup = false;

        jQuery('.ajax-popup-link').magnificPopup({
            type: 'ajax',
            tLoading: '<span class="spinner"></span><img class="ajax-loader" src="http://cdn.zeeandco.co.uk/skin/frontend/enterprise/zeeandco/images/ajax-loader-white.gif" alt="" /><p class="mfp-progress">Please wait...</p>'
        });


        if (typeof blockPopup != "undefined" && blockPopup == true) {
            return;
        }

        var expiration = new Date();
        var expiration = new Date( expiration.getTime() + (1000*60*60*24*365) );
        var popup = Mage.Cookies.get("zee_newsletter_popup");

        var cookieValue = "3";
        var showPopup = false;

        if (popup == "visited") {
            cookieValue = popup;
        } else if (popup != null) {
            var clicks = parseInt(popup);
            if (clicks == 1) {
                cookieValue = "visited";
                showPopup = true;
            } else if (clicks != NaN) {
                cookieValue = clicks - 1;
            }
        }

        if(window.location.hash === '#popup') {
            showPopup = true;
            cookieValue = "visited";
        }

        Mage.Cookies.set("zee_newsletter_popup", cookieValue, expiration, "/");

        if (showPopup) {
            setTimeout(function() {
                jQuery( ".ajax-popup-link" ).trigger( "click" );
            }, 2000);
        }
    });
</script>
