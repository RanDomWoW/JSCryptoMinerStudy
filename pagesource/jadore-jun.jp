<script type="text/javascript">
    var suggest_word = null;
    $(document).ready(function(){
        $('.suggest_key').autocomplete({
            source: function( req, res ) {
                $.ajax({
                    type: "GET",
                    url: "http://jun.search.zetacx.net/api/suggest/",
                    data: {
                        q_word: req.term,
                        limit:5,
                        mode:1
                    },
                    dataType: "json",
                    success: function(result){
                        res(result);
                    }
                });
            },
            search:function(e, ui){
                suggest_word = $(this).val();
            },
            open:function( e, ui ){
            	$('.ui-autocomplete').off('menufocus hover mouseover mouseenter');
                $(this).autocomplete('widget').css('z-index', 1001).css('text-align','left');
            },
            select:function( e, ui ){
                ga('send', 'event', 'サジェスト検索', '検索ワード', suggest_word+'|'+ui.item.value);
//                console.log('input : '+suggest_word);
//                console.log('suggest : '+ui.item.value);
            },
            autoFocus: false,
            delay: 1,
            minLength: 1
        });
    });
</script>
