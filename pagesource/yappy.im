    <script>
        var messageCount;
var refresh = 3.0;

var updateNum = function () {

    $.get("stats/messages", function (data) {

        if (messageCount == null) {
            var options = {
                useEasing: false,
                useGrouping: true,
                separator: ',',
                decimal: '.',
                prefix: '',
                suffix: ''
            };

            messageCount = new CountUp("numMessages", 0, data, 0, refresh, options);
            messageCount.start();
            window.setTimeout(updateNum, ((refresh * 1000) - 100));
        }
        else {
            messageCount.update(data);
            window.setTimeout(updateNum, ((refresh * 1000) - 100));
        }
    });
}

$(document).ready(function () {
    window.setTimeout(updateNum, 1000);
});

    </script>
