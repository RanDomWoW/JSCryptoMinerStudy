			$("#bottom_slider").data('owlCarousel').destroy(); // DESTROY SLIDE
		});
		
		$( "#main_slider" ).bind( "goToFirstSlide", function( e ) {
			var window_width = $(window).width();
			
			
			if(window_width < 995){
				clearInterval(slideAutoplay);
			}
			
			if( window_width < 640 ){
				main_owl.jumpTo(0);
				secondary_owl.jumpTo(0);
				
			}
			else{
				var i = 1;
			
				main_owl.jumpTo(0);
				secondary_owl.jumpTo(0);
				$("#secondary_slide").find(".slide_thumb").each(function(){
					if($(this).hasClass('active')){
						$(this).removeClass('active');
					}
					
					if(i == 1){
						$(this).addClass('active');
					}
					i++;
				});
			}
			
			current_slide = 1;
		});
		
		
		$("#secondary_slide a").click(function(){
		
			window.main_drag = false;
			
			setTimeout(function(){
				window.main_drag = true;
			},1000);
			
			var go_to = $(this).attr('go_to');
			
			var diff = go_to - current_slide;
			
			if(diff > 0){ 
				for (var i = 0; i < diff; i++){
					nextSlide();
				}		
			}
			else{ // BACKWARDS
				for (var i = 0; i > diff; i--){
					prevSlide();
				}		
			}
			
			
		});
		
		function prevSlide(swipe){
			
			var window_width = $(window).width();
			
			if(current_slide > 0){
				current_slide --;
			}
			else{
				current_slide = slides_count;
			}
			
			if(swipe != 'swipe'){
				main_owl.prev();
			}
			else{
				clearInterval(slideAutoplay);
			}
			
			$('.arrow_blocker').css('display','block');
				
			if( window_width > 639){
				if(current_slide != slides_count && current_slide<slides_count-1 && current_slide!=1){
					secondary_owl.prev();
				}
			}
			else{
				secondary_owl.prev();
			}
			
			var active_thumb = $('#secondary_slide a.active').removeClass('active');
			var last_thumb = $('#secondary_slide a').last();
			var active_parent = active_thumb.parent().parent();
			var prev_parent = active_parent.prev();		
			var new_active = prev_parent.find('a.slide_thumb');
				
			if(new_active.attr('class') == 'slide_thumb'){
				new_active.addClass('active');
			}
			else{
				last_thumb.addClass('active');
			}
			
			
			setTimeout(function(){
				$('.arrow_blocker').css('display','none');
			},600);
		}
		
		function nextSlide(swipe){
			
			var window_width = $(window).width();
			
			if(current_slide < slides_count){
				current_slide++;
				//alert('current slide increment');
			}
			else{
				current_slide = 1;
				secondary_owl.next();
			}
			
			
			if(swipe != 'swipe'){		
				main_owl.next();
			}
			else{
				clearInterval(slideAutoplay);
			}
			
			$('.arrow_blocker').css('display','block');
			
			//setTimeout(function(){
				//alert('current slide check');
				
				if( window_width > 639){
					if(current_slide > 2 && current_slide <  slides_count ){
						secondary_owl.next();
					}
				}
				else{
					secondary_owl.next();
				}
				
				var active_thumb = $('#secondary_slide a.active').removeClass('active');
				var first_thumb = $('#secondary_slide a').first();
				var active_parent = active_thumb.parent().parent();
				var next_parent = active_parent.next();		
				var new_active = next_parent.find('a.slide_thumb');
					
				if(new_active.attr('class') == 'slide_thumb'){
					new_active.addClass('active');
				}
				else{
					first_thumb.addClass('active');
				}
				
			//},250);
			
			
			setTimeout(function(){
				$('.arrow_blocker').css('display','none');
			},600);
			
			
		}
		
		function prevSlideMobile(){
		
			main_owl.prev();
			
		}
		
		function nextSlideMobile(){
			
			main_owl.next();
			
		}
		
		destroyBuildSecondarySlide();
		setTimeout(function(){alignHomeBoxes();},650);
	});
	
	function alignSecondarySlide(){
		
			//alert('alignSecondarySlide');
			
			var window_width = $(window).width();
			var main_slide_height = $('#main_slider').height();
			var secondary_slide_top = 0;
			
			if(window_width < 996 && window_width > 767){
				secondary_slide_top = $('#main_slider').height() - 70;			
				$('#secondary_slide').css('top',secondary_slide_top+'px');
			}
			
			if( window_width < 768 && window_width>639){
				secondary_slide_top = $('#main_slider').height() - 60;			
				$('#secondary_slide').css('top',secondary_slide_top+'px');
			}
			
			if(window_width > 996){
				$('#secondary_slide').css('top','388px');
			}
			
	}
	
	function alignHomeBoxes(){
		
		var window_width = $(window).width();
		var rightbox_height = $('.home_bottom .right').height();
		
		if( window_width > 639 && window_width < 996){
			$('.home_bottom .left').css('height',(rightbox_height+17)+'px');
		}
		
		if( window_width > 995 ){
			$('.home_bottom .left').css('height','220px');
		}
		
		if( window_width < 640 ){
			$('.home_bottom .left').css('height','170px;');
		}
	}
	
	function destroyBuildSecondarySlide(){
		var window_width = $(window).width();
		
		if(window_width < 640){
			$('#bottom_slider').trigger('destroySecondarySlide');
		}
		else{
			$('#bottom_slider').trigger('buildSecondarySlide');
		}
	}
	
	function goToFirstSlide(){
		$('#main_slider').trigger('goToFirstSlide');
	}

	
	var resizeTimeout;
	
	$( window ).resize(function() {
		var window_width = $(window).width();
	
		clearTimeout(resizeTimeout);
		resizeTimeout = setTimeout(function(){    
			
			setTimeout(function(){
				alignSecondarySlide();
			},500);
			
			alignHomeBoxes();
			destroyBuildSecondarySlide();
			goToFirstSlide(); // GO TO FIRST SLIDE ON MAIN AND SECONDARY SLIDE 
			
		}, 500);
	});
	
</script>


