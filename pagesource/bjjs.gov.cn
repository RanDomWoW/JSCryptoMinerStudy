function cIpt(ipt,ipt_class){
	var iptI = document.getElementById(ipt);
	var iptT = iptI.value;
	iptI.onfocus = function(){
		if(this.value == iptT){
			this.value = "";
			$(this).addClass(ipt_class);
		}	
	}
	iptI.onblur = function(){
		if(this.value == ""){
			this.value = iptT;
			$(this).removeClass(ipt_class);
		}	
	}
}
