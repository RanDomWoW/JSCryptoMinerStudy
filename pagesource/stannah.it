				function removeclass(theclass){
					var y = document.getElementsByClassName(theclass)[0];
					if(y != null){
						y.removeAttribute("class");
					}
				}
					
				function segundoapenas(i){
					var menus2click = document.getElementById(liid[i]);
					menus2click.addEventListener("click", function(event){
						if (numeberclicks[i] == 0){
							event.preventDefault();
							numeberclicks[i]=1;
						};
					});
				};
				
				//Core functions
				function opensubmenu(id){
					//For the case when one submenu its already open
					//we need to remove the class to close the submenu
					//that its open, to free space to open another one.
					removeclass('opensubul');
					//Reset all click, except this
					resetnumberclick(id);
					//Identification of the ul child to open
					var identification = "li-item-" + id;
					var d = document.getElementById(identification);
					d.className = "opensubul";
					console.log(numeberclicks);
				}
				
				function openmenu(){
					//Push the menu left to right
					var d = document.getElementById('mobile_menu');
					d.style.left = "0";
					//Show the close option
					var d = document.getElementById('tig-close');
					d.style.display = "block";
				}
				
				function menuclose(){
					//Push the menu back - Right to left
					var d = document.getElementById('mobile_menu');
					d.style.left = "-100%";
					//Remove option to close the menu
					var d = document.getElementById('tig-close');
					d.style.display = "none";
					//Clean everything
					resetnumberclick(0);
					removeclass();
				}
				
				function showsearch(){
					var d = document.getElementById('pesquisa');
					var iconsearch = document.getElementById('iconsearch');
					d.style.display = "block";
					iconsearch.setAttribute( "onClick", "closesearch()" );
				}
				
				function closesearch(){
					var d = document.getElementById('pesquisa');
					var iconsearch = document.getElementById('iconsearch');
					d.style.display = "none";
					iconsearch.setAttribute( "onClick", "showsearch()" );
				}
				
				function showon(navid){
					if(navid == 'contributi'){
							closetab('domande');
							document.getElementById("tab_domande").className = "navbar-post";
							document.getElementById("tab_contributi").className = "navbar-post active";
						}
					else if (navid == 'domande'){
							closetab('contributi');
							document.getElementById("tab_contributi").className = "navbar-post";
							document.getElementById("tab_domande").className = "navbar-post active";
						}
					var getnav = document.getElementById(navid);
					getnav.style.display = "block";
				}
				
				function closetab(navtoclose){
					if(navtoclose == 'contributi' || navtoclose == 'domande'){
						document.getElementById("tab_domande").className = "navbar-post";
						document.getElementById("tab_contributi").className = "navbar-post";
					}
					var getnavtoclose = document.getElementById(navtoclose);
					getnavtoclose.style.display = "none";
				}
				
				function showtab(tabid){
					var getthetab = document.getElementById('panel-faq-'+tabid);
					var nextactionisclose = document.getElementById('title-'+tabid);
					getthetab.style.display = "block";
					nextactionisclose.setAttribute( "onClick", "closetabtext('"+tabid+"')" );
					nextactionisclose.className = "accordion-toggle";
				}
				
				function closetabtext(tabid){
					var getnavtoclose = document.getElementById('panel-faq-'+tabid);
					getnavtoclose.style.display = "none";
					var nextactionisopen = document.getElementById('title-'+tabid);
					nextactionisopen.setAttribute( "onClick", "showtab('"+tabid+"')");
					nextactionisopen.className = "accordion-toggle collapsed";
				}
				
				function manualmovegallery(left_value, number_btn){
					var get_gallery_slide = document.getElementById('gallery-slide');
					get_gallery_slide.style.left = "-"+left_value+"px";
					removeclass('flex-active');
					var get_the_gallery_button = document.getElementById('gallerybtn-'+number_btn);
					get_the_gallery_button.className ='flex-active';
				}
				
			/*Start - use the details.php - Andreia*/
				function tabOpen(idSelect){

				    var tabLinks = new Array();
				    var contentDivs = new Array();
				    var tabListItems = document.getElementById('productList').childNodes;

				  for ( var i = 0; i < tabListItems.length; i++ ) {
				        if ( tabListItems[i].nodeName == "LI" ){

				          var tabLink = getFirstChildWithTagName( tabListItems[i], 'A' );
				          var id = getHash(tabLink.getAttribute('onclick'));
				          tabLinks[id] = tabLink;
				          contentDivs[id] = document.getElementById(id);

				          if(id==idSelect){
				            tabListItems[i].className="active center tab-"+id;
				          }else{
				            tabListItems[i].className="center tab-"+id;
				          }
				        }
				      }
				      

				    // Hide all content divs except the first
				      var i = 0;

				      for ( var id in contentDivs ) {
				        if ( id == idSelect ) {contentDivs[id].className = 'tab-pane fade in active';}else{contentDivs[id].className = 'tab-pane fade';}
				        i++;
				      }

				    }


			    function getFirstChildWithTagName( element, tagName ) {
			      for ( var i = 0; i < element.childNodes.length; i++ ) {
			        if ( element.childNodes[i].nodeName == tagName ) return element.childNodes[i];
			      }
			    }

			    function getHash(url) {
			      var hashPos = url.lastIndexOf ( "('" );
			      hashPos=url.substring( hashPos + 2 );
			      return hashPos.substring(0, hashPos.length - 2);
			    }
			/*End - use the details.php - Andreia*/
