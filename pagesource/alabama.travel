<script src='https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js'></script>
<script defer='defer'>
$(function() {
  var $c = $('#carousel'),
      $w = $(window);
  $c.carouFredSel({
    align: false,
    items: 4,
    scroll: {
      items: 1,
      duration: 2000,
      timeoutDuration: 1000,
      easing: 'linear',
      pauseOnHover: 'immediate'
    }
  });
  $w.bind('resize.example', function() {
    var nw = $w.width();
    if (nw < 990) {
      nw = 990;
    }
          
    $c.width(nw * 3);
    $c.parent().width(nw);
          
  }).trigger('resize.example');
  //Pretty view month, day, year
  $('#datetimepicker4').fdatepicker({
    format: 'mm/dd/yyyy'
  }).data('datepicker');
  
  //Date submitted has to be in format day, month, year to work correctly with fest-events filter
  $('#events-form').submit(function( event ) {
      //Get filter start_date, split it and re-arrange it
      var initDate = $('[name="filter[start_date]"]').val()
      var initDateSplit = initDate.split("/");
      var year = initDateSplit[2]
      var month = initDateSplit[0]
      var day = initDateSplit[1]
      var subDate = day+"/"+month+"/"+year
      
      //Set the new filter start_date
      $('[name="filter[start_date]"]').val(subDate).hide()          

  })

  })
  // Clear the form field if user navigates back.
  $(window).bind("pageshow", function() {
      $('[name="filter[start_date]"]').val('');
      });
</script>

