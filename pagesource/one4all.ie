<script>
    Validation.methods['validate-existing-password'] = Validation.get('validate-password');
    Validation.add('validate-password', 'Please use 8 characters. The Password must contain  both uppercase and lowercase characters.', function(v) {
        var pass=v.strip(); /*strip leading and trailing spaces*/
        if (pass.length < 8){return false;}if (pass.toLowerCase() == pass || pass.toUpperCase() == pass){return false;}        return true;
    });
    Validation.add('validate-existing-password', 'Please use 8 characters. The Password must contain  both uppercase and lowercase characters.', function(v) {
        var pass=v.strip(); /*strip leading and trailing spaces*/
        if (pass.length < 8){return false;}if (pass.toLowerCase() == pass || pass.toUpperCase() == pass){return false;}        return true;
    });
    document.observe("dom:loaded", function() {
        $$('#login-form #pass').
            invoke('removeClassName', 'validate-password').
            invoke('addClassName', 'validate-existing-password');
    });
</script>

