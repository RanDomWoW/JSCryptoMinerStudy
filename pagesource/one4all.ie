		function activateMobileMenu($)
	{
		if ($(window).width() < 960)
		{
			$('#mobnav').show();
			$('.vertnav-top').addClass('mobile');
			$('#nav').addClass('mobile');
		}
		else
		{
			$('#nav').removeClass('mobile');
