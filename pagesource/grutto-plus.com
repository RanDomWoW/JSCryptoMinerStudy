    <script type="text/javascript">
        window.onload = function () {
            setTimeout(function () {
                var ua = navigator.userAgent;
                if (ua.match("MSIE") || ua.match("Trident")) {
                    //console.log('IE');
                    $('#main_top > div').addClass('bxslider');
                    $('#adjust_slider').animate({
                        opacity: 1
                    }, 500);
                    $('.bxslider').bxSlider({
                        infiniteLoop: true,
                        auto: true,
                        pause: 5000,
                        speed: 800,
                        pager: true,
                        controls: true,
                        slideWidth: 712,
                        minSlides: 1,
                        maxSlides: 1,
                        moveSlides: 1,
                        slideMargin: 4
                    });
                } else {
                    //console.log('Not-IE');
                    $('#main_top > div').addClass('single-item');
                    $('#adjust_slider').animate({
                        opacity: 1
                    }, 500);
                    $('.single-item').slick({
                        //centerMode: true,
                        slidesToShow: 3,
                        slidesToScrooll: 1,
                        infinite: true,
                        autoplay: true,
                        autoplaySpeed: 5000,
                        speed: 800,
                        dots: true,
                        responsive: [
                            {
                                breakpoint: 640,
                                settings: {
                                    slidesToShow: 1,
                                    slidesToScroll: 1,
                                    dots: true
                                }
                            }
                        ]
                    });
                }

                // dots
                var winWidth = $(window).width();
                if (winWidth >= 640) {
                    var sum = $('.slick-track > div').length / 2 - 1;
                    $('.slick-dots li').remove();
                    for (i = 0; i < sum; i++) {
                        $('.slick-dots').append('<li aria-hidden="true" role="presentation" aria-selected="false" aria-controls="navigation0' + i + '" id="slick-slide0' + i + '"><button type="button" data-role="none" role="button" aria-required="false" tabindex="' + i + '">' + i + '</button></li>');
                        if (i == 0) {
                            $('.slick-dots li').addClass('slick-active');
                        }
                    }
                }//// dots

                var ua = navigator.userAgent;
                if (ua.match("MSIE") || ua.match("Trident")) {
                    var w = $(window).width();
                    var ww = w / 2 - 395;
                    $('.bxslider').css('left', ww + 'px');
                }
            }, 500);
        };

        $(window).resize(function () {
            var ua = navigator.userAgent;
            if (ua.match("MSIE") || ua.match("Trident")) {
                var w = $(window).width();
                var ww = w / 2 - 395;
                $('.bxslider').css('left', ww + 'px');
            }

        });
    </script>

