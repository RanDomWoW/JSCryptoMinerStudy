                $.ui.autocomplete.prototype._renderMenu = function (ul, items) {
                    var that = this,
                    currentEntity = "";
                    $.each( items, function( index, item ) {                        
                        
                            var li;
                            if (item.entity != '' && item.entity != currentEntity ) {
                                ul.append( "<li class='ui-autocomplete-category'>" + item.entity + "s</li>" );
                                currentEntity = item.entity;
                            }
                            
                            li = that._renderItemData( ul, item );                            
                            if (item.entity) {                                
                                li.attr( "aria-label", item.entity + " : " + item.label );
