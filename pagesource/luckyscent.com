            var tickerModule = (function ($) {

                var total = 0,
                    totalLoaded = 0,
                    targets = [],
                    render = function() {
                        if(total != totalLoaded)
                            return;


                        $.each(targets, function(i,o) {                            
                            $.each(targets[o].items, function(ii,oo) {                                      
                                $(oo.item).append(oo.data).appendTo(oo.target);
                            });
                        });

                        //return;

                        $.each(targets, function(i,o) {
                            var groupSize = targets[o].items.length,
                                maxHeight = 0,
                                maxRows = 1,
                                defaultHeight= 26;
                                
                                $.each($(o + ' > ul > li'), function(ii,oo) {
                                    //console.log($(oo).height(), $(oo).innerHeight(), $(oo).outerHeight());
                                    if($(oo).outerHeight() > maxHeight)
                                        maxHeight = $(oo).outerHeight(); 
                                        
                                });

                                if(Math.round(maxHeight/defaultHeight) > 1)
                                    maxRows = Math.round(maxHeight/defaultHeight);

                                $.each($(o + ' > ul > li'), function(ii,oo) {                                         
                                    var numRows = maxRows - (Math.round($(oo).outerHeight()/defaultHeight));    
                                    
                                    if(numRows > 0)                                         
                                        $(oo).css('line-height', (defaultHeight * numRows) + 'px');    
                                    
                                    $(oo).css('height', maxHeight);                               
                                });

                           

                            $(o).css({
                                        'position':   'initial', 
                                        'visibility': 'initial',
                                        'display': 'none'
                                    }); 

                                    //console.log(maxRows, defaultHeight);
                                                      
                            $(o + ' > ul').newsTicker({
                                speed: 1000, duration: 7500, pauseOnHover: 1, 
                                max_rows: 1, row_height: maxHeight,
                                autostart: groupSize > 1 ? 1 : 0                          
                            });

                            $(o).fadeIn('slow', function() {
                                                            
                            });

                        });


                        
                    },
                    init =  function() {
                                $.getJSON(window.location.origin + '/ajax/ticker.asp', {}, function(data) {    

                                        // build target groups first
                                        $.each(data, function(i,o) {                        
                                            if($(o.targetselector).length > 0) { // target exists
                                                if($.inArray(o.targetselector, targets) == -1) { // target array doesn't exist - create
                                                    targets.push(o.targetselector);
                                                    targets[o.targetselector] = {
                                                        "hasHigherPriorityItems" : false,
                                                        "items" : []
                                                    }
                                                    $(o.targetselector).empty().append($('<ul class="list-unstyled"></ul> '));
                                                }
                                                    
                                                if(o.tickertext != '') {
                                                    targets[o.targetselector].items.push({
                                                        "name": o.modulename,
                                                        "item": $('<li data-name="' + o.modulename + '"></li>'),
                                                        "target": $(o.targetselector + ' > ul'),    
                                                        "shouldLoad": true,                                                    
                                                        "load": function() {
                                                            this.data = o.tickertext;
                                                            this.loaded = true;
                                                            totalLoaded++;
                                                            render();
                                                        },
                                                        "data": "",
                                                        "loaded": "",
                                                        "priority": o.priority
                                                    });

                                                    if(o.priority < 1)
                                                        targets[o.targetselector].hasHigherPriorityItems = true;
                                                }       
                                                        
                                                    
                                                if(o.tickerurl != '') {
                                                    targets[o.targetselector].items.push({
                                                        "name": o.modulename,
                                                        "item": $('<li data-name="' + o.modulename + '"></li>'),
                                                        "target": $(o.targetselector + ' > ul'),       
                                                        "shouldLoad": true,
                                                        "load" : function() {
                                                            var self = this;
                                                            $.ajax({
                                                                url: o.tickerurl,
                                                                type: "GET",
                                                                dataType: "html",
                                                                success: function (response) {
                                                                    self.data = response;
                                                                    self.loaded = true;
                                                                    totalLoaded++;
                                                                    render();
                                                                }
                                                            });
                                                        },
                                                        "data": "",
                                                        "loaded": false,
                                                        "priority": o.priority
                                                    }); 

                                                    if(o.priority < 1)
                                                        targets[o.targetselector].hasHigherPriorityItems = true;
                                                }
                                            }                        
                                                                                        
                                        });

                                        // loop through groups to prioritize                                        
                                        $.each(targets, function(i,o) {
                                            if(targets[o].hasHigherPriorityItems) {
                                                targets[o].items = targets[o].items.filter(function(a,b) {                                                       
                                                        if(a.priority < 1) {
                                                            total++;
                                                            return true;
                                                        } else {
                                                            a.shouldLoad = false;
                                                            return false;
                                                        }

                                                });
                                            } else {
                                                total = total + targets[o].items.length;
                                            }                                            
                                        });
                                        
                                        //console.log(targets);

                                        // load all data
                                        $.each(targets, function(i,o) {
                                              $.each(targets[o].items, function(ii,oo) {
                                                  if(oo.shouldLoad)
                                                    oo.load();
                                              });
                                        });
                                        
                                });
                            }
                return {
                    "Init": init
                }                
            })(jQuery);

            tickerModule.Init();

            

