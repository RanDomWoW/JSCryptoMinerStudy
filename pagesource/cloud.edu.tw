					var displayImg = sp.smImgUrl == "" || sp.smImgUrl == null ? "/fileman/users/paul/default.png":sp.smImgUrl;
					html += '<li data-id="';
					html += sp.smid;
					html += '" ';
					html += ' data-more="';
					html += (i > 2 ? '1':'0');
					html += '" ';
					html += ' style="cursor:pointer; position:relative; display: block; border-bottom: 1px solid #fff; padding: 10px 0;';
					html += (i > 2 ? ' display:none':'');
					html += '"><img style="width: 20px;" src="'+displayImg+'"/>';
					html += '<span  name="editableSp" style="line-height: 1.3rem;" onclick="redirectToClientPage(\'' + sp.mmid +'\',\'' + sp.smid +'\')"> '+sp.smName+'</span>';
					html += '<i name="removeMyFunc" class="fa fa-times-circle-o" aria-hidden="true" style="position:absolute; top:0; right: 0; display:none;" onclick="removeMySp(this)"></i></li>';
					
					indexHtml += '<div name="person" data-pos="'+i+'" class="item" >';
					indexHtml += '<img style="width: 50%;margin-top: 10px;" src="'+displayImg+'" onclick="redirectToClientPage(\'' + sp.mmid +'\',\'' + sp.smid +'\')"/>';
					indexHtml += '<br/>';
					indexHtml += '<span style="font-size: 1.5rem;line-height: 1.3rem;">'+sp.smName+'</span></div>';
					$("#spSlideDown").css("display","block");
					$("#spSlideUp").css("display","none");
				}
				$("#sortable").html(html);
				$("#editingMyFuncBtns > div").hide();
				$("#editingMyFuncBtns > div").html("");
				if(indexHtml == ""){
					$("#editingMyFuncBtns > div").html("沒有選擇個人化功能");
					$("#editingMyFuncBtns > div").show();
				}
				if($("#personArea").length != 0){
					$("#personArea").css({"margin-left":"0"});
					curPosMap["person"] = 0;
					$("#personArea").html(indexHtml == ""?"<div style='padding-top: 25%;'>沒有選擇個人化功能</div>":indexHtml);
				}
			},beforeSend:function(){
				isquery = true;
	        },
	        complete:function(){
	        	isquery = false;
	        }
		});
	}
