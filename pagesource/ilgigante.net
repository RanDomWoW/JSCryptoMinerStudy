var l_i = 0

$(function() {

 $(".div_news").each(function(i)
    {
      statusArr.push($(this).html());
      $(this).text("");
    });

CambiaNews();
});

function CambiaNews()
{
  SetNewsPanel("li_news1");
  l_i++;
  
  setTimeout("CambiaNews();", 3000);
}
function SetNewsPanel(p_li)
{
  if (l_i >= statusArr.length)
  {
    l_i=0
  }
  
  var l_Testo = statusArr[l_i]
  
  $("#" + p_li).fadeOut("slow", function()
                                {
                                  $(this).html(l_Testo).fadeIn("slow");
                                });
}


