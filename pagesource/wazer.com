<script>
$(function() {

  // setup slideshows (lightbox)
  $("div:not(.project-gallery) .slideshow-lightbox").each(function(){
    
    var lightboxID = $(this).attr('id');
    
    setTimeout(function() {
        console.log("slick this: " + lightboxID);

        $("div:not(.projects-gallery) #" + lightboxID).slickLightbox({
          itemSelector: '> .image-wrapper > img',
          src: 'src',
          slick: {
            initialSlide: 0,
            infinite: false
          },
          closeOnBackdropClick: false,
          captionPosition: 'dynamic',
          caption: function(e) { 
            var caption = $(e).attr("alt");
            if( caption != 'undefined' ) {
              return $(e).attr('alt');
            }
          }
        });

    }, 250); // end timeout
      
    $(this).find(".slideshow-title").on( "click", function(){
      $(this).find(".image-wrapper:nth-child(1) img").trigger( "click" );
    });
      
    var openlightbox = location.hash.length ? location.hash.split('/'):[];
    if(openlightbox.length) {
      var el = openlightbox.shift();   
      if ( el && el === '#' + lightboxID ) {
        setTimeout(function() {
         $( 'html, body' ).animate({
            scrollTop:( $(el).offset().top - $(window).height() / 2 )
         }, 0, function(){              
         }).promise().then(function() {
            $("#" + lightboxID).find(".image-wrapper:nth-child(1) > img").trigger( "click" );
         }); 
        }, 250); // end timeout
      }; // end if slideshow matches hash
    }; // end if hash
  
  }); // end each .slideshow-lightbox
});
</script>
  
