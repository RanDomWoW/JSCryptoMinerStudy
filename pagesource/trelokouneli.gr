           function ajax_loader(){
                var next_page_value = $('.ajax-load-paged').val();
                var max_page_value = $('.ajax-load-paged').attr('max-paged');
                if( next_page_value == max_page_value ) {
                    return;
                }               
                $.ajax({
                   type: 'post',
                   url: 'https://www.trelokouneli.gr/wp-admin/admin-ajax.php',
                   data:{
                        action:'latest_posts',
                        paged:next_page_value
                   },
                   beforeSend: function() {
                    $('.ajax-loadmore').show();
                  },
                   success: function(response){
                     $('#home-ajax-latest').append(response);
                     history.replaceState( null, null, '/page/'+next_page_value );
                     var next_page_value_update = parseInt( next_page_value ) + 1;
                     $('.ajax-load-paged').val(next_page_value_update);
                     $('.ajax-loadmore').hide();
                   } 
                });  
           }            
           
           var processing;
            $(window).scroll(function(){ 
                 if (processing)
                    return false;            
               
                if ( ($(document).height() <= ( $(window).scrollTop() + $(window).height())) &&  !processing ) {                
          
                     processing = true;
                    
                      setTimeout(function() {
                        ajax_loader();
                        processing = false;      
                     }, 1000 );
                          
                }
            });            
        });
