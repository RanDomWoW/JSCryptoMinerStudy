<script type="text/javascript">
    function changeTodayText(elem) {
        changeTextByNextPrev(elem);
        if (jQuery(elem) != undefined) {
            var parent = jQuery(elem).parent();
            var toDayDiv = parent.find('.ajax__calendar_footer');
            var todayText = toDayDiv.text();
            toDayDiv.text('');
            toDayDiv.text('Heute:' + todayText.substring(todayText.lastIndexOf(":") + 1, todayText.length));
        }
    }

    function changeTextByNextPrev(elem) {
        jQuery('.ajax__calendar_next, .ajax__calendar_prev').click(function() {
            var todyDiv = jQuery(this).parent().parent().parent().find('.ajax__calendar_footer');

            if (todyDiv != undefined) {
                var todayText = todyDiv.text();
                todyDiv.text('');
                todyDiv.text('Heute:' + todayText.substring(todayText.lastIndexOf(":") + 1, todayText.length));
            }
        });
    }
</script>

