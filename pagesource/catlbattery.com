<script src="http://www.catlbattery.com/static/web/js/main.js?v=v3" type="text/javascript" charset="utf-8"></script><script src="http://www.catlbattery.com/static/web/js/js/index.js?v=v3" type="text/javascript" charset="utf-8"></script><link rel="stylesheet" href="http://www.catlbattery.com/static/web/js/nd/main.css?v=v3" type="text/css" media="screen" charset="utf-8"><script src="http://www.catlbattery.com/static/web/js/nd/jquery.easing.1.3.js?v=v3" type="text/javascript" charset="utf-8"></script><script src="http://www.catlbattery.com/static/web/js/ibanner.js?v=v3" type="text/javascript" charset="utf-8"></script><script src="http://www.catlbattery.com/static/web/js/jquery.mousewheel.js?v=v3" type="text/javascript" charset="utf-8"></script></body>
<script>
	$(function(){
		//$("html").css("overflow","hidden");
		var Page  = 0,
		Mouse = true,
		wHeight = $(window).height();
		$(".index-wrap .part").height(wHeight);
		$(".index-wrap .part").eq(5).height(610);
		$(".index-page span").eq(0).addClass('cur')
		$("html, body").stop().animate({scrollTop: 0},1000, "easeInOutCubic")
		$('body').on('mousewheel', function(event) {
			event.preventDefault();
			if(event.deltaY > 0){
				if(!Mouse){
					return
				}
				Mouse = false;
				Page --
				if(Page < 0){
					Page = 0
					Mouse = true;
					return
				}
				$(".index-page span").eq(Page).addClass('cur').siblings().removeClass('cur')
				$("html, body").stop().animate({scrollTop: Page * wHeight}, 1000, "easeInOutCubic", function(){
					Mouse = true;
				})
			}else{ // 向下
				if(!Mouse){
					return
				}
				Mouse = false;
				Page ++
				//console.log(Page);
				if(Page == $(".part").length){
					Page = $(".part").length - 1
					Mouse = true;
					return
				}
				$(".index-page span").eq(Page).addClass('cur').siblings().removeClass('cur')
				$("html, body").stop().animate({scrollTop: Page * wHeight}, 1000, "easeInOutCubic", function(){
					Mouse = true;
				})
				
			}
		})
		$(".index-page span").click(function(){
			$(this).addClass('cur').siblings().removeClass('cur')
			if(!Mouse){
				return
			}
			Mouse = false;
			Page = $(this).index();
			$("html, body").stop().animate({scrollTop: Page * wHeight}, 1000, "easeInOutCubic", function(){
				Mouse = true;
			})
		})
	})
</script>
</html>