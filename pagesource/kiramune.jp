<script type="text/javascript">
$(document).ready(function(){
    var winWidth = $(window).width();


pv_stopplay = function(a){

    $(".swiper-wrapper .swiper-slide").each(function(index){
      var vio_ID = $(this).find("video").attr("id");
      var vio_h = $(this).find("video");
     
      if(vio_ID!=undefined){
        document.getElementById(vio_ID).currentTime="0";
        document.getElementById(vio_ID).pause();
      }
    });

    if(a!= undefined){
      var vio_plID = $(".swiper-wrapper .swiper-slide").eq(a).find("video").attr("id");
      if(vio_plID!=undefined){
       document.getElementById(vio_plID).volume="0";
       document.getElementById(vio_plID).play();
      }
    }
};



$(".swiper-container").swiper({
    nextButton: '.top_next',
    prevButton: '.top_prve',
    autoplay : 16000,
    effect : 'fade',
    longSwipesRatio : 0.1,
    //preventClicksPropagation:true,
    //preventClicks : false,//默认true
    onInit: function(swiper){
      var ncd = swiper.activeIndex;
      pv_stopplay(ncd);
      var bglink =  $(".swiper-slide-active img").attr("src");
      $(".sld_bg").css({"background-image":"url("+bglink+")"});
    },
    onSlideChangeEnd: function(swiper){
      if(winWidth>750){
      var ncd = swiper.activeIndex;
      pv_stopplay(ncd);

      }

      var bglink =  $(".swiper-slide-active img").attr("src");
      $(".sld_bg").css({"background-image":"url("+bglink+")"});

    },

    pagination: '.swiper-pagination',
    paginationClickable: true,
    paginationBulletRender: function (swiper, index, className) {
      return '<span class="' + className + '"><img src="'+$(".swiper-wrapper .swiper-slide").eq(index).attr("data-smallimg")+'"></span>';
    }

});

  BlocksIt_start = function(){
    winWidth = $(window).width();
    var col = 4;
    if(winWidth <= 750) {
      col = 2;
    }
    $('.grid_mob').BlocksIt({
      numOfCol: col,
      offsetX: 4,
      offsetY: 10
    });
  }
  BlocksIt_start();

    $(".inner_ph img").lazyload({  
      effect: "fadeIn",
      load:function(){
        BlocksIt_start();
      }
    }); 
    
    //window resize
    $(window).resize(function() {
      BlocksIt_start();
    });

  $(".list_nav a").click(function(){
    var O_txet = $(this).text();
    $(".news_inner").removeClass("grid").removeAttr("style").hide();
    $(".inner_ioc span").each(function(){
      var O_intext = $(this).text();
      if(O_txet.indexOf(O_intext)>=0){
        $(this).parents(".news_inner").addClass("grid").show();
      }
    });

    BlocksIt_start();
    return false;

  });

