// mobile gallery load more
function loadMoreGallery(){
     jQuery(".double-bounce").show();
    jQuery('.gallery-load-more').hide();
     totalSlides=jQuery("figure[class*='gallery-item']").length;
     inicio = indexGalleries *6;
     fin= inicio+ 6;
     console.log(inicio, fin, totalSlides);
     if(fin > totalSlides){
       fin= totalSlides;
     }else if(fin < totalSlides){
      jQuery('.gallery-load-more').delay(2000).fadeIn();
     }
      setTimeout(function () {
         jQuery(".gallery-columns-3 figure[class*='gallery-item']" ).slice( inicio, fin ).css( "display", "block" );
         jQuery(".double-bounce").hide();
         indexGalleries++;  
      }, 1000);
}      

