    }
    if (!govCnPushObject) {
        return;
    }
 
 

    //向表格填充内容
    for (i = 0; i < govCnPushObject.length && (rowCount == "" || (rowCount != "" && i < parseInt(rowCount))); i++) {
 
        var li=document.createElement("div");
            li.className=liclass;
            var timespan = document.createElement("span");
            timespan.className=timeclass;
            timespan.appendChild(document.createTextNode(govCnPushObject[i]['pubDate']));
            li.appendChild(timespan);
            var tagA = document.createElement("a"); //创建链接
            tagA.className=aclass;
            tagA.setAttribute('href', govCnPushObject[i]['link']);
            tagA.setAttribute('target', '_blank');
           var gtitle = govCnPushObject[i]['title'];
            if(gtitle.length>titlelength){
            gtitle=gtitle.substring(0,titlelength-2)+'...';
         }
            tagA.appendChild(document.createTextNode(gtitle));
            li.appendChild(tagA);  
 
        contentDom.appendChild(li);
    }
}
	
