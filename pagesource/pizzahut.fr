    <script type="text/javascript">


        document.addEventListener("DOMContentLoaded", function() {



            imagesSmall = [];
            imagesMedium = [];
            imagesLarge = [];

            
            if(window.innerWidth < 600){
                images = imagesSmall;
            }else if(window.innerWidth < 1400){
                images = imagesMedium;
            }else{
                images = imagesLarge;
            }

            if (images.length > 1) {

                $('#bgcycle-next-button').click(function (e) {
                    clearTimeout(bgCycleTOId);
                    clearInterval(bgCycleIntId);
                    cycleToNextImage();
                    bgCycleTOId = setTimeout(function () { bgCycleIntId = setInterval(cycleToNextImage, 6000);}, 10000);
                });

                $('#bgcycle-previous-button').click(function (e) {
                    clearTimeout(bgCycleTOId);
                    clearInterval(bgCycleIntId);
                    cycleToPreviousImage();
                    bgCycleTOId = setTimeout(function () { bgCycleIntId = setInterval(cycleToNextImage, 6000);}, 10000);
                });

                var bgcyclePagerHtml = '<ul>';

                for (var im = 0; im < images.length; im++) {
                    bgcyclePagerHtml += '<li><a href="javascript:void(0)" onclick="pagerClicked(' + im + ');"><span>' + im + '</span></a></li>'
                }

                bgcyclePagerHtml += '</ul>';

                $('#bgcycle-pager').html(bgcyclePagerHtml);

            } else {

                $('#bgcycle-next-button, #bgcycle-previous-button, #bgcycle-pager').hide();

            }

            $("body").backgroundCycle({
                imageUrls: images,
                fadeSpeed: 1000,
                duration: 6000,
                backgroundSize: SCALING_MODE_COVER
            });
        });

    </script>

