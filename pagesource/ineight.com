			label = $(this).find('span').html().replace('&nbsp;',' ');
		}
		if(label.includes('</svg>')) {
			label = $(this).find('svg').attr('class');
		}
		ga('send', 'event', 'external-link', 'click', label);
	});

	// homepage panels...
	$('.panel .btn').click(function(e) {
		var label = $(this).html().replace('&nbsp;',' ');
		if(label.includes('<span>')) {
			label = $(this).find('span').html().replace('&nbsp;',' ');
		}
		ga('send', 'event', 'home-slider-button', 'click', label);
	});

	// homepage card flippin'...
	$('.panel .card .frontside a').click(function(e) {
		var label = $(this).parent().find('h4').html().replace('&nbsp;',' ');
		ga('send', 'event', 'card-flip', 'click', label);
	});

	// solutions modals...
	$('#software-solutions .card a').click(function(e) {
		var label = $(this).closest('.card').attr('id');
		ga('send', 'event', 'modal', 'click', label);
	});

	// nav link events...
	$('#menu-util-nav a').click(function(e) { 
		var label = $(this).find('span.item_text').html().replace('&nbsp;',' ');
		ga('send', 'event', 'util-nav', 'click', label);
	});

	$('#menu-main-nav a').click(function(e) {
		var label = $(this).find('span.item_text').html().replace('&nbsp;',' ');
		ga('send', 'event', 'main-nav', 'click', label);
	});

	// footer events...
	$('.mkd-page-footer a').click(function(e) {
		var label = $(this).html();
		ga('send', 'event', 'footer', 'click', label);
	});

});
</script>