		<script type='text/javascript'>
			var _xhr;
			function getStories(person){
			
			    $('.stories_container').html('');
			    var url;
			    if(person.nameEn!=null)
			    {
			        url='//www.oneman.gr/gynaikes/'+person.nameEn+'/?widget=rssfeed&view=feed&contentId=1480449';
			    }else{
			        url='//www.oneman.gr/gynaikes/?widget=rssfeed&view=feed&contentId=1480449';
			    }
			    if(_xhr){
			        _xhr.abort();     
			    }
			    _xhr=  $.ajax({
			        url :url,
			        success: function(data, textStatus, jqXHR) {
			            var entries = $(data).find("entry");
			            var articles = [];
			            var title;
			            var divs = "<div class='stories_container'>";            
			            var img;            
			            if(entries.length>0)
			            { 
			                $('.grid_8.area_main .stories_container').append("<div style='font-family: sans-serif;font-weight: bold;font-size: 14px;margin:20px 0;'>ΓΡΑΨΑΜΕ ΓΙΑ ΑΥΤΗΝ</div>");
			            }
			            for(var i = 0; i < 4;i++) {
			                var entry = entries[i];
			                var article = {
			                    'title' : $(entry).find("title").text(),
			                    'image' : $(entry).find("link[type='image/jpeg']").attr('href'),
			                    'link' : $(entry).find("link[rel='alternate']").attr('href')
			                };
			                articles[i] = article;
			                var clazz = "";
			                if((i+1)%4 == 1 ){
			                    clazz = "alpha";
			                } else if((i+1)%4 == 0){
			                    clazz = "omega";
			                }
			                img=article.image;
			                
			                divs += (i+1)%4 == 1 ? "<div class='grid_8'>" : "";
			                divs += "<div class='grid_2 " + clazz+"'>";
			                if(img!=null){
			                    divs += "<a href='"+article.link +"' target='_blank'><img class='thumb' src='"+img.replace('w300','w140').replace('BINARY','ALTERNATES')+"'/></a>";
			                }
			                divs += "<h2><a class='title' target='_blank' href='"+ article.link +"'>" + article.title +"</a></h2>";
			                divs += "</div>";
			                divs += (i+1)%4 == 0 ? "</div>" : "";
			            }
			            divs += "</div>";                                 
			            $(".grid_8.area_main .stories_container").append(divs);
			            
			        },
			        "dataType" : "xml"
			    });
			    /*share buttons dynamic links*/
			    var str=encodeURIComponent('//www.oneman.gr/top_100/top_100_2012/'+'?fb_param='+person.id+'#'+person.id);
			    $('.share .f_book iframe').attr('src','//www.facebook.com/plugins/like.php?send=false&layout=button_count&width=300&show_faces=false&action=like&colorscheme=light&font=arial&height=21&appId=150049175019623&href='+str);
			    /*end share buttons dynamic links*/
			
			    $('iframe.banner_dynamic').attr('src','//static.24media.gr/temp/txt-links/oneman/banner.html?'+person.id);
			
			    $('iframe.twitter_button').attr('src','//platform.twitter.com/widgets/tweet_button.html?url='+str+'&text='+person.name+'&count=none');
			
			$(".endCompetition").text("Η ψηφοφορία ολοκληρώθηκε, βλέπετε τις υποψήφιες σε αλφαβητική σειρά μέχρι την τελική ανακοίνωση.");
			}
			
		</script>
