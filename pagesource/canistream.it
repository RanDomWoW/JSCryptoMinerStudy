    function fblogin() {
        FB.login(function(response) {
            if (response.authResponse) {
                // connected
                FB.api('/me', function (response) {
                    $.post(
                        "/users/fblogin",
                        response, function (data) {

                            $(".nav-logoff .dropdown").removeClass("open");
                            $("#signupDialog").modal("hide");
                            if (data.status) {
                                $.setCurrentUser(data.User._id);
                                $.handleUserLoggedIn(data);
                                if (window.wantsToBeReminded != null) {
                                    $.showReminderPopup(window.wantsToBeReminded.type, window.wantsToBeReminded.affiliate, window.wantsToBeReminded.movie);
                                    window.wantsToBeReminded = null;
                                }
                            }
                            else {
                                alert("There was an error: " + data.status_message);
                            }
                        },
                        'json'
                    );
                });
            } else {
                // cancelled
            }
        },{perms:'email'});
    }

