	function addMarker(location, titre, lien, pmagasin) {

		var iconImage=  new google.maps.MarkerImage(
			 "https://www.nozarrivages.com/wp-content/themes/nozarrivages/img/marker.png",
			 new google.maps.Size(10, 10),
			 new google.maps.Point(0, 0),
			 new google.maps.Point(5, 5)
		);


		var marker = [pmagasin, new google.maps.Marker({
				position: location,
				map: map,
				icon : iconImage,
				title: titre,
			}),
			lien
		];
		markers.push(marker);
		allmarkers.push(marker);

		google.maps.event.addListener(marker[1], 'click', function() {
			  document.location.href 	= lien;
		});

	}

	function setMapOnAll(map) {
		for (var i = 0; i < markers.length; i++) {
		 	markers[i][1].setMap(map);
		}

	}

	function gestionMap(pmagasin){

		setMapOnAll(null);
		markers = [];

		for (var i = 0; i < allmarkers.length; i++) {
			if(allmarkers[i][0] == pmagasin){
				var mark 		= [pmagasin, allmarkers[i][1], allmarkers[i][2]];
				var positions 	= allmarkers[i][1]
				var latlng 		= positions['position'].toString();
				latlng 			= latlng.replace("(", "");
				latlng 			= latlng.replace(")", "");
				latlng 			= latlng.split(', ');
				markers.push(mark);
			}
		}


		for (var i = 0; i < allmarkers.length; i++) {
			var mark 		= [pmagasin, allmarkers[i][1], allmarkers[i][2]];
			var latlng2 	= positions['position'].toString();
			latlng2 			= latlng2.replace("(", "");
			latlng2 			= latlng2.replace(")", "");
			latlng2 			= latlng2.split(', ');

			var v1 = new google.maps.LatLng(latlng[0],latlng[1]);
			var v2 = new google.maps.LatLng(latlng2[0],latlng2[1]);

			markers.push(mark);
		}

		map.setZoom(12);
		map.setCenter(new google.maps.LatLng(latlng[0], latlng[1]));

		setMapOnAll(map);

	}

