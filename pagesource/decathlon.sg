 checkout<i class="icon-chevron-right right"></i> </span> </a></div></div></div><div class="crossseling"></div></div><div class="layer_cart_overlay"></div><div class="blockuserinfo-container"><div id="header_user"></div> <script>$(document).ready(function() {
                $.ajax({
                    type: "POST",
                    url: baseUri + 'modules/blockuserinfo/ajax-blockuserinfo.php',
                    data: '',
                    success: function(data){
                        $('#header_user').html(data);
                        initUserHoverListener();
                    }
                });
            });

            function initUserHoverListener()
            {
                var header_user_info = new HoverWatcher('#header-compte-menu-link');
                var logout_block = new HoverWatcher('#header_user .logout_block');
                var is_touch_enabled = false;


                if ('ontouchstart' in document.documentElement)
                    is_touch_enabled = true;

                $(document).on('click', '#header_user .header_user_info a', function(e){
                    e.preventDefault();
                    e.stopPropagation();

                    // Simulate hover when browser says device is touch based
                    if (is_touch_enabled)
                    {
                        if ($(this).next('.logout_block:visible').length && !logout_block.isHoveringOver()) {
                            $("#header_user .logout_block").stop(true, true).slideUp(450);
                        }
                        else {
                            $("#header_user .logout_block").stop(true, true).slideDown(450);
                        }
                        return;
                    }
                    else {
                        window.location.href = $(this).attr('href');
                    }
                });

                $(document).on('hover', '#header_user .header_user_info', function(e) {
                    if(e.type == 'mouseenter') {
                        $("#header_user .logout_block").stop(true, true).slideDown(450);
                    }
                    else if (e.type == 'mouseleave') {
                        setTimeout(function(){
                            if (!header_user_info.isHoveringOver() && !logout_block.isHoveringOver())
                                $("#header_user .logout_block").stop(true, true).slideUp(450);
                        }, 200);
