    <script>
        function getNewData(list, scrollTop, windowHeight) {
            var el = list['el'];
            var url = list['url'];

            var items = el.find('.item');
            var lastItem = items.last();
            var lastItemOffsetTop = lastItem.offset().top;

            if (list['active'] && scrollTop + windowHeight > lastItemOffsetTop) {
                // While the ajax loading is occurring, disable the script
                list['active'] = false;

                $.get(url + '/' + items.length, function (data) {
                    list['el'].append(data);

                    // After we append the data, enable the script again
                    list['active'] = true;
                });
            }
        }

        var scrollEvent = function () {
            var lists = window.activeInfinityLists;
            var windowHeight = $(window).height();
            var scrollTop = $(this).scrollTop();

            for (var i = 0, len = lists.length; i < len; i++) {
                getNewData(lists[i], scrollTop, windowHeight);
            }
        }

        function infinityScroll(el, url) {
            var list_obj = {
                el: el,
                url: url,
                active: true
            };

            if (typeof window.activeInfinityLists == 'object') {
                window.activeInfinityLists.push(list_obj);
                return;
            }

            window.activeInfinityLists = [list_obj];

            // infinity scroll is removed in place of a load more button.
            // to do the load, just attach scrollEvent() to onclick of load more
            //$(window).on('scroll', scrollEvent);
        }

        infinityScroll($('#events ul'), '/ajax/homepage-events');
