				  },
				  complete: function(number) {
					//alert(number);
					slider_data = $('#homepage-all-slides-box').data('plugin_slidesjs'); 	
					total_slides = $(".slidesjs-slide").length;
					
					if(number == total_slides){
						slide_to_adjust = 0;
					}else{
						slide_to_adjust = number-1;
					}
					$( "div.slidesjs-slide[slidesjs-index="+slide_to_adjust+"]" ).css({"z-index":"90"});
					
					allow_scrolling = true;
					check_slider_visibility(number);
				  }
				},
				
				
				navigation: false,
				play: {
				  active: false,
				  auto: auto_rotate,
				  interval: 10000,
				  swap: false,
				  pauseOnHover: false,
				  restartDelay: 5000,
				  effect : "fade"
				}, 
				pagination: {
					effect: "fade"
				},
				effect: {
				  slide: {
					// Slide effect settings.
					speed: 1000
					  // [number] Speed in milliseconds of the slide animation.
				  },
				  fade: {
					speed: fade_speed,
					  // [number] Speed in milliseconds of the fade animation.
					crossfade: true
					  // [boolean] Cross-fade the transition.
				  }
				}
				
			});
			slider_data = $('#homepage-all-slides-box').data('plugin_slidesjs'); 		
		});
		
		$("div#next-slide-btn a").click(function(event){
			event.preventDefault();
			slider_data = $('#homepage-all-slides-box').data('plugin_slidesjs'); 		
			slider_data.next("fade");
		});
		
		
		$(document).on('touchmove', function(e) {
			if ( ! allow_scrolling ) {
				e.preventDefault();
			}else{
				return true;
			}
		});
		
		$("#homepage-all-slides-box").on('touchmove', function(e) {
			if ( ! allow_scrolling ) {
				e.preventDefault();
			}else{
				return true;
			}
		});
		
		//#homepage-all-slides-box
		
		$(document).on('touchstart', function(e) {
			if ( ! allow_scrolling ) {
				e.preventDefault();
			}else{
				return true;
			}
		});
		$(document).on('touchend', function(e) {
			if ( ! allow_scrolling ) {
				e.preventDefault();
			}else{
				return true;
			}
		});
		
		//div#homepage-slides-wrapper
		
		
	});
	 
  </script>
