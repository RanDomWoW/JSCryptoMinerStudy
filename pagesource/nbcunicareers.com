<script type="text/javascript">

	/*== Show/hide curtain boxes ==*/
	first_criteria_option = $("div.jobsDropNav li a").eq(0);
	first_criteria_option.addClass("on-display");
	first_criteria_option.parent().next("span").addClass("on-display");
	
	$("div.jobsDropNav li a").click(function(event){
		event.preventDefault();
		box_to_show = $(this).attr("href");
		$(".jobTab").hide();
		$("#"+box_to_show).show();
		$("div.jobsDropNav li span").removeClass("on-display");
		$(this).parent().next("span").addClass("on-display");
		$("div.jobsDropNav li a").removeClass("on-display");
		$(this).addClass("on-display");
	});
	
	/*== Redirect ==*/
	$("#search-divisions-mobile form span select").change(function(){
		new_location = $(this).val();
		window.location = new_location;
		//alert(new_location);
	});

    /*==Search Levels checkboxes==*/
    $('#intern').click(function(){
        if($('#intern').is(':checked')) {
        $('.checkbox-btn.intern').addClass('checked');
        } else {
        $('.checkbox-btn.intern').removeClass('checked');
        }
    });
    $('#entry').click(function(){
        if($('#entry').is(':checked')) {
        $('.checkbox-btn.entry').addClass('checked');
        } else {
        $('.checkbox-btn.entry').removeClass('checked');
        }
    });
    $('#experienced').click(function(){
        if($('#experienced').is(':checked')) {
        $('.checkbox-btn.experienced').addClass('checked');
        } else {
        $('.checkbox-btn.experienced').removeClass('checked');
        }
    });
</script>		

	         
	
