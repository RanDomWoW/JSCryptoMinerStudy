<script type="text/javascript">
    var formSubscribed = (function () {

        var email = $('#sbw_email'),
            form = $('#sbw_form'),
            submit = $('#sbw_submit'),
            resultMessage = $('#sbw_result'),
            resultMessage2 = $('#sbw_result_update'),
            urlGateWay = '/online/gatewayForContacts/newContacts';

        var setEvents = function () {
            form.on('submit', function () {
                if(validationForm()){
                    ajaxRequest();
                }
                return false;
            });
        };

        var validationForm = function () {

            var flag = true;

            if (email.val() != '') {
                var r = /^[\w\.\d-_]+@[\w\.\d-_]+\.\w{2,4}$/i;
                if (!r.test(email.val())) {
                    email.tooltip({'title': 'Некорректное значение'});
                    email.tooltip('show');
                    flag = false;
                }
            }else{
                email.tooltip({'title': 'Заполните email'});
                email.tooltip('show');
                flag = false;
            }


            return flag;

        };

        var ajaxRequest = function () {
            $.ajax({
                url: urlGateWay,
                type: "GET",
                data: form.serialize(),
                dataType: "json",
                success: function(data){
                    console.log(data);
                    form.addClass('hide');
                    evn(2);
                    if(data.resultContact.statusConfirm == "0")
                        resultMessage.removeClass('hide');

                    else
                        resultMessage2.removeClass('hide');
                }
            });
        };


        var start = function () {
            setEvents();
        };

        return start();
    });


    $(document).ready(function () {
        formSubscribed();
    });
</script>

