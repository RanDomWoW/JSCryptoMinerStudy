function FadeIn1(opa) {
if (opa <= 100) {
	document.getElementById('champ').style.filter = "alpha(opacity:"+opa+")";  // IE のソース
	document.getElementById('champ').style.opacity = opa/100;   //Mozilla
	
	if (opa == 0) document.getElementById('champ').style.visibility = 'visible';
		opa += opacnt;
		setTimeout("FadeIn1("+opa+")", timer);
	}
}

$(function() {
	opa=0;  //透明度　０は透明
	opacnt=10;　//透明度の増減の間隔
	timer=400;  
	// フェードイン
	FadeIn1(opa);
	
});
</script>
