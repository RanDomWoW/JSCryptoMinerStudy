<script>

var headimg = new Array();
var headtext = new Array();
var headsort = new Array();
var link = new Array();
var logo = new Array();
var text_color = new Array();

headimg[0] = "/upload/iblock/dcc/back2_03.jpg";
logo[0] = "logo1";
text_color[0] = "#00377B";
link[0] = "";
headtext[0] = "";
headsort[0] = "50";
document.getElementById('headselector').innerHTML+='<span class=selectpoint id=headslide0 onClick=headerfunc(0,tthis,animtime,headdelay)> </span>';
img0 = new Image(); img0.src=headimg[0];

headimg[1] = "/upload/iblock/ee4/main_page_gr.jpg";
logo[1] = "logo1";
text_color[1] = "#00377B";
link[1] = "";
headtext[1] = "";
headsort[1] = "40";
document.getElementById('headselector').innerHTML+='<span class=selectpoint id=headslide1 onClick=headerfunc(1,tthis,animtime,headdelay)> </span>';
img1 = new Image(); img1.src=headimg[1];

headimg[2] = "/upload/iblock/4fa/4.jpg";
logo[2] = "logo1";
text_color[2] = "#00377B";
link[2] = "";
headtext[2] = "";
headsort[2] = "30";
document.getElementById('headselector').innerHTML+='<span class=selectpoint id=headslide2 onClick=headerfunc(2,tthis,animtime,headdelay)> </span>';
img2 = new Image(); img2.src=headimg[2];


var headdelay = 12000;
var animtime = 500;
var ind_new = 0; var ind_old = 0;
tt="";tthis=0;

headerfunc(ind_new,ind_old,animtime,headdelay);

function headerfunc(nnew,oold,aanim,hhead){

	if(typeof(headimg[nnew])!="string" && typeof(headtext[nnew])!="string"){
		nnew=0;
	}
if(nnew==0){

    if(typeof(document.getElementById("birsd"))=="object"){
        if(document.getElementById("birsd"))
        document.getElementById("birsd").innerHTML="20-й День рождения";
    }else{
        // alert(typeof(document.getElementById("birsd")))
    }

}else{
    if(typeof(document.getElementById("birsd"))=="object"){
        if(document.getElementById("birsd"))
        document.getElementById("birsd").innerHTML="Мир без преград";
    }else{
        //alert(typeof(document.getElementById("birsd")))
    }
}


	document.getElementById("headimga").style.background="url("+headimg[oold]+") center 0 no-repeat";
	document.getElementById("headimga").style.display="block";
	document.getElementById("headimga").href=link[oold];
	document.getElementById("headimgb").style.display="none";
	document.getElementById("headimgb").style.background="url("+headimg[nnew]+") center 0 no-repeat";
	document.getElementById("headimgb").href=link[nnew];
	
	var animcha = $("#headimga");
	animcha.fadeOut(aanim);
	//alert(logo[nnew]);
	$("#main_logo").removeClass("logo1");
	$("#main_logo").removeClass("logo2");
	$("#main_logo").removeClass("logo3");
	$("#main_logo").removeClass("logo4");
	$("#main_logo").addClass(logo[nnew]);
	$(".head p:first").css("color",text_color[nnew]);
	
	var animchb = $("#headimgb");
	animchb.fadeIn(aanim);
	

	if(!headtext[nnew]){
		document.getElementById("headmsg").style.display="none";
		document.getElementById("headtext").innerHTML="";
	}else{
		document.getElementById("headmsg").style.display="";
		document.getElementById("headtext").innerHTML=headtext[nnew];
	}
	document.getElementById("headslide"+oold).style.background="";
	document.getElementById("headslide"+nnew).style.background="white";
	
	clearTimeout(tt);
	tt = window.setTimeout("headerfunc("+(nnew+1)+","+nnew+","+aanim+","+hhead+")",headdelay);
	tthis=nnew;
	
}


</script>
