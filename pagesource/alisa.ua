  <script>
    function calculateShippingTime(spr, wh){
      wh = wh.split(',');
      var alisa_working_days = [0, 1, 1, 1, 1, 1, 0];
	
      var value = 0;
      var start_date = new Date();
	 
	  
      if ( wh.indexOf('36') != -1){ // "Афина"
          value = +start_date; // сегодня
          if (start_date.getDay() == 0 || start_date.getHours() >= 15){ // завтра
              start_date.setHours(0);
              value = +start_date.setDate(start_date.getDate() + 1);
          }
          if (start_date.getDay() == 6 && start_date.getHours() >= 15){ // послезавтра
              value = +start_date.setDate(start_date.getDate() + 2);
          }
		  check_date=start_date;
		  while(alisa_working_days[start_date.getDay()]==0){
		  value = +start_date.setDate(check_date.getDate()+1);
		  }
      } else{ // "Афины" нет, перебираем склады, выбираем самый большой срок
          value = [];
          for (var i = 0; i < wh.length; i++){
              if (spr[wh[i]]) { // в справочнике есть такой склад
                  var w = spr[wh[i]];

                  if (w.special_date != '' && +new Date(w.special_date) > +start_date){ // установлена специальная дата, считаем от неё
                      start_date = new Date(w.special_date);
                  }

                  // start_date - нерабочий день поставщика?
                  while (!w.working_days[start_date.getDay()]){
                      start_date.setDate(start_date.getDate() + 1);
                      start_date.setHours(0); // сбрасываем часы перед проверкой deadline
                  }

                  // проверка deadline
                  var deadline = w.deadline.split(':');
                  if (start_date.getHours() > deadline[0] || (start_date.getHours() == deadline[0] && start_date.getMinutes() > deadline[1])){
                      // рабочий день поставщика, но время - после deadline
                      // добавляем 1 день и ищем следующий рабочий день поставщика
                      start_date.setDate(start_date.getDate() + 1);
                      while (!w.working_days[start_date.getDay()]){
                          start_date.setDate(start_date.getDate() + 1);
                      }
                  }
                  while (w.shipping_days){
                      start_date.setDate(start_date.getDate() + 1);
                      if (alisa_working_days[start_date.getDay()]) w.shipping_days--;
                  }

                  value.push(+start_date);
              }
          }
          if (value.length){
              value = Math.max.apply(null, value);
          } else {
              value = 0;
          }
      }

      return value;
  }
  </script>
