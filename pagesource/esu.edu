<script>
	 
	$('a').live('click', function(e) {
		if (ga.hasOwnProperty('loaded') && ga.loaded === true) {
			target = $(e.target);
			url = target.closest("a").attr("href");
			if (url) {	
				category = 
					$.contains($('#headerSearch').get(0), target.get(0)) ? 'Home - Search' : 
					$.contains($('#header').get(0), target.get(0)) ? 'Home - Site Header' : 
					$.contains($('#dropDownNav').get(0), target.get(0)) ? 'Home - Mega Menu' : 
					$.contains($('#mainslider').get(0), target.get(0)) ? 'Home - Success Stories' : 
					$.contains($('#homethree .grid_5:first-child').get(0), target.get(0)) ? 'Home - Featured Stories' : 
					$.contains($('#homethree .grid_5:not(:first-child):not(.lastone)').get(0), target.get(0)) ? 'Home - Featured Events' : 
					$.contains($('#homethree .grid_5.lastone').get(0), target.get(0)) ? 'Home - Trending' : 
					$.contains($('#infoResources').get(0), target.get(0)) ? 'Home - Information & Resources' : 
					$.contains($('#happenings').get(0), target.get(0)) ? 'Home - Happenings' : 
					$.contains($('#footer').get(0), target.get(0)) ? 'Home - Site Footer' : 
					'other';

				if ($.contains($('#mainslider').get(0), target.get(0)) && url == "#") {
					return; // next and forward buttons; click automatically every few seconds
				}
				ga('send', 'event', category, 'click', url, {
				  'transport': 'beacon'
				});
			}
		}
	});
</script>
