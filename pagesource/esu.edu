<script>
	var insider_url = "//quantum.esu.edu/insider/?json=1&count=3&exclude=excerpt,comment_count,comments,title_plain,content,slug,date,modified,categories,tags,author&thumbnail_size=thumbnail&callback=test";
	 
	 function truncText (text, maxLength, ellipseText){
        ellipseText = ellipseText || '&hellip;';      
        if (text.length < maxLength) 
            return text;
        var m = text.substr(0, maxLength).match(/([^A-Za-z0-9_]*)[A-Za-z0-9_]*$/);
        if(!m) return ellipseText;
        var lastCharPosition = maxLength-m[0].length;
        if(/[\s\(\[\{]/.test(text[lastCharPosition])) lastCharPosition--;
        return (lastCharPosition ? text.substr(0, lastCharPosition+1) : '') + ellipseText;
      }
	
	  function changeProtocol(text) {
		  var http_url = text.replace(/https:/, "http:");
		  return http_url;;
	  }
	
	function pageload(){
		$.ajax({
			url: insider_url,
			dataType: "jsonp",
			crossDomain: true,
			success: success_handler,
			error: error_handler,
			timeout: 2500
		});
		
		function success_handler(data){
			for(var i=0; i <= 2; i++){
				var shortText = data.posts[i].title.replace(/(<p>|<\/p>)/g, "").replace(/&#8217;/g,"'");
				shortText = truncText(shortText, 70, '...');
				$('#insider-featured-'+i+' .insider-featured-excerpt').text(shortText);
				$('#insider-featured-'+i+' .insider-featured-url').attr('href', changeProtocol(data.posts[i].url));
				$('#insider-featured-'+i+' .insider-featured-image').attr('src', data.posts[i].thumbnail);
			}
		}
		
		function error_handler(data) {
			$('#insider-featured-0 .insider-featured-excerpt').text("ESU Insider is temporarily unavailable.");
			$('#insider-featured-0 .insider-featured-url').attr('href', '#');
			$('#insider-featured-0 p a').remove();
			for(var i=1; i <= 2; i++){
				$('#insider-featured-'+i).remove();
			}
			$('#insider-featured-0').parent().parent().children("div.twitter-reply").remove();
		}
	}	
</script>

