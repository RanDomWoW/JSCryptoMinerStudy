<script type="text/javascript">

slideTimer=null;

jQuery.fn.getHiddenDimensions = function(includeMargin) {
    var $item = this,
        props = { position: 'absolute', visibility: 'hidden', display: 'block' },
        dim = { width:0, height:0, innerWidth: 0, innerHeight: 0,outerWidth: 0,outerHeight: 0 },
        $hiddenParents = $item.parents().andSelf().not(':visible'),
        includeMargin = (includeMargin == null)? false : includeMargin;

    var oldProps = [];
    $hiddenParents.each(function() {
        var old = {};

        for ( var name in props ) {
            old[ name ] = this.style[ name ];
            this.style[ name ] = props[ name ];
        }

        oldProps.push(old);
    });

    dim.width = $item.width();
    dim.outerWidth = $item.outerWidth(includeMargin);
    dim.innerWidth = $item.innerWidth();
    dim.height = $item.height();
    dim.innerHeight = $item.innerHeight();
    dim.outerHeight = $item.outerHeight(includeMargin);

    $hiddenParents.each(function(i) {
        var old = oldProps[i];
        for ( var name in props ) {
            this.style[ name ] = old[ name ];
        }
    });

    return dim;
}
function debounce(func, wait, immediate) {
    var timeout;
    return function() {
        var context = this, args = arguments;
        var later = function() {
            timeout = null;
            if (!immediate) func.apply(context, args);
        };
        var callNow = immediate && !timeout;
        clearTimeout(timeout);
        timeout = setTimeout(later, wait);
        if (callNow) func.apply(context, args);
    };
};
function fixSize($img){
    var $ancestor = jQuery('.modArticleSlider');
    var parentH=$ancestor.height();
    var parentW=$ancestor.width();
    var d=$img.getHiddenDimensions();
    var H=d.height;
    var W=d.width;
    var wr=parentW/W;
    var hr=parentH/H;
    if(H==0||W==0){
    	$img.css({'width':'100%','height':'auto'});
    }else{
	    if(H*wr>=parentH){
			$img.css({'width':parentW,'height':H*wr});
	    }else{
	    	$img.css({'height':parentH,'width':W*hr});
	    }
    }
    
}

	function changeSlide(index){
		clearTimeout(slideTimer);
		//jQuery('.modArticleSlider .slideItem').stop();
		jQuery('.modArticleSlider .slideItem').finish();
		var $ancestor = jQuery('.modArticleSlider');
	    	var $cur=$ancestor.find(".active");
	   	var $next;
	    	if(typeof index!="undefined"){
	    		$next = $ancestor.find(".slideItem").eq(index);
	    	}else{
	    		$next = $cur.next('.slideItem');
	   	 }
	    	if( $next.length == 0 ) {
	        	$next = $ancestor.find(".slideItem").eq(0);
	    	}
	    	var $nextImg=$next.find(".slideImage");
		fixSize($nextImg);
		var i=$next.index();
		jQuery('.modArticleSlider .slides-pagination a').removeClass("current");
		jQuery('.modArticleSlider .slides-pagination a').eq(i).addClass("current");
	    	$cur.fadeOut(1000);
	    	$cur.removeClass('active');
	   	$next.fadeIn(1000);
	   	$next.addClass('active');
	  	 slideTimer=setTimeout(function(){ changeSlide(); }, 5000);
	}
        	
	changeSlide();
	jQuery('.modArticleSlider .slides-pagination a').click(function(e){
		e.preventDefault();
		changeSlide(jQuery(this).index());
		return false;
	});

</script>



