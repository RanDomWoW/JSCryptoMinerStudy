function frmSearchProcess()
{
	var errorString = '';
	if(document.getElementById('bType').value == '')
	{
		errorString += '-> Please Select Product type.\n';
	}
	else
	{
		if(document.getElementById('frmManu').value == '1')
		{
			if(document.getElementById('manufactID').value == '')
			{
				errorString += '-> Please Select Manufacturer.\n';
			}
			
			if(document.getElementById('modelID').value == '')
			{
				errorString += '-> Please Select Model.\n';
			}
			
			if(document.getElementById('stateID').value == '')
			{
				errorString += '-> Please Select State.\n';
			}
			
			if(document.getElementById('frmSearchCity_Wheeler').value == '')
			{
				errorString += '-> Please Select City.\n';
			}
		} 
		else if(document.getElementById('frmManu').value != '1')
		{
			if(document.getElementById('capacity').value == '')
			{
				errorString += '-> Please Select Capacity.\n';
			}
			
			if(document.getElementById('stateID').value == '')
			{
				errorString += '-> Please Select State.\n';
			}
			
			if(document.getElementById('frmSearchCity_Wheeler').value == '')
			{
				errorString += '-> Please Select City.\n';
			}
		}
		
		
	}
	if(errorString != '')
	{
		alert(errorString);
		return false;
	}
	else
	{
		if(document.getElementById('frmManu').value == '1')
		{
			return true;
		}
		else if(document.getElementById('bType').value == '2')
		{
			document.frm.action = 'search_inverter_result.php';
		}
		else if(document.getElementById('bType').value == '3')
		{
			document.frm.action = 'search_inverter_result.php';
		} else {
			var vbType = $("#bType").val();
			var vcapcity = $("#capacity").val();
			var name = $('#bType option:selected').text();
			var stateCity = $("#stateID").val()+'-'+$("#frmSearchCity_Wheeler").val();
			var url= 'product.php?name='+name+'&proID='+vbType+'&orderBy=asc&min_val=0&max_val=99999&capcity='+vcapcity+'&manufacture=0-0&state='+stateCity+'&type=filter&frm=1';	
			//alert(url);
			document.location.href = url;
			return false;
		}
	}
}

