<script type="text/javascript">
    $(document).ready(function() {
        $('.call-me').click(function () {
            if ($('#call-me').length === 0) {
                $.ajax({
                    type: "GET",
                    url: '/home/call-me',
                    success: function(data) {
                        $(data).insertAfter('#footer');

                        validateAjaxForm("form-call-me", "");
                        /* 
                        * VisualCaptcha.NET
                        * @ Stéfane Lavergne
                        */
                        var callMeEl = $('#call-me-captcha').visualCaptcha({
                            imgPath: '/Content/visual-captcha/img/',
                            captcha: {
                                numberOfImages: 5,
                                routes: { start: "/captcha/start", image: "/captcha/image", audio: "/captcha/audio" }
                            }
                        });

                        // use next code for getting captcha object
                        callMeEl.data('captcha');

                        $('#call-me').modal('show');

                    },
                    error: function() {
                    }
                });
            } else {
                $('#call-me').modal('toggle');
            }

        });
    });
</script>
