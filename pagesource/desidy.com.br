    <script>
        function atualizarDestaques() {
            $.ajax({
                type: 'GET',
                url: '/Home/VagasDestaque',
                dataType: 'html',
                cache: false,
                async: true,
                success: function (data) { $('#vagas').html(data); }
            });
        }

        function atualizarEmpresas() {
            $.ajax({
                type: 'GET',
                url: '/Home/EmpresasDestaque',
                dataType: 'html',
                cache: false,
                async: true,
                success: function (data) { $('.partners').empty().html(data); }
            });
        }

        $(document).ready(function () {
            atualizarDestaques();
            setInterval(atualizarDestaques, 3000);

            $('#popBanner').modal('show');
            //atualizarEmpresas();
            //setInterval(atualizarEmpresas, 3000);
        });
    </script>

