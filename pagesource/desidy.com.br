    <script src="/js/app.js" type="text/javascript"></script>
    <script>
        var map;
        $(document).ready(function () {
            App.init();
            $('.cnpj').mask('00.000.000/0000-00');
            $('.cpf').mask('000.000.000-00');
            $('.datahora').mask('00/00/0000');
            $('.decimal').mask("#.##0,00", { reverse: true });
            $('.inteiro').mask("000");

            $('#funcao').autocomplete({
                serviceUrl: '/Utils/GetFuncoes',
                type: 'POST',
                onSelect: function (suggestion) {
                    $("#funcaoId").val(suggestion.data);
                }
            });

            $('#localizacao').autocomplete({
                serviceUrl: '/Utils/GetCidadesAC',
                type: 'POST',
                onSelect: function (suggestion) {
                    $("#localizacaoId").val(suggestion.data);
                }
            });

            $('#emp_funcao').autocomplete({
                serviceUrl: '/Utils/GetFuncoes',
                type: 'POST',
                onSelect: function (suggestion) {
                    $("#emp_funcaoId").val(suggestion.data);
                }
            });

            $('#emp_localizacao').autocomplete({
                serviceUrl: '/Utils/GetCidadesAC',
                type: 'POST',
                onSelect: function (suggestion) {
                    $("#emp_localizacaoId").val(suggestion.data);
                }
            });

            //Candidatar-se
            $(document).on("click", ".candidatar_vaga", function () {
                var botao = $(this);
                var codigo = $(this).attr("id").replace("candidatar", "");
                $.ajax({
                    type: 'POST',
                    url: '/Vagas/Candidatar',
                    data: {
                        id: codigo
                    },
                    cache: false,
                    success: function (dados) {
                        botao.next('div').find(".well").empty().html(dados);
                    }
                });
            });

            $(".oculto").hide();
            $(".inf").click(function (event) {
                event.preventDefault();
                var nodo = $(this).attr("href");

                $(".nav-detail").show();

                if (nodo == "#painelcandidatos") {
                    $(".nav-detail").addClass("azul-bg");
                }
                else {
                    $(".nav-detail").removeClass("azul-bg");
                }

                /*if ($(nodo).is(":visible")) {
                    $(nodo).hide();
                    return false;
                } else {
                    $(".oculto").hide("slow");
                    $(nodo).fadeToggle("fast");
                    return false;
                }*/

                if (!$(nodo).is(":visible")) {
                    $(".oculto").hide("slow");
                    $(nodo).fadeToggle("fast");
                    return false;
                }
            });

                 $("#painelvagas").show();
                 $(".nav-detail").removeClass("azul-bg");


            $(".carousel-inner .item").first().addClass("active");

            $(document).on("change", "#Password, .datahora", function () {
                var parts = $(this).val().split("/");

                if (parts[2].length > 2)
                    ano = parts[2];
                else if (parseInt(parts[2]) > 50)
                    ano = "19" + parts[2];
                else
                    ano = "20" + parts[2];

                $(this).val(parts[0] + "/" + parts[1] + "/" + ano);
            });
        });
    </script>
    
