	<script type="text/javascript">
	    jQuery(document).ready(function($) {
	        var count = 2;
	        var total = 0;
	        var category_id = '';
	        $(window).scroll(function(){
	                if  ($(window).scrollTop() == $(document).height() - $(window).height()){
	                   if (count > total){
	                   	  	return false;
	                   }else{
	                   		loadArticle(count, category_id);
	                   }
	                   count++;
	                }
	        });

	        function loadArticle(pageNumber, cat_id){	        		$('a#inifiniteLoader').show('fast');
	                $.ajax({
	                    url: "http://nure.ua/wp-admin/admin-ajax.php",
	                    type:'POST',
	                    data: "action=infinite_scroll&page_no="+ pageNumber + '&loop_file=loop&categor_id='+cat_id,
	                    success: function(html){
	                    	$('a#inifiniteLoader').hide('slow');
	                        $(".inner_contents_block").append(html);    // This will be the div where our content will be loaded
	                    }
	                });
	            return false;
	        }

	    });

	</script>


