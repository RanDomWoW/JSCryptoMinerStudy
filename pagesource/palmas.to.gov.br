<script>
        $(function() {
            // Function responsible for init the news carousel - by: Thyago Neves
            $("#carousel-news").carouFredSel({
                height: 384,
                width: "100%",
                direction: "left",
                items: {
                    visible: 1,
                    minimum: 1,
                    width: 673
                },
                scroll: {
                    fx: "crossfade",
                    easing: "easeInOutSine",
                    duration: 700
                },
                auto: {
                    timeoutDuration: 6000,
                    pauseOnHover: true,
                    progress: "#bar"
                },
                pagination: {
                    container: '#carousel-selector',
                    event: 'click',
                    pauseOnHover: true,
                    numItemName: 'data-number',
                    anchorBuilder: function(nr) {
                        var src = $(this).children().attr('url-miniatura').replace('682x384', '143x80').replace('682x383', '143x80');
                        src = src.replace('sem-foto.jpg', 'sem-foto.small.jpg');
                        var numitem = $(this).children().attr('data-number');
                        return '<img data-number="'+ numitem +'" src="'+ src +'" border="0" />';
                    }
                },
                messages: {
                    container: '#carousel-summary-news',
                    msgSelected: 'news'
                }
            });

        });

    </script>


            
            

