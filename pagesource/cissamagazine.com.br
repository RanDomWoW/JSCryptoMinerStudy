                                            <script type="text/javascript">
                            (function () {
                                'use strict';
                                
                                setTimeout(function(){
                                    $('.local-pesquisa .selected').trigger('click');
                                },1000);

                                $('body').on('click','.local-pesquisa .title-selected',function(event){
                                    event.stopPropagation();

                                    var $this = $(this);
                                    $this.toggleClass('selected');

                                    if(($this).hasClass('selected')){
                                        $('.local-pesquisa .options').show().stop(true).animate({
                                            opacity: 1
                                        });
                                    }else{
                                        $('.local-pesquisa .options').stop(true).animate({
                                            opacity: 0
                                        }, {
                                            complete: function () {
                                                $(this).hide();
                                            }
                                        });
                                    }
                                });

                                $('.local-pesquisa .options span').on('click',function(){
                                    var $this = $(this);

                                    $('.options span').removeClass('selected');
                                    $this.addClass('selected');

                                    $('.local-pesquisa .title-selected .content').html($this.text());
                                    $('#frm-search').attr('action', $this.data('tipo-pesquisa'));
                                });

                                $('body').on('click', function () {
                                    var $title = $('.local-pesquisa .title-selected');

                                    if($title.hasClass('selected')){
                                       $title.toggleClass('selected');
                                       $('.local-pesquisa .options').stop(true).animate({
                                            opacity: 0
                                        }, {
                                            complete: function () {
                                                $(this).hide();
                                            }
                                        });
                                    }
                                });
                            })();
