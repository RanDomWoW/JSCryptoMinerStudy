<script>
(function() {

  var NEXT_SLIDE = -1,
      PREVIOUS_SLIDE = -2;

  var $slider = $('.home-slider'),
      $slides = $slider.find('.slide');

  var current = null,
      count   = $slides.length;

  function showSlide(n) {

    if (n === current) {
      return;
    } else if (n === NEXT_SLIDE) {
      n = current + 1;
    } else if (n === PREVIOUS_SLIDE) {
      n = current - 1;
    }

    if (n < 0) n = count - 1;
    if (n > count - 1) n = 0;

    $slides.not($slides.eq(n)).removeClass('active')
    $slides.eq(n).addClass('active');

    current = n;
  }

  function closeVideo() {
    $('.full-screen-video').remove();
  }

  function openVideo(sources) {
    closeVideo();

    $wrapper = $('<div/>')
                 .addClass('full-screen-video')
                 .append('<video controls loop/>')
                 .append('<i class="icon icon-remove close-video"/>')
                 .appendTo(document.body);

    $wrapper.on('click', '.close-video', closeVideo);

    var $video = $wrapper.find('video');

    sources = sources.split(',');

    for (var i = 0, l = sources.length, source, $source, url, type; i < l; i++) {
      if (source = sources[i].split(';')) {
        $source = $('<source/>').attr('src', source[0]);
        if (source.length > 1) {
          $source.attr('type', source[1]);
        }
        $source.appendTo($video);
      }
    }

    $video[0].play();
  }

  $slider.find('.video').on('click', function() {
    openVideo($(this).data('src'));
  });

  showSlide(Math.round(Math.random() * count));

  if (count > 1) {
    var $prev = $('<a class="prev">Anterior</a>')
                  .appendTo($slider),
        $next = $('<a class="next">Siguiente</a>')
                  .appendTo($slider);

    $prev.on('click', showSlide.bind(this, PREVIOUS_SLIDE));
    $next.on('click', showSlide.bind(this, NEXT_SLIDE));
  }

})();
</script>
