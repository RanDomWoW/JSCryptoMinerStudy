  var $switcher = $('');

  $('#home-browse .switcher a').on('click', function(ev) {
    var $this = $(this),
        active = $this.hasClass('active'),
        $target = $('#' + $this.attr('href').substr(1));

    $this.toggleClass('active');
    $target.toggleClass('active');

    if (active) {
      $this.closest('.switcher').find('a').removeClass('active');
      $this.closest('#home-browse').find('.links').removeClass('active');
    } else {
      $this.closest('.switcher').find('a').not($this).removeClass('active');
      $this.closest('#home-browse').find('.links').not($target).removeClass('active');
    }

    ev.preventDefault();
  });
});
