                    $j(f).append(html);
                    $j(input_id).focus();
                } else {
                    $j('#mce-'+resp.result+'-response').show();
                    $j('#mce-'+resp.result+'-response').html(msg);
                }
            }
        } catch(e){
            $j('#mce-'+resp.result+'-response').show();
