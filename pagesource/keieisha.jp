<script>
	$(document).ready(function(){
		myresize();

		$(window).resize(function() {
			myresize();
		});

		function myresize(){
			var nY = $(window).height() - $("#headerWrap").height()-30;
			var nX = $(window).width();
			
			var rat_win = nX/nY;
			var rat_video = 1045/588;
			
			if(rat_video>rat_win) {
				$(".top_video").addClass("top_video02");
			}
			else {
				$(".top_video .videodiv").css({"height":nY});
				$(".top_video").removeClass("top_video02");
			}
			
			var nY_slide = $(window).height() - $("#headerWrap").height()+10;
			var txt_top = Math.ceil((nY_slide - 258)/2);
			$(".wrapslide").css({"height":nY_slide});
			$(".wrapslide li").css({"height":nY_slide});
			$(".wrapslide li .txt01").css({"padding-top":txt_top});
		}
		
		$('.wrapslide').find('li').each(function(){
			$(this).animate({"opacity": 0});
			$(this).find(".txtWrap").animate({"opacity": 0});

			if($(this).hasClass('active')){
				$(this).animate({"opacity": 1});
				$(this).find(".txtWrap").addClass('txtshow');
				$(this).find(".txtWrap").animate({"opacity": 1},4000);
			}
		});


		var array1 = [];
		$('.wrapslide').find('li').each(function(){
			array1.push($(this).attr('id'));
		});
		var lenarr = array1.length ;
		setInterval(function(){
			for(var i = 0 ; i < lenarr ; i++)	{
				var j = i+1;
				$('.wrapslide').find('li').each(function(){
					if($(this).attr('id') == array1[i]){
						if($(this).hasClass('active')){
							$(this).animate({"opacity": 0},500,function() {
								$(this).removeClass('active');
								$(this).find(".txtWrap").removeClass('txtshow');
								$(this).find(".txtWrap").animate({"opacity": 0},500)
								if(j >= lenarr){
									$(this).parent().children('li:first-child').addClass('active');
									$(this).parent().children('li:first-child').animate({"opacity": 1},1200,function() {
										$(this).find(".txtWrap").addClass('txtshow');
										$(this).find(".txtWrap").animate({"opacity": 1},1200)
									});
								}else{
									$(this).next('#'+array1[j]).addClass('active');
									$(this).next('#'+array1[j]).animate({"opacity": 1},1200,function() {
										$(this).find(".txtWrap").addClass('txtshow');
										$(this).find(".txtWrap").animate({"opacity": 1},1200)
									});
								}
							});

							i = lenarr;
						}
					}

				});
			}
		},8000);
		
	});
</script>
<script>
	function setCookie(cname, cvalue, exdays) {
		var d = new Date();
		d.setTime(d.getTime() + (exdays*24*60*60*1000));
		var expires = "expires="+d.toUTCString();
		document.cookie = cname + "=" + cvalue + "; " + expires;
	}
	function getCookie(cname) {
		var name = cname + "=";
		var ca = document.cookie.split(';');
		for(var i = 0; i < ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0) == ' ') {
				c = c.substring(1);
			}
			if (c.indexOf(name) == 0) {
				return c.substring(name.length, c.length);
			}
		}
		return "";
	}
	function runck(){
		var cname = "testck";
		var cvalue = "https://www.keieisha.jp/common/video/kjp_movie02.mp4";
		var exdays = "1521698787";
		var used = getCookie(cname);
		var link_video = "common/video/kjp_movie02.mp4";
		var array_link = new Array();
		array_link["A"] = "https://www.keieisha.jp/common/video/kjp_movie01.mp4";
		array_link["B"] = "https://www.keieisha.jp/common/video/kjp_movie02.mp4";
		if (used != "") {
			if(used == "A"){
				$(".top_video .videodiv video").attr('src',array_link["B"]);
				cvalue = "B";
				setCookie(cname,cvalue,exdays);
			}else{
				$(".top_video .videodiv video").attr('src',array_link["A"]);
				cvalue = "A";
				setCookie(cname,cvalue,exdays);
			}
		} else {
			cvalue = "B";
			setCookie(cname,cvalue,exdays);
		}
	}
	runck();
</script>

