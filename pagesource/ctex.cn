<style type="text/css">
.zbx_wrapper {width:474px; height:173px; border:1px solid #0879c4;padding:0;margin:0 auto; }
.zbx_wrapper img{width:474px; height:173px;border:0;margin:0; padding:0;}
/* tmall focus */
#zbx_focus {width:474px; height:173px; overflow:hidden; position:relative;}
#zbx_focus ul {height:173px; position:absolute;list-style:none;margin:0; padding:0;}
#zbx_focus ul li {float:left;list-style:none;margin:0; padding:0;width:474px; height:173px; overflow:hidden; position:relative; background:#e1e1e1;}
#zbx_focus ul li div {position:absolute; overflow:hidden;}
#zbx_focus ul li .undis{ display:block;}
#zbx_focus .btnBg {position:absolute; width:474px; height:24px; right:0; bottom:0;  /*background:#000;*/}
#zbx_focus .btn {position:absolute; height:20px; right:0; bottom:4px; padding-right:5px;}
#zbx_focus .btn span {display:inline-block; _display:inline; _zoom:1;background:#fff; width:20px; height:20px; line-height:20px; text-align:center; font-size:14px; /*font-family:"Microsoft YaHei",SimHei;*/ margin-right:5px; cursor:pointer; font-weight:bold;color:#333;}
#zbx_focus .btn span.on {background:#ea1600; color:#fff;font-weight:bold;}
</style>

<script src="/js/jquery/jquery-1.6.min.js"></script>
<script type="text/javascript">
$(function() {
	var sWidth = $("#zbx_focus").width(); //获取焦点图的宽度（显示面积）
	var len = $("#zbx_focus ul li").length; //获取焦点图个数
	var index = 0;
	var picTimer;
	
	//以下代码添加数字按钮和按钮后的半透明长条
	var btn = "<div class='btnBg'></div><div class='btn'>";
	for(var i=0; i < len; i++) {
		btn += "<span>" + (i+1) + "</span>";
	}
	btn += "</div>"
	$("#zbx_focus").append(btn);
	$("#zbx_focus .btnBg").css("opacity",0.5);
	
	//为数字按钮添加鼠标滑入事件，以显示相应的内容
	$("#zbx_focus .btn span").mouseenter(function() {
		index = $("#zbx_focus .btn span").index(this);
		showPics(index);
	}).eq(0).trigger("mouseenter");
	
	//本例为左右滚动，即所有li元素都是在同一排向左浮动，所以这里需要计算出外围ul元素的宽度
	$("#zbx_focus ul").css("width",sWidth * (len + 1));
	
	//鼠标滑入某li中的某div里，调整其同辈div元素的透明度，由于li的背景为黑色，所以会有变暗的效果
	$("#zbx_focus ul li div").hover(function() {
		$(this).siblings().css("opacity",0.7);
	},function() {
		$("#zbx_focus ul li div").css("opacity",1);
	});
	
	//鼠标滑上焦点图时停止自动播放，滑出时开始自动播放
	$("#zbx_focus").hover(function() {
		clearInterval(picTimer);
	},function() {
		picTimer = setInterval(function() {
			if(index == len) { //如果索引值等于li元素个数，说明最后一张图播放完毕，接下来要显示第一张图，即调用showFirPic()，然后将索引值清零
				showFirPic();
				index = 0;
			} else { //如果索引值不等于li元素个数，按普通状态切换，调用showPics()
				showPics(index);
			}
			index++;
		},3000); //此3000代表自动播放的间隔，单位：毫秒
	}).trigger("mouseleave");
	
	//显示图片函数，根据接收的index值显示相应的内容
	function showPics(index) { //普通切换
		var nowLeft = -index*sWidth; //根据index值计算ul元素的left值
		$("#zbx_focus ul").stop(true,false).animate({"left":nowLeft},500); //通过animate()调整ul元素滚动到计算出的position
		$("#zbx_focus .btn span").removeClass("on").eq(index).addClass("on"); //为当前的按钮切换到选中的效果
	}
	
	function showFirPic() { //最后一张图自动切换到第一张图时专用
		$("#zbx_focus ul").append($("#zbx_focus ul li:first").clone());
		var nowLeft = -len*sWidth; //通过li元素个数计算ul元素的left值，也就是最后一个li元素的右边
		$("#zbx_focus ul").stop(true,false).animate({"left":nowLeft},500,function() {
			//通过callback，在动画结束后把ul元素重新定位到起点，然后删除最后一个复制过去的元素
			$("#zbx_focus ul").css("left","0");
			$("#zbx_focus ul li:last").remove();
		}); 
		$("#zbx_focus .btn span").removeClass("on").eq(0).addClass("on"); //为第一个按钮添加选中的效果
	}
});

</script>

