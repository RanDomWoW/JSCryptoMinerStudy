		$('#article-view-content-div').on('copy', function(e){			
			var sel = window.getSelection();
			var copyFooter = "<br/><br/>출처 : <a href='" + document.location.href + "'>" + "BIBLIA(http://www.biblia.co.kr)" + "</a><br/>";
