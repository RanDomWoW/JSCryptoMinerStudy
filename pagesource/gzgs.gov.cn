<script type="text/javascript">
function checkValue(){
	var searchKey=document.getElementById("searchKey").value;
	if(!searchKey){
		alert("请输入关键字！");
		return false;
	}
	var patrn=/[`~!@#$%^&*()_+<>?:"{},.\/;'[\]]/g;
	if(patrn.test(searchKey)){  
        alert("您输入的关键字中包含有非法字符，请重新输入！");  
        document.getElementById("searchKey").value="";
        return false;     
    }
	return true;
}
</script>
