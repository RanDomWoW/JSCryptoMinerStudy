<script type="text/javascript">
	$(function(){//替换文本中的空格
		ReplacementMethod("li.box-1 a");//普通文本的
		ReplacementMethod("td.tBg a");//政务公开下的
	});
	function ReplacementMethod(objects){
		$(objects).each(function(index) {
			var test = $(this).html();
            var tit = $(this).attr("title");
			test = test.replace(/(\n)/g, ""); 
			test = test.replace(/(\t)/g, ""); 
			test = test.replace(/(\r)/g, ""); 
			test = test.replace(/<\/?[^>]*>/g, ""); 
			test = test.replace(/\s*/g, "");
			tit = tit.replace(/(\n)/g, ""); 
			tit = tit.replace(/(\t)/g, ""); 
			tit = tit.replace(/(\r)/g, ""); 
			tit = tit.replace(/<\/?[^>]*>/g, ""); 
			tit = tit.replace(/\s*/g, "");
			$(this).html(test);
			$(this).attr("title",tit);
        });
	}
</script>
