<script type="text/javascript">
	
	String.prototype.trim = function() {
		return this.replace(/(^\s*)|(\s*$)/g, "");
	}
function refocus() {
		var getquery = document.getElementById("keyword");
		getquery.value = "";
		getquery.autocomplete = "off";
		getquery.focus();

	}

	function reblur() {
		var getquery = document.getElementById("keyword");
		if(getquery.value == "") {
			getquery.value = "请输入关键字";
		}
	}
	function sub() {
		//必需，先将文本转换为utf-8
		document.charset = "utf-8";
		var fixvalue = document.getElementById("keyword").value;
		fixvalue = fixvalue.replace(/\~|\!|\*|\?|\^|\\|\(|\)|\[|\]|\+|\-|\:|\{|\}|\"/g, "");
		if(fixvalue.trim() == "") {
			document.getElementById("keyword").value = "";
			alert("关键字不能为空！");
			return false;
		} else if(fixvalue.trim() == "请输入关键字"){
			alert("请输入有效的关键字搜索！");
			return false;
		} else {
			window.open('/search.html?keyword=' + encodeURI(fixvalue)); //传入参数
		}
		return false;
	}
	//回车提交方法
	function EnterSub() {
		var len = arguments.length;
		var event;
		if(len == 1) {
			event = arguments[0];
		}
		var evt = event ? event : (window.event ? window.event : null);
		if(evt.keyCode == 13) {
			//必需，先将文本转换为utf-8
			document.charset = "utf-8";
			var fixvalue = document.getElementById("keyword").value;
			fixvalue = fixvalue.replace(/\~|\!|\*|\?|\^|\\|\(|\)|\[|\]|\+|\-|\:|\{|\}|\"/g, "");
			if(fixvalue.trim() == "") {
				document.getElementById("keyword").value = "";
				alert("关键字不能为空！");
				return false;
			} else {
				window.open('/search.html?keyword=' + encodeURI(fixvalue)); //传入参数
			}
			return false;
		}
	}
</script>
