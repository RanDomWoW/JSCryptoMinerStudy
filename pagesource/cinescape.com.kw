                jQuery("#now-showing").removeClass('in');
                jQuery("#now-showing").removeClass('active');
                jQuery("#coming-soon").addClass('active');
                jQuery("#coming-soon").addClass('in');
                //jQuery('.novo-mobileupdate').fadeIn(500);
            }
            if (tech == 'promotions') {
                setTimeout(function () {
                    $('li[rel=Tab-Promotions] a').trigger("click");
                }, 200)
            }
            jQuery(".cc-sitesearch").click(function () {
                if (jQuery(window).width() < 992) {
                    jQuery(".cc-menutrigger").css('z-index', '9');
                }
                jQuery(".cc-search-blk").toggleClass('in');
                jQuery(".cc-sitesearch").hide();
                if (jQuery(".cc-search-blk").hasClass('in')) {
                    jQuery(".cc-search-blk .formgroup").addClass('open');
                    setTimeout(function () {
                        jQuery(".cc-search-blk .cc-search-list").addClass('down');
                    }, 500);
                } else {
                    jQuery(".cc-search-blk .cc-search-list").removeClass('down');
                    setTimeout(function () {
                        jQuery(".cc-search-blk .formgroup").removeClass('open');
                    }, 1500);
                }
            });
            jQuery(".search-close").click(function () {
                if (jQuery(window).width() < 992) {
                    jQuery(".cc-menutrigger").css('z-index', '1004');
                }
                jQuery(".cc-search-blk").removeClass('in');
                jQuery(".cc-sitesearch").show();
            });

            jQuery("body").click(function () {
                setTimeout(function () {
                    if (jQuery(window).width() < 992) {
                        jQuery(".cc-menutrigger").css('z-index', '1004');
                    }
                    jQuery(".cc-search-blk").removeClass('in');
                    jQuery(".cc-sitesearch").show();
                }, 100);

                setTimeout(function () {
                    jQuery(".cc-search-blk .formgroup").removeClass('open');
                }, 100);
                jQuery(".cc-search-blk").removeClass('down');
            });

            jQuery(".cc-search-blk, .cc-sitesearch").click(function (e) {
                e.stopPropagation();
            });

            /*Language Option*/
            jQuery(".cc-lang-options li.cc-english").click(function () {
                jQuery(".cc-lang-options li.cc-arabic").slideToggle('500');
            });
            jQuery("body").click(function () {
                setTimeout(function () {
                    jQuery(".cc-lang-options li.cc-arabic").hide('300');
                    jQuery(".cc-lang-options li.cc-english").show();
                }, 100);
            });

            jQuery(".cc-lang-options li.cc-english, .cc-lang-options li.cc-english").click(function (e) {
                e.stopPropagation();
            });

            jQuery(window).resize(function () {
                if (jQuery(".cc-search-blk").hasClass('in')) {
                    if (jQuery(window).width() < 992) {
                        console.log('x');
                        jQuery(".cc-menutrigger").css('z-index', '9');
                    } else {
                        console.log('y');
                        jQuery(".cc-menutrigger").css('z-index', '1004');
                    }
                }
            });
        });
    </script>
