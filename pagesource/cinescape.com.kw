        var delete_cookie = function (name) {
            document.cookie = name + '=;expires=Thu, 01 Jan 1970 00:00:01 GMT;path=/';
        };
        function movieDetails(movieId, movieName) {
            $('.ajax-loader-block').fadeIn();
            // cinema = "All";
            delete_cookie('movieflow');

            // note the comma in the alert shows separate phrases
            cinema = "";

            date = "";

            window.location.href = '/movie-details/' + movieId + '/' + movieName;
            $('.ajax-loader-block').fadeOut();


        }

        function ComingSoonPageMaster(movieId, movieName) {
            var cinemaId = "";
            var date = "cs";
            //var postdata = {
            //    cinemaid: cinemaId,
            //    date: date,
            //    movieid: movieId,
            //    type: "cs",
            //}
            $.ajax({
                type: "POST",
                url: '/Quickbook/MovieDetails',
                // data:postdata,
                contentType: "application/json; charset=utf-8",
                data: "{\"cinemaid\":\"" + cinemaId + "\",\"movieid\":\"" + movieId + "\",\"date\":\"" + date + "\",\"type\":\"" + date + "\"}",

                dataType: "json",
                success: function (data) {
                    if (data != "") {
                        window.location.href = '/movie-details/' + movieId + '/' + movieName;

                    }
                    else {
                        window.location.href = '/movie-details/' + movieId + '/' + movieName;
                        $('.ajax-loader-block').fadeOut();
                    }
                },
                error: function (data) {
                }
            });


        }
