     //滚动数字
     var options = {
       useEasing : true,
       useGrouping : true,
       separator : ',',
       decimal : '.',
       prefix : '',
       suffix : ''
     };
     var dd1=document.getElementById("tra-data").innerHTML;
     var dd2=document.getElementById("user-earn-int").innerHTML;
     var dd3=document.getElementById("cum-tra-time").innerHTML;

     var demo = new CountUp("tra-data", 0, dd1, 0, 2.5, options);
