<script type="text/javascript">jQuery(document).ready(function () {
$(".btn-save-company").click(function(){
    $.ajax({
        type : 'post',
        url : '/saas/sites/add',
        data : $(".form-key-customer").serialize(),
        dataType : "json",
        success : function (data) {
            if (data.errno == '0') {
                $('#addNewenter').modal('toggle');
                $(".form-key-customer input").val("");
                $("#accountinfo-remark").val("");
                alert(data.error);
            }  else {
                alert(data.error);
            }
        }
    });
});

$('#accountinfo-verifycode').on('keyup', function () {
    var _this = $(this);
    var input_code = _this.val();
    if (input_code.length == 4) {
        $.ajax({
            url: '/saas/site/checkcaptcha',
            dataType: 'json',
            type: 'get',
            data: {verifycode: input_code},
            success: function(res) {
                if (res.errno != 0) {
                    $(".field-accountinfo-verifycode").addClass("has-error").find(".help-block").empty().html("图片验证码不正确");
                    _this.val('');
                } else {
                    $(".field-accountinfo-verifycode").removeClass("has-error").find(".help-block").empty();
                }
            }
        })
    } else if (input_code.length > 4) {
        _this.val('');
    } else if (input_code.length < 4) {
        $(".field-accountinfo-verifycode").addClass("has-error").find(".help-block").empty().html("图片验证码不正确");
    }
})

$("#get-mobile").on('click', function () {
    var phone = $("#accountinfo-mobile").val();
    var verifycode = $('#accountinfo-verifycode').val();
    var rule = /^(1[3|4|5|8|7][0-9])\d{8}$/;
    var t = 60;

    if (phone != 'undefined' && phone != '' && rule.test(phone)) {
        $(".field-accountinfo-mobile").removeClass("has-error").find(".help-block").empty();
        if ($("#get-mobile").hasClass("stop")) {
            return false;
        }
    } else {
        $(".field-accountinfo-mobile").addClass("has-error").find(".help-block").empty().html("请输入正确的手机号码");
        return;
    }

    if (verifycode == '' || verifycode == 'undefined') {
        $(".field-accountinfo-verifycode").addClass("has-error").find(".help-block").empty().html("请输入正确的图片验证码");
        return;
    }

    $.ajax({
            url : '/saas/site/ajaxsendmobilecode',
            type : 'get',
            dataType : 'json',
            data : {
                phone : phone,
                verifycode : verifycode
            },
            success : function (data) {
                if (data.errno == 0) {
                    $("#accountinfo-mobile").attr("readonly", "true");
                    var int = setInterval(function () {
                        t--;
                        if(t < 0){
                            clearInterval(int);
                            $("#get-mobile").html("获取短信验证码");
                            $("#accountinfo-mobile").removeAttr("readonly");
                            $("#get-mobile").removeClass("stop");
                        } else {
                            var str = "重新获取("+ t +")s";
                            $("#get-mobile").html(""+ str +"");
                            $("#get-mobile").addClass("stop");
                        }
                    },1000);
                    return false;
                } else if (data.errno == 1) {
                    $('#register-notice').modal('toggle');
                    $('#register-notice').find(".modal-body").empty().text("手机格式错误");
                } else if (data.errno == 4) {
                    $('#register-notice').modal('toggle');
                    $('#register-notice').find(".modal-body").empty().text("获取短信验证码过快，请稍后再尝试获取");
                } else if (data.errno == 5) {
                    $('#register-notice').modal('toggle');
                    $('#register-notice').find(".modal-body").empty().text("图片验证码错误，请输入图片中的验证码");
                } else {
                    $('#register-notice').modal('toggle');
                    $('#register-notice').find(".modal-body").empty().text("发送失败");
                }
            },
            error : function (data) {
                $('#register-notice').modal('toggle');
                $('#register-notice').find(".modal-body").empty().text("短信验证码下发失败，请重试");
            }
        });
});
