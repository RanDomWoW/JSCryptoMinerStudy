		<script>
		   var myApp = angular.module('myApp',[]);
		   myApp.controller('myctlr',function($scope,$http){
		        $("#otp").css("display", "none");
		        $("#password").css("display", "none");
		        
		        $scope.checkuser = function($event){
		            
		           $event.preventDefault();
		            
		           mobile   = $scope.mobile;
		           password = $scope.password;
		           otp      = $scope.otp;
		           
		         $("#msg").css("display", "none");
		     
		      if($("#btn_gen_otp").hasClass( "mob" ) === true && mobile === undefined){
		             $("#msg").css("display", "block");
		             $scope.message = "Enter Registered Mobile Number !!";
		             return false;
		       }
		         
		       if($("#btn_gen_otp").hasClass( "otp" ) === true && (otp === undefined || otp === '')){
		             $("#msg").css("display", "block");
		             $scope.message = "Enter OTP Value !!";
		             return false;
		       }
		      
		       
		       if($("#btn_gen_otp").hasClass( "password" ) === true && password === undefined){
		             $("#msg").css("display", "block");
		             $scope.message = "Enter Password Value !!";
		             return false;
		         }
		      
		       
		           $http.post("http://kdcampus.online/index.php/Login_user/forgetpass",{'mob':mobile,'otp':otp,'pass':password}).success(function(data,success){
		           //console.log(data);
    		        if(data != ''){
    		           
    		                
    		           $scope.showTheForm="hideByDefault";
    		           if(mobile != '' && password == undefined && otp == undefined){
    		               userid = data.mobileno;
    		               $("#mobile").css("display", "none");
    		               $("#btn_gen_otp").removeClass("mob");
    		               $("#otp").css("display", "block");
    		               $("#btn_gen_otp").addClass("otp");
    		               //$scope.mobile = undefined; 
    		           } else if(otp != '' && mobile != undefined && password == undefined){
    		               
    		               $("#btn_gen_otp").removeClass("otp");
    		               $("#btn_gen_otp").addClass("password");
    		               $("#otp").css("display", "none");   
    		               $("#password").css("display", "block");
    		               $scope.otp = undefined; 
    		               
    		           }else if(password != '' && mobile != undefined && otp == undefined){
    		               
    		               $("#password").css("display", "none");
    		               alert('Password Change Successfull, Login Your Account !');
    		               window.location.href = "http://kdcampus.online/index.php/Login_user";
    		               
    		           } 
    		        }else{
    		              $("#msg").css("display", "block");
    		            if(mobile != '' && password == undefined && otp == undefined){
    		               $scope.message = "This Mobile Number Not Registered !"; 
    		               
    		            }else if(otp != '' && mobile != undefined && password == undefined){
    		               $scope.message = "OTP Incorrect,Please Enter correct OTP !";
    		                $scope.otp = undefined; 
    		            }
    		         }
    		       }); 
    		    
		     }
		 });
 </script>
 <script>
       var updateApp = angular.module('updateApp',['ngMessages']);
     updateApp.controller('updatectlr',function($scope,$http){
       $scope.mobile = "";
       $scope.mobile = "8218241644";
       sendotp = '';
       $scope.readpro = false;
       $scope.updatemobile = function($event){
          $event.preventDefault(); 
          $("#msg").css("display", "none");
          mobile      = $scope.mobile;
          new_mobile  = $scope.new_mobile;
          new_otp     = $scope.otp;
          //alert(new_mobile);
          if(new_otp !== undefined && new_otp != sendotp){
            $scope.message = "OTP Incorrect,Please Enter correct OTP !"; 
            $("#msg").css("display", "block");
            return false;
          }
          
         if(new_mobile === undefined){
            $scope.message = "Enter New mobile Number !!"; 
            $("#msg").css("display", "block");
            return false;
          }
         
         $http.post("http://kdcampus.online/index.php/Login_user/update_mobile",{'old_mob':mobile,'new_mob':new_mobile,'otp':new_otp}).success(function(data,success){
             alert(data.msgg);
             if((data.statuss) === true){
                $scope.readpro = true; 
                sendotp        = data.otp; 
                $("#otp").css("display", "block"); 
              }else if((data.statuss) == 2){
                alert('New Number Successfully Updated,Please Login to your account !!');  
                window.location.href = "http://kdcampus.online/index.php/Login_user";
            }
         });
       }
    });
     
 </script>
	
	
