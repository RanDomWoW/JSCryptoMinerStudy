<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<script>
    menu = false;
    $("#icon").click(function() {
        $("#icon").toggleClass("rotated");
        console.log("rotating butan");
        sidebar = $("#sidebar-wrapper");
        if (!menu) {
            sidebar.css({"left": 0});
            sidebar.show();
            sidebar.animate({"left": '+=250'});
        } else {
            sidebar.animate({"left": '-=250'});
        }
        menu = !menu;

    })
</script>
