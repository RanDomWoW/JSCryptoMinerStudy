function cities() {
jQuery.ajax({
url: "/site/cities?region="+$("#geo-region").val(),
type: "GET",
dataType: "json",
success: function(response) {
if (response.result == 'OK') {
$("#geo-city").find("option").remove();
for(var i in response.cities) {
$("#geo-city").append('<option value="'+i+'">'+response.cities[i]+'</option>');
}
}
},
error: function(response) {
console.log(response);
}
});
}
function setCity() {
var url = $('#set-city-form form').attr('action');
$('#set-city-form').hide();
$('#set-city-saving').show();
jQuery.ajax({
url: url,
type: "POST",
data: $('#set-city-form form').serialize(),
dataType: "json",
success: function(response) {
if (response.result == 'OK') {
location.reload();
} else {
console.log(response);
$('#set-city-saving').hide();
$('#set-city-form').show();
alert('Ошибка выбора города!');
}
},
error: function(response) {
console.log(response);
$('#set-city-saving').hide();
$('#set-city-form').show();
alert('Ошибка выбора города!');
}
});
return false;
}
