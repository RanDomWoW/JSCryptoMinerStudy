<!-- Шаблон формы авторизации -->
<script type="text/x-template" id="auth-form-template">
    <form class="auth-form" method="post">
        <div class="form-group text-center errors" v-if="user_data.auth_error">
            <span class="error-title"><span class="icon-error">&times;</span> Авторизация завершилась неудачей</span>
            <span class="error-text">
                <span v-html="user_data.auth_error_message"></span>
                Если ошибка повторяется, попробуйте восстановить <br> доступ или обратиться к менеджеру магазина.
            </span>
        </div>
        <div class="form-group">
            <input type="text" v-model="user_data.login" name="login" class="form-control" placeholder="Логин или E-mail">
        </div>
        <div class="form-group">
            <input type="password" v-model="user_data.password" name="password" class="form-control" placeholder="Пароль">
        </div>
        <div class="form-group row">
            <div class="col">
                <label>
                <input name="remember_me" v-model="user_data.remember_me"  value="1" type="checkbox">
                Запомнить меня
                </label>
            </div>
            <div class="col text-right">
                <a href="/personal/?forgot_password=yes">Забыли пароль?</a>
            </div>
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-outline-success mx-auto d-block"
                    :disabled="user_data.submit_disabled"
                    @click="checkAuth"
            >Войти</button>
        </div>
    </form>
</script>

<script type="text/javascript">
        var data = {
        user_data: {
            login: '',
            password: '',
            remember_me: '',
            submit_disabled: true,
            auth_error: false,
            auth_error_message: '',
            return_url: '/?authorized=Y'
        }
    };

    Vue.component('auth-form', {
        template: '#auth-form-template',
        data: function () {
            return data
        },
        watch: {
            'user_data.login': function () {
                this.activeButton();
                console.log(this.user_data);
            },
            'user_data.password': function () {
                this.activeButton();
            }
        },
        methods: {
            activeButton: function () {
                if (this.user_data.login.length > 0 && this.user_data.password.length > 0) {
                    this.user_data.submit_disabled = false;
                } else {
                    this.user_data.submit_disabled = true;
                }
            },
            checkAuth: function (event) {
                event.preventDefault();
                var self = this;
                self.user_data.auth_error = false;
                self.user_data.submit_disabled = true;

                axios.post('/local/api/user.php?action=checkLogin', self.user_data)
                    .then(function (response) {
                        if(response.data.error) {
                            self.user_data.auth_error_message = response.data.auth_result.MESSAGE;
                            self.user_data.auth_error = true;
                        } else {
                            // Авторизуем
                            self.user_data.auth_error = false;
                            document.location.href = self.user_data.return_url;
                        }
                        self.user_data.submit_disabled = false;
                    })
                    .catch(function (error) {
                        alert('Что-то пошло не так. Попробуйте ещё раз.');
                        self.user_data.submit_disabled = false;
                    });
            }
        }
    });
    
    var vue = new Vue({
        el: '.wrapper',
        data: function () {
            return data_all;
        },
        watch: {
            'basket.check_all': function (event) {
                for (key in this.basket.items) {
                    this.basket.items[key].check = event;
                }
            },
            'small_search': _.debounce(function (event) {
                var self = this;
                self.small_search_data.founded = false;
                self.show_preloader = true;
                self.search_all_result = '/search/?q=' + event;
                axios.post('/search/?q=' + event + '&ajax=y')
                    .then(function (response) {
                        self.small_search_data = response.data;
                        self.show_preloader = false;
                    })
                    .catch(function (error) {
                        console.log('Data not loaded...');
                        self.show_preloader = false;
                    });
            },
            500),
        },
        created: function () {
            this.page_loaded = true;
            this.fetchData()
        },
        methods: {
            fetchData: function () {
                var self = this;
                self.basket.show_preloader = true;
                axios.post('/local/api/basket.php?action=getBasket')
                    .then(function (response) {
                        self.basket = response.data.basket;
                        self.basket.show_preloader = false;
                    })
                    .catch(function (error) {
                        console.log('Data not loaded...');
                        self.basket.show_preloader = false;
                    });

            },
            addToBasket: function (id, delayed) {

                if(delayed !== undefined) {
                    delayed = true;
                }

                var self = this;
                self.basket.show_preloader = true;

                if(this.basket.delayed[id]) {
                    this.returnToBasket(this.basket.delayed[id].ID)
                    return true;
                }

                axios.post('/local/api/basket.php?action=add', {
                    id: id,
                    quantity: 1,
                    delayed: delayed
                })
                    .then(function (response) {
                        self.basket = response.data.basket;
                        self.basket.show_preloader = false;
                    })
                    .catch(function (error) {
                        console.log('Data not loaded...');
                        self.basket.show_preloader = false;
                    });
            },
            setQuantity: function (id, quantity) {
                var self = this;
                if (quantity < 1) {
                    quantity = 1;
                }
                self.basket.show_preloader = true;
                axios.post('/local/api/basket.php?action=setQuantity', {
                    id: id,
                    quantity: quantity
                })
                    .then(function (response) {
                        self.basket = response.data.basket;
                        self.basket.show_preloader = false;
                    })
                    .catch(function (error) {
                        console.log('Data not loaded...');
                        self.basket.show_preloader = false;
                    });
            },
            // Для инпута, чтобы обновлять с задержкой
            inputQuantity: _.debounce(
                function (id, quantity) {
                    this.setQuantity(id, quantity);
                },
                500),
            deleteFromBasket: function (id) {
                var self = this;
                self.basket.show_preloader = true;
                axios.post('/local/api/basket.php?action=delete', {
                    id: id
                })
                    .then(function (response) {
                        self.basket = response.data.basket;
                        self.basket.show_preloader = false;
                        if(response.data.basket.delayed_count < 1) {
                            self.show_in_basket = 'basket';
                        }
                    })
                    .catch(function (error) {
                        console.log('Data not loaded...');
                        self.basket.show_preloader = false;
                    });
            },
            delay: function (id) {
                var self = this;
                self.basket.show_preloader = true;
                axios.post('/local/api/basket.php?action=delay', {
                    id: id
                })
                    .then(function (response) {
                        self.basket = response.data.basket;
                        self.basket.show_preloader = false;
                    })
                    .catch(function (error) {
                        console.log('Data not loaded...');
                        self.basket.show_preloader = false;
                    });
            },
            addDelay: function (id) { // Откладываем товар или что-то ещё с ним делаем
                if(this.basket.delayed[id]) {
                    this.deleteFromBasket(this.basket.delayed[id].ID)
                } else if (this.basket.items[id]) {
                    this.delay(this.basket.items[id].ID);
                } else {
                    this.addToBasket(id, true);
                }
            },
            returnToBasket: function (id) {
                var self = this;
                self.basket.show_preloader = true;
                axios.post('/local/api/basket.php?action=returnToBasket', {
                    id: id
                })
                    .then(function (response) {
                        self.basket = response.data.basket;
                        if(response.data.basket.delayed_count < 1) {
                            self.show_in_basket = 'basket';
                        }

                        self.basket.show_preloader = false;
                    })
                    .catch(function (error) {
                        console.log('Data not loaded...');
                        self.basket.show_preloader = false;
                    });
            },
            setBasketMode: function (mode) {
                this.show_in_basket = mode;
            },
            clearBasket: function () {
                var self = this;
                self.basket.show_preloader = true;
                axios.post('/local/api/basket.php?action=clearBasket')
                    .then(function (response) {
                        self.basket = response.data.basket;
                        self.basket.show_preloader = false;
                        if(response.data.basket.delayed_count < 1) {
                            self.show_in_basket = 'basket';
                        }
                    })
                    .catch(function (error) {
                        console.log('Data not loaded...');
                        self.basket.show_preloader = false;
                    });
            }
        }
    });

</script>

