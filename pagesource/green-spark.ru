                <div class="body">
                    <div class="results">
                        <preloader :class_name="'preloader'" :inverted="true" v-if="show_preloader"></preloader>
                        <div class="result-item" v-for="item in small_search_data.items">
                            <div class="name">
                                <a :href="item.DETAIL_PAGE_URL" v-html="item.NAME"></a>
                            </div>
                            <div class="photo">
                                <div class="prod-image" :style="{ background: 'url(' + item.PREVIEW_PICTURE.src + ') no-repeat'}"></div>
                            </div>

                            <div class="price">
                                {{item.PRICES.PRICE.DISCOUNT_VALUE}} <i class="fa fa-rub"></i> <br>
                                <svg class="icon-batery"><use :xlink:href="'#' + item.QUANTITY.icon"/></svg>
                            </div>

                            <div class="basket-buttons">
                                <div class="in-basket text-muted" v-if="basket.items[item.ID]">Товар <a
                                            href="/basket/">в корзине</a></div>
                                <div class="quantity-set btn-group" v-if="basket.items[item.ID]">
                                    <div class="btn control minus"
                                         @click="setQuantity(basket.items[item.ID].ID, basket.items[item.ID].QUANTITY - 1)">
                                        <span>&mdash;</span></div>
                                    <input type="text" class="quantity-input form-control"
                                           v-model="basket.items[item.ID].QUANTITY"
                                           @keyup="inputQuantity(basket.items[item.ID].ID, basket.items[item.ID].QUANTITY)"
                                           name="quantity[]" value=""/>
                                    <div class="btn control plus"
                                         @click="setQuantity(basket.items[item.ID].ID, basket.items[item.ID].QUANTITY + 1)">
                                        <span>+</span></div>
                                </div>
                                <div class="in-basket text-muted" v-if="!basket.items[item.ID]"></div>
                                <button @click="addToBasket(item.ID)"
                                        :disabled="basket.show_preloader || item.CAN_BUY == false"
                                        v-if="!basket.items[item.ID]"
                                        type="button"
                                        class="btn btn-outline-success to-basket">В корзину
                                </button>
                            </div>
                        </div>
                    </div>

