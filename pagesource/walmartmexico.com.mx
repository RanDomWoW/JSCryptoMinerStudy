    <script type="text/javascript">
            function headerScroll() {
                var header = document.getElementById("landing-header"),
                    globHead = document.getElementById("global-corp-header"),
                    top = window.getScrollOffsets().y;
                    if( header ){
                        var  headerH = header.offsetHeight;
                        if (top <= headerH) {
                            header.className = header.className.replace(/\bscrolled\b/,'');

                            if (document.documentElement.className.indexOf('ie8') != -1) {
                                if(globHead.className.indexOf('noshow') === -1){
                                    globHead.className += ' noshow' ;
                                }
                            }
                        } else {
                            if(header.className.indexOf('scrolled') === -1){
                                header.className += ' scrolled';
                            }

                            if (document.documentElement.className.indexOf('ie8') != -1) {
                                globHead.className = globHead.className.replace(/\bnoshow\b/,'');
                            }
                        }
                    }
            }
            function getScrollOffsets() {
                // This works for all browsers except IE versions 8 and before
                if ( window.pageXOffset != null )
                   return {
                       x: window.pageXOffset,
                       y: window.pageYOffset
                   };

                // For browsers in Standards mode
                var doc = window.document;
                if ( document.compatMode === "CSS1Compat" ) {
                    return {
                        x: doc.documentElement.scrollLeft,
                        y: doc.documentElement.scrollTop
                    };
                }

                // For browsers in Quirks mode
                return {
                    x: doc.body.scrollLeft,
                    y: doc.body.scrollTop
                };
            }
            window.onscroll = headerScroll;
        </script>
