          var insertText = '<li tabindex="0" data-target="#myCarousel" data-slide-to="' + i + '"></li>';
          // appending items to the list
          $('#myCarousel ol').append(insertText);
          //adding active class to each slide as they come into view
           $( "#myCarousel .carousel-inner .item" ).first().addClass( "active" );
					 $( ".carousel-indicators li" ).first().addClass( "active" );

        }

        //initializing the carousel, 7 seconds per slide, pause on hover
          $('.carousel').carousel({
            interval:17000,
            pause: "hover"
          });

  });
      </script>

