<script type="text/javascript">
function equal_heights(group,extra) {
  group.height("auto");
  var maxHeight = 0;
  group.each(function() {
    if ($(this).height() > maxHeight) {
      maxHeight = $(this).height();
    }
  });
  maxHeight = maxHeight + extra;
  group.height(maxHeight);
}  

function resize_divs() {
// $(".item").display("none");
// $(".active").display("block");
 equal_heights($(".s14_mid"),15);
 equal_heights($(".s14_top"),10);
 equal_heights($(".s14_quote"),10);
 equal_heights($(".s14_mid2"),5);
 equal_heights($(".banner_item"),0);
 equal_heights($(".sign_mid_div"),15);
 equal_heights($(".sign_bot_div"),15);
 }


$(document).ready(function() {
 resize_divs();
});

$(window).resize(function() {
 resize_divs();
});

</script>
        
