<script>
    window.getCookie = function(name) {
        var match = document.cookie.match(new RegExp(name + '=([^;]+)'));
        if (match) {
            return match[1];
        } else {
            return null;
        }
    }

    applicationLanguage = window.getCookie("hoxtonLanguage");
    if (applicationLanguage) {
        document.write('<script src="/scripts/i18n/angular-locale_' + applicationLanguage + '.js"><\/script>');
        document.write('<script src="/scripts/i18n/jquery-ui/datepicker-' + applicationLanguage + '.js"><\/script>');
    } else {
        applicationLanguage = "en";
    }

    $.datepicker.setDefaults($.datepicker.regional[window.applicationLanguage]);
    moment.lang(window.applicationLanguage);
</script>

