                        window.location.href.includes('/meeting-rooms') === false) {

                        var idleSeconds = 0;
                        if (getCookie('idleSeconds')) {
                            idleSeconds = (getCookie('idleSeconds') * 1);
                        }

                        // update idle seconds and check every second
                        var idleTimer = setInterval(function () {

                            if (!idlePopupPaused) {
                                idleSeconds++;
                                document.cookie = "idleSeconds=" + idleSeconds + ";path=/";

                                if (idleSeconds >= popupDelay) {
                                    clearTimeout(idleTimer);
                                    document.cookie = "idleTriggered=true;path=/";

                                    if (chosenVariation === 1) {
                                        //push to data layer if they've not done an availability search
                                        if (!getCookie('reservation')) {
                                            dataLayer.push({
                                                'event': 'twoMinNoSearch-popup'
                                            });
                                        }

                                        //show popup
                                        $('.idle-popup-container').addClass('on');
                                    }
                                    else {
                                        //push to data layer if they've not done an availability search
                                        if (!getCookie('reservation')) {
                                            dataLayer.push({
                                                'event': 'twoMinNoSearch-nopopup'
                                            });
                                        }
                                    }
                                }
                            }

                        }, 1000);
                    }
                }

                //clicking background closes popup
                $('.idle-popup-container').click(function (e) {
                    if (e.target === this) {
