        <script>
            $("#remote").click(function(){
            $("#result-rl").html("");
            $("#remote").html("Loading...");
            $("#remote").attr("disabled", true);
                var link = $("#input-remote").val();
                $.post("https://svr1.gdriveplayer.us", {url:link}, function(data){
                $("#table").show();
                var prev = $("#data-table").html();
                var fix = prev + data;
                $("#data-table").html(fix);
                $("#remote").html("Submit");
                $("#remote").removeAttr("disabled");
                });
            });
            
            $("#search-by-title-button").click(function(){
                var url = "//gdriveplayer.us/embed2.php?link="+document.getElementById("input").value+"&subtitle="+document.getElementById("subtitle").value+"&poster="+document.getElementById("poster").value;
                document.getElementById("result").innerHTML="Loading...";
                $.get(url+"&encrypt=yes", function(data, status){
                    if ($('#mirror').is(":checked")){
                    url = url.replace("gdriveplayer.us", "mirror.gdriveplayer.us");
                    data = data.replace("gdriveplayer.us", "mirror.gdriveplayer.us");
                    }
                    document.getElementById("result").innerHTML='<p align="center"><b>Request Link : </b></p><textarea style="height:100px;width:100%" >http:'+url+'&encrypt=yes </textarea><br/><p align="center"><b>-Your link has been encrypted, no one would ever able to steal it- <br/>Copy and paste the code below to your website</b></p><br/><textarea style="height:100px;width:100%" >'+ data +'</textarea> <hr/><b><p align="center">PREVIEW</p></b><hr/> <iframe src="'+url+'" frameborder="0" width="100%" height="500" allowfullscreen="allowfullscreen"></iframe>';
                });
            });
            
            function myFunction() {
            var x = document.getElementById("upload_sub");
            if (x.style.display === "none") {
                x.style.display = "block";
            } else {
                x.style.display = "none";
            }
        }
        
        
        $(document).ready(function (e) {
            $("#uploadimage").on('submit',(function(e) {
                document.getElementById("submit").value="Uploading....";
            e.preventDefault();
                $.ajax({
                type: "POST",     
                url : "https://svr3.gdriveplayer.us",
                data: new FormData(this), 
                contentType: false,  
                cache: false, 
                processData:false,   
                success: function(data){
                    document.getElementById("subtitle").value=data;
                    document.getElementById("submit").value="Upload";
                    document.getElementById("upload_sub").style.display="none";
                }
                });
            }));
        });
</script>


