  if( kproducts.length > 0 ) {
    window.kias.push({
      cmd: "setPageSettings",
      pageType: kpagetype,
      products: kproducts,
      tags: ktags,
    });
    if( kpagetype === "thankyou_page" ) {
      window.kias.push({
        cmd: "checkout",
        products: kproducts,
      });
    }
  }
  if( kcart.length > 0 ) {
    window.kias.push({
      cmd: "setCartProducts",
      products: kcart,
    });
  }
  if( kbannername !== "none" ) {
    window.kias.push({
      cmd: "setPageSlots",
      slots: {
        super: kbannername,
        super2: kbannername2,
      },
    });
  }
</script>

