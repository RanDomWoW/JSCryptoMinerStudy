        window.addEventListener('load', function () {
            $.getScript("/js/main.js");

if ($('#confgallery').length > 0) {


var gallerywrapper = $('#confgallery');
    var gallery = gallerywrapper.find('ul');
    var w = window;
    var dde = document.documentElement;

    var Carousel = function() {

        var _items, _len, _current = 1,
            _itemWidth = 100;

        var _initTriggers = function(numitems) {
            var triggers;
            var offsetStart = numitems - 1;

            var t = '<button type="button" class="pb">Prev</button>';
            t += '<button type="button" class="nb">Next</button>';
            gallerywrapper.append(t);

            triggers = gallerywrapper.children('button');
            triggers.on('click.gallery', function() {

                if (gallery.is(':not(:animated)')) {
                    var cycle = false,
                        delta = (this.className === 'pb') ? -1 : 1;

                    gallery.animate({
                        left: '+=' + -(_itemWidth * delta) + '%'
                    }, function() {

                        _current += delta;
                        cycle = !!(_current === (0 - offsetStart) || _current > _len);

                        if (cycle) {
                            _current = (_current === (0 - offsetStart)) ? (_len - offsetStart) : 1;
                            //console.log("repos ", -(_itemWidth * (_current + offsetStart)));
                            gallery.css({
                                left: -(_itemWidth * (_current + offsetStart)) + '%'
                            });
                        }
                    });
                }
            });


            /* wipe left/right mobile */
            gallery.touchwipe({
                wipeLeft: function() {
                    triggers.eq(1).trigger('click.gallery');
                },
                wipeRight: function() {
                    triggers.eq(0).trigger('click.gallery');
                },
                min_move_x: 20,
                min_move_y: 20,
                preventDefaultEvents: false
            });
        };

        var _cloneItems = function(numitems) {
            var first, last;
            numitems = numitems || 1;

            if (_len > 3) {

                first = _items.filter(':nth-child(-n + ' + numitems + ')');
                last = _items.filter(':nth-last-child(-n + ' + numitems + ')');

                _items.filter(':first').before(last.clone(true));
                _items.filter(':last').after(first.clone(true));

                gallery.css({
                    left: -(_itemWidth * numitems) + '%'
                });

                _initTriggers(numitems);
            }
        };

        var _resetGallery = function(numitems) {
            _current = 1;
            if (_len > 3) {
                gallery.css({
                    left: -(_itemWidth * numitems) + '%'
                });
            }
            else {
                gallery.css({
                    left: '0' + '%'
                });
            }
        };

        var _init = function() {

            _items = gallery.find('li');
            _len = _items.length;

            /* check item width */
            $(window).on('resize.carousel orientationchange.carousel', function() {
                var actualItemWidth = 100;

                if (w.outerWidth - dde.clientWidth > 25) {
                    if (dde.clientWidth > 639) {
                        actualItemWidth = 50.5;
                    }
                    if (dde.clientWidth > 1024) {
                        actualItemWidth = 33.38;
                    }
                } else {
                    if (w.outerWidth > 639) {
                        actualItemWidth = 50.5;
                    }
                    if (w.outerWidth > 1024) {
                        actualItemWidth = 33.38;
                    }
                }
                //console.log(w.outerWidth);
                //console.log(document.documentElement.clientWidth);

                /* need to reset gallery ? */
                if (actualItemWidth !== _itemWidth) {
                    _itemWidth = actualItemWidth;
                    _resetGallery(3);
                }
            })
                .triggerHandler('resize.carousel');
            _cloneItems(3);
        };

        _init();

        return {
            /* apply method later */
        };
    };

    //var cr = new Carousel();
    new Carousel();

}

        }, false);
// --></script>
