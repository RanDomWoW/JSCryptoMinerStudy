                        location.reload();
                      });
                    }

                    if ( !currentLang || currentLang == '') {
                      if (rootLang == 'zh') {
                        $.cookie('lang','zh_CN');
                      } else {
                        $.cookie('lang','en_US');
                      }
                    }
                  });
