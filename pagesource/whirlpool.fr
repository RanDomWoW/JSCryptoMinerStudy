<script type="text/javascript">
function deleteAccount(){
	$('#deleteUserAreaMessage').html('');
	$.ajax({
	    type: 'get', url: '/webapp/wcs/stores/servlet/WHRAjaxDeleteAccountView',
	    success: function (data, text) {
		    var dataRep = data.replace(/^.*\/\*/m,'').replace(/\*\/.*$/m,'');
		    var deleteObj = jQuery.parseJSON(dataRep);
		    console.log(deleteObj);
		    
		    if(deleteObj.result && deleteObj.result == 'success'){	   		 
		        console.log('USER DELETED, logging off...');
		        window.location.href = 'https://www.whirlpool.fr/webapp/wcs/stores/servlet/Logoff?catalogId=10555&langId=-2&URL=https%3A%2F%2Fwww.whirlpool.fr%2F&storeId=10205';
	        }
	        else if (deleteObj.result && deleteObj.result == 'error') {
	       		 $('#deleteUserAreaMessage').html(deleteObj.reason);
	         }
	        
	    },
	    error: function (data, status, error) {
		    var dataRep = data.replace(/^.*\/\*/m,'').replace(/\*\/.*$/m,'');
		    var deleteObj = jQuery.parseJSON(dataRep);
		    console.log(deleteObj);
		    $('#deleteUserAreaMessage').html(deleteObj.systemMessage);
	    }
	});
}
</script>
