<script>
	$( document ).ready(function() {
     $('.toggleShoppingCart').click(function(e){
     	if(this.id){
	     	var vendor = this.id.replace('toggle_','');
			if($(this).html().trim() == '−'){
				$(this).html('&plus;');
				$('#body_'+vendor).css("visibility", "collapse");
				$('#result_'+vendor).css("visibility", "collapse");
				/*$('#subToggleReOrder_'+vendor).html('&plus;');
				$('#subToggleRequested_'+vendor).html('&plus;');
				$('.subToggleReOrder_'+vendor).hide();
				$('.subToggleRequested_'+vendor).hide();*/
			}else{
				$(this).html('&minus;');
				$('#body_'+vendor).css("visibility", "visible");
				$('#result_'+vendor).css("visibility", "visible");
			}
     	}
     });
     // Sprung aus Shop klappt alle Warenkoerbe zu und den Warenkorb gerade geklickten Shop wieder auf
     console.log()
     if(location.hash.length
     	&& location.hash.indexOf("cartVendor_") >= 0){
		$('.toggleShoppingCart').each(function(){
	     	var vendor = this.id.replace('toggle_','');
			$(this).html('&plus;');
			$('#body_'+vendor).css("visibility", "collapse");
			$('#result_'+vendor).css("visibility", "collapse");
		});
	  	var vendor = location.hash.replace('#cartVendor_','');
		$('#toggle_'+vendor).html('&minus;');
		$('#body_'+vendor).css("visibility", "visible");
		$('#result_'+vendor).css("visibility", "visible");
     }
     $('.subToggleShoppingCart').click(function(e){
     	if(this.id){
			if($(this).html().trim() == '−'){
				$(this).html('&plus;');
				$('.'+this.id).hide();
			}else{
				$(this).html('&minus;');
				$('.'+this.id).show();
			}
     	}
     });
	});

</script>


