	InitPreloader = function(height, imgClass) {
		preloaderContent = "<div style='height:" + height + ";' class='ym-g100 ym-gl ym-textCenter'><img class='" + imgClass + "' src='" + preloaderImgSrc + "' border='0' /><br /></div>";

