	SetPreloader = function(id, height, imgClass) {
		if(height == null) {
			height = '550px';
		}
		if(imgClass == null) {
			imgClass = 'ym-i25';
		}
		$("#" + id).html(InitPreloader(height, imgClass));
	}

	GetUserID = function() {
