        });
	});

	AddVariantToShoppingcart = function(articleID,vendorID,isRefreshPressed){

		if(isRefreshPressed == null){
			isRefreshPressed = false;
		}

		var variantVECount = 0;

		ar_form = new Array();
		$('.variant_formfield').each(function(index) {
			if(! this.id.match(/submit/i) && ! this.id.match(/oldPrice/i)) {

				if($(this).val().length == 0){
					temp_val = 0;
				}else{
					temp_val = $(this).val();
					
					temp_val = temp_val.replace(/\W/g,"");
				}
				ar_form[index] = this.id + ":" + temp_val;

				variantVECount = variantVECount + parseInt(temp_val);
			}
		});
		if(ar_form.length > 0) {
			li_form = ar_form.join(",");
		} else {
			li_form = "";
		}

		
		temp_urlForm = url_loadIt + "&site=variantSave" + "&userID=" + GetUserID() + "&formfields=" + escape(li_form) + "&vendorID=" + vendorID + "&articleID=" + articleID + "&isTradesafeFancybox=" + true;
		$("#variant_detail_form").load(temp_urlForm, function() {
			
			$("#variant_submit").click(function() {
				AddVariantToShoppingcart(articleID,vendorID,true);
			});

			
			isError = $.trim($("#variantError").html());
			
			if(isError == "false"){
						temp_url = url_loadIt + "&site=variantToShoppingcart&userID=" + GetUserID() + "&formfields=" + escape(li_form) + "&vendorID=" + vendorID + "&articleID=" + articleID + "&shoppingcartID=" + shoppingcartID;
						
						window.setTimeout('$("#variant_toShoppingcart").load(temp_url);',200);

						$('#fancybox-close').click();
						window.setTimeout('parent.location.reload(true);',350);
			}
		});
	}

	InitShoppingcartVariant = function(){
		
		$(".shoppingcart_variant").click(function() {
			SetPreloader("variant_detail");


			li_value = $(this).attr("alt");
			ar_value = li_value.split(",");

			vendorID = ar_value[0];
			articleID = ar_value[1];
			shoppingcartID = ar_value[2];

			if(this.href.length > 0) {
				
				temp_url = url_loadIt + "&site=variant" + "&userID=" + GetUserID() + "&ve=" + 0 + "&vendorID=" + vendorID + "&articleID=" + articleID + "&shoppingcartID=" + shoppingcartID + "&isTradesafeFancybox=" + true;

				$("#variant_detail").load(temp_url, function() {
					$("#variant_link").fancybox().trigger('click');

					
					$("#variant_submit").click(function() {
						AddVariantToShoppingcart(articleID,vendorID,false);
					});

					$("a#variant_refresh").click(function() {
						ar_form = new Array();
						$('.variant_formfield').each(function(index) {
							if(! this.id.match(/submit/i)) {

								if($(this).val().length == 0){
									temp_val = 0;
								}else{
									temp_val = $(this).val();
									
									temp_val = temp_val.replace(/\W/g,"");
								}
								ar_form[index] = this.id + ":" + temp_val;
							}
						});
						if(ar_form.length > 0) {
							li_form = ar_form.join(",");
						} else {
							li_form = "";
						}

						temp_urlForm = url_loadIt + "&site=variantSave" + "&userID=" + GetUserID() + "&formfields=" + escape(li_form) + "&vendorID=" + vendorID + "&articleID=" + articleID + "&isTradesafeFancybox=" + true;
						$("#variant_detail_form").load(temp_urlForm, function() {
							
							$("#variant_submit").click(function() {
								AddVariantToShoppingcart(articleID,vendorID,true);
							});
						});
						return false;
					});
				});
			};
		});
	}

	$(document).ready(function() {
		InitShoppingcartVariant();
	});

	
	
	


    
    
