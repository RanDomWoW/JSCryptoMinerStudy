    <script>
        jQuery(document).ready(function ($) {
            var local = qs('Local');

            var url = $('#url').val();
            if (url == "")
                url = "";

            var tipoview = readCookie('tipoview');

            //if (tipoview == '') {
            //    tipoview = 'lista';
            //}

            if (!tipoview) {
                tipoview = 'grid';
            }

            //var premium2 = $('.premium-lote-modelo').clone();
            //var premium3 = $('.premium-lote-modelo').clone();
            //var premium4 = $('.premium-lote-modelo').clone();		

            //premium2.removeClass('premium-lote-modelo  hidden').appendTo($('.jssor-lotes'));
            //premium3.removeClass('premium-lote-modelo  hidden').appendTo($('.jssor-lotes'));
            //premium4.removeClass('premium-lote-modelo  hidden').appendTo($('.jssor-lotes'));

            var jssor_1_SlideshowTransitions = [
                { $Duration: 1800, $Opacity: 2 }
            ];

            var jssor_1_options = {
                $AutoPlay: true,
                $Idle: 10000,//time
                $SlideshowOptions: {
                    $Class: $JssorSlideshowRunner$,
                    $Transitions: jssor_1_SlideshowTransitions,
                    $TransitionsOrder: 1,
                    //$SlideDuration: 500
                },
                $ArrowNavigatorOptions: {
                    $Class: $JssorArrowNavigator$,
                    $ChanceToShow: 1
                },
                $BulletNavigatorOptions: {
                    $Class: $JssorBulletNavigator$,
                    $Scale: false
                }
            };

            /********** Busca Padrao **********/


            var p = $('#ContentPlaceHolder1_pagina').val();
            if (p == "")
                p = 0;

            var f = $('#ContentPlaceHolder1_filtro').val();
            if (f == "")
                f = 0;

            var c = $('#ContentPlaceHolder1_categoria').val();
            if (c == "")
                c = 0;

            var c = $('#ContentPlaceHolder1_marca').val();
            if (c == "")
                c = 0;

            var u = $('#ContentPlaceHolder1_estado').val();
            if (u == "")
                u = 0;

            var ci = $('#ContentPlaceHolder1_cidade').val();
            if (ci == "")
                ci = 0;


            $.ajax({
                url: (local > 0) ? 'ajaxhome.json' : 'https://' + window.location.host + '/' + url + 'ajaxhomeVIP/' + p + '/' + f + '/' + c + '/' + u + '/' + ci + '/',
                method: 'GET', cache: false
            })
                         .done(function (a) {

                             /********** Inicio Pagincao *********/

                             $('#rl_leilao_pagination').empty();

                             var cat = $('#ContentPlaceHolder1_drpCategoria').val();
                             var uf = $('#ContentPlaceHolder1_drpEstado').val();
                             var cid = $('#ContentPlaceHolder1_drpCidade').val();


                             var paginatorinputs = '';

                             var totalpaginas = a.Home.LeilaoPaginas;
                             var paginaatual = a.Home.PaginaAtual;

                             if (paginaatual == 1) {
                                 paginatorinputs += '';
                             } else {
                                 paginatorinputs += '<li ><a href="https://' + window.location.host + '/' + url + 'home/' + (paginaatual - 1) + '/0/' + cat + '/' + uf + '/' + cid + '/">&laquo;</a></li>';
                             }
                             var i = 1;
                             while (i <= totalpaginas) {
                                 var active = '';
                                 if (i == paginaatual)
                                     active = 'class="active"';

                                 paginatorinputs += '<li ' + active + '><a href="https://' + window.location.host + '/' + url + 'home/' + i + '/0/' + cat + '/' + uf + '/' + cid + '/">' + i + '</a></li>';

                                 i++;
                             }

                             if (paginaatual == totalpaginas) {
                                 paginatorinputs += '';
                             } else {
                                 paginatorinputs += '<li ><a href="https://' + window.location.host + '/' + url + 'home/' + (paginaatual + 1) + '/0/' + cat + '/' + uf + '/' + cid + '/">&raquo;</a></li>';
                             }

                             $('#rl_leilao_pagination').append(paginatorinputs).show();

                             /********** Fim Paginacao ***********/


                             /********** Inicio Banner **********/
                             $(a.Banner).each(function (i, j) {
                                 if (j.Link != "") {
                                     $('<div data-p="112.50" style="display: none;"><a href="' + j.Link + '"><img data-u="image" src="' + j.Imagem + '" /></a></div>').appendTo('.banners');
                                     //$('<div data-p="112.50" style="display: none;">' + j.Imagem + '</div>').appendTo('.banners');
                                 } else {
                                     $('<div data-p="112.50" style="display: none;"><img data-u="image" src="' + j.Imagem + '" /></div>').appendTo('.banners');
                                 }

                             })


                             var jssor_1_slider = new $JssorSlider$("jssor_1", jssor_1_options);
                             function ScaleSlider() {
                                 var refSize = jssor_1_slider.$Elmt.parentNode.clientWidth;
                                 if (refSize) {
                                     refSize = Math.min(refSize, 1140);
                                     jssor_1_slider.$ScaleWidth(refSize);
                                 }
                                 else {
                                     window.setTimeout(ScaleSlider, 30);
                                 }
                             }
                             ScaleSlider();
                             $(window).bind("load", ScaleSlider);
                             $(window).bind("resize", ScaleSlider);
                             $(window).bind("orientationchange", ScaleSlider);

                             /********** Fim Banner **********/

                             /********** Inicio Estatisticas********/

                             $('.total_carros').text(a.Estatisticas.Carros);
                             $('.total_motos').text(a.Estatisticas.Motos);
                             $('.total_caminhoes').text(a.Estatisticas.Caminhoes);
                             $('.total_outros').text(a.Estatisticas.Outros);
                             $('.total_total').text(a.Estatisticas.Total);
                             $('.total_imoveis').text(a.Estatisticas.Imoveis);

                             /*********** Fim Estatisticas ***********/
                             /********** Inicio Categorias( Tipo) **********/
                             var cat = '';
                             $(a.Categoria).each(function (i, j) {
                                 cat += '<option value="' + j.Codigo + '">' + j.Descricao + '</option>';
                             })

                             $('#ContentPlaceHolder1_drpCategoria').append(cat);
                             /********** Fim Categorias **********/

                             /********** Marcas **********/
                             var marca = '';
                             $(a.Marca).each(function (i, j) {
                                 marca += '<option value="' + j.Codigo + '">' + j.Descricao + '</option>';
                             })
                             //$('#ContentPlaceHolder1_drpMarca').empty();
                             $('#ContentPlaceHolder1_drpMarca').append(marca);

                             /***********Fim Marcas
							 
                             /********** Inicio Estados**********/
                             var estado = '';
                             $(a.Estado).each(function (i, j) {

                                 estado += '<option value="' + j.Sigla + '">' + j.Nome + '</option>';
                             })
                             //console.log(estado);

                             $('#ContentPlaceHolder1_drpEstado').append(estado);

                             /********** Fim Estados **********/
                             /********** Inicio Leiloes **********/
                             //console.log(a.Leilao);
                             $(a.Leilao).each(function (i, j) {
                                 // lista
                                 if (tipoview == 'lista') {


                                     var leilao = $(".leilao-modelo").clone();
                                     leilao.removeClass('leilao-modelo hidden').appendTo($('.home-leilao'));
                                     //leilao.attr('LeilaoCod', j.LeilaoCod);
                                     leilao.find('.rl_foto').css("background-image", "url(" + j.Foto + ")");
                                     leilao.find('.rl_titulo').text(j.Titulo);
                                     leilao.find('.rl_descricao').html(j.Descricao);										// Pode vir nulo
                                     leilao.find('.rl_dataleilao').html(j.DataLeilao);
                                     leilao.find('.rl_leilaoTipo').text(j.LeilaoTipo);
                                     leilao.find('.rl_leiloeiro').text(j.Leiloeiro);
                                     leilao.find('.rl_lotes').html(j.Lotes);
                                     leilao.find('.rl_cidade').html(j.Cidade);
                                     leilao.find('.rl_leilao_link').attr("href", j.Link);
                                     leilao.find('.rl_localidade').html(j.Localidade);
                                     leilao.find('.rl_statusmensagem').text(j.StatusMensagem);
                                     leilao.find('.rl_classstatusmensagem').removeClass('aberto online breve').addClass(j.classStatusMensagem)
                                     leilao.find('.rl_classlocalidade1').removeClass('localidade_aberta localidade_fechada').addClass(j.classLocalidade1)
                                     leilao.find('.rl_classlocalidade2').removeClass('localidade_aberta localidade_fechada').addClass(j.classLocalidade2)

                                     //var leilao = $(".leilao-modelo").clone();
                                     //leilao.removeClass('leilao-modelo hidden').appendTo($('.home-leilao'));
                                     ////leilao.attr('LeilaoCod', j.LeilaoCod);
                                     //leilao.find('.rl_foto').css("background-image", "url(" + j.Foto + ")");
