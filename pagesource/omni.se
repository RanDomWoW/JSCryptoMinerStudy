sources&quot;:[{&quot;type&quot;:&quot;Byline&quot;,&quot;published&quot;:&quot;2018-03-18T11:43:47Z&quot;,&quot;authors&quot;:[{&quot;id&quot;:&quot;1011e06a-d941-4a59-9498-86cc8232176a&quot;,&quot;title&quot;:&quot;Matilda Svensson Glaser&quot;}],&quot;fallback_email&quot;:&quot;redaktion@omni.se&quot;},{&quot;type&quot;:&quot;Image&quot;,&quot;byline&quot;:{&quot;title&quot;:&quot;Fredrik Sandberg / TT / TT NYHETSBYRÅN&quot;},&quot;caption&quot;:{&quot;value&quot;:&quot;Arkivbild.&quot;},&quot;source_id&quot;:&quot;sdlsx79b778-nh.jpg&quot;,&quot;image_asset&quot;:{&quot;id&quot;:&quot;e4845fab-297c-4398-9bd6-5c12932f10ca&quot;,&quot;size&quot;:{&quot;width&quot;:3940,&quot;height&quot;:2676}},&quot;characteristics&quot;:{&quot;figure&quot;:false,&quot;sensitive&quot;:false}},{&quot;ref&quot;:&quot;category&quot;,&quot;id&quot;:&quot;36480a9056b9423daf4538f7aa62bad2&quot;,&quot;title&quot;:&quot;Inrikes&quot;,&quot;type&quot;:&quot;StoryVignette&quot;},{&quot;type&quot;:&quot;Title&quot;,&quot;text&quot;:{&quot;value&quot;:&quot;Rickards skadestånd gick till gärningsmannen&quot;}},{&quot;type&quot;:&quot;Text&quot;,&quot;paragraphs&quot;:[{&quot;text&quot;:{&quot;value&quot;:&quot;Rickard skallades på krogen och skulle få skadestånd – men pengarna råkade i stället betalas ut till gärningsmannens konto, skriver Aftonbladet.&quot;},&quot;block_type&quot;:&quot;paragraph&quot;},{&quot;text&quot;:{&quot;value&quot;:&quot;Misshandeln inträffade för fyra år sedan, då Rickard jobbade som ordningsvakt på en krog. Ifjol fick han besked att Kronofogden skulle utmäta gärningsmannens bil och ett par andra värdeföremål som skulle betala ersättningen. Pengarna fortsatte dröja, och från Kronofogden fick Rickard beskedet att något tycktes ha ”gått snett”. Pengarna hade nämligen satts in på gärningsmannens konto, och nu fanns inga pengar kvar för utbetalning.&quot;},&quot;block_type&quot;:&quot;paragraph&quot;},{&quot;text&quot;:{&quot;value&quot;:&quot;Kronofogden skyller det hela på ”den mänskliga faktorn och olyckliga omständigheter”. Myndighetens rättsavdelning har inlett en process om skadestånd för felhantering, och ärendet granskas också av Justitieombudsmannen.&quot;},&quot;block_type&quot;:&quot;paragraph&quot;}],&quot;vignette&quot;:{&quot;title&quot;:&quot;Inrikes&quot;,&quot;bullet_color&quot;:{&quot;red&quot;:27,&quot;green&quot;:164,&quot;blue&quot;:222}}},{&quot;url&quot;:&quot;http://www.aftonbladet.se/nyheter/a/WLeGMa/rickards-skadestand-betalades-ut-till-garningsmannen&quot;,&quot;data&quot;:{&quot;canonical_url&quot;:&quot;https://www.aftonbladet.se/a/WLeGMa&quot;},&quot;type&quot;:&quot;Url&quot;,&quot;title&quot;:{&quot;value&quot;:&quot;Handläggare: ”Han har möjlighet att kräva skadestånd för hanteringen”&quot;},&quot;source&quot;:&quot;Aftonbladet&quot;},{&quot;url&quot;:&quot;http://tt.omni.se/skadestand-gick-till-garningsmannen/a/6n829r&quot;,&quot;data&quot;:{&quot;canonical_url&quot;:&quot;http://tt.omni.se/skadestand-gick-till-garningsmannen/a/6n829r&quot;},&quot;type&quot;:&quot;Url&quot;,&quot;title&quot;:{&quot;value&quot;:&quot;Rickard: ”Det har gått fyra år och har inte hänt någonting”&quot;},&quot;source&quot;:&quot;TT&quot;}],&quot;meta&quot;:{&quot;changes&quot;:{&quot;bumped&quot;:null,&quot;created&quot;:&quot;2018-03-18T10:20:54Z&quot;,&quot;updated&quot;:&quot;2018-03-18T11:43:47Z&quot;,&quot;published&quot;:&quot;2018-03-18T11:43:47Z&quot;,&quot;bumped_value&quot;:false,&quot;first_published&quot;:&quot;2018-03-18T11:43:47Z&quot;},&quot;version&quot;:27,&quot;is_sticky&quot;:false,&quot;newsvalue&quot;:6,&quot;is_deleted&quot;:false,&quot;is_sponsored&quot;:false,&quot;machine_tags&quot;:{&quot;gender&quot;:&quot;none&quot;},&quot;news_lifetime&quot;:2,&quot;custom_properties&quot;:{&quot;bump&quot;:false,&quot;sticky&quot;:&quot;&quot;}},&quot;tags&quot;:[{&quot;type&quot;:&quot;descriptor&quot;,&quot;title&quot;:&quot;Brott&quot;,&quot;topic_id&quot;:&quot;1c5c0370e3c1dd5d4f41ea72b0e8880e94e7c0d4&qu