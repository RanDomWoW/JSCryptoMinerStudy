<script type="text/javascript">

// JavaScript Document

        /**
         *
         *
         */
        function getXhr() {
            var xhr = null;
            if (window.XMLHttpRequest) // Firefox et autres
                xhr = new XMLHttpRequest();
            else if (window.ActiveXObject) { // Internet Explorer
                try {
                    xhr = new ActiveXObject("Msxml2.XMLHTTP");
                } catch (e) {
                    xhr = new ActiveXObject("Microsoft.XMLHTTP");
                }
            }
            else { // XMLHttpRequest non supporté par le navigateur
                alert("Votre navigateur ne supporte pas les objets XMLHTTPRequest...");
                xhr = false;
            }
            return xhr;
        }

        function refreshActualite(limitTime) {
            setTimeout(function () {
                window.forcing = true;
                if (jQuery('.left_menu_info_continu .liste_menu_info_continu').length !== 0) {
                    jQuery('.left_menu_info_continu .liste_menu_info_continu').find('a.active').trigger('click');
                    refreshActualite(limitTime);
                }
            }, limitTime);
        }

        window.forcing = false;
        var current_theme = null;

        function depliMenuLeft(e) {
            if (jQuery(window).width() < 768) {
                var parentLI = jQuery(e).closest('ul');
                if (jQuery(parentLI).find('li:nth-child(n+2)').is(':visible')) {
                    jQuery(parentLI).find('li:nth-child(n+2)').hide();
                    jQuery(parentLI).find('li:nth-child(1) i').attr('class', 'fa fa-angle-down');
                }
                else {
                    jQuery(parentLI).find('li:nth-child(n+2)').slideDown();
                    jQuery(parentLI).find('li:nth-child(1) i').attr('class', 'fa fa-angle-up');
                }
            }
        }

        function getActualite(pathDirController, pathDir, theme) {
            if (current_theme == theme
                    && window.forcing === false)
                return false;
            var carousel = jQuery('#carousel').nnCarousel();
            carousel.showLoader();
            jQuery.ajax({
                type: "POST",
                url: pathDirController,
                mode: 'abort',
                data: "pathDir=" + pathDir + "&themeGallerie=" + theme,
                dataType: 'html',
                success: function (data) {
                    window.forcing = false;
                    current_theme = theme;
                    carousel.reloadContent(data);
                }
            });
            return false;
        }

        function redirectMeteo(path) {
            if (path != '') {
                window.document.location.href = path;
            }
        }

        function openWindow(url, nom, option) { //v2.0
            window.open(url, nom, option);
        }

        function displayFlouDiw() {
            var x = document.getElementById('blocActuReg').offsetLeft;
            var y = document.getElementById('blocActuReg').offsetTop;
            var h = document.getElementById('blocActuReg').offsetHeight;
            var w = document.getElementById('blocActuReg').offsetWidth;

            document.getElementById('blocActuRegLoad').style.display = "block";
            document.getElementById('blocActuRegLoad').style.left = x + "px";
            document.getElementById('blocActuRegLoad').style.top = y + "px";
            document.getElementById('blocActuRegLoad').style.width = w + "px";
            document.getElementById('blocActuRegLoad').style.height = "189px";
        }

        function getActualiteRegion(pathDirController, pathDir) {

            displayFlouDiw();
            var xhr = getXhr();
            xhr.onreadystatechange = function () {
                if (xhr.readyState == 4 && xhr.status == 200) {
                    result = xhr.responseText;
                    document.getElementById('blocActuRegLoad').style.display = "none";
                    document.getElementById('blocActuReg').innerHTML = result;
                }
            }

            if (!pathDir) {
                pathDir = 'nordpasdecalais';
            }
            //console.log("pathdir = " + pathDir);
            if (pathDirController && pathDir) {
                xhr.open("POST", pathDirController, true);
                xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
                xhr.send("theme=" + pathDir);
            }
            else {
                alert('Error');
            }
        }

        $(document).ready(function () {
            jQuery('#carousel').nnCarousel();
            if (jQuery('ul.liste_menu_info_continu li a').length > 0) {
                jQuery("ul.liste_menu_info_continu li a").click(function () {
                    if (jQuery(window).width() > 767) {
                        jQuery("ul.liste_menu_info_continu li a").removeClass('active');
                        jQuery(this).addClass('active');
                    }
                });
            }
        });
</script>

