@media only screen and (max-width: 637px) {
.edtechno-post-thumbnail-holder{float:none;width:100%;-webkit-box-flex: 0;-webkit-flex: 0 0 100%;-moz-box-flex: 0;-ms-flex: 0 0 100%;flex: 0 0 100%;-webkit-box-ordinal-group:2;-webkit-order:1;-moz-box-ordinal-group:2;-ms-flex-order:1;order:1;margin:0 0 15px 0;}
.edtechno-post-content-holder{float:none;width:100%;-webkit-box-flex: 0;-webkit-flex: 0 0 100%;-moz-box-flex: 0;-ms-flex: 0 0 100%;flex: 0 0 100%;-webkit-box-ordinal-group:3;-webkit-order:2;-moz-box-ordinal-group:3;-ms-flex-order:2;order:2;}
}
.edtechno-post-bottom-ad{margin:10px 0;overflow:hidden;}
.edtechno-post-bottom-matched-content{margin:20px 0 10px;overflow:hidden;}
.edtechno-popular-posts-widget{overflow:hidden;}
.edtechno-popular-post{padding:8px 0;overflow:hidden;}
.edtechno-popular-post .edtechno-popular-post-title{padding-bottom:4px;font:normal normal 14px Oswald,Arial,Helvetica,sans-serif;text-transform:uppercase;}
.edtechno-popular-post .edtechno-popular-post-thumbnail{float:left;margin:0 10px 3px 0;}
.edtechno-popular-post .edtechno-popular-post-thumbnail img{padding:0;margin:0;display:block;}
.edtechno-popular-entry-meta{font:normal normal 11px Oswald,Arial,Helvetica,sans-serif;letter-spacing:0.5px;text-transform:uppercase;margin:0 0 3px 0;}
.edtechno-social-widget{list-style:none !important;list-style-type:none !important;font-size:16px;}
.edtechno-social-widget li{padding:0 !important;margin:0 !important;border:none !important;line-height:1;}
.edtechno-social-widget li a{color:#fff !important;background:#333;padding:10px 10px;margin:0 0 5px 0;display:block;font-size:16px;line-height:16px;height:36px;}
.edtechno-social-widget li a[href*="facebook.com"]{background-color:#3b5998;}
.edtechno-social-widget li a[href*="twitter.com"]{background-color:#00aced;}
.edtechno-social-widget li a[href*="plus.google.com"]{background-color:#dd4b39;}
.edtechno-social-widget li a[href*="instagram.com"]{background-color:#517fa4;}
.edtechno-social-widget li a[href*="youtube.com"]{background-color:#bb0000;}
.edtechno-social-widget li a[href*="pinterest.com"]{background-color:#bd081c;}
.edtechno-social-widget li a[href*="/feeds/posts/default"]{background-color:#dc622c;}
.edtechno-social-widget li i{margin-right:10px;}
.edtechno-social-widget li a i{display:inline-block;background:#000;padding:10px;margin:-10px 10px -10px -10px;width:36px;height:36px;text-align:center;}
.edtechno-entry-meta-feed{margin:0 0 5px 0;line-height:1.6;}
.edtechno-entry-meta-feed a{color:#34A7E0;}
.edtechno-entry-meta-feed a:hover{color:#000000;}
.edtechno-sharethis-wrapper{margin:15px 0 20px;}
.edtechno-entry-meta{margin:0 0 15px 0;text-align:center;padding:5px;border-top:1px dashed #ddd;border-bottom:1px dashed #ddd;}
.edtechno-entry-meta a{color:#34A7E0;}
.edtechno-entry-meta a:hover{color:#000000;}
.edtechno-blog-status-message{margin:0 0 25px 0;font-size:18px;text-align:center;}
.edtechno-alert{background:#333;display:inline-block;color:#fff;padding:5px 10px;}
.edtechno-alert a{color:#fff;text-decoration:underline!important;}
.edtechno-error-view #edtechno-main-wrapper{width:auto;float:none;margin:0 0 20px 0;padding:0;}
.edtechno-error-view #edtechno-sidebar-wrapper{width:auto;float:none;margin:0;padding:0;}
.edtechno-error-view #edtechno-crosscol-wrapper{display:none;}
.edtechno-item-view .post-body{}
.edtechno-rp-posts{margin:15px auto 10px;}
.edtechno-related-posts-title{margin:10px 0 10px 0;padding:0;border-bottom:2px solid #000000;color:#ffffff;font:normal normal 15px Oswald, Arial, Helvetica, sans-serif;line-height:1;text-decoration:none;text-transform:uppercase;}
.edtechno-related-posts-title span{background:#000000;padding:5px 10px;color:#ffffff;display:inline-block;}
.edtechno-related-posts,.edtechno-related-post{margin:0;padding:0;word-wrap:break-word;overflow:hidden;}
.edtechno-related-posts{display:flex;flex-direction:row;justify-content:space-between;align-items:stretch;flex-wrap:wrap;align-content:stretch;}
.edtechno-related-posts .edtechno-related-post{float:left;width:49%;flex: 0 0 49%;position:relative;margin-bottom:10px;}
.edtechno-related-posts .edtechno-related-post:nth-of-type(2n+1){clear:both;}
.edtechno-related-posts .edtechno-related-post-thumbnail-wrap{overflow:hidden;}
.edtechno-related-posts .edtechno-related-post-thumbnail-link{display:block;}
.edtechno-related-posts .edtechno-related-post-thumbnail{width:100%;max-width:100%;height:auto;display:block;padding:0;margin:0;}
.edtechno-related-post-title{left:0;right:0;bottom:0;padding:20px 20px;position:absolute;background:-moz-linear-gradient(top,rgba(0,0,0,0) 0%,rgba(0,0,0,0.9) 100%);background:-webkit-gradient(linear,left top,left bottom,color-stop(0%,rgba(0,0,0,0)),color-stop(100%,rgba(0,0,0,0.9)));background:-webkit-linear-gradient(top,rgba(0,0,0,0) 0%,rgba(0,0,0,0.9) 100%);background:-o-linear-gradient(top,rgba(0,0,0,0) 0%,rgba(0,0,0,0.9) 100%);background:-ms-linear-gradient(top,rgba(0,0,0,0) 0%,rgba(0,0,0,0.9) 100%);background:linear-gradient(to bottom,rgba(0,0,0,0) 0%,rgba(0,0,0,0.9) 100%);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#00000000',endColorstr='#80000000',GradientType=0);font:normal normal 20px Oswald, Sans-serif;color:#fff;line-height:1.4;}
.edtechno-related-post-title a{color:#fff;text-decoration:none;text-shadow:0 1px 0 #000;}
@media screen and (max-width:419px){
.edtechno-related-posts .edtechno-related-post{float:none;clear:both;width:100%;flex: 0 0 100%;margin-bottom:10px;}
}
.edtechno-postmetadata{float:left;width:65px;margin-bottom:10px;margin-right:10px;}
.edtechno-postmetadata span{display:block;text-align:center;}
.edtechno-postmetadata a{text-decoration:none;}
.edtechno-postmetadata a:hover,.edtechno-postmetadata a:focus{text-decoration:none;}
.edtechno-meta-date{background:#34A7E0;width:65px;color:#ffffff !important;padding:10px 0;font-family:Oswald,Helvetica,Arial,Verdana,sans-serif;}
.edtechno-meta-date a,.edtechno-meta-date a:visited{color:#ffffff !important;}
.edtechno-meta-date span{display:block;line-height:110%;text-transform:uppercase;}
.edtechno-meta-date .edtechno-meta-date-month{font-size:20px;}
.edtechno-meta-date .edtechno-meta-date-day{font-size:30px;}
.edtechno-meta-date .edtechno-meta-date-year{font-size:16px;}
.edtechno-meta-author{background:#ccc;color:#333;width:65px;min-height:11px;padding:5px 0;font-size:10px;line-height:11px;text-align:center;margin-bottom:0;}
.edtechno-comments-title{margin:10px 0 10px 0;padding:0;border-bottom:2px solid #000000;color:#ffffff;font:normal normal 15px Oswald, Arial, Helvetica, sans-serif;line-height:1;text-decoration:none;text-transform:uppercase;}
.edtechno-comments-title span{background:#000000;padding:5px 10px;color:#ffffff;display:inline-block;}
.showpageArea{float:left}
.showpageOf{margin:0 8px 0 0;padding:3px 8px;color:#000;background:#fff;border:1px solid #e5e5e5;display:inline-block;}
.showpagePoint{color:#fff;background:#000;padding:3px 8px;margin:2px;text-decoration:none;border:1px solid #000;display:inline-block;}
.showpageNum a{padding:3px 8px;margin:0 4px;text-decoration:none !important;background:#fff;color:#000;border:1px solid #e5e5e5;display:inline-block;}
.showpageNum a:hover{color:#000 !important;background:#f6f6f6;border:1px solid #e5e5e5;text-shadow:0px 1px 0px #fff !important;}
.showpage a{padding:3px 8px;margin:0 4px;text-decoration:none !important;background:#fff;color:#000;border:1px solid #e5e5e5;-webkit-box-shadow:0 0 1px 0 rgba(0, 0, 0, 0.1);-moz-box-shadow:0 0 1px 0 rgba(0, 0, 0, 0.1);box-shadow:0 0 1px 0 rgba(0, 0, 0, 0.1);}
.showpage a:hover{color:#000 !important;background:#f6f6f6;border:1px solid #e5e5e5;text-shadow:0px 1px 0px #fff !important;}
.showpageArea{margin:10px 0;color:#000;}

