<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="description" content="The world's largest online Korean beauty store with the lowest prices shipped directly from Korea">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="google-site-verification" content="tmOxY-AZH7cTyFz5dmtvDOZlWkVHZSNCBVM9GBXuOjY" />

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-63945226-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-63945226-1');
    </script>

            <meta name="google-site-verification" content="mQnUyFPd6cMRl3RDj5FrVsqavcQgZctum2DCbYFk9W4">
    
    <title>Althea</title>
    <link rel="shortcut icon" href='http://althea.kr/favicon.png'>
    <link rel="stylesheet" href='http://althea.kr/css/app.css'>
    <style>
        html{
            background-color:#424242;
        }

        body {
            text-align: center;
            color: #ffffff;
            position: relative;
        }

        #app {
            padding-top: 50px;
        }

        @media (min-width: 1200px) {
            .container {
                width: 970px;
            }
        }

        @media (min-width: 768px) {
            #app {
                padding-top: 100px;
            }
        }

        #altheaLogo {
            background-image: url('http://d1g2tbmsc7njs7.cloudfront.net/splash-white.png');
            background-position: center;
            background-repeat: no-repeat;
            background-size: contain;
            width: 100%;
            height: 90px;
        }

        #altheaCountry {
            padding-top: 5px;
            height: 15px;
            color: #ffffff;
            letter-spacing: 5px;
            font-size: 12px;
        }

        #countryList .item-country {
            padding-top: 10px;
            padding-bottom: 10px;
            text-align: center;
        }

        #subTitle {
            padding-top: 40px;
            padding-right: 30px;
            padding-bottom: 40px;
            padding-left: 30px;
            color: #bdbdbd;
            letter-spacing: 5px;
        }

        #countryList p {
            padding-top: 10px;
            padding-bottom: 10px;
        }

        #countryList img {
            width: 50px;
            height: 50px;
            border-radius: 25px;
            border-color: transparent;
            border-width: 3px;
            border-style: solid;
        }

        .item-country.hover img {
            border-color: #ef5a8e !important;
        }

        .item-country:active img {
            border-color: #ba245a !important;
        }

        #countryList .restOfWorld img {
            width: 80px;
            height: 80px;
            padding: 10px;
            background-color: #000;
        }

        #id-telco-popup {
            /*display: none;*/
            /*position: absolute;*/
            /*top:0;*/
            /*left:0;*/
            background-color: #fff;
            /*width:350px;*/
            /*height:auto;*/
        }

        .modal-header {
            background-color: #ef5a8e;
            color: #ffffff;
            border-top-left-radius: 5px;
            border-top-right-radius: 5px;
            padding-left: 15px;
            padding-right: 15px;
            padding-top: 9px;
            padding-bottom: 5px;
        }

        .modal-body{
            color: #161616;
        }

        .highlight{
            font-weight:bold;
            font-size:15px;
            color:#ef5a8e;
        }
    </style>
</head>
<body>
<div id="app" class="container">
    <div class="row">
        <div id="altheaLogo">
        </div>
        <div id="altheaCountry">

        </div>

        <h4 id="subTitle">
            SELECT YOUR COUNTRY
        </h4>

        <div id="countryList" class="container">
            <div class="row">
                <div id="my-country" class="item-country col-sm-2 col-xs-4">
                    <img src="/image/flags_1x1/malaysia.svg"/>

                    <p>
                        MALAYSIA
                    </p>
                </div>

                <div id="sg-country" class="item-country col-sm-2 col-xs-4">
                    <img src="/image/flags_1x1/singapore.svg"/>

                    <p>
                        SINGAPORE
                    </p>
                </div>

                <div id="ph-country" class="item-country col-sm-2 col-xs-4">
                    <img src="/image/flags_1x1/philippines.svg"/>

                    <p>
                        PHILIPPINES
                    </p>
                </div>

                <div id="id-country" class="item-country col-sm-2 col-xs-4">
                    <img src="/image/flags_1x1/indonesia.svg"/>

                    <p>
                        INDONESIA
                    </p>
                </div>

                <div id="th-country" class="item-country col-sm-2 col-xs-4">
                    <img src="/image/flags_1x1/thailand.svg"/>

                    <p>
                        THAILAND
                    </p>
                </div>

                <div id="tw-country" class="item-country col-sm-2 col-xs-4">
                    <img src="/image/flags_1x1/taiwan.svg"/>

                    <p>
                        TAIWAN
                    </p>
                </div>

                <div id="us-country" class="item-country col-sm-2 col-xs-4">
                    <img src="/image/flags_1x1/united-states-of-america.svg"/>

                    <p>
                        US
                    </p>
                </div>

                <div id="inter-country" class="item-country col-sm-2 col-xs-4">
                    <img src="/image/flags_1x1/earth.svg"/>

                    <p>
                        REST OF WORLD
                    </p>
                </div>

                <!-- Modal -->
                <div class="modal fade" id="selectTelcoModal" tabindex="-1" role="dialog">
                    <div class="modal-dialog modal-sm" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                                            aria-hidden="true">&times;</span></button>
                                <h4 class="modal-title" id="myModalLabel">Select Telco</h4>
                            </div>
                            <div class="modal-body">
                                <h4>
                                    Do you use <span class="highlight">TELKOMSEL</span> ?
                                </h4>
                            </div>
                            <div class="modal-footer">
                                <button type="button" id="yesTelcoBtn" class="btn btn-default btn-block"
                                        data-dismiss="modal">
                                    Yes
                                </button>

                                <button type="button" id="noTelcoBtn" class="btn btn-default btn-block"
                                        data-dismiss="modal">
                                    No
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Modal -->
                <div class="modal fade" id="introOthersModal" tabindex="-1" role="dialog">
                    <div class="modal-dialog modal-sm" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                                            aria-hidden="true">&times;</span></button>
                                <h4 class="modal-title" id="myModalLabel">Suggestions</h4>
                            </div>
                            <div class="modal-body">
                                <div>
                                    Note: We are experiencing some technical difficulties with your TelCo provider. In the interm, we suggest you use our <span class="highlight">Mobile App</span> OR visit our <span class="highlight">US site</span> to make orders.
                                </div>
                                <div class="row">
                                    <div id="app-store" class="item-country col-xs-6">
                                        <img src="/image/flags_1x1/app-store.svg"/>

                                        <p>
                                            iOS
                                        </p>
                                    </div>
                                    <div id="play-store" class="item-country col-xs-6">
                                        <img src="/image/flags_1x1/play-store.svg"/>

                                        <p>
                                            Android
                                        </p>
                                    </div>

                                    <div id="id-to-us-country" class="item-country col-xs-6">
                                        <img src="/image/flags_1x1/united-states-of-america.svg"/>

                                        <p>
                                            US
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            
            
            
        </div>
    </div>
</div>
<div id="bg-layer">

</div>
<script src="/js/app.js"></script>
<script>
    $(document).ready(function () {
        console.log("ready!");
        $(".col-sm-3").click(function (e) {
            console.log("thing top: " + $(this).position().top);
            //$('html,body').animate({ scrollTop: 0 }, 'slow');
            //return false;
        });

        var altheaCountryContainer = $("#altheaCountry");
        var body = $("html");
        var app = $("#app");
        var altheaLogo = $("#altheaLogo");
        var subTitle = $("#subTitle");

        var myCountry = $("#my-country");
        var sgCountry = $("#sg-country");
        var phCountry = $("#ph-country");
        var idCountry = $("#id-country");
        var thCountry = $("#th-country");
        var twCountry = $("#tw-country");
        var usCountry = $("#us-country");
        var interCountry = $("#inter-country");
        var idToUsCountry = $("#id-to-us-country");

        var appStore = $("#app-store");
        var playStore = $("#play-store");

        var yesTelcoBtn = $("#yesTelcoBtn");
        var noTelcoBtn = $("#noTelcoBtn");

        var selectTelcoModal = $("#selectTelcoModal");
        var introOthersModal = $("#introOthersModal");

        var _hoveredJqCountryContainer = null;

        function hoverCountry(jqCountryContainer, countryName) {
            if (_hoveredJqCountryContainer) {
                _hoveredJqCountryContainer.removeClass('hover');
            }

            _hoveredJqCountryContainer = jqCountryContainer;

            jqCountryContainer.addClass('hover');
            altheaCountryContainer.text(countryName);
        }

        function redirectCountry(countryCode) {
            if (countryCode) {
                document.location.href = "/setCountry/" + countryCode;
            }
        }

        myCountry.click(function (e) {
            console.log("my country clicked");
            redirectCountry("my");
        });

        myCountry.hover(function (e) {
            console.log("my country hover");
            hoverCountry(myCountry, "KOREA | MALAYSIA");
            return false;
        });

        sgCountry.click(function (e) {
            console.log("sg country clicked");
            redirectCountry("sg");
            return false;
        });

        sgCountry.hover(function (e) {
            console.log("sg country hover");
            hoverCountry(sgCountry, "KOREA | SINGAPORE");
            return false;
        });

        phCountry.click(function (e) {
            console.log("ph country clicked");
            redirectCountry("ph");
            return false;
        });

        phCountry.hover(function (e) {
            console.log("ph country hover");
            hoverCountry(phCountry, "KOREA | PHILIPPINES");
            return false;
        });


        idCountry.click(function (e) {
            console.log("id country clicked");
            selectTelcoModal.modal();
            return false;
        });

        idCountry.hover(function (e) {
            console.log("id country hover");
            hoverCountry(idCountry, "KOREA | INDONESIA");
            return false;
        });

        thCountry.click(function (e) {
            console.log("th country clicked");
            redirectCountry("th");

            return false;
        });

        thCountry.hover(function (e) {
            console.log("th country hover");
            hoverCountry(thCountry, "KOREA | THAILAND");
            return false;
        });

        twCountry.click(function (e) {
            console.log("tw country clicked");
            redirectCountry("tw");

            return false;
        });

        twCountry.hover(function (e) {
            console.log("tw country hover");
            hoverCountry(twCountry, "KOREA | TAIWAN");
            return false;
        });

        usCountry.click(function (e) {
            console.log("us country clicked");
            redirectCountry("us");
            return false;
        });

        usCountry.hover(function (e) {
            console.log("id country hover");
            hoverCountry(usCountry, "");
            return false;
        });

        interCountry.click(function (e) {
            console.log("inter country clicked");
            redirectCountry("international");
            return false;
        });

        interCountry.hover(function (e) {
            console.log("inter country hover");
            hoverCountry(interCountry, "");
            return false;
        });

        idToUsCountry.click(function (e) {
            console.log("id to us clicked");
            redirectCountry("us");
            return false;
        });

        idToUsCountry.hover(function (e) {
            console.log("id to us hover");
            hoverCountry(idToUsCountry, "KOREA | US");
            return false;
        });

        appStore.click(function (e) {
            console.log("appStore clicked");
            window.open('https://itunes.apple.com/my/app/althea/id1178916278','_blank');

            return false;
        });

        appStore.hover(function (e) {
            console.log("appStore hover");
            hoverCountry(appStore, "");
            return false;
        });

        playStore.click(function (e) {
            console.log("playStore clicked");
            window.open('https://play.google.com/store/apps/details?id=kr.althea.release','_blank');
            return false;
        });

        playStore.hover(function (e) {
            console.log("playStore hover");
            hoverCountry(playStore, "");
            return false;
        });

        yesTelcoBtn.click(function (e) {
            selectTelcoModal.modal();
            setTimeout(function () {
                introOthersModal.modal();
            }, 300);
        });

        noTelcoBtn.click(function (e) {
            selectTelcoModal.modal();
            setTimeout(function () {
                redirectCountry("id");
            }, 100);
        });
    });
</script>
</body>
</html>