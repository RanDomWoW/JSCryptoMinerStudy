<script type="text/javascript">
    function closePop_SignUp() {
        $('.signup_enjoy').hide();
    }

    function isEmailValid(email) {
	     var re = /[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?/i;
	     return re.test(email)		
    }

    function subscribeEmail(element) {
        var selector = $(element);

        if ($(selector).val() == '' || isEmailValid($(selector).val()) == false) {
            alert("メールアドレスをもう一度確認してください。");
            $(selector).focus();
            return false;
        }

        document.getElementById("pcOptinSubscribe").disabled = true;
        addSubscribeEmail(element);
        return false;
    }

    function addSubscribeEmail(selector) {
            var eMail = $(selector).val();

            $.ajax({
                type: "POST",
                url: "/Controls/Subscribe/EmailSubscribe.aspx",
                data: {
                    Email: eMail,
                    subTime: nowTime(),
                    emailType: 'forever21'
                },
                success: function (sResultText) {
                    if (sResultText == "") {                        
                        $(selector).val('');
                        $('.signup_enjoy').hide();
                        $('#email_result').show();
                    } else {
                        alert(sResultText);
                        $(selector).focus();
                    }
                    document.getElementById("pcOptinSubscribe").disabled = false;
                    return;
                },
                error: function () {
                    alert("登録できませんでした。もう一度登録し直してください。");
                    $(selector).focus();
                    document.getElementById("pcOptinSubscribe").disabled = false;
                    return;
                }
            });  
    }

