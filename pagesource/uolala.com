    <script type="text/javascript">
        function startsWith2(str, prefix) {
            if (str.length < prefix.length)
                return false;
            for (var i = prefix.length - 1; (i >= 0) && (str[i] === prefix[i]); --i)
                continue;
            return i < 0;
        }
        
        function showMmoreConversations(viewMoreButton) {
            //show loader
            $('.loader-more-comments').show();
            var oldOnclick = $('.view-more-comments').attr('onclick');
            $('.view-more-comments').click("return false;");
            var lastPostID =  $('#lastID').val();

            $.ajax({
                type: 'POST',
                url: "/default.aspx/showMoreConversations",
                data: JSON.stringify({ lastPostID: lastPostID, UolalaPageLanguageCode: getUolalaPageLanguageCode(), UolalaUrlLanguageCode: getUolalaUrlLanguageCode(), UolalaPageCountryCode: getUolalaPageCountryCode() }),
                contentType: 'application/json; charset=utf-8',
                async: true,
                dataType: "json",
                success: function (resp) {
                    //hide loader
                    $('.loader-more-comments').hide();
                    $('.view-more-comments').click(oldOnclick);
                    if (startsWith2(resp.d, 'Error:') == false && startsWith2(resp.d, 'Erreur:') == false) {
                        if (resp.d.indexOf("Empty") == -1){
                            var res = resp.d.split("-|-");
                            
                            //Save last postID
                            $('#lastID').val(res[0]);
                            
                            //Remove current view more posts button
                            var parent = $('.view-more-comments')[0].parentNode.parentNode.parentNode;
                            $(parent).remove();

                            //Append new posts
                            $('#groupCommentsDiv').append(res[1]);
                        }
                        else
                        {
                            $('.loader-more-comments').hide();
                            alert("No se han encontrado más conversaciones");
                        }
                    }
                    else {
                        alert(resp.d);
                    }
                },
                error: function (msg) {
                    $('.loader-more-comments').hide();
                    alert("Error: Ha ocurrido un error al intentar visualizar más conversaciones");
                }
            });
        }
    </script>

