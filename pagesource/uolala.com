        // addAddressToMap() is called when the geocoder returns an answer.
        function addAddressToMap(response, status) {
            if (status !== google.maps.GeocoderStatus.OK) {
                document.getElementById("cityName").value = '';
                document.getElementById("countryCode").value = '';
                document.getElementById("countryName").value = '';
                document.getElementById("longitude").value = '';
                document.getElementById("latitude").value = '';
            } else {
                place = response[0];
                var localityName = '';
                var countryName = '';
                var countryCode = '';
                for (var i = 0; i < place.address_components.length; i++) {
                    if (place.address_components[i].types[0] == "locality") {
                        localityName = place.address_components[i].long_name;
                    }
                    if (place.address_components[i].types[0] == "political") {
                        localityName = place.address_components[i].long_name;
                    }
                    if (place.address_components[i].types[0] == "country") {
                        countryName = place.address_components[i].long_name;
                        countryCode = place.address_components[i].short_name;
                    }
                }

                if (localityName == '' || countryName == '') {
                    document.getElementById("cityName").value = '';
                    document.getElementById("countryCode").value = '';
                    document.getElementById("countryName").value = '';
                    document.getElementById("longitude").value = '';
                    document.getElementById("latitude").value = '';
                } else {
                    document.getElementById("cityName").value = localityName;
                    document.getElementById("countryCode").value = countryCode;
                    document.getElementById("countryName").value = countryName;
                    document.getElementById("longitude").value = place.geometry.location.lng();
                    document.getElementById("latitude").value = place.geometry.location.lat();
                }
            }
            __doPostBack('bSearchBarClick', '');
        }

