                function ajaxContentBox(e) {
            if($("div#" + e + " #content-box").is(":empty"))
            {
                $.ajax({
                    url: base_url + "/UI/LoadMoviesHomepage/" + e,
                    type: "GET",
                    dataType: "json",
                    success: function(e) {
                        switch (e.type) {
                            case "random-movies":
                                $("#random-movies #content-box").html(e.content);
                                break;
                            case "top-view":
                                $("#top-view #content-box").html(e.content);
                                break;
                            case "top-rated":
                                $("#top-rated #content-box").html(e.content);
                                break;
                        }
                    }
                })
            }
        }

        function ajaxContentBoxS(e) {
            if($("div#" + e + " #content-box").is(":empty"))
            {
                $.ajax({
                    url: base_url + "/UI/LoadSeriesHomepage/" + e,
                    type: "GET",
                    dataType: "json",
                    success: function(e) {
                        switch (e.type) {
                        	case "series-recent":
                                $("#series-recent #content-box").html(e.content);
                                break;
                            case "random-series":
                                $("#random-series #content-box").html(e.content);
                                break;
                            case "top-view-series":
                                $("#top-view-series #content-box").html(e.content);
                                break;
                        }
                    }
                })
            }
        }

