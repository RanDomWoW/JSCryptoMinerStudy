                $( myTable.find("tbody tr") ).each(function() {

                    var fila=$(this);

                    var i=0;

                    var col=total-$( fila.find("td") ).length;

                    $( fila.find("td") ).each(function() {

                        var titulo = $(this);

                        titulo.attr("data-title",array[i+col]);

                        i=i+1;

                    }); 

