<script> 
jQuery(document).ready(function() {
	jQuery("#socialize").mouseover(function(){
		jQuery(".as_social").show();
	}); 
	jQuery("#socialize").click(function(){
		jQuery(".as_social").show();
	});	
	jQuery(".as_social ul").mouseleave(function(){
		jQuery(".as_social").hide();
	});	
	jQuery(".back-to-top").css({"display": "none"});	
	jQuery(".back-to-down").css({"display": "none"});	
	var offset = 0;
	var offsetTop = 150;
	var duration = 300;	 
	var elem = jQuery('body');
	jQuery(window).scroll(function() {								   
		if (jQuery(this).scrollTop() > offset) {			
			jQuery(".back-to-down").fadeIn(duration);
			if (jQuery(this).scrollTop() > offsetTop) {
				jQuery(".back-to-top").fadeIn(duration);
			}
		} 
		else {			
			jQuery(".back-to-down").fadeOut(duration);	
			jQuery(".back-to-top").fadeOut(duration);				
		}
		if(jQuery(window).scrollTop() == jQuery(document).height()-jQuery(window).height())
		{
			jQuery(".back-to-down").fadeOut(duration);
		}
		if(jQuery(window).scrollTop() < offsetTop)
		{
			jQuery(".back-to-top").fadeOut(duration);
		}		
	});	
	jQuery(".back-to-top").click(function(event) {
		event.preventDefault();
		jQuery("html, body").animate({scrollTop: 0}, duration);		
		return false;
	});
	jQuery(".back-to-down").click(function(event) {	
		event.preventDefault();	
		jQuery("html, body").animate({scrollTop: jQuery('body').height()}, 600);		
		return false;
	});
});
//video speed calculate
var varSpeed = 0;	
var imageAddr = "/portals/0/Images/test-small.jpg"; 
var downloadSize = 106496; //bytes
function MeasureConnectionSpeed() {
	var startTime, endTime;
	var download = new Image();
	download.onload = function () {
		endTime = (new Date()).getTime();
		showResults();
		//console.log("load");
		startvideo();
	}		
	download.onerror = function (err, msg) {
		varSpeed = 0;
		//console.log(varSpeed);
		startvideo();
	}		
	startTime = (new Date()).getTime();
	var cacheBuster = "?nnn=" + startTime;
	download.src = imageAddr + cacheBuster;
	function showResults() {
		var duration = (endTime - startTime) / 1000;
		var bitsLoaded = downloadSize * 8;
		var speedBps = (bitsLoaded / duration).toFixed(2);
		var speedKbps = (speedBps / 1024).toFixed(2);
		var speedMbps = (speedKbps / 1024).toFixed(2);  
		//alert(speedMbps + "asdf");
		varSpeed = speedMbps;
		//alert("Speed: " + varSpeed);
	}			
}
//video speed calculate End	
jQuery(window).load(function(){
	MeasureConnectionSpeed();	
//	jQuery("#big-video-vid_html5_api").attr('style', 'top: -30px !important');
});
function startvideo(){
	var wd = jQuery(window).width();
	jQuery(".overlayText").css({"height":"440px"});
	jQuery(".overlayTextArea").css({"width":wd});
	var BV = new jQuery.BigVideo({container: jQuery('.mainVideoContainer'),useFlashForFirefox:false,doLoop:true});
	BV.init();	
	var mp4Video;
	var webmVideo;				
	if(varSpeed >= 5)
	{					
		var mp4VideoMain		="http://newauto.automaticdevelopment.com/assets/bigVideo/speedVideos/slide1VideoLoopNoSound1mbps.mp4";
		var webmVideoMain		="http://newauto.automaticdevelopment.com/assets/bigVideo/speedVideos/slide1VideoLoopNoSound1mbps.webm";				
	}
	else if(varSpeed >= 3 && varSpeed < 5)
	{
		var mp4VideoMain		="http://newauto.automaticdevelopment.com/assets/bigVideo/speedVideos/slide1VideoLoopNoSound1mbps.mp4";
		var webmVideoMain		="http://newauto.automaticdevelopment.com/assets/bigVideo/speedVideos/slide1VideoLoopNoSound1mbps.webm";
	}
	else if(varSpeed >= 1 && varSpeed < 3)
	{
		var mp4VideoMain		="http://newauto.automaticdevelopment.com/assets/bigVideo/speedVideos/slide1VideoLoopNoSound1mbps.mp4";
		var webmVideoMain		="http://newauto.automaticdevelopment.com/assets/bigVideo/speedVideos/slide1VideoLoopNoSound1mbps.webm";
	}
	else if(varSpeed <  1)
	{
		var mp4VideoMain		="http://newauto.automaticdevelopment.com/assets/bigVideo/speedVideos/slide1VideoLoopNoSound1mbps.mp4";
		var webmVideoMain		="http://newauto.automaticdevelopment.com/assets/bigVideo/speedVideos/slide1VideoLoopNoSound1mbps.webm";
	}
	var os = navigator.platform;
	osVal = os.substr(0,3);
	if(osVal == 'Win'){
		BV.show([
			 { type: "video/mp4",  src: mp4VideoMain }
		]);
	}
	else if(osVal == 'Mac')
	{
		if(navigator.userAgent.indexOf("Firefox")!=-1)
		{
			BV.show([
				{type: "video/webm", src: webmVideoMain}
			]);
			BV.getPlayer().volume(0);
			BV.getPlayer().play();
		}
		else
		{
			BV.show([
				{type: "video/mp4", src: mp4VideoMain}						 
			]);
			BV.getPlayer().volume(0);
			BV.getPlayer().play();
		}
	}
	if (navigator.userAgent.match(/(opera mini|ipod|iphone|up.browser|up.link|mmp|symbian|smartphone|midp|wap|vodafone|o2|pocket|kindle|mobile|psp|treo|nokia|blackberry|pre\/0.1|android)/i))
	{
		BV.show([
			 { type: "video/mp4",  src: mp4VideoMain }
		]);
	}	
	BV.getPlayer().volume(0);
	BV.getPlayer().play();
	jQuery(".playVideo").hide();
	jQuery(".soundOff").hide();	
	jQuery(".soundOn").show();
	jQuery(".fullScreenoff").hide();
	jQuery(".playVideo").click(function(){										
		BV.getPlayer().play();
		BV.getPlayer().volume(1);	
		jQuery(".playVideo").hide();
		jQuery(".pauseVideo").show();		
		//jQuery(".bannerImageVideo").css({"display":"none"});
		if(wd<=1024)
		{
			jQuery(".soundOff").hide();
			jQuery(".soundOn").hide();
		}
		else
		{
			jQuery(".soundOff").show();
			jQuery(".soundOn").hide();
		}
	});	
	jQuery(".pauseVideo").click(function(){
		BV.getPlayer().pause();
		jQuery(".pauseVideo").hide();
		jQuery(".playVideo").show();			
		if(wd<=767)
		{
			var heCal=jQuery("#big-video-vid_html5_api").height();
			jQuery(".videoArea").css({"height":heCal});
		}
		else
		{
			//jQuery(".bannerImageVideo").css({"display":"block"});
			//jQuery(".overlayText").css({"height":"440px"});
			jQuery(".videoArea").css({"height":"440px"});	
			//jQuery(".playVideo").hide();
			//jQuery(".soundOff").hide();	
			//jQuery(".playVideoTop").show();
		}
	});	
	jQuery(".soundOff").click(function(){
		BV.getPlayer().volume(0);
		jQuery(".soundOff").hide();
		jQuery(".soundOn").show();
	});				
	jQuery(".soundOn").click(function(){
		BV.getPlayer().volume(1);
		jQuery(".soundOn").hide();
		jQuery(".soundOff").show();
	});
	BV.getPlayer().on('ended', function(){
		BV.getPlayer().pause();
		BV.getPlayer().volume(0);		
		jQuery(".playVideo").show();		
		jQuery(".pauseVideo").hide();
		jQuery(".soundOn").show();
		jQuery(".soundOff").hide();
	});	
	jQuery(".fullScreenoff").click(function(){
		toggleFullScreen();
		jQuery(".fullScreenon").show();
		jQuery(".fullScreenoff").hide();
	});
	jQuery(".fullScreenon").click(function(){
		toggleFullScreen();
		jQuery(".fullScreenon").hide();
		jQuery(".fullScreenoff").show();
		jQuery("header, .container, .containerMiddle, footer").hide();	
	});	
	
	this.fullScreenMode = document.fullScreen || document.mozFullScreen || document.webkitIsFullScreen; // This will return true or false depending on if it's full screen or not.		
	jQuery(document).on ('mozfullscreenchange webkitfullscreenchange fullscreenchange',function(){																						
		this.fullScreenMode = !this.fullScreenMode;						
		if(!this.fullScreenMode)
		{
			jQuery('.fullScreenon, .bannerImageVideo').css('display','block');			
			jQuery('.fullScreenoff').css('display','none');
			jQuery("header, .container, .containerMiddle, footer").show();
			jQuery(".videoArea").css({"height":"440px"});
			jQuery("#big-video-vid_html5_api").attr('style', 'top: -30px !important');
		}
		else if(this.fullScreenMode)
		{
			var heCal=jQuery(window).height();			
			jQuery(".videoArea").css({"margin-top":"0px"});
			jQuery(".videoArea, #big-video-vid_html5_api").css({"height":heCal});
			jQuery("#big-video-vid").attr('style', 'height: '+ heCal +' !important');
			jQuery('.fullScreenon, .bannerImageVideo').css('display','none');			
			jQuery('.fullScreenoff').css('display','block');
		}
	});	
}
jQuery(window).keydown(function(event){
    if (event.keyCode == 13 || event.keyCode == 70) {		
      event.preventDefault();	  
      return false;
    }
});
jQuery(window).resize(function(){
	var wd = jQuery(window).width();
	jQuery(".overlayTextArea").css({"width":wd});
	this.fullScreenMode = document.fullScreen || document.mozFullScreen || document.webkitIsFullScreen; // This will return true or false depending on if it's full screen or not.				
	if(!this.fullScreenMode)
	{
		jQuery(".videoArea").css({"height":"440px"});
	}
});
