                                $('.ajax_result2').html(result2);
                            }else{
                                $('.ajax_result1').html(response);
                            }
							
						},
					dataType: 'html',
					cache:false
				});
                
                $('.ajax_result2').html('<option value="0">選択しない</option>');
                
			}
            
