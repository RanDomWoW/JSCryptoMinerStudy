<script> jq_10(document).ready(function(){ show_hide(".eb_hide_wrap",".eb_hide_body"); }); </script>
<script>
function show_hide(head, body){
         jq_10(head).children("h3").children("span").text("+");
         jq_10(body).hide();
         jq_10(head).children("h3").click(function(){
           if(jq_10(this).children("span").text() == "-"){
              jq_10(this).parents(head).children(body).hide('fast');
              jq_10(this).children("span").text("+");
           }else
           {
               jq_10(this).parents(head).children(body).show('fast');
              jq_10(this).children("span").text("-");
           }

       });
    }
</script>

