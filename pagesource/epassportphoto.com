    epp.twittercb = function(input) { // Called via JSONP (see below).
        var html;
        var results = input.results;
        if (results.length == 0) {
            html = '<div class="tweet">Oh no! We couldn\'t find any tweets about ePassportPhoto.com. Just mention "epassportphoto" in your tweet and it will show up here.</div>';
        } else {
            for (var i = 0, len = results.length; i < len; ++i) {
                var tweet = results[i];
                html = '<div class="tweet"><img src="' + tweet.profile_image_url + '" style="float:left;margin:3px 3px 0 0" />' +
                    '<a href="http://twitter.com/' + tweet.from_user + '">' + tweet.from_user + '</a> ' + tweet.text + '<div class="clear"></div></div>';
            }
        }
        $('#tweets').html(html);
    };

    function initPage() {
        var pattern = new RegExp('Passport', 'i');
        var standard = $('#standard');

        standard.change(function(e) {
            var form = $('#service-form');
            form.attr('action', $('#standard').val());
        });

        var getUrl = function(id, country, standard) {
            var urlParts = [
                    encodeURIComponent(id),
                    encodeURIComponent(country),
                    encodeURIComponent(standard)
            ];
            return urlParts.join('/');
        };

        var lastCountry = null;
        $('#country').on('change keypress', function(e) {
            // Reset the standard options for the selected country.
            var countryName = $(this).val();
            if (countryName == lastCountry)
                return;

            standard.empty();

            var standards = epp.countries[countryName];
            for (var i = 0, len = standards.length; i < len; ++i) {
                var currentStandard = standards[i];
                var standardId = currentStandard[0];
                var standardName = currentStandard[1];
                var standardDimensions = currentStandard[2];
                var url = getUrl(standardId, countryName, standardName);
                standard.append(
                        $('<option></option>')
                                .attr('value', url)
                                .text(standardName + ' (' + standardDimensions + ')')
                );
            }

            var defaultStandard = null;
            for (i = 0; i < standards.length; i++) {
                var name = standards[i][1];
                if (name.search(pattern) != -1) {
                    defaultStandard = name;
                    var defaultStandardUrl = getUrl(defaultStandard[0], countryName, defaultStandard[1]);
                    $(standard).val(defaultStandardUrl).change();
                    break;
                }
            }
            if (standard[0] && standard[0].jcf && !standard[0].jcf.options.showNativeDrop) {
                standard[0].jcf.destroy();
            }
            jcf.customForms.replaceAll();
        }).change();
    }

    $(function() {
        initPage();
    });

    window.onpageshow = initPage;
