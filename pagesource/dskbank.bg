                if (input.is("[type=file]")) {
                    var sizeRestrictions = input.closest("div[data-sf-config]").attr("data-sf-config");
                    var anchor = document.createElement('a');
                    anchor.innerHTML = sizeRestrictions;
                    var sizeRestrictionsAsJson = JSON.parse(anchor.textContent);
                    var maxSize = sizeRestrictionsAsJson.MaxFileSizeInMb;
                    return 'Невалиден размер на файл, моля използвайте размер по-малък от ' + maxSize + 'MB';
                }
            },
            textArea: 'Моля пишете на кирилица',
            pattern: function (input) {
                if (input.is("[type=email]")) {
                    return 'Невалиден формат на и-мейл';
                }

                if (input.parents(".js-validate-cyrillic").first().length == 1) {
                    return 'Невалиден формат, моля въведете вашите имена на кирилица';
                }

                if (input.parents(".js-validate-cyrillic-two-names").first().length == 1) {
                    return 'Моля въведете име и фамилия на кирилица';
                }

                if (input.parents(".js-validate-egn").first().length == 1) {
                    return 'Невалиден формат на ЕГН';
                }

                if (input.parents(".js-validate-phone-number").first().length == 1) {
                    return 'Невалиден телефонен номер';
                }

                if (input.parents(".js-validate-idcard-number").first().length == 1) {
                    return 'Невалиден номер на лична карта';
                }

                if (input.parents(".js-validate-idcard-address").first().length == 1) {
                    return 'Невалиден адрес';
                }

                if (input.parents(".js-validate-zip-code").first().length == 1) {
                    return 'Невалиден пощенски код';
                }

                if (input.parents(".js-validate-latin-two-names").first().length == 1) {
                    return 'Невалиден формат, моля въведете име и фамилия, използвайки латиница';
                }
            },
            adultEGN: 'Услугата е достъпна само за пълнолетни граждани.',
        };

        var validator = $("#85ac2aac-4d38-650e-9d59-ff0200048325").kendoValidator({
            messages: validationMessages,
            rules: {
                fileFormat: function (input) {
                    if (input.is("[type=file]")) {
                        var allowedFormats = input.attr("accept");
                        var allowedFormatsArray = allowedFormats.split(",");
                        var files = input.eq(0)[0].files;
                        if (files.length != 0) {
                            for (var i = 0; i < files.length; i++) {
                                var fileFormat = files[i].name.substr(files[i].name.indexOf('.'));
                                if ($.inArray(fileFormat, allowedFormatsArray) == -1) {
                                    return false;
                                }
                            }
                        }
                    }

                    return true;
                },
                fileSize: function (input) {
                    if (input.is("[type=file]")) {
                        var files = input.eq(0)[0].files;
                        if (files.length != 0) {
                            var sizeRestrictions = input.closest("div[data-sf-config]").attr("data-sf-config");
                            var anchor = document.createElement('a');
                            anchor.innerHTML = sizeRestrictions;
                            var sizeRestrictionsAsJson = JSON.parse(anchor.textContent);
                            var minSize = sizeRestrictionsAsJson.MinFileSizeInMb;
                            var maxSize = sizeRestrictionsAsJson.MaxFileSizeInMb;
                            if (maxSize != 0) {
                                for (var i = 0; i < files.length; i++) {
                                    var fileSize = files[i].size;
                                    if (!(minSize * 1048576 < fileSize && fileSize < maxSize * 1048576)) {
                                        return false;
                                    }
                                }
                            }
                        }
                    }

                    return true;
                },
                adultEGN: function (input) {
                    if (input.parents(".js-validate-adult-egn").first().length == 1 && input.is("[type=text]")) {
                        var egn = input.val();
                        var today = new Date();
                        if (egn.substring(2, 3) === '4' || egn.substring(2, 3) === '5') {
                            var difference = (Number(today.getFullYear()) - Number('20' + egn.substr(0, 2)));
                            if (Number(difference) < 18) {
                                return false;
                            }
                            else if (Number(difference) == 18) {
                                if (Number(egn.substring(2, 4)) - 40 < (Number(today.getMonth()) + 1)) {
                                    return true;
                                }
                                else if (Number(egn.substring(2, 4)) - 40 == (Number(today.getMonth()) + 1)) {
                                    if (Number(egn.substring(4, 6)) <= Number(today.getDate())) {
                                        return true;
                                    }
                                }
                            }
                        }
                        else {
                            if ((Number(today.getFullYear()) - Number('19' + egn.substr(0, 2))) > 18) {
                                return true
                            }
                            else if ((Number(today.getFullYear()) - Number('19' + egn.substr(0, 2))) == 18) {
                                if (Number(egn.substring(2, 4)) < (Number(today.getMonth()) + 1)) {
                                    return true;
                                }
                                else if (Number(egn.substring(2, 4)) == (Number(today.getMonth()) + 1)) {
                                    if (Number(egn.substring(4, 6)) <= Number(today.getDate())) {
                                        return true;
                                    }
                                }
                            }
                        }

                        return false;
                    }
                    return true;
                },
                textArea: function (input) {
                    if (input[0].tagName == "TEXTAREA") {
                        if (input.parents(".js-validate-cyrillic-text").first().length == 1) {
                            var textareaValue = input.val();
                            var expression = input[0].getAttribute("data-pattern");
                            var regex = new RegExp(expression);
                            var match = textareaValue.match(regex);
                            return match != null && textareaValue == match[0]
                        }
                    }
                    return true;
                }
            }
        }).data("kendoValidator");

        $("#85ac2aac-4d38-650e-9d59-ff0200048325 button[type='submit']").click(function (event) {
            if (!validator.validate()) {
