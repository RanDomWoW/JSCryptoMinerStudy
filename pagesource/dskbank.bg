                var sources = $('input[id*=utm_source]');
                if (sources.length>0) {
                    for (var i = 0; i < sources.length; i++) {
                        sources.eq(i).val(utm_source);
                    }
                }

                var medium = $('input[id*=utm_medium]');
                if (medium.length>0) {
                    for (var i = 0; i < medium.length; i++) {
                        medium.eq(i).val(utm_medium);
                    }
                }

                var consultantReferenceParam = $('input[id*=ConsultantReference]');
                if (consultantReferenceParam.length>0) {
                    for (var i = 0; i < consultantReferenceParam.length; i++) {
                        consultantReferenceParam.eq(i).val(consultantReference);
                    }
                }
            }
        });

        $("#85ac2aac-4d38-650e-9d59-ff0200048325 button[type='submit']").ajaxComplete(function () {
            $("#85ac2aac-4d38-650e-9d59-ff0200048325 button[type='submit']").removeAttr("disabled");
        });

        function getUrlVars() {
            var vars = [], hash;
            var hashes = window.location.search.slice(window.location.search.indexOf('?') + 1).split('&');
            for (var i = 0; i < hashes.length; i++) {
                hash = hashes[i].split('=');
                vars.push(hash[0]);
                vars[hash[0]] = hash[1];
            }
            return vars;
        }
    });
</script>


