<script>
// Get the modal
var modal = document.getElementById('mg_kd-myModal');
var modal2 = document.getElementById('mg_kd-myModal2');

// Get the button that opens the modal
var btn = document.getElementById("mg_kd-myBtn");

// Get the <span> and <a> element that closes the modal
var span = document.getElementsByClassName("mg_kd-close")[0];


// When the user clicks the button, open the modal 
btn.onclick = function() {
    modal.style.display = "block";
    modal2.style.display = "block";
}

// When the user clicks on <span> or <a> (x), close the modal
span.onclick = function() {
    modal.style.display = "none";
    modal2.style.display = "none";
    document.getElementById("first_span").innerHTML = '';
    document.getElementById("second_span").innerHTML = '';
    document.getElementById("third_span").innerHTML = '';
}

// When the user clicks anywhere outside of the modal, close it
window.onclick = function(event) {
    if (event.target == modal) {
        modal.style.display = "none";
        modal2.style.display = "none";
        document.getElementById("first_span").innerHTML = '';
        document.getElementById("second_span").innerHTML = '';
        document.getElementById("third_span").innerHTML = ''; 
    }
}
</script>

