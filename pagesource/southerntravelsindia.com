<script>
    $(function() {
        $('input[type=text]').each(function() {
            titleText = $(this).attr('placeholder');
            $(this).attr('title', titleText);
            $(this).attr('data-toggle', 'tooltip').tooltip({

                placement: "right",
                trigger: "focus"


            });

           
        });



        $('.placescoveredtbl .pcwrap ul li a').click(function(e) {
