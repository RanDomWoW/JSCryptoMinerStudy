            chk = false;
            return false;
        }
        if (document.getElementById("UCFooter_txtEmail").value == "") {
            var msg = "Please enter the email.";
            alert(msg);
            document.getElementById('UCFooter_txtEmail').focus();
            chk = false;
            return false;
        }
        if (document.getElementById("UCFooter_txtDeparturedate").value == "") {
            var msg = "Please enter the departure date.";
            alert(msg);
            document.getElementById('UCFooter_txtDeparturedate').focus();
            chk = false;
            return false;
        }
        if (document.getElementById("UCFooter_txtArrivaldate").value == "") {
            var msg = "Please enter the arrival date.";
            alert(msg);
            document.getElementById('UCFooter_txtArrivaldate').focus();
            chk = false;
            return false;
        }
        if (document.getElementById("UCFooter_txtShortDescription").value == "") {
            var msg = "Please enter short description";
            alert(msg);
            document.getElementById('UCFooter_txtShortDescription').focus();
            chk = false;
            return false;
        }
        if (chk) {
            document.getElementById('UCFooter_btnSubmitDe').style.display = 'none';
        }
    }
    function isNumberKey(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode
        if (charCode > 31 && (charCode < 48 || charCode > 57))
            return false;
        return true;
    }
    function Check_Length(my_Object) {
        var maxLen = 250;
        if (document.getElementById("UCFooter_txtShortDescription").value.length >= maxLen) {
           // document.getElementById("UCFooter_txtShortDescription").value = '';
            var msg = "Please enter maximum 250 characters allowed.";
            alert(msg);
            document.getElementById('UCFooter_txtShortDescription').focus();
        }
    }
    function CheckOnlyCharacter(evt) {
        var kk
        kk = (evt.which) ? evt.which : event.keyCode
        if ((kk >= 65 && kk <= 90) || (kk >= 97 && kk <= 122) || kk == 32 || kk == 190 || kk == 8 || kk == 9 || kk == 127 || kk == 16 || kk == 20 || kk == 46) {
            return true;
        }
        alert("Please enter characters only.");
        return false;
    }
</script>
