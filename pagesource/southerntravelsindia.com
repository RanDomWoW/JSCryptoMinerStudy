        var charCode = (evt.which) ? evt.which : event.keyCode
        if (charCode > 31 && (charCode < 48 || charCode > 57))
            return false;
        return true;
    }
    function Left(str, n) {
        if (n <= 0)
            return "";
        else if (n >= String(str).length)
            return str;
        else
            return String(str).substring(0, n);
    }


    function chkNumeric(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode
        if (charCode > 31 && (charCode < 48 || charCode > 57))
            return false;
        return true;
