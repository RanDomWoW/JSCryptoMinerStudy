          $.each(urlTargeting, function (k, v) {
              googletag.pubads().setTargeting(k, v);
          });

          googletag.pubads().enableSingleRequest();
          googletag.pubads().disableInitialLoad();
          googletag.pubads().collapseEmptyDivs();
          googletag.enableServices();
        });

        googletag.pubads().addEventListener('slotRenderEnded', function(event) {
          if(event && event.slot){
            window.en_slots = window.en_slots || {};
            window.en_slots[event.slot.getAdUnitPath()]=1;
          }

          if (event.isEmpty){
            var adslotID = event.slot.getSlotElementID();
            $("#" + adslotID).siblings('.adLabel').hide();
          }

          if (event.slot === slots['Superboard1']) {
            parallaxAdsInit(parallaxAdId);
            loadStickySkyscraper();
          }

        });

      }

      // Intersection Observer for adunits
      var config = {
        rootMargin: '50% 0%',
        threshold: 0
      };
      var adObserver = new IntersectionObserver( function( entries, self )
      {
        entries.forEach( function ( entry )
        {
          var ad_slot = entry.target.getAttribute('ID');
          if (entry.isIntersecting || entry.intersectionRatio > 0)
          {
            if(ad_slot)
            {
              googletag.cmd.push(function ()
              {
                googletag.pubads().refresh([ slots[ad_slot] ]);
              });
              self.unobserve(entry.target);
            }
          }
        });
      }, config);
      var adunits = document.querySelectorAll('.adunit');
      adunits.forEach(function (target) {
        adObserver.observe(target);
      });

}


function getUrlTargeting()
{
  // Get the url and parse it to its component parts using regex from RFC2396 Appendix-B (https://tools.ietf.org/html/rfc2396#appendix-B)
  var urlMatches = (window.location.toString()).match(/^(([^:/?#]+):)?(\/\/([^/?#]*))?([^?#]*)(\?([^#]*))?(#(.*))?/);
  var matchedAuthority = urlMatches[4] || '';
  var matchedPath = (urlMatches[5] || '').replace(/(.)\/$/, '$1');
  var matchedQuery = urlMatches[7] || '';
  // Get the query params for targeting against
  var params = matchedQuery.replace(/\=/ig, ':').split('&');

  return {
      UrlHost: matchedAuthority,
      UrlPath: matchedPath,
      UrlQuery: params
  };
}
