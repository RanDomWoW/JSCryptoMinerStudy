<script>

function mouseX(evt) {
if (evt.pageX) return evt.pageX;
else if (evt.clientX)
   return evt.clientX + (document.documentElement.scrollLeft ?
   document.documentElement.scrollLeft :
   document.body.scrollLeft);
else return null;
}

function mouseY(evt) {
if (evt.pageY) return evt.pageY;
else if (evt.clientY)
   return evt.clientY + (document.documentElement.scrollTop ?
   document.documentElement.scrollTop :
   document.body.scrollTop);
else return null;
}



var mapNowSel;


function mapOver(uri, el) {

$('#flag_' + uri).addClass('sel');

mapNowSel = document.getElementById('mapTitle' + uri);
mapNowSel.className = 'sel';
document.getElementById('mapNameHint').innerHTML = el.alt;
document.getElementById('mapNameHint').style.display = 'block';
}

function mapOut() {
$('.country').removeClass('sel');

mapNowSel.className = '';
mapNowSel = null;
document.getElementById('mapNameHint').style.display = 'none';
}

function getMousePos(e) {
document.getElementById('mapNameHint').style.left = (mouseX(e) + 10) + 'px';
document.getElementById('mapNameHint').style.top = (mouseY(e) + 10) + 'px';
}

document.onmousemove = getMousePos;

</script>

