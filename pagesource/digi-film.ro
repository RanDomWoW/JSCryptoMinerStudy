$(window).load(function(){
  $(' #wrapper > #page > #home_rotator > .flexslider').flexslider({
    animation: "slide",
    start: function(slider){
        $('body').removeClass('loading');
    }
  });
});



$(function() {
    var visible_items = 10;
    var final_position = 7;
    var last_final_position = visible_items - final_position;
    
    var tree = '#wrapper #page #home_rotator #top-program-tv .widget.sidebar-program-tv .sidebar-tv-list .tab_1_tv';
    var li = $(tree + ' li');
    var total_items = li.length;
    
    if(total_items > visible_items){// hidden items
    
        var active_item = ( $(tree + ' li.activ').index() )+1;

        /* higher than active final position*/
        if(active_item > final_position){
               
            if( active_item > visible_items ){//in hidden area

                var x = total_items - active_item;
            
                if(x < last_final_position ){
                    var number_of_items_to_del = active_item - final_position - last_final_position + x; 
                    $( tree + ' li:lt(' + number_of_items_to_del + ')' ).hide();
                }else{
                    var number_of_items_to_del = active_item - final_position;
                    $( tree + ' li:lt(' + number_of_items_to_del + ')' ).hide();
                }  
                
            }
            
        }
        
    }
    
});



</script>
