						str += '<div style="float:left;width: 100%;height:30px;margin-top: 30px;text-align: center;font-size: 12px" class="grey8">';
						str += json.errstr;
						str += '</div>';
						$('#BeEditorDiv').html(str);
						setTimeout(function(){
							diag.close();
						},2000);
					}else{
						str = '<div class="font16 font_yahei yellow1 font_bold" style="float:left;width: 100%;margin-top: 20px;text-align: center;">加入成功</div>';
						str += '<div style="float:left;width: 100%;height:30px;margin-top: 30px;text-align: center;font-size: 12px" class="grey8">您是本社第';
						str += json.data.users;
						str += '位社员</div>';
						$('#BeEditorDiv').html(str);
						setTimeout(function(){
							window.location.reload();
						},2000);
					}
				});
			};
			diag.thinklaterEvent = function(){
				diag.close();
			};
			diag.show();
			return false;
		}else{
			return true;
		}
	}

