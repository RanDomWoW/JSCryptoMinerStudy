    $(document).ready(function(){

        
        $.getJSON('/FlackTwister/' + (location.pathname == '/' ? 6813 : 9815) + '/', function (data) {
            var rndr = function (tpl, data) {
                if (typeof tpl != 'undefined')
                    return tpl.split('{{show.url}}').join(data.show_url)
                        .split('{{show.name}}').join(data.show_name)
                        .split('{{img.src}}').join(data.img_url)
                        .split('{{stage.url}}').join(data.stage_url)
                        .split('{{stage.name}}').join(data.stage_name)
            };
            for (var labid in data) {
                var lab = $('#pers-lab-' + labid);
                var nlen = {show: labid == 1 ? 60 : 65, stage: labid == 1 ? 60 : 65};
                var item = lab.find('[type="template"]').remove();
                  if (labid==21 && data[labid][1].length < 3) {
                    continue;
                  }
                if (data[labid][1].length > 0) {
                    if (lab.hasClass('promo-side-banner-right') || lab.hasClass('promo-side-banner-left')) {
                        checkSideBanners();
                    } else {
                        lab.show();
                    }
                } else {
                    continue;
                }
                if (lab.find('.appender').size() > 0) {
                    lab = lab.find('.appender')
                }

                for (var i in data[labid][1]) {
                    var bb = data[labid][1][i];
                    bb.show_name = bb.show_name.length > nlen.show ? (bb.show_name.substr(0, nlen.show) + '...') : bb.show_name;
                    bb.stage_name = bb.stage_name.length > nlen.stage ? (bb.stage_name.substr(0, nlen.stage) + '...') : bb.stage_name;
                    lab.append(rndr(item.html(), bb));
                }
                var after = lab.find('[type="template-after"]').remove();
                if (after.length > 0) {
                    eval(after.html())
                }
            }
        });
        $('#afk').load();

        $(window).resize(checkSideBanners);

        $.get('/private/getWishlist/', function(res){
            $('.ticketland').append(res);
        });

        function checkSideBanners(){
            var maxWidthForShowSideBanners = 1560;
            var currentWindowWidth = $(this).width();
            if(currentWindowWidth > maxWidthForShowSideBanners) {
                $(".promo-side-banner-left,.promo-side-banner-right").show();
                return true;
            } else {
                $(".promo-side-banner-left,.promo-side-banner-right").hide();
                return false;
            }
        }
    });
</script>

