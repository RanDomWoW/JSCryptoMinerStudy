	          kota = kota.replace('Kota ', '');
	          kota = kota.replace('Kabupaten ', '');
	        }
	      }
	      if (administrasi2 == 0) {
	        kota = data.results[0].address_components[0].long_name;
	      }
	          var location = $('#location').val();
	          var radius = $('#radius').val();
	          $.post( "https://wahana.com/Ajaxx/temukanAgenMaps", 
	          {'radius':radius, 'lat':lati, 'long':long, 'kabkot':kota },
	              function() {}).done(function(data){
	                  // console.log(JSON.parse(data));
	                  setMarker(JSON.parse(data), lati, long);
	                  setList(JSON.parse(data), lati, long);
	          });
      });
    }

