		function trackShiping(){
            var ttk 	= $("#ttk").val();
            var ttkNav 	= $("#ttkNav").val();
            var t;
            if(ttk.length == 0 && ttkNav.length == 0){            	
            	$('#alertModal #myModalLabel').html('Error');
            	$('#alertModal .modal-body').html('data anda belum lengkap!');
            	$('#alertModal').modal('show');
            	return false;
            }
            if (ttk.length != 0) {
            	t = ttk;
            }
            else
            {
            	t = ttkNav;
            }
            $.post( "https://wahana.com/Ajaxx/getTtkHome", 
                {'ttk':t },
                    function() {}).done(function(data){
                    	if (data != 0) {
	                        $('#resultTtk').html(data);
	                        $('#collapseOne').collapse();
                            // $("#collapseOne").collapse('toggle');
                            $("#collapseOne").collapse('show');
                            $("#collapseOne").collapse('shown');
                    	}
                    	else {
                    		$('#resultTtk').html('');
                    		$('#alertModal .modal-body').html('data tidak ada!');
            				$('#alertModal').modal('show');
                    	}
            });
        }

