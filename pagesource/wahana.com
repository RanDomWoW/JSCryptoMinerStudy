	<script type="text/javascript" id="scriptMaps">

  var myLat;
  var myLng;
  var markers = [];
        
  function clickAgen()
        {
              $('#myModal').modal({
                show: 'true'
            }); 

        }
        
    function clickTracking()
        {
              $('#myModal2').modal({
                show: 'false'
            }); 

        }

  function startGeolocation()
  {  	
	  if (navigator.geolocation) {
	    navigator.geolocation.getCurrentPosition(showPosition, showError);
	  } else {
	    // Browser doesn't support Geolocation
	    $('#alertModal #myModalLabel').html('Error');
	    $('#alertModal .modal-body').html('Your browser not support maps geolocation!');
		$('#alertModal').modal('show');
	  }
  }

  function getDirection(t){
    var lat = $(t).data("lat");
    var lng = $(t).data("lng");
    var lat1 = $(t).data("lat1");
    var lng1 = $(t).data("lng1");
    var from = '';
    var to = '';
    from+=lat+','+lng;
    to+=lat1+','+lng1;
    Direction(from.toString(), to.toString());
  }

  function Direction(from, to) {
    var directionsService = new google.maps.DirectionsService;
    var directionsDisplay = new google.maps.DirectionsRenderer;
    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 7,
      center: {lat: 41.85, lng: -87.65}
    });
    directionsDisplay.setMap(map);
    
    calculateAndDisplayRoute(directionsService, directionsDisplay, from, to);
  }

  function calculateAndDisplayRoute(directionsService, directionsDisplay, from, to) {
    directionsService.route({
        origin: from,
        destination: to,
      travelMode: google.maps.TravelMode.DRIVING
    }, function(response, status) {
      if (status === google.maps.DirectionsStatus.OK) {
      	// change routes
      	var far = response.routes[0].legs[0].distance.value;
      	var duration = response.routes[0].legs[0].duration.text;
      	far = (far/1000).toFixed(1);
      	var result = "<div style='background-color:yellow;' class='divBack' onClick='startGeolocation1()'>BACK</div><br>";
      	var result = result + "" + far + " Km - " + duration +"<br>";      	
      	
        for (var i =0; i < response.routes[0].legs[0].steps.length; i++){
            result+=response.routes[0].legs[0].steps[i].instructions+"<br>"
        }
        $('ul.list-alamat').html(result);
        directionsDisplay.setDirections(response);
      } else {
      	$('#alertModal #myModalLabel').html('Error');
      	$('#alertModal .modal-body').html('Directions request failed due to ' + status);
		$('#alertModal').modal('show');
      }
    });
  }

  function showError(error) {
    myLat = -6.279890;
    myLng = 106.761894;
    // getGeocode(myLat, myLng);
    getLocation(myLat, myLng, 25);    
  }

  function showPosition(position) {
      myLat = position.coords.latitude;
      myLng = position.coords.longitude;
      // console.log('lat: '+myLat);
      // getGeocode(myLat, myLng);
      getLocation(myLat, myLng, 25); 
  }

  function getLocation(myLat, myLng, radius){
    $.post( "https://wahana.com/Ajaxx/temukanAgenMaps", 
    {'radius':5, 'lat':myLat, 'long':myLng},
        function() {}).done(function(data){
          // alert(data);
        	// console.log('data :'+ data);
          setMarker(JSON.parse(data), myLat, myLng);   
          // setList(JSON.parse(data), myLat, myLng);
    });
  }

    function setList(location, lat, lng){

      $.post( "https://wahana.com/Ajaxx/setListAgentHome", 
  		{'location':location, 'lat':lat, 'lng':lng },
  		    function() {}).done(function(data){
        		$('.find-agent').html(data);
  		});
      }

    function setMarker(location , lat, lng){
      var map = new google.maps.Map(document.getElementById('map'), {
        zoom: 12,
        center: new google.maps.LatLng(lat,lng),
        mapTypeId: google.maps.MapTypeId.ROADMAP
      });

      var markerPusat = {
          url: 'https://wahana.com/assets/images/wahana/pusat2.png', // url
          scaledSize: new google.maps.Size(40, 40), // scaled size
      };

     	

      var beachMarker = new google.maps.Marker({
        position: {lat: lat, lng: lng},
        map: map,
        icon: markerPusat
      });
          

      var infowindow = new google.maps.InfoWindow();

       beachMarker.addListener('mouseup', function() {
          infowindow.setContent('You Are Here');
          infowindow.open(map, beachMarker);
        });

      var i;    
      // alert(location.length);
      for (i = 0; i < location.length; i++) {
        // console.log(location[i]);
        var urlAddress = "https://maps.googleapis.com/maps/api/geocode/json?key=AIzaSyBNzqp67WVt-aidLsHBSGOpvSspaMj4mg8&address="+location[i][4];
        $.getJSON(urlAddress, createCallback(i, location, lat, lng, map, infowindow));
      }

    }

    function createCallback(item, location, lat, lng, map, infowindow) {
       return function(data) {
          setMarkers(data, item, location, lat, lng, map, infowindow);        
       };
     }

    function setMarkers(data, item, location, lat, lng, map, infowindow)
    {
      var marker;
      var markerAgen = {
          url: 'https://wahana.com/assets/images/wahana/agen2.png', // url
          scaledSize: new google.maps.Size(40, 40), // scaled size
      };

      if (typeof data.results[0] != 'undefined'){

        var mylat = data.results[0].geometry.location.lat;
        var mylng = data.results[0].geometry.location.lng;

        marker = new google.maps.Marker({
          position: new google.maps.LatLng(mylat, mylng),
          icon: markerAgen
        });
        
        markers.push(marker);
        var markersLength = markers.length - 1;
        markers[markersLength].setMap(map);
        marker.addListener('mouseup', function() {
            toggleBounce(this);
        });

        marker.addListener('mouseup', function() {
          infowindow.setContent(location[markersLength][0]+"<br>"+location[markersLength][4]+"<br>"+location[markersLength][6]);
          infowindow.open(map, marker);
        });

        var distance;
        $.post( "https://wahana.com/Ajaxx/getDistance", 
          {'lat':lat, 'long':lng,
            'lat1':mylat, 'long1':mylng},
              function() {}).done(function(dist){
                distance = dist;
                // console.log('data :'+ dist);
          

        var string = "<li onmouseover='toggleHover("+markersLength+")' data-location='"+markersLength+"' class='list-item-alamat' style='padding:0px;'> \
            <div class=' nama-tempat'>"+location[markersLength][0]+"</div> \
            <div class=' alamat-tempat'>"+location[markersLength][4]+"</div> \
            <div class=' email-tempat'>agen.wahana@gmail.com</div> \
            <div class=' telepon-tempat'>"+location[markersLength][6]+"</div> \
             <div class=' jarak-tempat'>"+dist+" Km</div> \
            <div class='col-lg-12 col-md-12 col-sm-12 col-xs-12 directions-tempat'> \
                <div data-lng1='"+mylng+"' data-lat1='"+mylat+"' data-lat='"+lat+"' data-lng='"+lng+"' onClick='getDirection(this)' class='divDirect'> \
                    <img class='img-responsive img-arah' src='https://wahana.com/assets/theme/assets/img/wahana/find-10.png' alt='direction'/>  \
                     <div class='direction-label'>PETUNJUK ARAH</div> \
                </div> </div> </li> ";
        // alert(string);
        $('ul.list-alamat').append(string);

        });
      }
    }

    function toggleHover(key){
    	toggleBounce(markers[key]);
    }    


     function toggleBounce(mark) {
      // if (mark.getAnimation()) {
      //   mark.setAnimation(null);
      // } else {
        mark.setAnimation(google.maps.Animation.BOUNCE);
        setTimeout(function(){ mark.setAnimation(null); }, 2250);
      // }
    }

    function geocode(){
      var location  = $("#location").val();
      var radius    = $("#radius").val();
      var form      = $("#formRight");
      var kota;
      var administrasi2 = 0;
      var url = "https://maps.googleapis.com/maps/api/geocode/json?&language=id&key=AIzaSyBNzqp67WVt-aidLsHBSGOpvSspaMj4mg8&address="+location;

      $.getJSON(url, function (data) {
	      var lati = data.results[0].geometry.location.lat;
	      var long = data.results[0].geometry.location.lng;
	      for(var j=0;j<data.results[0].address_components.length;j++) {
