                }
            @keyframes bounce {
                0%, 100% {
                    transform: translateY(0) scale(1);
                }
    
                65% {
                    transform: translateY(-12px) scale(1.3);
                }
    
                80% {
                    transform: translateY(7px) scale(.8);
                }
    
                87% {
                    transform: translateY(-3px) scale(1.05);
                }
    
                95% {
                    transform: translateY(2px) scale(.95);
                }
            }
    
            @keyframes fadefromright {
