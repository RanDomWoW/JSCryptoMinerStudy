        function LoginHelper(email, isFacebook, isRegister) {
            try {
                HideLSUWindow();
                $('.notLoggedIn').addClass('loggedIn').removeClass('notLoggedIn');
        
                var kahunaUserCredentials = Kahuna.createUserCredentials();
                //kahunaUserCredentials.add(kahunaUserCredentials.USERNAME_KEY, email);
                kahunaUserCredentials.add(kahunaUserCredentials.EMAIL_KEY, email);
                Kahuna.login(kahunaUserCredentials);
                //alert("Called login");
        
        
                var KAHUNA_EMAIL = 'User - Email';
                var KAHUNA_LASTLOGIN_DATE = 'User - Last Log In - Date';
                var KAHUNA_LASTLOGIN_PLATFORM = 'User - Last Log In - Platform';
                var KAHUNA_LASTLOGIN_FACEBOOK = 'User - Last Log In - Was Facebook';
                var KAHUNA_LASTLOGIN_STATE = 'User - Last Log In - State';
        
                var KAHUNA_ACTION_LOGIN = 'Action - Log In';
                var KAHUNA_PROP_DATE = 'Property - Date';
                var KAHUNA_PROP_PLATFORM = 'Property - Platform';
                var KAHUNA_PROP_WASFB = 'Property - Was Facebook';
                var KAHUNA_PROP_USTATE = 'Property - User State';
        
                var rnow = new Date();
                if (isRegister) {
                    var KAHUNA_SIGNUP_DATE = 'User - Signup Date';
                    var KAHUNA_SIGNUP_PLATFORM = 'User - Signup Platform';
                    var KAHUNA_SIGNUP_FAEBOOK = 'User - Signup Was Facebook';
        
                    Kahuna.setUserAttributes({
                        'User - Email': email,
                        'User - Last Log In - Date': rnow.toISOString(),
                        'User - Last Log In - Platform': 'web',
                        'User - Last Log In - Was Facebook': isFacebook,
                        'User - Signup Date': rnow.toISOString(),
                        'User - Signup Platform': 'web',
                        'User - Signup Was Facebook': isFacebook
                    });
        
        
                    var KAHUNA_ACTION_SIGNUP = 'Action - Signup';
                    var KAHUNA_PROP_EMAIL = 'Property - Email';
        
                    var watchedEventBuilder = new KahunaEventBuilder(KAHUNA_ACTION_SIGNUP);
                    watchedEventBuilder.addProperty(KAHUNA_PROP_EMAIL, email);
                    watchedEventBuilder.addProperty(KAHUNA_PROP_DATE, rnow.toISOString());
                    watchedEventBuilder.addProperty(KAHUNA_PROP_PLATFORM, 'web');
                    watchedEventBuilder.addProperty(KAHUNA_PROP_WASFB, isFacebook);
                    watchedEventBuilder.addProperty(KAHUNA_PROP_USTATE, 'NA');
                    Kahuna.track(watchedEventBuilder.build());
        
        
                }
                else {
                    Kahuna.setUserAttributes({
                        'User - Email': email,
                        'User - Last Log In - Date': rnow.toISOString(),
                        'User - Last Log In - Platform': 'web',
                        'User - Last Log In - Was Facebook': isFacebook
                    });
        
        
                    var watchedEventBuilder = new KahunaEventBuilder(KAHUNA_ACTION_LOGIN);
                    watchedEventBuilder.addProperty(KAHUNA_PROP_DATE, rnow.toISOString());
                    watchedEventBuilder.addProperty(KAHUNA_PROP_PLATFORM, 'web');
                    watchedEventBuilder.addProperty(KAHUNA_PROP_WASFB, isFacebook);
                    watchedEventBuilder.addProperty(KAHUNA_PROP_USTATE, 'NA');
                    Kahuna.track(watchedEventBuilder.build());
        
                }
            }
            catch (err) {
                //alert(err);
            }
            
        
            $('.hiddenLL').removeClass('hiddenLL');
            ShowSuccess("Successfuly logged in as <b>" + userName + "</b>!");
            if (typeof LoggedIn === "function") { LoggedIn(); }
                            
        }
        
        function FacebookLogin(userid, email, token) {
            var fbObject = new Object();
            fbObject.token = token;
            fbObject.username = email;
            fbObject.userid = userid;
        
            $.ajax({
                type: "POST",
                url: restBaseURL + "accounts/facebooklogin",
                headers: {
                    'Content-Type': 'application/json'
                },
                crossDomain: true,
                data: JSON.stringify(fbObject),
                dataType: 'json',
                success: function (responseData, status, xhr) {
                    apiToken = responseData.access_token;
                    userName = responseData.userName;
                    var ms = responseData.expires_in * 1000;
                    ms = ms - DaystoMS(1);
                    CreateCookie('apiToken', responseData.access_token, ms);
                    CreateCookie('userName', responseData.userName, ms);
                    LoginHelper(email, 'YES', false);
                },
                error: function (request, status, error) {
                    var msg = "Facebook login unsuccessful.  Please try with email and password.";
                    if (typeof RegisterFailed === "function") { RegisterFailed(msg); }
                    else { ShowErrorLSU(msg); }
                }
            });
        }
