                success: function (data, status, jqXHR) {
                    if (data != null) {
                        var promotions = JSON.parse(data.d);

                        if (promotions != null && promotions.IsSuccess) {
                            // $("#latest-promotion-list").html(promotions.Data);
                            if (promotions.DiscountList != null) {
                                var content = "";
                                promotions.DiscountList.forEach(function (promotion) {
                                    content += "<div class=\"latestPromo-miniContainer\">";
                                    content += "<div class=\"categoryType-imageHolder\"><a href=\"" + promotion.Url + "\"><img alt=\"" + promotion.Title + "\" src=\"" + promotion.ImageUrl + "\" /></a></div>";
                                    content += "<div class=\"categoryType-text\"><p><a href=\"" + promotion.Url + "\">" + promotion.OverviewDescription + "</a></p></div>";
                                    content += "</div>";
                                });
                                $("#latest-promotion-list").html(content);
                            }

                            $("#lblErrorMsg").hide();
                            $("#btnRefresh").hide();
                        } else {
                            $("#lblErrorMsg").show();
                            $("#btnRefresh").show();
                        }
                    } else {
                        $("#lblErrorMsg").show();
                        $("#btnRefresh").show();
                    }
                    $("#btnRefresh").attr("disabled", false);
                },
                error: function (jqXHR, status, error) {
                    $("#lblErrorMsg").show();
                    $("#btnRefresh").show();
                    $("#btnRefresh").attr("disabled", false);
                }
            });
        }

        $("#btnRefresh").on("click", function (evt) {
            evt.preventDefault();
            $("#btnRefresh").attr("disabled", true);
            GetLastPromotion();

        });
    });
</script>
        

