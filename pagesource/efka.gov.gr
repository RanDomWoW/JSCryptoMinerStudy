		}

		function memossrc_reset(){
			document.form2.year_src.selectedIndex=0;
			document.form2.group_src.selectedIndex=0;
			document.form2.num_src.value='';
			document.form2.keyword_src.value='';
			
			return false;
		}

		function faqsrc_reset(){
			document.form2.keyword_src.value='';
			
			return false;
		}
		function stats_submit(){
			if (document.form2.st_categ.value == 1) {
				document.form2.action = 'stat_report_step2.cfm';
				document.form2.submit();
			}else{
				document.form2.action = 'stat_report_results2.cfm';
				document.form2.submit();
			}
		}
		function stats_reset(mystep){
			if(mystep=='1'){
				document.form2.st_categ.selectedIndex=0;
			}
			if(mystep=='2'){
				document.form2.st_year.selectedIndex=0;
			}
			return false;
		}

		function diak_reset(){
			document.form2.dtmhma.selectedIndex=0;
			return false;
		}
		function goto_steg(mysteg_cat_id){
			document.form2.steg_cat_id.value=mysteg_cat_id;
			document.form2.submit();
		}
