        element.last().find(".caption_inner").css("border-color", "#fff");

        $('.editors_choice .element').hover(
            function () {
                $('.editors_choice .element_hover').removeClass('element_hover');
                $(this).addClass('element_hover');

                element_hover = $('.element_hover');
                element = $('.element');

                element.find(".caption_inner").css("border-bottom", "1.5px solid #8B8B9C");
                element_hover.prev("").find(".caption_inner").css("border-color", "#fff");
                element_hover.find(".caption_inner").css("border", "none");

                element.find(".caption_inner").css("font-weight", "normal");
                element_hover.find(".caption_inner").css("font-weight", "bold");
                element_hover.prev("").find(".caption_inner").css("font-weight", "normal");
                element_hover.next("").find(".caption_inner").css("font-weight", "normal");
                element.last().find(".caption_inner").css("border-color", "#fff");
            },
            function () {
                $(this).removeClass('.element_hover');
            }
        );
    });
