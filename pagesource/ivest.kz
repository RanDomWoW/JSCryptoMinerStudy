	<script type="text/javascript">
		$(document).ready(function () {

			if ($("#LoginForm_username").val() != '') /* Проверка заполненности полей на случай если запоминалка паролей вставила уже значения. Если значения вставлены, удаляем текст у label для логина и пароля. */
			{
				$("#LoginForm_username").val('');
				$("#LoginForm_password").val('');
			}

			$(".last.auth").on('change', "span > input, .left_col > input", function (e) {
				labelForm();
			});

			$(".last.auth").on('click', '.left_col>label', function (e) {
				$(this).css('display', 'none').next('input').trigger('focus');
				return false;
			});

			$(".span-5").on('focus', "span > input, .left_col > input", /* Получение фокуса полю в форме. */
				function (e) {
					var clicked = $(e.target);
					clickedId = clicked.attr("id");

					if (clickedId == "LoginForm_username") /* Если поле логин получило фокус, удаляем текст в label для логин. */
					{
						//clicked.prev().text('');
						clicked.prev().css('display', 'none');
					}

					else if (clickedId == "LoginForm_password") /* Если поле пароль получило фокус, удаляем текст в label для пароль. */
					{
						//clicked.prev().text('');
						clicked.prev().css('display', 'none');
					}
				});

			$(".span-5").on('blur', "span > input, .left_col > input", /* Потеря фокуса полем в форме. */
				function (e) {
					var clicked = $(e.target),
						clickedId = clicked.attr("id");

					if (clickedId == "LoginForm_username") /* Если ушли из поля логин и его значение пусто, добавляем текст в label для логин. */
					{
						if (clicked.val() == '') clicked.prev().css('display', 'block');//.text('e-mail');
					}

					else if (clickedId == "LoginForm_password") /* Если ушли из поля пароль и его значение пусто, добавляем текст в label для пароль. */
					{
						if (clicked.val() == '') clicked.prev().css('display', 'block');//.text('пароль');
					}
				});

			function labelForm() {
				if ($("#LoginForm_username").val() != '' && $("#LoginForm_password").val() != '') {
					$(".last.auth label").css('display', 'none');
				}
				if ($(".left_col #LoginForm_username").val() != '' && $(".left_col #LoginForm_username").val() != '') {
					$(".last.auth #login_pop label").css('display', 'none');
				}
			}

		});
	</script>
