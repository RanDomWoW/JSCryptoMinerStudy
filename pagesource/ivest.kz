<script type="text/javascript">
	var rs = GetRiseSet(, );
	/*
	 * Use as values:
	 * rs.sun.rise.hours, rs.sun.rise.minutes
	 * rs.sun.set.hours, rs.sun.set.minutes
	 * rs.moon.rise.hours, rs.moon.rise.minutes
	 * rs.moon.set.hours, rs.moon.set.minutes
	 */
	var dayows = ['Воскресенье', 'Понедельник', 'Вторник', 'Среда', 'Четверг', 'Пятница', 'Суббота'];
	var months = ['января', 'февраля', 'марта', 'апреля', 'мая', 'июня', 'июля', 'августа', 'сентября', 'октября', 'ноября', 'декабря'];
	var zeros = ['00', '0', ''];
	var d = new Date();
	var day = d.getDate();
	var dayow = dayows[d.getDay()];
	var month = months[d.getMonth()];
	$('#cal_date_day').html(dayows[d.getDay()]);
	$('#cal_date').html(String(day) + ' ' + month);

	OutputTime();

	$('#cal_sun_rise').html(rs.sun.rise.hours + ':' + rs.sun.rise.minutes);
	$('#cal_sun_set').html(rs.sun.set.hours + ':' + rs.sun.set.minutes);
	$('#cal_moon_rise').html(rs.moon.rise.hours + ':' + rs.moon.rise.minutes);
	$('#cal_moon_set').html(rs.moon.set.hours + ':' + rs.moon.set.minutes);
	day = new String(d.getDate());
	var mon = new String(d.getMonth() + 1);
	//$('#money_date').html(zeros[day.length]+day+'.'+zeros[mon.length]+mon);

	setInterval(function () {
		OutputTime();
	}, 500);

	function OutputTime() {
		var hs = new String((new Date()).getHours());
		var mn = new String((new Date()).getMinutes());
		var sec = new String((new Date()).getSeconds());

		$('#cal_date_time').html(zeros[hs.length] + hs + ':' + zeros[mn.length] + mn + ':' + zeros[sec.length] + sec);
	}
</script>

