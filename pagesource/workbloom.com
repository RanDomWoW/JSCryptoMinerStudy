<script type="text/javascript">
    jQuery(document).ready(function () {
        getLocation();
        $(".btn-find-job").click(function (event) {
            window.location.href = 'https://jobs.workbloom.com/search-results' + "?search=" + $("#lookingfor").val() + "&location=" + $("#lookingwhere").val();
        });
        $('#lookingfor').keypress(function (e) {
            var key = e.which;
            if (key == 13)  // the enter key code
            {
                $('.btn-find-job').click();
                return false;
            }
        });
        $('#lookingwhere').keypress(function (e) {
            var key = e.which;
            if (key == 13)  // the enter key code
            {
                $('.btn-find-job').click();
                return false;
            }
        });
    });

    function getLocation() {
        $.ajax({
            type: 'GET',
            url: "/umbraco/surface/Findjob/GetLocation",
            data:
            {
            },
            success: function (data) {
                var res = JSON.parse(data.result);
                if (res.status == "success") {
                    var loc = res.city + ", " + res.regionName + ", " + res.country;
                    $("#lookingwhere").val(loc);
                }
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
            }
        });
    }
</script>
