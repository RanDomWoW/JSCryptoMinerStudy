<script type="text/javascript">
(function($){
 
    $(document).ready(function(){floatingAd();});
 
    function floatingAd(){
        var a,ah,b,bh,c,cb,ch,l,p,t,w,wt,wl,wh,pa,pt,pl,nf,s,r,gc,aw,bm,cp;
 
        //edit start
        a=$('#gad-fixed-l');
        b=$('#utilities');
        c=$('#main');
        gc=0;
        bm=0;
        //edit end
 
        aw = a.closest('.plugin-memo');
        aw.css({'overflow':'hidden'});
        cp = $('#container').css('position');
        p='static';
 
        w=$(window);
        l=b.offset().left;
        t=aw.offset().top;
 
        s=function(){
            bh=b.outerHeight();
            ch=c.outerHeight();
            ah=aw.outerHeight();
            cb=c.offset().top+ch;
 
            wt=w.scrollTop();
            wl=w.scrollLeft();
            wh=w.height();
            nf=(wt>t)&&(ch>bh)?true:false;
            p=!nf?'static':(wt+ah)>cb?'absolute':'fixed';
 
            if(p==='fixed'){
                b.css({
                    position:p,
                    top:'',
                    bottom:wh-ah-bm,
                    left:l-wl,
                    'margin-left':(gc===1)?0:''
                });
            }
            else if(p==='absolute'){
                b.css({
                    position:p,
                    top:cb-bh,
                    bottom:'',
                    left:(cp=='relative')?0:l
                });
            }
            else{
                b.css({
                    position:p,
                    'margin-left':(gc===1)?'-100%':''
                });
            }
        };
 
        r=function(){
            b.css({
                position:'static'
            });
            wl=w.scrollLeft();
            l=b.offset().left+0;
            if(p==='fixed'){
                b.css({
                    position:p,
                    left:l-wl
                });
            }
            else if(p==='absolute'){
                b.css({
                    position:p,
                    left:(cp=='relative')?0:l
                });
            }
        };
 
        w.on('load',s);
        w.on('scroll',s);
        w.on('resize',r);
    }
 
})(jQuery);
