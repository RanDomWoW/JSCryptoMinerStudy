<script>
    $("#inputs-toggled-row").toggle();
    function InitLocalStorageLists()
    {
        $(".filter-list li ul").toggle();
        ClearForm('home-search-form');
        $(".filter-list li").click(function(evt){
            var $this = $(this);
            if($this.parent().parent().hasClass("greece-flist")) {
                $this.css("max-height", 250);
                $(".cyprus-flist").hide();
                $this.parent().parent().css("max-height", 245);
            }
        });
        $(".filter-list li").click(function(evt){
            evt.stopPropagation();
            evt.preventDefault();
            $this = $(this);
            SetInputToForm($this);
            $this.find("ul:first").toggle(500);
            $this.toggleClass("selected");
        });
        $('form.smart-form').each(function(){
            var form = $(this);
            form.find('button[type="submit"]').click(function(){
                $(this).prop('disabled', true);
            });
        });
    }

    var localHomeStorageTimeout = 3600000;
    var categoryList, clientCategoryList, areaListGreece, cyprusList;

    if($.jStorage.storageAvailable()) {
        categoryList = $.jStorage.get('home-categoryList');
        clientCategoryList = $.jStorage.get('home-clientCategoryList');
        areaListGreece = $.jStorage.get('home-areaListGreece');
        cyprusList = $.jStorage.get('home-cyprusList');
    }
    if(categoryList == null || clientCategoryList == null || areaListGreece == null || cyprusList == null)
    {
        $("#CategoryList").html('<img src="/assets/images/loader.gif" />');
        $("#ClientCategories").html('<img src="/assets/images/loader.gif" />');
        $("#AreaListerGreece").html('<img src="/assets/images/loader.gif" />');
        $("#AreaListerCyprus").html('<img src="/assets/images/loader.gif" />');
        $.ajax({
            method: "POST",
            url: "/elGR/frontend-ajax/get-home-search-filters",
            cache: true,
            //data: { klados: $("#klados").val() },
            success: function(resp){
                resp = JSON.parse(resp);
                //console.debug(resp);
                $.jStorage.flush();
                categoryList = resp.CategoryList;
                if($.jStorage.storageAvailable()) {
                    $.jStorage.set('home-categoryList', categoryList);
                    $.jStorage.setTTL('home-categoryList', localHomeStorageTimeout);
                }

                clientCategoryList = resp.ClientCategoryList;
                if($.jStorage.storageAvailable()) {
                    $.jStorage.set('home-clientCategoryList', clientCategoryList);
                    $.jStorage.setTTL('home-clientCategoryList', localHomeStorageTimeout);
                }

                areaListGreece = resp.AreaListerGreece;
                if($.jStorage.storageAvailable()) {
                    $.jStorage.set('home-areaListGreece', areaListGreece);
                    $.jStorage.setTTL('home-areaListGreece', localHomeStorageTimeout);
                }

                cyprusList = resp.AreaListerCyprus;
                if($.jStorage.storageAvailable()) {
                    $.jStorage.set('home-cyprusList', cyprusList);
                    $.jStorage.setTTL('home-cyprusList', localHomeStorageTimeout);
                }
                $("#CategoryList").html(categoryList);
                $("#ClientCategories").html(clientCategoryList);
