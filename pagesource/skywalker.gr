(function() {
var v = document.getElementsByClassName("youtube-player");
for (var n = 0; n < v.length; n++) {
var p = document.createElement("div");
p.innerHTML = labnolThumb(v[n].dataset.id);
p.onclick = labnolIframe;
v[n].appendChild(p);
}
})();

function labnolThumb(id) {
return '<img class="youtube-thumb" src="https://i.ytimg.com/vi/' + id + '/hqdefault.jpg"><div class="play-button"></div>';
}

function labnolIframe() {
var iframe = document.createElement("iframe");
iframe.setAttribute("src", "//www.youtube.com/embed/" + this.parentNode.dataset.id + "?autoplay=1&autohide=2&border=0&wmode=opaque&enablejsapi=1&controls=1&showinfo=0&fs=1&rel=0");
iframe.setAttribute("frameborder", "0");
iframe.setAttribute("id", "youtube-iframe");
iframe.setAttribute("allowfullscreen", "");
this.parentNode.replaceChild(iframe, this);
}

    function ClearForm(id)
    {
        var inputs = $('#'+id).find("input");
        $.each(inputs, function(i,item){
            $(item).val('');
        });
    }

    function DeselectJS(type)
    {
        switch(type)
        {
            case "Countrysky":
                $('#home-search-form').find('input[name="'+"Countrysky"+'"]').val("");
            case "GeoDepartment":
                $('#home-search-form').find('input[name="'+"GeoDepartment"+'"]').val("");
            case "Statesky":
                $('#home-search-form').find('input[name="'+"Statesky"+'"]').val("");
            case "Citysky":
                $('#home-search-form').find('input[name="'+"Citysky"+'"]').val("");
            case "Municipalitysky":
                $('#home-search-form').find('input[name="'+"Municipalitysky"+'"]').val("");
        }
    }

    function HideSameLevel(li)
    {
        var lis = li.parent().children('li');
        var id = li.find('a:first').attr('data-id');
        $.each(lis, function(i, value){

            if(id != $(value).find('a:first').attr('data-id')) {
                //alert(id+" "+$(value).find('a:first').attr('data-id'));
                $(value).hide();
            }
        });
    }

    function ShowSameLevel(li)
    {
        var lis = li.parent().children('li');
        var id = li.find('a:first').attr('data-id');
        $.each(lis, function(i, value){

            if(id != $(value).find('a:first').attr('data-id')) {
                //alert(id+" "+$(value).find('a:first').attr('data-id'));
                $(value).show();
            }
        });
    }



    function SetInputToForm(elem)
    {
        $a = elem.find('a:first');
        var type = $a.attr('data-type');
        var id=$a.attr('data-id');
        DeselectJS(type);
        var input = $('#home-search-form').find('input[name="'+type+'"]');
        var curValue = input.val();

        if($this.hasClass('selected')) {
            input.val("");
            ShowSameLevel(elem)
            if($this.parent().parent().hasClass("greece-flist")) {
                $this.parent().parent().removeAttr("style");
                $(".cyprus-flist").show();
            }
            else if($this.parent().parent().hasClass("cyprus-flist")) {
                $this.parent().parent().removeAttr("style");
                $(".greece-flist").show();
                $("#greece-flist-header").show();
            }
        }
        else {
            HideSameLevel(elem);
            input.val(id);
            if($this.parent().parent().hasClass("cyprus-flist")) {
                //$this.css("max-height", 125);
                $(".greece-flist").hide();
                $("#greece-flist-header").hide();
                $this.parent().parent().css("max-height", 245);
            }
            else if($this.parent().parent().hasClass("greece-flist")) {
                //$this.css("max-height", 125);
                $(".cyprus-flist").hide();
                $this.parent().parent().css("max-height", 215);
            }
        }
    }
</script>
