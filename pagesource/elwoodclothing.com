              rjQuery.ajax({
                  type: 'POST',
                  data: data,
                  url: '/cart/update.js',
                  dataType: 'json',
                  success: function() { 
                    window.location.href='/cart'
                  }
              }); 
          } else {
               window.location.href='/cart'
          }
      } catch (e) {
