<script>
  
  (function(){
	'use strict';

	//Caching these for easier access
	var orig = null;
	var clone = null;

	//Fuck this 5ever
	function generateRoushVehicles(){
		//Clear the models
		clone.querySelector("#Model").innerHTML = '<option>Model</option><option value="F-150">F-150</option><option value="Mustang">Mustang</option>';
	}

	function correctSelect(el){
		var arr = el.getElementsByTagName("option");
		for(var i = 0; i < arr.length; i++){
			var sel = arr[i];
			//console.log(sel);
			if(sel.getAttribute("value")){
				sel.setAttribute("value", sel.innerText);
			}
		}
	}

	function touch(upto){
		if(upto >= 4)
			orig.querySelector("#Category").dispatchEvent(new Event("change", {'bubbles': true}));
		if(upto >= 3)
			orig.querySelector("#Make").dispatchEvent(new Event("change", {'bubbles': true}));
		if(upto >= 2)
			orig.querySelector("#Model").dispatchEvent(new Event("change", {'bubbles': true}));
		if(upto >= 1)
			orig.querySelector("#Year").dispatchEvent(new Event("change", {'bubbles': true}));

		for(var i = 0; i < 10; i++){
			hideNotNissan();
		}
	}

	function hideNotNissan(){
		if(clone.querySelector("#Category").value == "New" || clone.querySelector("#Category").value == "new"){
			var children = clone.querySelector("#Make").children;
			var parent = clone.querySelector("#Make");
			var len  = children.length;
			var queue = [];
			for(var i = 0; i < len; i++){
				var child = children[i];
				console.log("Iterating over child ", child);
				if(child.getAttribute("value") != "Kia" && child.getAttribute("value") != "kia"){
					console.log("Pushing ", child);
					queue.push(child);
				}
			}
			for(var i = 0; i < queue.length; i++){
				queue[i].setAttribute("style", "display: none;");
			}

		}
	}

	//Check the category... more Pass-through
	function cloneCategoryListen(){
		console.log("Listening...");
		orig.querySelector("#Category").value = this.value;
		touch(4);
		window.setTimeout(function(){
			console.log(orig.querySelector("#Make"));
			clone.querySelector("#Make").innerHTML = orig.querySelector("#Make").innerHTML;
			hideNotNissan();

			window.setTimeout(function(){
				clone.querySelector("#Year").innerHTML = orig.querySelector("#Year").innerHTML;
				//touch(3);
			}, 300);
		}, 800);
	}

	//Listen to the make of the search so we know to send roush to the original
	function cloneMakeListen(){
		console.log("Clone selected: " + this.value);
		//touch(3);
		correctSelect(orig.querySelector("#Make"));
		correctSelect(orig.querySelector("#Model"));
		correctSelect(clone.querySelector("#Model"));
		correctSelect(clone.querySelector("#Make"));
		var make = this.value;
		if(make === "Roush" || make === "roush"){
			orig.querySelector("#Category").value = "dealercustomized";
			correctSelect(orig.querySelector("#Make"));
			correctSelect(clone.querySelector("#Make"));
			orig.querySelector("#Make").value = "Ford";
			touch(3);
			window.setTimeout(function(){
				//clone.querySelector("#Model").innerHTML = orig.querySelector("#Model").innerHTML;
				generateRoushVehicles();
				correctSelect(clone.querySelector("#Model"));
				touch(3);
			}, 600);
		}
		else {
			orig.querySelector("#Make").value = this.value;
			correctSelect(clone.querySelector("#Make"));
			touch(3);
			window.setTimeout(function() {
				clone.querySelector("#Model").innerHTML = orig.querySelector("#Model").innerHTML;
				correctSelect(clone.querySelector("#Model"));
			}, 600);
		}
	}

	//Pass-through for model and year
	function cloneModelListen(){
		orig.querySelector("#Model").value = this.value;
		touch(2);
		window.setTimeout(function(){
			clone.querySelector("#Year").innerHTML = orig.querySelector("#Year").innerHTML;
		}, 300);
	}
	function cloneYearListen(){
		orig.querySelector("#Year").value = this.value;
	}

	//Send the click through the clone to the original
	function cloneSubmitClick(){
		orig.querySelector("button.btn-primary").click();
	}

	function fixSearch(){
		orig = document.querySelector("div.global-search.horizontal");
		orig.id = "originalSearch";
		clone = orig.cloneNode(true);
		clone.id = "cloneSearch";
		orig.parentNode.insertBefore(clone, orig);
		orig.className = "fix-roush";

		/*var roushOp = document.createElement("option");
		roushOp.innerText = "Roush";
		roushOp.setAttribute("value", "dealercustomized");
		orig.querySelector("#Category").appendChild(roushOp);*/

		//Add event listeners fake the search box
		clone.querySelector("#Category").addEventListener("change", cloneCategoryListen);
		clone.querySelector("#Make").addEventListener("change", cloneMakeListen);
		clone.querySelector("#Model").addEventListener("change", cloneModelListen);
		clone.querySelector("#Year").addEventListener("change", cloneYearListen);
		clone.querySelector("button.btn-primary").addEventListener("click", cloneSubmitClick);
	}

	window.setTimeout(fixSearch, 100);
})();

</script>

