        <script>
            $(function () {
                // DOM Handles

                var _dom = function () {
                    var widget = $(".global-search");
                    var selDlr = widget.find("#DealerSelectedId");
                    var selCat = widget.find("#Category");
                    var selMake = widget.find("#Make");
                    var selModel = widget.find("#Model");
                    var selYear = widget.find("#Year");

                    var obj = {
                        widget: widget,
                        formText: widget.find("#global-search-text"),
                        formSels: widget.find("#global-search-sels"),
                        q: widget.find("#q"),
                        selDlr: selDlr,
                        selCat: selCat,
                        selMake: selMake,
                        selModel: selModel,
                        selYear: selYear,
                        sels: [selCat, selMake, selModel, selYear],
                        btn: widget.find("button"),
                    };

                    if (selDlr.length !== 0) {
                        obj.sels.unshift(selDlr);
                    }

                    return obj;
                }();

                // Clear Form

                refactor.form.resetForm(_dom.formText);

                // Helpers

                function buildSelect(select, label, list, selectedItem) {
                    select.html("<option value=''>" + label + "</option>");

                    for (var i in list) {
                        var opt = $("<option />", { value: list[i], text: list[i] });

                        if (selectedItem && list[i].toString().toLowerCase() === selectedItem.toString().toLowerCase()) {
                            opt.attr("selected", "selected");
                        }

                        select.append(opt);
                    }
                }

                // Event Handlers

                var selDirty = false;

                function qChanged() {
                    if (selDirty) {
                        _dom.sels.forEach(function (obj) {
                            obj.val("");
                        });
                        selChanged(_dom.sels[0], false);
                    }
                }

                function selChanged(sel, clearQ) {
                    if (clearQ) {
                        _dom.q.val("");
                    }

                    selDirty = sel !== _dom.sels[0]

                    if (sel === _dom.sels[_dom.sels.length - 1]) return;

                    var found = false;
                    for (var i = 0; i < _dom.sels.length; i++) {
                        var _sel = _dom.sels[i];
                        if (_sel === sel) {
                            found = true;
                        } else if (found) {
                            _sel.prop("disabled", true);
                            _sel.val("");
                        }
                    }

                    var request = {
                        category: refactor.form.getValue(_dom.selCat),
                        make: refactor.form.getValue(_dom.selMake),
                        model: refactor.form.getValue(_dom.selModel),
                        year: refactor.form.getValue(_dom.selYear),
                    };

                    if (_dom.selDlr.length !== 0) {
                        request.dealerId = _dom.selDlr.val();
                    }

                    refactor.ajax("/inventory/searchservice", "POST", request, {
                        success: function (resp) {
                            if (resp.Success) {
                                var r = resp.Obj;
                                if (r.Category) {
                                    _dom.selCat.val(r.Category);
                                }
                                buildSelect(_dom.selMake, "Make", r.Makes, r.Make);
                                buildSelect(_dom.selModel, "Model", r.Models, r.Model);
                                buildSelect(_dom.selYear, "Year", r.Years, r.Year);

                                var found = false;
                                for (var i = 0; i < _dom.sels.length; i++) {
                                    var _sel = _dom.sels[i];
                                    if (_sel === sel) {
                                        found = true;
                                    } else if (found) {
                                        _sel.prop("disabled", false);
                                    }
                                }
                            }
                        }
                    });
                }

                function btnClicked() {
                    if (refactor.form.getValue(_dom.q) != "") {
                        _dom.formText.trigger("submit");
                    }
                    else if ((_dom.selDlr.length !== 0 && _dom.selDlr.val() != "") || _dom.selCat.val() != "" || _dom.selMake.val() != "" || _dom.selModel.val() != "" || _dom.selYear.val() != "") {
                        _dom.formSels.trigger("submit");
                    }
                    else {
                        _dom.q.focus();
                    }
                }

                // Wire Up Event Handlers

                _dom.q.on("keyup", qChanged);
                _dom.sels.forEach(function (obj) { obj.on("change", function () { selChanged(obj, true); }); });
                _dom.btn.on("click", btnClicked);
            });
        </script>
