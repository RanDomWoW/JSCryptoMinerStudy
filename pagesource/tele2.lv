            <p>Tu jau esi Tele2 klients. Iepazīsties ar <a href="/ipasi-piedavajumi/">īpašiem piedāvājumiem esošiem klientiem</a>!</p>
        </div>
    </div>
</div>

<script type="text/javascript">
    function FixSidePopPos() {
        var cp=$('#sidecontpop');
        var n=75;
        var p='fixed';

        var b= $("body").height();
        var t=$("#page").height();
        var f=$(".footer_push").height();
        var e = Math.max($("html").scrollTop(), $("body").scrollTop());
        var i = t-e-b-f;
        if (i < 0 || $("body").width() < 768) {
            n=25;
            p='relative';
        }
        if ($("body").width() < 768) {
            n = 0;
        }
        cp.css("bottom", n + "px"), cp.css("position", p);
    }
    $(window).resize(function() {
        FixSidePopPos();
    });
    $(window).load(function() {
        FixSidePopPos();
    });
    $(document).on("touchstart touchend touchcancel touchleave touchmove", function() {
        FixSidePopPos();
    });
    $(window).on("scroll", function() {
        FixSidePopPos();
    });
    $(document).ready(function() {
        $('#sidecontpop').appendTo('#content');
        FixSidePopPos();
        $('.close_cpop').click(function () {
            $('#sidecontpop').hide();
            document.cookie = 'sp1=' +1+ ';path=/';
            return false;
        });
        $('#sidecontpop .nac_badge, #nac_done, #nac_done2, #sidecontpop .close').click(function() {
            if($('#sidecontpop').hasClass('fly_hide')) {
                $('#sidecontpop').removeClass('fly_hide');
                $('#nac_vispiet').show();
                $('#nac_done').hide();
            } else {
                $('#sidecontpop').addClass('fly_hide');
            }
            FixSidePopPos();
            if ($(this).attr('id')!='nac_done' && $(this).attr('id')!='nac_done2') {
                return false;
            }
        });
        $('#sidepopform').submit(function(){
            $('#sidepopform .cinput_error_msg').hide();
            $('#sidepopform .error_errt2').hide();
            $('#sidepopform .sidecontcom').show();

            $('#sidepopres2').hide();
            $('#nac_done').hide();
            $('#nac_done2').hide();
            $('#nac_loader').hide();

            var df=$('#sidepopform .cinput.default.invalid');
            if (df.length>0) {
                $(df).parent().find(".error_noinfo").show();
                FixSidePopPos();
                return false;
            }
            var di=$('#sidepopform .cinput.invalid');
            if (di.length>0) {
                $(di).parent().find(".error_errinfo").show();
                FixSidePopPos();
                return false;
            }

            var v=$('#contpop_tel').val();
            v=v.replace(' ','');
            if (v.substring(0,1)=='+') {
                v=v.substring(1);
                $('#contpop_tel').val(v);
            }
            if (v.substring(0,3)=='371') {
                v= v.substring(3);
                $('#contpop_tel').val(v);
            }

            var cok=true;
            if (v.length>8 || v.length<8) {
                cok = false;
            }
            var v2=v.replace(/[^\d]/,'');
            if (v2!=v) {
                cok = false;
            }
            if (!cok) {
                $('#contpop_tel').parent().parent().find(".error_errinfo").show();
                FixSidePopPos();
                return false;
            }

            $('#contpop_send').hide();
            $('#nac_loader').show();
            var ok=true;
            $.ajax({
                dataType: "json",
                url: '?cmd=get&cid=3742&check_number='+$('#contpop_tel').val(),
                beforeSend: function(ob){
                    $('#nac_vispiet').hide();
                    $('#nac_loader').show();
                },
                success: function(data) {
                    $('#nac_loader').hide();
                    $('#nac_vispiet').show();
                    if (data.error) {
                        if (data.final) {
                            $('#nac_vispiet').hide();
                            $('#sidepopres2').show();
                            $('#sidepopres2').html(data.error);
                        } else {
                            $('#sidepopres2').show();
                            $('#contpop_send').show();
                            $('#sidepopres2').html(data.error);
                        }
                        ok = false;
                        FixSidePopPos();
                        return false;
                    }
                    if (data.t2) {
                        //$('#sidecontpop .error_errt2').show();
                        //$('#sidecontpop .sidecontcom').hide();
                        $('#nac_vispiet').hide();
                        $('#nac_done2').show();
                        dataLayer.push({'event': 'error_msg', 'alert_msg':'Portin pop callback: Jau Tele2 klients'});
                        ok = false;
                        FixSidePopPos();
                        return false;
                    }

                    var urlv = 'index.php?cmd=popup&cid=3742';
                    urlv = urlv+'&contpop_send=CH987sax';
                    urlv = urlv+'&tel='+encodeURIComponent($('#contpop_tel').val());

                    $.ajax({url:urlv,dataType:'json',beforeSend: function(ob){
                        $('#nac_vispiet').hide();
                        $('#nac_loader').show();
                    }}).done(function(data) {
                        $('#nac_vispiet').show();
                        $('#nac_loader').hide();
                        if(data.ok==1) {
                            $('#nac_vispiet').hide();
                            $('#nac_done').show();
                        } else {
                            $('#sidepopres2').show();
                            $('#contpop_send').show();
                            $('#sidepopres2').html(data.err);
                        }
                    })
                        .fail(function() {
                            $('#nac_loader').hide();
                            $('#nac_vispiet').show();
                            $('#sidepopres2').show();
                            $('#contpop_send').show();
                            $('#sidepopres2').html('Informāciju nosūtīt neizdevās. Mēģiniet atkārtoti vēlāk');
                        });
                }
            });
            if (!ok) {
                FixSidePopPos();
                return false;
            }

            var newel=document.createElement("input");
            var el=document.getElementById('sidepopform');
            newel.type='hidden';
            newel.name='survch';
            newel.value='CH987s';
            el.appendChild(newel);
            FixSidePopPos();
            return false;
        });
    })
</script>

