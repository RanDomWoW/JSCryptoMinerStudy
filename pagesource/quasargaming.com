<script>
    function flipAnotherDeviceInfo() {
		setTimeout(function() {
			var formObj = $('form#LoginForm');
			$('#loginCreateAccountInfo').show();
			$(formObj).closest('.newmodal_payment_title').show();
			$(formObj).children('*:not(.hidden)').show();
			$(formObj).find('.loginErrorDiv').html('').hide();
			$(formObj).find('#anotherDeviceText').html('');
			$(formObj).find('#anotherDevice').hide();
			$(formObj).css('border-bottom', '1px solid #ddd');
			$('#anotherDeviceInfo').hide();
		}, 1000);
    }
</script>
