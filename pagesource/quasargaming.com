<script type="text/javascript">
    var currentSlide = 1;
    var globalTimer = [];

    function delayShowElement(s, _find, delay) {
        var t = setTimeout(function(){s.find(_find).addClass("show")}, delay);
        globalTimer.push(t);
    }

    function hideElement(s) {
        s.removeClass("show");
        var t1 = setTimeout(function () {s.find(".show").removeClass("show")}, 600);
        var t2 = setTimeout(function () {s.css("visibility", "hidden")}, 1200);
        globalTimer.push(t1);
        globalTimer.push(t2);
    }

    function clearGlobalTimer() {
        for (var i=0;i<globalTimer.length;i++) {
            clearTimeout(globalTimer[i]);
        }
    }

    $(function() {
        var pager = $(".homepage-slider > .fs-pager-wrapper > a");
        var s = $(".homepage-slider > .slide");
        var pauseBtn = $(".clouds-box .pauseplay-btn");
        var slidesCount = s.length;
        var s0 = s.eq(0);
        var clicked = false;
        $(".homepage-slider > .slide:first-child").addClass('show').css('visibility', 'visible');
        delayShowElement(s0, ".slider_title", 600);
        delayShowElement(s0, ".slider_desc", 600);
        delayShowElement(s0, ".slider_btn", 600);
        setInterval(function(){
            if ($(".homepage-slider").hasClass("play-slides")) {
                var s1 = s.eq(currentSlide);
                s1.addClass("show");
                s1.css("visibility", "visible");
                delayShowElement(s1, ".slider_title", 600);
                delayShowElement(s1, ".slider_desc", 600);
                delayShowElement(s1, ".slider_btn", 600);
                pager.removeClass("active");
                pager.eq(currentSlide).addClass("active");
                var prevSlide = (currentSlide > 0) ? (currentSlide - 1) : (slidesCount - 1);
                var s2 = s.eq(prevSlide);
                hideElement(s2);
                currentSlide++;
                if (currentSlide == slidesCount) {
                    currentSlide = 0;
                }
            }
        }, 6000);

        pauseBtn.on("click", function() {
            if ($(this).hasClass("pause")) {
                $(this).addClass("play");
                $(this).removeClass("pause");
                $('.homepage-slider').removeClass('play-slides');
            } else {
                $(this).addClass("pause");
                $(this).removeClass("play");
                $('.homepage-slider').addClass('play-slides');
            }
        });

        pager.on("click", function() {
            if (!$(this).hasClass("active")) {
                clearGlobalTimer();
                // change slide
                var s2 = $(".homepage-slider > .slide.show");
                hideElement(s2);
                currentSlide = $(this).data("slide");
                var s1 = $(".homepage-slider > .slide").eq(currentSlide);
                s1.addClass("show");
                s1.css("visibility", "visible");
                delayShowElement(s1, ".slider_title", 600);
                delayShowElement(s1, ".slider_desc", 600);
                delayShowElement(s1, ".slider_btn", 600);
                pager.removeClass("active");
                pager.eq(currentSlide).addClass("active");
            }
        });
    });
</script>
