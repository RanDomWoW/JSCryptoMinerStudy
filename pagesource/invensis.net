/* Code for missing alt attribute on images */

$(document).ready(function() {
  
  $('img').each(function(){
    var $img = $(this);
    var filename = $img.attr('src').replace(new RegExp("-", "g"), ' ');
    var filename1 = filename.substring(filename.lastIndexOf('/')+1);    
    var attr = $(this).attr('alt');
    var title = $(this).attr('title');
     // Checking alt is missing or not
    if (typeof attr == typeof undefined || attr == false) {
    $img.attr('alt', filename1.substring(0, filename1.indexOf('.')));  
    }
    // Checking title is missing or not
    if (typeof title == typeof undefined || title == false) {
     $img.attr('title', filename1.substring(0, filename1.indexOf('.')));
    }
  });

  /* Removing the underline from anchor which is dies not have any href value */
  $('a').each(function() {   
    if ($("this").attr('href') != '')
     {          
          $("this").css("text-decoration","none");         
     } 
     else {
        $("this").css("text-decoration","underline");
     }

  });
  /* Removing the tile from casestudy showcase */
  $("#casestudy").removeAttr("title");

});

