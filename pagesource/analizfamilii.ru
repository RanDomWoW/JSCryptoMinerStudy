<script type="text/javascript">
	function trim(str) {
	  	return str.replace(/^\s+/g, '').replace(/\s+$/g, '');
	}	
	function validate_cyrilic(value)
	{		
		var reg = /[Ёёа-яА-Я-]+/;
		value = trim(value);
		if (value.match(reg) == value)
			return true;
		else
			return false;
	}    
    function clearIfFilled(i, v, out)
    {
    	if(out)
    	{
    		if(i.value == '')
            {
                i.value = v;
                i.style.color = 'silver';
                i.style.fontStyle = 'italic';
            }
    	}
    	else if(i.value == v)
        {
    		i.value = '';
            i.style.color = 'black';
            i.style.fontStyle = 'normal';
        }
    }	
	function validateFormAnaliz(echo) {
			
		var surname = document.getElementById('surname');
		if((!trim(surname.value)) || (trim(surname.value) == 'Введите фамилию')) {
			if(echo) alert('Укажите фамилию, которую хотите проанализировать');
			return false;
		}
		else if (!validate_cyrilic(trim(surname.value))) {
			if(echo) alert('Фамилия может состоять только из символов русского алфавита и тире(-)');
			return false;
		}		
		
		return true;	
	}
</script>
