<script type="text/javascript">
		
	function validate_mail(value)
	{
		var reg = /[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?\.)+[a-zA-Z0-9](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?/;
		if (value.match(reg))
			return true;
		else
			return false;
	}	
			
	function showBig(name, width, height)
    {
    	var win = window.open ('/images/view.php?img=trees/s'+name+'.jpg', '','width='+width+',height='+height+',scrollbars=0,resizable=0');
    	win.focus();
    } 
	
	function validateFormTree(echo) {
					
				var contact_email = document.getElementById('contact_email');
		var contact_surname = document.getElementById('contact_surname');
		var contact_name = document.getElementById('contact_name');
		
		if(!trim(contact_surname.value)) {
			if(echo) alert('Укажите Вашу фамилию');
			return false;
		}
		else if (!validate_cyrilic(trim(contact_surname.value))) {
			if(echo) alert('Фамилия может состоять только из символов русского алфавита и тире(-)');
			return false;
		}	
		
		if(!trim(contact_name.value)) {
			if(echo) alert('Укажите Ваше имя');
			return false;
		}
		else if (!validate_cyrilic(trim(contact_name.value))) {
			if(echo) alert('Имя может состоять только из символов русского алфавита и тире(-)');
			return false;
		}		
        
        if(!trim(contact_email.value))
		{
			if(echo) alert('Укажите Ваш email');
			return false;
		}		
		else if (!validate_mail(trim(contact_email.value))) {
			if(echo) alert('Неправильный адрес электронной почты');
			return false;
		}		
				
		return true;	
	}
</script>
