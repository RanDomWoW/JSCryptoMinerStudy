$(function(){
	var offset=$('.otlan-nav').offset();
	var hi=offset.top+$('.otlan-nav').height();
	$(document).scroll(function(){
		var top=$(document).scrollTop();
		if(top>hi){
			$('.otlan-nav').removeClass('otlan-icon2').addClass('otlan-icon1');
			$('#otlanNavbar').addClass('navbar-fixed-top');
		}else{
			$('.otlan-nav').removeClass('otlan-icon1').addClass('otlan-icon2');
			$('#otlanNavbar').removeClass('navbar-fixed-top');
		}
	});
});
</script>
