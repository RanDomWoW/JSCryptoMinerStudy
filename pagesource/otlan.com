	$('#otlan-menu').addClass('on');
},function(){
	$('a[data-target="#childMenu3"]').removeClass('active');
	var self=$(this);
	setTimeout(function(){
		if(!$('a[data-target="#childMenu3"]').hasClass('active')){
			self.hide();
		}
		if(!$('.otlan-menu-item').hasClass('active')){
			$('#otlan-menu').removeClass('on');
		}
	},500);
});
