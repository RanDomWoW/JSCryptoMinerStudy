<script type="text/javascript">

            //$("#headerSearchTextBox").val('');

            $(document).ready(function(){
                var controller_url = '/users/usersearchajax';
                var total_pages_url = '/users/gettotaluserscount';
                
                var action_name = "featuredCategories";

                var load_more_track_click = 2;
                $(".persitant-search .global_search").keyup(function(e){
                    $('body').animate({scrollTop:0},'2000');
                    if($(".persitant-search").hasClass('show')){
                        //alert('asd');
                        $("body #search_results").addClass('padd-mobile-top');
                    }else{
                        $("body #search_results").removeClass('padd-mobile-top');
                    }
                    });

                $(".global_search").keyup(function(e){
                    
                    // hack to prevent flickering issue of fixed-header on search

                    if(!(e.keyCode == 16 || e.keyCode == 9)){
                        var track_click = 1;
                        var searched_text = $(this).val();
                        /*var searched_text = $("#headerSearchTextBox").val();*/
                        $('.global_search').val(searched_text);
                        $('.animation_image_default_search').show();
                        $.ajax({
                            method: "POST",
                            url: controller_url,
                            data: { 'page': track_click, 'searched_string': searched_text, 'load_type': 'default', 'action_name' :action_name },
                            success: function(data){
                                $('.animation_image_default_search').hide();
                                //$(".body").hide("");
                                $('#search_results').html(data);
                                track_click++;
                                
                                // var numProfiles = $('.home-list ul li').length;
                                //     console.log(e.keyCode, numProfiles);
                                // if(numProfiles>1 && numProfiles<=4) {
                                //     $('.home-list').addClass('height-offset');
                                // }
                                // else {
                                //     $('.home-list').removeClass('height-offset');
                                // }
                            }
                        })
                    }

                });

                
                /*to get the previous data while navigating back */
                (function(){ 
                    var track_click = 1;
                    var searched_text = $(".global_search").val();
                    $('.animation_image_default_search').show();
                    if(Boolean(searched_text)){
                        $.ajax({
                            method: "POST",
                            url: controller_url,
                            data: { 'page': track_click, 'searched_string': searched_text, 'load_type': 'default','action_name' :action_name  },
                            success: function(data){
                                $('.animation_image_default_search').hide();
                                $(".body").html("");
                                $('#search_results').html(data);
                                track_click++;
                            }
                        });
                    }
                })();

                /*to get current page if text is empty in search box*/
                $(".global_search").keyup(function(e){
                    var searched_text = $(".global_search").val();
                    if(!Boolean(searched_text)){
                      location.reload(true);
                      
                  }
              });

                $('body').on('click', '.search_load_more', function (){
                    $(this).hide(); 
                    $('.animation_image').show(); 
                    var searched_text = $(".global_search").val();
                    $.ajax({
                        method: "POST",
                        url: total_pages_url,
                        data: { searched_string: searched_text },
                        success: function(data){
                            if(data > 0){

                                var total_pages = data;
                                
                                if(load_more_track_click <= total_pages) 
                                {
                                    $.ajax({
                                        method: "POST",
                                        url: controller_url,
                                        data: { 'page': load_more_track_click, 'searched_string': searched_text, 'load_type': 'load_more' },
                                        success: function(data){
                                            $('.animation_image').hide();
                                            $("#results").append(data);
                                            load_more_track_click++;
                                            if(load_more_track_click <= total_pages) 
                                            {
                                                $(".search_load_more").show();
                                            }
                                        }
                                    });
                                }
                                
                            }
                        }
                    });   
                });
            });
        </script>
