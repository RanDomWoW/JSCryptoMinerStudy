    <script>
        $(document).ready(function(){
            $(".sticky-wrapper").sticky({ 
              topSpacing: 0,
          });

            $(".sticky-wrapper").on('sticky-start', function(){
                $(".home-header").css("display", "block");
                $('.show-on-scroll').show();
                $('body').addClass('padded');
                $('.menu-overlay').hide();
            //$('#sticky-header-search').css('display','block');
            $('.navbar-collapse.collapse').removeClass('in');
            //$('#sticky-header-search').focus();
            $('#hide-menu').hide();
            $("body").trigger("close");
        });
            $(".sticky-wrapper").on('sticky-end', function(){
                $(".home-header").css("display", "none");
                $('.ham-menu-listing').removeClass('d_menu');
            //$('#sticky-header-search').css('display','none');
            $('body').removeClass('padded');
            $('.menu-overlay').hide();
            //$('#fixed-header-search').focus();
            //$('.sticky-wrapper').css('display','none');
            $('#hide-menu').show();
            $("body").trigger("close");
        });

            $('.mobile-toggle-search').click(function(e) {
              e.preventDefault();
              $('.persitant-search').toggleClass("show");
          })

            $('.ham-menu').click(function(e){
             e.preventDefault();
             
             $('.ham-menu-listing').toggleClass('d_menu');
             $('.menu-overlay').toggle();
             
         });
            if($(window).width() <= 767){
                $('.navbar-toggle').click(function(e){
                //$('body').toggleClass('no-overflow');
            }); 
                $('.x-item').click(function(e){
                //$('body').toggleClass('no-overflow');
            });     
            }

            $('.navbar-toggle').click(function(e){
                e.preventDefault();
                $('.menu-overlay').show();             
            });
            $('.close-button').click(function(e){
                e.preventDefault();
                $('.menu-overlay').hide(); 
                $(".ham-menu-listing").removeClass('d_menu');    
            });

            $(".ham-menu").click(function(e){

                if(!$(".ham-menu-listing").hasClass('d_menu')){
              //$(".ham-menu-listing").css("display", "none");
              $(".ham-menu-listing").addClass('d_menu');
              $(".ham-menu-listing").show();
          }
          
          $("x-item").click (function(){
            $(".menu-overlay").hide();
        });
          


      });

            $('.mobile-toggle-search').click(function() {
            //$('.headline, .fixed-bar').css("top", "126")
            if($(".persitant-search").hasClass('show')){
                $(".headline.fixed-bar1").css("top", "114px");
            }
            else {
                $(".headline.fixed-bar1").css("top", "60px");
            }
        });
        });

        

        $('#open-button').click(function(){
            $('body').addClass("show-menu");
            $('.menu-wrap.bubble-menu').show();
            $('#hide-menu').show();

        });

        $('#open-button1').click(function(){
            $('body').addClass("show-menu");
            $('.menu-wrap.bubble-menu').show();
            //$('#hide-menu').show();
        });

        $('#close-button').click(function(){
            $(".menu-wrap.bubble-menu.hide-menu").animate({width: 'toggle'});
            $('.menu-overlay').hide();
            setTimeout( function() {
                $(".menu-wrap.bubble-menu.hide-menu").hide();
            }, 100 );
        });

        

        


    </script>

    <script type="text/javascript">
