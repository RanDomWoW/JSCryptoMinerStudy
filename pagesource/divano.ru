          var min=Math.floor(((this.tl-today)%(24*60*60*1000))/(60*1000))%60;
          var minTens = Math.floor(min/10);
          var minOnes = min%10;
          var sec=Math.floor(((this.tl-today)%(24*60*60*1000))/1000)%60%60;
          var me=this;

          if( ( this.tl - today ) > 0 ){
           timer += '<span class="countdown-img-wrap pure"><img src="/local/templates/tpl_copy/images/renewed/clock.png" class="countdown-img" /></span>'
           timer 
