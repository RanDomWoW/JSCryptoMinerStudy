           timer += '<span class="number-wrapper pure"><span class="number min pure">'+minTens+'</span><span class="number min pure">'+minOnes+'</span><span class="caption pure">мин</span></span>';
           this.elem.innerHTML = timer;
           tid = setTimeout( function(){me.countDown();},1000000 );
          }else{
           this.elem.innerHTML = this.mes;
           return;
          }
