    <script type="text/javascript">

        function slideIn(id, oldId){
            //alert("old " + oldId + " new " + id);
            $(oldId).fadeOut(0, function(){
                $('.slide .slide-copy').attr("style","");
                $('.slide .slide-food').attr("style","");
                $(id).fadeIn(1000, function(){
                    $(id+' .slide-copy').show(500);
                    $(id+' .slide-copy').animate({ left: '100px'}, 250, "swing", function(){
                        $(id+' .slide-food').show(500);
                        var l = '350px';
                        if(id==='#slide1'){l='650px'}
                        $(id+' .slide-food').animate({ left: l}, 250, "swing");
                    });
                });
            });//.css('display','none');
        }

        function setControl(i){
            $('#slide-controls img').attr('src', "http://popeyeschicken.ca/images/btn/btn-slide-red.png");
            $('#slide-controls img').each(function (index){
                if(i===(index+1)){
                    $(this).attr('src', 'http://popeyeschicken.ca/images/btn/btn-slide-white.png');
                }
            });
        }

        var totalSLides = 2;
        var slideId= totalSLides;
        var numSlides = totalSLides;
        var t;
        var stopSlider = false;
        var oldSlideId = totalSLides;

        function flipToSlide(index) {
            if(!stopSlider)
            {
                oldSlideId = slideId;
                slideId = index; //((slideId)%numSlides) +1; // set to next slide
                setControl(slideId);
                slideIn('#slide'+slideId, '#slide'+oldSlideId);
                var next = ((slideId)%numSlides) +1;
                t=setTimeout("flipToSlide("+ next +")",10000); //wait
            }
        }

        function flipToSlideOnClick(index) {
            oldSlideId = slideId;
            slideId = index; //((slideId)%numSlides) +1; // set to next slide
            setControl(slideId);
            slideIn('#slide'+slideId, '#slide'+oldSlideId);
        }

        $(document).ready(function () {
            flipToSlide(1);
            $('#slide-controls img').each(function (index){
                $(this).click(function(){
                    stopSlider = true;
                    setControl(index+1);
                    flipToSlideOnClick(index+1);
                });
            });

            $('#open-popculture').click(function(){
                stopSlider = true;
                stopPopCulture = false;

                $('#popculture').show(10, function(){
                    $('#popculture').animate({width: '100%'}, 500, "swing");
                    if(bFeedSet){
                        rollingFeed();
                    }
                    else{
                        setUpFeed();
                    }
                });
            });
            $('#close-popculture').click(function(){
                stopSlider = true;
                stopPopCulture = true;
                $('#popculture').animate({width: '0%'}, 500, "swing", function(){
                    $('#popculture').hide();
                });
            });
             $('#btn-play').click(function(){
                $('#btn-play').hide();
                $('#btn-pause').show();
                stopPopCulture = false;
                //rollingFeed();
            });
            $('#btn-pause').click(function(){
                $('#btn-pause').hide();
                $('#btn-play').show();
                stopPopCulture = true;
            });


