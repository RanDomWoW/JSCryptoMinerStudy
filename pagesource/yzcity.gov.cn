            <div id="resultWLWZ" style="display: none"></div>
            <script>
                $(function () {
                    var host = "http://admin.yzcity.gov.cn/hchat/front/site/1/hotsubject.html";
                    var limitI = 6;
                    var hostNoPath = getHostNoPath(host);

                    $.ajax({
                        url:"http://admin.yzcity.gov.cn/u/app/getHtml",
                        type:"post",
                        dataType : "jsonp",
                        jsonp : "callback",
                        callback : "ajaxQuery",
                        data:{url:host},
                        success:function (data) {
                            var dataHtml = data.html;
                            var tr = dataHtml.match(/var initData = \[.*]/)[0];
                            tr = tr.replace("var initData = [","").replace(",]","");
                            //输出到result的div里
                            $("#resultWLWZ").html(tr);

                            var list = [];
                            $("#resultWLWZ").find("li").each(function (i, item) {
                                if(i<limitI){
                                    //console.log(item);
                                    var data = {};
                                    //图片和链接
                                    var aaa = $(item).find(".border_top_bottom a")[0];
                                    var img = $(aaa).find("img")[0];

                                    var imgHref = hostNoPath + $(aaa).attr("href");
                                    var imgSrc = hostNoPath + $(img).attr("src");
                                    data["imgHref"] = imgHref;
                                    data["imgSrc"] = imgSrc;

//                    var data2 = {};
                                    $(item).find("table table tbody tr").each(function (i2, item2) {
                                        var biaoTi = $(item2).find("td")[0];//标题
                                        if($(biaoTi).find("a")){biaoTi=$(item2).find("td a")[0];}
                                        var neiRong = $(item2).find("td")[1];//内容
//                        data2[$(biaoTi).text()] = $(neiRong).text();
                                        if(i2===0)data["title"] = $(biaoTi).text();
                                        if(i2===1)data["time"] = $(neiRong).text();
                                        if(i2===3)data["describe"] = $(neiRong).text();
                                        if(i2===4)data["guest"] = $(neiRong).text();
                                    });

                                    list.push(data);
                                }
                            });

                           // console.log(list);//获得列表 ,title、time、describe、guest、imgHref、imgSrc
                            //拼接到目录里
                            var str = "";
                            $(list).each(function (i, item) {
                                var obj = item;
                                if(obj.describe.length>42)obj.describe = obj.describe.substring(0,42)+ "...";
                                if(i===0){//首个
                                    str +='<dl> <dt><img src'+'="'+obj.imgSrc+'"/></dt>';
                                    str +='<dd> <table width="100%" border="0" cellpadding="0" cellspacing="0"> <tr> <td width="80" valign="top" class="cor4 font-b">访谈主题：</td>';
                                    str +='<td class="cor"><a href'+'="'+obj.imgHref+'" target="_blank">'+obj.title+'</a></td>';
                                    str +='</tr> <tr> <td valign="top" class="cor4 font-b">访谈时间：</td>';
                                    str +='<td>'+obj.time+'</td>';
                                    str +='</tr> <tr> <td valign="top" class="cor4 font-b">主题描述：</td>';
                                    str +='<td>'+obj.describe+'</td>';
                                    str +='</tr><tr>';
                                    str +='<td valign="top" class="cor4 font-b">嘉&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;宾：</td>';
                                    str +='<td>'+obj.guest+'</td> </tr></table> </dd> <div class="cl"></div> </dl>';
                                    str +='<ul class="padd-t">';
                                }else{
                                        str+='<li><span>'+obj.time+'</span><a href'+'="'+obj.imgHref+'" target="_blank" style="cursor: pointer">'+obj.title+'</a></li>';
                                }
                                str+='</ul>';
                            });

                            $("#con_wlwz_2").append(str);
                        }
                    });
                });

                //去掉了最后一个的域名地址
                function getHostNoPath(url) {
                    var split = url.split("/");
                    var newHref = "";
                    for(var i=0;i<split.length-1;i++){
                        newHref += split[i]+"/";
                    }
                  		newHref = newHref.replace("admin","www");
                    return newHref;
                }

            </script>
