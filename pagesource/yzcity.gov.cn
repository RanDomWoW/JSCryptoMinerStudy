 /**获得央网或的方法，根据域名start**/
 function getDataByHost(number){
         getData("/u/yzinfo/getYW",number,"con_three_2");//央网
         //getData("/u/yzinfo/getSW",number,"con_three_3");// (host.search("swtj"))
         getSW_newMethod();
 }
 /**获得央网或的方法，根据域名end**/
 /**ajax异步获取数据的方法,参数为第几页start**/
 function getData(url,number,id) {
     $.ajax({
         type: "get",//使用post方法访问后台
         dataType: "json",//
         url: url,//要访问的后台地址
         contentType: "application/json;charset=utf-8",
         data: {"number": number},//要发送的数据
		 beforeSend:function(){
			 $('#'+id).append('<div id="jiazai">加载中...</div>');
			 },
         success: function (data) {
			 $('div').remove('#jiazai');
             if (data.result) {
                 var datalist = data.data;
                 $.each(datalist, function (i, item) {
                     if(i<8){
                         var title = item.title;//标题
                         if(title.length>35){
                             title = title.substring(0,35)+"...";
                         }
                         var hrefName = item.href;//链接
                         var date = item.date.substring(5);//时间
                         //添加数据
						 if(url.indexOf("YW") != -1){
							date = date.split(".")[0]+"-"+date.split(".")[1];
							$('#'+id).append('<li>' +
                             '<span>['+date+']</span><a href="'+hrefName+'" target="_blank">'+title+'</a>' +
                             '</li> ');//央网
						 }else{
							$('#'+id).append('<li>' +
                             '<span>['+date+']</span><a href="'+hrefName+'" target="_blank">'+title+'</a>' +
                             '</li> ');//央网
						 }

                         $('#con_three_2').append();//
                     }
                 });
             }
         }
     });
 }
 /**ajax异步获取数据的方法,参数为第几页end**/
function AddFavorite(sURL, sTitle) {
		sURL = encodeURI(sURL);
		try{
				window.external.addFavorite(sURL, sTitle);
		}catch(e) {
				try{
						window.sidebar.addPanel(sTitle, sURL, "");
				}catch (e) {
						alert("加入收藏失败，请使用Ctrl+D进行添加,或手动在浏览器里进行设置.");
				}
		}
}

//调用其他接口获取信息
function getSW_newMethod(){
    $.ajax({
        url : 'http://www.hunan.gov.cn/szf/zfhyjtj//pushinfo.jsonp',
        dataType : "jsonp",
        scriptCharset:"UTF-8",
        jsonp : "pushInfoJsonpCallBack",
        jsonpCallback:"pushInfoJsonpCallBack",
        timeout:2000,
        success : function(data) {
            $.each(data,function(i,item){
                if(i<8){
                    var title = item.title;//标题
                    if(title.length>35){
                        title = title.substring(0,35)+"...";
                    }
                    var hrefName = item.link;//链接
                    var date = item.pubDate;//时间
                    data = date.split('-')[1] + '-' + date.split('-')[2];
                    //添加数据
                    $('#con_three_3').append('<li>' +
                        '<span>['+data+']</span><a href="'+hrefName+'" target="_blank">'+title+'</a>' +
                        '</li> ');//

                }
            })
        }
    });
}

