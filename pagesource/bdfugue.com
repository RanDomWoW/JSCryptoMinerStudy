<script type="text/javascript">
var temp = true;

function displayMobile(elm)
{
	if(temp == true && window.innerWidth < 650)
	{
		if(elm.classList.contains('li-focus'))
		{
			elm.classList.remove('li-focus');
			elm.querySelector('.submenu').classList.remove('visible');
elm.querySelector('.second_submenu').classList.remove('visible');
		}
		else
		{
			var visible = document.querySelectorAll('.visible');
			var lifocus = document.querySelectorAll('.li-focus');
			for(let i = 0; i < visible.length; i++)
			{
				visible[i].classList.remove('visible');
				lifocus[i].classList.remove('li-focus');
			}
			elm.querySelector('.submenu').classList.add('visible');
			elm.classList.add('li-focus');
		}
	}
	else
	{
		temp = true;
	}
}

function displaySubmenuMobile(elm)
{
	if(window.innerWidth < 650)
	{
		if(elm.classList.contains('li-focus-submenu'))
		{
			elm.classList.remove('li-focus-submenu');
			elm.querySelector('.second_submenu').classList.remove('visible');
		}
		else
		{
			elm.querySelector('.second_submenu').classList.add('visible');
			elm.classList.add('li-focus-submenu');
		}
		temp = false;
	}
}
</script></div>
