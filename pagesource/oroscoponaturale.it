    console.log('"#text-11" element not found, please check Sticky Element plugin settings');
    return;
  }
  if ( !$('#bottom-sidebar').length && console) {
    console.log('"#bottom-sidebar" element not found, please check Sticky Element plugin settings');
    return;
  }

  if (winwidth >= 960) {

    var el = $("#text-11");
    var elwidthpx = el.css('width');
    var elleftmar = el.css('margin-left');
    var eltop;
    var elheight;
    var elleft = el.offset().left;

    $('<div/>').attr('id', 'clone').css('width',elwidthpx).insertAfter( $('#text-11') );

    var style = document.createElement('style');
    style.type = 'text/css';
    style.innerHTML = '.sticky-element-fixed { position: fixed; top: 0; width: ' + elwidthpx + ' !important; }';
    document.getElementsByTagName('head')[0].appendChild(style);

    $(window).scroll(function() {

      if (typeof eltop === "undefined" ) {
        eltop = el.offset().top;
      }
      elheight = el.outerHeight();

      var end = $("#bottom-sidebar");
      endtop = end.offset().top;
      var winscroll = $(window).scrollTop();

      if (winscroll > eltop) {
        $("#text-11").addClass('sticky-element-fixed');
        $("#text-11").css("left", elleft);
        $("#text-11").css("margin-left", 0);
        $('#clone').css('height',elheight);
        $('#clone').css('width',elwidthpx);
      } else {
        $("#text-11").removeClass('sticky-element-fixed');
        $("#text-11").css("left", "auto");
        $("#text-11").css("margin-left", elleftmar);
        $('#clone').css('height',0);
        $('#clone').css('width',0);
      }

      if (winscroll + elheight > endtop) {
        var amount = endtop - (winscroll + elheight);
        $("#text-11").css("top", amount + "px");
      } else {
        var amount = endtop - (winscroll + elheight);
        $("#text-11").css("top", "");
      }

    });
  }

});
