#header .description{padding-left:20px;color:#B4B4B4;font-size:14px;padding-top:0px;margin-top:-25px;text-shadow:0px 1px 0px #000;}
#header h1 a,#header h1 a:visited{color:#FFFFFF;text-decoration:none}
#header h2{padding-left:15px;color:#B4B4B4;font:14px Arial,Helvetica,Sans-serif}
#header2{float:right;width:51%;overflow:hidden;}
#header2 .widget{padding:20px 15px 0px 0px;float:right}
/* Outer-Wrapper----------------------------------------------- */
#outer-wrapper{width:1024px;margin:20px auto 0px;padding:0;text-align:$startSide;font:$bodyfont;background:url(https://3.bp.blogspot.com/-NpjejyeeqYs/WRphEzLibNI/AAAAAAADb4I/qwjF9MUYl4k_iVHdLDgiyn1Mb50OReQzACLcB/s1600/lankahotnews%2Bhedar.png) left top repeat-x;}
#content-wrapper{background:#FFFFFF;border-top:3px solid #FFFFFF;}
