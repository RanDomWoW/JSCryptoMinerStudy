overflow:hidden;/* fix for long non-text content breaking IE sidebar float */
}
#rsidebar-wrapper{width:220px;float:right;padding-top:0px;padding-right:2px;margin-left:0px;margin-right:0px;word-wrap:break-word;/* fix for long text breaking sidebar float in IE */
