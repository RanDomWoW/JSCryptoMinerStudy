var ifset = false;
$(document).ready(function(){
	
	// to check if it is mobile and if cookie is set
	var cookie = document.cookie.split(';');
	for (var i = 0; i < cookie.length; i++) {

		var chip = cookie[i],entry = chip.split("="),name = entry[0];
		if(name.indexOf("downloadBanner") >= 0){
			ifset = true;
			break;
		}
		if((i+1) == cookie.length){
			if(ifset == false){
				var userAgent = navigator.userAgent || navigator.vendor || window.opera;
				if(userAgent.match( /iPad/i ) || userAgent.match( /iPhone/i ) || userAgent.match( /iPod/i ) || (userAgent.match( /Android/i ))){
					if((window.location.href).indexOf("alexa") < 0){
						$("body").css("overflow","hidden");
						$(".downloadBanner").show();
					}
				}
			}
		}
	}
	
	$(window).scroll(function(){
		if((($(this).scrollTop()>100) && ($(window).width() < 800)) || ((($(this).scrollTop() > 500) && ($(window).width() > 800)))){
			$('.scrollTop').fadeIn();
		}else{
			$('.scrollTop').fadeOut();
		}
	});

	$('.scrollTop').click(function(){
		$("html, body").animate({scrollTop:0},600);
		return false;
	});
});
function closereset(){
	window.location.href="https://www.royalchallengers.com";
}
function continueToWebsite(){
	$(".downloadBanner").hide();
	$("body").css("overflow","auto");
	document.cookie="downloadBanner=seen; expires=Thu, 18 Dec 2017 12:00:00 UTC";
}
function linkToBanner(){
	$(".downloadBanner").hide();
	$("body").css("overflow","hidden");
	document.cookie="downloadBanner=seen; expires=Thu, 18 Dec 2017 12:00:00 UTC";
	var userAgent = navigator.userAgent || navigator.vendor || window.opera;
	if( userAgent.match( /iPad/i ) || userAgent.match( /iPhone/i ) || userAgent.match( /iPod/i ) ){
		window.location.href="https://itunes.apple.com/in/app/official-rcb-app/id516455943?mt=8&referrer=utm_source%3DRCB%2520Website%26utm_medium%3Dbanner%26utm_campaign%3DRCB%2520App";
		
	}else if(userAgent.match( /Android/i )){
		window.location.href="https://play.google.com/store/apps/details?id=com.royalchallengers.android&referrer=utm_source%3DRCB%2520Website%26utm_medium%3Dbanner%26utm_campaign%3DRCB%2520App";
	}
}
</script>
