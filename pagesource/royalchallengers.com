    <link rel="stylesheet" type="text/css" href="https://www.royalchallengers.com/sites/all/themes/rcb2017theme/css/flexslider.css">
<link rel="stylesheet" type="text/css" href="https://www.royalchallengers.com/sites/all/themes/rcb2017theme/css/slick.css">
<script src="https://www.royalchallengers.com/sites/all/themes/rcb2017theme/scripts/slick.min.js"></script>
<script>
$(document).ready(function(){
	callInitialize();
	});
function hideInfo(){
	$(".overlay2").hide();
	$(".passwordInfo").hide();
}
function callInitialize(){
	$('.slidesWrap').flexslider({
		controlNav:false,
		directionNav:false,
		slideshow: true,
		start: function(slider){
			$('.slidesWrap').removeClass('loading');			
			var activeslide = slider.currentSlide;
					},
		after: function(slider){
			$('.slideThumbWrap li').removeClass('active');
			var activeslide = slider.currentSlide;		
					}
	});
	
	$('.slideThumbWrap li').mouseover(function(){
		if($(".slidesWrap .flex-active-slide").attr('rel') != $(this).attr('rel')){
			slideTo("slidesWrap",$(this).attr('rel'));
			$('.slidesWrap').flexslider('pause');
		}
	}); 
	
	/*TabbedPanel init*/
	$('#tabbedPanel ul.tabs').tabs();

	//Accordion init
	$('.teamPanel .collapsible').collapsible({});
            
	//init the flexsliders within the tabs
	initTabbedSlider();
     
	$('#tabbedPanel ul.tabs li a').on("click",function(){
		if($(this).html() == "RCB Photos"){
			$(".multimedia").attr('href','https://www.royalchallengers.com/photos');
			$('.photoSlider').flexslider({
				animation:'slide',
				controlNav:false,
				slideshow:false,
				start: function(slider){
					$('.photoSlider').removeClass('loading');
					$(".photoSliderThumb").addClass("active");
				},
				after: function(slider){
					$(".tabbedPanelContent ul li").removeClass("active");
					$(".photoSliderThumb"+slider.currentSlide).addClass("active");
				}
			});
			setTimeout(function(){ $('.photoSlider').data("flexslider").flexAnimate(1); $('.photoSlider').data("flexslider").flexAnimate(0); },100);
		}
		else if($(this).html() == "RCB Videos"){
			$(".multimedia").attr('href','https://www.royalchallengers.com/videos');
			$('.videoNoSlider').flexslider({
				animation:'slide',
				controlNav:false,
				slideshow:false,
				start: function(slider){
					$('.videoNoSlider').removeClass('loading');
					$(".videoNoSliderThumb0").addClass("active");
				},
				after: function(slider){
					$(".tabbedPanelContent ul li").removeClass("active");
					$(".videoNoSliderThumb"+slider.currentSlide).addClass("active");
				}
			});
			var totalData = $('.videoNoSlider  ul li .title p').length;
			setTimeout(function(){$('.videoNoSlider').data("flexslider").flexAnimate(totalData); },100);
			setTimeout(function(){$('.videoNoSlider').data("flexslider").flexAnimate(0); },150);
		}else{
			$(".multimedia").attr('href','https://www.royalchallengers.com/videos/insider');
		}
	})
    
	
	var windowWidth = $(window).width();
	if(windowWidth < 992){ var maxitem = 2; }
	else{ var maxitem = 3; }
	
	//init the schedule slider
	$('.scheduleslider .slides').slick({
		slidesToShow: 3,
		slidesToScroll:3,
		infinite:false,
		arrows:true,
		dots:false,
		responsive: [{
            breakpoint: 480,
            settings: {
            arrows: true,
            centerMode: false,
            slidesToShow: 1,
			slidesToScroll:1
            }
        }]
	});
}        
function initTabbedSlider(){
			var sliderName = "videoInsiderSlider";
		$("."+sliderName).flexslider({
		animation:'slide',
		controlNav:false,
		slideshow:false,
		start: function(slider){
			$("."+sliderName).removeClass('loading');
			$("."+sliderName+"Thumb0").addClass("active");
		},
		after: function(slider){
			$(".tabbedPanelContent ul li").removeClass("active");
			$("."+sliderName+"Thumb"+slider.currentSlide).addClass("active");
		}
	});
}
function slideTo(type,key){
	$(".tabbedPanelContent ul li").removeClass("active");
	$('.'+type).data("flexslider").flexAnimate(parseInt(key));
	$('.'+type+'Thumb'+key).addClass("active");
}
</script>
