  <script>
    $('#category').on('change', function(){
      params = {};
      params.id = $(this).val();
      $("#summon").empty();
      $(".summon-info").empty();
      $(".message-summon").empty();
      $(".message-info").empty();
      $("#summon").show();

      doAjaxPost("/index/getsummons", params, function(data) {

        if (data.data.length) {
          var options = '<option value="">Elige una convocatoria</option>';
          var selected;
          $.each(data.data, function( index, value ) {
            options += '<option value="'+value.id+'">'+value.number+' '+value.name+'</option>';
          });
          $("#summon").append(options);
        } else {
          $(".message-summon").append('Aún no hay convocatorias para esa categoría');
          $("#summon").hide();
        }
          
        });
        
    });

    $('#summon').on('change', function(){
      params = {};
      params.id = $(this).val();
      $(".summon-info").empty();

      doAjaxPost("/index/getsummoninfo", params, function(data) {
          if (data.data) {
            $.each(data.data, function( index, value ) {
                  $(".summon-info").append('<span class="date-title">Fecha de Apertura</span><span class="date open">' + value.openDate + '</span>');
                  $(".summon-info").append('<span class="date-title">Fecha de Cierre</span><span class="date">' + value.closeDate + '</span><br>');

              });
          } else {
            $(".summon-info").append('<span>Aún no hay información sobre esa Convocatoria</span>');
          }
        });

      
    });

    $('#summon_fne').on('change', function(){
      var id = $(this).val();

      doAjaxGet("/index/getsummongraph/id/"+id, function(data) {
          $('#observatorio-fne-grafica').html(data);
      });
 
    });
  </script>
