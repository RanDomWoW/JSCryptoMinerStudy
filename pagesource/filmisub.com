		if (page>0) msg = msg + '&page=' + page;

		if ( filtersort != null ) {
			filtersort = filtersort.split(',');
			msg = msg + '&sortby=' + filtersort[0] + '&order=' + filtersort[1];
		}

		msg = msg.replace('catlistradio=&', '');
		if ( msg.substr(-14) == '&catlistradio=' ) msg = msg.replace('&catlistradio=', '');
		msg = msg.replace('catlistradio', 'catlist%5B%5D');
		msg = msg.replace('to-year=2017&', '');
		msg = msg.replace('from-year=1914&', '');
		msg = msg.replace('from-rating=0&', '');
		msg = msg.replace('to-rating=10&', '');

		if ( !$('.filterdisplaynone').is(':visible') ) {
			var msgar = msg.split("&");
			var index;
			for (index = 0; index < msgar.length; ++index) {
				if ( msgar[index].substr(0,7) == 'actors=' || msgar[index].substr(0,9) == 'director=' || msgar[index].substr(0,12) == 'description=' || msgar[index].substr(0,6) == 'oscar=' || msgar[index].substr(0,12) == 'goldenglobe=' || msgar[index].substr(0,7) == 'cannes=' ) msg = msg.replace('&'+msgar[index], '');
			}
		}

		if (typeof notfilter !=="undefined") msg = msg + '&notfilter=yes';

		if (typeof notfilter !=="undefined") {
			if ( filtersort[1] == 'asc' ) {
				var resorder_new_filter = 'desc';
				var ctrelka = ' &uarr;';
			} else {
				var resorder_new_filter = 'asc';
				var ctrelka = ' &darr;';
			}
			var li = '';
			var n = { date:'Дата',news_read:'Популярност',year:'Година',imdb:'IMDB',comm_num:'Коментари',rating:'Рейтинг',lenght:'Времетраене' };
					for ( indx in n ) {
						if ( filtersort[0] == 'imdb' && indx == 'imdb' ) li = li + "<li class=\""+resorder_new_filter+"\"><a href=\"#\" onclick=\"call('imdb,"+resorder_new_filter+"', null, 'yes'); return false;\">"+n[indx]+ctrelka+"</"+'a></li>\r\n';
						else if ( filtersort[0] == indx ) li = li + "<li class=\""+resorder_new_filter+"\"><a href=\"#\" onclick=\"call('"+filtersort[0]+","+resorder_new_filter+"', null, 'yes'); return false;\">"+n[indx]+ctrelka+"</"+'a></li>\r\n';
						else li = li + "<li><a href=\"#\" onclick=\"call('"+indx+",desc', null, 'yes'); return false;\">"+n[indx]+"</"+'a></li>\r\n';
					}
			$('#filter-sort ul').html(li);
		}

		$('#dle-content').before('<div id="loading-content" style="width:1000px; height:'+$("#dle-content").height()+'px;"><div></div></div>');

		$.ajax({
			type: 'GET',
			url: '/?do=filter_new',
			data: msg,
