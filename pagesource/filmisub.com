	function call (filtersort, page, notfilter) {
		if (typeof notfilter === "undefined") $('#filter-sort').hide();

		if (typeof notfilter === "undefined") $("#filter_newform").parent().nextUntil("#dle-content").slideUp();

		if (page>0) var destination = $('#dle-content').offset().top;
		if (page>0) $('html, body').animate({ scrollTop: destination }, 800);

