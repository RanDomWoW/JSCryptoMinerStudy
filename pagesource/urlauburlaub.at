        <script>
          var message = "";
          var clickedLeafletHref = "";
          var clickedLayerTarget = null;

          jQuery(document).ready(function () {
            console.log("Anzeige Map start...");

            initMap();
          });

          function initMap() {
            popup = L.popup({
              offset: new L.Point(0, -2),
              closeButton: false,
              zoomAnimation: false,
              autoPan: false
            });
            var mymap = L.map('m-map_region-container', {
              zoomAnimation: false,
              //minZoom: 5,
              zoomControl: false,
              attributionControl: false,
              dragging: false,
              touchZoom: false,
              scrollWheelZoom: false,
              doubleClickZoom: false,
              boxZoom: true,
              tap: true,
              keyboard: false,
              closePopupOnClick: false
            });
            loadGEOJson(window.urlauburlaub.templateDir + "/geo/2.json", mymap);
          }

          function fitToBounds(mymap) {
            mymap.invalidateSize();
            mymap.fitBounds(geojsonLayer.getBounds(), {padding: [0, 0]});
          }

          function setMarkedStyle(e) {
            e.target.setStyle({
              fillColor: "#B61716",
              fillOpacity: 1
            });
          }
          function setNotAvailableStyle(e) {
            e.target.setStyle({
              fillColor: "#CCCCCC",
              fillOpacity: 1
            });
          }

          function loadGEOJson(file, mymap) {
            $.getJSON(file, function (response) {
              geojsonLayer = L.geoJson(response, {
                style: {
                  fillColor: "#d9dadb",
                  weight: 1,
                  opacity: 1,
                  color: "#c3c5c6",
                  fillOpacity: 0.7
                },
                onEachFeature: function (feature, layer) {
                  if (1 == feature.properties.iso) {
                    layer.setStyle({
                      fillColor: "#b01115",
                      fillOpacity: 1
                    });
                  }
                  layer.on({
                    mousemove: function (e) {

                      if (layer.feature.properties.name && layer.feature.properties.name.length > 0) {
                        if (message) {
                          setMarkedStyle(e);
                          popup.setLatLng(e.latlng).setContent(message).openOn(mymap);
                        } else {
                          var popupMsg = "<span class='map-popup-headline'>" + layer.feature.properties.name + "</span>";
                          setMarkedStyle(e);
                          popup.setLatLng(e.latlng).setContent(popupMsg).openOn(mymap);
                        }
                      }
                    },
                    mouseout: function (e) {
                      if (1 != feature.properties.iso) {
                        geojsonLayer.resetStyle(e.target);
                      }
                      mymap.closePopup();
                    },
                    click: function (e) {
                      if (isMobile) {
                        if (clickedLeafletHref == e.target.feature.properties.href) {
                          window.location.href = baseUrl + e.target.feature.properties.href;
                        } else {
                          if (clickedLayerTarget != null) {
                            geojsonLayer.resetStyle(clickedLayerTarget);
                          }
                          clickedLayerTarget = e.target;
                          mymap.closePopup();
                          clickedLeafletHref = e.target.feature.properties.href;
                          if (layer.feature.properties.name && layer.feature.properties.name.length > 0) {
                            if (message) {
                              setMarkedStyle(e);
                              popup.setLatLng(e.latlng).setContent(message).openOn(mymap);
                            } else {
                              var popupMsg = "<span class='map-popup-headline'>" + layer.feature.properties.name + "</span>";
                              setMarkedStyle(e);
                              popup.setLatLng(e.latlng).setContent(popupMsg).openOn(mymap);
                            }
                          }
                        }
                      } else {
                        window.location.href = baseUrl + e.target.feature.properties.href;
                      }
                    }
                  });
                }
              }).addTo(mymap);
              fitToBounds(mymap);
            });
          }
        </script>
