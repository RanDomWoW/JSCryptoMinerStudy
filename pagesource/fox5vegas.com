<script type="text/javascript">
// PLATFORM.EventMan.registerToEvent(function() {
	PLATFORM.EventMan.registerToEvent(function() {
//	function CStsa() {
		
		
		// Process P tag issue
		$(".wnDVSummary").each(function(index) {
			
			$(this).find("p:first span").addClass("iceSummary");
			var oddPText = '';
			//$(this).find("p").innerHtml();
			$(this).find("p").each(function(index) {
				if (index > 0) {
					oddPText = oddPText + $(this).text() + "<br>";
				}
			});
			oddPText = oddPText.substring(0, oddPText.lastIndexOf("<br><br>"))
			if ($(this).find(".iceSummary").text() == '') {
				$(this).find(".iceSummary").html(oddPText);
				
			}
		});
		
		// Load story data
		var leftData = $wn('#WNTopStoryWrap .wnDS-20 .wnDVSummary').wnDVSummaryExtract();
		var rightData = $wn('#WNCol2 .wnDS-20 .wnDVSummary').wnDVSummaryExtract();
		if (leftData.length < 1 || rightData.length < 1) {
			$wn("#csTSA").parents(".wnDVUtilityBlock").hide();
			return;
		}

		function setBadge(stObj) {
			var badgeInsert = '';
			if (stObj.toLowerCase().indexOf("breaking") != -1) {
				badgeInsert = '<span class="badge breaking">' + stObj + '</span>';
			} else if (stObj.toLowerCase().indexOf("developing") != -1) {
				badgeInsert = '<span class="badge developing">' + stObj + '</span>';
			} else if (stObj.toLowerCase().indexOf("update") != -1) {
				badgeInsert = '<span class="badge update">' + stObj + '</span>';
			} else if (stObj.toLowerCase().indexOf("stream") != -1) {
				badgeInsert = '<span class="badge stream">' + stObj + '</span>';
			} else {
				badgeInsert = '<span class="badge">' + stObj + '</span>';
			}
			return badgeInsert;
		}

		// Build left side
		var story = leftData[0];

		if ((navigator.userAgent.toLowerCase().indexOf('chrome') > -1 || navigator.userAgent.toLowerCase().indexOf('trident') > -1) && /DT/.test(story.dateText)) {
			var mDate = new Date(story.dateText);
			mDate = (mDate.setHours(mDate.getHours() + 1));
			story.dateText = mDate;
		}

		if (CSHELPERS.secondsSince(story.dateText) < 3600) {
			story.custDate = CSHELPERS.timeAgo(story.dateText, "1 hours");
		}

		if (!story.graphicSrc) {
			story.graphicSrc = csTSA.tsaBackupImage;
		}

		if (story.sectionText.length > 0) {
			story.sectionText = setBadge(story.sectionText);
		}

		$wn('#csTSA .left').csFormatterHtml('<li>' + '<a class="sImage" href="{$headlineHref}">' + '<img src="{$graphicSrc}" />' + '</a>' + '<h4 class="sHeadline">' + '<a href="{$headlineHref}">{if $custDate}<span class="badge new">NEW</span>{/if}{$sectionText}<span class="text">{$headlineText}</span>{if $isClip}<img class="video" src="http://ftpcontent.worldnow.com/meredith/custom/2012/images/icon_video.png" />{/if}</a>' + '</h4>' + '{if $custDate}<div class="sTimestamps">Updated: {$custDate}</div>{/if}' + '<div class="sSummary">{$summaryHtml}<a class="sSummaryLink" href="{$headlineHref}">More &gt;</a></div>' + '</li>', story);

		// Build right side
		$wn.each(rightData, function(i) {
			if (i >= 10) {
				return false;
				//break
			}
			if ((navigator.userAgent.toLowerCase().indexOf('chrome') > -1 || navigator.userAgent.toLowerCase().indexOf('trident') > -1) && /DT/.test(this.dateText)) {
				var mDate = new Date(this.dateText);
				mDate = (mDate.setHours(mDate.getHours() + 1));
				this.dateText = mDate;
			}

			if (CSHELPERS.secondsSince(this.dateText) < 3600) {
				this.custDate = CSHELPERS.timeAgo(this.dateText, "1 hours");
			}
			if (this.sectionText.length > 0) {
