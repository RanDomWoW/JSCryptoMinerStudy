<script type="text/javascript">
	$(function(){
		$('.window-switch').click(function(){
			$(this).toggleClass('close');
			$('.window-body').toggleClass('close');
		});
		
		function validate_float_window(form)
        {
            var result=true;
            if(form.find(".name").val().length==0){
                form.find(".name").addClass("error");
                result=false;
            }
            else{
                form.find(".name").removeClass("error");
            }

            if(form.find(".phone").val().length==0||!/^\d{11}$/.test(form.find(".phone").val())){
                form.find(".phone").addClass("error");
                result=false;
            }
            else{
                form.find(".phone").removeClass("error");
            }

            if(form.find(".company").val().length==0){
                form.find(".company").addClass("error");
                result=false;
            }
            else{
                form.find(".company").removeClass("error");
            }
            if(form.find(".office").val().length==0){
                form.find(".office").addClass("error");
                result=false;
            }
            else{
                form.find(".office").removeClass("error");
            }
            return result;
        };
        $("#window-sub").click(function(e){
            e.preventDefault();
            var form=$(this).parents("form");
            if(validate_float_window($(form[0]))){
                $.ajax({
                    type:"post",
                    data:form.serialize()+"&&extData[bigdata]="+encodeURIComponent("招商会报名"),
                    url:"http://www.zhiziyun.com:80/addApplicant.action",
                    success:function(){
                        alert("申请成功！");
                    }
                });
            }
        });
	});
</script>
