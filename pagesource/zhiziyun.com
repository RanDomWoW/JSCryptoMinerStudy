			<ul>
				<li><input placeholder="姓名" type="text" name="name" class="name"/></li>
                <li><input placeholder="电话" type="text" name="phone" class="phone"/></li>
                <li><input placeholder="公司" type="text" name="companyname" class="company"/></li>
                <li><input placeholder="职位" type="text" name="office" class="office"/></li>
