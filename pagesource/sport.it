<script >
	function selectionRegione(val)
	{
		$("#linkSportReg").hide();
		if (val != "")
		{
			$("#selectSportReg").show();
			$("#selectSportReg").val("");
		}else
			$("#selectSportReg").hide();
		
	}
	function selectSport(val)
	{
		var myXml = "";
		
		myXml = "indice_regionali.xml";
		
		$.ajax({
			type: "GET",
			url: "/xml/rec/" + myXml,
			dataType: "xml",
			success: function(xml){
				count=1;
				var maxel = $(xml).find('comp').length;
				$('#linkSportReg').html("<option>Seleziona competizione</option>");
				$('#linkSportReg').show();
				
				$(xml).find('comp').each(function(index){
						sValue = "";
							var g = $(this).attr('level2').toLowerCase();
							
							var h = $('#selectRegione').find(":selected").attr("value");
							
							if (g.indexOf(val)>=0 &&  $(this).attr('level1').toLowerCase() == h)
							{
								if ($(this).attr('level3'))
									sValue = $(this).attr('level3');

								if ($(this).attr('level4'))
									sValue += " - " + $(this).attr('level4');
							}
								
						
						if (sValue.length > 0)
						{
							var link = $(this).attr('url');
							
							//link = link.replace("/2015-2016/","/");
							link = link.replace("/regionali/","/");
							
							$('#linkSportReg').append($('<option>', {
								value: link,
								text: sValue
							}));
						}
						//console.log(sValue);
						
						count++;
			
				});
			},
			error: function() {
				$('#linkSport').hide();
				//alert(error.val());
			}
		});		
		
		
	}
	
	function gotoComp(val)
	{
		console.log (val);
		top.location.href = val;
		
		
	}
</script>


