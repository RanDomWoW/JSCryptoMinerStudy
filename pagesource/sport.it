<script type="text/javascript">
	var selectedtab='7233';
	
	function setSelectedTestPlan(el) 
	{  
       $(".live-home ul li").removeClass("selected");
       $("#" + el).closest('li').addClass("selected");
		selectedtab = el;
		setCampionato("#camp" + el);
    };



    function setCampionato(al) {      
	   $(al).find(".match:first").addClass("visible");
	  
       $(".live-home-partite").addClass("hidden");
       $(al).removeClass("hidden");
       $(".arrow2_style").addClass('arrow2');
       $(".arrow2_style").removeClass('sottosopra hidden');
       $(".hidden-live").addClass('hidden');
    };


$("document").ready(function(){

/* live home - back up */

    $( ".glyphicon-chevron-up" ).click(function() {
	  $(".live-home2").addClass("live-home2up");
	  $(this).addClass("hidden");
	  $(".glyphicon-chevron-down").removeClass("hidden");
	  $( ".live-home2 .title" ).css( "cursor", "pointer" );
	});

/* live home - go down */

	
    $( ".glyphicon-chevron-down" ).click(function() {
    					console.log($( ".live-home2" ).hasClass("live-home2up"));
	  $( ".live-home2" ).removeClass("live-home2up");
	  $(this).addClass("hidden");
	  $(".glyphicon-chevron-up").removeClass("hidden");
	  $( ".live-home2 .title" ).css( "cursor", "pointer" );
	});

	

	setSelectedTestPlan(selectedtab);
/* live home arrows left-right */

	$(".live-nav-right").click (function() {	
		compSel = $(this).parent();
		itemSel = compSel.find(".visible");
		if (itemSel.next(".match").length > 0)
			itemSel.next(".match").addClass("visible");
		else
			compSel.find(".match:first").addClass("visible");
		
		itemSel.removeClass("visible");
	});
	
	$(".live-nav-left").click (function() {
		compSel = $(this).parent();
		itemSel = compSel.find(".visible");
		if (itemSel.prev(".match").length > 0)
			itemSel.prev(".match").addClass("visible");
		else
			compSel.find(".match:last").addClass("visible");
		
		itemSel.removeClass("visible");
	});
	var inizializeElement = true;
	var count =0;
	var timerTemp = 20000;
	readliveXML();
	var myInterval = setInterval(readliveXML,timerTemp);
//          url: "/temporeale/liveresult.xml",
	
	function readliveXML()
	{
			$.ajax({ 
					type: "GET",
					url: "http://www.sport.it/temporeale/calcio/liveresult.xml",
					dataType: "xml",
					cache: false,
					success: function(xml) 
					{
            // FCM INIT
            console.log("**** READXML");
						console.log(xml);
            // FCM END
						count=0;
						 $(xml).find('match').each(function(){
							id = $(this).attr("idmatch");
							//checkLiveresult(id,$(this))
						});
					},
					error:function()
					{
						console.log("no XML");
						clearInterval(myInterval);
					}
			});
	}
	
		function checkLiveresult(id,xml) {
		console.log('**** CHECK RESULT:'+id);
		var textAlert = "";
		var mymatch = $("#" + id);
		
		if (!mymatch)
			return false;
		console.log(xml);
		typegolStr = "";
		
		golA= Number($(xml).find("gols").attr("golA"));
		golB = Number($(xml).find("gols").attr("golB"));
		goltempA = mymatch.find(".nResult").html();
		goltempB = mymatch.find(".nResult2").html();
		mymatch.removeClass("matchHover");
    //FCM tempo =$(xml).attr("time");
		tempo =$(xml).attr("matchtime");
		tempoDesc= '';
    
		if (tempo == '0')
			return false;		
		
    if (tempo=='PT') {
      tempoDesc = "Primo tempo";
      classMatch= 'green';
    } else if (tempo=='ST') {
      tempoDesc = "Secondo tempo";
      classMatch= 'green';
    } else if (tempo=='1TS' || tempo=='2TS') {
      tempoDesc = "Supplementari";
      classMatch= 'green';
    } else if (tempo=='INT' || tempo=='INTS'  || tempo=='INTP') {
      tempoDesc = "Intervallo";
      classMatch= 'green';
    } else if (tempo=='RINV') {
      tempoDesc = "Rinviata";
      classMatch= 'yellow';
    } else if (tempo=='SOSP') {
      tempoDesc = "Sospesa";
      classMatch= 'yellow';
    } else if (tempo=='RIG') {
      tempoDesc = "Rigori";
      classMatch= 'green';
    } else if (tempo=='TERM') {
      tempoDesc = "Terminata";
      classMatch= 'red';
    }
		

		mymatch.find(".statusTime").html(tempoDesc);
		
		var btrue=false; 
		goleador="";
		textAlert="";
		team="";
		if (inizializeElement)
		{
			mymatch.find(".statusMatch").removeClass("yellow");
			mymatch.find(".statusMatch").removeClass("orange");
			mymatch.find(".statusMatch").removeClass("red");
			mymatch.find(".statusMatch").removeClass("green");
      
      mymatch.find(".statusMatch").addClass(classMatch);

			if (goltempA == "") {
				mymatch.find(".nResult").html(golA);
			}			
			if (goltempB == "") {
				mymatch.find(".nResult2").html(golB);
			}			
			//return false;
			
		}
		
	  goltempA = mymatch.find(".nResult").html();
		goltempB = mymatch.find(".nResult2").html();
		
		if (golA > goltempA) {
			goleador = $(xml).find("goleador:first");
			team = mymatch.find(".nTeam").html();
			mymatch.find(".nResult").html(golA);
			mymatch.find(".nResult2").html(golB);
			btrue=true;
		} else if (golB > goltempB) {
			goleador = $(xml).find("goleador:first");			
			team = mymatch.find(".nTeam2").html();	
			mymatch.find(".nResult2").html(golB);
			mymatch.find(".nResult").html(golA);
			btrue=true;
		}
		
		if (btrue==true) {
			typegol = goleador.attr("type");
			if (goleador.attr("min")==undefined)
				min="";
			else				
				min = goleador.attr("min")+ "'";
			if (typegol == "A")
				typegolStr = "(autorete)";
			if (typegol == "R")
				typegolStr = "(rigore)";				
			textAlert = "GOL " + team + " - "+min+" " + goleador.text() + " " + typegolStr;
			
		   idcomp = mymatch.parent().attr("id");
		   idcomp = idcomp.replace("camp", ""); 
			   
		   setTimeout(function() { changeMatchForResultDelay(idcomp,mymatch,textAlert)}, count*4000);
		   count++;
			   
			  
			}
			
		}	
		
		
	function changeMatchForResultDelay(idcomp,mymatch,textAlert)
	{
		if (idcomp != selectedtab)
			setSelectedTestPlan(idcomp);
		
		compSel = $("#camp" + selectedtab);
 
	   $("#wraptab").val(idcomp);
	   setSelectedTestPlan(idcomp);
	  
		itemSel = mymatch.parent().find(".visible");
		itemSel.removeClass("visible");
		mymatch.addClass("visible");
		
