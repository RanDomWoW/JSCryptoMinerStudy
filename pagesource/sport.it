</div>
<br clear='all'/>
<script >
	function selectionSport(val)
	{
		var myXml = "";
		
		if (val=='basket')
			myXml = "indice_basket.xml";
		else if (val=='volley')
			myXml = "indice_volley.xml";
		else if (val=='calcio')
			myXml = "indice_calcio.xml";
		else 
			myXml = "indice_altrisport.xml";
		
		$.ajax({
			type: "GET",
			url: "/xml/rec/" + myXml,
			dataType: "xml",
			success: function(xml){
				count=1;
				var maxel = $(xml).find('comp').length;
				$('#linkSport').html("<option>Seleziona il campionato</option>");
				if (val == "calcio")
				{
					$('#linkSport').append("<option value='/live/calcio/serie-a/risultati-classifica-calendario-diretta.htm'>Serie A</option>");
					$('#linkSport').append("<option value='/live/calcio/serie-b/risultati-classifica-calendario-diretta.htm'>Serie B</option>");
					$('#linkSport').append("<option value='/live/calcio/champions-league/risultati-classifica-calendario-diretta.htm'>Champions League</option>");
					$('#linkSport').append("<option value='/live/calcio/europa-league/risultati-classifica-calendario-diretta.htm'>Europa League</option>");
					$('#linkSport').append("<option value='/live/calcio/bundesliga/risultati-classifica-calendario-diretta.htm'>Bundesliga</option>");
					$('#linkSport').append("<option value='/live/calcio/liga/risultati-classifica-calendario-diretta.htm'>Liga</option>");
					$('#linkSport').append("<option value='/live/calcio/ligue-1/risultati-classifica-calendario-diretta.htm'>Ligue 1</option>");
					$('#linkSport').append("<option value='/live/calcio/premier-league/risultati-classifica-calendario-diretta.htm'>Premier League</option>");
					$('#linkSport').append("<option value='/live/calcio/copppa-italia/risultati-classifica-calendario-diretta.htm'>Coppa Italia</option>");
					$('#linkSport').append("<option value='/live/calcio/euro-2016/girone-a/risultati-classifica-calendario-diretta.htm'>Euro 2016 qualificazioni</option>");
				}
				if (val == "basket")
				{
					$('#linkSport').append("<option value='/live/basket/serie-a/risultati-classifica-calendario-diretta.htm'>Serie A</option>");
					$('#linkSport').append("<option value='/live/basket/nba/risultati-classifica-calendario-diretta.htm'>NBA</option>");
				}
				$('#linkSport').show();
				$(xml).find('comp').each(function(index){
						sValue = "";
						if (myXml == "indice_altrisport.xml")
						{
							var g = $(this).attr('level1').toLowerCase();
							//console.log (val.indexOf(g));
							if (g.indexOf(val)>=0)
							{
								if ($(this).attr('level1'))
									sValue = $(this).attr('level1');
								if ($(this).attr('level2'))
									sValue += " - " + $(this).attr('level2');
								if ($(this).attr('level3'))
									sValue += " - " + $(this).attr('level3');
							}
						}
						else
						{
							if ($(this).attr('level2') != 'nazionale')
								sValue = $(this).attr('level2') + " - ";							
							if ($(this).attr('level3'))
								sValue += $(this).attr('level3');
							if ($(this).attr('level4'))
								sValue += " - " + $(this).attr('level4');
						}			
						
						if (sValue.length > 0)
						{
							var link = $(this).attr('url');
							link2 = link;
							if ( val == "calcio" || val == "basket" || val == "volley")
							{
								//val = val.replace("/calcio","");
								console.log(val);  
								link = link.replace("/nazionali","");
								link = link.replace("/regionali","");
							}
							
							if (link2.indexOf("region") < 0)
							{
								$('#linkSport').append($('<option>', {
									value: link,
									text: sValue
								}));
							}
						}
						//console.log(sValue);
						
						count++;
			
				});
			},
			error: function() {
				$('#linkSport').hide();
				//alert(error.val());
			}
		});		
		
		
	}
	
	function gotoComp1(val)
	{
		console.log (">>>>>>>>>>>>>" + val);
	
		top.location.href = val;
		
		
	}
</script>


