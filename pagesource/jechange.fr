    /**
     * Cette fonction retourne la date d’expiration du cookie de consentement
     * @author Emprunté à la CNIL
     * @see http://www.cnil.fr/vos-obligations/sites-web-cookies-et-autres-traceurs/outils-et-codes-sources/la-mesure-daudience/
     */
    function getCookieExpireDate() {
        var cookieTimeout = 34214400000; // Le nombre de millisecondes que font 13 mois
        var date = new Date();
        date.setTime(date.getTime() + cookieTimeout);
        return date.toGMTString();
    }

    /**
     * Cette fonction positionne le cookie d'acceptation des conditions d'utilisation
     */
    function acceptationCookies() {
        var bandeau = document.getElementById("bandeauCookiesCnil");
        if (bandeau == null) {
            return false;
        }
        bandeau.parentNode.removeChild(bandeau);
        document.cookie = 'hasConsent=true; expires='+ getCookieExpireDate() + '; path=/';
    }

    /**
     * Fonction d'affichage du bandeau explicitant les conditions d'utilisation du site
     */
    function afficheBandeauCnil() {
        if (document.cookie.indexOf('hasConsent=') > -1) {
            // Déjà setté
            document.getElementById("bandeauCookiesCnil").style.display = "none";
            return;
        }
        document.getElementById("bandeauCookiesCnil").style.display = "block";
        var tabElements = document.getElementsByTagName("a");
        for (var i = 0; i < tabElements.length; i++) {
            var element = tabElements[i];
            if (!element.hasAttribute("href") || element.getAttribute("href") == "") {
                continue;
            }
            element.addEventListener("click", acceptationCookies, false);
        }
        tabElements = document.querySelectorAll(".js-linkify");
        for (var i = 0; i < tabElements.length; i++) {
            var element = tabElements[i];
            if (!element.hasAttribute("data-href") || element.getAttribute("data-href") == "") {
                continue;
            }
            element.addEventListener("click", acceptationCookies, false);
        }
        tabElements = document.querySelectorAll(".facebox");
        for (var i = 0; i < tabElements.length; i++) {
            var element = tabElements[i];
            if (!element.hasAttribute("data-href") || element.getAttribute("data-href") == "") {
                continue;
            }
            element.addEventListener("click", acceptationCookies, false);
        }
    }

    if (typeof window.addEventListener == 'function') {
        window.addEventListener("load", afficheBandeauCnil, false);
    } else {
        window.attachEvent("onload", afficheBandeauCnil);
    }

    // ]]>
</script>
