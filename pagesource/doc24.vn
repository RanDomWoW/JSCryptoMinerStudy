$('#city_id').select2({placeholder: 'Chọn tỉnh, thành phố'});
$('#place_id').select2({placeholder: 'Chọn quận, huyện'});
$('#school_id').select2({placeholder: 'Chọn trường đang học'});
$('#grade_id').select2({placeholder: 'Trình độ học vấn của bạn'});
var curr_city_id = "";
var curr_place_id = "";
var curr_school_id = "";
var regx_number = /[0-9]+/;
if(regx_number.test(curr_city_id))
{
	$.ajax({
		url: 'https://doc24.vn//places/get_place_for_city',
		data:{city_id: curr_city_id},
		type: 'POST',
		dataType: 'json',
		success: function(res)
		{
			var html_place = "";
			for(var i = 0; i < res.length; i++)
			{
				if(regx_number.test(curr_place_id) && curr_place_id == res[i]['id'])
					html_place += "<option value='"+res[i]['id']+"' selected>"+res[i]['title']+"</option>";
				else
					html_place += "<option value='"+res[i]['id']+"'>"+res[i]['title']+"</option>";
			}
			$('#place_id').html(html_place);
			$('#place_id').select2({placeholder: 'Chọn quận, huyện'});
			var place_id = $('#place_id').val();
			var city_id = $('#city_id').val();
			$('#school_id').select2("val","");
			$.ajax({
				url: 'https://doc24.vn//schools/get_school_for_place',
				data:{city_id: city_id, place_id: place_id},
				type: 'POST',
				dataType: 'json',
				success: function(res)
				{
					var html_school = "";
					for(var i = 0; i < res.length; i++)
					{
						if(regx_number.test(curr_school_id) && curr_school_id == res[i]['id'])
							html_school += "<option value='"+res[i]['id']+"' selected>"+res[i]['title']+"</option>";
						else
							html_school += "<option value='"+res[i]['id']+"'>"+res[i]['title']+"</option>";
					}
					html_school += "<option value='999999'>Không còn đi học</option>";
					$('#school_id').html(html_school);
					$('#school_id').select2({placeholder: 'Chọn trường đang học'});
				},
				error: function()
				{
					console.log("không lấy được danh sách trường");	
				}
			});
		},
		error: function()
		{
			console.log("không lấy được danh sách quận huyện");	
		}
	});
}
