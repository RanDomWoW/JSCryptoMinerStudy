<script type="text/javascript">
	var anzTagesdealsNachgeladen = 0;
	var tagesDealsSperre = false;
	$(document).ready(function(){
		if($.cookie("lsa_start_box") != "1"){
			$('.start-box').show();
			$.cookie('lsa_start_box', "1", { expires: 180, path: '/' });
		}
		
		$(window).scroll(function(){
			if(!tagesDealsSperre && anzTagesdealsNachgeladen < 2){
				if($('#last_msg_loader').is(":within-viewport-bottom")){
					tagesDealsSperre=true;
					anzTagesdealsNachgeladen++;
					ladeTagesdealsNach();
				}
			}
		});
		
		/* Wenn Smartphone, Videos kleiner abspielen */
		if($(window).width() < 420){
			$('.youtube_vid').each(function() {
				var vid = $(this).attr('href');
				vid = vid.replace("height=360&width=640", "height=240&width=300");
				$(this).attr('href', vid);
			});
		}else
		if($(window).width() < 800){
			$('.youtube_vid').each(function() {
				var vid = $(this).attr('href');
				vid = vid.replace("height=360&width=640", "height=250&width=450");
				$(this).attr('href', vid);
			});
		}
	});
</script>
