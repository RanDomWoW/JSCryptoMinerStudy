<script type="text/javascript">
(function() {
  'use strict';

  var videoElem = document.getElementById('fc-hero__video');

  if (videoElem) {

  	var videoSource = document.querySelectorAll('.fc-hero__video source');
  	var videoPoster = '';
  	videoPoster = 'https://cdn.papershift.com/20160519145459/Dienstplanung-online-einfach-schnell-und-flexibel-mit-Papershift.jpg';  	var videoLoaded = false;

  	// add poster - faster this way
  	videoElem.style.backgroundImage = 'url(' + videoPoster + ')';

  	// responsive video
  	function responsiveVideo(videoElement, sources) {

  	  if (window.innerWidth > 767) {

  	    if (!videoLoaded) {
  	      videoElem.style.backgroundImage = 'url(' + videoPoster + ')';
  	    }

  	    [].forEach.call(sources, function(source) {
  	        var src = source.getAttribute('data-src');
  	        source.setAttribute('src', src);
  	    });

  	    setTimeout(function() {
  	          videoElement.load();
  	          videoLoaded = true;
  	    }, 2000);


  	  } // endif
  	}

  	// load
  	window.addEventListener('load' , function() {
  	  responsiveVideo(videoElem, videoSource);
  	}, false);

  	// resize
  	window.addEventListener('resize' , function() {

  	  if (!videoLoaded) {
  	    responsiveVideo(videoElem, videoSource);
  	  }

  	}, false);

  }

}());


</script>






