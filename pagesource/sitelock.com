function updateCountry(){
    var countryCode = $('div.selected-dial-code').html();
    var countryName = $('div.selected-flag').attr('title').toString();
    var countryAbbr = $('li.country.highlight.active').attr('data-country-code');
    $('input#ccode').val(countryAbbr);
    $('input#fullInfo').val(countryName);
        
    var marker1 = countryName.indexOf('(');
    var marker2;
    if(marker1 < 0 ) {
        marker2 = countryName.indexOf(':');
    } // End if
    if(marker1 > 0) {
        updatedCountry = countryName.slice(0,  marker1 )  ;
    } else {
        updatedCountry = countryName.slice(0, marker2  );
    } // End if-else
        
    $('input#hidden').val(countryCode);
    $('input#countryName').val(updatedCountry);
    return true;
} // End 'updateCountry'



