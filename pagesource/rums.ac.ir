function showTabContent(divId){
	 var $__Parent=document.getElementById("ul1");
	 var $$_ElementsCount=$__Parent.getElementsByTagName("li").length;
	 for(var i=1;i<=$$_ElementsCount;i++)
	 {
	    if(i==divId.substring(7,8)){
	        document.getElementById("News"+i).className="active";
	        document.getElementById("tabNews"+i).className="active";
	    }
	    else{
	        document.getElementById("News"+i).className="content";
	        document.getElementById("tabNews"+i).className="content";
	    }
	}
}
 
 
 
