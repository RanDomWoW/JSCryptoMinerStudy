<script type="text/javascript">
 function slideshowImg(slideshowid, existing) { 
   var ssname = "s";
   var ssmax = 21;
   var n = "";
   do {
     n = '/slideshow/'+ssname+Math.floor(Math.random()*ssmax)+'.jpeg';
   } while (n==existing);
   return n;
 }
 function slideshowFrame(slideshowid) { 
   var s1 = $(slideshowid+" :first-child");
   var s2 = s1.next();

   var ax = 0;
   var ay = 0;
   var bx = 598-1024;
   var by = 418-520;
   if (Math.random() > 0.5) {
     var t = ax; ax = bx; bx = t;
   }
   if (Math.random() > 0.5) {
     var t = ay; ay = by; by = t;
   }
   var time = 2000; // actual time is this *4
   
   s1.stop().appendTo(slideshowid); // put s1 in front
   s2.stop().animate({
    left: ax+'px', 
    top: ay+'px', 
    opacity: 1.0
   }, 0);
   s2.attr('src', slideshowImg(slideshowid, s1.attr('src')));
   ox = parseInt(s1.css('left'));
   oy = parseInt(s1.css('top'));
   s1.animate({
    left: ((ax + bx*3)/4)+'px', 
    top: ((ay + by*3)/4)+'px', 
   }, time*3, "linear").animate({
    left: bx+'px', 
    top: by+'px', 
    opacity: 0.0
   }, time, "linear");
 }
 function slideshowStart(slideshowid) {
     slideshowFrame(slideshowid);
     window.setInterval("slideshowFrame('"+slideshowid+"')", 8000); 
 }

 $(function() {
   $("#slideshow :first-child").attr('src', slideshowImg('#slideshow'));
   slideshowStart('#slideshow');      
   
