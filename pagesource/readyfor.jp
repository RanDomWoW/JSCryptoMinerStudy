  function getCookie(name) {
    var cookieData = document.cookie;
    var key = name + "=";

    var idx = cookieData.indexOf(key);
    if (idx == -1) return null;
    idx += key.length;

    var end = cookieData.indexOf(";", idx);
    if (end == -1) end = cookieData.length;

    return decodeURIComponent(cookieData.substring(idx, end));
  }

  var hostnamePattern = new RegExp("^https://readyfor.jp(.*$)");
  var reportError = function(evt) {
    var filename = evt.filename;
    if (hostnamePattern.test(filename)) {
      filename = filename.match(hostnamePattern)[1];
    }

    var action = filename + ":" + evt.lineno + "," + evt.colno;
    var label  =
      evt.message + ", " +
      "href: " + location.href + ", " +
      "rf_uuid: " + getCookie("rf_uuid") + ", " +
      "userAgent: " + window.navigator.userAgent;

    ga('set', 'nonInteraction', true);
    ga('send', 'event', {
      eventCategory: 'JSError',
      eventAction: action, 
      eventLabel: label
    });
  };
  if (window.addEventListener) {
    window.addEventListener("error", reportError);
  } else {
    // Compatibility for IE8
    window.attachEvent("onerror", reportError);
  }
</script>
