						new_source += '?r=' + random;
					}

					if(decodeURIComponent(image.src.replace(/(http:|https:)/,'')) !== new_source.replace(/(http:|https:)/,'')){
						// Предзагрузка картинок для Google Page Speed
						if(image.className.indexOf('preload') != -1){
							image_preloader(new_source);
						}
						//change img src to basePath + response
						if(setSize == 1){
							if(matches != null){
								image.style.height = matches[1] + 'px';
								image.src = 'https://s.winestyle.ru/templates/default/StylesNew/images/empty.png';
								
								// После определения размера еще раз проверим попадает ли изображение (на этот раз его нижняя граница)
								if(dontload == 1 && !stopload){
									offsetBlock += parseInt(matches[1]);
									if(sT <= offsetBlock && offsetBlock <= (hw + sT)) dontload = 0;					
								}
							}
						}
						if(dontload == 0){

							(function(image,new_source){
								
								//не показываем анимацию
								image.src = new_source;
								image.onload = function(){
									if (this.className.indexOf('resetheight') != -1) this.style.height = 'auto';

									image.style.opacity = 1;
									this.onload = function(){};
								}	

							})(image, new_source);

						}
					}
					//break loop
					break;
				}
			}
		}
		setTimeout(function(){mir_exe = 0},100);
	}

	makeImagesResponsive(document.body, 1, 1);

	cssLoader();

</script>







