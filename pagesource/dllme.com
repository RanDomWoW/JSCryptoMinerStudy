			<script>
				(function () {
					function load_comments() {
						$.post("", {action: "load_comments", offset: $("#comments .comments > article").length}, function (data) {
								$("#comments .comments").append(data);
								var comm = $("#comments .comments > article");
								$("#comments div.load").toggle(comm.length < parseInt($("#comments div.load").attr("data-total")))
								if(comm.length)
									$("#comments .comments > p.nocomments").remove();
							});
					}

					$("#comments div.open").one("click", function () {
							$(this).hide();
							$("#comments div.add").show();
							load_comments();
							$("#comments").get(0).scrollIntoView();
						});
					$("#comments div.load").on("click", load_comments);
					$("#details div.open-comments").on("click", function () {
							$("#comments div.open").click();
							$("#comments").get(0).scrollIntoView();
						});

					var add_box = $("#comments div.add");
					add_box.find("button").on("click", function () {
							var name = add_box.find("input[name=name]").val();
							var comment = add_box.find("textarea").val();
							var captcha = add_box.find("input[name=captcha]").val();

							if(!name) {
								alert("Name is required");
								return false;
							}

							if(!comment) {
								alert("Comment is required");
								return false;
							}

							if(!captcha) {
								alert("Captcha is required");
								return false;
							}

							$.post("", {action: "add_comment", name: name, comment: comment, captcha: captcha}, function (data) {
									if(data.substr(0, 7) != "SUCCESS") {
										alert(data);
										return;
									}

									$("#comments .comments").prepend(data.substr(7 + 2));
									$("#comments div.load").attr("data-total", parseInt($("#comments div.load").attr("data-total")) + 1)

									add_box.find("input[name=name], input[name=captcha], textarea").val("");
									var source = add_box.find(".captcha img").attr("src");
									add_box.find(".captcha img").attr("src", (source.indexOf("?") == -1 ? source : source.substr(0, source.indexOf("?"))) + "?" + Date.now());

									$("#comments .comments > p.nocomments").remove();
								});

							return false;
						});
					$("div.search-holder").on("click", "#autocomplete-box li a", function() {
							$(this).closest("div.search-holder").find("input[name=keyword]").val($(this).text());
							$(this).closest("form").submit();
							return false;
						});
					var ac_post = null, ac_processing = false, ac_last = "";
					$("input[name=keyword]").on("change keyup", function () {
							var ac_val = $(this).val();
							if(ac_processing || ac_val == ac_last)
								return;
							else
								ac_processing = true;
							ac_last = ac_val;
							if($("#autocomplete-box").length)
								$(this).after($("#autocomplete-box"));
							else
								$(this).after('<ul id="autocomplete-box"></ul>');
							var box = $("#autocomplete-box");
							if(ac_post) {
								ac_post.abort();
								ac_post = null;
							}
							if(ac_val)
								ac_post = $.post("/advanced-search.php", {autocomplete: ac_val}, function (result) {
										box.empty();
										var elem;
										for(i in result) {
											elem = $('<li><a href="#"></a></li>');
											elem.find("a").text(result[i]);
											box.append(elem);
										}
										ac_post = null;
									});
							else
								box.empty();
							ac_processing = false;
						});
				})();
			</script>

