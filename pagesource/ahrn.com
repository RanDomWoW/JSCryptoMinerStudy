                <script>
                function callBase() {
                    var baseName = document.getElementById('home_search').value;
                    var cleanBaseName = encodeURIComponent( baseName );
                    // console.log(cleanBaseName);

                    $.ajax({
                        type: "GET",
                        url:  'get_base_id.php',
                        data: { 'basename':cleanBaseName },
                        error: function(xhr) { return false; },
                        success: function(response) {
                            handleBaseIdResult(response);
                        }
                    });
                }

                function handleBaseIdResult(result) {

                    if ( ! result ) {
                        var errorBox = document.createElement("div");
                        errorBox.className = "home_search_error";
                        var error = document.createElement("p");
                        error.className = "error";
                        var errorText = document.createTextNode("Base not found!");
                        error.appendChild(errorText);
                        errorBox.appendChild(error);

                        document.getElementsByTagName('body')[0].appendChild(errorBox);

                        return false;
                    }

                    window.open(result, '_self'); 
                }

            </script>
