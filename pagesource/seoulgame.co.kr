<script type="text/javascript">
var CurrentPage = window.location.href; //현재 주소



var startArr = new Array(3);
startArr[0] = CurrentPage.indexOf("n_keyword="); //n_keyword= 가 등정하는 인덱스 번호
startArr[1] = CurrentPage.indexOf("DMKW="); //DMKW= 가 등정하는 인덱스 번호
startArr[2] = CurrentPage.indexOf("smtg_ad="); //smtg_ad= 가 등정하는 인덱스 번호


for(var i =0; i<3; i++)
{
	if(startArr[i] != -1){
		var list = CurrentPage.substring(startArr[i]); 
		//세 개 중 키워드는 의도적으로 넣지 않는 이상 검색엔진에선 무조건 1개의 변수만 받는다.
		//다른 검색엔진 또는 직접 원본 주소를 입력하는 경우 list는 수행하지 않고 전체 길이를 받는다.
		//전체 길이를 받는다는 것은 키워드가 없다는 뜻
	}
}

var filter = "win16|win32|win64|mac|macintel";
if ( navigator.platform  ) {
	if ( filter.indexOf( navigator.platform.toLowerCase() ) > 0 ) {
		if(startArr[0] != -1 || startArr[1] != -1 || startArr[2] != -1){
			//location.href = "/index.php?" + list; //PC 페이지 경로
			document.write("<meta http-equiv='refresh' content='0; url=/index.php?" + list + "'>");
		}
		else
		{
			//location.href = "/index.php" //네이버를 경유하지 않았을때, 키워드(n_keyword)가 없을때
			document.write("<meta http-equiv='refresh' content='0; url=/index.php'>");
		}
	} else {
		if(startArr[0] != -1 || startArr[1] != -1 || startArr[2] != -1){
				//location.href = "/m/?" + list; //모바일 페이지 경로
				document.write("<meta http-equiv='refresh' content='0; url=/m/?" + list + "'>");
		}
		else
		{
				//location.href = "/m/"; //모바일 페이지 경로(네이버를 경유하지 않았을때)
				document.write("<meta http-equiv='refresh' content='0; url=/m/'>");
