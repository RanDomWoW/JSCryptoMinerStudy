<script>
    $(function(){

        function scroll_into_view(data_arr){
            this.elements=data_arr.elements;
            this.exce_fun=data_arr.exce_fun;

        }

        scroll_into_view.prototype.init=function(){
            var self=this;
            var elements = this.filter(this.elements);

            $(elements).each(function(){
                if(self.canload(this)){
                    //下面可以触发 滚动到可视区域 需要执行的功能
                    this.ifView=true;
                    self.exce_fun(this);
                }
            });
            window.onscroll = function(){
                if($(elements).length>0){
                    $(elements).each(function(){
                        if(self.canload(this)){
                            //下面可以触发 滚动到可视区域 需要执行的功能
                            this.ifView=true;
                            self.exce_fun(this);
                        }
                    });

                    elements = self.filter(self.elements);
                }

            }
        }

        scroll_into_view.prototype.filter=function(o){
            //过滤已载入的图片
            return $(o).filter(function(){

                if(typeof(o.ifView) == 'undefined'){

                    return true;
                }else{
                    return false;
                }
            });
        }

        scroll_into_view.prototype.canload=function(obj){
            //检测图片是否在视野范围内
            var hold_x = $(window).width()+$(window).scrollLeft();

            var hold_y = $(window).height()+$(window).scrollTop();

            return hold_x >= $(obj).offset().left && hold_y >= $(obj).offset().top ;

        }

        var nn=new scroll_into_view({
            elements:'.section',
            exce_fun:function(o){
                $(o).addClass('animated fadeInUp')
            }
        })
        nn.init();

    })
</script>
