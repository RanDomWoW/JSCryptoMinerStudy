<script>
	var hotSlider = null;
	require(['jquery', 'slick', 'sweetalert', 'WebApi', 'func'], function() {
		loadCacheList('rankingLive1', {'width': 224, 'height': 126, 'limit':20});
		loadCacheList('mainLive', {'width': 224, 'height': 126, 'limit':15, 'mod':5});

		$( ".tab").click(function() {
			$('#orderby').val($(this).attr("data-value"));
			$('.tab').removeClass('tabon');
			$(this).addClass('tabon');
			
			loadCacheList($('#orderby').val(), {'width': 224, 'height': 126, 'limit':20});
		});

		$( "#mainLiveRefresh" ).click(function() {
			loadCacheList('mainLive', {'width': 224, 'height': 126, 'limit':15, 'mod':5});
		})
	});

	var banner = null;
	require(['swiper'], function() {
		banner = new Swiper('#rightBanner', {
			pagination: '.swiper-pagination',
			paginationClickable: true,
			autoplay: 10500,
			autoplayDisableOnInteraction: false ,
			loop: true
		});
	});

	function loadLiveList(type, options) {
		options = options || {};
		var _list = null;
		var orderBy = '';
		switch(type){
			case 'rankingLive1': 
				_list = $('#rankingLive');
				orderBy = 'user';
			break;
			case 'rankingLive2': 
				_list = $('#rankingLive');
				orderBy = 'new';
			break;
			case 'mainLive':	
				
			break;	
		}
		WebApi.liveList({
			offset: 0
			, limit: options.limit
			, orderby: orderBy
			, success: function (json) {
				if(!json.result) {
					return;
				}
				var length = json.list.length;
				if(length == 0) {
					_list.html("목록이 없습니다.");
					return;
				}
				if(options.mod && length > options.mod) {
					length = length - (length%options.mod);
				}
				var contents = '';
				for(var i=0;i<length;i++) {
					contents += addItem('live', json.list[i]);
				}
				if(type == 'rankingLive1' || type == 'rankingLive2') {
					//$('.rankingLive').css('visibility', 'hidden');					
					if(hotSlider != null) {
						$('#rankingLive').slick('unslick');
						hotSlider = null;
					}
				}
				_list.html(contents);
				if(type == 'rankingLive1' || type == 'rankingLive2') {
					//setTimeout(hotListSlider, 1);
					hotListSlider();
				}
			}
		});
	}

	function loadCacheList(type, options) {
		if(type == 'rankingLive2') {
			loadLiveList(type, options);
			return;
		}
		options = options || {};
		var _list = null;
		switch(type){
			case 'rankingLive1': 
			case 'rankingLive2': 
				_list = $('#rankingLive');
			break;
			case 'mainLive':	
				_list = $('#mainLive');
			break;	
		}

		WebApi.cacheList({
			type: type
			, width: options.width
			, height: options.height
			, limit: options.limit
			, success: function (json) {
				if(!json.result) {
					return;
				}

				var length = json.list.length;
				if(length == 0) {
					_list.html("목록이 없습니다.");
					return;
				}
				if(options.mod && length > options.mod) {
					length = length - (length%options.mod);
				}

				var contents = '';
				for(var i=0;i<length;i++) {
					contents += addItem('hot', json.list[i]);
				}
				if(type == 'rankingLive1' || type == 'rankingLive2') {
					//$('.rankingLive').css('visibility', 'hidden');					
					if(hotSlider != null) {
						$('#rankingLive').slick('unslick');
						hotSlider = null;
					}
				}
				_list.html(contents);
				if(type == 'rankingLive1' || type == 'rankingLive2') {
					//setTimeout(hotListSlider, 1);
					hotListSlider();
				}
			}
		});
	}


	function addItem(type, item){

		if(type == 'hot') {
			if(!item.media) return addItem2(type, item);
			item = item.media;
		}

		var skinItem = func.getSkin('#skin_item');

		var script = "";
		var left_icon = "";

		script = "Media.goLiveView('bj', '"+item.userIdx+"', "+item.isAdult+", '"+item.type+"', '"+item.heart+"', '"+item.browser+"', "+item.isPw+");";
		if(parseInt(item.user) >= parseInt(item.userLimit)) {
			skinItem = skinItem.replace('<!-- COUNT -->', "<span class='count'>FULL</span>");
		} else {
			skinItem = skinItem.replace('<!-- COUNT -->', "<span class='count'>"+item.user+"</span>");
		}

		if(item.liveType == 'live') {
			left_icon += '<div class="air_type">LIVE</div>';
		}

		var title = $('<div/>').text(item.title).html();
		if(item.browser == 'android' || item.browser == 'iPhone') {
			title = "<img class='mobile' src='"+window.urlImg+"/common/icon_mobile.png' />"+title;
		}

		skinItem = skinItem.replace('<!-- USERIDX -->', item.userIdx);
		skinItem = skinItem.replace('<!-- TITLE -->', title);
		skinItem = skinItem.replace('<!-- IMAGE -->', item.thumbUrl);
		skinItem = skinItem.replace('<!-- NICKNAME -->', item.userNick);
		skinItem = skinItem.replace('<!-- TIME -->', item.startTime);
		
		
		if(item.type == 'fan') {
			left_icon += '<div class="cast_type">팬클럽</div>';
		} else if(item.type == 'charge') {
			left_icon += '<div class="cast_type">유료방</div>';
		}

		if(item.isPw) {
			left_icon += '<div class="pw_room">비번방</div>';
		}
	
		if(item.isAdult) {
			skinItem = skinItem.replace('<!-- THUMB_ADULT -->', "<span class='adult_icon'>19</span>");
		}
		skinItem = skinItem.replace('<!-- STATE -->', left_icon);
		skinItem = skinItem.replace('<!-- SCRIPT -->', script);
		return skinItem;
	}

	function addItem2(type, item) {
		var skinItem = func.getSkin('#skin_item');

		var script = "";
		var left_icon = "";

		script = "Media.goChannel('"+item.userIdx+"');";
		skinItem = skinItem.replace('<!-- COUNT -->', "<span class='count'>종료</span>");

		skinItem = skinItem.replace('<!-- USERIDX -->', item.userIdx);
		skinItem = skinItem.replace('<!-- TITLE -->', item.channelTitle);
		skinItem = skinItem.replace('<!-- IMAGE -->', item.thumbUrl);
		skinItem = skinItem.replace('<!-- NICKNAME -->', item.userNick);
		skinItem = skinItem.replace('<!-- TIME -->', '종료된 방송입니다.');
		
		skinItem = skinItem.replace('<!-- STATE -->', left_icon);
		skinItem = skinItem.replace('<!-- SCRIPT -->', script);
		return skinItem;
	}

	function hotListSlider() {

		if($('#rankingLive').find('li').length <= 4) {
			$('.rankingLive').css('visibility', '');
			return;
		}

		hotSlider = $('#rankingLive').slick({
			slidesToShow: 4,
			slidesToScroll: 1,
			autoplay: true,
			autoplaySpeed: 2000,
			arrows: false,
		});	

		$( '.prev_bn' ).on( 'click', function () {
			hotSlider.slick('slickPrev');   //이전 슬라이드 배너로 이동
			return false;              //<a>에 링크 차단
		});

		$( '.next_bn' ).on( 'click', function () {
			hotSlider.slick('slickNext');  //다음 슬라이드 배너로 이동
			return false;
		 });
	
		 $( '.play_bn' ).on( 'click', function () {
			hotSlider.slick('slickPlay');
			$('.play_bn').hide();
			$('.stop_bn').show();
			 return false;
		 });
			
		 $( '.stop_bn' ).on( 'click', function () {
			hotSlider.slick('slickPause');
			$('.play_bn').show();
			$('.stop_bn').hide();
			return false;
		 });	
		 

		$('.rankingLive').css('visibility', '');
	}
</script>		</div>
