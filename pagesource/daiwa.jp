<script><!--

// マーケット動画キャッシュ対策
$(function(){
var tvDate = new Date();
var tvUrl = "https://89ea29602173b9d9f6f81fa884072a38.cdnext.stream.ne.jp/contents/daiwajp/index_pc_top.html?rnd_num=" + tvDate.getTime();
$("#iframe_marketTv").attr("src",tvUrl);

/* tvFixed */
	//TV用LIVE配信パネル
	var tvObj = $("#tvFixed");
	/* SP TV用LIVE配信パネルフェードアウト処理 */
	function tvFixedFade(){
		if($(window).scrollTop() > 300 && $(window).width() < 768)tvObj.fadeOut();
	}
	$("#tvClose").click(function() {
		var rightPosition = $("#tvFixed").css("right");
		var tvObjWidth = $("#tvFixed").outerWidth() * -1;
		if (tvObj.hasClass("on")) {
			tvObj.animate({
				right: rightPosition
			}, 500);
			tvObj.removeClass("on");
		} else {
			tvObj.animate({
				right: tvObjWidth
			}, 500);
			tvObj.addClass("on");
		}
	});
	$(window).on('scroll',tvFixedFade);
	var tvDomain = "http://www.daiwatv.jp";
	var dataUrl = tvDomain + "/contents/external/meta/retail/retail_gadgetMeta.jsonp" + "?rnd_num=" + tvDate.getTime();
	var dataMovie = "";
	//ajax
	$.ajax({
		cache: false,
		type: "GET",
		scriptCharset: "utf-8",
		dataType:"jsonp",
		jsonpCallback: "retail_gadgetMeta",
		crossDomain : true,
		url: dataUrl,
		success: function(data) {
			dataMovie = data;
		},
		error: function(data) {
			return false;
		},
		complete: function(data) {
			if(dataMovie.market_list.length){
				var onairKeyObj = "";
				// undefined 定義
				var undefined;
				// 表示する動画選択
				keyLoop:for (var key in dataMovie) {
					// YYYY/mm/dd H:M:S
					var dateLocal = new Date($("#dateLocal").html());
					if (key == "market_list") {
						for (var i = 0; i < dataMovie[key].length; i++){
							var openTime = new Date(dataMovie[key][i].retail_time.open_date + " " + dataMovie[key][i].retail_time.open_time);
							var closeTime = new Date(dataMovie[key][i].retail_time.close_date + " " + dataMovie[key][i].retail_time.close_time);
							if (dateLocal > openTime && dateLocal < closeTime && dataMovie[key][i].onair == 1) {
								onairKeyObj = dataMovie[key][i];
								break keyLoop;
							}
						}
					} else {
						// createdDateの除外
						if (dataMovie[key].retail_time !== undefined) {
							var openTime = new Date(dataMovie[key].retail_time.open_date + " " + dataMovie[key].retail_time.open_time);
							var closeTime = new Date(dataMovie[key].retail_time.close_date + " " + dataMovie[key].retail_time.close_time);
							if (dateLocal >= openTime && dateLocal <= closeTime) {
								onairKeyObj = dataMovie[key];
								break keyLoop;
							}
						}
					}
				}
				//表示
				if (onairKeyObj != "") {
					var rand = Math.floor(Math.random() * 3);
					var extension = onairKeyObj.thumbnail_list_pc[rand].split("/").pop().split(".").pop().toLowerCase();
					var fileName = onairKeyObj.thumbnail_list_pc[rand].split("/").pop().split(".").shift();
					tvObj.find("a").attr("href", onairKeyObj.link_url + "?rtop=" + onairKeyObj.id + "&thumb=" + fileName);
					tvObj.find(".tvCaption").html(onairKeyObj.caption);
					if (extension == "gif" || extension == "png" || extension == "jpg" || extension == "jpeg") {
						tvObj.find(".tvImg").attr("src", tvDomain + onairKeyObj.thumbnail_list_pc[rand]).show();
					} else {
						tvObj.find(".tvIframe").attr("src", onairKeyObj.thumbnail_list_pc[rand]).show();
						tvObj.addClass("movieStyle");
					}
					tvObj.find(".tvContentsTitle span").html(onairKeyObj.contentsTitle);
					tvObj.find(".tvDescription").html(onairKeyObj.description);
					tvObj.show();
				}
			}
		}
	});

});
// --></script>
