	// ヘッダーメニュー
	function docClickEvent(c,bc){
		$(document).on('click',function(e){
			if($(e.target).hasClass(c)){
				$('body').removeClass(bc);
				$(document).off('click');
			}
		});
	}
	if($('#mainMenu') && $('#mainMenu2')){
