<script>
	/* スライダーの処理
	------------------------------------------------------------------------------*/
	var _flg;

	$(window).on('load',function(){
		_flg = $(window).width()>=768;

		_uaInit();
		var mv_a = $('.bxslider').bxSlider({
			mode: 'horizontal',
			moveSlides: 1,
			infiniteLoop: true,
			slideWidth: 0,
			auto: true,
			speed: 800,
		});

		var _bxOpt = _flg ? {
			mode: 'horizontal',
			moveSlides : 1,
			minSlides : 3,
			slideMargin : 15,
			infiniteLoop : true,
			speed : 200,
			auto : true,
			pager : false,
			controls : false,
		}:
		{
			mode: 'horizontal',
			minSlides : 3,
			maxSlides : 3,
			slideMargin : 10,
			infiniteLoop : true,
			speed : 200,
			auto : true,
			pager : false,
			controls : false,
			onSliderLoad : function(){
				var _m = $(window).width()/2 - 332.5;
				if(_m < 0)$('.bnr-slider').css('margin-left',_m);
			},
		};
		var a = $('.bnr-slider').bxSlider(_bxOpt);
		if(_flg)a.stopAuto();
		$(window).on('resize',function(){
			var isSp = $(window).width()<768;
			if(isSp){
				var _m = $(window).width()/2 - 332.5;
				if(_m < 0)$('.bnr-slider').css('margin-left',_m);
			}else{
				$('.bnr-slider').css('margin-left',0);
			}
			if(_flg == true && isSp){
				_flg = false;
				$('.productsgrid').attr('data-col',2);
				$('.recomgrid .grid').attr('data-col',1);

				mv_a.reloadSlider({
					mode: 'horizontal',
					moveSlides: 1,
					infiniteLoop: true,
					slideWidth: 0,
					auto: true,
					speed: 800,
				});

				a.startAuto();
				_runUaTargetEvents();
			}else if(_flg == false && !isSp){

				if($('#newyearBanner').css('display')=='block'){
					setTimeout(bannerFirstView,4000);
				}

				_flg = true;
				$('.productsgrid').attr('data-col',3);
				$('.recomgrid .grid').attr('data-col',2);

				a.stopAuto();
				_runUaTargetEvents();
			}

		});
	});

	/* uaによるリンク先切り替え処理
	------------------------------------------------------------------------------*/
	var _uaTarget = {
		// 口座開設
		'account':{ 'pc' : '/open/', 'sp' : '/sp/open/'},
		// 口座開設方法
		'howto':{ 'pc' : '/service/account/', 'sp' : '/sp/service/account/' },
		// ダイワの特長
		'characteristic' : { 'pc' : '/service/point/', 'sp' : '/sp/service/point/' },
		// サービス一覧
		'servicemenu' : { 'pc' : '/service/list/', 'sp' : '/sp/service/list/' },
		// 商品一覧はこちら
		'products' : { 'pc' : '/products/', 'sp' : '/sp/products/' },
		// 投資信託
		'finance' : { 'pc' : '/products/fund/', 'sp' : '/sp/products/fund/' },
		// ＩＰＯ（新規公開株式）
		'openipo' : { 'pc' : '/products/equity/ipo/list.html', 'sp' : '/sp/products/equity/ipo/list.html' },
		// ＰＯ（公募・売出株式）
		'existipo' : { 'pc' : '/products/equity/po/list.html', 'sp' : '/sp/products/equity/po/list.html' },
		// 常設バナーキャンペーン一覧
		'bnrcampaign' : { 'pc' : '/campaign/', 'sp' : '/sp/campaign/' },
		// ダイワプレミアムサービス
		'daiwapremium' : { 'pc' : '/service/premium/', 'sp' : '/sp/service/premium/' },
		// 動画ラインナップ更新時間はこちら
		'marketmovie' : { 'pc' : '/market/', 'sp' : '/sp/market/#mk_mov' },
		// その他のマーケットレポートはこちら
		'marketreport' : { 'pc' : '/market/#mk_report', 'sp' : '/sp/market/#mk_report' },
		// その他のマーケットレポートはこちら
		'marketinfo' : { 'pc' : '/market/', 'sp' : '/sp/market/' },
		// 指数一覧
		'marketlist' : { 'pc' : '/market/worldmap/list.html', 'sp' : '/sp/market/list/' },
		// 為替
		'market' : { 'pc' : '/market/worldmap/?ind=4&type=3', 'sp' : '/sp/market/list/fx.html' },
		// nisa
		// 'nisabanner' : { 'pc' : '/nisa/', 'sp' : '/smartphone/nisa/index.html' },
		// つみたてnisa
		'tsuminisa' : { 'pc' : '/nisa/tsumi_nisa/', 'sp' : '/smartphone/nisa/tsumi_nisa.html' },
		// 日経平均など
		'jaave' : { 'pc' : '/market/chart/?ric=.N225', 'sp' : '/sp/market/chart/?ric=.N225'},
		'nydow' : { 'pc' : '/market/chart/?ric=.DJI', 'sp' : '/sp/market/chart/?ric=.DJI'},
		'usd' : { 'pc' : '/market/chart/?ric=JPY=X', 'sp' : '/sp/market/chart/?ric=JPY=X'},
		'eur' : { 'pc' : '/market/chart/?ric=EURJPY=X', 'sp' : '/sp/market/chart/?ric=EURJPY=X'}
	}
	var _flg;
	function _uaInit(){
		_flg = window.matchMedia( '(min-width: 768px)' ).matches;

		var _al = $('.js-carousel-top a,.bxslider-area a');

		_setTargetURL();
	};

	function _runUaTargetEvents(){
		_setTargetURL();
	}

	function _setTargetURL(){
		$.each(_uaTarget,function(i,v){
			var _t = '.ua-target-'+i;
			var _val = _getUrl(v);

			$(_t).attr('href',_val);
		});
	}
	function _getUrl(v){
		return window.matchMedia( '(min-width: 768px)' ).matches?
		v['pc']:v['sp'];
	}
</script>
