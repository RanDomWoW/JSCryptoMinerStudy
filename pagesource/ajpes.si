    <script>
        $("#id_tag1, #id_tag2, #id_tag3").change(function () {
            if ($(this).val() == '-1') {
                $(this).val('');

            }
            var url;
            url = "/MDScripts/ajax.asp?method=getTiles"

            if ($("#id_tag1").val() != '' && $("#id_tag1").val() != '-1') url = url + "&id_tag1=" + $("#id_tag1").val();
            if ($("#id_tag2").val() != '' && $("#id_tag2").val() != '-1') url = url + "&id_tag2=" + $("#id_tag2").val();
            if ($("#id_tag3").val() != '' && $("#id_tag3").val() != '-1') url = url + "&id_tag3=" + $("#id_tag3").val();

            $("#tiles_reload").removeClass("in");

            $.ajax({
                method: "GET",
                url: url
            })
            .done(function (html) {
                $("#tiles_reload").html(html);
                $("#tiles_reload").addClass("in");
                $(".matchHeight").matchHeight();

                $(".tileClick").click(function (e) {
                    pinTilesClick(this);
                });

                $(".ico-pin").click(function (e) {
                    e.preventDefault();
                    pinTiles(this);
                });

                $(function () {
                    $('[data-toggle="tooltip"]').tooltip({ trigger: 'hover' })
                })

            });
        });

    </script>

    
