<script type="text/javascript">
if (typeof TalentumAdHooks === 'undefined') {
TalentumAdHooks = {
adLoaded: function (placementElement) {},
adPlacementEmpty: function (placementElement) {},
adPlacementPending: function (placementElement) {}
};
}
function talentumOnAdtechComplete (placementElement) {
var minimumAdHeight = 50;
var height = placementElement.height();
if (height >= minimumAdHeight) {
TalentumAdHooks.adLoaded(placementElement);
return;
}
var descendants = placementElement.find('*').not("script");
for (var i = 0; i < descendants.length; i++) {
height = $(descendants[i]).height();
if (height >= minimumAdHeight) {
TalentumAdHooks.adLoaded(placementElement);
return;
}
}
var img = placementElement.find('img');
if (img.length) {
var src = img.attr('src');
if (src && src.indexOf('adtech.de/images/Default_Size_16_1x1.gif') >= 0) {
TalentumAdHooks.adPlacementPending(placementElement);
return;
}
var allAreCompleteAndTiny = true;
for (var i = 0; i < img.length; i++) {
if (!img[i].complete || img[i].naturalHeight >= minimumAdHeight) {
allAreCompleteAndTiny = false;
break;
}
}
if (allAreCompleteAndTiny) {
TalentumAdHooks.adPlacementEmpty(placementElement);
checkAdditionalAd(placementElement);
return;
}
}
else if (placementElement.find("script[src*='widespace']").length) {
if (placementElement.find('.wsNoAd').length) {
TalentumAdHooks.adPlacementEmpty(placementElement);
checkAdditionalAd(placementElement);
return;
}
}
else if (height == 0 && !placementElement.find("canvas,div,embed,figure,iframe,object").length) {
TalentumAdHooks.adPlacementEmpty(placementElement);
checkAdditionalAd(placementElement);
return;
}
}
function talentumEnqueueAd (placementElementId, placementId, boundsMin, boundsMax) {
var placementElement = $('#' + placementElementId);
if (placementElement.length) {
if (typeof ADTECH === 'undefined') {
$(function() {
TalentumAdHooks.adPlacementEmpty(placementElement);
});
}
else {
ADTECH.config.placements[placementElementId] = {
responsive : {
useresponsive: true, bounds: [{
id: placementId,
min: boundsMin,
max: boundsMax
}]
},
params: {
target: '_blank',
},
complete: function() {
setTimeout(function(){
talentumOnAdtechComplete(placementElement);
}, 500);
}
};
ADTECH.enqueueAd(placementElementId);
}
}
}
</script>
<!-- W-END:[6308710] -->

