    function showMacInstallButtonIf() {
      if (SpoonCommon.isMacDesktop()) {
        document.getElementById('mac-install-btn').style.display = 'inline-block';
      } else {
        document.getElementById('mac-install-btn').style.display = 'none';
      }
    }

  </script>
