  function loadFallbackIconsIf() {
    var $icons = $('i[data-icon-fallback]')

    $icons.each(function (i, icon) {
      var $this = $(icon)
      var src = $this.attr('data-icon-fallback')

      if (src.indexOf(".svg") >= 0) {
        // Load svg into <object> tag and cover it with an invisible div.
        // Click events will not propagate out of the object context, so we need to block them from entering.
        $this.replaceWith('<div class="icon-fallback-wrap"><object class="icon-fallback" type="image/svg+xml" data="' + src + '"/><div class="icon-fallback-mask"></div></div>')
      } else {
        $this.replaceWith('<img class="icon-fallback" src="' + src + '"/>')
      }
    })
