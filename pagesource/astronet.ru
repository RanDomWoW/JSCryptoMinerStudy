<script type="text/javascript" charset="koi8-r">
/*
	initOnLoad получает в качестве аргумента строку - id элемента, который требуется  
	идентифицировать, не дожидаясь полной загрузки документа. 
	Также можно указать "body" - в этом случае будет проверяться состояние объекта document.body. 
*/

function initOnLoad(sElementName) {  
	var oElement = (sElementName == "body") ? document[sElementName] : 
		document.getElementById(sElementName); 
	if(oElement != null && typeof(oElement) != "undefined") { runningMessage(); } 
	else { setTimeout(function() { initOnLoad(sElementName); }, 0); } 
} 

/*
	runningMessage - тестовая функция, которая начнет выполняться, не дожидаясь 
	окончательной загрузки страницы. 
*/

function runningMessage() { 
	var txt = "", 
	message = 
		"Начался конкурс \"Astronet-2007\";" +
		"на лучшую студенческую работу по Астрономии;" +
		"Конкурс приурочен к 175-летнему юбилею ГАИШ;" + 
		"Сроки подачи работ - до 1 марта 2007 года;" +
		"Работы принимаются в электронном виде на русском языке;" +
		"Приглашаем Вас принять участие в конкурсе!", 
	nxt = -1, 
	mark = "", 
	div = document.getElementById("message"), 
	input = div.getElementsByTagName("input")[0],  

	startRunning = function() {
		nxt++;
		if(message.charAt(nxt) != ";") {
  			if(mark == "") { mark = "_"; }
  			else if(mark == "_") { mark = ""; }
 			txt += message.charAt(nxt); 
			input.value = txt + mark;
  			delay = 40; 
		}
		else {
  			txt = "";
  			delay = 5000;
  			if(nxt == message.length - 1) { nxt = -1; } 
			if(input.value.indexOf("_") != -1) { 
				input.value = input.value.substr(0, input.value.length - 1); 
			} 
		}
		setTimeout(startRunning, delay); 
	} 
	startRunning(); 
} 

/*	Запускаем функцию initOnLoad, передав ей id нужного элемента. */

initOnLoad("message"); 
</script>

