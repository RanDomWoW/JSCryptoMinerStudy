      document.cookie = "emailAddress=" + $(this).val() + ";expires=" + myDate + ";domain=.upcity.com;path=/";
      document.cookie = "gaEventName=Contact Us" + ";expires=" + myDate + ";domain=.upcity.com;path=/";
    });

    var userId = null;
    if (document.cookie.indexOf("gaUserId") >= 0) {
      userId = document.cookie.split("; gaUserId=").pop().split(";").shift();
      dataLayer.push({'gaUserId': userId});
      dataLayer.push({'event':'dataLayerLoaded'});
    } else {
      if (document.cookie.indexOf("emailAddress") >= 0) {
        var emailAddress = document.cookie.split("; emailAddress=").pop().split(";").shift();
        var eventName = null;
        if (document.cookie.indexOf("gaEventName") >= 0) {
          eventName = document.cookie.split("; gaEventName=").pop().split(";").shift();
        }
        $.getJSON("https://app.upcity.com/api/contact/ga_token.json", { email: emailAddress, event_name: eventName }, function (data) {
          if (data.token != null) {
            var myDate = new Date();
            myDate.setMonth(myDate.getMonth() + 12);
            document.cookie = "gaUserId=" + data.token + ";expires=" + myDate + ";domain=.upcity.com;path=/";
            dataLayer.push({'gaUserId': data.token});
          }
          dataLayer.push({'event':'dataLayerLoaded'});
        });
      } else {
        dataLayer.push({'event':'dataLayerLoaded'});
      };
    };
  });
