                    liTmp = liTmp.replace(/\*\*/g,i > 2 ? 'cmn' : i);
                    liTmp = liTmp.replace(/\#\#/g,i + 1);
                    liTmp = liTmp.replace(/\%\%/g,item);
                    liTmp = liTmp.replace(/\^\^/g,qFlat + item.replace(/\s+/g,'+'));
                    ulCtnt += liTmp;
                }
            }
            $("#film").html(ulCtnt);
        }

