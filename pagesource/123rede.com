            var url = "./rss/music.json";
            $.getJSON(url,function(data){
                musicCb(data);
            });
        }

        function setFilm(){
            var url = "./rss/film.json";
            $.getJSON(url,function(data){
                filmCb(data);
            });
        }

        $(function(){
            setMusic();
            setFilm();
        });
