    cli_on.onclick = function(){
        if(flag){
            l_len = -290;
            timer = setInterval(slideleft,10);
        }else{
            l_len = 0;
            timer = setInterval(slideright,10);
        }   
    }
    function slideright(){
        if(l_len <= -290 ){
            clearInterval(timer);
            flag = !flag;
            return false;
        }else{
            l_len -= 10;
            combox.style.right = l_len +'px';
        }
    }
    function slideleft(){
        if(l_len >= 0 ){
            clearInterval(timer);
            flag = !flag;
            return false;
        }else{
            l_len += 10;
            combox.style.right = l_len +'px';
        }
    }

function getajaxHttp(){
    var xmlHttp;
    if(window.XMLHttpRequest){
        // code for IE7+, Firefox, Chrome, Opera, Safari
        xmlHttp = new XMLHttpRequest(); 
    }else{
        // code for IE6, IE5
        xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
    }
    return xmlHttp;
}

function ajaxrequest(url,methodtype,con,parameter,functionName,obj){
    var xmlhttp = getajaxHttp();
    xmlhttp.onreadystatechange = function(){
        if(xmlhttp.readyState == 4) {
            functionName(xmlhttp,obj);  
        }
    }
    xmlhttp.open(methodtype,url,con);
    xmlhttp.setRequestHeader("Content-type","application/x-www/form-urlencoded");
    var feedback = document.getElementById('feedback').value;
    var email = document.getElementById('email').value;
    xmlhttp.send("feedback="+feedback+"&email="+email+"&lg="+lg+"&url=123rede");
}

function callback(xmlhttp,obj){
    if(xmlhttp.responseText == '1'){
        document.getElementById("feedback_msg").innerHTML = "operação foi bem sucedida";
    }else{
        document.getElementById("feedback_msg").innerHTML = "<font color='red'>operação falhar</font>"; 
    }
}

var l2_len = 0;
function slideleft2(){
    var combox = document.getElementById("common_box");
    if(l2_len <= -290){
        clearInterval(waiter);  
        document.getElementById("send").value = "Enviar";
        document.getElementById("send").disabled = false;
        document.getElementById("feedback_msg").innerHTML = "";
        document.getElementById("feedback").value = "";
        document.getElementById("email").value = "";
    }else{
        l2_len -= 10;
        combox.style.right = l2_len + 'px';
    }
}

function jsonpHandle(a){
    if(a=="1"){
        document.getElementById("feedback_msg").innerHTML = "operação foi bem sucedida";
    }else{
        document.getElementById("feedback_msg").innerHTML = "<font color='red'>operação falhar</font>";
    }
    setTimeout("fb_clear()",2000);
}

function fb_clear(){
    waiter = window.setInterval("slideleft2()",10); 
}

function dosubmit(){
    document.getElementById('feedback_msg').innerHTML = ''; 
    var feedback = document.getElementById('feedback').value;
    if(feedback == ''){
        document.getElementById('feedback_msg').innerHTML = "<font color='red'>O conteúdo é bırakılamazo vazio</font>";
        document.getElementById('feedback').focus();
        return false;
    }
    var email = document.getElementById('email').value;
    var patrn = /^\s*\w+(?:\.{0,1}[\w-]+)*@[a-zA-Z0-9]+(?:[-.][a-zA-Z0-9]+)*\.[a-zA-Z]+\s*$/;
    if(!patrn.exec(email)){
        document.getElementById('feedback_msg').innerHTML = "<font color='red'>e-mail errado</font>";
        document.getElementById('email').focus();
        return false;
    }
    var JSONP = document.createElement("script");
    JSONP.type ="text/javascript";
    JSONP.src = "http://admin.123rede.com:8001/feedback/feedback.php?feedback="+feedback+"&email="+email+"&lg="+lg+"&url=123rede"+"&callback=jsonpHandle";
    document.getElementsByTagName("head")[0].appendChild(JSONP);
    document.getElementById("send").value = "Enviar...";
    document.getElementById("send").disabled = true;
    l2_len = 0;
}
