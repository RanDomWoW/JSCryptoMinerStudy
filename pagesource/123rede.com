<script type="text/javascript">
function GetCurrentTime() {
	var currentTime = "";
	var myweekday=""; 
	var myDate = new Date();
	var year = myDate.getYear();
	var myweekday=myDate.getDay();
	var month = parseInt(myDate.getMonth().toString()) + 1; //month是从0开始计数的，因此要 + 1
	year=(year > 200) ? year : 1900 + year; 
	if (month < 10) {
		month = "0" + month.toString();
	}
	var date = myDate.getDate();
	if (date < 10) {
		date = "0" + date.toString();
	}
	var hour = myDate.getHours();
	if (hour < 10) {
		hour = "0" + hour.toString();
	}
	var minute = myDate.getMinutes();
	if (minute < 10) {
		minute = "0" + minute.toString();
	}
	var second = myDate.getSeconds();
	if (second < 10) {
		second = "0" + second.toString();
	}
	
	if(myweekday == 0) weekday="  "; 
	else if(myweekday == 1) weekday="  "; 
	else if(myweekday == 2) weekday="  "; 
	else if(myweekday == 3) weekday="  "; 
	else if(myweekday == 4) weekday="  "; 
	else if(myweekday == 5) weekday="  "; 
	else if(myweekday == 6) weekday="  ";
	
	currentTime = year.toString() + "/" + month.toString() + "/" + date.toString() + " " + weekday + " " + hour.toString() + ":" + minute.toString() ; //以时间格式返回
	//document.getElementById("clock").innerHTML = currentTime;
    $("#clock").html(currentTime);
	window.setTimeout("GetCurrentTime()",100);
}
</script>
