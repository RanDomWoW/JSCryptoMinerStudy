        function addDefaultGeoSubsidiary(mapId, data) {

            var d = new Date();
            var weekday = new Array(7);
            weekday[0] = "SUN_";
            weekday[1] = "MON_";
            weekday[2] = "TUE_";
            weekday[3] = "WED_";
            weekday[4] = "THU_";
            weekday[5] = "FRI_";
            weekday[6] = "SAT_";
            var n = weekday[d.getDay()];


            var markers = [];
            var infowindows = [];

            var subsidiaryDescription = A.one('#subsidiaryDescription');
            var fullAddress = A.one('#fullAddress');
            var distance = A.one('#distance');
            var phone = A.one('#phone');
            var appuntamentoLink =$('#appuntamentoLink');
            var openingTime =A.one('#openingTime');

            for (var i = 0; i < data.length; i++) {
                var options = {
                    zoom : 17,
                    center : {
                        lat: parseFloat(data[i].lat),
                        lng : parseFloat(data[i].lng)
                    },
                    mapTypeId : google.maps.MapTypeId.ROADMAP
                };

                var map = new google.maps.Map(mapId, options);

                
                var url ='/o/bper-theme/images/subsidiary/icn-pointer-green-dark.png';
                var imageIcon = {
                    url : url,
                    size : new google.maps.Size(26, 35), // scaled size
                    origin : new google.maps.Point(0, 0), // origin
                    anchor : new google.maps.Point(13, 35)
                };
                var ot =data[i].openTime;
                var keyOpeningTimeAM=n+"AM";
                var keyOpeningTimePM=n+"PM";

                subsidiaryDescription.setHTML(data[i].name);

                fullAddress.setHTML(data[i].address);
                if (data[i].distanceDuration!='NA') {
                var directionUrl = 'https://www.google.com/maps/dir/' + data[i].mylat + ',' + data[i].mylng + '/' + data[i].lat + ',' + data[i].lng;
                distance.set("href", directionUrl);
                distance.set("target", '_blank');
                distance.setHTML(data[i].distanceDuration);
                } else
                     distance.setHTML("Indicazioni non disponibili");
                phone.setHTML(data[i].phoneNumber);
                appuntamentoLink.attr('data-param','__subsidiaryId:'+data[i].agencyId);
                appuntamentoLink.attr('data-filiale',data[i].agencyId);
                appuntamentoLink.attr('data-href',data[i].appuntamentoUrl);
                var openingTimeLabel='Orario odierno : ';
                openingTime.setHTML(openingTimeLabel + '<strong>' +ot[keyOpeningTimeAM] + ' / ' + ot[keyOpeningTimePM] +'</strong>');

                var htmlContent ='<div class="filiale-box__infowindow"><h1 class="filiale-box__infowindow__description">'+data[i].name+'</h1>'+
                '<div class="filiale-box__label"><span class="filiale-box__icon--full-address"></span><p>'+data[i].address+'</p></div>';
                if (data[i].distanceDuration!=undefined && data[i].distanceDuration!='NA') {
                    var directionUrl = 'https://www.google.com/maps/dir/' + data[i].mylat + ',' + data[i].mylng + '/' + data[i].lat + ',' + data[i].lng;
                    htmlContent=htmlContent+'<div class="filiale-box__label"><span class="filiale-box__icon--distance"></span><a href="'+directionUrl+'" target="_blank">'+data[i].distanceDuration+'</a></div>';
                } else
                    htmlContent=htmlContent+'<div class="filiale-box__label"><span class="filiale-box__icon--distance"></span>Indicazioni non disponibili</div>';
                htmlContent=htmlContent+'<div class="filiale-box__label"><span class="filiale-box__icon--phone"></span><p>'+data[i].phoneNumber+'</p></div></div>'+
                '<div class="subsidiary-url"><a class="link-cta link-cta-icon" href="' + data[i].detailsURL + '" >Dettaglio </a></div>';


                var marker = new google.maps.Marker({
                    position : new google.maps.LatLng(
                        parseFloat(data[i].lat),
                        parseFloat(data[i].lng)
                    ),
                    map : map,
                    icon : imageIcon,
                    clickable : true
                });
                var infowindow = new google.maps.InfoWindow();

                google.maps.event.addListener(marker, 'click', (function(marker,htmlContent, infowindow) {
                    return function() {
                        closeInfoWindows();
                        infowindow.setContent(htmlContent);
                        infowindow.open(map, marker);
                        infowindows.push(infowindow);
                    };
                })(marker, htmlContent, infowindow));

                google.maps.event.addListener(map, 'click', (function(marker,infowindow) {
                    return function() {
                        infowindow.close(map, marker);
                    };
                })(marker, infowindow));

                function closeInfoWindows() {
                    for (var k = 0; k < infowindows.length; k++) {
                    infowindows[k].close();
                    }
                }

            }
        }


        function addDefaultSubsidiaryByIdentification(mapId,identification) {

            A.io.request('https://www.bper.it/home?p_p_id=it_smc_bper_defaultsubsidiary_portlet_BperDefaultSubsidiaryPortlet_INSTANCE_H9xjQfVOu6Ut&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_cacheability=cacheLevelPage', {
                method : 'post',
                data : {
                    "_it_smc_bper_defaultsubsidiary_portlet_BperDefaultSubsidiaryPortlet_INSTANCE_H9xjQfVOu6Ut_cmd" : 'getSubsidiaryByIdentification',
                    "_it_smc_bper_defaultsubsidiary_portlet_BperDefaultSubsidiaryPortlet_INSTANCE_H9xjQfVOu6Ut_identification" : identification
                },
                on : {
                    success : function() {
                        var data = this.get('responseData');

                        data = JSON.parse(data);

                        if (data.length != 0) {

                            addDefaultGeoSubsidiary(mapId,data,detailsSubsidiary);
                        }else {
                            console.log("No data");
                        }
                    }
                }
            });
           }


        function getAMPM(dateInput) {
            var d = new Date(dateInput);
            var ampm = (d.getHours() >= 12) ? "PM" : "AM";
            return ampm;

        }



        //problem with chrome48: we need to remove class and to add the same class again 'cause it doesn't read correct class
        function refreshCssClass(){
            var g = getChromeVersion();

            if(g<50){
                $(".filiale-box__labels div").removeClass("filiale-box__label");
                setTimeout(function(){
                    $(".filiale-box__labels div").addClass("filiale-box__label");
                },400);
            }
        }

        function getChromeVersion () {
            var raw = navigator.userAgent.match(/Chrom(e|ium)\/([0-9]+)\./);
            return raw ? parseInt(raw[2], 10) : false;
        }
        getChromeVersion()
