<script>
    $(document).ready(function() {
        $.ajaxSetup({ cache: false });

        $.ajax({
            url: '/c/portal/facebook_feed',
            dataType: 'json',
            timeout: 1000,
            error: function(data, status, e) {
                console.log(data,status,e);
                var defaultMessage = "<p>Vicina oltre le attese.</p>";

                var defaultImage = "/documents/33222/160099/vicina-oltre-attese-thumb.jpg/395df65a-5afa-4345-6251-f39332e775db?t=1506531996158";
                var defaultImageTag = '<img src="'+defaultImage+'" />';

                $('#hahj_-fb-feed .fb-feed-message').append(defaultMessage);
                $('#hahj_-fb-feed .fb-feed-media').append(defaultImageTag);

                var t = new Date();

                $('#hahj_-fb-feed .fb-feed-relative-time').append(relativeTime(t));

            },
            success: function (response) {
                try{
                    if (response && !response.error) {
                        /* handle the result */
                        var elem = 0;

                        var fbPost = response.data[elem];
                        var fbPostType = fbPost.type;
                        //enum{link, status, photo, video, offer}
                        var fbPostCreatedTime = fbPost.created_time;
                        var newDate = new Date(fbPostCreatedTime * 1000);
                        formattedDate = newDate.toISOString();

                        var fbPostStatusType = fbPost.status_type;
                        var fbPostMessage = fbPost.message;
                        var fbPostPicture = fbPost.picture;//only for link
                        //photo -> album
                        //photo -> photo
                        var fbPostAttachments = fbPost.attachments.data;
                        var fbPostUrl = fbPostAttachments[0].url;

                        var fbPostMessageTags = '';

                        if (fbPost.message_tags){
                            fbPostMessageTags = fbPost.message_tags.length;
                        }

                        var reg = /https?:\/\/(www\.)?[-a-zA-Z0-9@:%._\+~#=]{2,256}\.[a-z]{2,4}\b([-a-zA-Z0-9@:%_\+.~#?&//=]*)/g;
                        var matches = fbPostMessage.match(reg);
                        if(matches!=null){
                            matches = matches.filter(function(item,pos,self){
                                return self.indexOf(item) == pos;
                            })
                            for (var i = 0; i < matches.length; i++){
                                var match = matches[i];
                                fbPostMessage = fbPostMessage.replace(match, '<a target="_blank" href="' + match + '">' + match + '</a>');
                            }
                        }


                        var fbPostFrom = fbPost.from;

                        var fbPostLink = fbPost.link;
                        var fbPostPermalinkUrl = fbPost.permalink_url;
                        var relativeData = relativeTime(formattedDate);

                        $('#hahj_-fb-feed .fb-feed-relative-time').append(relativeData);
                        $('#hahj_-fb-feed .fb-feed-message').append(fbPostMessage);

                        switch(fbPostType) {
                            case "album":
                                $('#hahj_-fb-feed .fb-feed-media').append('<a href="'+
                                    fbPostPermalinkUrl+'" target="_blank"><div class="album" style="background-image:url('+
                                    fbPostAttachments[0].subattachments.data[0].media.image.src+
                                    ')"></div></a>');
                                break;

                            case "video":
                                $('#hahj_-fb-feed .fb-feed-media').append('<a href="'+
                                        fbPostPermalinkUrl+'" target="_blank"><div class="video" style="background-image:url('+
                                        fbPostAttachments[0].media.image.src+')"><div class="fb-feed-media-video-overlay"></div></div></a>');
                                break;

                            default:
                                if (fbPostAttachments[0].type == "album") {
                                    $('#hahj_-fb-feed .fb-feed-media').append('<a href="'+
                                    fbPostPermalinkUrl+'" target="_blank"><div class="album" style="background-image:url('+
                                    fbPostAttachments[0].subattachments.data[0].media.image.src+
                                    ')"></div></a>');
                                }
                                else{
                                    $('#hahj_-fb-feed .fb-feed-media').append('<a href="'+
                                        fbPostPermalinkUrl+'" target="_blank"><div class="photo" style="background-image:url('+
                                        fbPostAttachments[0].media.image.src+')"></div></a>');
                                }
                        }
                    }
                    else {
                        var defaultMessage = "<p>Vicina oltre le attese.</p>";
                        var defaultImage = "/documents/33222/160099/vicina-oltre-attese-thumb.jpg/395df65a-5afa-4345-6251-f39332e775db?t=1506531996158";
                        var defaultImageTag = '<img src="'+defaultImage+'" />';

                        $('#hahj_-fb-feed .fb-feed-message').append(defaultMessage);
                        $('#hahj_-fb-feed .fb-feed-media').append(defaultImageTag);

                        var t = new Date();

                        $('#hahj_-fb-feed .fb-feed-relative-time').append(relativeTime(t));
                    }
                }catch(er){
                    console.log("errore cacht", er);
                }
            },
            type: 'GET'
        });
    });

    function relativeTime( date ){

        if (isNaN(date.valueOf())) {

		    if (Liferay.Browser.isIe()) {
				var m_array = ["Jan", "Feb", "Mar", "Apr", "May", "Jun",
                   "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];

                var d = date.split("T")[0];
    			var d = d.split("-");

                var t = date.split("T")[1];
    			var t = t.split(":");

    			date = new Date((parseInt(d[0]) + 1), (m_array.indexOf(d[1]) - 1), d[2], t[0], t[1]);
		    }
		    else {
		        date = new Date(date);
		    }
        }
        var date = new Date(date);
        var delta = Math.round((+new Date - date) / 1000);

        var minute = 60,
            hour = minute * 60,
            day = hour * 24,
            week = day * 7;

        var relativeDate;

        if (delta < 30) {
            relativeDate = 'adesso';
        }
        else if (delta < minute) {
            relativeDate = delta + ' secondi fa';
        }
        else if (delta < 2 * minute) {
            relativeDate = 'un minuto fa'
        }
        else if (delta < hour) {
            relativeDate = Math.floor(delta / minute) + ' minuti fa.';
        }
        else if (Math.floor(delta / hour) == 1) {
            relativeDate = '1 ora fa'
        }
        else if (delta < day) {
            relativeDate = Math.floor(delta / hour) + ' ore fa';
        }
        else if (delta < day * 2) {
            relativeDate = 'ieri';
        }
        else if (delta < day * 3) {
            relativeDate = 'tre giorni fa';
        }
        else if (delta < day * 4) {
            relativeDate = 'quattro giorni fa';
        }
        else if (delta < day * 5) {
            relativeDate = 'cinque giorni fa';
        }
        else if (delta < day * 6) {
            relativeDate = 'sei giorni fa';
        }
        else if (delta < day * 7) {
            relativeDate = 'sette giorni fa';
        }
        else {
            relativeDate = date.toLocaleDateString();
        }

        return relativeDate;
    }
</script>
