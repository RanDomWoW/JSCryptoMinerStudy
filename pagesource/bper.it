    function newsHeightADTNews(carousel) {
        var item = $(carousel).find('.item');
        var maxLinkTop = 0;

        $.each(item, function() {
            var link = $(this).find('.link-container');
            var linkTop = link.position().top;
            if (linkTop > maxLinkTop) {
                maxLinkTop = linkTop;
            }
        });

        $.each(item, function() {
            var link = $(this).find('.link-container');
            var linkTop = link.position().top;
            var linkMargin = maxLinkTop - linkTop;
            link.css('padding-top', linkMargin);
        });
    }
