		jQuery('.spotlight .open').show();
		jQuery('.spotlight .close').hide();
		jQuery('.spot_slide_new ul').cycle('pause');
		jQuery('.spotlight .spot_prev').attr("tabindex","0");
		jQuery('.spotlight .spot_next').attr("tabindex","0");
	});
