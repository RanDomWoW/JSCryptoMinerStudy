		jQuery('.spotlight .close').show();
		jQuery('.spotlight .open').hide();
		jQuery('.spotlight .spot_prev').attr("tabindex","0");
		jQuery('.spotlight .spot_next').attr("tabindex","0");
	});
	jQuery('.spotlight .open').focus(function(){
		jQuery('.spotlight').stop().animate({width: '241px'}, 200);
		jQuery('.spotlight .region-spotlight').stop().animate({width: '203px'}, 200);
		jQuery('.spotlight .close').show();
		jQuery('.spotlight .open').hide();
		jQuery('.spotlight .spot_prev').removeAttr("tabindex");
		jQuery('.spotlight .spot_next').removeAttr("tabindex");				
	});
	
	
	jQuery('.spotlight .close').click(function(){
		jQuery('.spotlight').stop().animate({width: '38px'}, 200);
