    <script>
        function ajaxSubmit(form) {
            $.ajax({
                url: '/auth/login',
                type: 'POST',
                data: {

                    login: form.elements['login'].value,
                    password: form.elements['password'].value,
                    submit: 1,
                    format: 'json',
                    rt: 'ajax'
                },
                success: function (data, status) {
                    //d = $.parseJSON(data);
                    if (data.success) {
                        window.location.href = "";
                    }
                    else {
                        var el = $('#auth-errors');
                        el.show();
                        el.html(data.message.join('<br />'));
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $('#auth-errors').addClass('alert');
                    $('#auth-errors').html("Error ocured: " + textStatus);
                }
            });
            return false;
        }
    </script>
    <div id="loginForm" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="loginFormLabel"
         aria-hidden="true">
        <form enctype="application/x-www-form-urlencoded" method="post" action="/auth/login/"
              onsubmit="return ajaxSubmit(this)" class="form-horizontal">

            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
                <h3 id="loginFormLabel">Вход в систему</h3>
            </div>

            <div class="modal-body">

                <div class="alert-error" id="auth-errors"></div>

                <div class="control-group">
                    <label class="control-label" for="login">Имя пользователя</label>

                    <div class="controls">
                        <input type="text" name="login" id="login" autofocus placeholder="" AUTOCOMPLETE="OFF">
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label" for="password">Пароль</label>

                    <div class="controls">
                        <input type="password" name="password" id="password" value="" AUTOCOMPLETE="OFF"/> &nbsp; <a
                                href="http://passport.pskovedu.ru/auth/restore_pass">Забыли пароль?</a>
                    </div>
                </div>
                <hr>
                <div class="control-group">
                    <label class="control-label">&nbsp;</label>

                    <div class="controls">
                        <button class="btn btn-large btn-primary" name="submit" id="submit" onclick="submit()"> Войти в
                            систему &rarr; </button>
                        &nbsp;
                        <button class="btn btn-large" data-dismiss="modal" aria-hidden="true"
                                onclick="window.location.href='http://passport.pskovedu.ru/auth/register'">
                            Зарегистрироваться?
                        </button>

                        <br><br><a class="btn btn-large btn-danger"
    href="http://passport.pskovedu.ru/auth/esia/redirect?returnTo=http://pskovedu.ru/page/c83aeb3b-a37c-4e71-b5b1-a48e096fdc49"> Вход через ЕСИА →</a>
                        <!--a href="entry.html">Забыли&nbsp;пароль?</a-->
                    </div>
                </div>

            </div>

        </form>
