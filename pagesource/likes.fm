var popup = document.getElementById('infoPopup').style
function showInfo(type){
if(popup.display == "block")
closeInfo()
else
popup.display = "block"
document.getElementById('box_body').innerHTML = document.getElementById(type + 'InfoTPL').innerHTML
document.getElementById('box_title').innerHTML = {enter: "Типичные проблемы и способы их решений", service: "Информация об услугах"}[type]
popup.top = {enter: "95px", service: "120px"}[type]
return false
}
function closeInfo(){
popup.display = "none"
}
function showFooter(){
setTimeout(function(){ document.getElementById("enterFooter").style.display = "block"}, 1000)
}
if(location.hash == "#info")
showInfo('service')
</script>


