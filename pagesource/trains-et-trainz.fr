var fog = {};
fog.draw = function(ctx, x, y){
ctx.fillStyle = "rgba( 255, 255, 255, " + Math.random() + " )";
ctx.arc( x, y, 10,0,Math.PI*2,true);
ctx.closePath();
ctx.fill();
};
fog.start = function(){
var ctx = document.getElementById('canvas').getContext("2d");
var x = 0;
var y = 0;
var t = setInterval( 
function(){
x = 300 + 300*Math.sin(x);
y = 300 + 300* -Math.cos(x); 
x += 2;
fog.draw(ctx, x, y);
}, 100);
};
