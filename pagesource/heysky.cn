	var playTime = 0;
	var playTimer = null;
	function incrPlayTime() {
        playTime = 0;
		playTimer = setInterval(function () {
			playTime++;
        },1000)
    }
    function stopPlayTime() {
        clearInterval(playTimer);
        zhuge.track('播放首页宣传视频', {
            '时间(秒)' : playTime
        });
    }

	$(function () {



        var mySwiper = new Swiper('.swiper-container',{
			loop: true,
			autoplay: 6000,
			onClick: function(swiper){
				//banner001的class 为 母亲节活动
				if($(swiper.clickedSlide).hasClass("banner001")){
					window.location.href="/mothersday";
				}
			}
		});

		/* $("#btnlogin,.cart").click(function(e) {
            location.href="/ebayLogin";
        });  */
		$("#btnheysky").click(function(e) {
			window.open("http://crm.heysky.com");
		});

		$("#banner3").click(function(e) {
			window.open("/account/christmas");
		});


		//登录
		$("#loginbtn").click(function () {
			var username = $("#username").val();
			if (!$.utils.isMobile(username)) {
				layer.msg('用户名格式不准确');
				return;
			}
			var password = $("#password").val();
			if ($.utils.isEmpty(password)) {
				layer.msg('请输入登录密码');
				return;
			}
			var rememberMe = false;
			if ($("#rememberMe").is(":checked")) {
				rememberMe = true;
			}
			$.ajax({
				url:'/normalLogin',
				type:'post',
				async:false,
				data:{username:username,password:password,rememberMe:rememberMe,tag:"reg"},
				success:function(data){
					if(data == "nouser"){
						layer.msg('用户名不存在');
					}else if (data == "passwordError") {
						layer.msg('登录密码错误');
					}else if (data == "fail") {
						layer.msg('登录失败');
					}else{
						if(jump_to.length == 0)
							location.href = "/dashboard";
						else
							location.href = jump_to;
					}
				}
			});
		});

		//注册
		$("#saveReg").click(function () {
			location.href="/account/register";
		});


		//找回密码
		$("#getpass").click(function () {
			location.href="/account/getpass";
		});

		//回车
		$('input:text:first').focus();
		var $inp = $('input[type="text"],input[type="password"],.login-btn');
		$inp.bind('keydown', function (e) {
			var key = e.which;
			if (key == 13) {
				e.preventDefault();
				var nxtIdx = $inp.index(this);
//                     alert("$inp[0]="+$inp[0].value+",(nxtIdx + 1)="+(nxtIdx + 1)+",$inp.length="+$inp.length+",(nxtIdx + 1) % $inp.length="+(nxtIdx + 1) % $inp.length)
//                     $inp[(nxtIdx + 1) % $inp.length].select();
				$inp[(nxtIdx + 1) % $inp.length].focus();
				if ((nxtIdx + 1) == 2) {
					$(".login-btn").click();
				}
			}
			if (key == 9) {
				e.preventDefault();
				var nxtIdx = $inp.index(this);
				$inp[(nxtIdx + 1) % $inp.length ].focus();
			}
		});

        //播放视频
        $(".video-but").click(function () {
            var myVideo=document.getElementById("indexVideo");
            window.setTimeout(function (){
                myVideo.play();
			}, 100);
            myVideo.currentTime = 0;
            incrPlayTime();
            $(".popup-video").show();
        });
        //关闭视频弹出框
        $(".popup-video .close").click(function () {
            var myVideo=document.getElementById("indexVideo");
            myVideo.pause();
            $(".popup-video").hide();
            stopPlayTime();
        });

        $(document).keyup(function(event){
            if(event.keyCode == 27 && !$(".popup-video").is(":hidden")){
                $(".popup-video .close").click();
			}
        });

	});

	function go(path) {
	    console.log(path);
		location.href=path;
	}

	function goEbayLogin() {
