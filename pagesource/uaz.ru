<script type="text/template" id="dealer-card-template">
    <div class="dealer-card <% if(country_id != '1'){ %> dealer-card_small <% } %>">

        <div class="dealer-card__name auto"><%= name %></div>

        <% if (marketing_name_service) { %>
        <div class="dealer-card__name service"><%= marketing_name_service %></div>
        <% } %>

        <% if (obj.isBestService === 'best-service' || obj.isBestService === '1') { %>
        <div class="dealer-card__service-cup">
            «Лучший дилер
            по послепродажному
            обслуживанию УАЗ»
        </div>
        <% } %>

        <% if (auto) { %>
        <div class="dealer-card__block">
            <div class="dealer-card__address-ico"></div>
            <div class="dealer-card__info">
                <%= fullAddress %>
            </div>
        </div>
        <% } else { %>

        <% if (service) { %>
        <div class="dealer-card__block">
            <div class="dealer-card__address-ico"></div>
            <div class="dealer-card__info">
                <%= fullTechAddress %>
            </div>
        </div>
        <% }} %>

        <% if (www) { %>
        <div class="dealer-card__block">
            <div class="dealer-card__site-ico"></div>
            <div class="dealer-card__info">
                <a target="_blank" data-event="outbound" href="http://<%= www %>/?utm_referrer=<%= location.host %>"><%= www %></a><br>
            </div>
        </div>
        <% } %>

        <% if (email) { %>
        <div class="dealer-card__block auto">
            <div class="dealer-card__mail-ico"></div>
            <div class="dealer-card__info">
                <a href="mailto:<%= email %>"><%= email.length <= 22 ? email : email.replace('@', '@<br>') %></a>
            </div>
        </div>
        <% } %>

        <% if (tech_email) { %>
        <div class="dealer-card__block service">
            <div class="dealer-card__mail-ico"></div>
            <div class="dealer-card__info">
                <a href="mailto:<%= tech_email %>"><%= tech_email.length <= 22 ? tech_email : tech_email.replace('@', '@<br>') %></a>
            </div>
        </div>
        <% } %>

        <% if (active_sale === '1') { %>
        <div class="dealer-card__block">
            <div class="dealer-card__car-ico ico-car"></div>
            <div class="dealer-card__info">
                Отдел продаж<br>
                <%= phone %>
            </div>
        </div>
        <% } %>

        <% if (active_service  === '1') { %>
        <div class="dealer-card__block">
            <div class="dealer-card__service-ico ico-service"></div>
            <div class="dealer-card__info">
                Сервис<br>
                <%= tech_phone %>
            </div>
        </div>
        <% } %>

        <% if (has_body_shop === '1'){ %>
        <div class="dealer-card__block">
            <div class="dealer-card__body-shop-ico ico-body-shop"></div>
            <div class="dealer-card__info">
                Кузовной цех<br>
                <%= phone %>
            </div>
        </div>
        <% } %>


        <% if (profi_sale === '1' || (profi_service === '1' && service)) { %>
        <div class="dealer-card__block dealer-card__block_profi">
            <div class="dealer-card__profi-ico"></div>
            <div class="dealer-card__info">
                УАЗ ПРОФИ
            </div>
        </div>
        <% } %>

        <div data-id="<%= id %>" class="dealer-card__detail-button btn <% if(active_sale === '1' && country_id === '1' && auto){ %>dealer-card__detail-button_first<% } %>"></div>
        <% if(active_sale === '1' && country_id === '1'){ %>
        <% if (auto) { %>
        <div data-popup="test-drive-popup" data-id="<%= id %>" data-name="<%= name %>" class="show-popup-button dealer-card__test-drive btn btn-green"></div>
        <% } %>
        <% } %>
    </div>
</script>

