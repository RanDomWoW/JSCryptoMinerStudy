    <script type="text/javascript">
        var doofinderCDN = '//cdn.doofinder.com/media/js/doofinder-classic.6.latest.min.js';
        (function (d, t) {
                f = d.createElement(t), s = d.getElementsByTagName(t)[0];
                f.async = 1;
                f.src = ('https:' == location.protocol ? 'https:' : 'http:') + doofinderCDN;
                f.setAttribute('charset', 'utf-8');
                s.parentNode.insertBefore(f, s)
            }(document, 'script')
        );

        var dfClassicLayers = [{
            urlHash: false,
            queryInput: "#autocomplete",
            appendTo: "#search-box-container",
            hashid: '24f9569509d140f2331795879479e8ab',
            zone: 'eu1',
            mainContainerId: 'doofinder-v2',
            searchParams: {
                rpp: 10,
                transformer: null,
            },
            display: {
                lang: 'fr',
                header: {},
                width: '995px',
                dleft: '-30px',
                closeOnClick: true,
                templateFunctions: {
                    myNote: function () {
                        return function (text, render) {
                            var noteDoubleDigit;
                            if (render(text).length == 1 && render(text) != "0") {
                                noteDoubleDigit = render(text);
                            }
                            else if (render(text).length == 2) {
                                noteDoubleDigit = render(text);
                            }
                            else return '<span class="emptyNote"></span>';

                            return '<img class="img-notes" src="/img/notes/' + noteDoubleDigit + '.gif" />'
                        }
                    },

                    newProduct: function () {
                        return function (text, render) {
                            /* NEW pour le nouvel export et true pour l'ancien export */
                            if (render(text) == 'NEW' || render(text) == 'true') {
                                return '<span class="new">nouveauté</span>'
                            }
                            /* Picto "Coin des affaires" */
                            else if (render(text) == 'CDA') {
                                return '<span class="new coindesaffaires">Coin des affaires</span>'
                            }
                            else return ''
                        }
                    },

                    formatTitle: function () {
                        return function (text, render) {
                            var res = render(text).split("|");
                            var title = res[0];
                            var statut = res[1];
                            if (statut == 'CAT' || statut == 'BRAND') {
                                isCat = true;
                                return '> <u>' + title + '</u>';
                            }
                            else
                                return title;
                        }
                    },

                    formatCellHeight: function () {
                        return function (text, render) {
                            if (render(text) == 'CAT') {
                                return ' special cat';
                            }
                            else if (render(text) == 'BRAND') {
                                return ' special brand';
                            }
                            else
                                return '';
                        }
                    },

                    prixBarre: function () {
                        return function (text, render) {
                            var res = render(text).split("|");
                            var result0 = res[0].replace('.', ',');
                            var result1 = res[1].replace('.', ',');
                            //hidde price for brand shop page
                            if (result1 == '0,00 &euro;') {
                                return '';
                            }
                            if (result0 == '0,00 &euro;' || result0 == '' || result0 == '0,00 CHF')
                                return '<p class="price">' + result1 + '</p>';
                            else
                                return '<p class="price"><span class="prixBarre">' + result0 + '</span><span class="prixPromo">' + result1 + '</span></p>';
                        }
                    },

                    ajoutPanier: function () {
                        return function (text, render) {
                            if (render(text) != '')
                                return '<a href="' + render(text) + '" id="hlAddShoppingCart" class="ajoutPanier" />';
                            else
                                return '';
                        }
                    },

                    stock: function(){
                        return function(text, render){
                            if(render(text) == '1'){
                                return '<p class="stockstatus">Envoi immédiat</p>'
                            }
                            else if(render(text) == '2'){
                                return '<p class="stockstatus">En stock</p>'
                            }
                            else if(render(text) == '3'){
                                return '<p class="stockstatus stockstatus_dispoF">Dispo fournisseur</p>'
                            }
                            else if(render(text) == '4'){
                                return '<p class="stockstatus stockstatus_dispo7">Dispo 7 jours</p>'
                            }
                            else if(render(text) == '5'){
                                return '<p class="stockstatus stockstatus_dispo7a15">Dispo 7 à 15 jours</p>'
                            }
                            else if(render(text) == '6'){
                                return '<p class="stockstatus stockstatus_dispo15">Dispo + de 15 jours</p>'
                            }
                            else if(render(text) == '7'){
                                return '<p class="stockstatus stockstatus_dispo15">Dispo + de 15 jours</p>'
                            }
                            else if(render(text) == '8'){
                                return '<p class="stockstatus stockstatus_pre">Précommande</p>'
                            }
                            else if(render(text) == '9'){
                                return '<p class="stockstatus stockstatus_out">Rupture</p>'
                            }
                            else return '<p class="stockstatus stockstatus_empty"></p>'
                        }
                    },

                    map_periodicite: function () {
                        return function (text, render) {
                            if (render(text) !== '') {
                                var per_text = findByKey(render(text).trim());
                                return per_text ? '<span style="">' + per_text + '</span>' : ''
                            }
                        }
                    }
                },
                results: {
                    template: document.getElementById('df-results-template-grid').innerHTML
                }
            }
        }];

        function findByKey(key) {
            //utilisation des tables simples au lieu des objets pour supporter internet explorer
            var period_map_key = ["1D", "1W", "1M", "3M", "6M", "2W", "1Y"];
            var period_map_value = ["/jour", "/semaine", "/mois", "/trimestre", "/semestre", " bimensuel", "/an"];
            for (var i = 0; i < period_map_key.length; i++) {
                if (period_map_key[i] === key) {
                    return period_map_value[i];
                }
            }
            return null;
        }
    </script>
