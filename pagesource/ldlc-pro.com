                        {{#stock}}{{codestockgen}}{{/stock}}
                        {{#prixBarre}}{{#format-currency}}{{refprice}}{{/format-currency}}|{{#format-currency}}{{price}}{{/format-currency}}{{#map_periodicite}}{{periodicite}}{{/map_periodicite}}{{/prixBarre}}
                    </div>
                </a>
                {{#ajoutPanier}}{{addtobasketlink}}{{/ajoutPanier}}
