                                    str2 += '<p class="' + cla + '"><span>' + a[1] + '</span><i>' + a[3] + '</i><i>' + a[4] + '</i><i>' + a[5] + '%' + array + '</i></p>';
                                }
                            }
                        });
                        $(".hqlist").html(str);
                        $(".infor-1 .ck").attr("src", $(".infor-1 .appear").attr("src") + "&rd=" + Math.random(1));

                        $(".infor-2").html(str2);
                        if (call) {
                            call();
                        }
                    }
                });
            };

