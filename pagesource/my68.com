                                }
                                else {
                                    that.addClass(def.activeCls).siblings().removeClass(def.activeCls);
                                    $($this.find(def.targetObj)).hide();
                                    $($this.find(def.targetObj).get(index)).show();
                                }
                            }, def.delay);
                        }).mouseout(function () {
                            clearTimeout(timer);
                        })
                    });
                }

