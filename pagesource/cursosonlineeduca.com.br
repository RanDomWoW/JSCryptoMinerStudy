<script type="text/javascript">
	var ajax = new sack();
	var currentClientID=false;
	function getClientData(){ 
   var clientId = document.getElementById('FormField_13').value.replace(/[^0-9]/g,'');

       if(clientId.length<=9 && clientId!=currentClientID){
