                                        <script>
 
  
 function displayPoll(){
	var id = pollid;
   

    $.ajax({
           url : '//pollapi.ebela.in/Poll/?qid='+id+'',
           dataType : 'json',
           type : 'GET',
           success : function(data){
                // console.log(data);
               $.each(data, function(index, row){
                //   console.log(row.article);
              var renderHTML = '';
                       renderHTML += '<p class="qtn" style="font-size: 18px; font-weight: bold; margin-bottom: 20px;">'+row.article.question+'</p>';
                       renderHTML += '<form id="myForm">';
                           $.each(row.article.answer, function(index1, row1){ 
                               renderHTML += '<label class="radio-inline"><input type="radio" name="myRadio" value="'+row1.answerId+'">'+row1.option+'</label>';  
                           });
                       renderHTML += '</form>';
                       renderHTML += ' <button type="submit" class="btn btn-default opinion_submit" onclick="displayChart('+row.article.pollId+');">Submit</button>';
               $("#opinion_div").html(renderHTML);
               });
           },
           error : function(){
           
           },
       });
}


function displayPollQues(){
	var id = pollid;
    $.ajax({
           url : '//pollapi.ebela.in/Poll/?qid='+id+'',
           dataType : 'json',
           type : 'GET',
           success : function(data){
                // console.log(data);
               $.each(data, function(index, row){
                //   console.log(row.article);
              var renderHTML = '';
                       renderHTML += '<p class="qtn" style="font-size: 18px; font-weight: bold; margin-bottom: 20px;">'+row.article.question+'</p>';
                   //console.log(row.article.question);
                   $("#opinoin_ques").html(renderHTML);
               });
           },
           error : function(){
           
           },
       });
}

function displayChart(queId){
	ga('send','event','Ebela Opinion Poll','Opinion Poll Submit Button Clicked','Poll Inserted');
	ga('send', { 'hitType': 'pageview', 'page': '/', 'title': 'Opinion Poll' });
   var ansId = $('form input[name="myRadio"]:checked').val();
   if($("form input:radio[name='myRadio']").is(":checked")){
       displayPollQues();
       showChart(queId, ansId);
   }else{
       alert("Please select an Option to participate in the poll");
		return false;
   }
}

function displayChartCookies(queId,ansId){
   displayPollQues();
    showChart(queId, ansId);
}


function showChart(queId, ansId){
    var renderHTML ='';
   var arr= [];
       $.ajax({
               url : '//pollapi.ebela.in/Poll/AbpTotalUserPollingServlet?q_id='+queId+'&ans_id='+ansId+'',
               dataType : 'json',
               type : 'GET',
               async : true,
               success : function(data){
                   $.each(data,function(index, row){
                       $.each(row.answer, function(indexnew, rownew){
                           arr.push(rownew.option);
                           arr.push(rownew.votes);
                     //      console.log(arr);
                       });
                        $(".opinion_col").css('display','none');
                          google.charts.load('current', {'packages':['corechart']});
                       if(arr.length == 4){
                       //    console.log(arr.length);
                         google.charts.setOnLoadCallback(drawChart);
                       }else if(arr.length == 6){
                           google.charts.setOnLoadCallback(drawChart1);
                       }else if(arr.length == 8){
                           google.charts.setOnLoadCallback(drawChart2);
                       }
                       //arr.length == 4
                       function drawChart() {
                           var data = google.visualization.arrayToDataTable([
                             ['option', 'Vote', { role: 'style' }, { role: 'annotation' }],
                            [arr[0],   parseInt(arr[1]), 'stroke-color: #703593; stroke-width: 2; fill-color:gold;', parseInt((parseInt(arr[1])*100)/total)+'%'],
                             [arr[2],   parseInt(arr[3]),'stroke-color: #703593; stroke-width: 2; fill-color: #b87333;', parseInt((parseInt(arr[3])*100)/total)+'%'],
                           ]);

                           var options = {
                             title: 'আপনার মত',
                             titleTextStyle:{ color: 'black',fontSize: 14,fontName:'Arial'},
                               hAxis: {
                                   textStyle: {
                                       color: 'black', 
                                       fontName: 'Arial Black', 
                                       fontSize: 11
                                   }
                               },
                               vAxis: { textPosition: 'none' },
                               chartArea: {
                                   width: 290,
                                   height: 170
                               },
                               legend: {position: 'none'},
                               tooltip: {
                                   trigger: "none"
                               }
                           };
                           var chart = new google.visualization.ColumnChart(document.getElementById('opinion_chart'));
                           chart.draw(data, options);
                       }
                       
                       // arr.length == 6
                        function drawChart1() {
                            var total = parseInt(arr[1])+parseInt(arr[3])+parseInt(arr[5]);
                           var data = google.visualization.arrayToDataTable([
                             ['option', 'Vote', { role: 'style' }, { role: 'annotation' }],
                             [arr[0],   parseInt(arr[1]), 'stroke-color: #fff; stroke-width: 0; fill-color:#056FBF;', parseInt((parseInt(arr[1])*100)/total)+'%'],
                             [arr[2],   parseInt(arr[3]),'stroke-color:#fff ; stroke-width: 2; fill-color:#023F6D;', parseInt((parseInt(arr[3])*100)/total)+'%'],
                             [arr[4],   parseInt(arr[5]),'stroke-color: #fff; stroke-width: 2; fill-color:#022946;', parseInt((parseInt(arr[5])*100)/total)+'%']
                           ]);
                            
                           var options = {
                             title: 'আপনার মত',
                             titleTextStyle:{ color: 'black',fontSize: 14,fontName:'Arial'},
                               hAxis: {
                                   textStyle: {
                                       color: 'black', 
                                       fontName: 'Arial Black', 
                                       fontSize: 12
                                   }
                               },
                               vAxis: { textPosition: 'none' },
                               chartArea: {
                                   width: 220,
                                   																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																									height: 140
                               },
                               legend: {position: 'none'},
                               tooltip: {
                                   trigger: "none"
                               }
                           };
                           var chart = new google.visualization.ColumnChart(document.getElementById('opinion_chart'));
                           chart.draw(data, options);
                       }
                       
                       // arr.length == 8
                        function drawChart2() {
                           var data = google.visualization.arrayToDataTable([
                              ['option', 'Vote', { role: 'style' }, { role: 'annotation' }],
                              [arr[0],   parseInt(arr[1]), 'stroke-color: #703593; stroke-width: 2; fill-color:gold;', parseInt((parseInt(arr[1])*100)/total)+'%'],
                             [arr[2],   parseInt(arr[3]),'stroke-color: #703593; stroke-width: 2; fill-color:#b87333;', parseInt((parseInt(arr[3])*100)/total)+'%'],
                             [arr[4],   parseInt(arr[5]),'stroke-color: #703593; stroke-width: 2; fill-color:silver;', parseInt((parseInt(arr[5])*100)/total)+'%']
                             [arr[6],   parseInt(arr[7]), 'stroke-color: #703593; stroke-width: 2; fill-color:#e5e4e2;', parseInt((parseInt(arr[7])*100)/total)+'%']
                           ]);

                         var options = {
                             title: 'আপনার মত',
                             titleTextStyle:{ color: 'black',fontSize: 14,fontName:'Arial'},
                               hAxis: {
                                   textStyle: {
                                       color: 'black', 
                                       fontName: 'Arial Black', 
                                       fontSize: 12
                                   }
                               },
                               vAxis: { textPosition: 'none' },
                               chartArea: {
                                   width: 210,
                                   height: 140
                               },
                               	 legend: {position: 'none'},
                               	 tooltip: {
                                     trigger: "none"
                                 }
                           };
                           var chart = new google.visualization.ColumnChart(document.getElementById('opinion_chart'));
                           chart.draw(data, options);
                       }
                   });
                       var prevValue = "";
                       var votedValue = "";
                   if(typeof $.cookie("test") !== "undefined"){
                       prevValue  = $.cookie("test");
                   }
                       
                   if(prevValue != ""){
						votedValue = prevValue;
                       var date = new Date();
                       var minutes = 30;
                       date.setTime(date.getTime() + (100));
                       
                       $.cookie('test', (prevValue), { expires: date });
                   } else {
						votedValue = queId;
                       votedValue1 = ansId;
                       var date = new Date();
                       var minutes = 30;
                       date.setTime(date.getTime() + (100));
                       																																																																																																																																																																														
                       $.cookie('test', (votedValue + ","+ votedValue1), { expires: date });
                   }
                   $("#opinion_chart").css('display','block');
               },
       });  
}
displayPoll();
  
</script>

                                      
                                  
