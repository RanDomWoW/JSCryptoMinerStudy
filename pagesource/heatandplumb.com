    function readTextFile(file)
    {
        var allText='';
        var rawFile = new XMLHttpRequest();
        rawFile.open("GET", file, false);
        rawFile.onreadystatechange = function ()
        {
            if(rawFile.readyState === 4)
            {
                if(rawFile.status === 200 || rawFile.status == 0)
                {
                    allText = rawFile.responseText;
                }
            }
        }
        rawFile.send(null);
       
        try {
            allText=JSON.parse(allText);
        } catch (e) {
            return false;
        }
        return allText;
    }
    function parseURLParams(url) {
        var queryStart = url.indexOf("?") + 1,
        queryEnd   = url.indexOf("#") + 1 || url.length + 1,
        query = url.slice(queryStart, queryEnd - 1),
        pairs = query.replace(/\+/g, " ").split("&"),
        parms = {}, i, n, v, nv;

        if (query === url || query === "") return;

        for (i = 0; i < pairs.length; i++) {
            nv = pairs[i].split("=", 2);
            n = decodeURIComponent(nv[0]);
            v = decodeURIComponent(nv[1]);

            if (!parms.hasOwnProperty(n)) parms[n] = [];
            parms[n].push(nv.length === 2 ? v : null);
        }
        return parms;
    }
    
    function makeSomeTxtFile(allCssJsonStr,usedSelctorJsonStr,wrongSelctorJsonStr,pageName){
        
        $.ajax({
            url: 'https://www.heatandplumb.com/core/ajax_functions.php',
            type: 'POST',
            dataType:'json',
            data: {
                'action': 'removeUnusedCss',
                'pageName': pageName,
                'usedSelctorJsonStr': usedSelctorJsonStr,
                'allCssJsonStr': allCssJsonStr,
                'wrongSelctorJsonStr': wrongSelctorJsonStr
            },
            success: function (res) {
                console.log(res);
                console.log(res['result']);
            },
            error: function () {
                console.log('Something Went Wrong With Removed Unsused Ajax');
            }
        });
        
        
    }</script></body></html>