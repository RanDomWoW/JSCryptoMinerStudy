<script>
var topic=document.getElementById("topicsT");
var slides=document.getElementsByClassName("topics");
var topicID=0;
var timer=null;
change();
function setSlide(n){
  clearInterval(timer);
  endX=startX=0;
  topicID+=n;

  if(topicID<0){
    topicID=slides.length-1;
  }else{
    topicID=topicID>=slides.length?0:topicID;
  }
  if(topic!=null){
    topic.style.top=-24*topicID+"px";
  }
  timer=setInterval("setNext()",5000);
}

function setNext(){
  clearInterval(timer);
  change();
}
function change(){

  clearInterval(timer);
  if(topic!=null){
    topic.style.top=-24*topicID+"px";
  }
  setSlide(1);
}

