                // Search toggle
        $(".navbar_search-icon").click(function (){
            var $el = $("#topmenu-navbar-collapse-1");
            var $gl = $("#navbar-all");
            if ($el.attr('class')=='collapse navbar-collapse') {
                $el.attr('class','hidden');
                $gl.attr('class','row navbar-all navbar--search-open');
                setTimeout(function () {
                    $("#search").focus();
                    $gl.attr('style','');
                    document.getElementById('search').focus();
                }, 400);
            } else {
                $gl.attr('class','row navbar-all');
                                setTimeout(function () {
                    $el.attr('class','collapse navbar-collapse');
                }, 400);
            }
        });

        // Ajax search result
        $(document).ready(function() {

            $('#search').bind('textchange', function () {

                if(window.a) clearTimeout(window.a);
                window.a = setTimeout(function(){

                    var search = $("#search").val();
                    if (search.length >= 3 && search.length < 30 ) {
                        $("#block-search-result").hide();
                        $.ajax({
                            type: "POST",
                            url: "/search/search",
                            data: "q="+search,
                            //dataType: "html",
                            cache: false,
                            success: function(data) {
                                $("#block-search-result").show();                                 $("#list-search-result").html(data);                             }
                        });
                    } else {
                                                $("#block-search-result").hide();
                    }
                }, 800);

            });
        });
    </script>


