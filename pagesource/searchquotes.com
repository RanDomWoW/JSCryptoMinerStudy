		$('#newmessage-id a').bind('click', function(e) {
		  
		  var target_q = $(this);
		  var q_id = target_q.siblings("input:hidden").val();
		  var ref_txt = target_q.text();
		
			 // Create the AJAX request  
			 $.ajax({  
				 type: "POST",
				 url: "http://www.searchquotes.com/sof/ajax_ch_votes_newmessage.php",
				 data: "qId=" +q_id+ "&refTxt=" +ref_txt+ "&nUserId=" +user_id,
				 success: function(msg) { 
					
					var temp = new Array();
					temp = msg.split('|||');
					
					var alert_msg ='';
					var act = temp[0];
					var cnt = temp[1];
						alert_msg = temp[2];
					var fav_cnt = temp[3];
					
					$('div.error-notification').hide();

					if(alert_msg != 'NO_MESSAGE'){
					 target_q.parent().append(dvHtml);
					 $("div.error-notification").html(alert_msg);
					 return;
					}

					if(ref_txt == 'up vote'){
						if(act == 1) target_q.addClass('vote-up-on');
						 else if(act == 2)	target_q.removeClass('vote-up-on');

					}else if(ref_txt == 'down vote'){
						if(act == 1) target_q.addClass('vote-down-on');
						 else if(act == 2)	target_q.removeClass('vote-down-on');

					}else if(ref_txt == 'newmessage'){
						if(act == 1) target_q.addClass('newmessage-on');
						 else if(act == 2)	target_q.removeClass('newmessage-on');

						 target_q.siblings(".newmessagecount").html(fav_cnt);
					
					}
					
					target_q.siblings("span.vote-count-post").html(cnt);
				}  
			 });  
		 
		});
		//endof questions


    });

