            $('.last-name .entry-feedback-wrapper #LastName').attr('aria-invalid', 'true');
            $('.last-name .field-validation-valid.form-validation-message.field-validation-error').html('<span id="LastName-error" class="has-error">The Last Name field is required.</span>');

            isValid = false;
        }

        if (document.getElementById("MailingListEmail").value == "") {
            $('.email').addClass('has-error');
            $('.email .entry-feedback-wrapper #MailingListEmail').attr('aria-required', 'true');
            $('.email .entry-feedback-wrapper #MailingListEmail').attr('aria-describedby', 'MailingListEmail-error');
            $('.email .entry-feedback-wrapper #MailingListEmail').attr('aria-invalid', 'true');
            $('.email .field-validation-valid.form-validation-message.field-validation-error').html('<span id="LastName-error" class="has-error">The Email field is required.</span>');

            isValid = false;
        }

        if (!isValid)
            if (document.getElementById("FirstName").value == ""
                || document.getElementById("LastName").value == ""
                || document.getElementById("MailingListEmail").value == "") {
                alert("Please complete entries.");
                return false;
            }

        if (!ergx.test(document.getElementById("MailingListEmail").value)) {
            alert("Invalid Email.");
            return false;
        }

        var model = {
            FirstName: $('#FirstName').val(),
            LastName: $('#LastName').val(),
            MailingListEmail: $('#MailingListEmail').val()
        };

        $.ajax({
            url: '/addon/submitmailinglist',
            type: 'POST',
            cache: false,
            data: { 'model': JSON.stringify(model) }
        })
        .success(function (result) {
            if (result && result.msg) {
                $('#mail-list-message').html(result.msg);
            }
        })
        .error(function () {
            $('#mail-list-message').html('An error occurred with your request. Please try again.')
        });
    }

