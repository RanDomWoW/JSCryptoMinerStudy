//валидация поля ИМЯ при регистрации
        $('body').on('keyup', '.valid_name', function (){
            var err_field = $(this).parent().find(".err-msg"),
            input_text = $(this).val(),
            submit = $(this).parents("article").find("a.form-submit");

            if(input_text){

                //первый символ заглавный
                $(this).val(input_text[0].toUpperCase() + input_text.slice(1));

                var regexp = /^[а-яё\s]+$/i;
                if(!regexp.test(input_text)) {
                    if(!$(this).hasClass("error-field"))
                    {
                        $(this).addClass("error-field");
                        $(this).parents("article").addClass("error");
                    }
                    err_field.text('Только русские символы');
                    if(!submit.hasClass("disabled")) submit.addClass("disabled");
                } else {
                    err_field.text('');
                    if(submit.hasClass("disabled")) submit.removeClass("disabled");
                    if($(this).hasClass("error-field")) {
                        $(this).removeClass("error-field");
                        $(this).parents("article").removeClass("error");
                    }
                }
            } else {
                err_field.text('');
                if(submit.hasClass("disabled")) submit.removeClass("disabled");
            }
            return false;
        })
    });

