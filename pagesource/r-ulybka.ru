            event.preventDefault();
            var _this = $(this),
                myFormData = new FormData(),
                bar = _this.parents("form").find('.progressbar');
            myFormData.append('ACTION', 'FILE_UPLOAD');
            myFormData.append('sessid', _this.parents("form").find("input[name=sessid]").val());
            myFormData.append('FILE', _this.prop("files")[0]);
            if (!_this.prop("files")[0]) return false;
            $.ajax({
                url: _this.parents("form").attr("data-request"),
                type: 'POST',
                contentType: false,
                processData: false,
                data: myFormData,
                dataType: 'json',
                xhr: function(){
                    var xhr = $.ajaxSettings.xhr();
                    xhr.upload.addEventListener('progress', function(evt){
                        if(evt.lengthComputable) {
                            var percentComplete = Math.ceil(evt.loaded / evt.total * 100);
                            bar.css('width', percentComplete + '%');
                            console.log(percentComplete);
                        }
                    }, false);
                    return xhr;
                },
                success: function(data){
                    bar.css('display','none');
                    if(data.STATUS == 'SUCCESS'){
                        $(_this).parents("form").find(".ajax-data").html(data.MESSAGE + "<div>Файл загружен!</div>");
                    }
                }
            });
        })
