        <script type="text/javascript">

define('jquery', [], function() {
    return $;
});

require(['picturefill'], function() {
});

require(['jquery'], function($) {
    $('#ORDER_FORM').on('click', 'a.del-select', function() {
        curDelivery = $(this).siblings('input');
        if(curDelivery.attr('checked')) {
            curDelivery.attr('checked', false);
            $(this).removeClass('added');
        } else {
            $(this).siblings('input').click();
        }

        submitOrderForm();

        return false;
    });

    $('#ORDER_FORM').on('click', '.del-select-type', function() {
        curDeliveryType = $(this).siblings('input');
        if(curDeliveryType.attr('checked')) {
            curDeliveryType.attr('checked', false);
        } else {
            curDeliveryType.click();
        }

        if($(this).hasClass('reload'))
            submitOrderForm();
    });

    $("body").on('click', "a.form-submit:not(.disabled)", function() {
        $(this).parents('form').trigger('submit');

        return false;
    });
    var previousParentField;
    $('.container-field input').on('click',function () {
        var parentField = $(this).closest('.container-field');
        if(previousParentField && !parentField.is(previousParentField) && !previousParentField.find('input').val()) {
            previousParentField.removeClass('on');
            previousParentField.find('label').removeClass('on');
        }
        parentField.addClass('on');
        $(parentField).find('label').addClass('on');
        previousParentField = parentField;
    });

    $('#my-progress-file').on('change', function() {
        $(this).siblings('.btn-add-photo').text($(this).val());
    });

    function showblocks(quantity){
        $('.dates-block').css("display","block");
        $('.dates-block .column-form:visible').css("display","none");
        for(var i = 1; i <= quantity; i++) {
            $('.dates-block-' + i).css("display","block");
        }
        $(".additional-text").text('');
        // if($(this).val() == 4) $(".additional-text").text('младших ');
    }

    //лк
    if(parseInt($('#kids_quantity').val()))
        showblocks($('#kids_quantity').val());

    $('#kids_quantity').on('change', function() {
        if($(this).val() > 0) {
            showblocks($(this).val());
        } else {
            $(".additional-text").text('');
            $('.dates-block').css("display","none");
        }
    });
    $('.success-message').on('click', function() {
        $(this).parent().find('.success-message-bcgr').remove();
        $(this).remove();
    });


    function startCountdown() {
        var timerId = setInterval(function() {
            if($(".send-again .time_countdown").text() > 1) {
                $(".send-again .time_countdown").text($(".send-again .time_countdown").text() - 1);
            } else {
                grecaptcha.reset();
                clearInterval(timerId);
                $(".send-again .time_countdown").text("")
                $(".send-again").css("display","none");
                $(".phone-field-input-step1 .send-code").removeClass("disabled");
            }
        }, 1000);
    }

    var renderCaptcha = function() {
        $('#g-recaptcha1').html('');
        var cid = grecaptcha.render('g-recaptcha1', {
            'sitekey' : '6LerUiQUAAAAALN6UYjh6Fko_6w8JYbOSkQRUWSl'
        });
        $('#g-recaptcha1').attr("cid", cid);
    };
    $('body').on('click', '.phone-field .change-number', function (){
        if(!$('#g-recaptcha1').html()) renderCaptcha();

        $(".phone-field-input-step3").slideUp(300);
        $(".phone-field-input-step2").slideUp(300);
        if(typeof isCheckoutPage == 'undefined') $(".phone-field-input-step1 .phone_conf").val("");
        $(".phone-field-input-step1").slideDown(300);
        $(".phone-field-input .error-block").html("");
    });
    $('body').on('click', '.phone-field-input-step1 input', function (){
        if(!$('#g-recaptcha1').html()) renderCaptcha();
    });

    $('body').on('click', '.phone-field .send-code', function (){
        if(!$(this).hasClass("disabled")) {
            var _this = $(this),
                phone = _this.parents(".phone-field-input").find(".phone_conf").val(),
                code = _this.parents(".phone-field-input").find(".code-input").val(),
                errorBlock = _this.parents(".phone-field-input").find(".error-block"),
                action = _this.attr("action"),
                preloader = _this.parents(".phone-field").find(".phone-preloader"),
                sendData = "";
            cropPhone = phone.replace(/[^-0-9]/gim,'');
            if((cropPhone.length == 11) || (_this.attr("action") == "confirm")) {
                errorBlock.html("");

                if(action === "get-code"){
                    if(!$('#g-recaptcha1').html()) renderCaptcha();
                    sendData = "phone="+cropPhone+"&captcha="+grecaptcha.getResponse($('#g-recaptcha1').attr("cid"));
                } else if(action === "confirm") {
                    sendData = "code="+code;
                }
                $.ajax({
                    type: "GET",
                    url: "/include/form/checkPhone.php",
                    data: "action="+action+"&"+sendData,
                    dataType: "json",
                    beforeSend:function(){
                        preloader.show();
                    },
                    success: function(data){
                        preloader.hide();
                        if(data.STATUS === "ERROR") errorBlock.html(data.MSG);
                        else if(data.STATUS === "SUCCESS") {
                            if(data.EVENT === "SMS_SENT") {
                                $(".phone-field-input-step1 .send-code").addClass("disabled");

                                if(!$(".send-again .time_countdown").text()) {
                                    $(".send-again .time_countdown").text($(".phone-field-input-step1 .send-again").attr("startcnt"));
                                    startCountdown();
                                }
                                $(".send-again").css("display","block");
                                $(".phone-field-input-step2 .phone-num").text(data.NUMBER);
                                $(".phone-field-input-step1").slideUp(300);
                                $(".phone-field-input-step2").slideDown(300);
                            } else if(data.EVENT === "CODE_CONFIRMED") {
                                $(".phone-field-input-step3 b span.phone-num").html(data.NUMBER);
                                $(".phone-field-input-step2").slideUp(300);
                                $(".phone-field-input-step3").slideDown(300);
                                if(data.UPDATE_DATA)  {
                                    if(typeof isPersonalPage != 'undefined') {
                                        $(".conf-fields--phone input").val(data.NUMBER).after("<span class=\"confirmed-msg\">Телефон подтвержден!</span>");
                                        $(".conf-fields--phone a.btn").remove();
                                    } else if(typeof isCheckoutPage != 'undefined') {
                                        var phoneInput = $('.checkout--delivery-contacts input.phone-mask'),
                                            phoneVal = $(".phone-field-input-step1 input").removeAttr("phone-not-conf").val().replace(/ /g,"");

                                        phoneInput.val(phoneVal).attr("data-num", phoneVal);

                                        $(".icon--check-icon").show();
                                        if($('.phone-confirm-number').length)
                                            $('.phone-confirm-number').remove();
                                        if ($(".checkout--delivery-contacts .phone-add-to-lk").length)
                                            $(".checkout--delivery-contacts .phone-add-to-lk").remove();
                                    }
                                }
