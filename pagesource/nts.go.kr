<script type="text/javascript">
	function changBtn(idx){
		switch(idx){
			case "1_1" : 
					$("#con03_1_1_1on").show(); 
					$("#con03_1_1_2on").hide();
					$("#con03_1_1_1 > a > img").attr("src","/images/main/blog_on.png")
					$(".con03_1_1 > .con_num_Title01 > .control").show()
					$("#con03_1_1_2 > a > img").attr("src","/images/main/fun_off.png")
			break;
			case "1_2" : 
					$("#con03_1_1_2on").show();
					$("#con03_1_1_1on").hide();
					$("#con03_1_1_2 > a > img").attr("src","/images/main/fun_on.png")
					$(".con03_1_1 > .con_num_Title01 > .control").hide()
					$("#con03_1_1_1 > a > img").attr("src","/images/main/blog_off.png")
			break;
			case "2_1" : $("#con03_1_2_1on").show(); $("#con03_1_2_2on").hide(); break;
			case "2_2" : $("#con03_1_2_2on").show(); $("#con03_1_2_1on").hide(); break;
		}
	}
	$(".museum_btn").on("click",function(e){
		var mu_idx = $(".museum_btn").index(this);
		if (mu_idx == 0){
			$("#con03_1_2_1on").show();
			$("#con03_1_2_2on").hide();
			$(".museum_btn").eq(0).find("img").attr("src","/images/main/museum_on.png");
			$(".museum_btn").eq(1).find("img").attr("src","/images/main/pb_rel_off.png");
		}else if(mu_idx == 1){
			$("#con03_1_2_2on").show();
			$("#con03_1_2_1on").hide();
			$(".museum_btn").eq(0).find("img").attr("src","/images/main/museum_off.png");
			$(".museum_btn").eq(1).find("img").attr("src","/images/main/pb_rel_on.png");
		}
	});
</script>
