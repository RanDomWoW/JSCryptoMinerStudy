/* Clear Floats
===================================*/
html body * span.clear,
html body * div.clear,
html body * li.clear,
html body * dd.clear {
background: none;
border: 0;
clear: both;
display: block;
float: none;
font-size: 0;
list-style: none;
margin: 0;
padding: 0;
overflow: hidden;
visibility: hidden;
width: 0;
height: 0}
.clearfix {
*zoom: 1}
.clearfix:before,
.clearfix:after {
display: table;
line-height: 0;
content: ""}
.clearfix:after {
clear: both}
