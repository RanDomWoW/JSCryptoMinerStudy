    <!-- adidas2017 lightbox -->
    <style>
        .adidas2017_lightbox{
            width: 100%;
            height: 100%;
            position: fixed;
            top: 0;
            left: 0;
            background: rgba(0,0,0,0.8);
            text-align: center;
            /*overflow-y: scroll;
            overflow-x: scroll;*/
            z-index: 8000;
            line-height: initial;
            font-family: "蘋果儷中黑","微軟正黑體","Microsoft JhengHei","微軟雅黑體","Microsoft YaHei",Arial;
        }
        .adidas2017_lightbox:before{
            content: '';
            width: 1px;
            height: 100%;
            display: inline-block;
            vertical-align: middle;
        }
        .adidas2017_lightbox_inner{
            display: inline-block;
            vertical-align: middle;
            background: #ecece2;
            width:800px;
            position: relative;
            padding-bottom: 40px;
            padding-top: 10px;

        }
        .adidas2017_btn_x{
            background: url('announce_2017/images/btn_x.png') no-repeat top center;
            width: 30px;
            height: 30px;
            position: absolute;
            right: -40px;
            top: 0;
            cursor: pointer;
            -webkit-transform: rotate(0deg);
            -ms-transform: rotate(0deg);
            transform: rotate(0deg);
            -webkit-transition: all .2s ease;
            -o-transition: all .2s ease;
            transition: all .2s ease;

        }
        .adidas2017_btn_x:hover{
            -webkit-transform: rotate(270deg);
            -ms-transform: rotate(270deg);
            transform: rotate(270deg);

        }
        ._lightbox_inner{
            width: 750px;
            margin: 0 auto;
            text-align: center;
        }
        ._lightbox_inner h1{
            font-size: 32px;
            color: #7e7d7d;
            margin: 10px 0 20px;

        }

        ._lightbox_inner h1 br{
            display: none;
        }


        ._lightbox_inner p{
            display: block;
            margin: 0 auto 30px;
            padding: 0 15px;
            box-sizing: border-box;
            color: #7e7d7d;
            font-size: 18px;

        }


        .adidas2017_lbbtnbox{
            width: 100%;
            font-size: 0;
        }

        .adidas2017_lbbtnbox a{
            text-align: left;
            display: inline-block;
            width: 95%;
            margin: 0 auto;
            color: #007fc7;
            line-height: 1.8;
            transition: all 0.5s;
            font-size: 18px;
            color: #fff;
            width: 32%;
            padding: 10px 40px 10px 19px;
            box-sizing: border-box;
            margin-right: 2%;
            line-height: 1.5;
            background:#7f7f7f url('announce_2017/images/btn_arr.png') no-repeat center right 10px;
            background-size: 35px;
            -webkit-transition: all .2s ease;
            -o-transition: all .2s ease;
            transition: all .2s ease;
            text-decoration: none;
        }
        .adidas2017_lbbtnbox a:hover{
            background:#b0b0b0 url('announce_2017/images/btn_arr.png') no-repeat center right 10px;
            background-size: 35px;

        }
        .adidas2017_lbbtnbox a span{
            display: inline;
            color:#ffc000;
            font-size: inherit;
        }
        .adidas2017_lbbtnbox a font{ 
            font-size: 16px;
        }
        ._lightbox_inner p {
            line-height: 1.8;
        }
         ._lightbox_inner p span{color: red; display: inline-block; font-size: 18px;}
        .adidas2017_lbbtnbox a:last-child{margin-right: 0;}
        .adidas2017_img{
            width: auto;
            margin: 0 auto;
            display: block;
        }
        @media screen and (max-width: 900px) {
            .pcOnly{ display: none; }
            .adidas2017_lightbox:before{display: none;}
            .adidas2017_lightbox_inner{
                width: 90%;
                margin-top: 20%;
            }
            .adidas2017_btn_x{
                top:-50px;
                right: 0;
            }
            ._lightbox_inner{
                width: 90%;
            }
            ._lightbox_inner h1{font-size: 23px;}
            ._lightbox_inner p{font-size: 15px;}
            ._lightbox_inner p{
                padding: 0 10px;
            }
           
            .adidas2017_lbbtnbox a{
                width: 100%;
                margin-right: 0;
                margin-bottom: 10px;
            }
        }
        @media screen and (max-width: 600px) {
            .adidas2017_lbbtnbox a{
                background:#7f7f7f url('announce_2017/images/btn_arr.png') no-repeat center right 10px;
                background-size: 20px;
                font-size: 15px;
            }
            .adidas2017_lbbtnbox a:hover{
                background:#b0b0b0 url('announce_2017/images/btn_arr.png') no-repeat center right 10px;
                background-size: 20px;

            }
            ._lightbox_inner h1 br{
                display: block;
            }


        }
        @media screen and (max-width: 480px) {
            .adidas2017_img{width: 80%;}
            .pcOnly{
                display: block;
            }
        }
        @media screen and (max-width: 360px) {
             ._lightbox_inner p{font-size: 13px;}
        }
    </style>
