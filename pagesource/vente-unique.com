    var loadMarketwave;
    var startClickMarketwave = function(){
        loadMarketwave = window.setTimeout(function() {
            var elems = document.getElementsByClassName('lil-button-redbrick');

            if(elems.length > 0)
            {
                var ele = elems[0];
                if (typeof ele.click == 'function') {
                    ele.click()
                } else if (typeof ele.onclick == 'function') {
                    ele.onclick()
                }
                endloadMarketwave();
            }
        }, 2000);
    };
    var endloadMarketwave = function(){
        clearTimeout(loadMarketwave);
    };
    var addCookieMarketwave = function(){
        jQuery.cookie('vu_marketwave', '1', {
            path: '/'
        });
    };
    var deleteVuMarketwave = function(){
        var content = document.getElementById('vu-marketwave');
        content.parentNode.removeChild(content);
    };
    var loadPluginMarketwave = function(){
        loadMarketwave = window.setTimeout(function(){
            if(typeof vuMarketwave === 'function') {
                vuMarketwave();
                endloadMarketwave();
            }
        },2000);
    };
    var vuMarketwaveOnline = function(){
        jQuery.ajax({
            type: 'GET',
            url: 'https://lc.linkinlive.com/GetState?sid=a8de7f62-153f-475b-83da-e41612979456',
            dataType: 'jsonp',
            success: function (response) {
                if (response['isChatOnline'] && response['isChatEnabled'] ) {
                    if(typeof vuMarketwave === 'function'){
                        vuMarketwave();
                        addCookieMarketwave();
                        startClickMarketwave();
                    }
                    deleteVuMarketwave();
                }else{
                    vuMarketwaveOffline();
                }
            },
            error:function()
            {
                console.log('error marketwave status');
            }
        });
    };
    var vuMarketwaveOffline = function(){
