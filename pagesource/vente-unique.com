function createDynamicTagNameByUrl(url){
    var name = '';
    var pos = '';

    pos = url.indexOf('?',0);
    if(pos > -1){
        url = url.substring(0,pos);
    }

    pos = url.indexOf('#',0);
    if(pos > -1){
        url = url.substring(0,pos);
    }

    pos = url.search('http');
    if(pos > -1){
        url = url.substring(6,url.length);
        url = url.split('.').join('_');
    }

    name = url.split('/').join('_');
    return name;
}

