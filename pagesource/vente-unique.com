    function launchAjaxTarget2Sell() {

        var json_cross = jQuery('#DBL1HRFVL220X3-1200-2').html();
        var json_similar = jQuery('#DBL1HRFVL220X3-1200-3').html();
        var json_news = jQuery('#DBL1HRFVL220X3-1000-1').html();

        // reco générique pour panier, recherche …
        var json_generic = jQuery('.t2s-reco-generic').html();

        if (json_cross || json_similar || json_news || json_generic) {

            clearInterval(interval);
            json_object_cross = json_cross ? JSON.parse(json_cross) : false;
            json_object_similar = json_similar ? JSON.parse(json_similar) : false;
            json_object_news = json_news ? JSON.parse(json_news) : false;
            json_object_generic = json_generic ? JSON.parse(json_generic) : false;

            var data = {};
            data['type'] = 'target2sell';
            data['cross'] = getDataFromJson(json_object_cross);
            data['similar'] = getDataFromJson(json_object_similar);
            data['news'] = getDataFromJson(json_object_news);
            data['generic'] = getDataFromJson(json_object_generic);

            if (json_news) {
                sendAjax(data, {'news': '#T2S-news-container'});
            }
            else if (json_cross || json_similar) {
                sendAjax(data, {'cross': '#T2S-cross-sell-container', 'similar': '#T2S-similar-container'});
            }
            else if (json_generic) {
                sendAjax(data, {'generic': '#T2S-reco-generic-container'});
            }
        }
    }

    function sendAjax(data, containers) {
        jQuery.ajax({
            type: 'POST',
            url: '/ajax/product-box',
            data: data,
            success: function (response) {

                for (var key in containers) {
                    if (response[key].length) {
                        jQuery(containers[key]).html(response[key]);
                        jQuery(containers[key]).closest('.box').show();
                    }
                }
            }
        });
    }

    function getDataFromJson(json) {
        data = {};

        if (json) {
            jQuery.each(json.blocks, function (i, e) {
                jQuery.each(e.items, function (j, item) {
                    data[item.externalID] = {
                        'id': item.externalID,
                        'position': item.po,
                        'ru': item.ru,
                        'sp': item.sp,
                        'url': item.url
                    };
                });
            });
        }

        return data;
    }

