<script>
	$( document ).ready(function() {
		
		var englishtxt = 0;
		if (window.location.pathname.split('/')[1] == 'en') {
			//set english
			$("#dionicebox h2 strong").text("Share Info");
			$("#dionicebox div table tbody tr:first th").text("Zagreb Stock Exchange");
			$("#dionicebox p:eq(0)").html("Data displayed with a 15 minute delay. Showing data from previous trading day until trading begins. Currency is HRK.");
