                                                                                $("#stockprice th").html("Cijena " + dionica.Datum);
                                                                                $("#stockprice td").html( dionica.Cijena + " kn");
                                                                }
                                                                
                                                },
                                                error:function(jqXHR, textStatus, errorThrown){
                                                                alert("Došlo je do greške kod prikaza cijene dionica.");
