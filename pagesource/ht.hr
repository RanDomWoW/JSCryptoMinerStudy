                                                dataType: "jsonp",
                                                crossDomain: true,          
                                                cache:false, 
                                                success: function(data){
                                                                /* parse JSON */
                                                                var dionica = $.parseJSON(data);
                                                                if (englishtxt == 1) {
                                                                                $("#stockprice th").html("Price " + dionica.Datum);
                                                                                $("#stockprice td").html( dionica.Cijena.replace(",", ".") + " HRK");
