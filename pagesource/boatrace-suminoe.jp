    <script>
      $(function(){
        //トップ カレンダー
        $.ajax({
          url: '/uploads/cal/tenbo_date.txt',
          cache: false,
          success: function(tenboData){

            var getDate = new Date();
            var m = '0'+(getDate.getMonth()+1);
              m = m.slice(-2);
            var d = '0'+getDate.getDate();
              d = d.slice(-2);
            var Ymd = ''+getDate.getFullYear()+m+d;
            var splitFile = tenboData.split('\n');

            var funcInAry = function(target, ary){
              var hasFlg = false;
              if( typeof(splitFile)=='object' ){
                //in_array
                ary.forEach(function(sdate, index){
                  if( sdate===target && sdate!='' ){
                    hasFlg = true;
                    return false;
                  }
                });
              }
              return hasFlg;
            }

            $.each($('a[data-type="honjo_cell"]'), function(k, v){
              var url = $(this).attr('href');
              var sdate = url.match(/\d{8}/);
              if( funcInAry(sdate[0], splitFile) ){
                $(this).removeAttr('target').attr('href', '/modules/raceinfo/?page=index_tenbo&target_day='+sdate);
              }
            });
          }
        });
      });
    </script>
