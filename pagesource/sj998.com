	        var dlgtrigger = document.querySelector( '[data-dialog]' ),
	            somedialog = document.getElementById( dlgtrigger.getAttribute( 'data-dialog' ) ),
	            dlg = new DialogFx( somedialog );
	
	        dlgtrigger.addEventListener( 'click', dlg.toggle.bind(dlg) );
	
	    })();
	
	    (function ($) {
	        'use strict';
	        $('.item').on("click", function () {
	            $(this).next().slideToggle(100);
	            $('.iteminfo').not($(this).next()).slideUp('fast');
	        });
	    }(jQuery));
	    
	    function feedAdd(){
	    	var con = $('#feedcontent').val();
	    	var info = $('#feedinfo').val();
	    	if(con == ''){
	    		alert("请输入反馈内容");  
		        return false; 
	    	}
	    	if(info == ''){
				alert('请输入手机号码');return false;
			}else if(!(/^1[34578]\d{9}$/.test(info))){ 
		        alert("手机号码有误，请重填");  
		        return false; 
		    }
	    	$.ajax({
	            type:"POST",
	            url: "http://ajax.sj998.com/?module=channel&controller=index&action=feedback",
	            data: {content:con,info:info},
	            dataType: "jsonp",
	            success: function(data) {
	            	$('#myModal').modal('hide');
	            	alert('感谢你的反馈!');
	            	$("#feedcontent").val('');
	            	$("#feedinfo").val('');
	            }
	        });
	    }
