	function autoSearch() {
        var wq = $('#wq');
        var result = $('#wq_result');


        wq.keyup(function() {
            var words = $(this).val();
            result.css('display', 'none');
			
            if(words) {
                $.ajax({
                    url : 'http://ajax.sj998.com/?action=search',
                    dataType: 'jsonp',
                    data: {q: words},
                    success: function(data) {
                        result.empty();
                        var html = '';
                        if(!$.isEmptyObject(data)) {
							html += '<form class="article-search" method="get" action="http://www.sj998.com/search/" style="height:30px;">';
							html += '<input type="text" name="key" value="'+words+'" style="display:none;"/>';
							html += '<button class="pull-right" type="submit">搜索“'+words+'”的相关文章</button>';
							html += '</form>';
							
                            html += '<ul class="unstyle">';
                            $.each(data, function(index, value) {
                                html += '<li class="clearfix">';
                                html += '<a class="pull-left" href="http://www.sj998.com/'+value['id']+'/">'+value['name']+'</a>';
                                html += '<small class="pull-right"><a class="gery" href="http://'+value['pinyin']+'.sj998.com">'+value['industryName']+'</a></small>';
                                html += '</li>';
                            });
                            html += '</ul>';
                        }
                        else {
							html += '<form class="article-search" method="get" action="http://www.sj998.com/search/">';
							html += '<input type="text" name="key" value="'+words+'" style="display:none;" />';
							html += '<button class="pull-right" type="submit">搜索“'+words+'”的相关文章</button>';
							html += '</form>';
                            html += '<h3>未找到相关项目</h3>';
                        }

                        result.append(html).css('display', 'block');
                    }
                });
            }
        });

        $('body').click(function() {
            result.css('display', 'none');
        });
    }
</script>	
