    var timeout_var;
	function set_sub_menu_hover(id)
	{
		$('#hd_sub_menu_id').val(id);
		var sub_menu_id = $('#hd_sub_menu_id').val();

		if(sub_menu_id != "")
			$('#' + sub_menu_id).removeClass().addClass("sub-left-menu");

		var left_sub_cat_html = $('#left_sub_cat_'+id).val();
		if(left_sub_cat_html == "") {
			$.ajax({
				type: "POST",
				url:  site_url + "category_page/get_left_sub_sub_menu/" + id,
				async: false,
				success: function(html) {
					var obj = html.split('$*#*$');
					html = obj[0];
					var desc = obj[1];
					if(html != "0")
					{
						$('#left_sub_cat_'+id).val(html);
						$('#right-part-detail-pro1').css("display","block");
						$('#right-part-detail-pro1').html(html);
						$('#right-part-detail-pro2').css("display","none");
						$('#right-part-detail-pro3').css("display","none");
						$('#right-part-detail-pro4').css("display","none");
						$('#data #right-part-detail-pro2').css("display","none");
						if(desc.trim()!=''){
							$('.cat-desc').show().html(desc);
						}else{
							$('.cat-desc').hide();
						}
						cat_desc.push(id.toString()+'####'+desc.toString());
					}
				}
			});
		}
		else
		{
			$('#right-part-detail-pro1').css("display","block");
			$('#right-part-detail-pro1').html(left_sub_cat_html);
			$('#right-part-detail-pro2').css("display","none");
			$('#right-part-detail-pro3').css("display","none");
			$('#right-part-detail-pro4').css("display","none");
			$('#data #right-part-detail-pro2').css("display","none");
			$('.cat-desc').html(cat_desc);
			//to show cat description
			for(var i=0; i<=cat_desc.length-1; i++) {
			   var str = cat_desc[i].toString();
			   str = str.split('####');
			   var catid = parseInt(str[0]);
			   var catdesc = str[1];
			   if(catid==id){
			   		if(catdesc.trim()!=''){
			   			$('.cat-desc').show().html(catdesc);
					}else{
						$('.cat-desc').hide();
					}
			   		break;
			   }
			}
		}
		apply_breadcrumb();
	}

