    <script type="text/javascript">
        function static_submenu(get_sub_menu_hover_id, get_menu_hover_id)
        {
            if (get_sub_menu_hover_id != "" && get_menu_hover_id != 8)
            {

                $("#sub-mega-hover-" + get_sub_menu_hover_id).removeClass().addClass("sub-mega-hover");
                $("#mega-hover-" + get_menu_hover_id + " ul").each(function () {
                    if ($(this).attr("class") == "sub")
                    {
                        $(this).attr('id', 'selectedul');
                        $(this).css('padding-top', 5);
                        $('#mega-hover-' + get_menu_hover_id).removeClass().addClass("dc-mega-li");
                    }
                });

                $('#selectedul').attr('style', "display: block;");

            }
        }
        function common_func_for_menu(get_sub_menu_hover_id, get_menu_hover_id)
        {
            if (get_menu_hover_id != "" && get_menu_hover_id != 8)
            {
                $('#top_menu_div').removeClass().addClass("grey");
                $('.mega-menu').attr("id", 'mega-menu-10');
                $('.first').css('height', '');
                static_submenu(get_sub_menu_hover_id, get_menu_hover_id);
            }
        }
        $(document).ready(function () {

            var get_menu_hover_id = $('#hd_get_menu_id').val();
            var get_sub_menu_hover_id = $('#hd_get_sub_menu_id').val();

            if (get_menu_hover_id != "" && get_menu_hover_id != 8)
            {
                if (get_sub_menu_hover_id == "")
                    get_sub_menu_hover_id = get_menu_hover_id;
                $('#top_menu_div').removeClass().addClass("grey");
                $('.mega-menu').attr("id", 'mega-menu-10');
                static_submenu(get_sub_menu_hover_id, get_menu_hover_id);
            }
            $(".sub_menu_link").mouseenter(function () {

                $('.sub_menu_link_image').hide();
                var li_id = $(this).attr("id");

                if (li_id != "")
                {
                    li_id = "li_" + li_id;
                    $('#' + li_id).show();
                    $('.history-part-img').css('padding', '-10px');
                    if (li_id == 'li_sub-menu-hover-6') {
                        load_catalogs_page();
                    }
                }
                var get_parent_class_name = li_id.replace("li_sub-", "");
                var get_menu_parent_id = get_parent_class_name;
                get_parent_class_name = get_parent_class_name.replace("menu", "mega");
                get_parent_class_name = $('#' + get_parent_class_name).attr('class');
                if (get_menu_hover_id != "")
                    $('#selectedul').attr('style', "display: block;");
                if (get_menu_hover_id != "" && get_parent_class_name != "dc-mega-li" && get_menu_hover_id != 8)
                {
                    $('#top_menu_div').removeClass().addClass("grey");
                    $('.mega-menu').attr("id", 'mega-menu-10');
                    $(".dc-mega").attr('style', "");
                    $('.first').css('height', '');
                } else
                {
                    get_menu_parent_id = get_menu_parent_id.replace("menu", "mega");
                    $('.mega-menu').attr("id", 'mega-menu-9');
                    $('#top_menu_div').removeClass().addClass("white");
                    $('#selectedul').attr('style', "display: none;");
                    $("#mega-hover-" + get_menu_parent_id).removeClass().addClass("dc-mega-li sub-mega-hover");
                    $(".dc-mega").attr('style', "height:40px;");
                    $('.first').css('height', '503px');
                }
            });

            var timeout_top_menu_var;
            function set_delay_top_menu(li_id)
            {
                $('.sub_menu_link_image').hide();
                if (li_id != "")
                {
                    li_id = "li_" + li_id;
                    $('#' + li_id).show();
                    $('#' + li_id).css('padding', 0);
                }
            }
            $(".all_sub_menu_link").mouseenter(function () {
                var li_id = $(this).attr("id");
                timeout_top_menu_var = setTimeout(function () {
                    set_delay_top_menu(li_id)
                }, 500);
            });
            $(".all_sub_menu_link").mouseleave(function () {
                clearTimeout(timeout_top_menu_var);
            });

            $(".dc-mega-li").mouseenter(function () {

                var id = $(this).attr("id");
                var top_sub_menu_cnt = $('#hd_top_sub_menu_cnt').val();
                var top_sel_sub_menu = $('#hd_top_sub_menu_sel_id').val();
                var anchor_tag = 'sub-' + id;
                anchor_tag = anchor_tag.replace("mega", "menu");

                if (top_sub_menu_cnt)
                {
                    var cur_id = id.replace("mega-hover-", "");
                    if (get_menu_hover_id != "")
                        $('#selectedul').attr('style', "display: block;");
                    if (top_sel_sub_menu == cur_id && get_menu_hover_id != 8)
                    {
                        $('#top_menu_div').removeClass().addClass("grey");
                        $('.mega-menu').attr("id", 'mega-menu-10');
                        if ($("#" + anchor_tag))
                            $("#" + anchor_tag).attr('style', "color: #7D1518 !important; font-weight:bold");
                        $('.first').css('height', '');
                        if (get_menu_hover_id != "")
                        {
                            $("#mega-hover-" + get_menu_hover_id).removeClass().addClass("dc-mega-li mega-hover");
                            $('#selectedul').attr('style', "display: block;");
                        }
                    } else
                    {
                        $('.mega-menu').attr("id", 'mega-menu-9');
                        $('#top_menu_div').removeClass().addClass("white");
                        if ($("#" + anchor_tag))
                            $("#" + anchor_tag).attr('style', "height:40px;color: #7D1518 !important; font-weight:bold");
                        $('.first').css('height', '503px');
                        $('#selectedul').attr('style', "display: none;");
                        $("#mega-hover-" + id).removeClass().addClass("dc-mega-li sub-mega-hover");
                    }
                }

            })

            $("#top_menu_div").mouseleave(function () {
                $(".dc-mega").attr('style', "");
                common_func_for_menu(get_sub_menu_hover_id, get_menu_hover_id);
            });

            $(".media-part").mouseleave(function () {
                common_func_for_menu(get_sub_menu_hover_id, get_menu_hover_id);
            });

            $(".detail-part").mouseleave(function () {
                common_func_for_menu(get_sub_menu_hover_id, get_menu_hover_id);
            });

            $(".box").mouseenter(function () {
                common_func_for_menu(get_sub_menu_hover_id, get_menu_hover_id);
            });

            $(".cont-bottom").mouseenter(function () {
                common_func_for_menu(get_sub_menu_hover_id, get_menu_hover_id);
            });

            $(".history-part-img").mouseenter(function () {
                common_func_for_menu(get_sub_menu_hover_id, get_menu_hover_id);
            });

            $(".right-part-detail-pro").mouseenter(function () {
                common_func_for_menu(get_sub_menu_hover_id, get_menu_hover_id);
            });
        });
        function load_catalogs_page() {
                    $.post('https://www.rothco.com/catalogs_gallery', {}, function (result) {
                        $('#catalogs_data').html(result);
                    });
            }
    </script>
