            $.post('https://www.rothco.com/ajax/set_express',{
                express: $('#express').prop('checked')
            });
        });

        $('#text_item').keypress(function(e) {
            if(e.which == 13)
            {
                search_item();
                return false;
            }
            else
            {
                if ((e.which >= 48 && e.which <= 57) || (e.which >= 65 && e.which <= 90) || (e.which >= 97 && e.which <= 122) || e.which == 32 || e.which == 8 || e.which == 45 || e.which == 95 || e.which == 0)
                {
                    if(e.ctrlKey)
                    {
                        e.preventDefault();
                        return false;
                    }
                }
                else {
                    e.preventDefault();
                    return false;
                }
            }
        });

        $('#text_keyword').keypress(function(e) {
            if(e.which == 13)
            {
                search_keyword();
                return false;
            }
            else
            {
                if ((e.which >= 48 && e.which <= 57) || (e.which >= 65 && e.which <= 90) || (e.which >= 97 && e.which <= 122) || e.which == 32 || e.which == 8 || e.which == 45 || e.which == 95 || e.which == 0)
                {
                    if(e.ctrlKey)
                    {
                        e.preventDefault();
                        return false;
                    }
                }
                else {
                    e.preventDefault();
                    return false;
                }
            }
        });
    });

    function search_item()
    {
        if($("#text_item").val() != "")
        {
            var url = site_url+'search2/item/'+$("#text_item").val();
            $.post(url,{},function(new_url){
                if(new_url != 'Not Found') {
                    location.href = new_url;
                }
                else {
                    alert('Item Number Could Not be Found! To see restricted items please log into your dealer account first.')
                }
            });
        }
    }

    function search_keyword()
    {
        if($("#text_keyword").val() != "")
        {
            location.href = site_url+'search/keyword/'+$("#text_keyword").val();
        }
    }

</script>


