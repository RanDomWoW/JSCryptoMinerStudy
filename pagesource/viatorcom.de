      $.ajax({
        url: '/de/7132/content/account/ajax-signOut.jspa?a=' + Math.random(),
        cache: false,
        success: function (data) {
          
          if (location.href.indexOf('/profile/') > 0 || (location.href.indexOf('/content/account/') > 0 && location.href.indexOf('/SSL/') < 0 ) || location.href.lastIndexOf('userProfileEdit.jspa') > 0) {
            var nickname = '';
            location.href = 'https://www.viatorcom.de/de/7132/profile//' + nickname; 
          } else if(location.href.indexOf('/SSL/') > 0) {
            location.href = getHomePage();
          } else if (location.href.indexOf('/customer-care') > 0) {
            location.href = 'https://www.viatorcom.de/customer-care';
          } else if (location.href.indexOf('/booking/checkout') > 0) {
            location.href= getHomePage();
          } else if (location.href.indexOf('/booking/shopping-cart') > 0) {
            location.href= getHomePage();     
          } else {
            location.reload();
          }
        }
      }); 
    });



  });

