		$(function(){	

			var count = 0;

			var fdc = 0;

			function Inf(Increase){

				$("body").removeAttr("class");

				$("body").attr("class",Increase);

			}

			$('#inf').click( function(){

				count += 1; 

				fdc=0;

				if(count==1){    

					Inf("inf");

				}

				if(count==2){

					Inf("inf-1");

				}

				if(count==3){

					Inf("inf-2");

				}

			})

			$('#dcf').click( function(){

				count = 0; 

				fdc += 1; 

				

				if(fdc==1){

					Inf("dcf");

				}

				if(fdc==2){

					Inf("dcf-1");

				}

				if(fdc==3){

					Inf("dcf-2");

				}

			})

			$('#nrf').click( function(){

				count = 0; 

				fdc = 0; 

				$("body").removeAttr("class");

