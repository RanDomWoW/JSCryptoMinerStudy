		});
	});
	
	$("#sorgubtn").click(function(){
		var cvb= $('input[name=queryans]:checked').val();
		$("#sorgudiv").css("cursor","wait");
		$("#span_qr").text("Nəticələr");
		if(cvb>0)
		{
			$.ajax({
				  url: "/az/ajax/sorgu",
				  type: "GET",
				  data: {'cvb':cvb},
				  success: function (data) {
						$("#sorgucvb").html(data);
						$("#sorgucvb").css("display","block");
						$("#sorgudiv").css("cursor","pointer");
					}
					// ,
					// error: function (jqXHR, textStatus, errorThrown) {
						// alert(textStatus + ': ' + errorThrown);
					// }
				});
		} else{alert("Heç bir cavab seçilməyib");}
	});
	
	$("#inputsrcEX").on('input',function(e){
	  // alert($(this).val());
	   var text=$(this).val().toLowerCase();
	   var stext="";
	   $(".datalistEX li").css("display","none");
	   if(text.length>0){$(".datalistEX").css("display","block");}
	   $( ".datalistEX li" ).each(function() {
	      stext=" "+$(this).text().toLowerCase();
		  if(stext.search(text)>0)
		  {
			$(this).css("display","block");
		  }
		});
	});
	$("#silEX").click(function(){
		$(".datalistEX").css("display","none");
		$("#inputsrcEX").text("");
	});
	
