			$("#span_qr").text("Nəticələr");
			$("#sorgucvb").css("display","none");
		} else{
			$("#sorgudiv").css("cursor","wait");
			$.ajax({
				  url: "/az/ajax/sorgucvb",
				  type: "GET",
				  data: {'queryid':queryid},
				  success: function (data) {
						$("#sorgucvb").html(data);
						$("#sorgucvb").css("display","block");
						$("#span_qr").text("Nəticəni gizlət");
						$("#sorgudiv").css("cursor","pointer");
					}
				});
		}
	}
</script>

