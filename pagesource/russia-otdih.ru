<script>
function head_sanatorium_list_get(id) {
	var getList = function(data) {
		if (data.length > 0) {
			$('#health-center-shadow').remove();
			$('#health-center').addClass('remove-health');
			$('#health-center').before('<select name="health-center" id="health-center" class="wSelect-el"></select>');
			$('.remove-health').next().remove();
			$('.remove-health').remove();
			$('#health-center').append(data).wSelect();
			$(".wSelect-options").mCustomScrollbar({
				scrollButtons: {
					enable: true
				}
			});
		}
	};
	$('#health-center + .wSelect').append('<div id="health-center-shadow" style="background:rgba(0, 0, 0, 0.4); ' +
		'position: absolute; top: 0px; left:0px; ' +
		'width: 100%; height:100%; z-index: 1; border-radius: 4px;"></div>');
	$.ajax({
		url: '/ajax/head_second_sanatorii_select.php',
		dataType: 'text',
		method: 'POST',
		data: 'SECTION_ID=' + id,
		success: getList
	});
}
$(document).ready(function() {
	$('#zone').change(function() {
		head_sanatorium_list_get($(this).val());
	});
	$('#head_sanatorium_btn').click(function() {
		window.location.href = $('#health-center').val();
	});
});
</script>
