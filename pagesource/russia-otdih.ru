<script>
$( "#slider-range" ).slider({
    range: true,
    min: 0,
    max: 22000,
    stop: function(event, ui) {
        jQuery(".costmin").val(jQuery("#slider-range").slider("values",0));
        jQuery(".costmax").val(jQuery("#slider-range").slider("values",1));
    },
    values: [ 0, 
              22000 ],
    slide: function( event, ui ) {
        $(".placement-block .costmin").val('От '+ui.values[0]);
        $(".placement-block .costmax").val('До '+ui.values[1]);
    },
    change: function( event, ui ) {
        $(".placement-block .costmin").val('От '+ui.values[0]);
        $(".placement-block .costmax").val('До '+ui.values[1]);
    }
});

$(".placement-block .costmin").val('От '+$("#slider-range").slider("values", 0));
$(".placement-block .costmax").val('До '+$("#slider-range").slider("values", 1));
$('#set_filter').click(function(){
    var tempicus = $(".placement-block .costmin").val().replace('От ','');
    $(".placement-block .costmin").val(tempicus);
    tempicus = $(".placement-block .costmax").val().replace('До ','');
    $(".placement-block .costmax").val(tempicus);
});
$(".placement-block .costmin").change(function(){
    var value1=$(".placement-block .costmin").val().replace(/[^0-9]/g,'');
    var value2=$(".placement-block .costmax").val().replace(/[^0-9]/g,'');
    if(parseInt(value1) > parseInt(value2)){
        value1 = value2;
        $(".placement-block .costmin").val('От '+value1);
    }
    $("#slider-range").slider("values",0,value1);    
});
$(".placement-block .costmax").change(function(){
    var value1=$(".placement-block .costmin").val().replace(/[^0-9]/g,'');
    var value2=$(".placement-block .costmax").val().replace(/[^0-9]/g,'');
    if(parseInt(value1) > parseInt(value2)){
        value2 = value1;
        $(".placement-block .costmax").val('До '+value2);
    }



$("#slider-range").slider("values",1,value2); 
});
</script>



