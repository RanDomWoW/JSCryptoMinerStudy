var callouts = jQuery('div.wpicture');
function removeWhiteSpace(){
  for(var i = 0; i < callouts.length; i++){
    var childrenCount = jQuery(callouts[i]).find('div.views-field-field-image div.field-content').children().length;

    if (callouts[i])
    if (childrenCount === 0) {
      jQuery(callouts[i]).css('padding-top', '70px');
    }
  }
}
jQuery(window).resize(function(){
  if (jQuery(window).width() < 420){
    if (!jQuery('body').hasClass('media')){
      removeWhiteSpace();
    }
  }
});



