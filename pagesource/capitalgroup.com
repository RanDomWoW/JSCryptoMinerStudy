<style>
	#cg-navigate-away-modal{
		max-width: 460px;
	}
	#cg-navigate-away-modal .link_cancel, #cg-navigate-away-modal .link_accept{
		font: 14px/18px "Avenir Next LT W01 Demi",Arial,sans-serif; 
		padding: 9px 15px;  
		text-align: center;
		cursor: pointer;
		margin: 5px;
	}
	#cg-navigate-away-modal .link_cancel{
		background-color: #99d7f2;
		background-image: linear-gradient(to bottom, #ccebf9, #99d7f2);
		color: #000 !important;  
		display: inline-block;
		text-transform: uppercase;
		border: 1px solid #99d7f2;
	}
	#cg-navigate-away-modal .link_accept:hover{
		background-color: #66c2ec !important;
		background-image: none !important;
	}
	#cg-navigate-away-modal .link_cancel:hover{
		background-color: #ccebf9 !important;
		background-image: none !important;
	}
	#cg-navigate-away-modal .link_accept{
		background-color: #009adf;
		background-image: linear-gradient(to bottom, #33aee5, #009adf);
		color: #fff !important;  
		display: inline-block;
		text-transform: uppercase;
		border: 1px solid #009adf;
	}
	#cg-navigate-away-modal .caption{
		padding: 10px;
	    text-align: right;
	}
	#cg-navigate-away-modal .modal-spacing{
		overflow-x: hidden;
		overflow-y: auto;
		max-height: 250px;
		padding-right: 10px;
	}
</style> 
<script>
	$(function(){
		$("a[data-navigate-away]").click(function(e){
			var linkWithAlert = $(this);
			e.preventDefault();
			$.ajax({
				  url: "/bin/notificationdialogservlet?path=" + linkWithAlert.attr("data-navigate-away"),
					success: function(r){
						var response = r.data;
						var modal = $("#cg-navigate-away-modal");
						$(".ns-modal-header h3", modal).html(response.title);
						$(".modal-spacing", modal).html(response.text);
						$(".link_cancel", modal).html(response.cancelBtnText);
						$(".link_accept", modal).html(response.acceptBtnText);
                   		modal.modal("show");
                   		$(".ns-modal-backdrop").off();
        				$(".ns-modal-backdrop").height($(document).height()); 
                   		
						$(".link_cancel", modal).click(function(){
			            	modal.modal("hide");
			            	$(".link_accept", modal).unbind();
			            });
						
						$(".ns-modal-header a", modal).click(function(){
							$(".link_accept", modal).unbind();
						});
						
		                $(".link_accept", modal).click(function(){
		                	if(linkWithAlert.attr("target") == "_blank"){
		                		window.open (linkWithAlert.attr("href"));
		                		modal.modal("hide");
		                	}else{
		                    	window.location = linkWithAlert.attr("href");
		                	}
		                });
					}
				});
		});
	});
</script> 
