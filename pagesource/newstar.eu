<script type="text/javascript">
var haTimer;
var haTimeout = 3000;
var haBaseTimeout = 3000;
var haItems = new Array();
function initAnimation()
{
var oAll = document.getElementById("homeanimationitems")
if(oAll != "")
{
var oAllContent = oAll.getElementsByTagName("div");
for(var i=0; i<oAllContent.length; i++)
{
if(oAllContent[i].className == "homeanimationitem")
{
haItems.push(oAllContent[i]);
}
}
}
}
function showNextAnimationContent()
{
var oShow = null;
for(var i=0; i<haItems.length; i++)
{
if(haItems[i].className == "homeanimationitem active")
{
if(i < haItems.length - 1)
oShow = haItems[i+1];
else
oShow = haItems[0];
break;
}
}
if(oShow == null && haItems.length > 0)
oShow = haItems[0];
if(oShow != null)
showAnimationContent2(oShow, haBaseTimeout);
}
function showAnimationContent(cId, timeout)
{
showAnimationContent2(document.getElementById(cId), timeout);
}
function showAnimationContent2(oEl, timeout)
{
window.clearTimeout(haTimer);
haTimeout = timeout;
if(oEl != null)
{
for(var i=0; i<haItems.length; i++)
{
if(haItems[i]== oEl)
{
haItems[i].className = "homeanimationitem active";
}
else
{
haItems[i].className = "homeanimationitem";
}
}
}
haTimer = window.setTimeout("showNextAnimationContent()", haTimeout)
}
initAnimation();
showNextAnimationContent();
</script>
