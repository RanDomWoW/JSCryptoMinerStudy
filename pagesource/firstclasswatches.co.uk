    var lbp = document.getElementById("local_pop");
    if (lbg) lbg.style.display = 'block';
    if (lbp) lbp.style.display = 'block';
    function currencyOverride(value,reload) {
      var cookie = (value) ? 'true' : 'false';
      document.cookie="currencyOverride="+cookie+";path=/";
      var lbg = document.getElementById("local_background");
      var lbp = document.getElementById("local_pop");
      if (lbg) lbg.style.display = 'none';
      if (lbp) lbp.style.display = 'none';
      if (reload) window.location.reload();
    }
