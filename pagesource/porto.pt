					var flag_x = true; var w = 0;
					$(function(){
						$(window).load(function(){
							setTimeout(function(){
								$("#block_383 li").css({float:"left",padding:"0 5px"});
							
								
								$("#block_383 li").each(function(){
									w += $(this).outerWidth() ;
