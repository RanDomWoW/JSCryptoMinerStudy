							$("#block_383").hover(function(){
								flag_x = false; 
							},
							function()
							{
								flag_x = true; 
								ticker_x();
							});
						});
					});
					
					function ticker_x()
					{
						$("#block_383 ul").animate({left:"-=5px"},100,"linear",function(){
							if(parseInt($(this).css("left")) * -1 > $(this).width() )
								$(this).remove();
							else
							{
								if(parseInt($(this).css("left")) * -1 > ($(this).width() - $(this).parent().width()) && $("#block_383 ul").length == 1)
								{
									new_ul(this);
								}
								
								$(this).stop();
								if(flag_x == true)
									ticker_x();
							}
						});

					}
					
					function new_ul(thi)
					{
						
						var ul = $("<ul></ul>");
						ul.html($(thi).html());
						ul.css({position:"absolute",width:w+"px",left:$(thi).parent().width()});
						$("#block_383 div").append(ul);
						
					}
