		(function(){

			var c = document.querySelectorAll('#block_582 .categories span');
			for (var i = 0; i < c.length; i++)
				c[i].onclick = function()
				{
					if(this.classList.contains('active'))
						this.classList.remove('active');
					else
						this.classList.add('active');

					get_list_off_events_by_categories();
				}

			inicialize_calender();
		
			document.querySelector('.thisWeekend').onclick 	= get_events_next_weekend;
			document.querySelector('.thisMonth').onclick 	= get_events_this_month;

		})();

		/*************************************************
		* INICIALIZAR O CALENDARIO COM OS EVENTOS
		*************************************************/
		function inicialize_calender()
		{			
			calendar = $('.clndr-master').clndr(
			{
				template: clndrTemplate,
				events:   es,
				clickEvents: 
				{
					click: function(target) 
					{
						day 	= target.date._i;	

						cat = [];
						var c = document.querySelectorAll('#block_582 .categories span.active');
						for (var i = 0; i < c.length; i++)
							cat.push(c[i].getAttribute('cat')); 

						window.location = 'http://www.porto.pt/agenda?day='+day+'&cat='+cat;
					}
				},
				multiDayEvents: { startDate: 'start', endDate:   'end' },
				showAdjacentMonths: 	 true,
				adjacentDaysChangeMonth: false,
								daysOfTheWeek: [ 'Dom', 'Seg', 'Ter', 'Qua', 'Qui', 'Sex', 'Sab'],
							});		
		}

		/*************************************************
		* SELECIONAR PROXIMO FIM DE SENAMA
		*************************************************/
		function get_events_next_weekend()
		{
			start 	= '2018-03-23';
			end 	= '2018-03-25';

			cat = [];
			var c = document.querySelectorAll('#block_582 .categories span.active');
			for (var i = 0; i < c.length; i++)
				cat.push(c[i].getAttribute('cat')); 

			window.location = 'http://www.porto.pt/agenda?day='+start+','+end+'&cat='+cat;	
		}

		/*************************************************
		* SELECIONAR ESTE MES
		*************************************************/
		function get_events_this_month()
		{
			start 	= '2018-03-01';
			end 	= '2018-03-31';

			cat = [];
			var c = document.querySelectorAll('#block_582 .categories span.active');
			for (var i = 0; i < c.length; i++)
				cat.push(c[i].getAttribute('cat')); 


			window.location = 'http://www.porto.pt/agenda?day='+start+','+end+'&cat='+cat;
		}

		/*************************************************
		* OBTER EVENTOS POR CATEGORIA
		*************************************************/
		function get_list_off_events_by_categories( )
		{
			delay(function(){
				cat = [];

				var c = document.querySelectorAll('#block_582 .categories span.active');
				for (var i = 0; i < c.length; i++)
					cat.push(c[i].getAttribute('cat')); 

				$.post(url_app + "ajax/call/objects_api/cmp_agenda/list_by_categories/582", "cat="+cat, function(response)
	            {            	
	            	calendar.setEvents(response);
	            },'JSON');	
			},1000);
		}

	</script>

