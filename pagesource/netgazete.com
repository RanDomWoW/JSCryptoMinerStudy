<script type="text/javascript">
var _sm_timeSwap=6000;
var _sm_counter=1;
var surmansetTimeout = null;
var _sm_item_count=5;

function surmanset_degistir(){
    $item = "#item_"+_sm_counter;
    var item2 = "item_"+_sm_counter;
    //console.log($item);
    //$($item).animate({opacity:1},100);
    $($item).fadeIn(100);
    $('#surmanset .item').each(function(){
        if($(this).attr('id')!=item2) {
            //$(this).animate({'opacity':0},100);
            $(this).fadeOut(100);
        }
    });
    $('#surmanset .sm_item').each(function(){
        $(this).removeClass('sm_selected');
    });
    $('#smpg_'+_sm_counter).addClass('sm_selected');
    _sm_counter++;
    if(_sm_counter>_sm_item_count) _sm_counter=1;
}
$(document).ready(function(){
    surmansetTimeout = setInterval("surmanset_degistir()",_sm_timeSwap);
    _sm_item_count = $('#surmanset_pager .sm_item').length;
    
    $('#surmanset .item').mouseenter(function(){
        _sm_counter= $(this).attr("data-index");
        clearInterval(surmansetTimeout);
    }).mouseleave(function(){
        surmansetTimeout = setInterval("surmanset_degistir()",_sm_timeSwap);
    });
    $('#surmanset .sm_item').mouseenter(function(){
        //console.log($(this).attr("data-index"));
        _sm_counter= $(this).attr("data-index");
        $item = "#item_"+$(this).attr("data-index");
        item2 = "item_"+$(this).attr("data-index");
        //$($item).animate({opacity:1},100);
        $($item).fadeIn(100);
        $('#surmanset .item').each(function(){
            if($(this).attr('id')!=item2) {
                //$(this).animate({'opacity':0},100);
                $(this).fadeOut(100);
            }
        });
        $('#surmanset .sm_item').each(function(){
            $(this).removeClass('sm_selected');
        });
        $('#smpg_'+_sm_counter).addClass('sm_selected');        
        clearInterval(surmansetTimeout);
    }).mouseleave(function(){
        surmansetTimeout = setInterval("surmanset_degistir()",_sm_timeSwap);
    });
    
    if(sayfa_id=="reklam_test" || 1==1){
        xajax_load_ads_special();
        if(sayfa_id=='bilgilendirme_sayfa') xajax_load_ads_special('ng','detay');
    }
});
</script>


