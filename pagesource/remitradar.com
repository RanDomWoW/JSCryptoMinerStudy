                if ($(window).width() < 991) {
                $('.m-nav .nav-main').animate({ left: "15px" });
                $('.m-nav').animate({ opacity: "1" });
                $('.m-close').animate({ opacity: "1" });
                $('#topHead').hide();
                }


            });

            $('.m-mob').on('click', function () {
                var winsize = $(window).width();
                if (winsize < 991) {
                    var isopen = $(this).hasClass('mopen');
                    if (!isopen) {
                        $(this).find('ul').css('display', 'block');
                        $(this).addClass('mopen');
                    } else {
                        $(this).find('ul').css('display', 'none');
                        $(this).removeClass('mopen');
                    }
                }


            });
        });
    </script>


    


    

