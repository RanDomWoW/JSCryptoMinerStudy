<script>

    function formatCountry(country) {

        if (!country.id) {
            return country.text;
        }
        var $country = $(
            '<span class="glyphicon flag flag-' + country.element.value.toLowerCase() + ' img-flag"></span><span>' + country.text + '</span>'
        );
        return $country;
    };

    function country_from_init() {

        $("#country-from")
        .on("select2:open", function () {

            $(".select2-results").addClass('nano');
            $(".select2-results__options").addClass('nano-content');

            $(".nano").nanoScroller({ alwaysVisible: true, iOSNativeScrolling: true });

            setTimeout(function () {
                $(".nano").nanoScroller({ alwaysVisible: true, iOSNativeScrolling: true });
            }, 500);
        });
    };

    function country_to_init() {

        $("#country-to")
        .on("select2:open", function () {

            $(".select2-results").addClass('nano');
            $(".select2-results__options").addClass('nano-content');

            $(".nano").nanoScroller({ alwaysVisible: true, iOSNativeScrolling: true });

            setTimeout(function () {
                $(".nano").nanoScroller({ alwaysVisible: true, iOSNativeScrolling: true });
            }, 500);
        });

    };

    // Select2 Note Start: The following code uses select2.full.min.js plugin and it make sure that when you search country name you match the letters form the beginning of the word only for select tag with id #country-from and #country-to

    function matchStart(term, text) {
        if (text.toUpperCase().indexOf(term.toUpperCase()) == 0) {
            return true;
        }

        return false;
    }

    $.fn.select2.amd.require(['select2/compat/matcher'], function (oldMatcher) {
        $("#country-from").select2({
            matcher: oldMatcher(matchStart),
            templateResult: formatCountry,
            templateSelection: formatCountry
        })
    });

    $.fn.select2.amd.require(['select2/compat/matcher'], function (oldMatcher) {
        $("#country-to").select2({
            matcher: oldMatcher(matchStart),
            templateResult: formatCountry,
            templateSelection: formatCountry
        })
    });

    // Select2 Note End

    country_from_init();

    country_to_init();

    $('#currency-from').selectpicker();

    $('#currency-to').selectpicker();



</script>

