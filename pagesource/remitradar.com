    <script>
        //Check which page we are on to add active class to the correct #topMain li
        
        $(function () {
            $('ul#topMain > li').each(function () {
                var isActive = "";
                $(this).find('a').each(function () {
                    if ($(this).attr('href') == window.location.pathname) {
                        isActive = "active";
                        return false;
                    }
                });

                if (isActive != "") {
                    $(this).addClass(isActive);
                    return false;
                }
            });
        });
    </script>


