    $(document).ready(function () {

        var menu = $('.navSection');
        var origOffsetY = menu.offset().top;
        var inShrinkMode = false;
        
        function scroll() {
            if ($(window).scrollTop() >= origOffsetY) {
                if (!inShrinkMode)
                {
                    $('.navSection').addClass('navbar-fixed-top');
                    $('.bodySection').addClass('siteContent-navbar-padding');
                    $('.navbarBrand').show();
                    //$('.secondary-navbar').hide();
                    $('.smartbarContainer').fadeIn("slow");
                    inShrinkMode = true;
                }
            } else {
                if(inShrinkMode)
                {
                    $('.navSection').removeClass('navbar-fixed-top');
                    $('.bodySection').removeClass('siteContent-navbar-padding');
                    $('.navbarBrand').hide();
                    //$('.secondary-navbar').show();
                    $('.smartbarContainer').hide();
                    inShrinkMode = false;
                }
            }


        }

        document.onscroll = scroll;

        wanderlustWebsite.contentPinBoard.totalItemsInBoard();

        //function that vertically aligns the ad slots once ads have been rendered
        googletag.cmd.push(function () {
            googletag.pubads().addEventListener('slotRenderEnded', function (event) {
                $('.adSlot>div').each(function (index) {
                    var adSlotHeight = $(this).parent().height();
                    var adHeight = $(this).height();
                    var padding = (adSlotHeight / 2) - (adHeight / 2);
                    $(this).css("padding-top", padding);
                });
            });
        });

        $(".socialPanel").jsSocials({
            showLabel: false,
            shareIn: "popup",
