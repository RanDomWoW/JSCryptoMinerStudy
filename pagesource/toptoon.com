$(document).on('click',function(){
	try
	{
		var arr = ['.mymenu_layer','.search_layer'],
			$target = $(event.target);
		$.each(arr,function(i,n){
			if (typeof($(n)) != "undefined" && $(n).css('display') != "none" && $target.closest(n).length <= 0) $(n).hide();
		});
	}
	catch (e)
	{
	}
});
