<input type="hidden" id="arr_gift_comic_idx" value="">
<script type="text/javascript" src="/assets/js/sugar.min.js"></script>
<script>
$(document).ready(function(){
	// 리스트 ajax 호출
	var data_recent_view	= localStorage.getItem("ep_data");
	if (data_recent_view !== null)
	{
		data_recent_view = JSON.parse(data_recent_view);
		var arr_recent_comic_id = [];
		$.each(data_recent_view,function(i,n){
			n.d = Sugar.Date.format(Sugar.Date.create(n.d), '%Y-%m-%d %H:%M:%S');
			arr_recent_comic_id.push(n.c);
		});

		// 인기순위 탭 내가본 만화 뱃지 체크
		var comic_id;
		$('#rank_slide').find('li').each(function () {
			var _this = $(this);
			comic_id= _this.data('id');

			if ($.inArray(comic_id, arr_recent_comic_id) > -1) {
                if(_this.find('.span_badge').length <= 0) {
					if(_this.find('.span_badge').find('span').length > 3) {
						var _target = _this.find('.span_badge').find('span').first();
						_target.remove();
					}
				    _this.find('.span_badge').append('<span class="renewal1711_txtbadge viewcomic"></span>');
                }
			}
		});
	}
	/* 업데이트 작품 노출 */
	else {
		$.ajax({
			url : "/main/get_update_comic",
			type : "POST",
			dataType : "JSON",
			cache : true,
			async : true,
			success : function(data){

				// 이용권 체크
                var arr_gift_comic_idx = $('#arr_gift_comic_idx').val().split(',');

                var html = '';
                var adult_badge = '';
                var gap = '';
                var badge = '';
                var badge1 = '';
                var author = '';
                $.each(data, function (key, val) {
                    if (key == 5) {
                        return false;
                    }

                    badge = '';
                    if (val.ep_last_upd =='1') {
                        if(val.ep_last_upd_type=='96') badge += '';
                        else if(val.ep_last_upd_type=='97') badge += '';
                        else badge += '<span class="renewal1711_txtbadge up2"></span>';
                    }

                    author = '';
					$.each(val.new_author_list, function (key1, val1) {
						if (key1 == 0) {
							author += val1.name;
						} else {
							author += ', ' + val1.name;
						}
					});

                    badge1 = '';
                    if (val.comic_icon&4) badge1 += '<span class="renewal1711_txtbadge recommend"></span> ';
                    if ($.inArray(val.comic_idx, arr_gift_comic_idx) > -1) badge1 += '<span class="renewal1711_txtbadge freeticket"></span>';

                    adult_badge = '';
                    if (val.adult_status == 1) {
                        adult_badge = '<span class="renewal1711_icon_adult19_bthumb"></span>';
                    }

                    html += '<li>';
                    html += '   <a href="/comic/ep_list/'+ val.comic_id +'">';
                    html += '       <div class="list_thumb" style="background-image:url('+ val.img_url_square +');">';
                    html += adult_badge;
                    html += '       </div>';
                    html += '       <div class="list_comic_info">';
                    html += '           <p class="comic_tit"><span class="tit_name">'+ val.comic_name + '</span></p>' + badge;
                    html += '           <p class="comic_episode">'+ val.episode_title +'</p>';
                    html += '           <p class="bottom_info">';
                    html += '               <span class="comic_lastviewday">'+ author +'</span>';
                    html += '               <span class="comic_badge">';
                    html += badge1;
                    html += '               </span>';
                    html += '           </p>';
                    html += '       </div>';
                    html += '   </a>';
                    html += '</li>';
                });
				if (html != '') {
					var today_week = $('#today_week').val();
                    $('#my_recent_update_desc').text('오늘 업데이트 된 작품을 확인하세요');
                    $('#my_recent_update_title').text('업데이트 작품');
                    $('#my_recent_view').html(html);
                    myrecent_view_child();
					$('#my_recent_div').show();
				}

			}
		});
    }

    //키워드 전체보기 처리
    $('.all_btn').click(function(){
        var target = $('.keyword_list li a.active');
        var idx = target.parent('li').data('idx');

        location.href = '/hashtag/' + idx;
        return false;
    });

    //키워드 선택 처리
    $('.keyword_list li').click(function(){
        var _this = $(this);
        var idx = _this.data('idx');
        var url = '/main/get_index_hash_comics_list/' + idx;

        $('.keyword_list li a.active').removeClass('active');
        _this.find('a').addClass('active');

        $.ajax({
            url : url,
            dataType : "JSON",
            cache : true,
            async : true,
            success : function(data){
                // 이용권 체크
                var arr_gift_comic_idx = $('#arr_gift_comic_idx').val().split(',');

                var html = '';
                var adult_badge = '';
                var gap = '';
                var badge = '';
                var badge1 = '';
                $.each(data, function (key, val) {
                    badge = '';
                    if(val.ep_last_upd =='1') {
                        if(val.ep_last_upd_type=='96') badge += '';
                        else if(val.ep_last_upd_type=='97') badge += '';
                        else badge += '<span class="renewal1711_txtbadge up2"></span>';
                    }

                    badge1 = '';
                    if(val.comic_type&32) badge1 += '<span class="renewal1711_txtbadge ico_end"></span> ';
                    if(val.comic_icon&4) badge1 += '<span class="renewal1711_txtbadge recommend"></span> ';
                    if($.inArray(val.comic_idx, arr_gift_comic_idx) > -1) badge1 += '<span class="renewal1711_txtbadge freeticket"></span>';

                    adult_badge = '';
                    
                    author = '';
					$.each(val.new_author_list, function (key1, val1) {
                        if(key1 == 0) author += val1.name;
                        else author += '&' + val1.name;
                    });

                    if(key == 0) html += '<li style="margin-left: 0px;">';
                    else html += '<li>';
                    html += '   <a href="/comic/ep_list/'+ val.comic_id +'">';
                    html += '       <div class="list_thumb" style="background-image:url('+ val.img_url_17_square +');">';
                    html += adult_badge;
                    html += '       </div>';
                    html += '       <div class="list_comic_info">';
                    html += '           <p class="comic_tit"><span class="tit_name">'+ val.comic_name + '</span></p>' + badge;
                    html += '           <p class="comic_episode">'+ val.episode_name +'</p>';
                    html += '           <p class="bottom_info">';
                    html += '               <span class="comic_lastviewday">'+ author +'</span>';
                    html += '               <span class="comic_badge">';
                    html += badge1;
                    html += '               </span>';
                    html += '           </p>';
                    html += '       </div>';
                    html += '   </a>';
                    html += '</li>';
                });
                if(html != '') {
                    $('#hash_comics_list').html(html);
                }

            }
        });
    });
});
</script>
