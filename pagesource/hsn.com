</div>
<script>
	(function($){
		var _tsInit = function(){
			var isMobile = !!$('body').is('.mobile'),
				isApp = !!$('body').is('.appPhone,.appTablet'),
				//sold out copy etc
				_soldout = {
					copy:'<span>SEE ALL OUR TOP DEALS</span>Hurry - good things come to those who shop fast.',
					cm: 'features-_-tswidget-_-shopdeals',
					href: '/shop/deals/dc'
				},
				//get the parent container
				parent = $('._autoTS'),
				//get the item container
				container = $('._autoTS ._in ._bot'),
				//get the number of choices container
				choice = $('._autoTS ._in ._top ._sub span span'),
				//storage for toplines
				toplines = {},
				//get the item
				_clone = container.find('a'),
				//clone cell
				cell,
				//one item
				isOne = false,
				//get the cm
				cm = _clone.attr('cm'),
				//if debugging sold out/not sold out and tsemail data
				debug = {
					//forcing multi items
					mt:(/(&|\?)tsm=[1-9]+/i).test(window.location.search),
					//forcing sold out
					so:(/(&|\?)tsso=[1-9]+/i).test(window.location.search),
					//forcing sold out on even items
					so2:(/(&|\?)tsso=2/i).test(window.location.search),
					//forcing not sold out
					nso:(/(&|\?)tsnso=[1-9]+/i).test(window.location.search)
				},
				tsemail = {
					//if this is the ts email page
					is:(/\/content\/.*\/333/i).test(window.location.pathname),
					//the link to use for the ts email page
					href:'/shop/todaysspecial/6198'
				},
				//ajax request data
				data = {},
				init = function(){
					if((/preview_dt=/i).test(window.location.search)){
						data.preview_dt = decodeURIComponent(window.location.search.replace(/(.*)(preview_dt=)([^%+]+)(.*)/i,'$3').replace(/\//g,'-'));
					}
					//remove the cm attr from the clone
					_clone.removeAttr('cm');
					//get items
					$(document).ready(function(){
						_doAjax();
					});
				},
				//build the carousel
				_doAjax = function(){
					$.ajax({
						url: '/api/product/feeds/ts',
						type:'GET',
						data:data,
						beforeSend: function (xhr) {
							xhr.setRequestHeader('X-Requested-With', { toString: function() { return ''; } } );
						},
						success: function(data){
							//if this is a multi-item debug, add the first item again
							if(!!debug.mt){
								data.Products.Products.push(data.Products.Products[0]);
								//console.debug(data.Products.Products);
							}
							//number of items
							var l = (!!data.Products && data.Products.Products.length) || 0;
							//display the correct version
							switch(true){
								case(l == 1):
									//if only one item
									parent.addClass('_one');
									isOne = true;
								break;
								case(l == 0):
									//if no items, hide the container
									parent.addClass('_hide');
									return 0;
								break;
								default:
									//if multi items, set the choice text
									parent.addClass('_multi');
									choice.html(l);
								break;
							}
							$.each(data.Products.Products, function(i,v) {
								//if this item has already been added or is a TDO, decrement number of items, skip
								//-using toplines because of multiple matrix items with different item numbers for the same product but same toplines showing up in the ts feed
								if((!!toplines[v.TopLine] && !debug.mt) || !!((/this.*day.*only/gi).test(JSON.stringify(v.Callouts)))){
									l--;
									return 1;
								}
								//store this topline in the obj so that future toplines can be checked against it
								toplines[v.TopLine] = true;
								//if the flag is sold out and we're not forcing not sold out, or we are forcing sold out on every other item and this is an even item, or we are forcing sold out on every item
								var soldout = (!!v.Flag && !!v.Flag.IsSoldOut && !debug.nso)
										|| (!!debug.so2 && i%2 == 0)
										|| (!debug.so2 && !!debug.so),
									//replace any &nbsp; with an actual space so wrapping works correctly
									topline = v.TopLine.replace(/\s/g,' '),
									//create the prodfull/grid/car image link, apps do not return #RECIPE# and use prodgrid
									img = v.ImageLink.Uri.replace('#RECIPE#',
										((!!tsemail.is && 'prodgrid')
											|| (!!isMobile && 'prodcar')
											|| 'prodfull')),
									//create the prodthumb image link by replacing prodfull/grid/car with prodthumb, works on apps
									thumb = img.replace(/prod(grid|full|car)/,'prodthumb'),
									//the alt text, encode double quotes so the html is not messed up
									alt = (topline.replace(/"/g,'&quot;')),
									//the price
									price = v.Pricing.Tier1,
									//the relative url, or the /todays-special link for just one item
									url = (!isOne && v.ProductLink.Uri.replace(/http(s)*:\/\/.*hsn\.(com|net)/,'')) || '/todays-special',
									//if there is a space in the price, then it's a price range
									isRange = !!(/\s/g).test(price),
									//get the flexpay copy and replace payment with FlexPay
									flex = (v.Flexpay || '').replace(/payment/i,'FlexPay');
								
								//create a clone of the cell
								cell = _clone.clone()
									//remove the native-link class for the apps so that the urls will be converted properly
									.removeClass('native-link')
									//set the url
									.attr('href',url)
									//set the CMs
									.attr('manual_cm_re',cm+v.Identity)
									.attr('manual_cm_sp',cm+v.Identity)
									//set the product id for the modal
									.attr('data-product-id',v.Identity)
									//set the preview url for the modal
									.attr('data-preview-url',url)
									//set the data-lazy image path, the thumbnail image path, and the alt text, use .end() to go back one and make cell equal the actual cell, and not the img
									.find('img')
										.attr('data-lazy',img)
										.attr('data-thumb',thumb)
										.attr('title',alt)
										.attr('alt',alt).end();

								if(!soldout){
									//if the item is not sold out, put in the topline, the note, the price, and the flex copy
									cell.find('.tl').html(topline).end()
										.find('.note span').html(v.Note || '').end()
										.find('.price').html(price).end()
										.find('.flex').html(flex);
									//if this is a price range, add the class to the cell, handles the font-size of the price so that the price fits into the container and does not look goofy
									(!!isRange && cell.addClass('_range'));
								} else {
									//else the item is sold out, mark the cell as sold out, fill in the topline, the sold out copy, set the link and CMs to the sold out link
									cell
										.addClass('_soldout')
										.find('.tl').html(topline).end()
										.find('.note span').html(_soldout.copy).end()
										.closest('a')
											.attr('href',_soldout.href)
											.attr('manual_cm_re',_soldout.cm)
											.attr('manual_cm_sp',_soldout.cm);
								}
								//add the cell to the carousel
								cell.appendTo(container);
							});
							//if there ends up only being one item because others were duplicates, set the body class and the url
							if(l == 1){
								//set the parent to use the styles for one item and not many
								parent.addClass('_one').removeClass('_multi');
								//set the link to /today-special if it's not sold out, else keep the already set link
								container.find('a').attr('href',function(){
									return (!(/_soldout/).test(this.className) && '/todays-special') || this.attributes.href.value;
								});
								//set isOne to true
								isOne = true;
							}
							//build the carousel
							_doCC();
						}
					});
				},
				_toggleSoldOut = function(slick,slide){
					//toggle the _soldout class on the container to handle the button copy for sold out items
					container[(((/_soldout/).test(slick.$slides[slide].className) && 'add') || 'remove')+'Class']('_soldout');
				},
				_doCC = function(){
					//if this is not the ts email page, create the carousel
					if(!tsemail.is){
						//if there are multi items, set the pd modal click event for desktop only
						if(!isOne){
							$('body:not(.mobile):not(.appTablet)').find(container)
								.on('click','.pd-modal:not(._soldout)',function(ev){
									ev.preventDefault();
									ev.stopPropagation();
									HSN.Instance().GetProductModal().Show($(this).attr('data-product-id'),$(this).attr('data-preview-url')+'/preview');
									return false;
								});
						}
						//create the carousel
						window._tsCC = container
							//remove the first link because it was the source of the clone and is empty
							.find('a:first').remove().end()
							//on init, check to see if the first item is sold out, and toggle the class on the container
							.on('init',function(ev,slick){
								_toggleSoldOut(slick,slick.currentSlide);
							})
							//on beforeChange, check to see if the next item is sold out, and toggle the class on the container
							.on('beforeChange', function(ev, slick, currentSlide, nextSlide){
								_toggleSoldOut(slick,nextSlide);
							})
							//create the carousel
							.slick({
								slidesToShow:1,
								slidesToScroll:1,
								arrows: false,
								fade: true,
								cssEase:'ease-in-out',
								speed:300,
								autoplaySpeed: 3000,
								infinite: true,
								lazyLoad: 'ondemand',
								dots: !isOne,
								appendDots:'._autoTS ._in ._top ._thumb',
								dotsClass:'pager-dots',
								customPaging: function(slick,i){
									//use the thumbnails as the pager dots for multi items
									//console.log('>>>ts pager: '+i);
									//console.debug(slick);
									var btn = slick.defaults.customPaging(slick,i),
										txt = '', img = $();
									//console.debug(btn);
									//i('<button type="button" />').text(t+1)
									if(!isOne){
										img = $(slick.$slides[i]).find('img').clone().css('opacity','')
											.attr('src',function(){ return $(this).attr('data-thumb'); });
										//txt = '<img src="'+img.attr('data-thumb')+'" alt="'+img.attr('alt')+'" title="'+img.attr('title')+'">';
									}
									//return '<div class="_dot">'+txt+'</div>';
									return btn.html('').append(img);
								}
							});
						
						//function for toggling autoplay
						window._stopTS = function(go){
							container.slick('slickSetOption','autoplay',(!!go || false),true);
						};
						//if there are multi items
						if(!isOne){
							//handle tsemail data-href
							(!!tsemail.is && container.closest('.cms-cell').attr('data-href',tsemail.href));
							//wait before starting the autoplay for non TS email pages
							setTimeout(function(){
								container.slick('slickSetOption','autoplay',!tsemail.is,true);
							},2000);
						}
					} else {
						//else it is the ts email page, no carousel
						//console.log('>>>> this is the ts email page:');
						
						//remove the first link because it was the source of the clone and is empty
						container.find('a:first').remove().end()
							//show image
							.find('[data-lazy]').attr('src',function(){ return $(this).attr('data-lazy'); });
						//add/remove soldout on container as necessary
						container[((!!container.find('._soldout').length && 'add') || 'remove')+'Class']('_soldout');

						//if there are multi items, add the thumbnail pager
						if(!!parent.is('._multi')){
							var _ul = $('<ul class="pager-dots" />').appendTo(parent.find('._thumb'));
							$.each(container.find('[data-thumb]'),function(i,v){
								_ul.append('<li'+((i == 0 && ' class="slick-active"') || '')+'>'
										+'<button type="button">'
											+'<img src="'+$(v).attr('data-thumb')+'" + alt="'+$(v).attr('topline')+'">'
										+'</button>'
									+'</li>')
							});
						}
					}
					
					//show the element
					container.closest('._autoTS').addClass('_done');
					
					//handle relative to absolute links on apps
					if(!!isApp){
						HSN.Instance().ConvertLinks();
					}
				};
			//fire init
			init();
		};
		//start the process
		_tsInit();
	})(jQuery);
</script>

