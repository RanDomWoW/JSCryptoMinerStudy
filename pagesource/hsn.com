				_cm2 = (_zone != '' && _zone.replace(/\d+/,_num[parseInt(_zone.replace(/[^\d]/g,''))])) || _carousel.attr('data-cm2') || 'cm2',
				init = function(){
					$('body:not(.mobile):not(.appTablet)').find(_container)
						.on('click','.pd-modal',function(ev){
							ev.preventDefault();
							ev.stopPropagation();
							HSN.Instance().GetProductModal().Show($(this).attr('data-product-id'),$(this).attr('data-preview-url')+'/preview');
							return false;
						});
					$(document).ready(function(){
						getF2Items();
					});
				},
				getF2Items = function(){
					doAjax('/api/product/list/'+_api,{take:(itemCount+1)},function(data,ts,xhr){
						var url, flag = '', topline = '', t = 0, recipe = (!!isMobile && 'prodcar') || 'prodgrid', imgAttr = 'data-lazy';
						$.each(data.Products.Products, function(i,v) {
							//-skip the TS, but still put 3 items in the carousel
							if((t >= itemCount)
								|| (typeof(v.Flag) != 'undefined' && !!v.Flag.IsTodaysSpecial)
								|| (/image-coming-soon/i).test(v.ImageLink.Uri)){
								return 1;
							}
							t++;
							flag = (typeof(v.Flag) != 'undefined' && '<div class="violator '+v.Flag.ClassName+'"></div>') || '';
							topline = (v.TopLine || '').replace(/\s/g,' ');
							url = v.ProductLink.Uri.replace(/http(s)*:\/\/.*hsn\.(com|net)/,'');
							_carousel.append('<div class="dbftItem">'
									 +'<a class="pd-modal zItem'+((flag != '' && ' violated') || '')+'" href="'+url+'" '
										+'manual_cm_re="'+_cm1+'-_-'+_cm2+'-_-product_'+v.Identity+'" '
										+'manual_cm_sp="'+_cm1+'-_-'+_cm2+'-_-product_'+v.Identity+'" '
										+'data-preview-url="'+url+'" '
										+'data-product-id="'+v.Identity+'">'
										+'<span class="dbftImg-o">'
											+'<img class="dbftImg" '+imgAttr+'="'+v.ImageLink.Uri.replace("#RECIPE#", recipe)+'" alt="'+topline+'" title="'+topline+'">'
										+'</span>'
									+'</a>'
								+'</div>');
						});
						doCarousel();
					});
				},
				doAjax = function(url,data,callback){
					$.ajax({
						url: url,
						type: 'GET',
						data: (data || {}),
						beforeSend: function (xhr) {
							xhr.setRequestHeader('X-Requested-With', { toString: function() { return ''; } } );
						},
						success: callback || (function(){}),
						error: function(ts){ window.console && window.console.error('>>> f3 doAjax error:'); window.console && window.console.debug(ts); }
					});
				},
				doCarousel = function(){
					window._f3CC = _carousel
						.on('beforeChange._f3c', function(event, slick, 
							currentSlide, nextSlide){
							//workaround for slick autoplay clone data-lazy issue
							//if we are wrapping around to the first slide again
							if(nextSlide == 0){
								//get the img src of that slide
								var src = $(slick.$slides[nextSlide]).find('[src]').attr('src');
								//find the clone that has that img and set src, remove extra attrs
								$(slick.$slider)
									.find('.slick-cloned [data-lazy="'+src+'"]')
									.attr('src',src)
									.removeAttr('data-lazy data-srcset data-sizes').end()
									//stop firing the beforechange event
									.off('beforeChange._f3c');
							}
						})
						.slick({
							slidesToShow:slides,
							slidesToScroll:slides,
							appendArrows:'._feature3 ._space_carousel ._dbft-zc ._controls',
