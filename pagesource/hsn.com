		}
		$(window).on('load', function() { 		
			if(window.location.href.indexOf('keynote=true') <= -1) {
				$('#YouTubeVideoPlayer').show();
			}		
		});
		
		//IE11 / Windows 7 Video Fix
		var isIE11 = !!window.MSInputMethodContext && !!document.documentMode;
		if (navigator.userAgent.indexOf('Windows NT 6.1') !=-1 && (isIE11 == true)) {
		   $('#yt-ov-msg').show();
		}
		
		//jquery plugin for checking if an element is in the viewport
		$.fn.onscreen = function() {
			var top = $(this).offset().top,
				bottom = top + $(this).outerHeight(),
				vTop = $(window).scrollTop(),
				vBottom = vTop + $(window).height();
			return bottom > vTop && top < vBottom;
		};
		//nav clicks
		$('#home-page-on-air-now-items-recently-aired ._nav-outer').on('click', 'a:not(.active)',function(ev) {
			ev.preventDefault();
			//make this option active and not the other one
			$(this).addClass('active')
				.closest('._nav-outer')
					.find('a').not($(this)).removeClass('active');
			
			//toggle playing of videos, start playing the other one
			//-do this before adding the active class so the state change event listener does not try to flag this as a user event
			$('.oan-yt').each(function(){
				var d = $(this).data('oan-player');
				switch(true){
					//if this is not mobile and the user did not stop the video previously and this is the video that is about to be on the screen, play it
					case(!isMobile && !d.__noplay && !$(this).hasClass('active')):
						d.playVideo();
					break;
					//else this is the other video, pause
					case(!!$(this).hasClass('active')):
						d.pauseVideo();
					break;
				}
			});

			//hide the elements and show the correct ones
			$('#home-page-on-air-now-items-recently-aired [data-oan-feed]')
				.removeClass('active')
					.filter('[data-oan-feed="'+$(this).attr('data-oan-id')+'"]')
					.addClass('active');
			
			return false;
		});

		//carousel array
		window._iraCC = [];
		
		var isMobile = !!$('body').is('.mobile'),
			//init function
			_recentInit = function(obj){
				(typeof(obj) != 'object') ? obj = {} : '';

				var count = (!!isMobile && 4) || 6;  
				//ajax call to get items
				$.ajax({
					url: '/api/product/feeds/'+obj.feed,
					type: 'GET',
					data: { take:13 },
					beforeSend: function (xhr) {
						xhr.setRequestHeader('X-Requested-With', { toString: function() { return ''; } } );
					},
					success: function(data,ts,xhr){
						var v = data.Products.Products.shift(),
							container = obj.carousel,
							url = v.ProductLink.Uri.replace(/http(s)*:\/\/.*hsn\.(com|net)/,''),
							mobile = !!$('body').is('.mobile'),
							recipe = (!!mobile && 'prodcar') || 'prodgrid',
							slides = 1;
						//remove the first item and put it into the on air now
						obj.oanimg.add(obj.oanbtn)
							.find('a')
								.attr("data-preview-url", url)
								.attr("data-product-id", v.Identity)
								.attr("href", url)
								.attr("manual_cm_re", "recommendations-_-"+obj.cm2+"-_-product_" + v.Identity)
								.attr("manual_cm_sp", "recommendations-_-"+obj.cm2+"-_-product_" + v.Identity)
							.end().find('img')
								.attr("src", v.ImageLink.Uri.replace("#RECIPE#", recipe))
								.attr("alt",v.TopLine.replace(/"/g,'&quot;'));
						obj.oancopy
							.find('span._onairnow_description').html(v.TopLine)
							.end().find('span._onairnow_price').html(v.Pricing.Tier1)
							.end().find('span._onairnow_price span').html(v.Pricing.Tier2)
							.end().find('a')
								.attr("data-preview-url", url)
								.attr("data-product-id", v.Identity)
								.attr("href", url)
								.attr("manual_cm_re", "recommendations-_-"+obj.cm2+"-_-product_" + v.Identity)
								.attr("manual_cm_sp", "recommendations-_-"+obj.cm2+"-_-product_" + v.Identity);

						//put the remaining products into the carousel
						$.each(data.Products.Products, function(i,v) {
							if(i%count == 0){
								container = $('<div class="mcItem-c" ></div>').appendTo(obj.carousel);
							}
							url = v.ProductLink.Uri.replace(/http(s)*:\/\/.*hsn\.(com|net)/,'');
							container.append('<a class="product-detail-modal mcItem-c" href="'+url+'" '
											+'manual_cm_re="recommendations-_-'+obj.cm2+'-_-product_'+v.Identity+'" '
											+'manual_cm_sp="recommendations-_-'+obj.cm2+'-_-product_'+v.Identity+'" '
											+'data-preview-url="'+url+'" '
											+'data-product-id="'+v.Identity+'">'
										+'<img class="zImg" data-lazy="'+v.ImageLink.Uri.replace("#RECIPE#", "prodcar")+'" alt="'+(v.TopLine.replace(/"/g,'&quot;'))+'" title="'+(v.TopLine.replace(/"/g,'&quot;'))+'">'
									+'</a>');
						});
						
						window._iraCC.push(obj.carousel
							.on('beforeChange._ira', function(event, slick, 
								currentSlide, nextSlide){
								//workaround for slick clone data-lazy issue
								//if we are wrapping around to the first slide again
								if(nextSlide == 0){
									//get the img src of that slide
									$.each($(slick.$slides[nextSlide]).find('[src]'),function(i,v){
										//get the src of the img
									 	var src = $(v).attr('src');
										//find the clone that has that img and set src, remove extra attrs
										$(slick.$slider)
											.find('.slick-cloned [data-lazy="'+src+'"]')
											.attr('src',src)
											.removeAttr('data-lazy data-srcset data-sizes');
									});
									//stop firing the beforechange event
									$(slick.$slider).off('beforeChange._ira');
								}
							})
							.slick({
								slidesToShow:slides,
								slidesToScroll:slides,
								appendArrows:obj.controls+' ._ccra ._controls',
								dots: true,
								appendDots:obj.controls+' ._ccra ._pager',
								dotsClass:'pager-dots',
								infinite: true,
								lazyLoad: 'ondemand'
							}));
					}
				});
			};//func
		$(document).ready(function(){
			//init hsn feed
			_recentInit({
				feed:'RecentlyOnAirNow1',
				carousel:$('._recently-aired ._ccra ._carousel'),
				oanimg:$('._recently-aired ._c._oa'),
				oancopy:$('._hsn_onairnow_container ._onairnow-data'),
