<script>
	(function($){
		var _trendingInit = function(){
			var isMobile = !!$('body').is('.mobile'),
				itemCount = ((!!isMobile && 6) || 20),
				slides = (!!isMobile && 2) || 4,
				init = function(){
					$('body:not(.mobile):not(.appTablet) ._hp ._homepage-product-carousel')
						.on('click','.pd-modal',function(ev){
							ev.preventDefault();
							ev.stopPropagation();
							HSN.Instance().GetProductModal().Show($(this).attr('data-product-id'),$(this).attr('data-preview-url')+'/preview');
							return false;
						});
					$(document).ready(function(){
						getTrendingItems();
					});
				},
				getTrendingItems = function(){
					//get the hot items from the attribute and put them into the _hi-items container
					doAjax('/api/product/list/9574',{take:(itemCount+1)},function(data,ts,xhr){
						var container = $('._homepage-product-carousel ._hp-pc ._carousel'), url, flag = '', topline = '', t = 0, recipe = (!!isMobile && 'prodfull') || 'prodfull', imgAttr = 'data-lazy';
						$.each(data.Products.Products, function(i,v) {
							//-skip the TS, but still put 9 or 10 items in the carousel
							if((t >= itemCount)
								|| (typeof(v.Flag) != 'undefined' && !!v.Flag.IsTodaysSpecial)
								|| (/image-coming-soon/i).test(v.ImageLink.Uri)){
								return 1;
							}
							t++;
							flag = (typeof(v.Flag) != 'undefined' && '<div class="violator '+v.Flag.ClassName+' ir"></div>') || '';
							topline = (v.TopLine || '').replace(/\s/g,' ');
							url = v.ProductLink.Uri.replace(/http(s)*:\/\/.*hsn\.(com|net)/,'');
							container.append('<div class="cItem">'
									+'<a class="pd-modal zItem'+((flag != '' && ' violated') || '')+'" href="'+url+'" '
											+'manual_cm_re="recommendations-_-homepageproductcarousel-_-product_'+v.Identity+'" '
											+'manual_cm_sp="recommendations-_-homepageproductcarousel-_-product_'+v.Identity+'" '
											+'data-preview-url="'+url+'" '
											+'data-product-id="'+v.Identity+'">'
										+flag
										+'<span class="zImg-o">'
											+'<img class="zImg" '+imgAttr+'="'+v.ImageLink.Uri.replace("#RECIPE#", recipe)+'" alt="'+topline+'">'
										+'</span>'
										+((!isMobile &&
											'<span class="_copy">'+topline+'</span>'
											+'<span class="_price-o'+(((/\s/).test(v.Pricing.Tier1) && typeof(v.Pricing.Tier2) != 'undefined' && ' _s') || '')+'">'
												+'<span class="_price">'
													+'<span class="_now">'+v.Pricing.Tier1+'</span>'
													+((typeof(v.Pricing.Tier2) != 'undefined' && '<span class="_reg"><span>'+v.Pricing.Tier2+'</span></span>') || '')
												+'</span>'
												+'<span class="_cta">BUY NOW</span>'
											+'</span>') || '')
									+'</a>'
								+'</div>');
						});
						doCarousel();
					});
				},
				doAjax = function(url,data,callback){
					$.ajax({
						url: url,
						type: 'GET',
						data: (data || {}),
						beforeSend: function (xhr) {
							xhr.setRequestHeader('X-Requested-With', { toString: function() { return ''; } } );
						},
						success: callback || (function(){}),
						error: function(ts){
							//window.console && window.console.error('>>> trending doAjax error:'); window.console && window.console.debug(ts);
						}
					});
				},
				doCarousel = function(){
					window._trendingCC = $('._homepage-product-carousel ._hp-pc ._carousel')
						.on('beforeChange._trending', function(event, slick, 
								currentSlide, nextSlide){
							//workaround for slick clone data-lazy issue
							//if we are wrapping around to the first slide again
							if(nextSlide == 0){
								//get the img src of that slide
								$.each($(slick.$slides[nextSlide]).add($(slick.$slides[nextSlide]).nextUntil('[data-slick-index="'+slick.options.slidesToScroll+'"]')).find('[src]'),function(i,v){
									//get the src of the img
									var src = $(v).attr('src');
									//find the clone that has that img and set src, remove extra attrs
									$(slick.$slider)
										.find('.slick-cloned [data-lazy="'+src+'"]')
										.attr('src',src)
										.removeAttr('data-lazy data-srcset data-sizes');
								});
								//stop firing the beforechange event
								$(slick.$slider).off('beforeChange._trending');
							}
						})
						.slick({
							slidesToShow:slides,
							slidesToScroll:slides,
							appendArrows:'._homepage-product-carousel ._hp-pc ._controls',
							dots: true,
							appendDots:'._homepage-product-carousel ._hp-pc ._pager',
							dotsClass:'pager-dots',
							infinite: true,
							autoplay: true,
							lazyLoad: 'ondemand'
						});
				};
			init();
		}//func
		_trendingInit();
	})(jQuery);
</script>


