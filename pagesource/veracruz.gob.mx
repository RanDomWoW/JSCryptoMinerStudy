					$('.date-select').addClass('hide');
				break;
			}
		}); 
		
		 $('.search-form').submit(function(){
			if($('.search-field').val() == ''){
				if($('.input-cat-selected').val()!='fecha'){	
					$('.search-field').attr("placeholder", "Favor de definir un criterio de busqueda");
					return false;	
				}else{
					$('.search-field').val(' ');
				}
