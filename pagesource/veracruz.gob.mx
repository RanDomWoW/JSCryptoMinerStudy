				if($('.search-field').val() == 'fecha'){	
					$('.search-field').val(' ');
					$('.search-form').submit();
				}else{
					$('.search-form').submit();
				}
			}
		});
		
		$('.input-daterange').datepicker({
			todayBtn: "linked",
			format: "yyyy-mm-dd"	
		});
	});
