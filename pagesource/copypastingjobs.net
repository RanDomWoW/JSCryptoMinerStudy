<script language="javascript">
    function calcPrice() {
        var adPrice = 3.00;
        var noLinks 	= $("#no_links").val();
        var workingHrs = $("#workinghrs").val();
        var earningPerDay = noLinks * adPrice;
        var earningPerMonth = earningPerDay  * 30;
        if(earningPerDay == '' || earningPerDay == 0) {
            $("#earningPerDay").val(0);
            $("#earningPerMonth").val(0);
        } else {
            $("#earningPerDay").val(earningPerDay);
            $("#earningPerMonth").val(earningPerMonth);
        }
    }
</script> 
    
