<script>
  $(document).ready(function(){
	//Check for email availability
  	$("#email_availability").hide();
	$("#email").blur(function()
	{
		var email = $("#email").val();
		$("#email_availability").hide();
		if(email.length > 3)
		{
			$("#email_availability").show();
			$("#email_availability").html('<font color="Green">Checking... </font>');
			$.ajax({  //Make the Ajax Request
				type: "POST", //Method
				url: "http://copypastingjobs.net/check_available.php",  //file name
				data: 'email='+ email +'&action=email',  //data
				success: function(server_response){
					$("#email_availability").html(server_response);
					if(server_response.indexOf("SUCCESS") == -1){
						$("#email_availability").html('<font color="red" class="errorFields">Email already exist.</font>');
					}else if(server_response.indexOf("ERROR") == -1){
						$("#email_availability").html('<font color="Green"> Available</font>');
					}
				}
			});
		}
		else
		{
			$("#email_availability").html('<font color="#cc0000" class="errorFields">Email Id is  too short</font>');
		}
	return false;
	});
	
	//Check for mobile availability
  	$("#mobile_availability").hide();
	$("#mobile").blur(function()
	{
		var mobile = $("#mobile").val();
		var phone = $("#phone").val();
		$("#mobile_availability").hide();
		if(mobile.length == 10)
		{
			$("#mobile_availability").show();
			$("#mobile_availability").html('<font color="Green">Checking... </font>');
			$.ajax({  //Make the Ajax Request
				type: "POST", //Method
				url: "http://copypastingjobs.net/check_available.php",  //file name
				data: 'mobile='+ mobile+'&phone='+ phone + '&action=mobile',  //data
				success: function(server_response){
					$("#mobile_availability").html(server_response);
					if(server_response=="SUCCESS"){
						$("#mobile_availability").html('<font color="Green"> Available</font>');
						
						
					}else if(server_response=="ERROR"){
						$("#mobile_availability").html('<font color="red" class="errorFields">Mobile number already exist.</font>');
					}
					else if(server_response=="BOTH"){
					//	$("#mobile_availability").html('<font color="red" class="errorFields">Both mobile number is same.</font>');
					}
				}
				
			});
		}
		else
		{
			$("#mobile_availability").html('<font color="#cc0000" class="errorFields">Mobile No is  too short</font>');
		}
	return false;
	});
	
	
	//Check for phone availability
  	$("#phone_availability").hide();
	$("#phone").blur(function()
	{
		var phone = $("#phone").val();
		var mobile = $("#mobile").val();
			
		$("#phone_availability").hide();
		if(phone.length == 10)
		{
			$("#phone_availability").show();
			$("#phone_availability").html('<font color="Green">Checking... </font>');
			$.ajax({  //Make the Ajax Request
				type: "POST", //Method
				url: "http://copypastingjobs.net/check_available.php",  //file name
				data: 'phone='+ phone+"&mobile="+mobile +'&action=phone',  //data
				success: function(server_response){
					$("#phone_availability").html(server_response);
					if(server_response=="SUCCESS"){
						$("#phone_availability").html('<font color="Green"> Available</font>');
					}else if(server_response =="ERROR"){
							$("#phone_availability").html('<font color="red" class="errorFields">Mobile number already exist.</font>');	
					}else if(server_response=="BOTH"){
						//$("#phone_availability").html('<font color="red" class="errorFields">Both mobile number is same.</font>');
					}
				}
			});
		}
		else
		{
			$("#phone_availability").html('<font color="#cc0000" class="errorFields">Mobile No is  too short</font>');
		}
	return false;
	});
	
  	$("#pincode_availability").hide();
	
	$("#pincode").blur(function()
	{
		var pincode = $("#pincode").val();
		$("#pincode_availability").hide();
		if(pincode.length == 6)
		{
			//$("#pincode_availability").show();
			//$("#pincode_availability").html('<font color="Green">Checking... </font>');
			$.ajax({  //Make the Ajax Request
				type: "POST", //Method
				url: "http://copypastingjobs.net/check_available.php",  //file name
				data: 'pincode='+ pincode +'&action=pincode',  //data
				success: function(server_response){
					//$("#pincode_availability").html(server_response);
					if(server_response.indexOf("SUCCESS") == -1){
