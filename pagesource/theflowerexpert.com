     							var optionGroupElement = document.createElement("optgroup");
				    			optionGroupElement.label = browseCategoryElements[i].getElementsByTagName("BrowseCategoryName")[0].childNodes[0].nodeValue;
				    			var childCategoryElements = browseCategoryElements[i].getElementsByTagName("Category");
				    			childCatsListSize = childCategoryElements.length;
