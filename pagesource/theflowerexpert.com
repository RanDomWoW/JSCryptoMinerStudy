     							}catch(ex){
     								datesdropdownList.add(y);// for IE
     							}

     						}
     					}
     				}
     			}
     		}else{
     			resetCategoryOptions(categorydropdownlist);
     			removeOptionSelected(datesdropdownList);
     		}
     	}
     	
     	function getDeliveryDatesForCategory(formId) {
			form=document.getElementById(formId);
			if(form){
					categoryId = form.CURRENT_CATEGORY_ID.value;
					catalogId = form.CURRENT_CATALOG_ID.value;
					datesdropdownList=form.delivery_date_gift;
					if(categoryId != "select") {
						removeOptionSelected(datesdropdownList);
						var openRequest = getAjaxRequest();
						var pageEventSink = "/control/getDeliveryDatesForGNI?categoryId="+ categoryId +"&catalogId=" + catalogId
                        if(formId == "findaflowerform")
                        	pageEventSink = "/control/getDeliveryDatesForPUF?categoryId="+ categoryId +"&catalogId=" + catalogId
						
						openRequest.open("POST", pageEventSink, false);
						openRequest.send(null);
						if (openRequest.readyState == 4) {
						    if (openRequest.status == 200 && (typeof openRequest.responseXML!=='undefined') && openRequest!='null') {
						    	var xmlDoc = openRequest.responseXML.documentElement;
						    	var timeZoneOffSet = xmlDoc.getElementsByTagName("TimeZoneOffSet")[0].childNodes[0].nodeValue;
					    		var dateElements = xmlDoc.getElementsByTagName("Date");
								dateListSize = dateElements.length;
								if(dateListSize > 0){
									for(var i=0; i < dateListSize; i++){
										var deliveryDateElements = dateElements[i].getElementsByTagName("DeliveryDate")[0].childNodes;
										var shippingDateStr = dateElements[i].getElementsByTagName("ShippingDate")[0].childNodes[0].nodeValue;
										var shippingDateTime = new Date(shippingDateStr);
										var currentTimeInRcptTimeZone = getTimeInTimezoneOnOffset(parseInt(timeZoneOffSet));
										if(shippingDateTime > currentTimeInRcptTimeZone) {
											var y=document.createElement("option");
											y.text=deliveryDateElements[0].nodeValue.replace(" ",", ");
											y.value=deliveryDateElements[0].nodeValue.split(" ")[1];
											try
							   			    {
							   			    	datesdropdownList.add(y,null);
											}catch(ex){
												datesdropdownList.add(y);// for IE
											}
										}
									}
								}
					}
				}
			} else {
				removeOptionSelected(datesdropdownList);
			}		
		}
	}
		function getTimeInTimezoneOnOffset(timezoneOffset){
			var d = new Date();
			// get the millis in terms of UTC/GMT... Replace timezone - so as to
			// parse this and get the UTC/GMT millis
			var millis = new Date(d.toUTCString().replace("GMT","")).getTime();
			// Now create a new date with Offset of the timeZone from UTC/GMT
			var convertedDate = new Date(millis+timezoneOffset);
			var debug = false;
			
			if(debug){
				var dt = new Date();
				console.log("UTC: "+dt.toUTCString());
				console.log("UTC Millis: "+millis);
				console.log("UTC Date: "+new Date(millis));
				console.log("UTC + offset: "+new Date(millis+timezoneOffset));
				console.log("timeZoneOffSet"+timezoneOffset/(1000*60*60));
			}
			return convertedDate;		
		}
		
