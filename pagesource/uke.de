<div class="calendar-entries entries margin hidden">
  <div after-render-repeat="" class="teaser-entry" ng-class="{'half-width': column !== ''}" ng-repeat="item in items">
    <div class="content">
      <div class="image" ng-if="item.imageLink">
        <img alt="{{item.imageDesc}}" ng-src="{{item.imageLink}}">
      </div>
      <div class="headline">
        <div class="introline">
          <span class="date" ng-if="item.date">
            {{item.date | germanDate}}
          </span>
          <span class="date" ng-if="item.begin">
            {{item.begin | germanDate}}
            <span ng-if="item.end &amp;&amp; item.begin != item.end">
              &nbsp;bis&nbsp;{{item.end | germanDate}}
            </span>
          </span>
          <span class="name" ng-if="item.topic">
            {{item.topic}}
          </span>
        </div>
      </div>
      <div class="copy">
        <h3 ng-if="item.title">
          {{item.title | htmlDecode}}
        </h3>
        <p>
          {{item.teasertext | htmlDecode}}
        </p>
      </div>
      <div class="bottom">
        <div class="additional-copy">
          <p ng-if="item.beginTime">
            <span ng-if="!item.endTime">ab&nbsp;</span>{{item.beginTime}} Uhr
            <span ng-if="item.endTime &amp;&amp; event.beginTime != item.endTime">
              bis {{item.endTime}} Uhr
            </span>
          </p>
          <p ng-if="item.additional_dates_string" class="additional-date-string">{{item.additional_dates_string}}</p>
        </div>
        <div class="additional-copy">
          <p>
            <span ng-if="item.building">
              {{item.building}}
              <span ng-if="item.floor">
                &nbsp;-&nbsp;
              </span>
            </span>
            <span ng-if="item.floor">
              Etage:&nbsp;{{item.floor}}
              <span ng-if="item.room">
                &nbsp;-&nbsp;
              </span>
            </span>
            <span ng-if="item.room">
              Raum:&nbsp;{{item.room}}
            </span>
          </p>
        </div>
        <div class="link" ng-if="item.detailLink">
          <a href="{{item.detailLink}}" target="_self">
            <img alt="Weitere Informationen" class="tooltip tooltipstered" src="/assets/default/images/icons/links_id_51_01.svg" title="Weitere Informationen">
          </a>
        </div>
      </div>
    </div>
  </div>
  <div class="margin pagination" ng-if="pagination">
    <div class="pagination-container">
      {{lastPage = 0;""}}
      <ul>
        <li class="left-arrow" ng-click="decPage()" ng-class="{'slick-disabled' : isFirstPage}">
          <img alt="Zurück" src="/assets/default/images/icons/pfeil_teaser_links_01.svg">
        </li>
        <li ng-if="thePage &gt; 1 &amp;&amp; pagination.pages.indexOf(thePage - 1) === -1" ng-repeat-start="thePage in pagination.pages">
          <span class="divider x-hidden">...</span>
