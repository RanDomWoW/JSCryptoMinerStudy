    <script type="text/javascript">
        $(document).ready(function () {
            $('.disableEvent').each(function () {
                $(this).click(function (event) {
                    event.preventDefault();
                    event.stopPropagation();
                });
            });

            // Reset value
            $('#remuneration_txt').attr('value', '- Sélectionner une rémunération -');
            var IdHidden = 'parentDomainList';
            var elementHidden = $('[name="' + IdHidden + '"]');
            $(elementHidden).val('');
            IdHidden = 'domaines';
            elementHidden = $('[name="' + IdHidden + '"]');
            $(elementHidden).val('');
            IdHidden = 'remuneration';
            elementHidden = $('[name="' + IdHidden + '"]');
            $(elementHidden).val('');
            IdHidden = 'localisation';
            elementHidden = $('[name="' + IdHidden + '"]');
            $(elementHidden).val('');
        });

        /** For Checking Boxes -- BEGIN **/
        var CurrentFunctionText;
        var CurrentLocalisationText;
        var CurrentExperienceText;
        $(document).ready(function () {
            CurrentFunctionText = $('input[name="domain_txt"]').attr('value');
            CurrentLocalisationText = $('input[name="localisation_txt"]').attr('value');
            CurrentExperienceText = $('input[name="experience_txt"]').attr('value');
            refreshFunctionSelected();
            refreshLocalisationSelected();
        });
        var refreshFunctionSelected = function () {
            var NumberFunction = 0;
            $('#selector_fonction .list-function li ul li').each(function () {
                if ($(this).children('a').first().hasClass('selected'))
                    NumberFunction++;
            });
            if (NumberFunction == 0) $('input[name="domain_txt"]').attr('value', CurrentFunctionText);
            else {
                if (NumberFunction == 1)
                    $('input[name="domain_txt"]').attr('value', '- ' + NumberFunction + ' élément sélectionné -');
                else
                    $('input[name="domain_txt"]').attr('value', '- ' + NumberFunction + ' éléments sélectionnés -');
            }
        }
        var refreshLocalisationSelected = function () {
            var NumberFunction = 0;
            $('#selector_localisation ul li ul li').each(function () {
                if ($(this).children('a').first().hasClass('selected'))
                    NumberFunction++;
            });
            if (NumberFunction == 0) $('input[name="localisation_txt"]').attr('value', CurrentLocalisationText);
            else {
                if (NumberFunction == 1)
                    $('input[name="localisation_txt"]').attr('value', '- ' + NumberFunction + ' élément sélectionné -');
                else
                    $('input[name="localisation_txt"]').attr('value', '- ' + NumberFunction + ' éléments sélectionnés -');
            }
        }
        var refreshExperienceSelected = function () {
            var NumberFunction = 0;
            $('#selector_experience ul li ul li').each(function () {
                if ($(this).children('a').first().hasClass('selected'))
                    NumberFunction++;
            });
            if (NumberFunction == 0) $('input[name="experience_txt"]').attr('value', CurrentExperienceText);
            else {
                if (NumberFunction == 1)
                    $('input[name="experience_txt"]').attr('value', '- ' + NumberFunction + ' élément sélectionné -');
                else
                    $('input[name="experience_txt"]').attr('value', '- ' + NumberFunction + ' éléments sélectionnés -');
            }
        }
        /** For Checking Boxes -- END **/


        var setValue = function (IdHidden, ValueHidden) {
            var elementHidden = $('[name="' + IdHidden + '"]');
            $(elementHidden).attr('value', ValueHidden);
        }
        var setAllFunction = function (domainID) {
            $('ul#subdomain-' + domainID).children('li').each(function () {
                var element = $(this).children('a');
                var tid = $(element).attr('id').substr(11, ($(element).attr('id').length - 11));
                setFunctionAll('domaines', tid, $(element));
                $(element).addClass('selected');
            });
            refreshFunctionSelected();
        }
        var setDomain = function (IdHidden, ValueHidden, Element) {
            setValueCheck(IdHidden, ValueHidden, Element);
        }
        var setFunctionAll = function (IdHidden, ValueHidden, Element) {
            var elementHidden = $('[name="' + IdHidden + '"]');
            var currentValues;
            if ($(elementHidden).val() != '')
                currentValues = $(elementHidden).val().split(',');
            else
                currentValues = [];
            var found = false;
            var NBValues = currentValues.length;
            for (var i = 0; i < NBValues; i++) {
                var value = currentValues[i];
                if (value == ValueHidden)
                    found = true;
            }
            if (found == false) {
                currentValues.push(ValueHidden);
                $(elementHidden).val(currentValues.join(','));
            }
        }


        var setFunction = function (IdHidden, ValueHidden, Element) {
            var elementHidden = $('[name="' + IdHidden + '"]');
            if ($(Element).hasClass('selected')) {
                var newValues = [];
                var currentValues = $(elementHidden).attr('value').split(',');
                var NBValues = currentValues.length;
                for (var i = 0; i < NBValues; i++) {
                    var value = currentValues[i];
                    if (value != ValueHidden)
                        newValues.push(value);
                }
                $(elementHidden).val(newValues.join(','));
                setTimeout(function () {
                    refreshFunctionSelected();
                    refreshLocalisationSelected();
                    refreshExperienceSelected();
                }, 20);
            } else {
                var currentValues;
                if ($(elementHidden).val() != '')
                    currentValues = $(elementHidden).val().split(',');
                else
                    currentValues = [];
                var found = false;
                var NBValues = currentValues.length;
                for (var i = 0; i < NBValues; i++) {
                    var value = currentValues[i];
                    if (value == ValueHidden)
                        found = true;
                }
                if (found == false) {
                    currentValues.push(ValueHidden);
                    $(elementHidden).val(currentValues.join(','));
                }
                setTimeout(function () {
                    refreshFunctionSelected();
                    refreshLocalisationSelected();
                    refreshExperienceSelected();
                }, 20);
            }
        }
        var activateDomain = function (domainID) {
            $('ul.domain-empty').hide();
            $('li#domain-' + domainID).show();
        }

        var disactivateDomain = function (domainID) {
            /* Disable all subdomain selected */
            $('#subdomain-' + domainID).children('li').each(function () {
                if ($(this).children('a').first().hasClass('selected')) {
                    var IDs = $(this).children('a').first().attr('id').split('-');
                    setFunction('domaines', IDs[2], $(this).children('a').first());
                    var element = $(this).children('a').first();
                    setTimeout(function () {
                        $(element).removeClass('selected');
                    }, 5);

                }
                setTimeout(function () {
                    refreshFunctionSelected();
                    refreshLocalisationSelected();
                    refreshExperienceSelected();
                }, 20);
            });
            $('li#domain-' + domainID).hide();
            var showEmpty = false;
            $('#selector_fonction').children('ul.list-function').children('li').each(function () {
                if ($(this).css('display') == 'none') {
                    showEmpty = true;
                }
            });
            if (showEmpty == true)
                $('ul.domain-empty').show();
        }
        var setValueCheck = function (IdHidden, ValueHidden, Element) {
            var elementHidden = $('[name="' + IdHidden + '"]');
            if ($(Element).hasClass('selected')) {
                $(Element).removeClass('selected');
                disactivateDomain(ValueHidden);
                var newValues = [];
                var currentValues = $(elementHidden).attr('value').split(',');
                var NBValues = currentValues.length;
                for (var i = 0; i < NBValues; i++) {
                    var value = currentValues[i];
                    if (value != ValueHidden)
                        newValues.push(value);
                }
                $(elementHidden).val(newValues.join(','));
            } else {
                $(Element).addClass('selected');
                activateDomain(ValueHidden);
                var currentValues
                if ($(elementHidden).val() != '')
                    currentValues = $(elementHidden).val().split(',');
                else
                    currentValues = [];
                var found = false;
                var NBValues = currentValues.length;
                for (var i = 0; i < NBValues; i++) {
                    var value = currentValues[i];
                    if (value == ValueHidden)
                        found = true;
                }
                if (found == false) {
                    currentValues.push(ValueHidden);
                    $(elementHidden).val(currentValues.join(','));
                }
            }
        }
        var setAllLocalisation = function (elementParent) {
            $(elementParent).parent().children('ul').children('li').each(function () {
                var element = $(this).children('a');
                var tid = $(element).attr('id').substr(13, ($(element).attr('id').length - 13));
                setFunctionAll('localisation', tid, $(element));
                $(element).addClass('selected');
            });
            refreshLocalisationSelected();
        }

        // Localisation Add-on - BEGIN
        $(document).ready(function () {
            $('.develop_loca').each(function () {
                $(this).click(function () {
                    if ($(this).parent().parent().children('ul').first().css('display') == 'none') $(this).text('[-]');
                    else $(this).text('[+]');
                    $(this).parent().parent().children('ul').first().toggle();

                });
            });

            $('.develop_loca_information').each(function () {
                $(this).click(function () {
                    if ($(this).attr('rel') == 'none') {
                        setAllLocalisation($(this).parent());
                        $(this).attr('rel', 'all');
                        $(this).text('- Tout Déselectionner -');
                    } else {
                        $(this).parent().parent().children('ul').children('li').each(function () {
                            var aLink = $(this).children('a');
                            var id = $(aLink).attr('id').split('_');
                            id = id[1];
                            if ($(aLink).hasClass('selected'));
                            setFunction('localisation', id, aLink);
                            $(aLink).removeClass('selected');
                        });
                        //refreshLocalisationSelected();
                        $(this).attr('rel', 'none');
                        $(this).text('- Tout Sélectionner -');
                    }
                })
            });
        });
        // Localisation Add-On - END
    </script>
