<script type="text/javascript">
$(document).ready(function() {
	$('body').delegate('.ana_menu', 'click', function(event) {
	$('#icerik').html('<div class="yukleniyor">&nbsp;</div>');
	var no=$(this).attr("href");
	var ids=$(this).attr("id");
		$.ajax({
			url: '/',
			data: 'sayfa='+ids,
			type: 'GET',
			cache: false,
			success: function(data) {
				$('#icerik').html(data);
