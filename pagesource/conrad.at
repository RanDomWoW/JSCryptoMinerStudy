<div class="staticinclude parbase section"><script type="text/javascript">
(function(cjs, $) {
	$(document).on("click", "*[href*='?ac='], *[href*='&ac=']", function(e) { 
		e.stopPropagation();
		e.preventDefault();
		
		cjs.com.ajaxHelpers($)["get"]($(this).attr("href"), null, function(data) {
			var elements = $("<div>").append(
					$(data.replace(/<script.*?>([\w\W\d\D\s\S\0\n\f\r\t\v\b]*?)<\/script>/gi, '')).remove("script")
			);
			var errors = elements.find("[data-has-error]");
			if (errors.length > 0 && errors.attr("class").toLowerCase() == "error-messages") {
				showResult(false);
			} else {
				showResult(true);
			}
		});
		
		function showResult(success) {
			var $cartContainer = $("[data-ajax-update='hovercart'] .ccpNavigationMain__item__wrapper"),
				$popup = $("<div class='ccpNavigationMain__item__drop' data-newest-popup='popup' style='display: none;'></div>"),
				$contentHolder = $("<div class='ccpNavigationMain__item__holder'></div>");
			$popup.append($contentHolder);
			$contentHolder.css( {
				'right': 0,
				'font-weight': 'bold'
			});	
				
			if(success) {
				$contentHolder.text("Der Vorteilscode wurde erfolgreich zum Einkaufswagen hinzugefügt!");
				$contentHolder.css("color", "#000000");
			} else {
				$contentHolder.text("Der Vorteilscode konnte nicht zum Einkaufswagen hinzugefügt werden!");
				$contentHolder.css("color", "#d10019");
			}
				
			$cartContainer.append($popup);
			var $addedPopup = $cartContainer.find("[data-newest-popup='popup']");
			$addedPopup.fadeIn("fast");
			setTimeout(function(){ 
				$addedPopup.fadeOut("fast", function() {
					$addedPopup.remove();
				});
			}, 7000);
			
		}
	});
	
	$(document).on("mouseenter", "[data-ajax-update='hovercart']", function() {
		$(document).find("[data-newest-popup='popup']").hide();
	});
})(cjs, window.jQuery)
</script>
