<script language="javascript" type="text/javascript">
function HotelSearchCheck()
    {
	if(document.form1.City.value == ""){
						alert("请填写正确的目的地!");
						return false;
	}
	if (document.form1.StartDate.value.length==0){
	    alert("请选择或填写入住日期！");
		document.form1.StartDate.focus();
		return false
	}	
	if(!isDateString(document.form1.StartDate.value)){
		alert("入住日期不符合格式规范或无效的日期！");
		document.form1.StartDate.focus();
		return false
	}
	if (stringToDate(document.form1.StartDate.value,true)<stringToDate("2018-3-20",true)){
	    alert("入住日期不能早于2018-3-20！");
		document.form1.StartDate.focus();
		return false
	}
	if (compareDate1(today1,document.form1.StartDate.value)){
		alert('本系统不支持晚于或等于'+document.form1.StartDate.value+'号的网上预订，如有需要请与酒店预订部联系，我们会竭诚为您服务。');
		document.form1.StartDate.focus();
		return false
	}
	if (document.form1.DepDate.value.length==0){
		alert("请请选择或填写离店时间！");
		document.form1.DepDate.focus();
		return false
	}	
	if(!isDateString(document.form1.DepDate.value)){
		alert("离店时间不符合格式规范或无效的日期！");
		document.form1.DepDate.focus();
		return false
	}
	if (stringToDate(document.form1.DepDate.value,true)<=stringToDate(document.form1.StartDate.value,true)){
		alert("离店时间不能早于或等于入住时间"+document.form1.StartDate.value+"！");
		document.form1.DepDate.focus();
		return false
	}

	document.form1.action = "/SearchResult.asp";
	return true;
}
</script>		
