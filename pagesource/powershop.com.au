          <!--Both call centres available-->
          function option3(){
              $("#dialog-during-hours").show();
              $("#dialog-out-hours-1").hide();
              $("#dialog-out-hours-2").hide();
              }
          function callBtnCheck(){
                if ($(window).width() > 480 || d.getHours() > 19) {
                option1();
                }
                else{
                option3();
                }
           }
          if (dayofweek < 6){
            <!-- If Business call centre is opened -->
            if(d.getHours() >= 8 && d.getHours() <= 17 ){
                <!-- Before 8:30am -->
                if(d.getHours() == 8 && d.getMinutes() <= 30 ){
                    $("#dialog-during-hours").hide();
                      $("#dialog-out-hours-1").show();
                    }
                <!-- After 5:30am -->
                else if (d.getHours() == 17 && d.getMinutes() <= 30 ){
                      window.resize = callBtnCheck;
                }
                <!-- If friday -->
                else if (dayofweek == 5){
                        if (d.getHours() < 17 ){
                            window.resize = callBtnCheck;
                        }
                        else if(d.getHours() > 17 && d.getHours() < 19){
                            option2();
                         }
                        else{
                            option1();
                        }
                    }
            }
            else{
                <!-- If Business call centre is closed but residential opened -->
                if(d.getHours() >= 8 && d.getHours() < 19 ){
                      option2();
                 }
                 <!-- If Both call centres are closed -->
                 else{
                      option1();
                 }
            }
          }
          <!-- If accessign on the weekend -->
          else{
              option1();
          }
      });
      </script>
