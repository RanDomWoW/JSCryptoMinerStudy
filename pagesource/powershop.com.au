<script>

// Assign height of nav container to push down content, making it visible not cut off by default
function responsiveNavHeight() {
  var navHeight = $('#navs').outerHeight();
  var navContainer = $('#navs-slot');

  navContainer.css({
    'height': navHeight + 'px'
  });
}

$( ".contactbtn" ).click(function(event) {
  event.stopPropagation();
  $('#dialog').toggle();
  $('#dialogBG').toggle();
  $("body").scroll(function(e){ e.preventDefault()});
});

$('#dialogBG').click(function(){
  $('#dialog').hide();
  $('#dialogBG').hide();
});

function subnavBehavior() {
  // Show subnav on hover
  if ($(window).width() > 720){
    $('.hassubmenu').hover(function(){
      $(this).children('.submenu').toggle();
    });

    // close sub navs if clicked anywhere except the subnav parent
    $('html').click(function(e) {
      var target = e.target;
      // Close submenu when clicked outside
      if (!$(target).is('.hassubmenu') && !$(target).parents().is('.hassubmenu')){
        $('.submenu').hide();
      }
    });
  }

  // hamburger menu
  else if ($(window).width() <= 720){

    function navToggle() {
      $(".hassubmenu .fa").click(function () {
        var submenu=$(this).parents().children(".submenu");
        var subArrow=$(this);
        $('.hassubmenu .submenu').not(submenu).hide();
        $('.hassubmenu .fa').not(subArrow).removeClass('fa-angle-down');
        submenu.slideToggle('fast');
        $(this).toggleClass('fa-angle-down');
      });
    }

    navToggle();

    $('html').on('touchend mouseup touchcancel',function(e) {
      var target = e.target;
      if (!$(target).parents().is('#navs')){
        $('.m720 .offering').slideUp( "slow");
      }
    });
  }
}
// end subnav function

$(document).ready(function() {
  subnavBehavior();
  responsiveNavHeight();
});


var $hamburger = $(".hamburger");
$hamburger.css('opacity', '0');

// Mobile hamburger click
function hamburgerClick() {

  $hamburger.css('opacity', '1');

  $hamburger.on("click", function(e) {
    // slide menu toggle
    e.preventDefault();
    $(this).parent().parent().find('.offering').slideToggle();
    // if has class 'is-active'
    if($(this).hasClass('is-active')){
      $(this).removeClass('is-active');
    } else {
      $(this).addClass('is-active');
    }
  }); // end hamburger click
}

$(window).load(function() {
  hamburgerClick();
});

// Window resize with timer
var resizeTimer;

$(window).on('resize', function(e) {

  clearTimeout(resizeTimer);
  resizeTimer = setTimeout(function() {

    // Run code here, resizing has "stopped"
    responsiveNavHeight();
  }, 250);

});

// If window gets resized
$( window ).resize(function() {

  if ($(window).width() > 720) {
    $('#dialog').fadeOut("fast");
    $('#dialogBG').fadeOut( "slow");
  }
});

$( ".mobile-nav-icon" ).click(function(event) {
  $('.submenu').hide();
});
</script>

