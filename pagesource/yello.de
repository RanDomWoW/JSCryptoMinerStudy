                    "Libraries/jquery-ui",
                    "Libraries/jquery.matchHeight"], function (ko, $, domReady, Select, Popover, Modal, Accordion, JQueryPlugins, KnockoutBindings) {
            domReady(function () {


                // These are the plugins used by the new design. They are registered once and applied to the document body.
                // In case you have rendered markup dynamically in client you probably should consider applying the plugins on the element containing it in order to enable their magic.
                JQueryPlugins.registerJQueryPlugins();
                JQueryPlugins.applyJQueryPluginsOnElementChildren(document.getElementsByTagName("BODY")[0]);

                // Register custom knockout bindings
                KnockoutBindings.registerKnockoutBindings();


                // main menu dropdown
                var menuItems = '.js-menu-trigger';
                var menuItemsSelectClass = 'menu__item--selected';
                $(menuItems).click(function (e) {
                    $(this).siblings(menuItems).removeClass(menuItemsSelectClass);
                    $(this).hasClass(menuItemsSelectClass) ? $(this).removeClass(menuItemsSelectClass) : $(this).addClass(menuItemsSelectClass);
                });


                // popovers (info i)
                var popover = $('.js-popover');
                $.each(popover, function (i, element) { new Popover($(element)); });

                // styled selects
                var ddsSelects = $('.js-ddselect-trigger');
                $.each(ddsSelects, function (i, element) { new Select($(element)); });

                // modals
                var modalTrigger = $(".js-modal-trigger");
                $.each(modalTrigger, function (i, element) {
                    new Modal($(element), {
                        overlayBackgroundColor: 'rgba(0, 0, 0, .35)',
                        closeSelector: '.js-modal-close',
                    });
                });

                // scroll to id or data attr selector
                var scrollItems = '.js-scroll-trigger';
                $(scrollItems).click(function (e) {

                    var isATag = $(this).is('a');
                    var selector = $(this).attr('href') || $(this).data('scrollto');
                    var offset = $(this).data('scrolloffset') || 0;
                    var target = $(selector);

                    if (target.length == 1) {
                        setTimeout(function () {
                            $('html, body').animate({
                                scrollTop: target.offset().top - offset
                            }, 350);
                        }, 100);
                        if (isATag) {
                            e.preventDefault();
                        }
                    }
                });

                // trigger feedback layer animation
                //var feedbackTrigger = $('.js-feedback-trigger');
                //var feedbackTarget = $('.js-feedback-target');
                //var feedbackActiveClass = 'is--active';

                //$(feedbackTrigger).on('click', function (e) {
                //    $(feedbackTarget).toggleClass(feedbackActiveClass);
                //});

                //$(document).on('click touchstart', function (e) {
                //    var targetClick = $(e.target).closest('#js-feedback-context').length;
                //    if (!targetClick) {
                //        e.stopPropagation();
                //        $(feedbackTarget).removeClass(feedbackActiveClass);
                //    }
                //});

                // apply accordion logic to all accordion elements
                Accordion.default.processElements();

                // set css class on sticky top toolbar if window is scrolled
                var stickyTopElement = $('#js-toolbar-top');
                checkSticky();
                $(window).on('scroll resize', function () {
                    setTimeout(function () {
                        checkSticky();
                    }, 100);
                });
                function checkSticky() {
                    var scrollTop = $(window).scrollTop();
                    var makeSticky = (scrollTop > 0) ? true : false;
                    stickyTopElement.toggleClass('is--sticky', makeSticky);
                }
            });
        });
    </script>

    

    
