        function isItemChecked() {
            for (i = 0; i < document.forms[0].elements.length; i++) {
                elm = document.forms[0].elements[i]
                if (elm.type == 'checkbox') {
                    if (elm.name.indexOf("chkItem") != -1) {
                        if (elm.checked) {
