    <script type="text/javascript">
    $(function(){
        // 创建工单
        $(window).scroll(function(e) {
            var myTop = $(window).scrollTop();
            if (myTop>100) {
                $('.suspension-form').animate({'bottom':'-2px'},1000);
            };
        });
        var cusNum=$('.cusnum').children('select').val();
        var indus=$('.industry').children('select').val();
        if (cusNum==0 && indus==0) {
            $('#susBtn').attr('disabled',true);
            $('#susBtn').css({'color':'rgba(255,255,255,.6)','cursor':'auto'});
        };
        $('.suspension-form-selector').children('select').change(function(){
            var cusNum=$('.cusnum').children('select').val();
            var indus=$('.industry').children('select').val();
            if (cusNum!=0 && indus!=0) {
                $('#susBtn').attr("disabled", false); ;
                $('#susBtn').css({'color':'#fff','cursor':'pointer'});
            }else{
                $('#susBtn').attr('disabled',true);
                $('#susBtn').css({'color':'rgba(255,255,255,.6)','cursor':'auto'});
            };
        })
        $('#submit-btn').click(function(){
            var clientName = $('#clientName').val();
            var pattern = /^1[345789]\d{9}$/;
            var clientPhone = pattern.test($('#clientPhone').val());
            var flag = true;
            if (clientName==null || clientName == "") {
                $('#clientName').css('border-color','#FA6463');
                $('.input-tips-name').show();
                $(this).removeAttr('href');
                flag = false;
            }else{
                $('#clientName').css('border-color','#f1f1f1');
                $('input-tips-name').hide();
            };
            if (!clientPhone) {
                $('#clientPhone').css('border-color','#FA6463');
                $('.input-tips').show();
                $(this).removeAttr('href');
                flag = false;
            } else {
                $('#clientPhone').css('border-color','#f1f1f1');
                $('.input-tips').hide();
            };
            if (flag) {
                $(this).attr({'href':'/arund/innovative-customer-service.pdf','download':'Udesk中国最具创新力客户服务平台解决方案.pdf'});
                acquireClientPhone();
            };
        })
        $('#clientPhone').focus(function(){
            $('.input-tips').hide();
        })
        $('#clientName').focus(function(){
            $('.input-tips-name').hide();
        })
        $('#clientPhone').blur(function(){
            $('#clientPhone').css('border-color','#f1f1f1');
        })
        $('#clientName').blur(function(){
            $('#clientName').css('border-color','#f1f1f1');
        })
        function acquireClientPhone(){
            var cusNum=$('.cusnum').children('select').val();
            var indus=$('.industry').children('select').val();
            var clientName = $('#clientName').val();
            var clientPhone = $('#clientPhone').val();
            $.ajax({
                url: "//yy-oss.udesk.cn/demand_solution",
                data: {
                    name:clientName,
                    phone_number:clientPhone,
                    company_type:indus,
                    agent_population:cusNum
                },
                type: "post",
                dataType: "json",
                success: function(result) {
                    if (result.code == 200) {
                        $('#myModal').modal('hide');
                    };
                }
            })
        }
        $('.solveClose').click(function(){
            $('.suspension-form').hide();
            $('.solveOpen').show();
        });
        $('.solveOpen').click(function(){
            $(this).hide();
            $('.suspension-form').show();
        })
    })
    </script>
