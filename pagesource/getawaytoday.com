<script type="text/ng-template" id="mini-cart-item.html">

    <h3 ng-attr-title="{{ item.DisplayName }}">{{item.DisplayName}}</h3>
    <button class="delete" type="button" ng-if="item.ShowDeleteOption" ng-click="deleteItem(item.WorksheetItemId)" ng-attr-title="{{item.DeleteItemToolTip}}"></button>

    
    <div class="roomDisplay" ng-if="item.WorksheetItemType == 0" ng-include="'mini-cart-room.html'"></div>
    <div class="voucher" ng-if="item.WorksheetItemType == 1" ng-include="'mini-cart-voucher.html'"></div>
    <div class="giftCertificate" ng-if="item.WorksheetItemType == 3" ng-include="'mini-cart-giftcertificate.html'"></div>
    <div class="pomp" ng-if="item.WorksheetItemType == 9" ng-include="'mini-cart-pomp.html'"></div>
    <div class="merchandise" ng-if="item.WorksheetItemType == 10" ng-include="'mini-cart-merchandise.html'"></div>
    
    
    

    
    <ul ng-if="item.ChildItems.length > 0">
        <li ng-repeat="item in item.ChildItems | filter:showItem" ng-include="'mini-cart-item.html'" ng-controller="CartItemController"></li>
    </ul>

</script>

<script type="text/ng-template" id="mini-cart-room.html">
    <span class="details">
        {{item.AdultCount}} Adult <span ng-if="item.ChildCount > 0">, {{item.ChildCount}} Child</span>
    </span>
</script>

<script type="text/ng-template" id="mini-cart-voucher.html">
    <span class="details">
        {{item.AdultCount}} Adult <span ng-if="item.ChildCount > 0">, {{item.ChildCount}} Child ticket(s)</span>
        <span ng-if="item.ChildCount == 0"> ticket(s)</span>
        <a title="Voucher Details" href="{{item.DetailUrl}}" rel="nofollow" ng-if="item.DetailUrl.length > 0">Details</a>
    </span>
</script>

<script type="text/ng-template" id="mini-cart-giftcertificate.html">
    <span class="details">
        ({{item.Amount | currency}})
    </span>
</script>

<script type="text/ng-template" id="mini-cart-pomp.html">
    <span class="details">
        <a href="javascript:{}" ng-click="showPompDialog(true)" class="details" title="Peace of Mind Plan details" rel="nofollow">Details</a>
    </span>
</script>

<script type="text/ng-template" id="mini-cart-merchandise.html">
    <span class="details">Qty: {{item.Quantity}}</span>
</script>
