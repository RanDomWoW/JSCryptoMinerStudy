  <script>
  $(window).on("load resize", function(){
    windowHeight = $(window).height();
    navHeight = $('#navigation').outerHeight();
    if(windowHeight < navHeight) {
      $('.content.video').css('height', navHeight);
    } else {
      $('.content.video').removeAttr('style');
    }            
  });
</script>

