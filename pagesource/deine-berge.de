				var B = $('<input type="button" value="Kommentar speichern" style="margin:5px;" />');
				var M = $('<div class="richtig">Kommentar wurde gespeichert</div>');
				var F = $('<div class="fehler">Kommentar mindestens 10 Zeichen.</div>');
				K.after(TA);
				TA.after(B);
				B.click( function(){
					TA.trigger("blur");
				});
				TA.blur(function(){
					var tOld = $(this).val();
					var T = tOld.replace(/(\r\n|\n|\r)/gm, '<br />');
					if(tOld.length>10){
						TA.hide();
						B.hide();
						F.hide();
						K.html(T).show();
						bC.addClass("showEdit");
						if(tOld!=Text){
							$.post(HTTP_SERVER + "ajax/editKommentar.php", {k: ""+tOld+"",id: ""+d.id+""}, function(data) { // Do an AJAX call
								
							});
							K.after(M);
							M.fadeOut(2000);	
						}
					}else{
						K.after(F);
						F.show();
					}
				});
			 });
			 
			 			 
