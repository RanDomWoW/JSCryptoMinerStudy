			 $(document).on("click",".boxComment .edit", function(){
				var bC = $(this).closest(".boxComment");
				var d = bC.data();
				var K = bC.find(".kText");	 
				var Text = K.html().trim().replace(/(<br>|<br \/>)/gi,"\n");
