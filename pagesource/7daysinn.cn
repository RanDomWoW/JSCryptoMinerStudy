function generateMixed(n) {
     var res = "";
     for(var i = 0; i < n ; i ++) {
         var id = Math.ceil(Math.random()*35);
         res += chars[id];
     }
     return res;
}

function popBTH(){
	var windowUrl = window.location.pathname;
	if(windowUrl == '/'){
		windowUrl = 'index.html';
	}else{
		windowUrl = windowUrl.substring(windowUrl.lastIndexOf('/')+1, windowUrl.length);
	}
	randKey = generateMixed(50);
	var address = $("#bth_url").val();
	$("#bth_div").css("display","block");
	document.documentElement.scrollTop=0;
	$.ajax({
		type:"POST",
		url: "ajax/ajax_Note.php?randKey="+randKey+"&action=note&fromUrl="+escape(windowUrl)+"&toUrl="+escape(address),
		dataType:"json",
		success: function(json){
		}
	});
