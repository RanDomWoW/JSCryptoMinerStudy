	document.getElementById('dtStart').value = today.getFullYear() + "-" + (today.getMonth() + 1)+ "-" + today.getDate();
	document.getElementById('dtEnd').value = endday.getFullYear() + "-" + (endday.getMonth() + 1)+ "-" + endday.getDate();
	
	
	/*广告图片切换*/
	clearInterval(slideshow_t);
	slideshow_count=$(".slideshow .slideshow_img").length;
	$(".slideshow .slideshow_img:not(:first-child)").hide();
	if(titleswitch==1){
		$(".h2_id .h2_text:not(:first-child)").hide();
	}
	$(".slideshow_box li").click(function() {
