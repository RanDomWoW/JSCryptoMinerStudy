		slideshow_n = slideshow_i;
		if (slideshow_i >= slideshow_count) return;
		$(".slideshow .slideshow_img").filter(":visible").fadeOut(1000).parent().children().eq(slideshow_i).fadeIn(1200);
		if(titleswitch==1){
			$(".h2_id .h2_text").filter(":visible").hide().parent().children().eq(slideshow_i).show();
		}
		$(this).toggleClass("on");
		$(this).siblings().removeAttr("class");
	});
	slideshow_t = setInterval("showAuto()",6000);
	$(".slideshow_box").hover(function(){clearInterval(slideshow_t)}, function(){slideshow_t = setInterval("showAuto()",6000);});
	
}

function postSearch(){
	var sid = '';
	var cityName = $("#city").val();
	var fromDate = $("#dtStart").val();
	var toDate   = $("#dtEnd").val();
	var hotel_name = $("#hotel_name").val();
	if(hotel_name == '请输入关键词'){
		hotel_name = '';
	}
	if(cityName == ''){
		alert('请选择城市');
		return false;
	}
	$.ajax({
		type:"POST",
		url: "ajax/ajax_getLogin.php",
		dataType:"json",
		success: function(json){
			if(json.code == 1){
				$('#inn_form').submit();
				return false;
			}else{
				sid = json.data;
				$.ajax({
					type:"POST",
					url: "ajax/ajax_getCity.php?cityName="+escape(cityName),
					dataType:"json",
					success: function(json){
						if(json.code == 0){
							var BTH_URLS = "http://www.plateno.com/hotel/pg/search.html?cityId=1&fromDate="+fromDate+"&toDate="+toDate+"&from=7days&brands=1,2,3,4,5&keyWord="+encodeURI(hotel_name);
						}else{
							var BTH_URLS = "http://www.plateno.com/hotel/pg/search.html?cityId="+json.data.cityId+"&cityName="+encodeURI(json.data.cityName)+"&fromDate="+fromDate+"&toDate="+toDate+"&from=7days&brands=1,2,3,4,5&keyWord="+encodeURI(hotel_name);
						}
						if(sid != ''){
							BTH_URLS += '&sid='+sid;
						}
						$("#bth_url").val(BTH_URLS);
						popBTH();
					}
				});
			}
		}
	});
}

function postFindHotel(){
	var ordermsg='例如：广州 中大 地铁';
	var LeaveMsg2=$("#wd").val();
	LeaveMsg2=$.trim(LeaveMsg2);
	var sid = '';
	if(LeaveMsg2=="" || LeaveMsg2==ordermsg){
		alert('请输入搜索关键字');
		$("#wd").val('');
		$("#wd").attr('style','color: rgb(51, 51, 51);');
		$("#wd").focus();
		return false;
	}else{
		$.ajax({
			type:"POST",
			url: "ajax/ajax_getLogin.php",
			dataType:"json",
			success: function(json){
				sid = json.data;
				if(json.code == 1){
					$('#find_hotel').submit();
					return false;
				}else{
					var BTH_URLS = "http://www.plateno.com/hotel/pg/search.html?from=7days&brands=1&keyWord="+encodeURI(LeaveMsg2);
					if(sid != ''){
						BTH_URLS += '&sid='+sid;
					}
					$("#bth_url").val(BTH_URLS);
					popBTH();
				}
			}
		});
	}
}

