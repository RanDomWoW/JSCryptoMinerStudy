<script>
function buildUserbar( data ){
	if( data ){
		data.image = data.image || require.siteUrl + "images/community/placeholder_profile.jpg";
		data.profileUrl = require.siteUrl + data.profileUri;

		$container.loadTemplate( "#user-active", data, { complete: function(){
			if ( !data.level || data.level == 0 ) {
				$('#level-up-button').loadTemplate("#level-up");
			}
			var $newContent = $( data.notificationContent );
			var $target = $( this );
			if( data.nicknameAutogenerated == 1 ){
				$target.find(".m--profile .m---text").addClass( "m---grab-nick" );
			}
			$newContent.find('#notifications').click(function (e) {
            	e.preventDefault();
                $('.notification-list').slideToggle(400);
            });
			$newContent.find('#clear-notifications').click(function (e) {
            	var $button = $(this);
            	var $notificationsList = $(".notification-list");
            	$notificationsList.addClass("loading");
            	$button.addClass("loading");
            	e.preventDefault();
				$.ajax({
				    dataType: "json",
				    type: "PUT",
				    data: { lastVisit: 1 },
				    url: require.siteUrl + "api/users/info",
				    success : function(){
				    	$notificationsList.html( "<li>Ingenting har skjedd siden sist</li>" ).fadeIn().addClass("list-empty");
				    	$("#notifications > .m---updates").text( 0 );
				    	$notificationsList.slideToggle(400);
				    }
				});
            });

			$target.find( "#notifications" ).replaceWith( $newContent );

		} } );
		$container.show();

		var $logoutButton = $("a#log-out");
		$logoutButton.click(function(e){
			e.preventDefault();
			$("#main-header .m-profile").addClass("loading");
			$(this).find(".m---icon").addClass("loading");
			$.ajax({
			    url: require.siteUrl + "api/users/logout",
			    type: 'get',
			    cache : false,
			    success: function( data ) {
			    	location.reload();
			    }
			});
		});
	}
	else {
		$container.loadTemplate( "#user-inactive" );
		$container.fadeIn();
	}
}
var $container = $("#main-header .m-profile .m-container");
$container.hide();
$.ajax({
	dataType : "json",
	cache : false,
	url : require.siteUrl + "api/users/me/",
	success : function( data ){
		buildUserbar( data );
	},
	error: function( data ){
		$container.loadTemplate( "#user-inactive" );
		$container.fadeIn();
	}
});
</script>
