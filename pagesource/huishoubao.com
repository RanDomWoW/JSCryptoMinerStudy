<script id="product-item" type="text/html">
    {{each productList item index}}
    <li class="product-item">
        <a class="link" href="/evaluate/{{ item['productid'] }}.html" target="_blank">
            <img class="product-logo" src="{{ imgHost + item['productlogo'] }}" alt="{{ item['productname'] }}"/>
            <h3 class="product-name">{{ item['productname'] }}</h3>
            <p class="product-desc">
                {{ if (item.activity_desc) }}
                    {{item.activity_desc}}
                {{/if}}

                {{if (!item.activity_desc)}}
                    最高回收价
                {{/if}}
            </p>
            <p class="product-price">¥{{ item['maxprice'] }}</p>
            {{if (item.icon) }}
                <img class="active-icon" src="{{item.icon}}" />
            {{/if}}
        </a>
    </li>
    {{ /each }}
    {{ if brandId != -1 }}
    <a class="show-more-product" rel="nofollow" href="{{moreUrl}}">
        <span class="text">更多</span>
    </a>
    {{ else }}
    <a class="show-more-product" rel="nofollow" href="{{moreUrl}}">
        <span class="text">更多</span>
    </a>
    {{ /if }}
</script>
