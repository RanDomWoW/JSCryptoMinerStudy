					$('#pc_'+tokens[0]).replaceWith(dyndatas[tokens[0]]);
				}
			}
			else if (key=='js') {
				$('body').append(dyndatas[key]);
			}
			else {
				$('#pc_'+key).replaceWith(dyndatas[key]);
			}
		}
