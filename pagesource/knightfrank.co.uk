                        function setPriceRangeMin(symbol, minPrice) {
                            return symbol + numberWithCommas(minPrice);
                        }
                        function setPriceRangeMax(symbol, minPrice, maxPrice, maxPriceLimit, label) {
                            var maxLabel = symbol + numberWithCommas(maxPrice);
                            if (minPrice === maxPriceLimit && maxPrice === maxPriceLimit) {
                                return label;
                            }
                            if (maxPrice === maxPriceLimit) {
                                maxLabel = label;
                            }
                            return maxLabel;   
                        }
                        function numberWithCommas(x) {
                            return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ',');
                        }
                                         
