<script type="text/javascript">
$(document).ready(function() {
	$("#imaJobSearchFormId").validate();
	$('#goButtnId').click(function(){
		ga('send', 'event', 'Right Panel', 'Click : Search Button','Quick Job Search');
		var expFrom = parseInt($("#expFromId option:selected").val());
		var isBlank=true;
		if($('#keyword').val().length<3 && $('#locationComboId').val().length<3 && $('#expFromId option:selected').val() == "") {
			isBlank=true;
		} else {
			isBlank=false;
		}
	  	if(isBlank){
			  //$('#errorMessage').html('Please provide one of the search criteria.');
			  //$('#errorMessage').show();
			  $('#keyword').addClass("error");
			  $('#locationComboId').addClass("error");
			  $('#homeExp').addClass("error");
			  return false;
	 	} else{
			$('#errorMessage').hide();
			var validationResult = $("#imaJobSearchFormId").valid();
			if(validationResult){
				var keyword = $('#keyword').val();
				var location = '';
				var jobLocationId = $('#hiddenLocId').val();
				if(keyword == ''){
					$('#keyWordCompare').val("Vacancy");
				}else{
					$('#keyWordCompare').val(keyword);
				}

				if(jobLocationId == ''){
					location = '';
					$('#locationNameCompare').val("India");
					$('#locationName').val(location);
					$('#hiddenLocId').val('');
				}else {
					location = $('#locationComboId').val();
					location = $.trim(location);
					$('#locationName').val(location);
					$('#hiddenLocId').val(jobLocationId);
					$('#locationNameCompare').val(location);
				}
				
				var seoUrl = "";
				if(keyword != '' && location !=''){
					seoUrl = "/jobs-"+keyword+"/Careers-"+location+"/Jobs-Vacancies-/Naukri/India-jobs-search";
				}else if(keyword != ''){
					seoUrl = "/jobs-"+keyword+"/Careers-India/Jobs-Vacancies-/Naukri/India-jobs-search";
				}else if(location !=''){
					seoUrl = "/jobs-Vacancy/Careers-"+location+"/Jobs-Vacancies-/Naukri/India-jobs-search";
				}else{
					seoUrl = "/jobs-Vacancy/Careers-India/Jobs-Vacancies-/Naukri/India-jobs-search";
				}
				submitFormUsingGet('imaJobSearchFormId',seoUrl);
			}
			return false;
	 	}
	  	return false;
	});
	
	$('#imaJobSearchFormId').on('keyup', function(e){
		var keycode = (e.keyCode ? e.keyCode : (e.which ? e.which : e.charCode));
		if (keycode == 13){
			e.preventDefault();
			$("#goButtnId").trigger('click');
		}
	});
});

/**
 * This function creates a bean in backend in first request. in second request it populates the data
 * @param formId
 * @param formUrl
 */
function submitFormUsingGet(formId, formUrl){
	var jQueryFormId = "#" + formId ;
	AJAX_GET_JSON_DATA("/createjobsearchbean", $("#" + formId ).serialize(), function(data){
		$.each(data, function(key, value) { 
			if(key == 'success' && value == 'success'){
				//now send get request
				window.location.href = formUrl;
 			}   
		});
	});
}
var location_index=0;
makeLocationAutoCompleter('locationComboId','/suggestLocations',location_index,'hiddenLocId','locationName');
</script>
