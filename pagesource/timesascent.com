            $('#opinionPoll > i.fa.fa-caret-up').hide();
        }else{
            $('#opinionPoll > i.fa.fa-caret-up').show();
            $('#opinionPoll > i.fa.fa-caret-down').hide();
        }
      $( "#enqBox" ).animate({
          right : right
      })
    });
	
});
</script>


