/* GLOBAL STYLES */
body {
    font-family:"museo-sans", sans-serif;
    color: rgba(28,43,57,0.85);
    line-height: 1;
}
h1, h2, h3, h4, h5, h6 {
    font-family:"museo-sans", sans-serif;
    color: rgba(28,43,57,1.0);
    font-weight: 300;
    line-height: 110%;
}
p {
    font-family:"museo-sans", sans-serif;
    color: rgba(28,43,57,0.75);
    font-weight: 100;
    font-size: 20px;
    font-style: normal;
    line-height: 120%;
}

a:link {
    font-family:"museo-sans", sans-serif;
    color: rgba(0,140,204,1.0);
    font-weight: inherit;
    text-decoration: none;
    -webkit-transition: color 0.35s;
    transition: color 0.35s;
}
a:visited {
    font-family:"museo-sans", sans-serif;
    color: rgba(0,140,204,1.0);
    font-weight: inherit;
    text-decoration: none;
}
a:hover {
    font-family:"museo-sans", sans-serif;
    color: rgba(0,140,204,0.65);
    font-weight: inherit;
    text-decoration: none;
}
a:active {
    font-family:"museo-sans", sans-serif;
    color: rgba(0,140,204,1.0);
    font-weight: inherit;
    text-decoration: none;
}

.back-to-top { /* Hide Back-To-Top Button */
    display: none !important;
}

.height-20 {
    padding: 0 0 0 0;
}
@media only screen and (max-width: 768px) {
    
    .height-20 {
        padding: 30px 0 30px 0;
    }
    
}

h3.cta-heading-3 { /* CTA h3 */
    line-height: 36px;
}
@media only screen and (max-width: 768px) {
    h3.cta-heading-3 {
        line-height: 36px;
        padding: 0 60px 0 60px;
    }
}


/* CUSTOM BUTTONS */
.btn {
    font-family:"museo-sans", sans-serif;
}
.btn {
    height: 40px;
    padding-top: 10px;
    padding-right: 20px;
    padding-left: 20px;
    font-size: 14px !important;
    border: none !important;
	-webkit-transition: background-color 0.35s;
    transition: background-color 0.35s;
    -webkit-transform: translate3d(0, 0px, 0);
    transform: translate3d(0, 0px, 0);
}
.btn:hover {
    height: 40px;
    padding-top: 10px;
    padding-right: 20px;
    padding-left: 20px;
    font-size: 14px !important;
    border: none !important;
    transform: translate3d(0, 0px, 0);
    -webkit-transform: translate3d(0, 0px, 0);
}
.btn--primary {
    -webkit-transition: background-color 0.35s !important;
    transition: background-color 0.35s !important;
}
.btn--primary .btn__text {
    font-size: 14px !important;
}
.btn--primary-1 {
    -webkit-transition: background-color 0.35s !important;
    transition: background-color 0.35s !important;
}
.btn--primary-1 .btn__text {
    font-size: 14px !important;
}
.btn--primary-2 {
    background-color: rgba(255,255,255,1.0);
    -webkit-transition: background-color 0.35s !important;
    transition: background-color 0.35s !important;
}
.btn--primary-2:hover {
    background-color: rgba(255,255,255,0.75);
    -webkit-transition: background-color 0.35s !important;
    transition: background-color 0.35s !important;
}
.btn--primary-2 .btn__text {
    font-size: 14px !important;
    color: rgba(28,43,57,0.85) !important;
}
.modal-instance .btn--primary {
    background-color: rgba(255,255,255,1.0);
    -webkit-transition: background-color 0.35s !important;
    transition: background-color 0.35s !important;
}
.modal-instance .btn--primary:hover {
    background-color: rgba(255,255,255,0.75);
}
.modal-instance .btn--primary .btn__text {
    color: rgba(28,43,57,0.85);
    font-size: 14px !important;
}


/* FIXED HEADER SHADOW */
.bar.pos-fixed {
    box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.65);
}


/* LOGO */
.bar .logo {
    min-height: 35px !important;
    max-height: 35px !important;
    height: 35px !important;
}

@media (max-width: 991px) {
    .bar--transparent:not(.bar--dark) .logo-light {
        display: block !important;
    }
}

/* MAIN NAVIGATION */
#menu-main li.menu-item a,
#menu-main li.menu-item a:visited {
    text-decoration: none;
    position: relative;
}
#menu-main li.menu-item a:after,
#menu-main li.menu-item a:visited:after {
    content: '';
    height: 3px;
    position: absolute;
    bottom: -5px;
    left: 0;
    width: 0%;
    background: rgba(0,140,204,1.0);
    transition: 0.2s;
}
#menu-main li.menu-item a:hover:after,
#menu-main li.menu-item a:visited:hover:after {
    width: 100%;
}

.dropdown .dropdown__content {
    box-shadow: 0 0 25px 0 rgba(0, 0, 0, 0.35);
    -webkit-box-shadow: 0 0 25px 0 rgba(0, 0, 0, 0.35);
}


