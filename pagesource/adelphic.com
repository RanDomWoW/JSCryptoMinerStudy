.bg--dark .btn:not(.btn--primary) { /* DARK HEADER ON SCROLL */
    background-color: rgba(114,128,138,0.75);
    -webkit-transition: background-color 0.35s;
    transition: background-color 0.35s;    
    border-color: none;
}
.bg--dark .btn:not(.btn--primary):hover { /* DARK HEADER ON SCROLL */
