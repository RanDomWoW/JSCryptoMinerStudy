<script>

$(function(){

    /* Listing click */

    


	$(".home-search form").submit(function(e){

        $form = $(this);

		if($form.attr("data-saved") != "true"){

			e.preventDefault();
			$form.find("button").addClass("active");

            var $link = $("#homesearch-value").val();
            var $q = $form.find("#homesearch").val();
            var $category = $form.find("#homesearch").attr("data-category");
            var $size = $form.find("select[name='size']").val();
            var $price = $form.find("select[name='price']").val();

            if(!$link){
                $link = "/shop";
                if($q) {
                    $link += ($link.indexOf("?") > 0 ? "&" : "?") + 'q=' + $q;
                }
            } else {
                $form.find("#homesearch").removeAttr("name");
            }

            $form.attr("action", $link);

            if($size){
                $link += ($link.indexOf("?") > 0 ? "&" : "?") + 'size=' + $size;
                $element = $form.find("select[name='size']");
                $size = ($element.prop('selectedIndex') ? $element.find("option:selected").text() : "");
            } else {
                $form.find("select[name='size']").removeAttr("name");
            }

            if($price){
                $link += ($link.indexOf("?") > 0 ? "&" : "?") + 'price=' + $price;
                $element = $form.find("select[name='price']");
                $price = ($element.prop('selectedIndex') ? $element.find("option:selected").text() : "");
            } else {
                $form.find("select[name='price']").removeAttr("name");
            }

            $vData = (
				"link=" + encodeURIComponent($link) +
                "&q=" + encodeURIComponent($q) +
                "&category=" + encodeURIComponent($category) +
				"&size=" + encodeURIComponent($size) +
				"&price=" + encodeURIComponent($price)
			);

            $.ajax({
				type: "POST",
				url: "/api/home?mode=search",
				data: $vData,
				cache: false,
				success: function(data) {
					$form.attr("data-saved", "true").submit();
				}
			});

		}

	});

	$("#homesearch").autocomplete({
		source: "/api/sitesearch",
		minLength: 2,
		select: function( event, ui ) {
			$("#homesearch-value").attr("value", ui.item.link);
			$("#homesearch").attr("data-category", ui.item.attribute);
		},
		messages: {
			noResults: "",
			results: function() {}
		},
        create: function () {
            $(this).data('ui-autocomplete')._renderItem = function (ul, item) {
				 return $("<li class='sitesearch'></li>")
					 .data("item.autocomplete", item)
					 .append("<a>" + item.label + "<small class='ml-2 text-muted'>" + item.attribute + "</small></a>")
					 .appendTo(ul);
            };
        },
        search: function () {
			$(".saved-search").addClass("hide");
		}

	}).blur(function(){

		setTimeout(
			function()
			{
				// Blur triggers before click
				$(".saved-search").addClass("hide");
			}, 200
		);

	}).focus(function(){

		if($(this).val().length == 0) {
			savedSearch();
		} else {
			$(this).autocomplete("search");
		}

	});

	function getCookie(cname) {
		var name = cname + "=";
		var ca = document.cookie.split(';');
		for(var i=0; i<ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1);
			if (c.indexOf(name) == 0) return decodeURIComponent( c.substring(name.length,c.length).replace(/\+/g,' ') );
		}
		return "";
	}

	function savedSearch() {

		var $element = $(".saved-search");

		if($element.attr("data-loaded") != "true") {

            var $cookie = getCookie("home-search");

            if($cookie.length > 0) {

                $pJson = JSON.parse($cookie);

                $.each($pJson, function(i, object) {

                    if((object.size + object.price) != "") {
                        $vDetails = '<div class="col-md-3 col-lg-3 hidden-sm-down">' + object.size + '</div>' +
                                    '<div class="col-md-3 col-lg-2 hidden-sm-down">' + object.price + '</div>';
                    } else {
                        $vDetails = "";
                    }

                    $element.append(
                        "<a href=\"" + decodeURIComponent(object.link) + "\" class=\"saved-search-item\">" +
                            "<div class=\"row\">" +
                                "<div class=\"col-10 col-md-5 col-lg-5 saved-search-item-q\">" +
                                    "<span>" + object.q + "</span> " +
                                    "<small>" + object.category + "</small>" +
                                "</div>" +
                                $vDetails +
                            "</div>" +
                        "</a>"
                    )

                });

            }

			$element.attr("data-loaded", "true");

		}

		if($element.children().length > 0) {
			$element.removeClass("hide");
		}

	}

	function loop(pElement, pStep, pMin, pMax) {

		var vRand = Math.round(Math.random() * (pMax * 1000 - pMin * 1000)) + pMin * 1000;
		var vStep = vRand.toString();

		if(pStep == 0){
			vStep = vStep.charAt(vStep.length-1);
		} else {
			vStep = pStep;
		}

		setTimeout(function() {
				pElement.text( pElement.attr("data-prefix") + numberWithCommas(numberWithoutCommas(pElement.text()) + parseInt(vStep)) );
				loop(pElement, pStep, pMin, pMax);
		}, vRand);

	}

	function numberWithCommas(x) {
		return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
	}

	function numberWithoutCommas(x) {
		return parseInt(x.toString().replace(/[^0-9]+/g, ""));
	}
    

	// onLoad

	loop( $("#stats-view"), 1, 1, 2);
	loop( $("#stats-sale"), 1, 5, 20);
    

    var $window = $(window);

    $window.on("load", function() {

        function hero(pCurrent) {

            var vTotal = $(".hero-background").length - 1;
            var vPrev = pCurrent - 1;

            if (vTotal > 0) {

                if (pCurrent > vTotal) {
                    pCurrent = 0;
                    vPrev = vTotal;
                }

                var $current = $("#hero-background-" + (pCurrent));
                var $prev = $("#hero-background-" + (vPrev));

                if ($current.hasClass("hero-background-blank") || $current.hasClass("hero-background-loading")) {
                    $next = pCurrent;
                } else {
                    $current.css('z-index', '1').removeClass("first").addClass("active");
                    $prev.css('z-index', '0').removeClass("active");
                    $next = pCurrent + 1;
                }


                /* Lazy load next background */

                var $img = $("#hero-background-" + ($next));

                if ($img.hasClass("hero-background-blank")) {

                    $img.removeClass("hero-background-blank").addClass("hero-background-loading");

                    $img.css("background-image", "url(" + $img.attr("data-src") + ")").imagesLoaded({background: true}, function () {
                        $img.removeClass("hero-background-loading").removeAttr("data-src");
                    });

                }

                setTimeout(function () {
                    hero($next);
                }, 5000);

            }

        }

        hero(0);

    });

    $window.on('scroll', function() {

        if($("#featured.empty").isOnScreen(600)) {

            $("#featured").removeClass("empty");

            $.ajax({
                type: "GET",
                url: "/api/home?mode=featured",
                cache: true,
                success: function(data) {
                    $("#featured").html(data);
                    $.doIsotope($("#featured"));
                }
            });

        }

    });

    if($("#featured.empty").isOnScreen(0)) {
        $window.scroll();
    }

    $(document).on("click", "#more", function (e) {
        e.preventDefault();

        $(this).removeAttr("id");

        var $url = $(this).attr("href");
        var $container = $("#featured .items");
        var $more = $(this).closest("li");

        $.ajax({
            type: "GET",
            url: $url,
            dataType:"html",
            cache: false,
            success: function(data) {

                var $content = $(data);

                $container
                    .isotope("remove", $more)
                    .append($content)
                    .isotope("appended", $content)
                    .isotopeHeight()
                    .isotope("layout");

                $.lazy();

            }
        });

    });

});

</script>

<script>

	$(function(){

		var aCountry = new Object();
		aCountry['AU'] = new Array("Australia","com.au");
		aCountry['NZ'] = new Array("New Zealand","co.nz");
		aCountry['US'] = new Array("the US","com");
		aCountry['GB'] = new Array("the UK","co.uk");
		aCountry['CA'] = new Array("Canada","ca");
		aCountry['ZA'] = new Array("South Africa","co.za");
		aCountry['PH'] = new Array("the Philippines","com.ph");
		aCountry['IE'] = new Array("Ireland","ie");
		aCountry['DE'] = new Array("Germany","de");
		aCountry['FR'] = new Array("France","fr");
		aCountry['ES'] = new Array("Spain","es");
		aCountry['NL'] = new Array("the Netherlands","nl");
		aCountry['HK'] = new Array("Hong Kong","com.hk");
		aCountry['SG'] = new Array("Singapore","com.sg");
		aCountry['AE'] = new Array("UAE","ae");
		aCountry['MX'] = new Array("Mexico","com.mx");

		$.getJSON('https://www.bigsite.com.au/api/geoip', function(location) {

			if(location != null){

				var vCode = location.country_code;

				if(vCode != ""){
					if(vCode != "US"){
						if(typeof aCountry[vCode] == "object"){
							$("#switchDomain #switchText").text(aCountry[vCode][0]);
							$("#switchDomain #switchLink").text("stillwhite." + aCountry[vCode][1]).attr("href", "https://www.stillwhite." + aCountry[vCode][1]);
							$("#switchDomain #switchLink").attr("data-country", vCode);
							$("#switchDomain").slideDown();
							ga('send', 'event', 'Domain', 'Popup', 'US > ' + vCode);
						}
					}
				}

			}

		});

		$("#switchDomain .close").click(function(e) {
			e.preventDefault();
			$("#switchDomain").slideUp();
		});

	});

</script>

