<script>

    var EuropayAPI = {
        send_request: function (req_data) {
            // data: object with form data
            var me = this;
            $.ajax({
                type: 'POST',
                url: location.protocol + '//' + location.host + '/payment/europay/',
                dataType: 'json',
                contentType: 'application/json',
                data: JSON.stringify(req_data),
                success: function (resp) {
                    if (resp.hasOwnProperty('payment_url')) {
                        var $redirectUrl_el = $('<input>')
                                .prop('type', 'hidden')
                                .prop('name', 'redirect_url')
                                .val(resp.payment_url),
                            $token = $('<input>')
                                .prop('type', 'hidden')
                                .prop('name', 'europay_token')
                                .val(resp.token);

                        $('form').append($redirectUrl_el).append($token);
                        me.$form_el.submit();
                    } else if (resp.hasOwnProperty('status')) {
                        if (resp['status'] === 'error') {
                            alert('Error')
//                            for (var k in resp['message']) {
//                                if (resp['message'].hasOwnProperty(k)) {
//                                    // TODO: make decent error message output
//                                    alert(resp['message'][k][0])
//                                }
//                            }
                        }
                    }
                },
                error: function (err) {
                    console.error(err);
                },
            });
        },

        prepare_request: function ($frm) {
            this.$form_el = $frm;
            var d = {};
            $.each($frm.serializeArray(), function (idx, val) {
                d[val['name']] = val['value'];
            });

            return d;
        },
    };

    $(function () {
        $('.js_pay_online').on('touchend, click', function(event) {
            event.preventDefault();
            $frm = $(this).closest('form');
//            $frm.off('submit');
            EuropayAPI.send_request(
                EuropayAPI.prepare_request($frm)
            );

        });
    })
</script>

