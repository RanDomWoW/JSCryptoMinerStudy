                    'z-index': '9999'
        });
        $(elem).focus();
    }
 
 	function payOnlineOff() {
      if ( !$('.js_pre_pay').is(":disabled") ) {
          $('.js_pay_online').off();
          $('.js_pre_pay')
            .off()
            .attr("disabled", "disabled")
            .css({
              opacity: 0.4,
              transform: "none",
