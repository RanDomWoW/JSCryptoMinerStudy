    <!-- Search Js -->
    <script type="text/javascript">
        $(document).ready(function () {

            jQuery("#tbInstagram").keyup(function () {
                if (event.keyCode == 13) {
                    submitInstagram();
                }
            });

            //function searchSubmit() {
            //    window.location.assign("tag/" + searched);
            //}
            jQuery("#tbPinterest").keyup(function () {
                if (event.keyCode == 13) {
                    submitPinterest();
                }
            });
        });
        function submitInstagram() {
            var searched = (jQuery("#tbInstagram").val());
            if (searched == '')
            searched = searched.replace(/ /g, '_');
            searched = searched.replace('#', '');
            searched = searched.replace('<', '');
            searched = searched.replace('>', '');
            searched = searched.replace('%', '');
            searched = searched.replace('&', '');
            searched = searched.replace('.', '');
            searched = searched.replace('*', '');
            searched = searched.replace('/', '');
            searched = searched.replace('~', '');

            if (searched == "") {
                searched = "ifeellucky";
            }
            window.location.assign("/i/tag/" + searched);
        }
        function submitPinterest() {
            var searched = (jQuery("#tbPinterest").val());

            if (searched == '')
            searched = searched.replace(/ /g, '_');
            searched = searched.replace('#', '');
            searched = searched.replace('<', '');
            searched = searched.replace('>', '');
            searched = searched.replace('%', '');
            searched = searched.replace('&', '');
            searched = searched.replace('.', '');
            searched = searched.replace('*', '');
            searched = searched.replace('/', '');
            searched = searched.replace('~', '');
            if (searched == "") {
                searched = "ifeellucky";
            }
            window.location.assign("/p/tag/" + searched);
        }
        $(function () {
            $.scrollUp();
        });
        $('.grid').masonry({
            itemSelector: '.grid-item',
            columnWidth: 300
        });

    </script>
