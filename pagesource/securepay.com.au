			jQuery(".video-popup").click(function () {
				jQuery("#youtube-stop").attr('src','');   
			});
			jQuery('#buss').click(function(){    
						jQuery('.mean-nav ul:first').slideUp();
						menuOn = false;
						jQuery('body').removeClass("mean-menu-active");	 				
						jQuery('.close-mean').css("display", "none");
						jQuery('.close-mean').css("left", "unset");
						jQuery('.close-mean').css("height", "unset");
						jQuery('.close-mean').css("width", "unset");				
						jQuery('.meanmenu-reveal').toggleClass("meanclose");
						jQuery('.meanmenu-reveal').html('<span /><span /><span />');
						jQuery('.bust').removeClass("busted"); 
						//console.log("YYY");
			});
		
			jQuery(window).resize(function(e) {
			   jQuery('.menu-mobile-inner ul.menu > li.menu-item.active').click(); 
			   setFooterPopEvents();
			});
			
			jQuery('.menu-mobile-inner ul.menu > li.menu-item').click(function(e){
				e.preventDefault();
				
				if( jQuery(this).find('ul').size()<=0 )
				{
					setFooterPopEvents();
					window.location.href = jQuery(this).find('a').attr('href');
					return true;
				}
							
				e.preventDefault();
				
				var index = jQuery('.menu-mobile-inner ul.menu > li.menu-item').index(jQuery(this));

				if( jQuery(this).hasClass('active'))
				{
					jQuery('.menu-mobile-inner ul.menu > li.menu-item').removeClass('active');
					jQuery('#menu-mobile-pop').hide();
					return false;
				}


				jQuery('.menu-mobile-inner ul.menu > li.menu-item').removeClass('active');
				jQuery(this).addClass('active');

				
					
				if( jQuery(window).width()<768)
				{
					
					var insertToE = (Math.ceil((index+1)/3)*3)-1;
					var ec = jQuery('.menu-mobile-inner ul.menu > li.menu-item').size();
					
					if( insertToE>(ec-1) )
						insertToE =(ec-1);
						
					
					jQuery('<div/>').attr('class','clear').insertAfter(jQuery('.menu-mobile-inner ul.menu > li.menu-item:eq('+insertToE+')'));
					//console.log("768 ");

					
					jQuery('#menu-mobile-pop').css('opacity',0);
					
					jQuery('#menu-mobile-pop').css({'margin-left':'0'}).html('<div class="top-line"> </div><ul>'+jQuery(this).find('ul').html()+'</ul>').css({'height':'auto'}).insertAfter(
						jQuery('.menu-mobile-inner ul.menu > li.menu-item:eq('+insertToE+')')
					).show();
					
					jQuery('#menu-mobile-pop .top-line').css('margin-left',(jQuery(this).offset().left-18)+'px');
					
					jQuery('#menu-mobile-pop').css('opacity',1);				
					
				}
				else
				{
					jQuery('#menu-mobile-pop').appendTo(jQuery('.menu-mobile.grid-container'));
					jQuery('#menu-mobile-pop .top-line').width(jQuery(this).find('a').width()+20);				
					jQuery('#menu-mobile-pop').html('<div class="top-line"> </div><ul>'+jQuery(this).find('ul').html()+'</ul>').css({'height':'auto'}).show();				
					jQuery('#menu-mobile-pop').css({'margin-left':(jQuery(this).offset().left-18)+'px'});
					
				}
				
				setFooterPopEvents();

				if( jQuery(window).width()<=320)
				{
    				jQuery( ".menu-mobile-inner ul.menu .clear" ).remove();
					//console.log("320 guys");
				jQuery( ".menu-mobile-inner ul.menu .clear" ).remove();
				}
				
				
				if( !window.footerLinkClicked )
				{
					jQuery("html, body").animate({ scrollTop: jQuery(document).height() }, 1000);
					window.footerLinkClicked = true;
				}
			});
		});
		
		
