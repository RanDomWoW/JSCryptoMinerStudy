    <script>
	
		jQuery(document).ready(function () {
			setFooterPopEvents();
		});
		
		function setFooterPopEvents()
		{
			jQuery('#nav-mobile .menu-item a').on('click touchend', function() {
				if( !jQuery(this).parent().hasClass('menu-item-has-children'))
				{
					var link = jQuery(this).attr('href');   
					window.open(link,'_self'); 		
					return false;   
				}
			}); 
		}
	
		jQuery(document).ready(function () {
			jQuery('#menu-column > ul > li > a').click(function() {
			
			jQuery('#footer .menu-mobile-inner #nav-mobile ul.sub-menu').css("display", "none");
			 jQuery('#nav-mobile #menu-column ul li').removeClass('active_menu');	
			  jQuery('#menu-column li').removeClass('active');
			  jQuery(this).closest('li').addClass('active');	
			  var checkElement = jQuery(this).next();
			  if((checkElement.is('ul')) && (checkElement.is(':visible'))) {
			  	//console.log("111");
				jQuery(this).closest('li').removeClass('active');
				jQuery(this).closest('li').removeClass('active_menu');
				 
				
				checkElement.slideUp('normal');
			  }
			  if((checkElement.is('ul')) && (!checkElement.is(':visible'))) {
			  	//console.log("222");
				jQuery('#menu-column ul ul:visible').slideUp('normal');
				jQuery(this).closest('li').addClass('active_menu');
				checkElement.slideDown('normal');

				//alert('second alert');

			  }
			  
			  setFooterPopEvents();
			  
			  if(jQuery(this).closest('li').find('ul').children().length == 0) {				
				return true;
			  } else {					
				return false;	
			  }		
			});
		});
	
		jQuery(document).ready(function () {
			jQuery('#main-nav').meanmenu();	
			jQuery('body').click(function(e) {			
				var ulOffset = jQuery('.mean-bar ul').offset();			
				if( jQuery('.mean-bar ul').width()>0 )
				{
					if( !(e.pageX >= ulOffset.left) )
					{
						jQuery('.mean-nav ul:first').slideUp();
						menuOn = false;
						jQuery('body').removeClass("mean-menu-active");	 				
						jQuery('.close-mean').css("display", "none");
						jQuery('.close-mean').css("left", "unset");
						jQuery('.close-mean').css("height", "unset");
						jQuery('.close-mean').css("width", "unset");				
						jQuery('.meanmenu-reveal').toggleClass("meanclose");
						jQuery('.meanmenu-reveal').html('<span /><span /><span />');

					}		
				}
			});
		});
		
		jQuery(document).ready(function () {
			if (jQuery(window).width() > 1024) {
				jQuery("#header .menu-item-has-children").hover(
				  function () {
					jQuery(this).addClass("active_hover");
					jQuery('#header-wrap').addClass("active_hover");
					jQuery('body').addClass("active_hover");
				  },
				  function () {
					jQuery(this).removeClass("active_hover");
					jQuery('#header-wrap').removeClass("active_hover");
					jQuery('body').removeClass("active_hover");
				  }
				);	
			}
		});
	
		jQuery(document).ready(function () {
			if (jQuery(window).width() >= 768) {
				jQuery(window).scroll(function() {
					if (jQuery(this).scrollTop()) {
						jQuery('#header-wrap').addClass('stickytop');
						jQuery('body').addClass("active_sticky");
					}
					else {
						jQuery('#header-wrap').removeClass('stickytop');
						jQuery('body').removeClass("active_sticky");
					}
				});	
			} else {	
				jQuery( "#header-wrap .grid-container" ).wrap( "<div class='mobile-sticky-wrap'></div>" );
				jQuery(window).scroll(function (event) {
					var scroll = jQuery(window).scrollTop();
					jQuery('#header').toggleClass('stickynav-wrap',
					 //add 'ok' class when div position match or exceeds else remove the 'ok' class.
					  scroll >= jQuery('#header-wrap').offset().top					
					);		
					
				});
				//trigger the scroll
				jQuery(window).scroll();//ensure if you're in current position when page is refreshed
			}	
			jQuery( ".bugme-close" ).click(function() {
				
				setTimeout(function() {
					jQuery('.mobile-sticky-wrap').addClass("mobile-sticky-wrap-inside");
					jQuery('#header-wrap').css("position", "fixed");
					jQuery('#slider-mobile').css("padding-top", "65px");
				 }, 1170);			  
			});			
		});
		
		var acc = document.getElementsByClassName("accordion");
		var i;

		for (i = 0; i < acc.length; i++) {
		  acc[i].onclick = function() {
			this.classList.toggle("active");
			var panel = this.nextElementSibling;
			if (panel.style.maxHeight){
			  panel.style.maxHeight = null;
			} else {
			  panel.style.maxHeight = panel.scrollHeight + "px";
			} 
		  }
		}
		
		jQuery(document).ready(function(jQuery){
			//open popup
			jQuery('.signup-popup-trigger').on('click', function(event){
				event.preventDefault();
				jQuery('.signup-popup').addClass('is-visible');
				jQuery('body').addClass("active_popup");
			});
			
			jQuery('.video-popup-trigger').on('click', function(event){
				event.preventDefault();
				jQuery(".video-popup iframe").attr("src", "https://www.youtube.com/embed/ZptC6Xd_dzs?ecver=2");
				jQuery('.video-popup').addClass('is-visible');
				jQuery('body').addClass("active_popup");
			});
			
			jQuery('.sales-popup-trigger').on('click', function(event){
				event.preventDefault();
				jQuery('.sales-popup').addClass('is-visible');
				jQuery('body').addClass("active_popup");
			});

			jQuery('.partner-popup-trigger').on('click', function(event){
				event.preventDefault();
				jQuery('.partner-popup').addClass('is-visible');
				jQuery('body').addClass("active_popup");
			});
	
			
			jQuery('.bio-popup-open').click(function(e) {
                e.preventDefault();				
				var n= jQuery(this).data('number');
				jQuery('.bio-popup-open').removeClass('is-visible');
				jQuery('.bio-popup-'+n).addClass('is-visible');
				jQuery('body').addClass("active_popup");
            });
			
			jQuery('.bio-popup-lteam-open').click(function(e) {
                e.preventDefault();				
				var n= jQuery(this).data('number');
				jQuery('.bio-popup-lteam-open').removeClass('is-visible');
				jQuery('.bio-popup-'+n).addClass('is-visible');
				jQuery('body').addClass("active_popup");
            });
			
		
	 
			
			//close popup
			jQuery('.popup-wrap').on('click', function(event){
				if( jQuery(event.target).is('.sp-popup-close') || jQuery(event.target).is('.popup-wrap') ) {
					event.preventDefault();
					jQuery(this).removeClass('is-visible');
					jQuery('#signup-content-1').show();
					jQuery('#signup-content-2').hide();
					jQuery('body').removeClass("active_popup");
				}			
			});
			
			jQuery('.popup-wrap').on('click', function(event){
				if( jQuery(event.target).is('.lt-popup-close') || jQuery(event.target).is('.popup-wrap') ) {
					event.preventDefault();
					jQuery(this).removeClass('is-visible');
					jQuery('#signup-content-1').show();
					jQuery('#signup-content-2').hide();
					jQuery('body').removeClass("active_popup");
				}			
			});
			
			//close popup when clicking the esc keyboard button
			jQuery(document).keyup(function(event){
				if(event.which=='27'){
					jQuery('.popup-wrap').removeClass('is-visible');
					jQuery('#signup-content-1').show();
					jQuery('#signup-content-2').hide();
				}
			});
			
			jQuery('.signup-here').click(function() {
				jQuery('#signup-content-1').hide();
				jQuery('#signup-content-2').show();
			});
		});
		
		
		window.footerLinkClicked = false;
