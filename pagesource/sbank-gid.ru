<script>
    $(document).ready(function () {
        var rates = [{"code":"BYN","value":29.3039,"nominal":1},{"code":"USD","value":57.4942,"nominal":1},{"code":"EUR","value":70.8099,"nominal":1},{"code":"KZT","value":17.818,"nominal":100},{"code":"PLN","value":16.8063,"nominal":1},{"code":"UAH","value":21.8402,"nominal":10}];
        rates.unshift({code: 'RUB', value: 1, nominal: 1});

        var sortRates1 = ['RUB', 'USD', 'EUR', 'BYN', 'UAH', 'PLN', 'KZT'];
        var sortRates2 = ['USD', 'EUR', 'RUB', 'BYN', 'UAH', 'PLN', 'KZT'];

        $.each( rates.sort(function (a,b) {
            return sortRates1.indexOf(a.code) - sortRates1.indexOf(b.code);
        }), function( i, currency ) {
            $('#rateValute_1').append('<option value="'+currency.value+'" ' +
                'data-nominal="'+currency.nominal+'" class="'+currency.code+'">'+currency.code+'</option>');
        });

        $.each( rates.sort(function (a,b) {
            return sortRates2.indexOf(a.code) - sortRates2.indexOf(b.code);
        }), function( i, currency ) {
            $('#rateValute_2').append('<option value="'+currency.value+'" ' +
                'data-nominal="'+currency.nominal+'" class="'+currency.code+'">'+currency.code+'</option>');
        });

        ex_rate();
    });

    function ex_rate() {
        var currency_amount = $('#ex_first_in').val();
        var left_valute = $('#rateValute_1 :selected');
        var right_valute = $('#rateValute_2 :selected');
        var result = left_valute.val() * currency_amount / right_valute.val();

        if (left_valute.val() !== right_valute.val()) {
            if (left_valute.data('nominal') < right_valute.data('nominal')) {
                result *= right_valute.data('nominal');
            } else {
                result /= left_valute.data('nominal');
            }
        }
        result = Math.round(result * 100) / 100;

        $('#ex_result_out').text(result);
        $('#ex_first_in_label').text(left_valute.text());
        $('.exchange-converter .result-area .label').text(right_valute.text());
    }
</script>
