            function getRandomInt(min, max)
            {
               return (Math.floor(Math.random() * (max - min + 1)) + min);
            }
          
           jQuery(document).ready(function($) {
               $(".green").each(function(){
                    $(this).html( getRandomInt(100, 500));
               }); 
               $(".red").each(function(){
                    $(this).html( getRandomInt(2, 30));
               });
           });

</script>





