    {
        if(!jQuery('.royalSlider').hasClass('rsFullscreen'))
        {
            jQuery(".rsImg").css("margin","0");
        }
    }
    
    function cambia_rsThumbs()
    {
        var rsGCaption_h = jQuery('.rsGCaption').outerHeight();			
        jQuery('.rsThumbs').css('margin-top',rsGCaption_h);	
    }
    
    jQuery('.royalSlider').click(function(){
        setTimeout('reajuste()',500);
    });
    
    setInterval('reajuste()',1000);
    
    //******* AJUSTAR EL TAMAÑO DE LOS THUMBAILS *********/
        
    cambia_rsThumbs();
    
    jQuery('.rsArrowIcn').click(function(){
        setTimeout('cambia_rsThumbs()',500);
    });
    
    jQuery('.rsNavItem').click(function(){
        setTimeout('cambia_rsThumbs()',500);
    });
</script>
