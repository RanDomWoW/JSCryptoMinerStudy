						window.location = $(this).attr('href');
					}
				});
			}
			else {
				$('.navbar-default .dropdown').off('mouseover').off('mouseout');
			}
		}
		
		$(window).resize(function() {
			bindNavbar();
		});
		
		bindNavbar();
		
		
	});
</script>
