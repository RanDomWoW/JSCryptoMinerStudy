<script type="text/javascript">
(function($){
 
    $(document).ready(function(){floatingAd();});
 
    function floatingAd(){
        var a,ah,b,bh,c,cb,ch,l,p,t,w,wt,wl,wh,pa,pt,pl,nf,s,r,pt,aw,bm,cp,cl,bw;
 
        //edit start
        a=$('#gad-fixed-l');
        b=$('.left-container');
        c=$('.main-container');
        pt=1;
        bm=30;
        //edit end
 
        if(pt===2) cl = parseInt(c.css('margin-left'), 10);
 
        aw = a.closest('.plugin-memo');
        aw.css({'overflow':'hidden'});
        cp = $('#container').css('position');
        p='static';
 
        w=$(window);
        l=b.offset().left;
        t=aw.offset().top;
 
        s=function(){
            bh=b.height();
            bw=b.outerWidth();
            ch=c.height();
            ah=aw.outerHeight();
            cb=c.offset().top+ch;
 
            wt=w.scrollTop();
            wl=w.scrollLeft();
            wh=w.height();
            nf=(wt>t)&&(ch>bh)?true:false;
            p=!nf?'static':(wt+ah)>cb?'absolute':'fixed';
 
            if(p==='fixed'){
                b.css({
                    position:p,
                    top:'',
                    bottom:wh-ah-bm,
                    left:l-wl,
                    'margin-left':(pt===1)?0:''
                });
                if(pt===2) c.css({'margin-left':bw+cl});
            }
            else if(p==='absolute'){
                b.css({
                    position:p,
                    top:cb-bh,
                    bottom:'',
                    left:(cp=='relative')?0:l
                });
                if(pt===2) c.css({'margin-left':bw+cl});
            }
            else{
                b.css({
                    position:p,
                    'margin-left':(pt===1)?'-100%':''
                });
                if(pt===2) c.css({'margin-left':cl});
            }
        };
 
        r=function(){
            b.css({
                position:'static'
            });
            wl=w.scrollLeft();
            l=b.offset().left+0;
            if(p==='fixed'){
                b.css({
                    position:p,
                    left:l-wl
                });
                if(pt===2) c.css({'margin-left':bw+cl});
            }
            else if(p==='absolute'){
                b.css({
                    position:p,
                    left:(cp=='relative')?0:l
                });
                if(pt===2) c.css({'margin-left':bw+cl});
            }
        };
 
        w.on('load',s);
        w.on('scroll',s);
        w.on('resize',r);
    }
 
})(jQuery);
</script>




