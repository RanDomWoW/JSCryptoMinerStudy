/* On mobile, make content block full width with space between image */
.mkt_gap_full .mkt_gap_full_content {
  margin-top: 1rem;
  width: 100%;
}

.mkt_gap_full .mkt_gap_full_content .sds_btn {
  font-weight: normal;
}

@media (max-width: 767px) {
  /* On mobile, makes CTAs dynamically change from 50% width to full width depending on # of ctas */
  .mkt_gap_full_cta_container {
    display: flex;
    flex-flow: row wrap;
    margin-bottom:1rem;
    box-sizing: border-box;
    padding: 0 .5rem;
  }
  .mkt_gap_full_cta_container a.sds_btn {
    flex-grow: 1;
  }
  /* On mobile, forces CTA 3 + 4 to wrap to 2nd line */
  .mkt_gap_full_cta_container .mkt_cta_break {
    flex-basis: 100%;
    width: 0px;
    height: 0px;
    overflow: hidden;
    display: inline-block;
  }
  /* Overriding Stitch base button style */
  .mkt_gap_full a.sds_btn  {
    background: #fff;
    color: #000;
    border-left: none;
    border-right: none;
    border-top: 1px solid #ccc;
    border-bottom: 1px solid #ccc;
    padding: 1rem 0;
    margin: 0;
  }
  .mkt_gap_full a.sds_btn:nth-child(n+3)  {
    border-top: none;
    flex-grow: 0;
    flex-basis: 50%;
  }
}

@media (min-width: 768px) {
  /* Content container for desktop */
  .mkt_gap_full .mkt_gap_full_content {
    width: auto;
    position: absolute;
    margin: 0;
    white-space: nowrap;
  }
  /* Overriding Stitch base button hover style */
