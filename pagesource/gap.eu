        var a = {
          'cid': cid
        };
    
        if (width) {
          a['width'] = width;
        }
    
        return a;
      }
    
      function chunkify(e,f,i){if(f<2)return[e];var l,r=e.length,s=[],h=0;if(r%f==0)for(l=Math.floor(r/f);h<r;)s.push(e.slice(h,h+=l));else if(i)for(;h<r;)l=Math.ceil((r-h)/f--),s.push(e.slice(h,h+=l));else{for(f--,r%(l=Math.floor(r/f))==0&&l--;h<l*f;)s.push(e.slice(h,h+=l));s.push(e.slice(l*f))}return s}
      function buildFlyout (flyout) {
        function loopCat (order) {
          var rootCatElt = '';
          var index;
          var currentColumn;
          var currentCategory;
          var row = {};
          row.columns = jQuery('');
          row.width = 0;
          var y;
          var totalColumns = flyout.width;
          var columnWidth = 0;
          var multi = 24 / totalColumns;
          var gridClass;
      
          for (y = 0; y < order.length; y++) {
            // probably should be currentColumn or current category
            currentColumn = order[y];
            var currentColumnEle = jQuery('<ul class="mm_listWrapper">');
            if (currentColumn.length > 1) {
              currentColumnEle.addClass('mm_multiCatColumn');
            }
            for (var x = 0; x < currentColumn.length; x++) {
              // Current Category Object in Order
              currentCategory = currentColumn[x];
              // user cid in Category Object to get matching category element on page
              rootCatElt = leftNav.find('li[data-cid="' + currentCategory.cid + '"]').clone();
              // If category element found on page
              if (rootCatElt.length > 0) {
                // if no width set, assume a width of one column
                if (!currentCategory.hasOwnProperty('width') || currentCategory.width === 1) {
                  currentCategory.width = 1;
                  currentColumnEle.append(rootCatElt);
                  columnWidth = 1;
                } else {
                  // is more than one column
                  columnWidth = currentCategory.width;
                  // list of all links
                  var itemsToSplit = rootCatElt.find('ul').detach();
                  itemsToSplit = itemsToSplit.find('li');
      
                  // split links into equal chunks
                  var splitToColumns = chunkify(itemsToSplit, currentCategory.width, true);
                  currentColumnEle.append('<li class="rootElt"><div class="mm_multiColumn"><div/></li>');
                  // wrap each column in a wrapper
