<script type="text/javascript">
	var wcdEmailParameterScrub = function(pageURL) {
	  if (query) {
	    query.parse({ rebuild: true });
	    // get the email field
	    var wcdEmailParameter = query.get('email');
	    if (wcdEmailParameter !== undefined) {
	      wcdEmailParameter = encodeURIComponent('email=' + wcdEmailParameter);
	      if (pageURL.indexOf(wcdEmailParameter + '%26') > -1) {
	        wcdEmailParameter = wcdEmailParameter + '%26';
	      }
	      //clear email from pageURL
	      pageURL = pageURL.replace(wcdEmailParameter, '');
	    } 
	    // get the emailAddress field
	    var wcdEmailAddressParameter = query.get('emailAddress');
	    if (wcdEmailAddressParameter !== undefined) {
	      wcdEmailAddressParameter = encodeURIComponent('emailAddress=' + wcdEmailAddressParameter);
	      if (pageURL.indexOf(wcdEmailAddressParameter + '%26') > -1) {
	        wcdEmailAddressParameter = wcdEmailAddressParameter + '%26';
	      }
	      //clear email from pageURL
	      pageURL = pageURL.replace(wcdEmailAddressParameter, '');
	    } 
	  }

	  // make sure there aren't any other hidden email fields
	  // and have a backup for query missing
	  if (pageURL.indexOf('%40') > -1) {
	    var midpoint = pageURL.indexOf('%40');
	    var endpoint = pageURL.indexOf('%26', midpoint);
	    // if email is at end of the string
	    // trim everything after last equal sign
	    if (endpoint === -1) {
	      var startpoint = pageURL.lastIndexOf('%3D');
	      pageURL = pageURL.substring(0,startpoint);
	    } else {
	      // if email is not at the end
	      // find last equal sign before email
	      var tempURL = pageURL.substring(0, midpoint);
	      // if multiple parameters, clear last one
	      // in tempURL
	      if (tempURL.indexOf('%26') > -1) {
	        var startpoint = tempURL.lastIndexOf('%26');
	      } else {
	        var startpoint = tempURL.lastIndexOf('%3D');
	      }
	      pageURL = pageURL.substring(0, startpoint) + pageURL.substring(endpoint);
	    }
	  }
	  return pageURL;
	}
</script>
<!-- END wcdEmailParameterScrub -->

