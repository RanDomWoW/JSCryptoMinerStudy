/* *********CTA drop down Extension*********** */
/*https://codepen.io/Tont/pen/hdsev*/
ul.mkt_gap_btn_dropdown_menu {
  z-index: 500;
  margin: 0 auto;
  -webkit-font-smoothing:antialiased;
}
ul.mkt_gap_btn_dropdown_menu > li {
  list-style-type:none;
  position:relative;
  display: inline-block;
  width: 100%;
}

ul.mkt_gap_btn_dropdown_menu > li label {
  padding: 1rem 0;
  background: #fff;
  border-bottom: 1px solid #ccc;
  border-top: 1px solid #ccc;
  margin: 0 1rem;
}
ul.mkt_gap_btn_dropdown_menu > li label:after {
  content:"+";
  padding-left:.5rem;
}
ul.mkt_gap_btn_dropdown_menu > li input:checked ~ label:after {
  content:"–";
  padding-left:.65rem;
}

/*hide the inputs*/
ul.mkt_gap_btn_dropdown_menu > li input {display:none}

/*show the second levele menu of the selected voice*/
ul.mkt_gap_btn_dropdown_menu > li input:checked ~ ul.mkt_gap_btn_dropdown_submenu {
  max-height:800px;
  transition:max-height 0.3s ease-in;
}
/*style for the second level menu*/
ul.mkt_gap_btn_dropdown_submenu {
  padding:0;
  list-style-type:none;
  background:#fff;
  transition:max-height 0.5s ease-out;
  position:relative;
  min-width:100%;
  z-index: 1200;
  padding: 0 .5rem;
  box-sizing: border-box;
  max-height: 0;
  overflow:hidden;
}
ul.mkt_gap_btn_dropdown_submenu li {
  padding:1rem 0;
  border-bottom: 1px solid #ccc;
  display: block;
  float: left;
  width: 50%;
