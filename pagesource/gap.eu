<script type="text/javascript">
var wcdEUUKSite;
var wcdLocaleIdx;
var wcdLocale;
var preferredLocale;

var wcdUKEUSiteDecider = function() {
	// Find domain suffix and seg wcdEUUKSite
	var euUkDomain = location.hostname;
	wcdEUUKSite = 'uk';
	if( euUkDomain.substr( euUkDomain.length - 3 ) == '.eu' ) {
		wcdEUUKSite = 'eu';
	}

	// Write domain specific CSS for marketing content based on wcdEUUKSite variable
	if( wcdEUUKSite == 'eu' ) {
		document.write('<style type="text/css"> .displayOnUK { display:none } .displayOnEU { display:block } span.displayOnEU { display: inline;} #footerLanguageToggle { display:block } #euLanguageDropdown { display:block } </style>');
	}
	else {
		document.write('<style type="text/css"> .displayOnUK { display:block } span.displayOnUK { display: inline;} .displayOnEU { display:none } #footerLanguageToggle { display:none }   #euLanguageDropdown { display:none } </style>');
	}

	// Customer service page domain references
	if( location.pathname == '/customerService/info.do' ) {
		jQuery( document ).ready( function(e) {
			var gapDomainRefs = jQuery('#mainContent span.gapDomainRef');
			if( gapDomainRefs.length > 0 ) {
				for( var i=0; i < gapDomainRefs.length; i++ ) {
					if( wcdEUUKSite == 'eu' ) {
						gapDomainRefs[i].innerHTML == 'gap.eu';
					}
					else {
						gapDomainRefs[i].innerHTML == 'gap.co.uk';
					}
				}
			}

			var brDomainRefs = jQuery('#mainContent span.brDomainRef');
			if( brDomainRefs.length > 0 ) {
				for( var i=0; i < brDomainRefs.length; i++ ) {
					if( wcdEUUKSite == 'eu' ) {
						brDomainRefs[i].innerHTML == 'bananarepublic.gap.eu';
					}
					else {
						brDomainRefs[i].innerHTML == 'bananarepublic.gap.co.uk';
					}
				}
			}
		});
	}

	// Set WCD locale value to English. Session-based. Run on dom:loaded
	jQuery( document ).ready( function(e) {
		/* DEFAULT all marketing content to English for all Languages */
		wcdLocale = 'en_GB';
		preferredLocale = 'en_GB';


		if( wcdEUUKSite == 'uk' ) {
			wcdLocale = 'en_GB';
			preferredLocale = 'en_GB';
		}
		// Does NOT touch locale cookie, just DOM value
		// Set WCD locale index value - used for checking/setting arrays of copy
		wcdLocaleIdx = 0; // English
		if( wcdLocale == 'de_DE' ) { wcdLocaleIdx = 0; } // German
		else if( wcdLocale == 'es_ES' ) { wcdLocaleIdx = 0; } // EU Spanish
		else if( wcdLocale == 'nl_NL' ) { wcdLocaleIdx = 0; } // Dutch

		/* overide Hamburger Nav Language links */
		jQuery( "#nav .universal_in-nav .nav--section_dark li:contains('Deutsch')" ).css( "display", "none" );
		jQuery( "#nav .universal_in-nav .nav--section_dark li:contains('Español')" ).css( "display", "none" );
		jQuery( "#nav .universal_in-nav .nav--section_dark li:contains('Nederlands')" ).css( "display", "none" );

		//console.info("Default EN: " + wcdLocaleIdx);
		//console.info("wcdLocale: " + wcdLocale);
		//console.info("preferredLocale: " + preferredLocale);

		if( window["Prototype"] !== undefined ) {
			document.fire('wcdLocale:ready');
		}
		jQuery(document).trigger('wcdLocale:ready');
	});
}
wcdUKEUSiteDecider();
</script>
