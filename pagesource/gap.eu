    color: #999;
    border-bottom: 2px solid #999;
  }
  /* Fixes flex styling of CTAs for desktop */
  .mkt_gap_full_cta_container {
    display: inline-flex;
  }
  .mkt_gap_full_cta_container .mkt_cta_break {
    display: none;
  }
}
@media (min-width: 1024px) {
  .mkt_gap_full_cta_container a.sds_btn--underline {
    font-size: .9375rem;
  }
