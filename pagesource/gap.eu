    function htmlDecode(inp){
      var replacements = {'&quot;':'"','&apos;':'\'','&amp;':'&', '&#x27;':'\'', '&#39;':'\'', '&#153':'&#8482;' , '&#124':'|', '&#174' : '®', '&#169' : '©'};
