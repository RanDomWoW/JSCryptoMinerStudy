			if( ( cookieLevelSetter.userOptInCookies != 'true' && seenCookiePopup != 'true' && kwidInURL != '1' && ! linkedFromCoi ) || interceptForce == 'true' ) {
				cookieLevelSetter.launchCookieIntercept();
			}
		}
	},
	
	// Checks for mktUniversalEUCookies and set cookie
	setEUCookiesCookie: function( userOptInCookieLevel ) {
		var currentCookieLevel = gidLib.getCookieVar('mktUniversalEUCookies', 'userOptInCookies');
