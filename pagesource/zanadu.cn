<script>
    $(function () {
        $('#top_full_width_carousel').carouFredSel({
            responsive: true,
            prev: ".carousel-wrapper .prev",
            next: ".carousel-wrapper .next",
            pagination: ".carousel-wrapper .pagination .bullets",
            auto: 3000,
            items: {
                visible: 1,
                height: '32.8%'
            },
            scroll: {
                fx: 'crossfade',
                duration: 1000,
                pauseOnHover: true,
                onBefore: function (data) {
                    var visible = data.items.visible;
                    if ($(visible).hasClass('advertise')) {
                        $('header .logo').fadeOut();
                    } else {
                        $('header .logo').fadeIn();
                    }
                }
            }
//            swipe: {
//                touch: true,
//                mouse: false,
//                fx: 'slide',
//                duration: 200
//            },

        });
        $('.next,.prev', '.carousel-wrapper').on('click', function () {
            $('#top_full_width_carousel').trigger('configuration', {'auto': false});
        });

        $(".carousel-wrapper").swipe({
            excludedElements: "button, input, select, textarea, .noSwipe",
            swipeLeft: function () {
                $('#top_full_width_carousel').trigger('next', 1);
            },
            swipeRight: function () {
                $('#top_full_width_carousel').trigger('prev', 1);
            }
        });
    });
</script>


