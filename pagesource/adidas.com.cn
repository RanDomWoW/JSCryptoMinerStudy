<script id="mini-shopping-cart-template" type="text/template">
	<div class="mincart-head">
		<p class="shop-num-box">
			购物袋 共 <span class="shop-num">{{itemCount}}</span> 件商品
		</p>
	</div>
	<div class="mincart-content">
		<div class="tinyscrollbar events-mincart-tinyscrollbar">
			<div class="viewport">
				<div class="overview">
					<ul>
						{{#each shoppingCartLineMap}}
							{{#ifnot isGift}}
								<li>
									<div class="mincart-item float-clearfix">
										{{#if extraData.isPersonalizationProduct}}
											<a class="mincart-img" href="{{base}}/itemToRedirect/{{itemCode}}?recipeId={{extraData.personalized.recipeId}}"> 
												{{#isNotNullOrEmpty extraData.personalized}}
													<img src="{{extraData.personalized.imageUrl}}&wid=230&hei=230">
												{{else}}
													<img src="{{#imageUrlHelper itemPic '230X230'}}{{/imageUrlHelper}}">
												{{/isNotNullOrEmpty}}
											</a>
										{{else}}
											<a class="mincart-img" href="{{base}}/itemToRedirect/{{itemCode}}"> 
												<img src="{{#imageUrlHelper itemPic '230X230'}}{{/imageUrlHelper}}">
											</a>
										{{/if}}

										<div class="mincart-detail">
											{{#if extraData.isPersonalizationProduct}}
												<a class="mincart-name" href="{{base}}/itemToRedirect/{{itemCode}}?recipeId={{extraData.personalized.recipeId}}"> 
													{{itemName}}
												</a>
											{{else}}
												<a class="mincart-name" href="{{base}}/itemToRedirect/{{itemCode}}"> 
													{{itemName}}
												</a>
											{{/if}}
											<p class="mincart-color">
												颜色: <span>{{extraData.color}}</span>
											</p>
											<p class="mincart-size">
												尺码: <span>{{extraData.size}}</span>
											</p>
											
											{{#if extraData.isPersonalizationProduct}}
												{{#isNotNullOrEmpty extraData.personalized.personalName}}
													<p class="">名字: <span>{{extraData.personalized.personalName}}</span></p>
												{{/isNotNullOrEmpty}}
												{{#isNotNullOrEmpty extraData.personalized.personalName}}
													<p class="">号码: <span>{{extraData.personalized.personalNumber}}</span></p>
												{{/isNotNullOrEmpty}}
												{{#isNotNullOrEmpty extraData.personalized.partValueMap.left_text}}
													<p class="">左脚文字: <span>{{extraData.personalized.partValueMap.left_text}}</span></p>
												{{/isNotNullOrEmpty}}
												{{#isNotNullOrEmpty extraData.personalized.partValueMap.left_text}}
													<p class="">右脚文字: <span>{{extraData.personalized.partValueMap.right_text}}</span></p>
												{{/isNotNullOrEmpty}}
												{{#isNotNullOrEmpty extraData.personalized.fee}}
													<p class="">印制费: <span>¥{{extraData.personalized.fee}}</span></p>
												{{/isNotNullOrEmpty}}
											{{/if}}
											<div class="price-item">
												{{#equals salePrice listPrice}}
													<span class="goods-price price-single">￥{{salePrice}}</span> 
												{{else}}
													<span class="goods-price">￥{{salePrice}}<del class="original-price">￥{{listPrice}}</del></span>
												{{/equals}}
													<span class="price-num">x {{quantity}}</span>
											</div>
											<a class="mincart-del events-mincart-del" lineId="{{id}}" quantity="{{quantity}}" subTotalAmt="{{subTotalAmt}}" key="{{extraData.index}}">删除</a>
										</div>
										{{#if extraData.isVipRoomProduct}}
											<div class="info-stock">商品已失效</div>
										{{else}}
											{{#equals status 'ITEM_LIFECYCLE_OFF_SHELF'}}
												<div class="info-stock">已下架</div>
											{{/equals}}
											{{#equals status 'OUT_OF_STOCK'}}
