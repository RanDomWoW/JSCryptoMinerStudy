        <script type="text/javascript">
        jQuery(function(){
            jQuery('.btn_vote input').click(vote_track); 
           
            jQuery(".basic").jRating({
    			bigStarsPath : 'http://www.radiosubasio.it/wp-content/plugins/charts2/components/jRating/icons/stars.png',
    			smallStarsPath : 'http://www.radiosubasio.it/wp-content/plugins/charts2/components/jRating/icons/small.png',
                phpPath: 'http://www.radiosubasio.it/wp-content/plugins/charts2/api.php?case=client&action=vote_track',
                length: 5,
                rateMax: 5,
                onSuccess: function(element){
                    var tot_votes = parseInt(jQuery('#track_id_'+jQuery(element).data('id')+' .ranking .tot_votes').text());
                    jQuery('#track_id_'+jQuery(element).data('id')+' .ranking .tot_votes').text(tot_votes+1);
                }
            });
        });
        function vote_track(){
            var obj = {
                track_id: jQuery(this).data('track_id'),
                vote: jQuery(this).val()
            }
            jQuery.post('http://www.radiosubasio.it/wp-content/plugins/charts2/api.php?case=client&action=vote_track', obj, function(data){
                jQuery('#track_id_'+obj.track_id+' .ranking .number').text(data.ranking);
                jQuery('#track_id_'+obj.track_id+' .ranking .tot_votes').text(data.tot_votes);
            });
        }
        
        function stopAllJw(id){
            
            for(i in jwplayer_instance){
                if(jwplayer_instance[i]!=id){
                    if(jwplayer(jwplayer_instance[i]).getState()=='PLAYING');
                        jwplayer(jwplayer_instance[i]).stop();
                }
            }
        }
        
        var jwplayer_instance = [];
        </script>
