function populate_home_posts(cat){
    
    var ajaxurl = 'wp-admin/admin-ajax.php';

    var data = {
		action: 'home_posts',
		cat: cat
	};
    
    jQuery('#mainNewsImage').html('<div class="loading">Loading...</div>');
