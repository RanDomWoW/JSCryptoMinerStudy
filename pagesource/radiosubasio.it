        <script type="text/javascript">   
        
        var jwplayer_istance = [];
          
        function isMobile(){
    	   return (/Zune|Windows Phone|Android|webOS|iPhone|iPad|iPod|BlackBerry/i).test(navigator.userAgent);
        }
        
        // Detect pc family
        function isPc(){
        	return !(/Zune|Windows Phone|Android|webOS|iPhone|iPad|iPod|BlackBerry/i).test(navigator.userAgent);
        }
        
        // Windows Phone detecting
        function isWindowsPhone(){
        	return (/Zune|Windows Phone/i).test(navigator.userAgent);
        }
        
        // Android Phone detecting
        function isAndroid(){
        	return (/Android/i).test(navigator.userAgent);
        }
        
        // iOS Phone detecting
        function isIOS(){
        	return (/iPhone|iPad|iPod/i).test(navigator.userAgent);
        }

        // Detect pc family
        function isFirefox(){
            return (/firefox/i).test(navigator.userAgent);
        }
        
        function switchFileFormat(url){
            if(isMobile()){
                if(isIOS()){
                    return url.replace('rtmp://','http://')+'/playlist.m3u8';
                }else{
                    return url.replace('rtmp://','rtsp://');
                }
            }else{
                return url;
            }
        }
        
        function switchFileFormat2(streamer, file){

                if(isIOS()){
                    return streamer.replace('rtmp://','http://')+'/default/'+fileExt(file)+':'+detectFirstSlash(file)+'/playlist.m3u8';
                }else{
                    return streamer.replace('rtmp://','rtsp://')+file;
                }

        }
        
        function fileExt(file){
            return file.split('.').pop();
        }
        
        function detectFirstSlash(file){
            return file.replace(/\/{0,}(.*)/, '$1');
        }
        
        function stopAllJwp(thisId){
            for(obj in jwplayer_istance){
                if(jwplayer_istance[obj]!=thisId)
                    jwplayer(jwplayer_istance[obj]).stop();
            }
        }
        
        // Detect flash plugin
        function flashExists(){ 
            if(window.ActiveXObject){
                try{
                    return new ActiveXObject('ShockwaveFlash.ShockwaveFlash');
                }catch(e){
                    return false;
                }
                
            }else{
                return typeof navigator.plugins != undefined && typeof navigator.plugins["Shockwave Flash"] == "object"; 
            } 
        }
        
        </script>
