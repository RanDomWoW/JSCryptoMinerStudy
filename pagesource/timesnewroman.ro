<script type="text/javascript">

    function posBanners() {
	
        var ww = jQuery(window).width();
	var header = jQuery('#header').height();
        var main = jQuery('#wrapper');
        var scrollTop = jQuery(window).scrollTop();
        var mainMargin = (ww - main.outerWidth())/2;
        var leftBanner = jQuery('#bannerLeft');
        var rightBanner = jQuery('#bannerRight');

        if (scrollTop <= (main.offset().top + header)) {
            scrollTop = main.offset().top + header;
        }
        if ( main.height() < (scrollTop + leftBanner.height())) {
            scrollTop = main.height() - leftBanner.height();
        }

        leftBanner.css({
            'position': 'absolute',
            'left': Math.ceil(parseFloat(mainMargin)) - 160,
            'width': '160px',
            'height': '600px',
            'top': scrollTop
        });
        rightBanner.css({
            'position': 'absolute',
            'left': Math.ceil(parseFloat(mainMargin)) + main.outerWidth(),
            'width': '160px',
            'height': '600px',
            'top': scrollTop
        });

        leftBanner.show();
        rightBanner.show();

    }

    jQuery(window).on('load resize scroll',function(e){
        posBanners();
    });

    jQuery.noConflict();
	
</script>

