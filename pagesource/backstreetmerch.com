                    product += "<h3>PRICE:&nbsp;<a href='" + data.Products[i].URL + "'>"+ data.Products[i].Currency +" "+ FormatCurrency(data.Products[i].UnitPrice) +"</a></h3>";
                }
                product += "<a href ='" + data.Products[i].URL + "' style='color:white' class = 'btn btn-custom-red'><i class='fa fa-shopping-cart'></i>&nbsp;Buy Now</a>";
                product += "</div>";
                product += "</div>";
                product += "</div>";
            }
            product += "</div></div>";
            $("#TopItems").append(product);
        });

    });

    function FormatCurrency(x) {
        x = x.toFixed(2);
        var retVal = x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
        return retVal;
    };

</script>



