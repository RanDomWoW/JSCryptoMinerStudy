<script>
  setInterval(function(){
  let data = JSON.parse(res||'');
  function timeAgo(time, now){
      var units = [
          { name: "second", limit: 60, in_seconds: 1 },
          { name: "minute", limit: 3600, in_seconds: 60 },
          { name: "hour", limit: 86400, in_seconds: 3600  },
          { name: "day", limit: 604800, in_seconds: 86400 },
          { name: "week", limit: 2629743, in_seconds: 604800  },
          { name: "month", limit: 31556926, in_seconds: 2629743 },
          { name: "year", limit: null, in_seconds: 31556926 }
      ];
      var diff = (new Date(now) - time) / 1000;

      var i = 0, unit;
      while (unit = units[i++]) {
          if (diff < unit.limit || !unit.limit){
              var diff =  Math.floor(diff / unit.in_seconds);
              return diff + " " + unit.name + (diff>1 ? "s" : "");
          }
      };
  }

  let index = Math.floor((Math.random() * Object.keys(data).length));
  let now =moment.tz(new Date(),'America/Chicago').format('YYYY/MM/DD HH:mm:ss')
  document.getElementById('time-ago').innerHTML = timeAgo(new Date(data[index]['date']), now) + ' ago';
  document.getElementById('proof-what').innerHTML = 'Recently ' + data[index]['action'] +' NowLifestyle.com';
  document.getElementById('proof-who').innerHTML = data[index]['name']+' from '+ data[index]['from'];
  $('#proof').toggleClass('show-proof');
  }, 5000);
</script>

