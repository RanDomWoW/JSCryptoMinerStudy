                if(this.displayTrigger) {
                    var flag = 0;
                    var scrollPos = 0;
                    if(this.displayTrigger == 'scroll_up') {
                        Event.observe(window, 'scroll', function() {
                            if (flag == 0) {
                                var yPos = window.scrollY;
                                if (yPos < scrollPos) {
                                    this.show.delay(this.displayDelay);
                                    flag = 1;
                                } else {
                                    scrollPos = yPos;
                                }
                            }
                        }.bind(this));
                    } else {
