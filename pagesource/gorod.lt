<!-- Действия -->
<form name="new_advert" id="new_advert" method="post" action="/">
  <div class="row" id="filter_list_adverts">
    <!-- Подать объявление -->
    <div class="col-md-3" style="margin-bottom: 1.5rem;">
      <a href="/adverts/new" id="new_advert_button" class="btn btn-block btn-primary">
        <i class="fas fa-plus fa-fw"></i>
        Подать объявление
      </a>
    </div>
    <!-- Сортировать -->
    <div class="col-md-6">
      <div class="row">
        <div class="col-md-8">
          <!-- Тип -->
          <div class="input-group" style="margin-bottom: 1.5rem;">
            <div class="input-group-prepend">
              <span class="input-group-text">
                Тип
              </span>
            </div>
            <select name="type_advert" class="form-control">
              <option value="0" selected="selected">Все</option>
              <option value="1" {type1}>Продам</option>
              <option value="2" {type2}>Куплю</option>
              <option value="3" {type3}>Услуги</option>
              <option value="4" {type4}>Объявляю</option>
            </select>
            <!-- Раздел -->
            <div class="input-group-append input-group-prepend">
              <span class="input-group-text">
                Раздел
              </span>
            </div>
            <select name="category_advert" class="form-control">
              <option value="0" selected="selected">Все</option>
              <option value="1" {category1}>Мобильная связь</option>
              <option value="2" {category2}>Комп. техника</option>
              <option value="3" {category3}>Флора\фауна</option>
              <option value="4" {category4}>Недвижимость</option>
              <option value="5" {category5}>Бытовая техника</option>
              <option value="6" {category6}>Транспорт</option>
              <option value="7" {category7}>Другое</option>
              <option value="8" {category8}>Услуги</option>
              <option value="9" {category9}>Мебель</option>
              <option value="10" {category10}>Хобби\Коллекционирование</option>
            </select>
          </div>
        </div>
        <!-- Сортировать -->
        <div class="col-md-4">
          <button type="submit" class="btn btn-secondary btn-block" id="sort-button" style="margin-bottom: 1.5rem;">
            <i class="fas fa-filter fa-fw"></i>
            Сортировать
          </button>
        </div>
      </div>
    </div>
    <!-- Искать -->
    <div class="col-md-3">
      <div class="input-group" style="margin-bottom: 1.5rem;">
        <input type="text" class="form-control" id="search_field" name="search" value="" placeholder="Поиск..." />
        <div class="input-group-append">
          <button type="submit" class="btn btn-secondary" id="search-button">
            <i class="fas fa-search fa-fw"></i>
          </button>
        </div>
      </div>
    </div>
  </div>
</form>

