										<input type="text" name="popup_home_email" id="popup_home_email" placeholder="E-posta" data-validate-type="email" />
					<span class="input-error">E-posta adresinizi yanlış girdiniz.</span>
				</div>
				<div class="modal-newsletter-gender form-group">
					<label for="one" class="custom-radio">
						<input id="one" type="radio" name="gender" value="K" data-validate-type="radio" />
						Kadın					</label>
					<label for="two" class="custom-radio">
						<input id="two" type="radio" name="gender" value="E" data-validate-type="radio" />
						Erkek					</label>
					<span class="input-error">Lütfen cinsiyetinizi seçiniz.</span>
				</div>
				<button id="devamEt" class="btn-primary" type="button" onclick="newsletterSubmit();">E-BÜLTENE ÜYE OL</button>
			</form>
			<div class="modal-newsletter-actions tablet-desktop-only">
				<button class="btn-text" onclick="newsletterModalClose();">Teşekkürler, şu an değil</button>
				<button class="btn-text" onclick="newsletterModalClose(); createCookie('lidy[popup-home]','1',365);">Bir daha gösterme</button>
			</div>
			<button class="modal-newsletter-close" type="button" onclick="newsletterModalClose();">Kapat</button>
		</div>
	</div>

	<script>
		function newsletterSubmit() {
			$('#popupHomeForm input').blur();

			if ($('#popupHomeForm .input-error:visible').length === 0) {
				$('.modal-newsletter').hide();
				var email = $('#popup_home_email').val();
				var gender = $('input[name=gender]:checked').val();
				$.ajax({
					url: '/Customer/ajax',
					type: 'POST',
					data: {act: 'popup-home', email: email, gender: gender},
					success: function(data) {
						if (data == 'VAR') {
							modal({
								content: "Aynı email adresiyle bültene kaydınız bulunmaktadır.",
								footer: true,
								footerButtons: [{
									buttonClass: 'btn-primary',
									buttonAction: 'confirm',
									buttonText: 'TAMAM'
								}],
								callback: function() {
									modalRemove();
									$('.modal-newsletter').show();
								}
							});
						} else {
							createCookie('lidy[popup-home]','1',365);
							modal({
								content: "Teşekkürler! Kupon kodunuz mail adresinize gönderilmiştir.",
								footer: true
							});
						}
					},
					error: function(data) {
						modal({
							content: "Lütfen tekrar deneyin."
						});
					}
				});
			}
		}

		function newsletterModalClose() {
			$('.modal-newsletter').remove();
		}

		$(document).ready(function() {
			$('body').on('click', function(evt) {
				if (evt.target.className === 'modal-newsletter') {
					newsletterModalClose();
				}
			});
		});
	</script>

