    <script type="text/javascript" language="javascript">
        $(document).ready(function () {
            $("#RETAIL").click(function () {
                $('.MobileMenuFrame').slideUp();
                if ($('#RETAILMENU').is(':hidden'))
                    $("#RETAILMENU").slideDown();
            });
            $("#WHOLESALE").click(function () {
                $('.MobileMenuFrame').slideUp();
                if ($('#WHOLESALEMENU').is(':hidden'))
                    $("#WHOLESALEMENU").slideDown();
            });


            $("#SMES").click(function () {
                $('.MobileMenuFrame').slideUp();
                if ($('#SMESMENU').is(':hidden'))
                    $("#SMESMENU").slideDown();
            });

            $("#CORPORATE").click(function () {
                $('.MobileMenuFrame').slideUp();
                if ($('#CORPORATEMENU').is(':hidden'))
                    $("#CORPORATEMENU").slideDown();
            });


        });
        $(document).ready(function () {
            var MenuName;
            $('.MenuButton').hover(
            function () {
                MenuName = '#' + $(this).attr('id') + 'MENU';
                $('.MenuFrame:not(' + MenuName + ')').fadeOut(250);
                if ($(MenuName).css('display') == 'none') { $(MenuName).show('slide', { direction: 'left' }, 600); }

            }, function () { });
            $('.MenuFrame').hover(function () { },
	  function () { window.setTimeout(function () { $('.MenuFrame').fadeOut(250); }, 100); });
        });

        function SwitchTopMenus(x) {
            $('.MenuFrame').fadeOut(250);
            if (x == 0)
            {
                $('#DIVISIONS_MENU').fadeOut(250);              
                $('#RETAIL_MENU').fadeIn(250);
                $('#CORPORATE_MENU').fadeOut(250);

                document.getElementById("Retail_btn").className = "Class1";
                document.getElementById("Divisions_btn").className = "Class2";                
                document.getElementById("corporate_btn").className = "Class2";

            }
            else if (x == 1)
            {
                $('#RETAIL_MENU').fadeOut(250);              
                $('#DIVISIONS_MENU').fadeIn(250);
                $('#CORPORATE_MENU').fadeOut(250);

                document.getElementById("Retail_btn").className = "Class2";                
                document.getElementById("Divisions_btn").className = "Class1";
                document.getElementById("corporate_btn").className = "Class2";
            }
            else if (x == 2) {
                $('#RETAIL_MENU').fadeOut(250);
                $('#DIVISIONS_MENU').fadeOut(250);               
                $('#CORPORATE_MENU').fadeOut(250);

                document.getElementById("Retail_btn").className = "Class2";
                document.getElementById("Divisions_btn").className = "Class2";               
                document.getElementById("corporate_btn").className = "Class2";
            }

            else if (x == 3) {
                $('#RETAIL_MENU').fadeOut(250);
                $('#DIVISIONS_MENU').fadeOut(250);                
                $('#CORPORATE_MENU').fadeIn(250);

                document.getElementById("Retail_btn").className = "Class2";
                document.getElementById("Divisions_btn").className = "Class2";                
                document.getElementById("corporate_btn").className = "Class1";
            }

        }
    </script>
    
