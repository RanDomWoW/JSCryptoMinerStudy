        function sendMobileEvent(name, str) {
            if ( getCookie('app')=='ios' || getCookie('app')=='android') {
                var now_date = "2018/03/21 00:23:50";
                var sme_obj = new Object();

                var str1 = str.split(';;');
                for (var i in str1) {
                    var str2 = str1[i].split(':');
                    sme_obj[str2[0]] = str2[1].replace(/ /g, '_');
                }

                if (name == 'join') {
                    var mobileKeyWords = new Array('iPhone', 'iPad', 'iPod', 'BlackBerry', 'webOS', 'IEMobile', 'Android', 'Windows CE', 'LG', 'MOT', 'SAMSUNG', 'SonyEricsson');
                    var device_name = '';
                    for (var word in mobileKeyWords) {
                        if (navigator.userAgent.match(mobileKeyWords[word]) != null) {
                            device_name = mobileKeyWords[word];
                            break;
                        }
                    }
                    sme_obj.join_date = now_date;
                    sme_obj.join_os = getCookie('app');
                    sme_obj.join_device = device_name;
                }

                if (name == 'deal_booking') {
                    sme_obj.deal_booking_date = now_date;
                }

                if (name == 'purchase') {
                    sme_obj.purchases_date = now_date;
                }

                if (name == 'add_card') {
                    sme_obj.add_date = now_date;
                }

                if (name == 'add_card_del') {
                    sme_obj.delete_date = now_date;
                    name = 'add_card';
                }

                if (name == 'add_to_cart') {
                    sme_obj.add_date = now_date;
                }

                if (name == 'add_to_cart_del') {
                    sme_obj.delete_date = now_date;
                    name = 'add_to_cart';
                }

                var params = JSON.stringify(sme_obj);
            }

            if ( getCookie('app') == 'ios') {
                var id = 'callAppIframe';
                var url = 'callapp:?:sendEvent:::' + name + ':::' + params;
                var iframe = document.getElementById(id);
                if (iframe !== null) {
                    iframe.parentNode.removeChild(iframe);
                }

                iframe = document.createElement('iframe');
                iframe.id = id;
                iframe.style.visibility = 'hidden';
                iframe.style.display = 'none';
                iframe.src = url;

                document.body.appendChild(iframe);
            } else if (getCookie('app') == 'android' && typeof window.eventJs != "undefined" && typeof window.eventJs.sendEvent != "undefined" ) {
                window.eventJs.sendEvent(name, params);
            }
        }

        
