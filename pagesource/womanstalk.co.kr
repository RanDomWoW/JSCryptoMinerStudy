        <script>
        
        new Swiper('.bnrListBullet', {
            pagination : '.swiper-pagination',
            paginationType: 'bullets',
            slidesPerView: 1,
            loop:true ,
            onInit:function(swiper) {
                if ( swiper.slides.length > 0) {
                    console.log(swiper.slides.length) ;
                    swiper.params.autoplay = 3000;
                    swiper.params.autoplayDisableOnInteraction = false ;
                    swiper.params.loop = true ;
                    swiper.startAutoplay();
                }
            }
        });

        
        var slideTimer ;
        var timeDealSlide = new Swiper('.timeDealBox .swiper-container', {
            spaceBetween: 30,
            slidesPerView: 'auto',
            centeredSlides:'true',
            pagination : '.swiper-pagination',
            paginationType: 'bullets',
            onImagesReady:function(){
                $('.timeDealBox .swiper-container .prodBox').css('visibility','visible');
            } ,
            onTransitionStart:function(swiper) {
                var myDate = new Date();
                var todayString = myDate.getFullYear() + "-" + (myDate.getMonth() + 1 < 10 ? '0' + (myDate.getMonth() + 1) : myDate.getMonth() + 1  ) + "-" +
                    (myDate.getDate() < 10 ? '0' + myDate.getDate() : myDate.getDate() );
                var timeString = (myDate.getHours() < 10 ? '0' + myDate.getHours() : myDate.getHours() )
                    + ':' + (myDate.getMinutes() < 10 ? '0' + myDate.getMinutes() : myDate.getMinutes() ) ;
                var divideTime = $(swiper.slides[swiper.activeIndex]).attr('data-timedeal-time').split(':') ;

                clearInterval(slideTimer);
                if ($(swiper.slides[swiper.activeIndex]).attr('data-timedeal-date') <= todayString && $(swiper.slides[swiper.activeIndex]).attr('data-timedeal-time') <= timeString ) {
                    $('div.timeDealList .titBox h3').html('오늘의 타임딜');

                    var now = Math.floor(new Date().getTime() / 1000) ;
                    var dealDate = $(swiper.slides[swiper.activeIndex]).attr('data-timedeal-endtime') - now;

                    slideTimer = setInterval(function() {
                        if ( dealDate > 0) {
                            var hour = Math.floor(dealDate / 3600),
                                min = Math.floor(dealDate % 3600 / 60),
                                sec = dealDate % 60;
                            hour = hour < 10 ? '0' + hour : hour ;
                            min = min < 10 ? '0' + min : min ;
                            sec = sec < 10 ? '0' + sec : sec ;
                            $('div.timeDealList .titBox p').html('<span>' + hour + '</span>시 <span class="count">' + min + '</span>분 <span class="count">' + sec + '</span>초 남음');
                            dealDate--;
                        } else {
                            clearInterval(slideTimer);
                        }
                    }, 1000);

                } else if( $(swiper.slides[swiper.activeIndex]).attr('data-timedeal-date') == todayString && $(swiper.slides[swiper.activeIndex]).attr('data-timedeal-time') > timeString) {
                    $('div.timeDealList .titBox h3').html('다음 타임딜');
                    $('div.timeDealList .titBox p').html('<span class="count">' + divideTime[0] + '</span>시 <span class="count">' + divideTime[1] + '</span>분 오픈');
                } else if ($(swiper.slides[swiper.activeIndex]).attr('data-timedeal-date') > todayString) {
                    $('div.timeDealList .titBox h3').html('내일의 타임딜');
                    $('div.timeDealList .titBox p').html('<span class="count">' + divideTime[0] + '</span>시 <span class="count">' + divideTime[1] + '</span>분 오픈');
                }
                $('div.timeDealList .titBox p span').each(function () {
                    $(this).prop('Counter',0).animate({
                        Counter: $(this).text()
                    }, {
                        duration: 600,
                        easing: 'swing',
                        step: function (now) {
                            var time = Math.ceil(now) ;
                            $(this).text( time < 10 ? '0' + time : time );
                        }
                    });
                });

            }
        });

        
        $('div.timeDealBox .btnDealView').on('click', function(){
            $(this).toggleClass('on').html( $(this).hasClass('on') ? $(this).html().replace('보기','닫기') : $(this).html().replace('닫기','보기')).siblings().slideToggle();
        });

        
        new Swiper('.hotDealList .swiper-container', {
            spaceBetween: 0,
            slidesPerView: 'auto',
            loop:true
        });

        
        new Swiper('.spProdWrap .swiper-container', {
            spaceBetween: 7,
            slidesPerView: 'auto',
        });

        
        var navih = $('.navi').height()
        $(document).ready(function() {
            $(window).scroll(function(){
                var scrollPosition = $(window).scrollTop() + navih ;
                var prodListLength = $('.mainProdList').length - 1;

                for ( var i = prodListLength ; i >= 0 ; i -- )
                {
                    if ( scrollPosition >= parseInt($('.mainProdList:eq(' + i + ')').position().top) )
                    {
                        var sticky = $('.mainProdList:eq(' + i + ') .tBox').clone();
                        if ( $('.stickyBar').length < 1 ) {
                            return $('.mainProdList:eq(' + i + ')').append(sticky.addClass('stickyBar'))
                        } else {
                            return $('.stickyBar').html( $('.mainProdList:eq(' + i + ') .tBox').html() )
                        }
