        function infinityScolls(container) {
            this.container = container ;
            this.template = this.container.data('item-template') ;
            this.target = this.container.data('item-target') ;
            this.uri = this.container.data('uri') ;
            this.expend = this.container.data('expend') ;
            this.pageLimit = this.container.data('item-limit') || 10 ;
            this.page = this.container.data('item-page') || 1 ;
            this.loading = this.container.data('item-loading') ;
            this.perItem = this.container.data('item-peritem') ;
            this.callback = this.container.data('item-callback') ;


            this.getStartPoint = function() {
                // 현재 스크롤 위치에서 가져올 동작 , 현재 위치에 존재하는
                return $(window).scrollTop() + screen.height > this.container.position().top + this.container.height() - 1000 ;
            } ;

            this.getData = function() {
                return 'uri=' + this.uri + '&expend=' + this.expend + '&page=' + this.page + '&pageLimit=' + this.pageLimit + '&template=' + this.template;
            } ;

            this.addElements = function() {
                if ( this.container == undefined || this.template == undefined || this.uri == undefined || this.target == undefined || ! this.getStartPoint() || this.loading )
                    return ;
                var $this = this ;
                $this.container.data('item-loading',true) ;
                if ( this.loading )
                    return ;
                $.ajax({
                    url: '/ajax/infinityItems' ,
                    method:'get',
                    data:this.getData() ,
                    dataType:'json',
                    success:function(result) {
                        if ( result.html != '' ) { // 붙일 엘레멘트가 있을시
                            var $html = $(result.html) ;
                            $html.find('img').each(function(){
                                if ( ! $(this).hasClass('lazyload')) {
                                    $(this).attr({'data-src':$(this).attr('src'),'src':''}).addClass('lazyload');
                                }
                            }) ;

                            if ( $this.perItem != undefined && $html.find($this.perItem).length > 0) {
                                $.each($html.find($this.perItem),function(){
                                    $this.container.find($this.target).append(this) ;
                                }) ;
                            } else {
                                $this.container.find($this.target).append($html.html());
                            }
                            if ( $this.callback != undefined ) {
                                window[$this.callback].apply(null,$this.container) ;
                            }
                        }
                        if ( result.code == '999' ) {
                            $this.container.data('item-loading',true) ;
                            return clearInterval(infinitiScroll) ;
                        }
                        $this.container.data('item-page', parseInt($this.page + 1)) ;
                        $this.container.data('item-loading',false) ;
                        return ;
                    } ,
                    error:function() {
                        $this.container.data('item-loading',true) ;
                        return clearInterval(infinitiScroll) ;
                    }
                }) ;
            } ;
            this.addElements();
        }

