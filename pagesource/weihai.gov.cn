            <style type="text/css">
/* qqshop focuse */
#focuse {width:293px; height:240px; overflow:hidden; position:relative;margin:0 auto;margin-left:10px;}
#focuse ul {height:200px; position:absolute;}
#focuse ul li {float:left; width:293px; height:240px; overflow:hidden; position:relative; background:#000;}
#focuse ul li div.photo_bt {position:absolute; bottom:2px; left:10px; font-size:14px; color:#ffffff; z-index:10; line-height:36px;}
#focuse .btnBg {position:absolute; width:293px; height:36px; left:0; bottom:0; background:#000;}
#focuse .btn {position:absolute; width:293px; height:17px; padding:5px 10px; right:0; bottom:0; text-align:right;}
#focuse .btn span {display:inline-block; _display:inline; _zoom:1; width:10px; height:10px;border-radius:10px; _font-size:0; margin-left:5px; cursor:pointer; background:#fff;}
#focuse .btn span.on {background:#fff;}
#focuse .preNext {width:45px; height:100px; position:absolute; top:50px; background:url(/picture/0/1508291327558808965.png) no-repeat 0 0; cursor:pointer;}
#focuse .pre {left:0;}
#focuse .next {right:0; background-position:right top;}
</style>
<script type="text/javascript">
$(function() {
	var sWidth = $("#focuse").width(); //获取焦点图的宽度（显示面积）
	var len = $("#focuse ul li").length; //获取焦点图个数
	var index = 0;
	var picTimer;
	
	//以下代码添加数字按钮和按钮后的半透明条，还有上一页、下一页两个按钮
	var btn = "<div class='btnBg'></div><div class='btn'>";
	for(var i=0; i < len; i++) {
		btn += "<span></span>";
	}
	btn += "</div><div class='preNext pre'></div><div class='preNext next'></div>";
	$("#focuse").append(btn);
	$("#focuse .btnBg").css("opacity",0.5);

	//为小按钮添加鼠标滑入事件，以显示相应的内容
	$("#focuse .btn span").css("opacity",0.4).mouseenter(function() {
		index = $("#focuse .btn span").index(this);
		showPics(index);
	}).eq(0).trigger("mouseenter");

	//上一页、下一页按钮透明度处理
	$("#focuse .preNext").css("opacity",0.2).hover(function() {
		$(this).stop(true,false).animate({"opacity":"0.5"},300);
	},function() {
		$(this).stop(true,false).animate({"opacity":"0.2"},300);
	});

	//上一页按钮
	$("#focuse .pre").click(function() {
		index -= 1;
		if(index == -1) {index = len - 1;}
		showPics(index);
	});

	//下一页按钮
	$("#focuse .next").click(function() {
		index += 1;
		if(index == len) {index = 0;}
		showPics(index);
	});

	//本例为左右滚动，即所有li元素都是在同一排向左浮动，所以这里需要计算出外围ul元素的宽度
	$("#focuse ul").css("width",sWidth * (len));
	
	//鼠标滑上焦点图时停止自动播放，滑出时开始自动播放
	$("#focuse").hover(function() {
		clearInterval(picTimer);
	},function() {
		picTimer = setInterval(function() {
			showPics(index);
			index++;
			if(index == len) {index = 0;}
		},4000); //此4000代表自动播放的间隔，单位：毫秒
	}).trigger("mouseleave");
	
	//显示图片函数，根据接收的index值显示相应的内容
	function showPics(index) { //普通切换
		var nowLeft = -index*sWidth; //根据index值计算ul元素的left值
		$("#focuse ul").stop(true,false).animate({"left":nowLeft},300); //通过animate()调整ul元素滚动到计算出的position
		//$("#focuse .btn span").removeClass("on").eq(index).addClass("on"); //为当前的按钮切换到选中的效果
		$("#focuse .btn span").stop(true,false).animate({"opacity":"0.4"},300).eq(index).stop(true,false).animate({"opacity":"1"},300); //为当前的按钮切换到选中的效果
	}
});

</script>
