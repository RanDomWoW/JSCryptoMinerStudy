            function CalculSimplu(pt2018) {
                var _suplQS = ""; //&simulare-salariu-2018";
                var mpt2018 = pt2018 === undefined ? false : pt2018;
                if($("input:radio[name='ancalcul']:checked").val() == 2018) {
