            function addCommas(nStr)
            {
                nStr += '';
                x = nStr.split('.');
                x1 = x[0];
                x2 = x.length > 1 ? '.' + x[1] : '';
                var rgx = /(\d+)(\d{3})/;
                while (rgx.test(x1)) {
                    x1 = x1.replace(rgx, '$1' + ',' + '$2');
                }
                return x1 + x2;
            }

            function IsNumeric(sText)
            {
                var ValidChars = "0123456789.";
                var IsNumber=true;
                var Char;

                for (i = 0; i < sText.length && IsNumber == true; i++)
                {
                    Char = sText.charAt(i);
                    if (ValidChars.indexOf(Char) == -1)
                    {
                        IsNumber = false;
                    }
                }
                return IsNumber;
            }

            function CalculezInValuta() {
                var curs = $("#txtCurs").val().replace(",", ".");
                if (curs == '0' || curs == '' || !IsNumeric(curs)) {
                    return;
                }

                var bff = Math.round(parseFloat($("#SalariuBrut").text()) / curs * 100) / 100;
                $("#SalariuBrutVal").text(bff);
                bff = Math.round(parseFloat($("#BonuriDeMasa").text()) / curs * 100) / 100;
                $("#BonuriDeMasaVal").text(bff);
                bff = Math.round(parseFloat($("#BugeteAngajat").text()) / curs * 100) / 100;
                $("#BugeteAngajatVal").text(bff);
                bff = Math.round(parseFloat($("#SalariuNet").text()) / curs * 100) / 100;
                $("#SalariuNetVal").text(bff);
                bff = Math.round(parseFloat($("#SalariuComplet").text()) / curs * 100) / 100;
                $("#SalariuCompletVal").text(bff);
                bff = Math.round(parseFloat($("#BugeteAngajator").text()) / curs * 100) / 100;
                $("#BugeteAngajatorVal").text(bff);
                bff = Math.round(parseFloat($("#BugeteTotal").text()) / curs * 100) / 100;
                $("#BugeteTotalVal").text(bff);
            }
