function GetQueryString(urlSearch,name)
{
     var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
     var r = urlSearch.substr(1).match(reg);
     if(r!=null) {
      return  unescape(r[2]); 
     }
     return null;
}
//公共方法 用于根据父节点行政区号（xzqh）,窗口类型编码（code）,用于li拼接在ul下的ul id属性值查询的数据进行拼接。
function getPortalDetail(xzqh,code,id){
	 $.ajax({
     	url: "wsbs/portalAddressDetail.action?parentxzqh="+xzqh+"&typecode="+code, 
        type: "POST", 
        dataType: "json",
        success: function(data) { 
        //alert(data.porInfo);
	        var bumenck ="";
	        if(code==10){
	        	$("#cunju").html("");
	        }
            $.each(data.porInfo,function(i,result){
            	if(i==0&&code==5){
            		getPortalDetail(result.xzqh,"10","#zhenjie");
            	}
            	if(i==0&&code==10){
            		getPortalDetail(result.xzqh,"11","#cunju");
            	}
            	 bumenck+=" <li  id="+result.xzqh+"><a href="+result.url+">"+result.shoreName+"</a></li>";
            })
            $(id).html(bumenck);
       	}
 	 }); 
}

	  
