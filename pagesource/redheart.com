    function openMega(evt, megaName) {
        var i, x, tablinks;
        x = document.getElementsByClassName("mega");
        for (i = 0; i < x.length; i++) {
            x[i].style.display = "none";
        }
        tablinks = document.getElementsByClassName("tablink");
        for (i = 0; i <= x.length; i++) {
            tablinks[i].className = tablinks[i].className.replace(" ddselect", "");
        }
        document.getElementById(megaName).style.display = "block";
        evt.currentTarget.className += " ddselect";
    }

