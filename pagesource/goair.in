<script>
    function bookevent(from,to,date) {
        from = from.slice(1, -1);
        to = to.slice(1, -1);
        date = date.slice(1, -1);
        var o1, d1, dd1;
        var datearray = date.split("/");
        dd1 = datearray[2] + '-' + datearray[1] + '-' + datearray[0];
        //return newdate;
        $.ajax({
            url: "/Umbraco/Surface/flight/GetStations",
            success: function (json) {
                var data = json;
                var departureStation = $.grep(data[0].stations, function (n, i) {
                    return n.value === from;
                });
                var destinationStation = $.grep(data[0].stations, function (n, i) {
                    return n.value === to;
                });
                if (departureStation.length > 0) {
                    o1 = departureStation[0].code;
                }
                if (destinationStation.length > 0) {
                    d1 = destinationStation[0].code;
                }
                var url = 'https://book.goair.in/' + 'Flight/Select?s=True&o1=' + o1 + "&d1=" + d1 + "&ADT=1&dd1=" + dd1 + "&mon=true";
                document.location.href = url;
            }
        });        
    }
</script>
    


