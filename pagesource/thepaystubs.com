        $(window).on("load", function () {
            var reviews = $('.trust-pilot .wrapper-right .widget-reviews .widget-review');
            var wrapperRight = $('.trust-pilot .wrapper-right');
            var widgetReviewsWidth = wrapperRight.outerWidth();
            var moveToRemainingItems = false;

            var widthReview = null;
            var itemsPerSlide = 0;
            if (widgetReviewsWidth > 0 && widgetReviewsWidth <= 530) {
                itemsPerSlide = 1;
            } else if (widgetReviewsWidth > 530 && widgetReviewsWidth <= 729) {
                itemsPerSlide = 2;
            } else if (widgetReviewsWidth > 729 && widgetReviewsWidth <= 974) {
                itemsPerSlide = 3;
            } else if (widgetReviewsWidth > 974) {
                itemsPerSlide = 4;
            }

            widthReview = widgetReviewsWidth / itemsPerSlide - 20;

            reviews.each(function (index) {
                this.style.left = parseInt(index * widthReview) + 'px';
            });

            wrapperRight.addClass('visible');

            var reviewsThereIsSpaceFor = widgetReviewsWidth / widthReview;
            var numberReviewsShown = reviewsThereIsSpaceFor > 1 ? Math.floor(reviewsThereIsSpaceFor) : 1;
            var numberReviewsSlides = Math.ceil(reviews.length / numberReviewsShown);
            var numberSlides = widthReview * numberReviewsShown;
            var currentSlide = 1;

            var remainingItems = reviews.length % itemsPerSlide;

            $('#review-arrow-right').click(function () {
                if (currentSlide < numberReviewsSlides) {
                    moveContent('next');
                }
            });

            $('#review-arrow-left').click(function () {
                if (currentSlide > 1) {
                    moveContent('prev');
                }
            });

            var updateArrows = function () {
                if (currentSlide >= numberReviewsSlides) {
                    $('#review-arrow-right').addClass('display-none')
                } else {
                    $('#review-arrow-right').removeClass('display-none')
                }

                if (currentSlide <= 1) {
                    $('#review-arrow-left').addClass('display-none')
                } else {
                    $('#review-arrow-left').removeClass('display-none')
                }
            };
            updateArrows();

            var moveContent = function (direction) {
                var movementNumber = numberSlides;
                if (direction === 'next') {
                    if (currentSlide === numberReviewsSlides && !moveToRemainingItems) {
                        movementNumber = remainingItems * widthReview;
                        moveToRemainingItems = true;
                    }
                    reviews.each(function () {
                        this.style.left = parseInt(parseInt(this.style.left) - movementNumber) + 'px';
                    });
                    currentSlide++;
                } else {
                    if (currentSlide === 2 && moveToRemainingItems === true) {
                        movementNumber = remainingItems * widthReview;
                        moveToRemainingItems = false;
                    }
                    reviews.each(function () {
                        this.style.left = parseInt(parseInt(this.style.left) + movementNumber) + 'px';
                    });
                    currentSlide--;
                }

                updateArrows();
            };
        });

        var targetOffset = $(".trust-pilot").offset().top - 1000;
        if (typeof CountUp !== 'undefined') {
            var numAnim = new CountUp("js-count-orders", 48876, 73314, 0, 3);
            var $w = $(window).scroll(function () {
