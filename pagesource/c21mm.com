<script type="text/javascript">
    function OpenPropertySummary(url) {
        $(".FullDetailsBtn").attr("href", url);
        $("#propSummaryModalBody").html("");
        $aa.showLoadingSpinner_Div(true, "#propSummaryModalBody");
        $("#propSummaryModalBody").load(url, function (responseTxt, statusTxt, xhr) {
            $aa.showLoadingSpinner_Div(false, "#propSummaryModalBody");
            if (statusTxt == "success")
            {
                var noOfVisits = getCookie("PDViews");
                if (noOfVisits != null) {
                    setCookie("PDViews", noOfVisits - 1);
                }
            }
        });
        if (typeof ga !== 'undefined') ga('aa_tracker.send', 'pageview', url);
        $('#propSummaryModal').modal('show'); //trick: the CSS style for body.modal-open above will prevent background from scrolling to top
    }
</script>



