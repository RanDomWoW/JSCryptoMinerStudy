			var i;
			var moment = document.getElementsByClassName("mymoment");
			if (momentIndex > moment.length){momentIndex = 1}    
			if (momentIndex < 1){momentIndex = moment.length}
			for (i = 0; i < moment.length; i++)
			{
				moment[i].className = moment[i].className.replace(" current", "");	  
			}
			moment[momentIndex-1].className += " current";
			momentIndex++;
			mTimer=setTimeout(showMoments,3200);
  
 
		}
