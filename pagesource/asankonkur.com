					$(".advicer").each(function(){
						var myVal=$(this).val();
						myVal=myVal.replace(/<script[^>]*>([\S\s]*?)<\/script>/gmi, '');
						myVal=myVal.replace(/(<([^>]+)>)/ig,"");
						$(this).val(myVal);
						if(myVal.length<2)
						{
							err++;
						}
					});
					if(err==0)
					{
						$.ajax({
								url:myUrl,
								type:"post",
								data: new FormData(this),
								dataType:"json",
								contentType:false,
								processData:false,
