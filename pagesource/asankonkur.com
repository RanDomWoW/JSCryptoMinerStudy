							}
							$("#basketcontent").css("display","block");
							$("#mycaret .w3-caret").text(result.cQty);
						},
						error:function()
						{
							alert("خطا در برقراری ارتباط با سرور !")
						}
					});
				});
				$(document).on("click",".ctrashcart",function(e)
				{
					e.preventDefault();
					var prId=$(this).attr("data");
					
					
					var addToBasket="action=trash&prid="+prId;
					
					$.ajax({
						url:'http://asankonkur.com/includes/trashcart.php',
						type:"post",
						data:addToBasket,
						dataType:"json",
						cache:false,
						success:function(result)
						{
						

							$("#basketcontent table tbody").html(result.basket);
							if($("#htop").hasClass("w3-hide"))
							{
								$("#basketcontent").css("top","80px");
							}
							else
							{
								$("#basketcontent").css("top","140px");
							}
							$("#basketcontent").css("display","block");
							$("#mycaret .w3-caret").text(result.cQty);
						},
						error:function()
						{
							alert("خطا در ارتباط با سرور !")
						}
					});
					
				});
			});
		</script>
		
	</body>
