<script type="text/javascript">

function hideButton(){
	$('#btnFitVehicle').addClass("hide");
}

function updateSearchBtnLabel(btn, label){
	$(btn).text(label);
}

function findByNameAndDim(){
    if (popgom.localeData.searchParams.checkDimensionData($('#searchByNameAndDim'))){
    	
    	var values = popgom.localeData.searchParams.getDimensionSearchProperties($('#searchByNameAndDim'), false);
    	var params = $.extend({}, values);
    	params.profileIdt = $('#searchByNameAndDim').find('[name="profileIdt"]').val();
    	params.moreSearchMode = "tire-size";
    	params.zipCode = $('#searchByNameAndDim').find('[name="zipCode"]').val();
    	
        $.getJSON("/jsonFindByProfile", params, function(jsonData) {
        	
            var articles = jsonData.articles;
            displayArticles(articles, jsonData);
            
            
            var displayDisclaimer = jsonData.displayDisclaimer;
            
            if(displayDisclaimer == 'true'){
            $.colorbox({
				href: "/templates/store/bfgoodrich/parts/bfgoodrich-disclaimer-modal.jsp",
				maxWidth: 900,
				transition: 'none',
				opacity: 0.5,
				fixed: true,
				close:'&times;',
				overlayClose: false,
				onComplete : function() {	   
				    $(this).colorbox.resize();
				}
			});
            }
        });
       
    	
    }
}

function findByNameAndVehicle(){
    if (popgom.localeData.searchParams.checkVehicleData($('#searchByNameAndVehicle'))){
        
        var values = popgom.localeData.searchParams.getVehicleSearchProperties($('#searchByNameAndVehicle'), false);
        var params = $.extend({}, values);
        params.profileIdt = $('#searchByNameAndVehicle').find('[name="profileIdt"]').val();
        params.moreSearchMode = "vehicle";
        params.zipCode = $('#searchByNameAndVehicle').find('[name="zipCode"]').val();
        
        $.getJSON("/jsonFindByProfile", params, function(jsonData) {
            
            var articles = jsonData.articles;
            displayArticles(articles, jsonData)

        });
        
    }
}

function displayArticles(articles, jsonData){
	
	$("#searchByNameResultList").empty();
	$("body, html").scrollTop ($('#resultListCorps').offset().top);
	
	var nbAvailableArticles = 0;
	
    if (articles.length > 0){
        $.each(articles, function() {
        	if (this.price != '0'){
            var resultLine ="";
            resultLine = "<div class='product-result'>";
            resultLine += "<div class='product-result-size-qty'>"
            resultLine += "<span class='product-size'>"+this.dimensions+"</span>";
            resultLine += "<div class='product-qty'>";
            resultLine += "<div class='product-qty-select'>";
            resultLine += "<button role='button' class='product-qty-button' onClick='decreaseQty("+this.articleId+")'>-</button>";
            resultLine += "<div class='product-qty-nb' id='product-qty-"+this.articleId+"'>" + (jsonData.isDifferentSizes ? 2 : 4) + "</div>";
            resultLine += "<button role='button' class='product-qty-button' onClick='increaseQty("+this.articleId+")'>+</button>";
            resultLine += "</div></div></div>";
            resultLine += "<div class='product-result-price-cta'>";
            resultLine += "<span class='product-price'>";
            var price = this.price.split('.');
            resultLine += "<sup>$</sup>"+price[0]+".<sup>"+price[1]+"</sup>";
            resultLine += "</span>";
            resultLine += "<a href='#_' onclick=\"tyredating.basket.addToBasket('"+this.supplierreferenceId+"', $('#product-qty-"+this.articleId+"').text()); return false;\" class='btn btn-primary'><i class='glyphicon glyphicon-shopping-cart'></i>&nbsp;Add to cart</a>";
            resultLine += "</div></div>";

            $("#searchByNameResultList").append(resultLine);
            nbAvailableArticles++;
            }
        });
        }
    
        if(nbAvailableArticles == 0){
            var noResultLine = "<div class='product-result'>";
            noResultLine += "<p class='product-result-ko'>No product available for your vehicle</p>";
            noResultLine += "</div>";
            $("#searchByNameResultList").append(noResultLine);
        }
}

function increaseQty(articleId){
    var qty = +$("#product-qty-"+articleId).text();
    if(qty < ''){
    	$("#product-qty-"+articleId).text(qty + 1)
    }
}

function decreaseQty(articleId){
    var qty = +$("#product-qty-"+articleId).text();
    if (qty>1)
    {
    	$("#product-qty-"+articleId).text(qty - 1)
    }
}

</script>









    
    
        
        
    

	
