	});
});
function msgmove(){ 
	var isIE=!!window.ActiveXObject;
	var isIE6=isIE&&!window.XMLHttpRequest;
	var ul = $(".roll");
	if($(ul).attr("name") != "hovered"){
		var height = 21;
		if(isIE6){
			$(ul).children(":first").css({"height":0});
		}else{ 
			$(ul).children(":first").animate({"opacity":"0","height":"0"},600,function(){
			
			$(ul).children(":last").after($(ul).children(":first"));
			$(ul).children(":last").css({"height":height});
			$(ul).children(":last").css({"opacity":"2"});
			});

			
		}
	}
	setTimeout("msgmove()",5000);  //设置5秒滚动一次
}
