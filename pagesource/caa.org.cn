<script>
	function tomore(id){
		window.location.href='index.php?me_id='+id;
	}
	var speed=30;
        var demo = document.getElementById("demo");
        var flag = 0;
	demo.scrollLeft = 0;
	function Marquee()
	{
            flag++;
            demo.scrollLeft++;
            if(flag - demo.scrollLeft > 10)
            {
                flag = 0;
                demo.scrollLeft = 0;
            }
	}
	var MyMar = setInterval(Marquee, speed);
	demo.onmouseover = function()
	{
		clearInterval(MyMar);
	}
	demo.onmouseout = function() 
	{
		MyMar=setInterval(Marquee,speed);
	}
        
        function slideLink(part_info)
        {
            var allNodes = $("[part]");
            var thisNode = $("[part='" + part_info + "']");
            allNodes.each(function()
            {
                $(this).attr("class", "block_title_name_02");
                var hide_class = $(this).attr("id");
                var linkNodes_hide = $("." + hide_class);
                linkNodes_hide.hide();
            });
            thisNode.attr("class", "block_title_name_02_on");
            var show_class = thisNode.attr("id");
            var linkNode_show = $("." + show_class);
            linkNode_show.show();
        }
