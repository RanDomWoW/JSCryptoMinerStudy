<script type="text/javascript">
$(function() {
var slick = $('.autoplay').slick({
	slidesToShow: 3,
	slidesToScroll: 1,
	autoplay: true,
	autoplaySpeed: 5000,
	appendArrows: $('#arrows'),
prevArrow: '<a class="slick-prev" href="#"><img src="img/slide_prev.png" alt="前のバナーを表示"></a>',
nextArrow: '<a class="slick-next" href="#"><img src="img/slide_next.png" alt="次のバナーを表示"></a>',
});

// 再生停止ボタンを準備（HTMLに直接追加でもいいです）
var stopBtn = $('<button id="tglBtn"><span id="startBtn">再生</span><span id="stopBtn">停止</span></button>');
 
// ページ送りの前に追加（あってもなくてもいいです）
stopBtn.prependTo('.autoplay .slick-dots');
 
// デフォルトは再生ボタンを非表示に
$('#startBtn').hide();
// 切り替えフラグを準備
var flg = "play";
 
// ボタンをクリックした時の処理
$('#tglBtn').click(function(){
  // toggleでフラグを切り替え
  $('#startBtn').toggle();
  // フラグの条件でボタンの切り替えと再生・停止の処理
  if(flg == "play") {
    $('#startBtn').show();
    $('#stopBtn').hide();
    flg = "stop";
    $('.autoplay').slick('slickPause');
  } else {
    $('#startBtn').hide();
    $('#stopBtn').show();
    flg = "play";
    $('.autoplay').slick('slickPlay');
  }
});

});
</script>

