<script>
$('.productItemThumbnails li')
    .mouseover(function(){
		images = $(this).parent().parent().parent().find('.productItemImg img, .productItemImgCl img');

        images[$(this).index() + 1].style.display = 'inline-block';
		images[$(this).index() + 1].src = images[$(this).index() + 1].getAttribute('img-src');
        images[0].style.display = 'none';
    })
    .mouseout(function(){
        images = $(this).parent().parent().parent().find('.productItemImg img, .productItemImgCl img');
        images[$(this).index() + 1].style.display = 'none';
        images[0].style.display = 'inline-block';
     });

$(function() {
	
    $('.jcarousel').each(function(index, obj){
        $(obj).jcarousel();
        var prev = $(obj).parent().find('.jcarouselPrev div');
        var next = $(obj).parent().find('.jcarouselNext');

        prev.click(function(){
            $(obj).jcarousel('scroll', '-=1');
            next.css('display', 'block');
            if (!$(obj).jcarousel('hasPrev')) {
                prev.css('display', 'none');
            }
        });
        next.click(function(){
            $(obj).jcarousel('scroll', '+=1');
            prev.css('display', 'block');
            if (!$(obj).jcarousel('hasNext')) {
                next.css('display', 'none');
            }
        });

        if ($(obj).find('li').length > 4) {
            next.css('display', 'block');
        }
    });
});

var log = 0;

