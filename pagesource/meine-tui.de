<script type="text/javascript">
	equalDivHeight('equalheight .container-frame');
	
	$(function() {
		$("#search-term").autocomplete({
			source : "/widgets/catalog-object-search/do-input-completion/ajax-request/true/json-request/true",
			minLength : 3,
			maxShowItems: 10,
			select : function(event, ui) {
				$('#search-term').val(ui.item.label);
				$('#object-code').val(ui.item.value);
				$("#catalog-submit").click();
			}
		});
	});
	//empty input onfocus
	$("#search-term").each(function() {
		var wert = $(this).val();
		$(this).focusin(function() {
			if ($(this).val() == wert) {
				$(this).val("");
			}
		}).focusout(function() {
			if ($.trim($(this).val()) == "") {
				$(this).val(wert);
			}
		});
	});
</script>
			    
    	

