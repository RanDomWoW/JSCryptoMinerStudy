<script type="text/javascript">
	/*
	 * Adds messages to the page-messages div.
	 *
	 * expects an array structure with keys 'error', 'warning', 'info', 'debug'
	 * and an associative subarray for each key with msg keys and msg texts.
	 */
	function showPageMessages(msgs) 
	{
		var s = "";
        		s = createPageMessageDivsForKey('error', msgs);
                		s += createPageMessageDivsForKey('warning', msgs);
                		s += createPageMessageDivsForKey('info', msgs);
                
		var targetDiv = $("#page-messages");

		targetDiv.html(s);
	}
	
	function createPageMessageDivsForKey(key, msgs) 
	{
		var s = "";
		if (key in msgs) {
			var msgs = $.map(msgs[key], function (value, index) { return value; });
			if (msgs.length > 0) {
				s += "<div class=\"alert-" + key + "\"><div class=\"wrapper p-1\">";

				$.each(msgs, function(index, msg) {
					msg = msg.substr(0, 1) == '!' ? msg.substring(1, msg.length) : msg;
					s += "<h2 class=\"text-xs-center\"><strong>" + msg + "</strong></h2>";
					gase('page-messages', 'show-' + key, msg);
				});

				s += "</div></div>";
			}
		}
		
		return s;
	}

	/*
	 * Shows a single popup message.
	 */
	function showPageMessagePopup(msgType, title, msg) 
	{
		$('#message-dialog-title').html(title);
		$('#message-dialog-body').html("<p>" + msg + "</p>");
		$('#message-dialog').modal('show');
	} 

	/*
	 * Shows an array of messages in a popup box.
	 */
	function showPageMessagesPopup(msgType, title, msgs) 
	{
		var html = "";
		$.each(msgs, function(index, msg) {
			msg = msg.substr(0, 1) == '!' ? msg.substring(1, msg.length) : msg;
			html += msg + "<br/>";
			gase('page-messages-popup', 'show-' + msgType, msg);
		});

		showPageMessagePopup(msgType, title, html);
	} 
	
		
    
    </script>	
