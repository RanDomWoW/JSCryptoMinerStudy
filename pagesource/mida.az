<script type="text/javascript" src="/assets/css/fancybox/jquery.fancybox-1.3.4.pack.js"></script>

<script type="text/javascript">
  
$(document).ready(function(){
  $('#mailing input').keypress(function (e) {
    if (e.which == 13) {
        var email = $('#email').val();
        $.ajax({
          url: "http://mida.az/subsciber.php", 
          type: "post", 
          dataType: "json", 
          data: { 
            "email": email
          },
          success: function(data){

            switch(data.sub_err) {
              case 'nomail':
                  $('#inline[href="#nomail"]').click();
                  break;
              case 'mail':
                    $('#inline[href="#nomail"]').click();
                  break;
              case 'subscibed':
                    $('#inline[href="#subscibed"]').click();
                  break;
              case 'subscibedyet':
                    $('#inline[href="#subscibedyet"]').click();
                  break;
            }
          }
        });
      return false; 
    }
  });
});

</script>

<script type="text/javascript">
    $(document).ready(function() {
    $("a#inline").fancybox({
      'hideOnContentClick': true
    });
  });
</script>

</body>
</html>