        $('.dropdown-submenu a.test').on("click", function(e){
            $(this).next('ul').toggle();
            e.stopPropagation();
            e.preventDefault();
        });
    });


    function checkRegexp( o, regexp ) {
        if ( !( regexp.test( o.val() ) ) ) {
            return false;
        } else {
            return true;
        }
    }

    function checkLength( o, max ) {
        if ( o.val().length > max  ) {
            //o.addClass( "ui-state-error" );
            return false;
        } else {
            return true;
        }
    }

    function check_form_force(form_id)
    {

        var  is_error = false;
        $('#'+form_id).find('[force="true"]').each(function()
        {
            if($(this).val()==null || $(this).val()=='')
            {
                alert('وارد کردن فیلدهای ستاره دار، الزامی است');

                $(this).focus();
                is_error = true;
                //alert('test='+is_error);
                return false;

            }

        });



        var regexp=
            /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;

        if(form_id== 'form1' && is_error == false && $('#email').val().length>0 && !checkRegexp($('#email'),regexp))
        {
            alert(' ایمیل را صحیح وارد نمایید.');
            is_error = true;
            return false;
        }

        if(form_id== 'Consultation' && is_error == false && $('#inputemail').val().length>0 && !checkRegexp($('#inputemail'),regexp))
        {
            alert(' ایمیل را صحیح وارد نمایید.');
            is_error = true;
            return false;
        }


        if(form_id== 'form1' && is_error == false && $('#mobile').val().length!=11  && $('#mobile').val().length>0 )
        {
            alert(' موبایل را صحیح وارد نمایید.');
            is_error = true;
            return false;
        }

        if(form_id== 'Consultation' && is_error == false && $('#inputmobile').val().length!=11  && $('#inputmobile').val().length>0)
        {
            alert(' موبایل را صحیح وارد نمایید.');
            is_error = true;
            return false;
        }

        if(form_id== 'reserve' && is_error == false && $('#mobile1').val().length!=11  && $('#mobile1').val().length>0)
        {
            alert(' موبایل را صحیح وارد نمایید.');
            is_error = true;
            return false;
        }
        if(form_id== 'reserve' && is_error == false && $('#melli').val().length!=10  && $('#melli').val().length>0)
        {
            alert(' کد ملی را صحیح وارد نمایید.');
            is_error = true;
            return false;
        }


        if(is_error)
        {
            return false;
        }
        else
        {
            return true;
        }



        //return true;


    }

</script>
<script type="text/javascript">
    //<![CDATA[
    $(document).ready(function(){
        // hide #back-top first
        $("#back-top").hide();
        // fade in #back-top
        $(function () {
            $(window).scroll(function () {
                if ($(this).scrollTop() > 600) {
                    $('#back-top').fadeIn();
                } else {
                    $('#back-top').fadeOut();
                }
            });
            // scroll body to 0px on click
            $('#back-top a').click(function () {
                $('body,html').animate({
                    scrollTop: 0
                }, 800);
                return false;
            });
        });

    });
    //]]>
</script>
