        <script>
            var  interval_Rotator = undefined;
            var stop_rotate = false;

            function theRotator() {
                if(typeof interval_Rotator != 'undefined')
                    { clearInterval(interval_Rotator); }  //
                if (!stop_rotate) {
                    interval_Rotator = setInterval('rotate()',5000);  //Это задержка при автоперелистывании
                }
            };

            function stop_Rotator() {
                clearInterval(interval_Rotator);
                stop_rotate = true;
            }

            function pause_Rotator() {
                clearInterval(interval_Rotator);
            }

            function current_news_num() {
                // номер текущей новости
                return $('.news_dot_selected').attr("id").replace("news_", "");	//текущая новость
            };

            function current_news() {
                //текущая новость
                return $("#news_paper_"+current_news_num());
            };

            function next_news_num2() {
                //номер следующей новости. Если последняя перехоinterval_Rotatorдим на первую!
                var next = $('.news_dot_selected').next();
                if ((next.attr("id") == undefined)) {
                    next = $('.news_dot').first();
                    if ((next.attr("id") == undefined))
                        return false;
                }
                    return next.attr("id").replace("news_", "");
            };

            function next_news() {
                if (next_news_num = next_news_num2())
                    return $("#news_paper_"+next_news_num2());
                else
                    return false;
            };


            function rotate() {
                var next = next_news();
                if (next) {
                    next_news_num = next_news_num2();
                    current_news().fadeOut(1000);
                    $('.news_dot_selected').addClass('news_dot').removeClass('news_dot_selected');
                    $('#news_'+next_news_num).addClass('news_dot_selected').removeClass('news_dot');
                    next.fadeIn(1000);

                }
            };


            $(document).ready(

                function(){

                    theRotator();  //включить перелистывание

                    $("#news_block").hover(  //Остановить перелистывание при наведении мышы
                        function(){
                            pause_Rotator();
                        },
                        function(){
                            theRotator();
                        }
                    );

                    $('.news_dot').click(function(){
                        //Нажата точка
                        if (!$(this).hasClass("news_dot_selected")) {
                            current_news().fadeOut(1000);	//текущая новость
                            $('.news_dot_selected').addClass('news_dot').removeClass('news_dot_selected');
                            $(this).addClass('news_dot_selected').removeClass('news_dot');
                            var num = this.id.replace("news_", "");		//новая новость
                            $("#news_paper_"+num).fadeIn(1000);

                        }
                        stop_Rotator();
                    });

                    click_block = false;
                    $('#news_close').click(function(){
                      if (!click_block) {
                        click_block = true;
                        click_timeout = setTimeout(function(){clearTimeout(click_timeout);click_block=false}, 500);

                        //Закрыть новость
                        var next = next_news();
                        if (next) {
                            next_news_num = next_news_num2();
                            current_news().fadeOut(1000);
                            $('.news_dot_selected').detach();
                            $('#news_'+next_news_num).addClass('news_dot_selected').removeClass('news_dot');
                            next.fadeIn(1000);
                        } else {
                            //новостей не осталось. Закрыть новостное окно
                            $('#news_block').detach();
                        }
                        stop_Rotator();
                      }
                    });
                }
            );
        </script>
