<script type="text/javascript">

	$(document).on('click','.add-to-cart', function(e){
		
		var form = $(this).closest('form'),
		id = form.find('input[name="pid"]'),
		qty = form.find('input[name="Quantity"]');
		var qtyVal = qty.val();
		if(!qtyVal) {
			qtyVal = "1";
		}
		var pushdata = {
			'items' : [
			           {'data' : id.val(),
						'eventqty' : qtyVal
						}
			           ]
		};
		_sf_track_data.push(['_setSFLanguage','en']);
		_sf_track_data.push(['_trackSFCartAdd',pushdata]);
	});
	
</script>
	


<script type="text/javascript">

	
	$(document).on('click', '.add-all-to-cart', function(e){
		
		var pushdata = {};
		pushdata.items = [];
		
		$.each($('body').find('button[class$=add-to-cart]'), function(index, buttonObj) {
			  
			var addToCartForm = $(buttonObj).closest('form');
			var formElements = addToCartForm[0].elements;
			var pid = formElements["pid"];
			var qty = formElements["Quantity"];
			var pidVal = pid.value;
			var qtyVal = qty.value;
			if(!qtyVal) {
				qtyVal = "1";
			}

			pushdata.items.push({'data' : pidVal, 'eventqty' : qtyVal});
		});
		_sf_track_data.push(['_setSFLanguage','en']);
		_sf_track_data.push(['_trackSFCartAdd',pushdata]);
	});

</script>	
	
