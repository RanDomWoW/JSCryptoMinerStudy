<script type='text/javascript'>
    function gup(name){
        name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
        var regexS = "[\\?&]"+name+"=([^&#]*)";
        var regex = new RegExp( regexS );
        var results = regex.exec( window.location.href );
        if(results == null){
            return null;
        }else{
            return results[1];
        }
    }

