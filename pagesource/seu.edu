var audioOn= false;
var mobile = false;
function toggleAudio() {
	if(audioOn) {
		$('#feature-audio')[0].pause();
		$('#volumetoggle').addClass('icon-volume-mute2');
		$('#volumetoggle').removeClass('icon-volume-medium');
		audioOn = false;
	} else {
		$('#feature-audio')[0].play();
		$('#volumetoggle').removeClass('icon-volume-mute2');
		$('#volumetoggle').addClass('icon-volume-medium');
		audioOn = true;
	}
}

function setWindow() {
	setHomepage();
}

$(window).resize(function() { setWindow(); });
$(window).load(function() { setWindow(); });

function setHomepage() {
	ww = $(window).width();
	wh = ww*9/16;
	
	$('.home-feature-wrap').css('height',$(window).height()-(parseFloat($('#ad-markerb').css('height'))-2));
	//$('.home-feature-wrap').css('height',$(window).height()+5);
	if(mobile) {
		$('.home-feature-wrap').css('min-height',500);
	}
	$('.ad-link-section ul').removeClass('small').removeClass('medium');
	
	if(ww <=500) {
		$('.ad-link-section ul').addClass('small');
	} else if(ww <= 900)  {
		$('.ad-link-section ul').addClass('medium');
	} 

	var setW = $(window).width();
	var setH = $(window).height()+5;
	
	var oldh = parseFloat($('.feature-img-container').css('height'));


	if(wh > oldh) {
		if(((oldh/wh)*ww) < ww) {
			$('.feature-img-container .feature-img-vid').removeAttr('height');
			$('.feature-img-container .feature-img-vid').attr('width',setW);
		} else {
			$('.feature-img-container .feature-img-vid').removeAttr('width');
			$('.feature-img-container .feature-img-vid').attr('height',setH);
		}
	} else {
		if(wh<oldh) {
			$('.feature-img-container .feature-img-vid').removeAttr('width');
			$('.feature-img-container .feature-img-vid').attr('height',setH);
		} else {
			$('.feature-img-container .feature-img-vid').removeAttr('height');
			$('.feature-img-container .feature-img-vid').attr('width',setW);
		}
	}
	
	var neww = parseFloat($('.feature-img-container .feature-img-vid').css('width'));
	var newh = parseFloat($('.feature-img-container .feature-img-vid').css('height'));
	
	if(newh < setH) {
		$('.feature-img-container .feature-img-vid').removeAttr('width');
		$('.feature-img-container .feature-img-vid').attr('height',setH);
	} else if(neww < setW) {
		$('.feature-img-container .feature-img-vid').removeAttr('height');
		$('.feature-img-container .feature-img-vid').attr('width',setW);
	}
	
	var neww = parseFloat($('.feature-img-container .feature-img-vid').css('width'));
	
	if(neww > ww) {
		neww = (neww - ww)/2;
		$('.feature-img-container .feature-img-vid').css('marginLeft',-neww);
	} else {
		$('.feature-img-container .feature-img-vid').css('marginLeft',0);
	}
	
	
}

$(function() {
	if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
		mobile = true;
		$("#featureVid").html('');	
		$("#featureVid").css('background-image','url(/img/home-feature-adventure.jpg)');
		$("#featureVid").css('height',$(window).height());
		setWindow();
	}
	setWindow();
});
