                                            '<span class=" to-copy copy-btn" id="copyurl" data-clipboard-target="#shortedlink_' + html.shortId.replace('/','_') + '" data-toggle="tooltip" data-placement="bottom" title="کپی">کپی</span></div>' +
                                            '<p class="title"><a target="_blank" href="' + html.original[0] + '"><input type="text" readonly="readonly" class="target-input" value="' + html.original[0] + '"></a></p>';
                                    html_tr+='<div id="target_info_'+html.shortId+'">';
                                    if(typeof html.target_info.favicon!="undefined")
                                    {
                                        html_tr+='<p class="title"><img class="favicon" src="' + html.target_info.favicon + '" alt="favicon" /><span>' + (html.title? html.title: '') + '</span></p>';
                                    }
                                    html_tr+='</div>';
                                    html_tr+='<p class="title">' + (html.target_info.description? html.target_info.description: '') + '</p>' +
                                            '</div>' +
                                            '<div class="qrcode" id="qr_' + shortenedCount + '"></div>';
                                    if (shortenedCount == 1) {
                                        html_tr = html_tr + cal_to_login;
                                    }
                                    html_tr = html_tr + '</div>';
                                    $('.lish-box').prepend(html_tr).find('.link-result').first().append($("#login-div"));

                                    getShortUrlTitle('http://lish.ir',html.shortId);

                                    $('.login-div').last().prev('.link').css('border', 'none');
                                    $('#url').css('text-align', 'right');
                                    $('#url').val('');
                                    $("body").scrollTop(0);
                                    $('#main-form').find('input').val('');
                                    $('#main-form').find('select').val('');
                                    $('.chosen-select').val('').trigger('liszt:updated');
                                    $('#url.fast_url').val(domain + html.shortURL);
                                    $('#edit_link_fast').show();
                                    $('#copyurl_fast').show();
                                    $('#shortenurl.sub').hide();
                                    $('#advanced_fast').hide();
                                    $('#url.fast_url').attr('readonly', 'readonly');
                                    jQuery('#qr_' + shortenedCount).qrcode({"width": 100, "height": 100, "text": domain + html.shortURL});
                                    $(".chosen-select").val('').trigger("chosen:updated");
                                    $('#url').select();
                                    $('.copy-btn').each(function () {
                                        $(this).on('click', function () {
                                            $(this).addClass('copied');
                                            setTimeout(function () {
                                                $('.copy-btn').removeClass('copied');
                                            }, 1001);
                                        })
                                    });
                                    if (jQuery.isFunction('resetMultiTarget')) {
                                        resetMultiTarget();
                                    }
                                    $('.link-result:gt(2)').hide();
                                } else {
                                    html = jQuery.parseJSON(html);
                                    var ht = '';
                                    for (var i in html.messages) {
                                        ht += (ht==''? '': '<br/>') + html.messages[i];
                                    }
                                    if (typeof jQuery.growl !== typeof undefined) {
                                        jQuery.growl.error({title: '', message: ht});
                                    } else {
                                        $('.ajax').html('<div class="alert alert-danger">' + ht + '</div>').fadeIn('slow');
                                    }
                                    $('.main-input').addClass('error');
                                }
                            }
                        </script>
