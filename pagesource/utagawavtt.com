        <script type="text/javascript">
        // parallax effect
        $(function () {
            if (typeof window.addEventListener !== 'function') {
                return;
            }
            window.requestAnimationFrame = window.requestAnimationFrame
                                            || window.mozRequestAnimationFrame
                                            || window.webkitRequestAnimationFrame
                                            || window.msRequestAnimationFrame
                                            || function(f){ setTimeout(f, 1000/60); };

            var introBack = document.getElementById('introBack');
            var waitFrame = false;
            function parallaxEffect(){
                var scrolltop = window.pageYOffset;                  // get number of pixels document has scrolled vertically
                introBack.style.bottom = -scrolltop * .25 + 'px';        // move introBack at 75% of scroll rate
                waitFrame = false;
            }
            parallaxEffect();
            window.addEventListener('scroll', function(){
                if (waitFrame) {
                    return;
                }
                waitFrame = true;
                requestAnimationFrame(parallaxEffect);
            }, false);
        });
        // end parallax effect

        // city search box
        $(function () {
            $('select[name=city]').select2( {
                placeholder: "Cibler une ville en France",
                minimumInputLength: 2,
                maximumSelectionLength: 2,
                ajax: {
                    url: "/map/ajax/suggest",
                    dataType: 'json',
                    delay: 250,
                    data: function (params) {
                        return {
                            q: params.term
                        };
                    },
                    cache: true
                }
            });
            var selectCity$ = $("select[name='city']");
            var inputCity$ = $(".cityBox .select2-search__field");

            // on select, redirect to search page
            selectCity$.on('select2:select', function (e) {
                var data = $(this).select2('data');
                if (data.length > 0) {
                    inputCity$.val(e.params.data.text);
                    const dC = 0.07;
                    const coord =   (parseFloat(e.params.data.c.x)-dC) + ',' +
                                    (parseFloat(e.params.data.c.y)-dC) + ',' +
                                    (parseFloat(e.params.data.c.x)+dC) + ',' +
                                    (parseFloat(e.params.data.c.y)+dC);
                    window.location.href = "/search?city="+e.params.data.id+"&w=["+coord+"]";
                }
                return true;
            });

            $(".cityBox .select2-selection").off('click');
            $(".cityBox .select2-selection, .cityBox .searchArrow").click(function (e) {
                if( !$('.cityBox .select2-container').hasClass('select2-container--open') ) {
                    selectCity$.select2('open');
                }
            });

            // once loaded, show cityBox
            $('.cityBox').removeClass('hidden');
        });
        // city search box
    </script>

