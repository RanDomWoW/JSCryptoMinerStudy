<script language="javascript">
	function createTag(normal){
		for(var i = 0 ;i<arr_images.length ; i++){
			obj_img = arr_images[i];
			
			//取得数组的第三个值
			var isVisible = obj_img[2];
			if(isVisible){
				//生成li
				var ifocus_li = document.createElement("li");
				//生成a
				var ifocus_a = document.createElement("a");
				//生成img
				var ifocus_img = document.createElement("img");
			
				ifocus_img.src ="/UpFiles/admain/"+obj_img[0];
				
				if(obj_img[1].length != 0 ){
					if(obj_img[1].substring(0,4) == "http" ||obj_img[1].substring(0,1) == "/"){
						ifocus_a.href = obj_img[1];
					}else{
						ifocus_a.href = "/"+obj_img[1];
					}
					//设定a标签的属性
					ifocus_a.target = "_blank";
				}
				//将a标签添加到li上
				ifocus_li.appendChild(ifocus_a);
				
				ifocus_img.width="772";
				ifocus_img.height="145";
				ifocus_img.border="0";
				
				//将图片添加到a标签上
				ifocus_a.appendChild(ifocus_img);
				//将li添加到ul上
				ifocus_ul.appendChild(ifocus_li);	
				//number++;
				if(normal != ""){
					number++;
				}else{
					break;
				}
			}	
		}
	}
</script>
<script language="javascript">
//引入了array.js以后，就能得到一个名称为arr_images的三维数组，其中第一代表图片名称、第二个代买图片href，第三个代表

//前台是否显示
//动态生成ifocus_piclist2层下面的ul、li个数
var ifocus_piclist2 = document.getElementById("ifocus_piclist2");
//获得下面显示图片编号的层，动态处理标号
var ifocus_btn2 = document.getElementById("ifocus_btn2");
var flag = false ;
var number = 0 ;
var obj_img;

	if(arr_images.length>0){
		//根据以上取得的数据个数，来判定生成多少个对应的li
		var ifocus_ul = document.createElement("ul");
		createTag("normal");
		
		//处理在末尾多增加一个和第一个相同的li
		if(arr_images.length > 1){
			createTag("");
		}
		//将生成的ul、li、img添加到最外层的div上
		ifocus_piclist2.appendChild(ifocus_ul);
	}
		//循环数组的长度，并且判断引入数据的长度为1，则不显示下标
		var b_ul = document.createElement("ul");
		b_ul.className = "zzjs_net";
			
		if(number > 1){
			for(var i = 0 ;i<number ; i++){
				//生成对应下标ul、li
				var b_li = document.createElement("li");
				if(i == 0){
					b_li.className="current";
					b_li.id="li1";
				}else{
					b_li.className="normal";
				}
				b_li.innerHTML=i+1;
				b_ul.appendChild(b_li);
			}
				
			//处理最后一张图片生成的下标，需隐藏
			for(var i = 0 ;i<number ; i++){
				//生成对应下标ul、li
				var b_li = document.createElement("li");
				b_li.style.width = "0";
				b_li.style.height = "0";
				b_ul.appendChild(b_li);
				break;
			}

		}
		//将ul、li添加到指定的层上
		ifocus_btn2.appendChild(b_ul);
		if(number>1){
			if(navigator.appName.charAt(0) =="M" && navigator.userAgent.indexOf("Opera") == -1){
				var focusPictureC = new FocusPicture("focusPictureC", "ifocus2", "ifocus_piclist2", 

"ifocus_btn2", "normal", "current", "149", number, "8000");
			}else{
				var focusPictureC = new FocusPicture("focusPictureC", "ifocus2", "ifocus_piclist2", "ifocus_btn2", "normal", "current", "145", number, "8000");
			}
			
			focusPictureC.startAutoChange();
			addOnLoadEvent(focusPictureC, focusPictureC.iFocusChange);
		}
		//隐藏i_banner层
		if(number == 0){
			document.getElementById("i_banner").style.display="none";
		}
</script>

