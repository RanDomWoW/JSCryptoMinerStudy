function uploadimage()
{
// alert("This feature is currently disabled. For now, input the URL of images hosted elsewhere.")
var w=480;
var h=240;
var left = (screen.width/2)-(w/2);
var top = (screen.height/2)-(h/2);
window.open("ycmaker/uploadimage.php",null,"scrollbars=no,location=no,width="+w+", height="+h+", top="+top+", left="+left);
}

function setFocus(object, newIndex, textValue) {
    var size = object.options.length;
        switch (textValue) {
          case 1:
            for (var t = 0; t < size; t++) {
                if (object.options[t].value == newIndex) {
                    object.options[t].selected = true;
                    break;
                }
            }
            break;
          case 2:
            for (var t = 0; t < size; t++) {
                if (object.options[t].text == newIndex) {
                    object.options[t].selected = true;
                    break;
                }
            }
        }
    }

function left(str, n){
        if (n <= 0)
            return "";
        else if (n > String(str).length)
            return str;
        else
            return String(str).substring(0,n);
}

function confirmSubmit(text)
{
var agree=confirm(text);
if (agree)
    return true ;
else
    return false ;
}

function updateSavedCards()
{
    http.open("POST", "ycmaker/fetchcards.php",true);
    http.onreadystatechange = handleResponse;
    var content_type = 'application/x-www-form-urlencoded';
    http.setRequestHeader('Content-Type', content_type);
    http.send("");
}

function deleteCard()
{
        confirmdelete=confirm("Are you sure you want to delete "+document.getElementById("cards").options[document.getElementById("cards").selectedIndex].text+"?");
        if (confirmdelete)
        {
        var cid=document.getElementById("cards").options[document.getElementById("cards").selectedIndex].value;
        document.getElementById("deletecard").value="Deleting...";
        document.getElementById("deletecard").disabled=true;
        http.open("POST", "ycmaker/deletecard.php",true);
        http.onreadystatechange = handleResponse;
        var content_type = 'application/x-www-form-urlencoded';
        http.setRequestHeader('Content-Type', content_type);
        http.send("cid="+cid);
        }
}

function openCard()
{
        document.getElementById("opencard").value="Opening...";
        document.getElementById("opencard").disabled=true;
        http.open("POST", "ycmaker/opencard.php",true);
        http.onreadystatechange = handleResponse;
        var content_type = 'application/x-www-form-urlencoded';
        http.setRequestHeader('Content-Type', content_type);
        http.send("cid="+document.getElementById("cards").options[document.getElementById("cards").selectedIndex].value);
}


function createRequestObject() {
    var ro;
    var browser = navigator.appName;
    if(browser == "Microsoft Internet Explorer"){
        ro = new ActiveXObject("Microsoft.XMLHTTP");
    }else{
        ro = new XMLHttpRequest();
    }
    return ro;
}

var http = createRequestObject();

function sndReq(target,action,sendtype) {
    if (sendtype=="get"){
        http.open("GET", target,true);
        }else{
        http.open("POST", target,true);
        }
    http.onreadystatechange = handleResponse;
    var content_type = 'application/x-www-form-urlencoded';
        http.setRequestHeader('Content-Type', content_type);
    http.send(action);
}

function openSet(a){
    var cc=0;
    document.getElementById("cards").length=0;
    setname=document.getElementById("sets").options[document.getElementById("sets").selectedIndex].value;
    for (t=0; t<numcards; t++) {
        if (yourcards[t][2]==1){
            if (yourcards[t][3]==setname){
                document.getElementById("cards").options[cc] = new Option();
                document.getElementById("cards").options[cc].value = yourcards[t][0];
                document.getElementById("cards").options[cc].text = yourcards[t][1]; 
                cc++;
            }
        }else if(setname=="unsorted"){
                document.getElementById("cards").options[cc] = new Option();
                document.getElementById("cards").options[cc].value = yourcards[t][0];
                document.getElementById("cards").options[cc].text = yourcards[t][1]; 
                cc++;       
        }
    }
    if (savedCard.length>0 && a){
        setFocus(document.getElementById("cards"),savedCard,2);
    }
}

function handleResponse() {
    if(http.readyState == 4){
        var response = http.responseText;
        var update = new Array();

        if(response.indexOf('|' != -1)) {
            
            update = response.split('|');
            updateType = update[0];
            updateResponse = update[1];
            
            // Debug
            // alert(updateType+":"+updateResponse);
            
            if (updateType=="fetch")
            {
                
                if (updateResponse=="success")
                {
                    var sets = new Array()
                    numcards=0;
                    var numsets=1;
                    var opencards=0;
                    document.getElementById("cards").length=0;
                    document.getElementById("sets").length=1;
                    document.getElementById("sets").options[0].value="unsorted";
                    document.getElementById("sets").options[0].text="Unsorted";
   
                        for (t=0; t<=update.length; t=t+4) {
                            
                            yourcards[numcards] = new Array(3);
                            yourcards[numcards][0] = update[t+2];
                            yourcards[numcards][1] = update[t+3];
                            yourcards[numcards][2] = update[t+4];
                            yourcards[numcards][3] = update[t+5];
                            if (yourcards[numcards][2]==1){
                                setname=yourcards[numcards][3];
                                if (sets[setname]!=1){
                                    sets[setname]=1;
                                    document.getElementById("sets").options[numsets] = new Option();
                                    document.getElementById("sets").options[numsets].value = yourcards[numcards][3];
                                    document.getElementById("sets").options[numsets].text = yourcards[numcards][3]; 
                                    numsets++;
                                }
                            }else{
                                    document.getElementById("cards").options[opencards] = new Option();
                                    document.getElementById("cards").options[opencards].value = yourcards[numcards][0]; 
                                    document.getElementById("cards").options[opencards].text = yourcards[numcards][1];
                                    opencards++;                
                            }
                            numcards++;
                        }
                    if (savedCard.length>0){
                        if (savedInSet=="1"){
                            setFocus(document.getElementById("sets"),savedSet,2);
                            openSet(1);
                        }else{
                            document.getElementById("sets").options[0].selected = true;
                        }
                        setFocus(document.getElementById("cards"),savedCard,2);
                    }
                }else
                {
                    if (updateResponse=="error")inset
                    if (updateResponse=="login"){
                        document.getElementById("cards").options[0].text="Login to open cards";
                        document.getElementById("sets").disabled=true;
                        document.getElementById("cards").disabled=true;
                        document.getElementById("opencard").disabled=true;
                        document.getElementById("deletecard").disabled=true;
                    }
                }
            }
            
            if (updateType=="delete")
            {
                if (updateResponse=="success")
                {
                    updateSavedCards();
                }else
                {
                    if (updateResponse=="error"){
                        alert("An error occurred while deleting card. Please try again later.");
                    }
                    if (updateResponse=="login"){
                        alert("You need to be logged in to delete your cards.");
                    }
                }
                document.getElementById("deletecard").value="Delete";
                document.getElementById("deletecard").disabled=false;    
            }
            
            if (updateType=="open")
            {
                if (updateResponse=="success")
                {
                    document.getElementById("name").value = update[2];
                    document.getElementById("cardtype").options.selectedIndex = update[3];
                    document.getElementById("subtype").options.selectedIndex=update[4];
                    document.getElementById("attribute").options.selectedIndex=update[5];
                    document.getElementById("level").options.selectedIndex=update[6];
                    document.getElementById("trapmagictype").options.selectedIndex=update[7];
                    document.getElementById("rarity").options.selectedIndex=update[8];
                    document.getElementById("picture").value=update[9];
                    document.getElementById("set1").value=update[10];
                    document.getElementById("set2").value=update[11];
                    document.getElementById("inset").checked=update[12];
                    document.getElementById("type").value=update[13];
                    document.getElementById("carddescription").value=update[14];
                    document.getElementById("circulation").value=update[15];
                    document.getElementById("atk").value=update[16];
                    document.getElementById("def").value=update[17];
                    document.getElementById("creator").value=update[18];
                    document.getElementById("year").value=update[19];
                    document.getElementById("serial").value=update[20];
                    savedCard=update[2];
                    savedInSet=update[12];
                    savedSet=update[10];
                    updatesetbutton();
                    makecard();
                }else
                {
                    if (updateResponse=="error"){
                        alert("An error occurred while opening card. Please try again later.");
                    }
                    if (updateResponse=="login"){
                        alert("You need to be logged in to open your cards.");
                    }
                }
                document.getElementById("opencard").value="Open";
                document.getElementById("opencard").disabled=false;
                updatefields();
            }
            
            if (updateType=="save")
            {
                if (updateResponse=="success")
                {
updateSavedCards();
                }else
                {
                    if (updateResponse=="error"){
                        alert("An error occurred while saving card. Please try again later.");
                    }
                    if (updateResponse=="login"){
                        alert("You need to be logged in to save your cards.");
                    }
                }     
                savedCard=update[2];
                savedInSet=update[3];
                savedSet=update[4];
                document.getElementById("save").value="Save";
                document.getElementById("save").disabled=false;                  
            }
            
            if (updateType=="upload")
            {
                if (updateResponse=="success")
                {
                    imageurl=update[2];
                    document.getElementById("isurl").style.background="";
                    document.getElementById("isurl").innerHTML='';
                    document.getElementById("url").value=imageurl;
                    document.getElementById("url").style.display="";
                }else
                {
                    if (updateResponse=="error"){
                        alert("An error occurred while uploading card. Please try again later.");
                    }
                    if (updateResponse=="login"){
                        alert("You need to be logged in to upload your cards.");
                    }
                    document.getElementById("isurl").style.background="";
                    document.getElementById("isurl").innerHTML='';
                    document.getElementById("isurl").style.display="none";
                    document.getElementById("url").style.display="none";
                }
            }
        }
    }
}

function ratecard(atk,def,level,ctype,effect)
{
        if (document.getElementById("subtype").selectedIndex=="effect"){effect=true;}else{effect=false;}
        level=Math.round(document.getElementById("level").options[document.getElementById("level").selectedIndex].value);
        ctype=document.getElementById("cardtype").options[document.getElementById("cardtype").selectedIndex].value;
atk=Math.round(document.getElementById("atk").value);
def=Math.round(document.getElementById("def").value);
        rec="Your card is well-rounded.";
        batk=.0024*atk;
        bdef=.0017*def;
    srating=(atk+def)*.0024;
        // reqlevel=Math.round((((batk+bdef)/2)+srating)/2);
        reqlevel=Math.round((batk+bdef+srating)/3);
        if (ctype=="fusion" || ctype=="ritual"){reqlevel=reqlevel-1;}
        if (reqlevel>level){
                        amount=reqlevel-level
                        if (amount<3){adj="a bit ";}
                        if (amount<5 & amount>2){adj="";}
                        if (amount>4){adj="much ";}
                        rec="Your card is "+adj+"too powerful for your chosen level. We'd recommend either raising your monster's level by about "+amount+", or lowering its ";
                        atkp=atk/(atk+def);
                        defp=def/(atk+def);
                        recatk=atk-( Math.round( ( ( 666.667*level )*atkp )/100 )*100 );
                        rec=rec+"ATK by about "+recatk+" and its DEF by about ";
                        recdef=def-(Math.round(((666.667*level)*defp)/100)*100);
                        rec=rec+recdef+".";
                        if (effect==true){
            rec=rec+" Note: This does not take into consideration your card's effect.";
                        }
            }
        alert(rec);
}

