<script type="text/javascript">
var uploader = null;

var removeFile;

$(document).ready(function() {
	uploader = new plupload.Uploader({
	    runtimes : 'html5,flash,silverlight,gears,browserplus,html4',
		browse_button : 'select_file_button',
		container : 'jpg_to_pdf_converter_form_container',
		max_file_size : '10mb',
	    url : '/upload_file.php',
	    chunk_size: '1mb',
	    multipart: true,
        multipart_params : {"PHPSESSID" : "ah9cgsu8p4ri6rqg4hv11mnb22"},
	    flash_swf_url : '/plupload/js/plupload.flash.swf',
		silverlight_xap_url : '/plupload/js/plupload.silverlight.xap',
		filters : [
			{title : "Image files", extensions : "jpg,jpeg,gif,png"}
		],
		resize : {width : 32000, height : 24000, quality : 100}
	});

	$('#convert_to_pdf_button').addClass('disabled');
	$('#convert_to_pdf_button').removeClass('btn-primary');
	$('#cancel_upload_button').addClass('disabled');
	$('#cancel_upload_button').click(function() {
		uploader.stop();
	});

	uploader.init();

	uploader.bind('FilesAdded', function(up, files) {
		for (var i in files) {
			fileQueued(files[i]);
		}
		fileDialogComplete();

		up.refresh(); // Reposition Flash/Silverlight

		$('#cancel_upload_button').removeClass('disabled');
		uploader.start();
	});

	uploader.bind('BeforeUpload', function(up, file) {
        up.settings.multipart_params.file_id = file.id;
    });

	uploader.bind('UploadProgress', function(up, file) {
		uploadProgress(file);
	});

	uploader.bind('Error', function(up, err) {
		$('#jpg_to_pdf_converter_form_container').append("<div>Error: " + err.code +
			", Message: " + err.message +
			(err.file ? ", File: " + err.file.name : "") +
			"</div>");
		up.refresh();
	});

	uploader.bind('FileUploaded', function(up, file) {
		updateCompletedFileNode(file);
	});

	uploader.bind('UploadComplete', function(up, file) {
		queueComplete();
	});

	$('#file_queue').sortable();
	$('#file_queue').disableSelection();

	spinnerImage = new Image();
	spinnerImage.src = "/thumbnail_spinner.gif";


	removeFile = function(fileId) {
		for (var i in fileList) {
			if (fileList[i].id = fileId) {
				fileList.splice(i, 1);
			}
		}
		$('#file_queue_element_' + fileId).remove();
		uploader.removeFile(fileId, false);
	}

	$('#convert_to_pdf_button').click(function() {
		var form = document.createElement('form');
    form.setAttribute('action', '/start_jpg_to_pdf_conversion.php');
    form.setAttribute('method', 'post');
    var params = {};
    params['file_order'] = getFileOrderString();
    params['margin_size'] = $('#margin_size').val();
    params['page_size'] = $('#page_size').val();
    params['image_size'] = $('#image_size').val();
    params['page_orientation'] = $('#page_orientation').val();
    params['new_pdf'] = '1';
    for (var p in params) {
        var inputvar = document.createElement('input');
        inputvar.setAttribute('type', 'hidden');
        inputvar.setAttribute('name', p);
        inputvar.setAttribute('value', params[p]);
        form.appendChild(inputvar);
    }
    document.body.appendChild(form);
    form.submit();
	});

	// Upload handlers

	var swfu;
	var fileList = [];
	var tmpFileList = [];
	var spinnerImage = null;

	function elementIdToFileId(elementId) {
		// Remove "file_queue_element_" prefix
		return elementId.substring(19);
	}

	function getFileOrderString() {
		var ids = [];
		var elements = $('#file_queue').children('.file_queue_element');
		for (var i = 0; i < elements.length; i++) {
			ids.push(elementIdToFileId(elements[i].id));
		}
		return ids.join(',');
	}

	function getHtmlFileNode(file) {
		return '<li class="file_queue_element" id="file_queue_element_' +
			file.id + '">' +
			file.name +
			'<img onclick="removeFile(\'' + file.id + '\')" src="/remove.png" class="remove_button" title="Remove from list"/>' +
			'<div id="progress_bar_container_' + file.id + '" class="progress_bar_container">' +
			  '<div class="progress">' +
			    '<div id="progress_bar_' + file.id + '" class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%">' +
			      '<span class="sr-only">0%</span>' +
			    '</div>' +
			  '</div>' +
			'</div>' +
			'<div id="file_thumbnail_container_' + file.id + '"></div>' +
			'</li>';
	}

	function updateCompletedFileNode(file) {
		$('#progress_bar_container_' + file.id).hide();
		$('#file_thumbnail_container_' + file.id).html(
			'<img class="picture_thumbnail" id="picture_thumbnail_' + file.id + '" src="/thumbnail_spinner.gif"/>');
		$('#picture_thumbnail_' + file.id).attr('src', '/picture.php?file_id=' + file.id);
	}

	function fileQueued(file) {
		idx = 0;
		while ((idx < tmpFileList.length) && (tmpFileList[0].name < file.name)) {
			idx++;
		}
		tmpFileList.splice(idx, 0, file);
	}

	function fileQueueError(file, errorCode, message) {
		alert("Queued error");
	}

	function fileDialogComplete() {
		for (var i in tmpFileList) {
			$('#file_queue').append(getHtmlFileNode(tmpFileList[i]));
		}
		fileList.concat(tmpFileList);
		tmpFileList = [];
	}

	function uploadProgress(file) {
		$('#progress_bar_' + file.id).css('width', file.percent + '%');
		$('#progress_bar_' + file.id).attr('aria-valuenow', file.percent);
		$('#progress_bar_' + file.id + ' span').html(file.percent + ' %');
	}

	function uploadSuccess(file) {
	}

	function uploadError(file, errorCode, message) {
	//	alert("Upload error");
	}

	function uploadComplete(file) {
	//	alert("Upload completed");
	}

	// This event comes from the Queue Plugin
	function queueComplete() {
		$('#cancel_upload_button').addClass('disabled');

		$('#convert_to_pdf_button').removeClass('disabled');
		$('#convert_to_pdf_button').addClass('btn-primary');
	}

});

</script>

