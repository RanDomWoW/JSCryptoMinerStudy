<script type="text/javascript">
    $(function() {
    	if (window.location.href.indexOf('user/password/change') < 0 && $('#showResetPasswordDialog').val() == 'true') { 
    		$('#reset-password-dialog').dialog({
    			autoOpen : true,
    			width : 500,
    			modal: true
    		});		
    		$(".ui-dialog-titlebar-close").hide();
    		$('#reset-password-dialog .js-cancel').click(function(){
    			$('#reset-password-dialog').dialog("close");
    			$.post('/user/changePasswordCancel');
    		});
    		$('#reset-password-dialog .js-reset-password').click(function(){			
    			pu.utils.window.redirect('/user/password/change');
    		});
    	}
    });
</script>