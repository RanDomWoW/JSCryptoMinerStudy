<script>
$('.mainCarousel').slick({
	arrows: true,
	infinite: true,
	speed: 500,
	slidesToShow: 6,
	adaptiveHeight: true,
	autoplay: false
});

$('#toggleCarousel').click(function(){
	$('#toggleCarousel, .carCarousel').hide();
	$('.toggleExplore').show();
});
$('.toggleExplore').click(function(){
	$('#toggleCarousel, .carCarousel').show();
	$('.toggleExplore').hide();
});

$('.carCarouselBudget .item').click(function(){
	var mm = $(this).attr('rel');
	if($(this).hasClass('active')){
		$('#'+mm).remove();
	}else{
		$('#carouselSearch').append('<input type="checkbox" class="makeModel" name="makeModel[]" value="'+mm+'" id="'+mm+'" checked/>');
	}
	$('#carouselSearch').submit();
});
$('#pmin, #pmax').change(function(){
	$('#priceMin').val($('#pmin').val());
	$('#priceMax').val($('#pmax').val());
	$('#carouselSearch').submit();
});
</script>
