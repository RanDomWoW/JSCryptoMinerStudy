        <div
            class="search__passengers__increaseButton"
            data-ng-click="onPlusClick($event)">
            <svg height="13" viewbox="0 0 12 13" width="12">
                <path d="M1.312 11.043l9.224-9.224m-.04 9.673l-9.22-9.216" fill="none" stroke="#514E42" stroke-linecap="round" stroke-width="2"></path>
            </svg>
        </div>
    </div>
</script>
    <script type="text/ng-template" id="lg-select-input.html">
    <div
        class="dropdown ng-cloak"
        data-ng-class="{ 'active' : isActive() }"
