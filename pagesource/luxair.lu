    <script type="text/ng-template" id="lg-date-input.html">
    <div class="datepicker_wrap datepicker_wrap--fullWidth">
        <input
            type="text"
            class="inputTextField inputTextField--border inputDatepicker"
            data-ng-class="{ 'ng-not-empty' : ngModel }"
            data-ng-focus="onFocus($event)"
            data-ng-value="ngModel | lgFormatDate:format"
            placeholder="{{ label }}"
            readonly=""/>
        <label
            class="inputTextField__label ng-cloak"
            data-ng-bind="label">
        </label>
        <span class="icon-datepicker"></span>
        <div
            class="datepicker__calendar"
            data-ng-if="state.active">
            <div class="datepicker__info">
            <!-- Year -->
                <div class="datepicker_year">
                    <input type="hidden" name="year" data-ng-value="year"/>
                    <label class="datepicker_yearLabel">Year</label>
                    <directive
                        class="dropdown--full dropdown--light"
                        data-lg-select-input=""
                        data-on-change="onYearChange(value)">
                        <directive
                            data-lg-select-option=""
                            data-ng-repeat="year in state.data.years"
                            data-ng-selected="isYearActive(year)"
                            data-ng-bind="year"
                            data-value="year">
                        </directive>
                    </directive>
                </div>
                <!-- Month -->
                <div class="datepicker_month">
                    <input type="hidden" name="month" data-ng-value="month"/>
                    <label class="datepicker_monthLabel">Month</label>
                    <directive
                        class="dropdown--full dropdown--light"
                        data-lg-select-input=""
                        data-on-change="onMonthChange(value)">
                        <directive
                            data-lg-select-option=""
                            data-ng-repeat="month in state.data.months"
                            data-ng-selected="isMonthActive($index)"
                            data-ng-bind="month"
                            data-value="$index">
                        </directive>
                    </directive>
                </div>
            </div>
            <!-- Calendar -->
            <div class="grid grid--center grid--collapsedVertical grid--collapsedHorizontal datepicker__calendar__dateRange">
                <div class="grid__cell grid__cell--xs-12 grid__cell--s-6">
                    <directive
                        data-lg-calendar=""
                        data-month="state.input.month"
                        data-is-active="isDateActive(value)"
                        data-is-enabled="isDateAvailable(value)"
                        data-on-change="onDateChange(value)"
                        data-on-navigate-previous="onMonthNavigatePrevious(event)"
                        data-on-navigate-next="onMonthNavigateNext(event)">
                    </directive>
                </div>
            </div>
        </div>
    </div>
</script>
    <script type="text/ng-template" id="lg-cookies-consent.html">
    <div
        data-ng-if="isActive()"
        class="ng-cloak alert__outer">
        <div class="alert alert--cookie">
            <div class="alert__inner">
                <a class="alert__link" data-ng-click="onDismiss()">OK</a>
                <div>
                    <h5 data-ng-transclude="title"></h5>
                    <p data-ng-transclude="message"></p>
                </div>
            </div>
        </div>
    </div>
</script>
    <script type="text/ng-template" id="lg-notifications.html">
    <div>
        <div
            data-ng-repeat="notification in getNotifications()"
            class="alert__outer">
            <div class="alert">
                <div class="alert__inner">
                    <label
                        for="alert__check"
                        class="alert__close"
                        data-ng-click="onDismiss(notification, false)">
                    </label>
                    <span data-ng-class="{
                        'alert__icon-airport-closure': notification.type === 'AIRPORT_CLOSED',
                        'alert__icon-generic-info': notification.type === 'ALERT',
                        'alert__icon-flight-diversion': notification.type === 'FLIGHT_DIVERSION',
                        'alert__icon-strike': notification.type === 'STRIKE',
                        'alert__icon-terminal-change': notification.type === 'TERMINAL_CHANGE',
                        'alert__icon-weather': notification.type === 'WEATHER'
                    }">
                    </span>
                    <div>
                        <h5 data-ng-bind="::notification.title"></h5>
                        <p data-ng-bind="::notification.message"></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>
    <script type="text/ng-template" id="lg-schedule-table.html">
    <table class="scheduleTable">
        <thead>
            <tr>
                <td>Operator</td>
                <td>Flight</td>
                <td data-ng-switch="hasDepartureInformation()">
                    <span data-ng-switch-when="true">Origin</span>
                    <span data-ng-switch-default="">Destination</span>
                </td>
                <td data-ng-switch="hasGateInformation()">
                    <span data-ng-switch-when="true">Gate</span>
                    <span data-ng-switch-default="">Terminal</span>
                </td>
                <td>Initial / Updated Schedule</td>
                <td>Status</td>
            </tr>
        </thead>
        <tbody>
            <tr data-ng-repeat="flight in model">
                <td class="scheduleTable__operator">
                    <span
                        class="scheduleTable__icon"
                        data-ng-class="{
                            'icon--luxair': flight.airlineName === 'LUXAIR',
                            'icon--bird': flight.airlineName === 'LUXAIRTOURS'
                        }">
                    </span>
                </td>
                <td class="scheduleTable__number" data-ng-bind="::flight.flightNumber"></td>
                <td>
                    <span data-ng-bind="::getAirportName(flight)"></span>
                    <span class="scheduleTable__via ng-cloak" data-ng-if="::flight.viaAirport.name">
                        <br/>
                        via: <span data-ng-bind="::flight.viaAirport.name"></span>
                    </span>
                </td>
                <td>
                    <span data-ng-bind="::getAirportArea(flight)"></span>
                </td>
                <td>
                    <span data-ng-bind="getScheduledTime(flight)"></span><br/>
                    <span data-ng-bind="getEstimatedTime(flight)"></span>
                </td>
                <td
                    class="scheduleTable__status"
                    data-ng-class="{
                        'scheduleTable__status--DEP': flight.flightStatusCode === 'DEP',
                        'scheduleTable__status--EXP': flight.flightStatusCode === 'EXP',
                        'scheduleTable__status--CNX': flight.flightStatusCode === 'CNX'
                    }">
                    <span data-ng-bind="::flight.flightStatus"></span>
                </td>
            </tr>
        </tbody>
    </table>
</script>
    <script type="text/ng-template" id="lg-calendar.html">
    <div class="dateRange">
        <div class="dateRange__monthName">
            <div
                class="dateRange__previousMonth"
                data-ng-click="onNavigatePrevious({event : event})">
            </div>
            <span data-ng-bind="getLabel()"></span>
            <div
                class="dateRange__nextMonth"
                data-ng-click="onNavigateNext({event : event})">
            </div>
        </div>
        <table class="dateRange__month">
            <thead>
                <tr class="dateRange__header">
                    <th scope="col">
                        <abbr title="Monday" data-ng-bind="1 | lgFormatWeekday"></abbr>
                    </th>
                    <th scope="col">
                        <abbr title="Tuesday" data-ng-bind="2 | lgFormatWeekday"></abbr>
                    </th>
                    <th scope="col">
                        <abbr title="Wednesday" data-ng-bind="3 | lgFormatWeekday"></abbr>
                    </th>
                    <th scope="col">
                        <abbr title="Thursday" data-ng-bind="4 | lgFormatWeekday"></abbr>
                    </th>
                    <th scope="col">
                        <abbr title="Friday" data-ng-bind="5 | lgFormatWeekday"></abbr>
                    </th>
                    <th scope="col">
                        <abbr title="Saturday" data-ng-bind="6 | lgFormatWeekday"></abbr>
                    </th>
                    <th scope="col">
                        <abbr title="Sunday" data-ng-bind="0 | lgFormatWeekday"></abbr>
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr
                    class="dateRange__week"
                    data-ng-repeat="week in state.calendar">
                    <td
                        class="dateRange__day"
                        data-ng-repeat="date in week track by $index"
                        data-ng-class="{
                            'dateRange__day--blank': (date === undefined),
                            'dateRange__day--enabled': isEnabled({ value: date }),
                            'dateRange__day--selected': isSelected({ value: date }),
                            'dateRange__day--active': isActive({ value: date }),
                            'dateRange__day--lowfare': isPromo({ value: date }),
                            'dateRange__day--start': isStart({ value : date }),
                            'dateRange__day--end': isEnd({ value : date }),
                        }"
                        data-ng-mouseenter="onEnter({ value: date })"
                        data-ng-mouseout="onOut({ value: date })"
                        data-ng-mousedown="!isEnabled({ value: date }) || onChange({ value: date })">
                        <span
                            class="dateRange__dayNumber"
                            data-ng-bind="::date.format('D')">
                        </span>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</script>
    <script type="text/ng-template" id="lg-car-search.html">
    <form name="carSearchForm" data-ng-submit="onSubmit($event)">
        <!-- Form Fields -->
        <input
            type="hidden"
            name="airportPickup"
            data-lg-model-tainted=""
            data-ng-model="state.form.airportPickup"
            required/>
        <input
            type="hidden"
            name="airportReturn"
            data-lg-model-tainted=""
            data-ng-model="state.form.airportReturn"
            required/>
        <input
            type="hidden"
            name="datePickup"
            data-ng-model="state.form.datePickup"
            required/>
        <input
            type="hidden"
            name="dateReturn"
            data-ng-model="state.form.dateReturn"
            required/>
        <input
            type="hidden"
            name="country"
            data-lg-model-tainted=""
            data-ng-model="state.form.country"
            required/>
        <div class="search__carForm grid grid--collapsedHorizontal grid--collapsedVertical">
            <!-- Pick Up -->
            <div class="grid__cell grid__cell--xs-6 grid__cell--s-5">
                <input
                    class="inputTextField search__input search__input--left"
                    data-lg-auto-clear=""
                    data-ng-class="{ 'inputTextField--error': carSearchForm.airportPickup.isTainted() }"
                    data-ng-blur="onAirportPickupBlur($event)"
                    data-ng-change="refreshAirportsPickup()"
                    data-ng-focus="onAirportPickupFocus($event)"
                    data-ng-model="state.input.airportPickup"
                    maxlength="32"
                    placeholder="Pick-up location"
                    tabindex="1"
                    type="text"/>
                <label class="search__inputLabel">Pick-up location</label>
                <div class="search__inputIcon"></div>
            </div>
            <!-- Drop Off -->
            <div class="grid__cell grid__cell--xs-6 grid__cell--s-5">
                <input
                    class="inputTextField search__input search__input--right"
                    data-lg-auto-clear=""
                    data-ng-class="{ 'inputTextField--error': carSearchForm.airportReturn.isTainted() }"
                    data-ng-blur="onAirportReturnBlur($event)"
                    data-ng-change="refreshAirportsReturn()"
                    data-ng-focus="onAirportReturnFocus($event)"
                    data-ng-model="state.input.airportReturn"
                    maxlength="32"
                    placeholder="Drop off location"
                    tabindex="2"
                    type="text"/>
                <label class="search__inputLabel">Drop off location</label>
            </div>
            <!-- Submit -->
            <div class="search__submit grid__cell grid__cell--xs-12 grid__cell--s-2">
                <div class="button">
                    <input
                        class="button__inner"
                        data-ng-disabled="carSearchForm.$invalid || carSearchForm.$pending"
                        tabindex="7"
                        type="submit"
                        value="Search"/>
                </div>
            </div>
            <!-- Airport Pickup List -->
            <div
                class="search__departure grid__cell grid__cell--xs-12 search__result search__result--left ng-cloak"
                data-ng-if="isAirportPickupActive()">
                <directive
                    data-lg-search-list=""
                    data-group-by="country"
                    data-model="state.data.airports"
                    data-on-change="setAirportPickup(value)">
                </directive>
            </div>
            <!-- Airport Return List -->
            <div
                class="search__arrival grid__cell grid__cell--xs-12 search__result search__result--right ng-cloak"
                data-ng-if="isAirportReturnActive()">
                <directive
                    data-lg-search-list=""
                    data-group-by="country"
                    data-model="state.data.airports"
                    data-on-change="setAirportReturn(value)">
                </directive>
            </div>
            <!-- Interval -->
            <div class="search__carPeriod">
                <input
                    type="text"
                    placeholder="Pickup"
                    class="inputTextField search__travelPeriod"
                    data-ng-value="state.form.datePickup | lgFormatDate:'Y-MM-DD'"
                    data-ng-focus="onDatePickupFocus($event)"
                    readonly=""
                    tabindex="3"/>
                <label class="search__travelPeriod__label">Pickup</label>
            </div>
            <div class="search__carPeriod">
                <input
                    type="text"
                    placeholder="Return"
                    class="inputTextField search__travelPeriod"
                    data-ng-disabled="state.form.oneWayTrip"
                    data-ng-value="state.form.dateReturn | lgFormatDate:'Y-MM-DD'"
                    data-ng-focus="onDateReturnFocus($event)"
                    readonly=""
                    tabindex="4"/>
                <label class="search__travelPeriod__label">Return</label>
            </div>
            <!-- Country of Residence -->
            <div class="search__residence">
                <input
                    class="inputTextField search__residenceInput"
                    data-lg-auto-clear=""
                    data-ng-class="{ 'inputTextField--error': carSearchForm.country.isTainted() }"
                    data-ng-blur="onCountryBlur($event)"
                    data-ng-change="refreshCountries()"
                    data-ng-focus="onCountryFocus($event)"
                    data-ng-model="state.input.country"
                    maxlength="32"
                    placeholder="Country of residence"
                    tabindex="5"
                    type="text"/>
                <label class="search__residence__label">Country of residence</label>
            </div>
            <!-- Driver Age -->
            <div class="search__driverAge">
                <input type="number"
                   name="age"
                   placeholder="Age"
                   class="inputTextField search__driverAgeInput"
                   data-min="18"
                   data-max="99"
                   data-lg-model-tainted=""
                   data-ng-class="{ 'inputTextField--error': carSearchForm.age.isTainted() }"
                   data-ng-model="state.form.age"
                   required
                   tabindex="6"/>
                <label class="search__driverAge__label">Age</label>
            </div>
            <!-- Calendar -->
            <div
                class="search__carPeriodInfo grid__cell grid__cell--xs-12 search__calendar__car ng-cloak"
                data-ng-class="{ 'search__calendar__car--return' : isDateReturnActive() }"
                data-ng-if="isIntervalActive()">
                <div class="grid grid--center grid--collapsedVertical grid--collapsedHorizontal">
                    <div class="grid__cell grid__cell--xs-12 grid__cell--s-6 search__calendar__dateRange search__calendar__dateRange--left">
                        <directive
                            data-lg-calendar=""
                            data-month="state.input.months[0]"
                            data-is-active="isDateActive(value)"
                            data-is-enabled="isDateAvailable(value)"
                            data-is-end="isDateReturn(value)"
                            data-is-start="isDatePickup(value)"
                            data-on-enter="onDateEnter(value)"
                            data-on-out="onDateOut()"
                            data-on-change="onDateChange(value)"
                            data-on-navigate-previous="onMonthNavigatePrevious(event)"
                            data-on-navigate-next="onMonthNavigateNext(event)">
                        </directive>
                    </div>
                    <div class="grid__cell grid__cell--xs-12 grid__cell--s-6 search__calendar__dateRange search__calendar__dateRange--right">
                        <directive
                            data-lg-calendar=""
                            data-month="state.input.months[1]"
                            data-is-active="isDateActive(value)"
                            data-is-enabled="isDateAvailable(value)"
                            data-is-end="isDateReturn(value)"
                            data-is-start="isDatePickup(value)"
                            data-on-enter="onDateEnter(value)"
                            data-on-out="onDateOut()"
                            data-on-change="onDateChange(value)"
                            data-on-navigate-previous="onMonthNavigatePrevious(event)"
                            data-on-navigate-next="onMonthNavigateNext(event)">
                        </directive>
                    </div>
                </div>
            </div>
            <!-- Country List -->
            <div
                class="search__residenceInfo grid__cell grid__cell--xs-12 search__result search__result--simple search__country ng-cloak"
                data-ng-if="isCountryActive()">
                <directive
                    data-lg-search-list=""
                    data-model="state.data.countries"
                    data-on-change="setCountry(value)">
                </directive>
            </div>
        </div>
    </form>
</script>
    <script type="text/ng-template" id="lg-flight-search.html">
    <form name="flightSearchForm" data-ng-submit="onSubmit($event)">
        <!-- Form Fields -->
        <input
            type="hidden"
            name="airportDeparture"
            data-ng-model="state.form.airportDeparture"
            required/>
        <input
            type="hidden"
            name="airportArrival"
            data-lg-model-tainted=""
            data-ng-model-options="{ allowInvalid: true }"
            data-ng-model="state.form.airportArrival"
            data-ui-validate-async="{ incompatible: 'validateAirportArrival($modelValue)' }"
            required/>
        <input
            type="hidden"
            name="dateDeparture"
            data-lg-model-tainted=""
            data-ng-model-options="{ allowInvalid: true }"
            data-ng-model="state.form.dateDeparture"
            data-ui-validate-async="{ unavailable : 'validateDateDeparture($modelValue)'}"
            required/>
        <input
            type="hidden"
            name="dateReturn"
            data-ng-model="state.form.dateReturn"
            data-ng-model-options="{ allowInvalid: true }"
            data-lg-model-tainted=""
            data-ng-required="isDateReturnRequired()"
            data-ui-validate-async="{ unavailable: 'validateDateReturn($modelValue)' }"/>
        <input
            type="hidden"
            name="adults"
            data-ng-model="state.form.adults"
            data-ng-model-options="{ allowInvalid: true }"
            data-ui-validate="{ combo: 'validatePassengers($modelValue)' }"/>
        <input
            type="hidden"
            name="youths"
            data-ng-model="state.form.youths"
            data-ng-model-options="{ allowInvalid: true }"
            data-ui-validate="{ incompatible: 'validateYouths($modelValue)' }"/>
        <input
            type="hidden"
            name="children"
            data-ng-model="state.form.children"
            data-ng-model-options="{ allowInvalid: true }"
            data-ui-validate="{ combo: 'validatePassengers($modelValue)' }"/>
        <input
            type="hidden"
            name="infants"
            data-ng-model="state.form.infants"
            data-ng-model-options="{ allowInvalid: true }"
            data-ui-validate="{ combo: 'validateInfants($modelValue)' }"/>
        <div class="search__flightForm grid grid--collapsedHorizontal grid--collapsedVertical">
            <!-- Departure -->
            <div class="search__departure grid__cell grid__cell--xs-6 grid__cell--s-5">
                <input
                    type="text"
                    placeholder="From"
                    class="inputTextField search__input search__input--left"
                    data-lg-auto-clear=""
                    data-ng-change="refreshAirportsDeparture()"
                    data-ng-blur="onAirportDepartureBlur($event)"
                    data-ng-focus="onAirportDepartureFocus($event)"
                    data-ng-keydown="onAirportDepartureKeydown($event)"
                    data-ng-model="state.input.airportDeparture"
                    maxlength="32"
                    tabindex="1"/>
                <label class="search__inputLabel">From</label>
                <div class="search__inputIcon"></div>
            </div>
            <!-- Arrival -->
            <div class="search__arrival grid__cell grid__cell--xs-6 grid__cell--s-5">
                <input
                    type="text"
                    placeholder="Where to?"
                    class="inputTextField search__input search__input--right"
                    data-lg-auto-clear=""
                    data-ng-class="{ 'inputTextField--error': flightSearchForm.airportArrival.isTainted() }"
                    data-ng-change="refreshAirportsArrival()"
                    data-ng-blur="onAirportArrivalBlur($event)"
                    data-ng-focus="onAirportArrivalFocus($event)"
                    data-ng-keydown="onAirportArrivalKeydown($event)"
                    data-ng-model="state.input.airportArrival"
                    data-ng-disabled="state.form.airportDeparture === undefined"
                    maxlength="32"
                    tabindex="2"/>
                <label class="search__inputLabel">To</label>
            </div>
            <!-- Submit -->
            <div class="search__submit grid__cell grid__cell--xs-12 grid__cell--s-2">
                <div class="button">
                    <input
                        type="submit"
                        value="Search"
                        class="button__inner"
                        data-ng-disabled="flightSearchForm.$invalid || flightSearchForm.$pending"
                        tabindex="6"/>
                </div>
            </div>
            <!-- Airport Departure List -->
            <div
                class="grid__cell grid__cell--xs-12 search__result search__result--left ng-cloak"
                data-ng-if="isAirportDepartureActive()">
                <div data-ng-show="state.data.airports === undefined" class="search__loading"></div>
                <directive
                        data-lg-search-list=""
                        data-group-by="country"
                        data-model="state.data.airports"
                        data-on-change="setAirportDeparture(value)">
                </directive>
            </div>
            <!-- Airport Arrival List -->
            <div
                class="grid__cell grid__cell--xs-12 search__result search__result--right ng-cloak"
                data-ng-if="isAirportArrivalActive()">
                <div data-ng-show="state.data.airports === undefined" class="search__loading"></div>
                <directive
                        data-lg-search-list=""
                        data-group-by="country"
                        data-model="state.data.airports"
                        data-on-change="setAirportArrival(value)">
                </directive>
            </div>
            <!-- Passengers -->
            <div
                class="search__passenger grid__cell grid__cell--xs-12 grid__cell--s-6">
                <input
                    type="text"
                    placeholder="Passengers"
                    class="inputTextField search__passengersInput"
                    data-ng-class="{ 'inputTextField--error': (flightSearchForm.adults.$invalid || flightSearchForm.youths.$invalid || flightSearchForm.children.$invalid || flightSearchForm.infants.$invalid) }"
                    data-ng-value="[state.form.adults, state.form.youths, state.form.children, state.form.infants] | lgFormatCounts:'Adults':'Youths':'Children':'Infants'"
                    data-ng-focus="onPassengersFocus($event)"
                    readonly=""
                    tabindex="3"/>
                <label class="search__passengers__label">Passengers</label>
            </div>
            <!-- Passengers Listing -->
            <div class="search__passengerInfo grid__cell grid__cell--xs-12 grid__cell--s-12 ng-cloak "
                 data-ng-if="isPassengersActive()">
                <div class="grid grid--center grid--collapsedVertical">
                    <div class="grid__cell grid__cell--xs-12 grid__cell--s-3 search__passengers__category"
                         data-ng-class="{ 'search__passengers__category--disabled': isYouthsActive() }">
                        <h4 class="search__passengers__headline">
                            Adults<span class="search__passengers__description"></span>
                        </h4>
                        <directive
                            data-label="Adults"
                            data-lg-decimal-input=""
                            data-min="0"
                            data-max="9"
                            data-ng-model="state.form.adults"
                            data-on-change="onAdultsChange(value)">
                        </directive>
                    </div>
                    <div class="grid__cell grid__cell--xs-12 grid__cell--s-3 search__passengers__category">
                        <h4 class="search__passengers__headline">
                            Youths<span class="search__passengers__description">12-24 years</span>
                        </h4>
                        <directive
                            data-lg-decimal-input=""
                            data-min="0"
                            data-max="9"
                            data-on-change="onYouthsChange(value)"
                            data-ng-model="state.form.youths">
                        </directive>
                    </div>
                    <div class="grid__cell grid__cell--xs-12 grid__cell--s-3 search__passengers__category"
                         data-ng-class="{ 'search__passengers__category--disabled': isYouthsActive() }">
                        <h4 class="search__passengers__headline">
                            Children<span class="search__passengers__description">2-11 years</span>
                        </h4>
                        <directive
                            data-lg-decimal-input=""
                            data-min="0"
                            data-max="9"
                            data-ng-model="state.form.children"
                            data-on-change="onChildrenChange(value)">
                        </directive>
                    </div>
                    <div class="grid__cell grid__cell--xs-12 grid__cell--s-3 search__passengers__category"
                         data-ng-class="{ 'search__passengers__category--disabled': isYouthsActive() }">
                        <h4 class="search__passengers__headline">
                            Infants<span class="search__passengers__description">0-1 years</span>
                        </h4>
                        <directive
                            data-lg-decimal-input=""
                            data-min="0"
                            data-max="9"
                            data-ng-model="state.form.infants"
                            data-on-change="onInfantsChange(value)">
                        </directive>
                    </div>

                </div>
            </div>
            <!-- Interval -->
            <div class="search__interval grid__cell grid__cell--xs-12 grid__cell--s-3">
                <input
                    type="text"
                    placeholder="Departure"
                    class="inputTextField search__travelPeriod"
                    data-ng-class="{ 'inputTextField--error': flightSearchForm.dateDeparture.isTainted() }"
                    data-ng-value="state.form.dateDeparture | lgFormatDate:'Y-MM-DD'"
                    data-ng-focus="onDateDepartureFocus($event)"
                    readonly=""
                    tabindex="4"/>
                <label class="search__travelPeriod__label">Departure</label>
            </div>
            <div class="search__interval grid__cell grid__cell--xs-12 grid__cell--s-3">
                <input
                    type="text"
                    placeholder="Return"
                    class="inputTextField search__travelPeriod"
                    data-ng-class="{ 'inputTextField--error': flightSearchForm.dateReturn.isTainted() }"
                    data-ng-disabled="state.form.oneWayTrip"
                    data-ng-value="state.form.dateReturn | lgFormatDate:'Y-MM-DD'"
                    data-ng-focus="onDateReturnFocus($event)"
                    readonly=""
                    tabindex="5"/>
                <label class="search__travelPeriod__label">Return</label>
            </div>
            <!-- Calendar -->
            <div
                class="search__intervalInfo grid__cell grid__cell--xs-12 search__calendar ng-cloak"
                data-ng-class="{ 'search__calendar--return' : isDateReturnActive() }"
                data-ng-if="isIntervalActive()">
                <div data-ng-show="state.data.dates === undefined" class="search__loading"></div>
                <div class="grid grid--center grid--collapsedVertical grid--collapsedHorizontal">
                    <div
                        class="grid__cell grid__cell--xs-12 search__calendar__switch">
                        Round trip
                        <directive
                            data-lg-switch-input=""
                            data-ng-model="state.form.oneWayTrip"
                            data-on-change="onTripTypeChange(value)">
                        </directive>
                        one way
                    </div>
                    <div
                        class="grid__cell grid__cell--xs-12 grid__cell--s-6 search__calendar__dateRange search__calendar__dateRange--left">
                        <directive
                            data-lg-calendar=""
                            data-month="state.input.months[0]"
                            data-is-active="isDateActive(value)"
                            data-is-enabled="isDateAvailable(value)"
                            data-is-end="isDateReturn(value)"
                            data-is-promo="isDatePromo(value)"
                            data-is-start="isDateDeparture(value)"
                            data-on-enter="onDateEnter(value)"
                            data-on-out="onDateOut()"
                            data-on-change="onDateChange(value)"
                            data-on-navigate-previous="onMonthNavigatePrevious(event)"
                            data-on-navigate-next="onMonthNavigateNext(event)">
                        </directive>
                    </div>
                    <div
                        class="grid__cell grid__cell--xs-12 grid__cell--s-6 search__calendar__dateRange search__calendar__dateRange--right">
                        <directive
                            data-lg-calendar=""
                            data-month="state.input.months[1]"
                            data-is-active="isDateActive(value)"
                            data-is-enabled="isDateAvailable(value)"
                            data-is-end="isDateReturn(value)"
                            data-is-promo="isDatePromo(value)"
                            data-is-start="isDateDeparture(value)"
                            data-on-enter="onDateEnter(value)"
                            data-on-out="onDateOut()"
                            data-on-change="onDateChange(value)"
                            data-on-navigate-previous="onMonthNavigatePrevious(event)"
                            data-on-navigate-next="onMonthNavigateNext(event)">
                        </directive>
                    </div>
                    <div class="grid__cell search__calendar__legend ng-cloak" data-ng-if="isPromo()">
                        <span class="search__calendar__lowfare__icon"></span>Lowest Fare
                    </div>
                </div>
            </div>
            <div class="search__errors grid__cell grid__cell--xs-12">
                <div
                        class="search__error ng-cloak"
                        data-ng-if="flightSearchForm.dateDeparture.isTainted() && flightSearchForm.dateDeparture.$error.unavailable">Unavailable departure date
                </div>
                <div
                        class="search__error ng-cloak"
                        data-ng-if="flightSearchForm.dateReturn.isTainted() && flightSearchForm.dateReturn.$error.unavailable">Unavailable return date
                </div>
                <div
                        class="search__error ng-cloak"
                        data-ng-if="flightSearchForm.airportArrival.isTainted() && flightSearchForm.airportArrival.$error.incompatible">Incompatible with the departure airport
                </div>
                <div
                        class="search__error ng-cloak"
                        data-ng-if="flightSearchForm.adults.$error.combo || flightSearchForm.children.$error.combo">Number of adults and children cannot exceed 9
                </div>
                <div
                        class="search__error ng-cloak"
                        data-ng-if="flightSearchForm.youths.$error.incompatible">
                    <p>The youth fare cannot be combined with other fares</p>
                </div>
                <div
                        class="search__error ng-cloak"
                        data-ng-if="flightSearchForm.infants.$error.combo">Number of infants cannot exceed the number of adults
                </div>
            </div>
        </div>
        <div class="search__footer grid">
            <div class="grid__cell">
                All fields are required
            </div>
            <div class="grid__cell">
                More than 9 travellers? <a href='/en/information/group-offers'>Click here</a>
            </div>
        </div>
    </form>
</script>
    <script type="text/ng-template" id="lg-search-list.html">
    <div>
        <div
            class="search__noResult ng-cloak"
            data-ng-show="state.groups.length === 0">
            <span>Sorry, no results found</span>
        </div>
        <div class="search__result__other" data-ng-mousedown="onMouseDown($event)" unselectable="on">
            <div
                class="search__result__group"
                data-ng-repeat="group in state.groups">
                <div
                    class="search__result__group__name"
                    data-ng-bind="::group.name">
                </div>
                <div
                    class="search__result__row"
                    data-ng-repeat="result in state.results[group.code]"
                    data-ng-mousedown="onChange({ value: result })">
                    <div
                        class="textlink search__result__name"
                        data-ng-bind="::result.name">
                    </div>
                    <div
                        class="search__result__code"
                        data-ng-bind="::result.code">
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>
    <script type="text/ng-template" id="lg-travel-search.html">
    <form name="travelSearchForm" data-ng-submit="onSubmit($event)">
        <!-- Form Fields -->
        <input
            type="hidden"
            name="airportDeparture"
            data-ng-model="state.form.airportDeparture"
            required/>
        <input
            type="hidden"
            name="destination"
            data-ng-model="state.form.destination"/>
        <input
            type="hidden"
            name="dateDeparture"
            data-ng-model="state.form.dateDeparture"
            data-ng-required="isDateDepartureRequired()"/>
        <input
            type="hidden"
            name="dateReturn"
            data-ng-model="state.form.dateReturn"
            data-ng-required="isDateReturnRequired()"/>
        <input
            type="hidden"
            name="adults"
            data-ng-model="state.form.adults"/>
        <input
            type="hidden"
            name="children"
            data-ng-model="state.form.children"
            data-ng-model-options="{ allowInvalid: true }"
            data-ui-validate="{ combo: 'validateInfants()', unfit: 'validateChildren()' }"/>
        <div class="search__travelForm grid grid--collapsedHorizontal grid--collapsedVertical">
            <!-- Departure -->
            <div class="search__departure grid__cell grid__cell--xs-6 grid__cell--s-5">
                <input
                    type="text"
                    placeholder="From"
                    class="inputTextField search__input search__input--left"
                    data-ng-model="state.input.airportDeparture"
                    maxlength="32"
                    readonly="readonly"
                    tabindex="1"/>
                <label class="search__inputLabel">From</label>
                <div class="search__inputIcon"></div>
            </div>
            <!-- Arrival -->
            <div class="search__arrival grid__cell grid__cell--xs-6 grid__cell--s-5">
                <input
                    type="text"
                    placeholder="Where to?"
                    class="inputTextField search__input search__input--right"
                    data-lg-auto-clear=""
                    data-ng-change="refreshDestinations()"
                    data-ng-blur="onDestinationBlur($event)"
                    data-ng-focus="onDestinationFocus($event)"
                    data-ng-model="state.input.destination"
                    data-ng-disabled="state.form.airportDeparture === undefined"
                    maxlength="32"
                    tabindex="2"/>
                <label class="search__inputLabel">To</label>
            </div>
            <!-- Submit -->
            <div class="search__submit grid__cell grid__cell--xs-12 grid__cell--s-2">
                <div class="button">
                    <input
                        type="submit"
                        value="Search"
                        class="button__inner"
                        data-ng-disabled="travelSearchForm.$invalid || travelSearchForm.$pending"
                        tabindex="6"/>
                </div>
            </div>
            <!-- Destination List -->
            <div
                class="grid__cell grid__cell--xs-12 search__result search__result--simple search__result--right ng-cloak"
                data-ng-if="isDestinationActive()">
                <div data-ng-show="state.data.destinations === undefined" class="search__loading"></div>
                <directive
                    data-lg-search-list=""
                    data-group-by="group"
                    data-model="state.data.destinations"
                    data-on-change="setDestination(value)">
                </directive>
            </div>
            <!-- Passengers -->
            <div
                class="search__passenger grid__cell grid__cell--xs-12 grid__cell--s-6">
                <input
                    type="text"
                    placeholder="Travelers"
                    class="inputTextField search__passengersInput"
                    data-ng-class="{ 'inputTextField--error': travelSearchForm.children.$invalid }"
                    data-ng-value="[state.form.adults, state.input.children] | lgFormatCounts:'Adults':'Children'"
                    data-ng-focus="onPassengersFocus($event)"
                    readonly=""
                    tabindex="3"/>
                <label class="search__passengers__label">Travelers</label>
            </div>
            <!-- Passengers Listing -->
            <div class="search__passengerInfo grid__cell grid__cell--xs-12 grid__cell--s-12 ng-cloak"
                 data-ng-if="isPassengersActive()">
                <div class="grid grid--center grid--collapsedVertical">
                    <div class="grid__cell grid__cell--xs-12 grid__cell--s-3 search__passengers__category">
                        <h4 class="search__passengers__headline">
                            Adults<span class="search__passengers__description"></span>
                        </h4>
                        <directive
                            data-lg-decimal-input=""
                            data-min="1"
                            data-max="4"
                            data-ng-model="state.form.adults"
                            data-on-change="onAdultsChange(value)">
                        </directive>
                    </div>
                    <div class="grid__cell grid__cell--xs-12 grid__cell--s-3 search__passengers__category">
                        <h4 class="search__passengers__headline">
                            Children<span class="search__passengers__description">0-17 years</span>
                        </h4>
                        <directive
                            data-lg-decimal-input=""
                            data-min="0"
                            data-max="4"
                            data-ng-model="state.input.children"
                            data-on-change="onChildrenChange(value)">
                        </directive>
                    </div>
                    <div
                        class="grid__cell grid__cell--xs-12 grid__cell--s-6 search__passengers__category"
                        data-ng-if="state.form.children.length > 0">
                        <h4 class="search__passengers__headline">
                            Child Age<span class="search__passengers__description">at day of departure</span>
                        </h4>
                        <div class="search__passengers__childage">
                            <input
                                class="inputTextField inputTextField--border"
                                data-ng-change="onChildAgeChange()"
                                data-ng-model="state.form.children[$index]"
                                min="0"
                                max="17"
                                data-ng-repeat="child in state.form.children track by $index"
                                type="number"/>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Interval -->
            <div class="search__interval grid__cell grid__cell--xs-12 grid__cell--s-3">
                <input
                    type="text"
                    placeholder="Earliest departure"
                    class="inputTextField search__travelPeriod"
                    data-ng-value="state.form.dateDeparture | lgFormatDate:'Y-MM-DD'"
                    data-ng-focus="onDateDepartureFocus($event)"
                    readonly=""
                    tabindex="4"/>
                <label class="search__travelPeriod__label">Earliest departure</label>
            </div>
            <div class="search__interval grid__cell grid__cell--xs-12 grid__cell--s-3">
                <input
                    type="text"
                    placeholder="Latest return"
                    class="inputTextField search__travelPeriod"
                    data-ng-disabled="state.form.oneWayTrip"
                    data-ng-value="state.form.dateReturn | lgFormatDate:'Y-MM-DD'"
                    data-ng-focus="onDateReturnFocus($event)"
                    readonly=""
                    tabindex="5"/>
                <label class="search__travelPeriod__label">Latest return</label>
            </div>
            <!-- Calendar -->
            <div
                class="search__intervalInfo grid__cell grid__cell--xs-12 search__calendar ng-cloak"
                data-ng-class="{ 'search__calendar--return' : isDateReturnActive() }"
                data-ng-if="isIntervalActive()">
                <div class="grid grid--center grid--collapsedVertical grid--collapsedHorizontal">
                    <div
                        class="grid__cell grid__cell--xs-12 grid__cell--s-6 search__calendar__dateRange search__calendar__dateRange--left">
                        <directive
                            data-lg-calendar=""
                            data-month="state.input.months[0]"
                            data-is-active="isDateActive(value)"
                            data-is-enabled="isDateAvailable(value)"
                            data-is-end="isDateReturn(value)"
                            data-is-start="isDateDeparture(value)"
                            data-on-enter="onDateEnter(value)"
                            data-on-out="onDateOut()"
                            data-on-change="onDateChange(value)"
                            data-on-navigate-previous="onMonthNavigatePrevious(event)"
                            data-on-navigate-next="onMonthNavigateNext(event)">
                        </directive>
                    </div>
                    <div
                        class="grid__cell grid__cell--xs-12 grid__cell--s-6 search__calendar__dateRange search__calendar__dateRange--right">
                        <directive
                            data-lg-calendar=""
                            data-month="state.input.months[1]"
                            data-is-active="isDateActive(value)"
                            data-is-enabled="isDateAvailable(value)"
                            data-is-end="isDateReturn(value)"
                            data-is-start="isDateDeparture(value)"
                            data-on-enter="onDateEnter(value)"
                            data-on-out="onDateOut()"
                            data-on-change="onDateChange(value)"
                            data-on-navigate-previous="onMonthNavigatePrevious(event)"
                            data-on-navigate-next="onMonthNavigateNext(event)">
                        </directive>
                    </div>
                </div>
                <div class="search__travelDuration bgcolor-lejaune text-center" data-ng-show="state.active">
                    <directive
                        class="dropdown--light"
                        data-lg-select-input=""
                        data-ng-model="state.form.duration"
                        label="Travel Period">
                        <directive data-lg-select-option="" data-value="">As Specified</directive>
                        <directive data-lg-select-option="" data-value="[7, 7]" selected="">1 Week</directive>
                        <directive data-lg-select-option="" data-value="[14, 14]">2 Weeks</directive>
                        <directive data-lg-select-option="" data-value="[21, 21]">3 Weeks</directive>
                        <directive data-lg-select-option="" data-value="[22, 28]">&gt; 3 Weeks</directive>
                        <directive data-lg-select-option="" data-value="[1, 4]">1-4 Nights</directive>
                        <directive data-lg-select-option="" data-value="[5, 8]">5-8 Nights</directive>
                        <directive data-lg-select-option="" data-value="[9, 12]">9-12 Nights</directive>
                        <directive data-lg-select-option="" data-value="[13, 15]">13-15 Nights</directive>
                        <directive data-lg-select-option="" data-value="[16, 22]">16-22 Nights</directive>
                        <directive data-lg-select-option="" data-value="[1, 1]">1 Night</directive>
                        <directive data-lg-select-option="" data-value="[2, 2]">2 Nights</directive>
                        <directive data-lg-select-option="" data-value="[3, 3]">3 Nights</directive>
                        <directive data-lg-select-option="" data-value="[4, 4]">4 Nights</directive>
                        <directive data-lg-select-option="" data-value="[5, 5]">5 Nights</directive>
                        <directive data-lg-select-option="" data-value="[6, 6]">6 Nights</directive>
                        <directive data-lg-select-option="" data-value="[7, 7]">7 Nights</directive>
                        <directive data-lg-select-option="" data-value="[8, 8]">8 Nights</directive>
                        <directive data-lg-select-option="" data-value="[9, 9]">9 Nights</directive>
                        <directive data-lg-select-option="" data-value="[10, 10]">10 Nights</directive>
                    </directive>
                </div>
            </div>
            <div class="search__errors grid__cell grid__cell--xs-12">
                <div
                        class="search__error ng-cloak"
                        data-ng-if="travelSearchForm.children.$error.combo">
                    Number of children under 2 years of age cannot exceed the number of adults
                </div>
                <div
                        class="search__error ng-cloak"
                        data-ng-if="travelSearchForm.children.$error.unfit">
                    Children age must be between 0 and 17 years
                </div>
            </div>
        </div>

    </form>
</script>
