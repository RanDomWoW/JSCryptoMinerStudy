    <script type="text/ng-template" id="lg-select-option.html">
    <div class="dropdown__item" data-ng-click="onClick($event)">
        <div class="dropdown__itemInner">
            <span data-ng-transclude=""></span>
        </div>
    </div>
</script>
    <script type="text/ng-template" id="lg-switch-input.html">
    <div class="switch">
        <input
            type="checkbox"
            class="switch__checkbox"
            data-ng-class="{ 'switch__checkbox--active': isActive() }" />
        <label
            class="switch__label"
