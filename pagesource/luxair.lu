         data-ng-class="{ 'search__passengers__input--disabled': isDisabled() }">
        <div
            class="search__passengers__decreaseButton"
            data-ng-click="onMinusClick($event)">
            <svg height="13" viewbox="0 0 12 13" width="12">
                <path d="M1.312 11.043l9.224-9.224" fill="none" stroke="#514E42" stroke-linecap="round" stroke-width="2"></path>
            </svg>
        </div>
        <input
            type="text"
            class="inputTextField inputTextField--border"
            data-ng-model="ngModel"
            data-ng-disabled="isDisabled()"
