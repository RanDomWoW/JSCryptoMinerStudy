nostojs(function (api) {

	// wait until nosto recommendations are loaded
	api.listen('postrender', function (postRenderElements) {

		window.fireEvent('nosto_postrender', postRenderElements);

		// render scrollers
		$$('.panel-scroller-nosto').each(function (el) {

			var containerId = el.get('id');

			var frameEl = el.getElement('.panel-frame');
			if (frameEl) {
				var panelFrameId = frameEl.get('id');

				var panels = $(panelFrameId).getElements('.panels-inner div').length,
					prev = $(containerId).getElement('.prev'),
					next = $(containerId).getElement('.next'),
					current = $(containerId).getElement('.nosto-current');

				var paginationEl = $(containerId).getElement('.nosto-pagination');
					paginationId = paginationEl.get('id');

				new Panels(panelFrameId, paginationId, {
					defaultPanel: 'panel-1',
					controls: containerId,
					mode: 'horizontal',
					transition: 'expo:out',
					scrollToOnStart: false,
					checkWindowSize: false,
					tabElement: 'li',
					onPanelChanged: function(link, id) {
						currentPanel = id.split('-')[1];
						var index = parseInt(id.slice(-1));
						prev.setStyle('display', 'block');
						next.setStyle('display', 'block');

						if (index == 1) {
							prev.setStyle('display','none');
						}

						if (index == panels) {
							next.setStyle('display','none');
						}

						current.set('text', index);
					}
				});
			}

		});
	});

