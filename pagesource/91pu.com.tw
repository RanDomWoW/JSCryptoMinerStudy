                    alert("獲取樂譜錯誤！");
                }
            });
            $.ajax({
                type: ajaxtype,
                url: ajaxpath + 'ajaxapi/videolists.php',
                data: {
                    type: 4,
                    number: 2,
                    page: 1,
                },
                contentType: ajaxct,
                dataType: ajaxdt,
                success: function(data) {
                    if (data.done == "ok") {
                        $('#v3').html('<a href="/video/hot"><img src="' + data.list[0].image + '"></a><code onclick="gotoPage(\'/video/hot\')" class="vdo_name">'+data.list[0].name+'</code>');
                        $('#v4').html('<a href="/video/"><img src="/uploads/allimg/170725/830-1FH52011290-L.png"><span>查看更多影片</span></a><code onclick="gotoPage(\'/video\')" class="vdo_name"></code>');
                    } else {}
                },
                error: function() {
                    alert("獲取樂譜錯誤！");
                }
            });
        });

        function getHotInfo() {
            var _t=4-hottype;
            if(_t==3){
                _t=2;
            }
            $('.tabs').removeClass('on').eq(_t).addClass('on');
            $.ajax({
                type: ajaxtype,
                url: ajaxpath + 'ajaxapi/gettones.php',
                data: {
                    order: 'hot',
                    sorttype: hottype,
                    number: 42
                },
                contentType: ajaxct,
                dataType: ajaxdt,
                success: function(data) {
                    if (data.done == "ok") {
                        $('#hotToneList').empty();
                        $.each(data.list, function(ix, va) {
                            $('#hotToneList').append(
                                '<div class="swiper-slide">\
                                    <div class="pic">\
                                        <code>' + (ix + 1) + '</code>\
                                        <a href="' + va.url + '">\
                                            <img src="' + zoomSingerFaceImg(va.singerface) + '" onerror="errorImage(this)">\
                                        </a>\
                                    </div>\
                                    <p class="music_name"><a href="' + va.url + '">' + va.name + '</a></p>\
                                    <p class="singer"><a href="' + va.singerurl + '">' + va.singer + '</a></p>\
                                </div>'
                            );
                        });
                        new Swiper('.hot_score .swiper-container', {
                            slidesPerView: 6,
                            spaceBetween: 10,
                            slidesPerGroup: 6,
                            loop: true,
                            loopFillGroupWithBlank: true,
                            navigation: {
                                nextEl: '.hot_score .swiper-button-next',
                                prevEl: '.hot_score .swiper-button-prev',
                            },
                        });
                    } else {}
                },
                error: function() {
                    alert("獲取樂譜錯誤！");
                }
            });
        }

        function setLocHash(t) {
            try {
                $.cookie('initSingerShow', t + '_' + hottype, {
                    expires: 1,
                    path: '/singer'
                });
            } catch (e) {}
        }
    </script>

</body>

