        for (var i = 0; i < datetime.length; i++) {
            var adddate = $(datetime[i]).html().split(' ')[0];
            var h1 = $(datetime[i]).html().split(' ')[1].split('-')[0];
            var h2 = $(datetime[i]).html().split(' ')[1].split('-')[1];
            var st = Date.parse(adddate + " " + h1);
            var et = Date.parse(adddate + " " + h2);
            var now = new Date();
            var css = "";
            if (now < st)
                css = "url(/images/tp_03.jpg) no-repeat";
            if (now >= st && now <= et)
                css = "url(/images/tp_06.jpg) no-repeat";
            if (now > et)
                css = "url(/images/tp_04.jpg) no-repeat";
            $(datetime[i]).next().css("background", css);
        }
      </script>
 
