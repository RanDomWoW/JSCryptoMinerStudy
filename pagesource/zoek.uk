  <div class="wrapper">
    <h1 class="brand-statement">Start your new job search here...</h1>
    <form class="search-form home" action="/search" method="get" autocomplete="off">
      <div>
        
        <div data-search-container>
          <fieldset class="search-form__fieldset">
            <legend class="visuallyhidden">Search for a job</legend>
            <div class="search-form__row search-form__row--first" data-suggestions-wrapper>
                <input type="hidden" name="sid" id="job-suggestion-id" data-suggestion-input value=""/>
                <label class="search-form__label form-label" for="job-keyword"></label>
                <input 
                  class="free-text-input search-form__input" 
                  type="text" 
                  name="q" 
                  id="job-keyword" 
                  data-keyword-input 
                  placeholder="e.g Engineering, sales, photoshop" 
                  value="" 
                  data-filter-item-text 
                    />
            </div>
            <div class="search-form__row search-form__row--last">
                <div class="search-form__location-container" data-location-suggestions-wrapper>
                <label class="search-form__label form-label" for="location">Job Location
                <span style="float: right;cursor: pointer;" data-location-button> Use my Location <img class="search-form__location-image" src=""></img></span>
                </label>
                  <input type="hidden" name="lat" id="location-latitude" data-latitude-input value=""/>
                  <input type="hidden" name="long" id="location-longitude" data-longitude-input value=""/>
                  <input type="hidden" name="addr" id="location-postal-address" data-postal-input value=""/>
                  <input 
                    class="free-text-input search-form__input search-form__input--location" 
                    type="text" 
                    name="location" 
                    id="location" 
                    autocomplete="off"
                    data-location-input 
                    placeholder="UK postcode, town or city" 
                    value="" 
                    data-filter-item-text 
                     
                     />
                  
                </div>
            </div>
          </fieldset>
          <div class="search-form__row-action">
            <button class="btn " data-matches-button >
                    Find jobs
            </button>
            
          </div>
        </div>
      </div>
      <script>
        var searchData = {
          searchRadius: '10',
          jobType: '',
          contractHours: '',
          salaryPeriod: '',
          minSalary: '',
          maxSalary: ''
        };
      </script>
      <div class="search-advanced ">
          <a class="btn btn--secondary " href="/my-account/profile/cvs?register&#x3D;true" id="btn-register" title="Create an account & improve your job search" rel="nofollow">Register CV</a>
        <a class="advanced-title" role="button" aria-controls="advanced" aria-expanded="false" href="#advanced">Show Advanced Search</a>
        <a href="#" role="button" id="clear-advanced">Clear Job Search</a>
        <input type="hidden" name="sortBy" />
        <div id="advanced" role="region" tabindex="-1" class="       ">
          <div class="span">
            <label class="control-heading">Travel Distance</label>
            <div class="filter-slider-container group labelled" data-default="10"><div class="filter-slider"></div></div>
            <div class="group">
              <div class="control"><input type="radio" name="searchRadius" value="5" id="searchRadius5" /><label for="searchRadius5">5 Miles</label></div>
              <div class="control"><input type="radio" name="searchRadius" value="10" id="searchRadius10" /><label for="searchRadius10">10 Miles</label></div>
              <div class="control"><input type="radio" name="searchRadius" value="25" id="searchRadius25" /><label for="searchRadius25">25 Miles</label></div>
              <div class="control"><input type="radio" name="searchRadius" value="50" id="searchRadius50" /><label for="searchRadius50">50 Miles</label></div>
              <div class="control"><input type="radio" name="searchRadius" value="100" id="searchRadius100" /><label for="searchRadius100">100 Miles</label></div>
              <div class="control"><input type="radio" name="searchRadius" value="150" id="searchRadius150" /><label for="searchRadius150">150 Miles</label></div>
            </div>
          </div>
          <div class="float">
            <label class="control-heading">Job Type</label>
            <div class="select-upgrade"></div>
            <div class="group indent">
              <div class="control"><input type="radio" name="jobType" value="Temporary" id="jobTypeT" /><label for="jobTypeT">Temporary</label></div>
              <div class="control"><input type="radio" name="jobType" value="Permanent" id="jobTypeP" /><label for="jobTypeP">Permanent</label></div>
              <div class="control"><input type="radio" name="jobType" value="Contract" id="jobTypeC" /><label for="jobTypeC">Contract</label></div>
            </div>
          </div>
          <div class="float">
            <label class="control-heading">Contract Type</label>
            <div class="select-upgrade"></div>
            <div class="group indent">
              <div class="control"><input type="checkbox" name="contractHours" value="FullTime" id="contractHoursF" /><label for="contractHoursF">Full-time</label></div>
              <div class="control"><input type="checkbox" name="contractHours" value="PartTime" id="contractHoursP" /><label for="contractHoursP">Part-time</label></div>
              <div class="control"><input type="checkbox" name="contractHours" value="Unspecified" id="contractHoursU" /><label for="contractHoursU">Unspecified</label></div>
            </div>
          </div>
          <div class="float">
            <label class="control-heading">Pay Period</label>
            <div class="select-upgrade"></div>
            <div class="group indent">
              <div class="control"><input type="radio" name="salaryPeriod" value="Unknown" id="salaryPeriodA" /><label for="salaryPeriodA">Any</label></div>
              <div class="control"><input type="radio" name="salaryPeriod" value="PerHour" id="salaryPeriodH" /><label for="salaryPeriodH">Per Hour</label></div>
              <div class="control"><input type="radio" name="salaryPeriod" value="PerDay" id="salaryPeriodD" /><label for="salaryPeriodD">Per Day</label></div>
              <div class="control"><input type="radio" name="salaryPeriod" value="PerAnnum" id="salaryPeriodAn" /><label for="salaryPeriodAn">Per Annum</label></div>
            </div>
          </div>
          <div class="float">
            <label class="control-heading">Pay Range</label>
            <div class="group range">
              <div class="control control-inline"><input type="number" min="0" max="250000" name="minSalary" style="-moz-appearance: none;" value="" id="minSalary" /></div>
              <div class="control control-inline"><input type="number" min="0" max="250000" name="maxSalary" style="-moz-appearance: none;" value="" id="maxSalary" /></div>
              <label for="minSalary">Min &#0163;</label>
              <label for="maxSalary">Max &#0163;</label>
            </div>
          </div>
          <div class="clearfix"></div>
          <div class="not-wide">
            <button class="btn" data-matches-button="">Find jobs</button>
          </div>
        </div>
      </div>    </form>
