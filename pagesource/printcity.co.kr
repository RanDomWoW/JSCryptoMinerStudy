        function show_time() {
            // 1000ms 단위로 시간 갱신 시키기
            window.setInterval("get_current_time()", 1000);
            get_current_time();
        }

        function get_current_time() {
            var time = document.getElementById("time");
            var now = new Date();
            var week = new Array("일", "월", "화", "수", "목", "금", "토");

            var clock = now.getFullYear() + ".";
            clock += (now.getMonth() + 1) + ".";
            clock += now.getDate() + "";
            clock += " (" + week[now.getDay()] + ") ";

			if (now.getHours() == 12 && week[now.getDay()] != "토" && week[now.getDay()] != "일") {
				$("#todayprint_a").css('display','block');
				$("#todayprint_b").css('display','none');
				$("#todayprint_c").css('display','none');
			}
			else if (now.getHours() == 13 && week[now.getDay()] != "토" && week[now.getDay()] != "일") {
				$("#todayprint_a").css('display','none');
				$("#todayprint_b").css('display','block');
				$("#todayprint_c").css('display','none');
			}
			else if (now.getHours() == 14 && week[now.getDay()] != "토" && week[now.getDay()] != "일") {
				$("#todayprint_a").css('display','none');
				$("#todayprint_b").css('display','none');
				$("#todayprint_c").css('display','block');
			} else {
				$("#todayprint_a").css('display','none');
				$("#todayprint_b").css('display','none');
				$("#todayprint_c").css('display','none');
			};
			

        };
