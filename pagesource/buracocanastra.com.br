html += 'codebase="http://fpdownload.macromedia.com/get/flashplayer/current/swflash.cab"> <param name="movie" value="';
html += cfileswf;
html += '"/> <param name="quality" value="high" /> <param name="allowScriptAccess" value="always" /> ';
html += '<param name="allowFullScreen" value="true"/> <param name="wmode" value="opaque"/>  <embed src="';
html += cfileswf;
