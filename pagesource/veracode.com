							liObj.clearQueue().stop();
							$('.bxslider-reason li').removeClass('active');
							$('.'+liObj.attr('class').split(' ')[0]).addClass('active');
							$('#large-detailed-image h2').html(liObj.attr('data-heading'));
							$('#large-detailed-image span').html(liObj.attr('data-span'));
							largeImageObj.attr('src', liObj.attr('data-large-image')).fadeTo(400,1);
						});
					}
				});
			}
		});
		/*setSlideHeight();
		$(window).resize(function(){setSlideHeight();});*/
	}
	
	
			
