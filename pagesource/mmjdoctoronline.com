               $(document).ready(function(){

                  $('.changeLang').on('click', function(event){
                      event.stopPropagation();
                      event.preventDefault();

                      $.ajax({
                        type: "POST",
                        url: "/language/change",
                        dataType: 'html',
                        success: function (data) {

                        if (data == true) {
                           location.reload();
                        }
                        
                        return;
                    },
                    beforeSend: function () {
                    }
                });


                  });

                   $('form#ForgotGetForm').submit(function(event) {


                       var form = $(this);

                       var input1 = $("form#ForgotGetForm input[name='data[Forgot][pwd1]']").val();
                       var input2 = $("form#ForgotGetForm input[name='data[Forgot][pwd2]']").val();

                       if(input1 === input2)
                       {
                           return true;
                       }
                       else
                       {
                           $('.error_register').html('The fields must be identical');
                           return false;
                       }

                       event.preventDefault(); // Prevent the form from submitting via the browser.

                   });

                   $("#hide_footer").click(function () {
                       $(".footer").slideToggle(200);
                       $("#hide_footer").toggleClass("footer_down");
                       $("body").toggleClass("body-zero");
                   });


