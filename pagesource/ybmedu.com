		$(function(){
			$("div.cen_box_inner").find("a").each(function(){
				if ($(this).attr("href").indexOf("siteURL="+hostUrl) <= 0 || $(this).attr("class") == "timetable"){
					$(this).attr({
						target :"_blank",
						title : "새창"
					}).addClass("target_blank")
				}
			})
			// 원투원은 신규사이트 개편으로 인해 DB에서 3차도메인으로 가는게 아닌 직접적으로 가겠금 변경.
