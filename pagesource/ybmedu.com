				$(this).attr("href" , "/ybmsisacom.asp?siteURL=www.ybm1to1.com");
			});
		})
		$("div.sp_course_num").find("a").click(function(e){
			e.preventDefault()
			var nextIdx = $(this).parent().index()
			toggleClass(nextIdx,"actived",$(this).parent().parent().find("li"))
			toggleClass(nextIdx,"on",$("div.sp_course_list").find("li"),"fade")
		})

		function rollImg(){
			var obj = $("div.sp_course_list").find("li")
			var nextIdx = obj.filter(".on").index()+1 < obj.length  ? obj.filter(".on").index()+1 : 0
			toggleClass(nextIdx,"on",obj,"fade")
			toggleClass(nextIdx,"actived",$("div.sp_course_num").find("li"))
		}

		function toggleClass (idx,clas,target,option){
			if (option== "fade"){
				target.filter("."+clas).fadeOut("slow",function(){
					$(this).removeClass(clas).hide();
				}).end().eq(idx).fadeIn("slow",function(){
					$(this).addClass(clas)
				})
			}else{
				target.removeClass(clas).eq(idx).addClass(clas)
			}

		}

		setInterval(rollImg,3000)





		function fnWinOpen(url,target,winSet)
		{
			window.open(url,target,winSet);
			return false;
		}

	</script>
