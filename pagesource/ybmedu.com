<script>
	$(document).ready(function(){
		$("#quickhold").click(function(){
			var pos=$("#quick").offset().top;
			var quickdiv=$(this).parent().parent().parent();
			if ($(this).find("span").text()=="HOLD")
			{
				quickdiv.removeClass("fix");
				quickdiv.css("top",pos);
				$(this).find("span").text("MOVE");
			}else{
				quickdiv.addClass("fix");
				quickdiv.css("top","");
				$(this).find("span").text("HOLD");
			}
		});
	});
</script>
	
