	<script>
		$(function() {
			$(".video-handle").on("click", function(event) {
				// Jamie Beck <jbeck@terabit.ca>

				var isYoutube = false;
				if($(this).data('youtubecode')){
					isYoutube = true;
					var youtubecode = $(this).data('youtubecode');
					var content = '<iframe id="fancybox-video" width="853" height="480" src="//www.youtube.com/embed/' + youtubecode + '?rel=0&wmode=transparent&autoplay=1&showinfo=0&modestbranding=1" frameborder="0" allowfullscreen ></iframe>';
				}
				else{
					var content = $($(this).attr('href')).clone().attr('id', 'fancybox-video').css('display', 'block').attr('autoplay', true);
				}
				var width = $(content).attr('width'), height = $(content).attr('height'), aspectRatio = height / width;

				var box = $.fancybox({
					openEffect: 'none',
					closeEffect: 'none',
					padding: 0,
					margin: 0,
					autoSize: true,
					closeBtn: false,
					helpers: {
						overlay: {
							css: {
								background: 'rgba(0, 0, 0, 0.85)'
							}
						}
					},
					afterShow: function(current, previous) {
						var width = document.getElementById('fancybox-video').parentElement.offsetWidth;
						if(!isYoutube){
							videojs('#fancybox-video', {}, function() {
								// loaded
								this.on('ended', function() {
									$.fancybox.close();
								});
							}).width(width).height(width * aspectRatio);
						}
						else{
							// youtube
							$('#fancybox-video').width(width).height(width * aspectRatio);
						}
					},
					afterClose: function() {
						if(isYoutube){
							$('#fancybox-video').remove();
						}
						else{
							videojs('#fancybox-video').dispose();
						}
					},
					content: content
				});

				event.preventDefault();
				return false;
			});

		}); // ready
	</script>
