			if (!$('#statistics_box').hasClass('start')){
				$('#statistics_box').addClass('start');
				
				var current = 580000;
				$({count: 0}).animate({count: current}, {
					duration: 1500,
					easing: 'linear',
					progress: function() { 
						$('#counter_total').text(Math.ceil(this.count)); 
					}
				});
				
				$('#counter_month').addClass('start');
