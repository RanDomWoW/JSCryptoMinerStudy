<script>
    $("#calOne").foundation();
    $(document).ready(function () {

        var event = new Object();
        event.StateName = "Missouri";
        event.RegionName = "Midwest";
        event.FilterByLocation = true;
        event.EventType = "";
        event.RegistrationUrl = "/Events/EventRegistration.aspx";
        event.ProductIdUrlParameter = "productId";
        event.LoginUrl = "/SignIn";
        event.Certs = "";

        var myTemplate = $.templates("#eventTemplate");

        getCalendarEvents();

        $("#calOne .button").click(function (e) {
            switch(this.id) { 
                case "all":
                    event.EventType = "";
                    event.FilterByLocation = false;
                    break;
                case "national":
                    event.EventType = "NATIONAL";
                    event.FilterByLocation = false;
                    break;
                case "statereg":
                    event.EventType = "STATECLIN";
                    event.FilterByLocation = false;
                    break;
                case "exam":
                    event.EventType = "EXAMPREP";
                    event.FilterByLocation = false;
                    break;
                case "EDUCOURSE":
                    event.EventType = "EDUCOURSE";
                    event.FilterByLocation = false;
                    break;
            }
            getCalendarEvents();
        });

        function setNice() {
            if ($("#calOne .cal-container").width() > 300) {
                $("#calOne").find(".layout")
                    .removeClass("col-xs-12").addClass("col-xs-6");
                $("#calOne").find(".cal-nav")
                    .removeClass("col-xs-12").addClass("col-xs-1");
                $("#calOne").find(".cal-nav-col")
                    .removeClass("col-xs-12").addClass("col-xs-11");
                $("#calOne").find(".button-group")
                    .removeClass("expanded").addClass("stacked");
                $('#calOne .calrow').foundation('getHeightsByRow', cb);
            }
            else {
                $('#calOne .calrow').foundation('getHeightsByRow', cb);
            }
        }
        function cb(e)
        {
            $("#calOne .calrow").foundation('applyHeightByRow', e);
        }

        //Show default filtered events
        function getCalendarEvents() {
            $.ajax({
                type: "POST",
                url: "/Svc/CalendarEventSvc.asmx/FilterCalEvents",
                data: JSON.stringify(event),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                beforeSend: function () {
                    $("#calOne .loadingModal").show();
                },
                success: function (msg) {
                    //Add results to the calendar interface                   
                    var htmlOut = myTemplate.render(msg.d);
                    $("#calOne .cal-container").html(htmlOut);
                    $("#calOne .cal-container").foundation();
                    setNice();
                    //remove loading anime
                    $("#calOne .loadingModal").hide();
                },
                error: function (ex) {
                    //remove loading anime
                    $("#calOne .loadingModal").hide();
                }
            });
        }
    });
</script>

