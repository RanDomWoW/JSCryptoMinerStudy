<script>
    $(document).ready(function() {
        var tm = new Date();
        var hours = tm.getHours();
        var counter = 562952;
        counter_ = counter.toString().replace(/(\d)(?=(\d\d\d)+([^\d]|$))/g, '$1 ');
        document.getElementById('countermobile').innerHTML = counter_;
        if (hours >= 8 && hours <= 22) {
            setInterval(function() {
                counter++;
                counter_ = counter.toString().replace(/(\d)(?=(\d\d\d)+([^\d]|$))/g, '$1 ');
                document.getElementById('countermobile').innerHTML = counter_;
            }, 60000)
        } else
            document.getElementById('countermobile').innerHTML = counter_;
    });
</script>




