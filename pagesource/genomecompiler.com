  }
  else {
    var utm_concat = JSON.parse(jQuery.cookie('utm_concat'));
    if (utm_concat) {
      var utm_source = utm_concat['utm_source'];
      var utm_medium = utm_concat['utm_medium'];
      var utm_term = utm_concat['utm_term'];
      var utm_content = utm_concat['utm_content'];
      var utm_campaign = utm_concat['utm_campaign'];
    }
    else {
      var utm_source = 0;
    }
  }
  
  var path = window.location.pathname;
  var search = window.location.search;

  mixpanel.track('WEBSITE_PAGE_VIEW',
   {
      'path': path,
      'search': search,
      'utm_source': utm_source,
      'utm_medium': utm_medium,
      'utm_term': utm_term,
      'utm_content': utm_content,
      'utm_campaign': utm_campaign,
      'utm_concat': utm_concat
   }
  );
  
  if (document.referrer && document.referrer.indexOf("genomecompiler.com") == -1) {
    function parseURL(url) {
      var a=document.createElement('a');
      a.href=url;
      return a.hostname;
    }
    mixpanel.people.set_once( "referring", parseURL(document.referrer) );
    jQuery.cookie('referring',parseURL(document.referrer),{domain: '.genomecompiler.com'});
  }

    if (utm_source)
    {
      mixpanel.people.set_once("utm_source", utm_source);
      if (utm_medium) { mixpanel.people.set_once("utm_medium", utm_medium); }
      if (utm_term) { mixpanel.people.set_once("utm_term", utm_term); }
      if (utm_content) { mixpanel.people.set_once("utm_content", utm_content); }
      if(utm_campaign) { mixpanel.people.set_once("utm_campaign", utm_campaign);}
    }
    
    var track_later_inner = function(ele,event_name,button_used,click,section_used) {
        console.log('Link Event Tracked '+event_name);
        ga('send', 'event', 'Design', 'Click', button_used);
        
    var today = new Date();
    var dd = today.getDate();
    var mm = today.getMonth()+1; //January is 0!
    var yyyy = today.getFullYear();
    mixpanel.people.set("Visit date",yyyy + "-" + mm + "-" + dd );
    
    date = new Date;
                if (utm_source)
        {
          string = JSON.stringify(
            {
            event_name: event_name,
      section_used: section_used,
            button_used: button_used,
            utm_s: utm_source,
            utm_m: utm_medium,
            utm_t: utm_term,
            utm_co: utm_content,
            utm_ca: utm_campaign,
            time: date.toISOString(),
            vd: yyyy + "-" + mm + "-" + dd
          });
        }
        else
        {
          string = JSON.stringify(
            {
            event_name: event_name,
      section_used: section_used,
            button_used: button_used,
            time: date.toISOString(),
            vd: yyyy + "-" + mm + "-" + dd
          });
        }
        var thirtyMinutesLater = new Date();
        thirtyMinutesLater.setMinutes(thirtyMinutesLater.getMinutes() + 30);
        jQuery.cookie('designer_click_event',string, { domain: '.genomecompiler.com', path: '/', expires: thirtyMinutesLater });
        console.log('Cookie saved for use by server later!');
        if (click == true)
        {
            window.location = jQuery(ele).prop('href');
        }
     }        

    var track_later = function(ele,event_name,event_type,click,section_used)
    {

        click = typeof click !== 'undefined' ? click : true;
    section_used = typeof section_used !== 'undefined' ? section_used : "Default";


        jQuery('body').on('click', ele, function(e) 
        {
            track_later_inner(ele,event_name,event_type,click,section_used);
        }
      );
    }

