.rect {
    width: 100%;
}
.rect:before {
    content: "";
    float: left;
    padding-bottom: 111%;
    vertical-align: middle;
}
.rect:after {
    content: "";
    display: table;
    clear: both;
}
@media (max-width: 620px) {
    .rect:before {
        padding-bottom: 40%;
    }
}
.allwhitetext,
.allwhitetext * {
    color: white !important;
}
p.zoom-notice {
    display: none;
}

.skinnybanner {
    width: 100%;
    padding: 10px;
    margin-bottom: 5px;
    color: white;
    text-align: center;
    font-weight: bold;
    display: block;
}
.skinnybanner:hover {
    color: white;
    opacity: 0.9;
    cursor: pointer;
}
.ticket {
    text-align: center;
    vertical-align: middle;
    font: 14px 'Futura', sans-serif;
    line-height: 20px;
}
.ticket > div {
    display: inline-block;
    vertical-align: middle;
}
.ticket .discount {
    font-size: 90px;
    line-height: 80px;
    font-weight: bolder;
    display: inline-block;
    font-family: 'Futura';
    position: relative;
    z-index: 100;
    margin-top: 30px;
}
.ticket .discount:after {
    content: "% OFF";
    font-size: 40px;
    line-height: 39px;
    text-align: left;
    width: 80px;
    font-weight: bolder;
    padding-left: 3px;
    display: inline-block;
}
.ticket .discount.upto:before {
    content: "UP TO";
    font-size: 16px;
    line-height: 29px;
    text-align: right;
    width: 50px;
    font-weight: bolder;
    padding-left: 3px;
    position: absolute;
    bottom: 100%;
    right: 80%;
}
.ticket .price {
    font-size: 90px;
    line-height: 80px;
    font-weight: bolder;
    display: inline-block;
    font-family: 'Futura';
    position: relative;
