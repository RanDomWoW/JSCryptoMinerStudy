		if (!!li.extra)
			var sValue = li.extra[0];
		else
			var sValue = li.selectValue;
		document.getElementById("tramite-id").value = sValue;
		document.getElementById("tram-sub").value = '';

		// Una vez elegido el lugar hacemos foco en el 
		// mismo campo para que el usuario pueda enviar 
		// el formulario
		document.getElementById("tram-busc").focus();
		return false;
	}

	function findValue_dir(li) {
		if (li == null)
			return alert("No se encontraron lugares con ese nombre");
		if (!!li.extra)
			var sValue = li.extra[0];
		else
			var sValue = li.selectValue;
		document.getElementById("directorio-id").value = sValue;
		document.getElementById("directorio-cat").value = '';
		document.getElementById("directorio-sub").value = '';

		// Una vez elegido el lugar hacemos foco en el 
		// mismo campo para que el usuario pueda enviar 
		// el formulario
		document.getElementById("directorio-buscar").focus();
		return false;
	}

	function selectItem(li) {
		findValue(li);
	}

	function selectItem_dir(li) {
		findValue_dir(li);
	}

	function formatItem(row) {
		return row[0];
	}

