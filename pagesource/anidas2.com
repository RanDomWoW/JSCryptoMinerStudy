    function show_menu_m(element_id, left, mark){
        $(element_id).animate({
            left: left
        }, 400, function(){
            if(mark==true)
                $('.line_right').show();
        });
        $(element_id).css({'display':'block'});
    }
    
    function hidden_menu_m(element_id, left, mark){
        $(element_id).animate({
            left: left
        }, 400, function(){
