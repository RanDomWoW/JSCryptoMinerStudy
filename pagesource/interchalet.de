		}
	}
	

	window.initVisitedObjects=function(){

		var resArry=icScope.visited.shwLstAjx('de');
		var vOut='';
		var iVisitedItemsMax=36;
		
			var vMin=6;
		

		if(resArry.length>0){
			jQuery.each(resArry, function(intIndex,objVal){
				objct=objVal[1];
				if(typeof objVal[7]!='undefined'){
					objctImg='//images.interchalet.de/objimg/a280/'+objVal[7].replace('a64','a280');
				}
				else{
					objctImg='';
				}
				vOut+='<li>';
				vOut+='<img id="angebote" width="130" height="85" src="'+ objctImg +'" />';
				vOut+='<div class="bg_title_teaser"> </div>';
				vOut+='<h4>'+ objVal[5] +'</h4>';
				vOut+='<span class="object-code">'+ objct +'</span>';
				vOut+='<p>ab &euro; '+ objVal[6] +'</p>';
			
				vOut+='<a title="'+ objct +'" onclick="icScope.ga.trackEvent(\'VisitedObjects_Slider\',\'click\',\''+ objct +'\');WhileSearch();" href="/detail.cfm?object_code='+ objct +'&amp;bquick=true'+ objct +'"></a>';
			
				vOut+='</li>';
			})
			if(resArry.length<vMin){
				for(var i=(resArry.length+1);i<=vMin;i++){
					vOut+='<li></li>';
				}
			}
			else{
				vDiff=Math.abs(iVisitedItemsMax-resArry.length)%vMin;
				for(var i=1;i<=vDiff;i++){
					vOut+='<li></li>';
				}
			}
		}
		else{
			for(var i=1;i<=vMin;i++){
				vOut+='<li></li>';
			}
		}
		jQuery('#VisitedItems').append(vOut);
		jQuery('#visited_list_carousel').show();
		
			jQuery('#VisitedItems').carouFredSel({
				auto	:false,
				items	:6,
				pagination:'#VisitedItems_Pager',
				next	:'#visited_items_next',
				prev	:'#visited_items_back',
				scroll	:{items:6,duration:500}
			});
		
	}

	if(typeof initVisitedObjects=='function' && typeof jQuery.cookie=='function'){
		if(window.JSON && ('function'===typeof window.JSON.parse)){
			jQuery(document).ready(initVisitedObjects);
		}
		else{
			jQuery('#recently_visited').hide();
		}
	}


