	function compare_date(d1, d2)
	{
		if(d1.datum.getTime() < d2.datum.getTime())
		{
			return -1;
		}
		if(d1.datum.getTime() > d2.datum.getTime())
		{
			return 1;
		}
		return 0;
	}
	
	
