	jQuery(document).ready(function(){
		var NLSR={
			'email':function(){
				if(jQuery('#nlsrEI').length==0){jQuery('#search_newsletter_msg').append('<p id="nlsrEI" style="display:none;"></p>');}

				var nlsrEI=jQuery('#nlsrEI');
				var patt=/^.+@.+[.].{2,}$/i;

				if(!patt.test(jQuery('#nlsr_clientEmail_f').val()) || jQuery('#nlsr_clientEmail_f').val()=="E\x2DMail\x2DAdresse"){
					NLSR.errors=true;
					jQuery('#nlsr_clientEmail').addClass('nlsrError');
					nlsrEI.html('Bitte\x20\xFCberpr\xFCfen\x20Sie\x20Ihre\x20E\x2DMail\x2DAdresse.').show();
				}else{
					jQuery('#nlsr_clientEmail').removeClass('nlsrError');
					nlsrEI.hide();
				}
			},
			'newsletter':function(){
				if(!(jQuery('#nlsr_sommer').attr('checked')) && !(jQuery('#nlsr_winter').attr('checked'))){
					jQuery('#nlsr_sommer').prop('checked', true);
					jQuery('#nlsr_winter').prop('checked', true);
				}
			},
			'data':function(){
				if(jQuery('#nlsrDI').length==0){jQuery('#search_newsletter_msg').append('<p id="nlsrDI" style="display:none;"></p>');}

				var nlsrDI=jQuery('#nlsrDI');
				var dataError=false;

				if(dataError){
					NLSR.errors=true;
					nlsrDI.html('Bitte\x20\xFCberpr\xFCfen\x20Sie\x20Ihre\x20Angaben.').show();
				}else{nlsrDI.hide();}
			},
			'sendIt':function(){
				if(!NLSR.errors){
					var nlsr_sommer=(jQuery('#nlsr_sommer').attr('checked')) ? "1" : "";
					var nlsr_winter=(jQuery('#nlsr_winter').attr('checked')) ? "1" : "";

					var nslr_gender="";
					if(jQuery('#nlsr_frau').attr('checked')){
						nslr_gender="F";
					}
					if(jQuery('#nlsr_herr').attr('checked')){
						nslr_gender="M";
					}

					jQuery('#search_newsletter_msg').hide();
					jQuery.ajax({
						type:'POST',
						url:'/w2/newsletter-kunden_rpc.cfm',
						data:{	f:jQuery('#nlsr_f').val(),
								language:jQuery('#nlsr_sprache').val(),
								salutation:nslr_gender,
								firstName:jQuery('#nlsr_firstName_f').val(),
								lastName:jQuery('#nlsr_lastName_f').val(),
								clientEmail:jQuery('#nlsr_clientEmail_f').val(),
								datenquelle:jQuery('#nlsr_datenquelle').val(),
								sommer:nlsr_sommer,
								winter:nlsr_winter,
								anmeldung:jQuery('#nlsr_anmeldung').val()
						},
						success:function(r){
							if(r=='201 Created'){
								jQuery('#search_newsletter_form').hide();
								jQuery('#search_newsletter_danke').show();
								setTimeout("NLSRReset()",8000);
							}else{alert("Die\x20Anmeldung\x20ist\x20fehlgeschlagen\x21");}
						}
					});
				}
			}
		};
		jQuery('#send').click(function (){
			NLSR.errors=false;
			NLSR.data();
			NLSR.email();
			NLSR.newsletter();
			if(!NLSR.errors){
				jQuery('#search_newsletter_msg').hide();
				NLSR.sendIt();
			}else{jQuery('#search_newsletter_msg').show();}
			return false;
		});
	});
	function NLSRReset(){
		jQuery('#search_newsletter_danke').hide();
		jQuery('#search_newsletter_form').hide();
		jQuery('#search_newsletter_normal').show();
		jQuery('#nlsr_frau').attr('checked',false);
		jQuery('#nlsr_frau').attr('checked',false);
		jQuery('#nlsr_firstName_f').val("Vorname");
		jQuery('#nlsr_lastName_f').val("Nachname");
		jQuery('#nlsr_clientEmail_f').val("E-Mail-Adresse*");
		jQuery('#nlsr_sommer').attr('checked',false);
		jQuery('#nlsr_winter').attr('checked',false);
	}
