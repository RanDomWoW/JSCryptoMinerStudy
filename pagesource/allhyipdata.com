<script>
    function loadmore() {
        $(".loadmore").hide();
        $(".loader").show();

        var offset = $(".search-result").length;

        $.getJSON("/search/popular/" + offset)
                .done(function(data) {
                    if (data.length == 0) {
                        $(".loader").hide();
                        $(".nomore").show();
                    } else {
                        for (var i=0; i<data.length; i++) {
                            addHYIP(data[i]);
                        }
                        $(".loader").hide();
                        $(".loadmore").show();
                    }
                })
                .fail( function(d, textStatus, error) {
                    console.error("getJSON failed, status: " + textStatus + ", error: "+error)
                });
    }

    function addHYIP(data) {
        var last = $(".search-result").last();
        var text = getHyipOverviewHtmlCustom(data);
        last.after(text);
    }

    function getHyipOverviewHtmlCustom(hyipData) {
        var text = '<a href="/hyip/' + hyipData.url + '" class="nostyle">';
