<script type="text/javascript">

    $(document).ready(function() {
        $(window).keydown(function(event){
            if((event.keyCode== 13) && ($(event.target)[0]!=$("textarea")[0])) {
                event.preventDefault();
                return false;
            }
        });

        $(".search-result").click(function() {
            clickedSearchResult(this);
        });
    });

    var oldquery = "";
    var nRequests = 0;

    $("#searchbar").keyup(function() {
        var query = $("#searchbar").val();
        if (query.length <= 1) {
            olquery = "";
            hideSearchResults();
            return;
        }
        if (query == oldquery) { return; }
        showSearchResults();
        showLoader();

        nRequests++;
        var myRequest = nRequests;
        setTimeout(function(){
            if (nRequests == myRequest) {
                $.getJSON("/search/" + query)
                        .done(function(data) {
                            if (nRequests != myRequest) {
                                return;
                            }
                            clearSearchResults();
                            if (data.length == 0) {
                                hideLoader();
                                showNoResults();
                            } else {
                                for (var i=0; i<data.length; i++) {
                                    addSearchResult(data[i], (i+1 == data.length));
                                }
                                hideLoader();
                                showSearchResults();
                                $("div.search-result-container").css("height", (100*i));
                            }
                        })
                        .fail( function(d, textStatus, error) {
                            console.error("getJSON failed, status: " + textStatus + ", error: "+error)
                        });
            }
        }, 200);

        oldquery = query;
    });

    function clearSearchResults() {
        $(".search-result-container .search-result").remove();
        $(".no-results").hide();
    }
    function showNoResults() {
        $(".no-results").show();
    }

    function hideSearchResults() {
        clearSearchResults();
        $(".search-result-container").fadeOut();
        $(".search-result-container").css("height", 0);
    }
    function showSearchResults() {
        $(".search-result-container").fadeIn();
    }
    function showLoader() {
        $(".loader").show();
        $(".search-result-container").css("height", 60);
    }
    function hideLoader() {
        $(".loader").hide();
    }

    function addSearchResult(result, last) {
        var div = getHyipOverviewHtmlSearch(result);
        $(".search-result-container").append(div);
        $(".search-result").click(function() {
            clickedSearchResult(this);
        });
    }

    function getHyipOverviewHtmlSearch(hyipData) {
        var text = '<a href="/hyip/' + hyipData.url + '" class="nostyle" onclick="clickedSearchResult(\'' + hyipData.url + '\')">';
        text += '<div class="search-result col-xs-12"';



        text += '>';

        if (hyipData.image == "") {
            text += '<div class="img img-icon"><i class="fa fa-question" aria-hidden="true"></i></div>';
        } else {
            text += '<div class="img"><img class="hyip-icon" src="/images/icons/' + hyipData.image + '.png"></div>';
        }

        text += '<div class="content"><h3>' + stringLimit(hyipData.name, 22) + '</h3>';
        text += '<p>' + stringLimit(hyipData.payouts, 35) + '</p>';
        text += '<p class="url">' + stringLimit(hyipData.url, 35) + "</p></div>";
        text += '<div class="icons"><div class="icon icon-time"><i class="fa fa-clock-o" aria-hidden="true"></i>';
        if (hyipData.onlineSince == 0) {
            text += 'Today';
        } else if (hyipData.onlineSince == 1) {
            text += 'Yesterday';
        } else {
            text += hyipData.onlineSince + " " + 'days';
        }
        text += '</div>';
        if (hyipData.status == "Paying") {
            text += '<div class="icon icon-status success"><i class="fa fa-check" aria-hidden="true"></i>Paying</div>';
        } else if (hyipData.status == "Waiting") {
            text += '<div class="icon icon-status waiting"><i class="fa fa-question" aria-hidden="true"></i>Waiting</div>';
        } else {
            text += '<div class="icon icon-status not-paying not-paid"><i class="fa fa-times" aria-hidden="true"></i>Not Paying</div>';
        }
        text += '</div></div></a>';
        return text;
    }

    function stringLimit(str, max) {
        if (str.length > max) {
            return str.substring(0, max) + "...";
        }
        return str;
    }

    function clickedSearchResult(url) {
        $.getJSON("/search/clicked/" + url);
    }

</script>
