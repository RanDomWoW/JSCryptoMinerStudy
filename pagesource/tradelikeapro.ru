<script>
		jQuery(document).ready(function($){
		if (document.getElementById('button_1'))
			document.getElementById('button_1').addEventListener("click", function() {
			sortTable(rating_1,range_drawdown_1,range_profit_1,range_existing_1);
			})  
			
		if (document.getElementById('button_2'))
			document.getElementById('button_2').addEventListener("click", function() {
			sortTable(rating_2,range_drawdown_2,range_profit_2,range_existing_2);
			})  
			
		if (document.getElementById('button_3'))
			document.getElementById('button_3').addEventListener("click", function() {
			sortTable(rating_3,range_drawdown_3,range_profit_3,range_existing_3);
			})  
			
		if (document.getElementById('button_4'))
			document.getElementById('button_4').addEventListener("click", function() {
			sortTable(rating_4,range_drawdown_4,range_profit_4,range_existing_4);
			})  
});

    function sortTable(table, drawdown, profit, existing) {
        var rows, switching, i, x1, x2, x3, y1, y2, y3, shouldSwitch;
        var c1 = drawdown.value;
        var c2 = profit.value;
        var c3 = existing.value;
        switching = true;
        var max1 = 0,
            max2 = 0,
            max3 = 0;
        rows = table.getElementsByTagName("TR");
        for (i = 1; i < (rows.length - 1); i++) {
            x1 = rows[i].getElementsByTagName("TD")[1].innerText;
            x2 = parseFloat(rows[i].getElementsByTagName("TD")[2].innerText) + 100;
            x3 = rows[i].getElementsByTagName("TD")[3].innerText;
            if (x1 > max1) max1 = x1;
            if (x2 > max2) max2 = x2;
            if (x3 > max3) max3 = x3
        }
        while (switching) {
            switching = false;
            rows = table.getElementsByTagName("TR");
            for (i = 1; i < (rows.length - 1); i++) {
                shouldSwitch = false;
                x1 = rows[i].getElementsByTagName("TD")[1].innerText;
                x2 = parseFloat(rows[i].getElementsByTagName("TD")[2].innerText) + 100;
                x3 = rows[i].getElementsByTagName("TD")[3].innerText;
                y1 = rows[i + 1].getElementsByTagName("TD")[1].innerText;
                y2 = parseFloat(rows[i + 1].getElementsByTagName("TD")[2].innerText) + 100;
                y3 = rows[i + 1].getElementsByTagName("TD")[3].innerText;
                if (((x1 / max1) * c1 + (1 / (x2 / max2)) * (c2) + (1 / (x3 / max3)) * (c3)) > ((y1 / max1) * c1 + (1 / (y2 / max2)) * (c2) + (1 / (y3 / max3)) * (c3))) {
                    shouldSwitch = true;
                    break
                }
            }
            if (shouldSwitch) {
                rows[i].parentNode.insertBefore(rows[i + 1], rows[i]);
                switching = true
            }
        }
    }
</script>

