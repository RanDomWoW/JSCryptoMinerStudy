$(function(){
	$('.flat-slider-true').after(function(){
		return $('.flat-slider-true').clone().switchClass('flat-slider-true','flat-slider-true_over');
	});
	$('.flat-slider-true').slick({
		slidesToShow: 1,
		slidesToScroll: 1,
		arrows: false,
		fade: true,
		asNavFor: '.slider-nav'
	});
	$('.flat-slider-true_over').slick({
		slidesToShow: 3,
