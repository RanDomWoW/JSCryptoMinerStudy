      function InitCatalogs()
      {
        $('.material-card > .mc-btn-action, img.big-image').unbind();
        $('.material-card > .mc-btn-action, img.big-image').click(function () {
            var card = (($(this).parent('.material-card').length == 0) ? $(this).closest(".material-card") : $(this).parent('.material-card'));
            var icon = (($(this).children('i').length==0) ? $(this).closest(".material-card").find("i.fa-info-circle") : $(this).children('i'));
            var date = (($(this).parent('.material-card').find('.catalog-aditional-info').length == 0) ? $(this).closest(".material-card").find(".catalog-aditional-info") : $(this).parent('.material-card').find('.catalog-aditional-info'));
            icon.addClass('fa-spin-fast');

            if (card.hasClass('mc-active')) {
                card.removeClass('mc-active');
                date.show();
                window.setTimeout(function() {
                    icon
                        .removeClass('fa-arrow-left')
                        .removeClass('fa-spin-fast');

                }, 800);
            } else {
                card.addClass('mc-active');
                date.hide();
                window.setTimeout(function() {
                    icon
                        .removeClass('fa-spin-fast')
                        .addClass('fa-arrow-left');

                }, 800);
            }
        });
      }

      $(function() {
        InitCatalogs();
    });
      
