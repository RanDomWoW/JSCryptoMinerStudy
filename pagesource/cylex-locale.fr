      var moreText = 'plus...';
      var lessText = 'moins...';

      function showMoreLess(el) {
        var moreLess = el.data('more-less');
        if (moreLess) {
          var countItems = el.children().length;
          if (countItems > moreLess) {
            el.children().slice(moreLess, el.children().length - 1).hide();
            var moreLink = el.children().find('a.truncate_more_link');
            moreLink.click(function(e) {
              var parentList = $(this).parent().parent();
              var limit = parentList.data("more-less");
              var prevItem = $(this).parent().prev();
              if (prevItem.is(':visible')) {
                parentList.children().slice(limit, parentList.children().length - 1).hide();
                $(this).text(moreText);
                if ($(this).hasClass('truncate_less_link')) {
                  $(this).attr('class', 'truncate_more_link');
                }
              } else {
                parentList.children().slice(limit, parentList.children().length - 1).show();
                $(this).text(lessText);
                if ($(this).hasClass('truncate_more_link')) {
                  $(this).attr('class', 'truncate_less_link');
                }
              }
              return false;
            });
          }
        }
      };
      $(document).ready(function() {
        $('.data-more-less').each(function(index){
          showMoreLess($(this));
        });
      });
      
    </script><script type="text/javascript">
      
      function switchHeaderImage(type) {
        switch(type) {
          case 'business':
            $('header').addClass('back-bussiness-img');
            $('header').removeClass('back-doctors-img');
            $('header').removeClass('back-lawyers-img');
            $('header').removeClass('back-petrolstation-img');
          break;
          case 'doctors':
            $('header').removeClass('back-bussiness-img');
            $('header').addClass('back-doctors-img');
            $('header').removeClass('back-lawyers-img');
            $('header').removeClass('back-petrolstation-img');
          break;
          case 'lawyers':
            $('header').removeClass('back-bussiness-img');
            $('header').removeClass('back-doctors-img');
            $('header').removeClass('back-petrolstation-img');
            $('header').addClass('back-lawyers-img');
          break;
          case 'petrolstation':
            $('header').removeClass('back-bussiness-img');
            $('header').removeClass('back-doctors-img');
            $('header').removeClass('back-lawyers-img');
            $('header').addClass('back-petrolstation-img');
          break;          
        }
      }
      
      function switchHeaderClaim(type) {
        switch(type) {
          case 'business':
            $('#directoryHeading').changeElementType('h1');
            $('#directoryHeading').attr('class', 'hugeh-heading');
            $('#doctorsHeading').changeElementType('p');
            $('#doctorsHeading').attr('class', 'h1 hidden');
            $('#lawyersHeading').changeElementType('p');
            $('#lawyersHeading').attr('class', 'h1 hidden');
            $('#petrolstationHeading').changeElementType('p');
            $('#petrolstationHeading').attr('class', 'h1 hidden');
            $("#search-what").css("display","block");
          break;
          case 'doctors':
            $('#doctorsHeading').changeElementType('h1');
            $('#doctorsHeading').attr('class', 'hugeh-heading');
            $('#directoryHeading').changeElementType('p');
            $('#directoryHeading').attr('class', 'h1 hidden');
            $('#lawyersHeading').changeElementType('p');
            $('#lawyersHeading').attr('class','h1 hidden');
            $('#petrolstationHeading').changeElementType('p');
            $('#petrolstationHeading').attr('class', 'h1 hidden');
            $("#search-what").css("display","block");
          break;
          case 'lawyers':
            $('#lawyersHeading').changeElementType('h1');
            $('#lawyersHeading').attr('class', 'hugeh-heading');
            $('#doctorsHeading').changeElementType('p');
            $('#doctorsHeading').attr('class', 'h1 hidden');
            $('#directoryHeading').changeElementType('p');
            $('#directoryHeading').attr('class', 'h1 hidden');
            $('#petrolstationHeading').changeElementType('p');
            $('#petrolstationHeading').attr('class', 'h1 hidden');  
            $("#search-what").css("display","block")
          break;
          case 'petrolstation':
            $('#petrolstationHeading').changeElementType('h1');
            $('#petrolstationHeading').attr('class', 'hugeh-heading');
            $('#directoryHeading').changeElementType('p');
            $('#directoryHeading').attr('class', 'h1 hidden');            
            $('#doctorsHeading').changeElementType('p');
            $('#doctorsHeading').attr('class', 'h1 hidden');
            $('#lawyersHeading').changeElementType('p');
            $('#lawyersHeading').attr('class', 'h1 hidden');
            $("#search-what").css("display","none");
          break;          
        }      
      }
      
