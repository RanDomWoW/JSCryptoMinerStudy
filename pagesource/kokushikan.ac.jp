	$('.mainslide_control.prev').on('click', function () {
		mainslide.slickPrev();
	});
});
</script>
<script>
		$('.tab_control li a').click(function () {
				setTimeout(function(){
						$(".heightLine").heightLine("refresh");
				},100);
		});
</script>
