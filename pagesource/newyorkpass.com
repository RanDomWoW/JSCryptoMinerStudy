<script type="text/javascript" src="/js/classie.js"></script>
<script type="text/javascript">
 function init() {
  window.addEventListener('scroll', function(e){
   var distanceY = window.pageYOffset || document.documentElement.scrollTop,
    shrinkOn = 1,
    rp_header = document.querySelector('#header_new_fixed'),
    rp_content = document.querySelector('#base_new_fixed');
   if (distanceY > shrinkOn) {
    classie.add(rp_header,'smaller'),
    classie.add(rp_content,'smaller');
   }
   else {
    if (classie.has(rp_header,'smaller')) {
     classie.remove(rp_header,'smaller');
    }
    if (classie.has(rp_content,'smaller')) {
     classie.remove(rp_content,'smaller');
    }
   }
  });
 }
 window.onload = init();
</script>


