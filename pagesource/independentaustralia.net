/*
Tiny Slider - removing the comments below and white space makes this a really small file (428bytes)
*/

function tnySldr(direction) {
  // find width of each list element DS = multipler size
  ds = $('.homeArticlesLatest').width()+parseInt($('.homeArticlesLatest').css('marginRight'));

  // what slide are we currently on SV = slide value
  sv = $.data(document.body, 'cs');

  // total number of slides before reset to slide 1 NS = number slide
  ns = $('.homeArticlesLatest').length;

  if (sv == ns) {
    // set cs back to 1 and start again
    $.data(document.body, 'cs', 1);

    // really simple animation
    $('.homeInnerLatest').animate({
      left : 0
    }, 250, 'swing');


  } else {
    // set cs back to 1 and start again
    $.data(document.body, 'cs', (sv + 1));

    // really simple animation
    if( direction == 'right'){
    	if( sv != ns ){
    		$.data(document.body, 'cs', (sv + 1));
		}
    	$('.homeInnerLatest').animate({
      		left : '-' + (ds * sv)
    	}, 750, 'swing');
    }else{
    	if( sv != 1 ){
	    	$('.homeInnerLatest').animate({
	      		left : '-' + (ds * (sv-1)-ds)
	    	}, 750, 'swing');
			if( sv != 1 ){
	    		$.data(document.body, 'cs', (sv - 1));
			}
		}
    }

  }

}

$(function() {
  // initial slide number is naturally #1
  $.data(document.body, 'cs', 1);

	$('.controlLeft').on('click', function(e){
		e.preventDefault();
		tnySldr('left');
	});
	$('.controlRight').on('click', function(e){
		e.preventDefault();
		tnySldr('right');
	});

});



