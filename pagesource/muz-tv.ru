<script>
jsLoader.require('domready', 'jquery', function() {
	var $block_onair_switcher = $('.x-block_onair_switcher'),
		$block_onair_switcher_parent = $block_onair_switcher.parent(),
	    $block_onair_container = $('.x-block_onair_container');
	$block_onair_switcher.click(function() {
		var $this = $(this),
			$container = $('#day_of_week_' + $this.attr('data-container_id'));
		$block_onair_switcher_parent.removeClass('p-active');
		$block_onair_container.hide();
		$this.parent().addClass('p-active');
		$container.show();
		if(!$this.attr('data-initialized')) {
			$.post('/ajax/?f=get_schedule', {date: $this.attr('data-date')}, function(rsp) {
				$this.attr('data-initialized', 1);
				$container.html(rsp);
				$container.jScrollPane();
			});
		}
		return false;
	});
	jsLoader.require('jquery.mousewheel', 'jquery.jscrollpane', function() {
		$('.x-scroll_pane').filter(':visible').jScrollPane();
	});
	// blink video play button
	var $onair_video_play = $('#onair_video_play');
	var blink = function() {
		if($onair_video_play.css('opacity')-0 == 0) {
			$onair_video_play.fadeTo('slow', 1); // hide
		} else {
			$onair_video_play.fadeTo('slow', 0); // show
		}
		setTimeout(blink, 1000);
	}
	blink();
});
</script>
