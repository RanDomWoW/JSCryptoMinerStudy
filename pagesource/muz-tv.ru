	<script>
	jsLoader.require('domready', 'jquery', function() {
		var i = 0,
			timer,
			mouse_on_viewer = false,
			default_timeout = 6000,
			$container = $('#main_promo'),
			$slider = $('#main_promo_slider'),
			$slides = $slider.find('li'),
			$hrefs = $('.x-main_promo_url'),
			$switchers = $container.find('.x-switch');
		function change_anons(timeout) {
			$slider.animate(
				{left: 0 - i * 1060}, // width of each image
				'slow',
				function() {
					$switchers.parent().removeClass('p-active').eq(i).addClass('p-active');
					var $slide = $slides.eq(i),
						pixel = $slide.attr('data-stat_pixel');
					if(pixel) {
						$slide.attr('data-stat_pixel', ''); // clear pixel data
						$(pixel).css({position: 'absolute', left: '-1000px'}).appendTo(document.body);
					}
					if(++i >= 2) {
						i = 0;
					}
				}
			);

				timeout = timeout ? timeout : default_timeout;
				if(timer) {
					clearTimeout(timer); // for Chrome
				}
				if(!mouse_on_viewer) {
					timer = setTimeout(function() {change_anons()}, timeout);
				}

		}
		// force change anons on click
		$switchers.click(function() {
			if(timer) {
				clearTimeout(timer);
			}
			i = parseInt($(this).attr('data-idx'));
			change_anons(2 * default_timeout);
			return false;
		});
		// replace href by count one
		$hrefs.mousedown(function() {
			var $this = $(this),
				countUrl = $this.attr('data-count_url');

			if(!countUrl) {
				return false;
			}

			$this.attr('href', countUrl);
		});
		$container.mouseenter(function() {
			if(timer) {
				clearTimeout(timer);
			}
			mouse_on_viewer = true;
		}).mouseleave(function() {
			if(timer) {
				clearTimeout(timer); // for Chrome
			}
			mouse_on_viewer = false;
			timer = setTimeout(function() {change_anons()}, default_timeout);
		});

			// start rotation on winload
			jsLoader.winload(function() {
				change_anons();
			});

	});
	</script>

