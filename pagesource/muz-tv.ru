<script>
jsLoader.require('domready', 'jquery', function() {
	var $tabs = $('.x-tab');
	var $tabs_content = $('.x-video_tab');
	var $muztv_clipyou_video = $('#muztv_clipyou_video');

	// tabs
	$tabs.click(function() {
		var $this = $(this);
		$tabs.parent().removeClass('p-active');
		$tabs_content.hide();
		$this.parent().addClass('p-active');
		$('.' + $this.attr('rel')).show().find('img:lt(4)').each(function() { // show tab content and set src for the first 4 images
			var $self = $(this);
			if($self.attr('data-src')) {
				$self.attr('src', $self.attr('data-src'));
			}
		});
		return false;
	});

	// OMG, they want tabs auto-change
	var mouse_on_tabcontent = 0;
	$muztv_clipyou_video.mouseenter(function() {
		mouse_on_tabcontent = 1;
	}).mouseleave(function() {
		mouse_on_tabcontent = 0;
	});
	setInterval(function() {
		if(mouse_on_tabcontent == 0) {
			var current_idx = 0;
			$tabs.each(function(idx, elm) {
				if($(this).parent().hasClass('p-active')) {
					current_idx = idx;
				}
			});
			++current_idx;
			if(current_idx == $tabs.length) {
				current_idx = 0;
			}
			$tabs.eq(current_idx).click(); // change tab by simulate click on it
		}
	}, 7000);
	// scroll
	for(var i=1; i<=$tabs.length; i++) {
		(function(i) {
			var $hscroll_buttons = $muztv_clipyou_video.find('.x-arrow_left_'+i+', .x-arrow_right_'+i);
			$hscroll_buttons.click(function() {
				var $this = $(this);
				$hscroll_buttons.unbind('click', arguments.callee);
				jsLoader.require('jquery.hscroll', function() {
					$muztv_clipyou_video.hscroll({
						item_class: 'x-item'+i,
						b_left_class: 'x-arrow_left_'+i,
						b_right_class: 'x-arrow_right_'+i,
						b_left_class_disabled: 'p-end',
						b_right_class_disabled: 'p-end'
					});
					$this.click(); // repeat click after hscroll init
				});
				return false;
			});
		})(i);
	}
	var re = /^(\/|https?:\/\/muz-tv.ru)/;
	$muztv_clipyou_video.find('a').each(function() {
		if(!re.test(this.href)) {
			this.target = '_blank'; // open clipyou links in new window
		}
	});

	// activate first tab
	$tabs.first().click();
});
</script>

