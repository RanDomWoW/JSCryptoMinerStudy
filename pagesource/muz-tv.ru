		$this.attr('data-url', popupUrl).addClass('x-popup');

		$this.click(); // повтор клика после инициализации

		return false;
	});

	// попапы
	$document.delegate('.x-popup', 'click.popupsInit', function(e) {
		var $this = $(this);
		var $popup_window = $('#popup_window');

		$document.undelegate('click.popupsInit');

		$document.delegate('.x-popup', 'click', function(e) {
			$popup_window.jqmShow(this);
		});

		jsLoader.require('jquery.jqmodal', function() {
			$popup_window.jqm({
				onShow: function(h) {
					h.w.html('Загрузка...').show().load($(h.t).attr('data-url'), function() {
						h.w.css({display: 'table'}); // должно быть display:table
