<script>
window.p = {}; // 'p' is a common namespace

window.p.serverTime = parseInt('1521364522')*1000;
window.p.onlineDelay = 30*1000; // задержка онлайн-трансляции, связанная с буферизацией и кодированием
window.p.diffServerTime = new Date().getTime() - window.p.serverTime;
window.p.getServerTime = function () {
	return new Date().getTime() - window.p.diffServerTime;
};

window.p.declension = function(n, w1, w2, w3) { // число => минута, минуты, минут
	var d = n % 10;
	var d2 = n % 100;
	if(d == 0 || (d > 4 && d <= 9) || (d2 > 10 && d2 < 15)){
		return w3;
	}else if(d == 1){
		return w1;
	}else{
		return w2;
	}
};
window.p.zeropad = function(n) {
	if(n < 10) {
		n = '0' + n;
	}
	return n;
};
window.p.getCookie = function(name) {
	var prefix = name + "=";
	var cookieStartIndex = document.cookie.indexOf(prefix);
	if (cookieStartIndex == -1) return null;
	var cookieEndIndex = document.cookie.indexOf(";", cookieStartIndex + prefix.length);
	if (cookieEndIndex == -1) cookieEndIndex = document.cookie.length;
	return unescape(document.cookie.substring(cookieStartIndex + prefix.length, cookieEndIndex));
}
window.p.setCookie = function(name, value, path, domain, expire) {
	if (path) {
		path = "; path=" + path;
	} else {
		path = "; path=/";
	}
	domain = "; domain=" + domain;
	var expiresDate = new Date();
	expiresDate.setTime(expiresDate.getTime() + (expire * 24*60*60*1000));
	expire = "; expires=" + expiresDate.toGMTString();
	document.cookie = name + "=" + escape(value) + path + domain + expire + ";";
}
window.p.url_plus = function(add, url, hash) {
	var args = {};
	if(!add) {
		add = '';
	}
	if(!url) {
		url = window.location.toString();
	}
	url = url.replace(/#.+/, ''); // remove hash from url

	var parts = url.split('?');
	var res = parts[0] + '?';
	var a_args = (parts[1] || '&').split('&');
	for(var p in a_args) {
		var a = a_args[p].split('=');
		if(a[0] && a[1]) {
			args[a[0]] = a[1];
		}
	}
	a_args = add.split('&');
	for(var p in a_args) {
		var a = a_args[p].split('=');
		if(a[0] && a[1]) {
			args[a[0]] = a[1];
		}
	}
	for(var p in args) {
		res += p + '=' + args[p] + '&';
	}
	res = res.slice(0, -1);
	if(hash) {
		res += '#' + hash;
	}
	return res;
}

/*
Трекер показов.
*/
var ImpressionTracker = function(config, cookieName) {
	this.config = config;
	this.cookieName = cookieName;

	for(var unit in this.config) {
		if(!this.config[unit].interval) {
			this.config[unit].interval = 0;
		}
	}

	this.parseTracker(window.p.getCookie(cookieName));
};
ImpressionTracker.prototype.canShow = function() {
	var canShow = true;
	var now = new Date();
	var lastImpression = new Date(this.tracker.last);

	for(var unit in this.config) {
		var last4compare = '';
		var now4compare = '';

		switch(unit) {
			case 'day':
				last4compare = lastImpression.getDate().toString() + lastImpression.getMonth().toString() + lastImpression.getFullYear().toString();
				now4compare = now.getDate().toString() + now.getMonth().toString() + now.getFullYear().toString();
				break;
			case 'week':
				last4compare = this.getWeek(lastImpression).toString() + lastImpression.getFullYear().toString();
				now4compare = this.getWeek(now).toString() + now.getFullYear().toString();
				break;
			default:
				break;
		}

		if(last4compare != now4compare) { // начался новый интервал для единицы измерения
			this.tracker.count[unit] = 0;
		}

		if(now.getTime() - lastImpression.getTime() < this.config[unit].interval) {
			canShow = false;
		}
		if(this.tracker.count[unit] >= this.config[unit].maxImpressions) {
			canShow = false;
		}
	}

	return canShow;
};
ImpressionTracker.prototype.addImpression = function(impressionTimestamp) {
	for(var unit in this.config) {
		++this.tracker.count[unit];
	}
	this.tracker.last = impressionTimestamp;

	window.p.setCookie(this.cookieName, this.stringifyTracker(), '/', 'muz-tv.ru', 365);
};
ImpressionTracker.prototype.stringifyTracker = function() {
	return JSON.stringify(this.tracker);
};
ImpressionTracker.prototype.parseTracker = function(string) {
	this.tracker = {last: 0, count: {}};
	try {
		this.tracker = JSON.parse(string || '');
	} catch(e) {}
	for(var unit in this.config) {
		if(!this.tracker.count[unit]) {
			this.tracker.count[unit] = 0;
		}
	}
};
// http://techblog.procurios.nl/k/news/view/33796/14863/calculate-iso-8601-week-and-year-in-javascript.html
ImpressionTracker.prototype.getWeek = function(date) {
	var target = new Date(date.valueOf());
	var dayNr = (date.getDay() + 6) % 7;

	target.setDate(target.getDate() - dayNr + 3);
	var firstThursday = target.valueOf();

	target.setMonth(0, 1);
	if (target.getDay() != 4) {
		target.setMonth(0, 1 + ((4 - target.getDay()) + 7) % 7);
	}

	return 1 + Math.ceil((firstThursday - target) / 604800000); // 604800000 = 7 * 24 * 3600 * 1000
};

window.p.ImpressionTracker = ImpressionTracker;

