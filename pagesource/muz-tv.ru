	<script>
	jsLoader.require('domready', 'jquery', function() {
		var time_diff = 1521364522000 - (new Date).getTime(),
			$notify_subscribe_popup = $('#notify_subscribe_popup'),
			recall_in = [10,30,60,120,240,720,1440],
			default_recall_in = 240
		;
		function remove() {
			$notify_subscribe_popup.hide();
			$notify_subscribe_popup.closest('.x-notify_container').removeClass('p-email p-sms');
		}
		$(document).click(function(e) {
			remove();
		});
		$notify_subscribe_popup.click(function(e) {
			e.stopPropagation(); // do not close block while clicking on it
		});
		$('body').delegate('.x-notify-button', 'click', function(e) {
			var $this = $(this),
				notify_by = $this.attr('data-notify_by'),
				$notify_container = $this.closest('.x-notify_container'),
				now = time_diff + (new Date).getTime(), // current time, taking into account time_diff
				start_time_ts = parseInt($this.attr('data-start_time_ts')),
				availableRecalls = []
			;

			remove();

			// hide options that should not be selected due to the fact that a reminder for that time can not be done
			for(var i=0; i<recall_in.length; i++) {
			var $option = $notify_subscribe_popup.find('option[value="' + recall_in[i] + '"]');
				if(start_time_ts - 60 * 1000 * recall_in[i] < now) {
					$option.hide().attr({disabled: true, selected: false});
				} else {
					$option.show().attr({disabled: false, selected: false});
					availableRecalls.push(recall_in[i]);
				}
			}

			// select default option
			if(availableRecalls.length) {
				var defaultSelectedRecall = availableRecalls[0];
				if(availableRecalls.indexOf(default_recall_in) !== -1) {
					defaultSelectedRecall = default_recall_in;
				}
				$notify_subscribe_popup.find('option[value="' + defaultSelectedRecall + '"]').show().attr({selected: true});
			} else {
				alert('Мы уже не сможем вам напомнить об этом, т.к. начало совсем скоро!');
				return false;
			}

			// set data for callback
			$notify_subscribe_popup
				.removeData()
				.data($.extend(
					{notify_by: notify_by, callback: $this.attr('data-callback')},
					$.parseJSON($this.attr('data-for-cb'))
				));

			// show apropriate block
			if(notify_by === 'email') {
				$notify_container.addClass('p-email');
			} else { // sms
				$notify_container.addClass('p-sms');
			}

			// show popup
			$notify_subscribe_popup.css('display', 'block').appendTo($notify_container);

			e.stopPropagation();
			return false;
		});
		$notify_subscribe_popup.submit(function(e) {
			var email = $notify_subscribe_popup.find('input[name="email"]').val(),
				phone_code = $notify_subscribe_popup.find('input[name="phone_code"]').val(),
				phone_number = $notify_subscribe_popup.find('input[name="phone_number"]').val(),
				data = $notify_subscribe_popup.data();

			try {
				if(data.notify_by == 'email') {
					if(! email) {
						throw new Error('Введите свой email');
					}
					if(email.search(/[A-Z0-9._%-]+@[A-Z0-9.-]+\.[A-Z]{2,4}/i) == -1) {
						throw new Error('Введите правильный email');
					}
					data.recall_in = $notify_subscribe_popup.find('.x-notify_by.x-email').find('select[name="recall_in"]').val();
					data.email = email;
				} else if(data.notify_by == 'sms') {
					if(! phone_code || phone_code.length != 3 || phone_code.search(/\D/) != -1) {
						throw new Error('Неверный код телефона');
					} 
					if(! phone_number || phone_number.length != 7 || phone_number.search(/\D/) != -1) {
						throw new Error('Неверный номер телефона');
					}
					data.recall_in = $notify_subscribe_popup.find('.x-notify_by.x-sms').find('select[name="recall_in"]').val();
					data.phone_code = phone_code;
					data.phone_number = phone_number;
				} else {
					throw new Error('Неизвестный способ уведомления');
				}
			} catch(e) {
				alert(e.message);
				return false;
			}

			if(!data.callback) {
				return false;
			}
			window[data.callback](data)
				.done(function(msg) {
					remove();
					if(msg) {
						alert(msg);
					}
				})
				.fail(function(msg) {
					alert(msg);
				})
			;

			e.stopImmediatePropagation(); // avoid jScrollPane issue with double submit handlers
			return false;
		});
	});
	</script>

