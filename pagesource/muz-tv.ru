			$('.b-player').muzplayer();
			$this.click(); // повтор клика после инициализации плеера
		});
		return false;
	});

	// микроплагин input helper
	(function($){
		var is_native = (typeof $('<input type="text" />').get(0).placeholder != 'undefined' ? 1 : 0);
		$.fn.ihelper = function(opts) {
			return this.each(function() {
				var $this = $(this);
				if(is_native) {
					$this.attr('placeholder', $this.attr('data-placeholder'));
				} else {
					$this.focus(function() {
						if(!$this.val() || $this.val() == $this.attr('data-placeholder')) {
							$this.val('');
						}
					}).blur(function() {
						if(!$this.val()) {
							$this.val($this.attr('data-placeholder'));
						}
					}).blur();
				}
			});
		};
	})(jQuery);
	$('input[data-placeholder], textarea[data-placeholder]').ihelper();

	// добавление в друзья
	$('.x-add_to_friend').click(function() {
