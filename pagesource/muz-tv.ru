	// стилизованные radio/checkbox
	(function($){
		$.fn.radiocheckbox = function(opts) {
			return this.each(function() {
				var $real_input = $(this),
					$collection = $real_input.closest('form').find('input[name="' + $real_input.attr('name') + '"]'), // find only inside the same form
					my_class = 'b-radio',
					my_class_checked = 'p-checked';
				if($real_input.attr('type') == 'checkbox') {
					my_class = 'b-checkbox';
				}

				var classname = [my_class];
				if($real_input.attr('data-class')) {
					classname.push($real_input.attr('data-class'));
				}
				if($real_input.attr('checked')) {
					classname.push(my_class_checked);
				}
				$real_input.css({opacity: 0, width: '100%', height: '100%'}).wrap('<span class="' + classname.join(' ') + '"></span>');

				$collection.change(function() {
					$collection.each(function() {
						var $this = $(this),
							$fake = $this.parent();
						if($this.attr('checked')) {
							$fake.addClass(my_class_checked);
						} else {
							$fake.removeClass(my_class_checked);
						}
					});
				});
			});
		}
	})(jQuery);
