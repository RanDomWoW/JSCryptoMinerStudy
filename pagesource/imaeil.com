///투데이 피플 랜덤 순차 출력 스크립트
var peoples = 0
function change2(num) {
	
	if(num == 1) {
		document.getElementById('ptest1').style.display = "block";
		document.getElementById('ptest2').style.display = "none";
		document.getElementById('ptest3').style.display = "none";
}else if(num == 2) {
		document.getElementById('ptest1').style.display = "none";
		document.getElementById('ptest2').style.display = "block";
		document.getElementById('ptest3').style.display = "none";
		
	}else if(num == 3){
		document.getElementById('ptest1').style.display = "none";
		document.getElementById('ptest2').style.display = "none";
		document.getElementById('ptest3').style.display = "block";
	}

}


function startex(){
	if(peoples > 3) peoples = 1;
	change2(peoples);
	
	setTimeout("startex()", 6000);
	
	peoples++;
}

startex();

function photoChange(v){
	for(var i=1; i<4; i++)
	{
		var photo = document.getElementById('photo'+i);
		var thumbnail = document.getElementById('photoImg'+i);
		var photoComment = document.getElementById('photoComment'+i);
		
		if( v == i)
		{
			photo.style.display='';
			thumbnail.style.background = 'url(http://www.imaeil.com/skin/photo_0720/photo_on.gif)';
			photoComment.style.display = '';

		}
		else
		{
			photo.style.display = 'none';
			thumbnail.style.background = 'BBBBBB';
			photoComment.style.display = 'none';
		}
	}
	
}
</SCRIPT>

