function ThumbEntry(){
    var thumbItem  = new Array();
    for( i=0; i<gEntry.length; i++ ) {
        if( gEntry[i].img ){
            thumbItem.push("<li><a href='"+gEntry[i].url+"' target='_blank'><div class='trim'><img src='"+gEntry[i].img+"' /></div><div class='caption'>"+gEntry[i].title+"</div></a></li>");
        }
    }
    
    for( i=0;i<5;i++ ){
        var randum = Math.floor( Math.random() * thumbItem.length );
        if(thumbItem[randum]){
            p(thumbItem[randum]);
            delete thumbItem[randum];
        }else{
            i--;
        }
    }
}



// 記事の変更
function ChangeEntry( start_flag ) {

    // カテゴリーチェック状況の読み込み
    if( start_flag ) {
        LoadCategory();
        if( cookie['filter'] != undefined ) {
            e('filter').value = cookie['filter'];
        }
        if( cookie['filter_gyaku'] == 0 ) {
            e('filter_gyaku').checked = false;
        }
        else if( cookie['filter_gyaku'] == 1 ){
            e('filter_gyaku').checked = true;
        }
    }

    var check_form = document.category_check;
    for( var i=0; i < check_form.length; i++ ) {
        if(check_form.elements[i].checked ) {
            $('#th0 ul li[category="'+check_form.elements[i].value+'"][parent=""]').show();
        } else {
            $('#th0 ul li[category="'+check_form.elements[i].value+'"]').hide();
        }
    }

    // フィルター処理
    if( e('filter').value ) {
        var reg = new RegExp( e('filter').value, "i" );
        var title = "";
        $('#th0 ul li[parent=""]:visible').each(function(){
            title = $(this).find('a[name="title"]').text();
            if( e('filter_gyaku').checked==true ) {
                if (!title.match(reg)) {
                    $(this).hide();
                }
            } else {
                if (title.match(reg)) {
                    $(this).hide();
                }
            }
        });
    }

    // RSSフィルター処理
    if( cookie['f_rid'] ) {
        var f_rid = [];
        if( cookie['f_rid'] != undefined ) {
            f_rid = cookie['f_rid'].split(',');
            for (var i in f_rid) {
                $('[rid="'+f_rid[i]+'"]').hide();
            }
        }
    }

    if (get['eid']) {
        $('#th0 ul li[eid="' + get['eid'] + '"]').show();
    }

    /*
	//ストライプ
	$('#th0 tbody tr[parent=""]:visible:even').removeClass("tr2");
	$('#th0 tbody tr[parent=""]:visible:even').addClass("tr1");
	$('#th0 tbody tr[parent=""]:visible:odd').removeClass("tr1");
	$('#th0 tbody tr[parent=""]:visible:odd').addClass("tr2");
*/
    if (get['eid']) {
        $('#th0 ul li[eid="' + get['eid'] + '"]').css('background-color', '#ffff55');
    }
    // 再表示ごとにイベントを再生成
    $('#th0 ul li[eid]').mouseover(function() {
        $(this).css('background-color', '#d6ebff');
    });

    $('#th0 ul li[eid]').mouseout(function() {
        if (get['eid'] && get['eid']==$(this).attr('eid')) {
            $('#th0 ul li[eid="' + get['eid'] + '"]').css('background-color', '#ffff55');
        } else {
            $(this).css('background-color', '');
        }
    });
}

