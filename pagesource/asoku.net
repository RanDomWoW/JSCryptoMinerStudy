function exOver(a){
    $(a).css('background-color', '#ddd')
        $(a).css('border', '1px solid #000')
}
function exOut(a){
    $(a).css('background-color', '#fff')
        $(a).css('border', '1px solid #fff')
}
function exReg(id){
    cookie = GetCookie();
    var new_rid = [];

    if( cookie['f_rid'] != undefined ) {
        new_rid = cookie['f_rid'].split(',');
    }
    if ($.inArray(id, new_rid) == -1) {
        new_rid.push(id);
        cookie['f_rid'] = new_rid.join(',');
        SetCookie('f_rid', cookie['f_rid'], 90*3600*24);
    }
    fadeMassage('フィルターを登録しました');
    ChangeEntry();

    return true;
}
function fadeMassage(str) {
    var id = 'fadeMassage';
    var scroll_t = $(document).scrollTop();
    var scroll_l = $(document).scrollLeft();
    var win_h = $(window).height();
    var win_w = $(window).width();

    if (!$('#'+id).size()) {
        $('body').append('<div id="'+id+'" style="position:absolute;display:none;z-index:9999;padding:10px;background-color:#000;color:#fff;font-weight:bold;border:1px solid #f00;-moz-border-radius:5px;-webkit-border-radius:5px;"></div>');
    }

    $('#'+id).html(str);
    $('#'+id).css('top', win_h/2-$('#'+id).height()/2+scroll_t).css('left', win_w/2-$('#'+id).width()/2+scroll_l).fadeIn(1000, function(){
        $(this).fadeOut(800);
    });
}

function createExMenu() {

    var new_id = 0;
    var matches = location.hash.slice(1).match(/^new_(\d+)$/);
    if (matches) {
        new_id = matches[1];
    }

    $('body').click(function(){
        if ($('#ex_regist').size()){
            $('#ex_regist').hide();
        }
    });
    $('ul.item-list li a[rss]').bind('click', function(e){
        $('#ex_regist').hide();
        var id = $(this).attr('rss');
        if (!$('#ex_regist').size()) {
            var div = $('<div id="ex_regist" class="rClickMenu"></div>');
            $('body').append(div);
        } else {
            var div = $('#ex_regist');
        }
        div.html('<div class="rss-mini-menu">' + $(this).attr('title') + '</div>');
        if (typeof cookie['f_rid'] != "string" || $.inArray(id, cookie['f_rid'].split(',')) == -1) {
            div.append('<a href="/site/b'+id+'.html" onmouseover="exOver(this);" onmouseout="exOut(this);">このブログのエントリー</a>');
            div.append('<a href="javascript:void(0);" onclick="exReg('+id+');" onmouseover="exOver(this);" onmouseout="exOut(this);">このブログを非表示</a>');
            div.append('<a href="/c/filter_list" onmouseover="exOver(this);" onmouseout="exOut(this);">非表示にしたサイト一覧</a>');
        } else {
            div.append('<div style="color:#aaa;padding:4px;margin:2px;">フィルター登録済み</div>');
        }
        div.css('top', gMouseY+20);
        div.css('left', gMouseX-100);
        div.slideDown(100);
        e.preventDefault();
        return false;
    });
}

