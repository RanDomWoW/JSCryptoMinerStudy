function HtmlEntry( obj, i ) {
    var rss = new Object();
    var bbb= '';
    var entry_class = '';
    var num = 0;
    var click = 0;
    var display = ""
    var parent = ""
    var group = "";
    var title = '';
    var rss_title = '';
    var show_date_flag = 0;		// 時間の見せ方を変更

    // RSS
    rss = gRss[obj.rss];

    // 希にゴミファイルがあるので排除（理由不明）
    if( rss == undefined ) {rss = {};rss.title='未取得';rss.id='0';}

    // 背景色
    if( obj.parent==0 ) {
        // 検索などの処理
        if (free_date) {
            show_date_flag = 1;
        }
    } else {
        entry_class = 'tr3 child';
        show_date_flag = 1;
        parent = obj.parent;
    }

    // クリック数の長さ
    if( gClickNum ) {
        click = parseInt( obj.click / gClickNum * 100 );
        // グループ化したもの用のはどめ
        if( click > 100 ) {
            click = 100;
        }
    }
    else {
        click = 0;
    }

    // グループ
    if( obj.group ) {
        group = '<a href="javascript:void(0);" onclick="ItemToggle('+obj.id+')" group=\"'+obj.id+'\" class="group"><i class="fa fa-plus toggle-icon"></i></a>';
    } else {
        group = '';
    }

    if( obj.title.length > 52 ) {
        title = obj.title.substring(0,36) + '...';
    } else {
        title = obj.title;
    }
    if( rss.title.length > 10 ) {
        rss_title = rss.title.substring(0,10) + '...';
    } else {
        rss_title = rss.title;
    }


    var img = (obj.img) ? '<span onmousemove="popImg(event, \'popImg'+obj.id+'\',\''+obj.img+'\');" onmouseout="popImg(event, \'popImg'+obj.id+'\');"><img src="/image/gazou.png" width="15px" style="position:relative;top:-2px;" /></span>' : '';

    bbb += '<li class="'+entry_class+'" eid="'+obj.id+'" rid="'+rss.id+'" category="'+obj.category+'" parent="'+parent+'"><dl>';
    bbb += '<dd class="image">'+img  +'</dd>';
    bbb += '<dd class="category"><img src="/image/icon'+obj.category_img+'.gif" title="'+AltDate(obj.time)+'" width="50px" class="c_icon"></dd>';
    bbb += '<dd class="toggle">'+group+'</dd>';
    bbb += '<dd class="entry"><a href="'+obj.url+'" target="_blank" onclick="clickEntry('+obj.id+','+obj.time+');" name="title" title="'+obj.title+'">'+title+'</a></dd>';
    bbb += '<dd class="meter"><meter value=".'+click+'"></meter></dd>';
    bbb += '<dd class="setting"><a href="javascript:void(0);" rss="'+rss.id+'" title="'+rss.title+'"><i class="fa fa-cog"></i></a></dd>';
    bbb += '</dl></li>';
    
    if( get['eid'] == obj.id ) {
        bbb += '<li rid="'+rss.id+'" class="'+entry_class+'" category="'+obj.category+'" parent="'+parent+'"><div id="middle_banner_position"></div></li>';
    }

    return bbb;
}


