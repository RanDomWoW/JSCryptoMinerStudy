    function clickslide(){
    }
    
    var $vsliderboxes = $('#vsliderboxes').children();
    function autoslide(){
        var $active = $vsliderboxes.filter('.scrollup');
        if ($active.index() === ($vsliderboxes.length - 1)) {
            //we need to loop back to the beginning
            var $next = $vsliderboxes.eq(0);
        } else {
            var $next = $active.next();
        }
        $active.slideUp(1000, function () {
            $active.removeClass('scrollup');
        });
        $next.slideDown(1000, function () {
            $next.addClass('scrollup');
        });
    }
    
    $('#vslidernav ul li').click(clickslide);
    
    window.setInterval(autoslide, 4000)
    
});}
