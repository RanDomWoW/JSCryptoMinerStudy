document.getElementById("searchHeaderSubmit").addEventListener("click",handleSearchClick);
function handleSearchClick(e){
    var searchInput = e.target.previousElementSibling.previousElementSibling.value;
    var filterOptions = document.getElementById("filterOptions");
    var queryCode = '';
    if(filterOptions){
        
        var getLis = filterOptions.getElementsByTagName('INPUT');
        for(var i=0;i<getLis.length;i++){
            
            var liId = getLis[i].id;
            var inputCheck = getLis[i];
            if(inputCheck.checked){
                queryCode =queryCode+'&fc['+i+']='+liId;
                
            }
            
            
        }
    }
    if(searchInput!=""){
        if(queryCode==""){
            window.location.href = "http://"+window.location.hostname+'/search-results?q='+searchInput; 
        }else{
        window.location.href = "http://"+window.location.hostname+'/search-results?q='+searchInput+queryCode;
        }
        
    }
}
if(document.getElementById("search_results_container")){

    document.getElementById("search_results_container").innerHTML ="";

    }
    function getUrlVars()
    {
        var vars = [], hash;
        var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
        for(var i = 0; i < hashes.length; i++)
        {
        hash = hashes[i].split('=');
        vars.push(hash[0]);
        vars[hash[0]] = hash[1];
        }
        return vars;
    }
    var pageNo = 1;
    var size=10;
    var from =0;
    var urlValue = getUrlVars()["q"];
    var fcode = [];
    if(urlValue && urlValue!=""){
        
        var otherGetParams = getUrlVars();
        for(var i=1;i<otherGetParams.length;i++){
            
            var valueAtt  = getUrlVars()[otherGetParams[i]];
            fcode.push(valueAtt);
            if(otherGetParams[i].indexOf('fc')>-1 && document.getElementById(valueAtt)){
                var inputEle = document.getElementById(valueAtt);
                inputEle.checked = true;
            }
        }
        document.getElementById("t018-77").value=decodeURIComponent(urlValue);
        submitSearchQuery(urlValue);
        
    }
    function submitSearchQuery(urlValue){
        
        var xmlhttp = new XMLHttpRequest();
        var url = "/ds-search/search";
        var params;
        if(fcode.length==0){
            params = "query="+urlValue+'&prId=574401b0000000440d000002&size='+size+'&from='+from;
        }
        else{
            params = "query="+urlValue+'&prId=574401b0000000440d000002&size='+size+'&from='+from+'&fcode='+JSON.stringify(fcode);
        }
        
        xmlhttp.open("POST", url, true);

        //Send the proper header information along with the request

        xmlhttp.onreadystatechange = function() {//Call a function when the state changes.
            if(xmlhttp.readyState == 4 && xmlhttp.status == 200) {
                var results = JSON.parse(xmlhttp.responseText);
                var response = results["r"];
                var html ="<div id='netResults'>Showing results <span id='resultsShownfrom'>1</span>-<span id='resultsShownto'>"+response.length+"</span> of <span id='totalResults'>"+results["t"]+"</span></div>";
                if(response.length==0){
                    html = "<div id='netResults'>No Results Found!</div>";
                    document.getElementById("search_results_container").innerHTML = html;
                    return;
                }
                for(var i=0;i<response.length;i++){
                    html = html+'<div class="searchResultData">';
                    var titleInfo = response[i]["_source"]["title"];
                    if(response[i]["highlight"]["title"]){
                        titleInfo = response[i]["highlight"]["title"];
                    }
                    html = html+'<a class="searchResultPageHeading" href="'+response[i]["_source"]["url"]+'">'+titleInfo+'</a>';
                    
                        if(response[i]["highlight"]["text"]){
                        
                        for(var j in response[i]["highlight"]["text"]){
                            html = html+'<p class="searchResultsPageInitialText">';
                            html = html+response[i]["highlight"]["text"][j];
                            html = html+'</p>';
                        }
                    }
                    var headTags = {"h1":"","h2":"","h3":"","h4":"","h5":"","h6":""};
                    for(var key in headTags){
                        if(response[i]["highlight"][key]){
                            html = html+'<p class="searchResultsPageInitialText">';

                            for(var j in response[i]["highlight"][key]){
                                
                            html = html+response[i]["highlight"][key][j];
                            }
                            html = html+'</p>';
                        }
                    }
                    html = html+'</div>';
                }
               
                if(results["t"] > 10){
                    
                    var noOfPages = Math.round(results["t"]/10);
                    var remainder = results["t"]%10;
                    if(remainder>0){
                        noOfPages = noOfPages+1;
                    }
                    html = html+'<div class="pagination" id="search_pagination" >';
                    var pageHtml = "";
                    if(pageNo>1){
                        var previousPage = pageNo-1;
                        pageHtml = pageHtml+'<a class="paginationLink" rel="0" href="javascript:;" id="'+previousPage+'">‹</a>';
                    }
                    pageHtml = pageHtml+'<a class="paginationLink" rel="0" href="javascript:;" id="'+pageNo+'">'+pageNo+'</a>';
                    
                    var nextPage = pageNo+1;
                    if(nextPage<=noOfPages){
                    pageHtml = pageHtml+'<a class="paginationLink" rel="0" href="javascript:;" id="'+nextPage+'">'+nextPage+'</a>';
                    }
                    
                    nextPage = nextPage+1;
                    if(nextPage<=noOfPages){
                    pageHtml = pageHtml+'<a class="paginationLink" rel="0" href="javascript:;" id="'+nextPage+'">'+nextPage+'</a>';
                    }
                    nextPage = nextPage+1;
                    if(nextPage<=noOfPages){
                        pageHtml = pageHtml+'<a class="paginationLink" rel="0" href="javascript:;" id="'+nextPage+'">›</a>';
                    }
                    html = html+pageHtml+'</div>'
                }
                document.getElementById("search_results_container").innerHTML = html;
                var paginationLinks  = document.getElementsByClassName('paginationLink');
                for(var i=0;i<paginationLinks.length;i++){
                    paginationLinks[i].addEventListener("click",sendSearchQuery);
                }
                document.getElementById("resultsShownfrom").innerHTML = (pageNo-1)*size+1;
                document.getElementById("resultsShownto").innerHTML = (pageNo-1)*size+response.length;
                
            }
        }
        xmlhttp.send(params);
    }

    function sendSearchQuery(e){
        
        var target = this;
        var targetId = parseInt(target.id);
         pageNo = targetId;
         if(targetId==1){
             targetId = 1;
         }
         else{
             targetId = targetId-1;
         }
         from = size*targetId;
         urlValue = getUrlVars()["q"];
         submitSearchQuery(urlValue);
    }

var min=14;
var max=20;
function increaseFontSize() {
   var p = document.querySelectorAll('span,p,li,td,h1,h2,h3,h4,h5,h6,em,time,code,strong,cite,sup,sub,legend,small,i');
   for(i=0;i<p.length;i++) {
      if(p[i].style.fontSize) {
         var s = parseInt(p[i].style.fontSize.replace("px",""));
      } else {
         var s = 14;
      }
      if(s!=max) {
         s += 1;
      }
      p[i].style.fontSize = s+"px"
   }
}
function decreaseFontSize() {
   var p = document.querySelectorAll('span,p,li,td,h1,h2,h3,h4,h5,h6,em,time,code,strong,cite,sup,sub,legend,small,i');
   for(i=0;i<p.length;i++) {
      if(p[i].style.fontSize) {
         var s = parseInt(p[i].style.fontSize.replace("px",""));
      } else {
         var s = 14;
      }
      if(s!=min) {
         s -= 1;
      }
      p[i].style.fontSize = s+"px"
   }   
}

