<script>

function enviarForm(){
var validacion=0;
   if($("#nombreCliente").val() == ""){
     $("#msgNombre").html("Debe ingresar nombre");
     $('#nombreCliente').focus();
     validacion=1;
   }else{
      $("#msgNombre").html("");
   }

   if($("#apellidoCliente").val() == ""){
     $("#msgApellido").html("Debe ingresar apellido");
     $('#apellidoCliente').focus();
     validacion=1;
   }else{
      $("#msgApellido").html("");
   }

   if($("#telefonoCliente").val() == ""){
     $("#msgFono").html("Debe ingresar telefono");
     $('#telefonoCliente').focus();
     validacion=1;
   }else{
      $("#msgFono").html("");
   }

   if($("#emailCliente").val() == ""){
     $("#msgEmail").html("Debe ingresar email");
     $('#emailCliente').focus();
     validacion=1;
   }else{
      if(!validarEmail($("#emailCliente").val())){
         $("#msgEmail").html("Debe ingresar email válido");
         $('#emailCliente').focus();
         validacion=1;
      }else{
         $("#msgEmail").html("");
      }
   }

   if($("#detalle").val() == ""){
     $("#msgDetalle").html("Debe ingresar consulta");
     $('#detalle').focus();
      validacion=1;
   }else{
      $("#msgDetalle").html("");
   }
  
   
   if(validacion == 1){
      return false;
   }

   $("#formContactenos").submit();
}
  
</script>
