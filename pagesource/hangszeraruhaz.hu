<script type="text/javascript">
//<![CDATA[
var fttol = 0;
var ftig = 0;
var filterIDs = [];
var filterGYIDs = [];
var filterMIDs = [];
var filterCUSTOMs = [];
var filterKEYWORDs = '';

var tmr;

var sumOnly = 0;

var tagIDs = new Array();
var tagGYIDs = new Array();
var tagMIDs = new Array();
var tagCUSTs = new Array();

function fltIDs(FID,MID,GYID,CUST)
{
   	clearTimeout(tmr);
   	$('#axRes').css("display",'none');

	isNew = '';
	if(FID != '')
	{
		for(i=0;i<filterIDs.length;i++) { if(filterIDs[i] == FID) { isNew = i; break; } }

		if(isNew !== '') { filterIDs.splice(isNew,1); }
		else { filterIDs.push(FID); }
	}
	else if(GYID != '')
	{
		for(i=0;i<filterGYIDs.length;i++) { if(filterGYIDs[i] == GYID) { isNew = i; break; } }

		if(isNew !== '') { filterGYIDs.splice(isNew,1); }
		else { filterGYIDs.push(GYID); }
	}
	else if(MID != '')
	{
		for(i=0;i<filterMIDs.length;i++) { if(filterMIDs[i] == MID) { isNew = i; break; } }

		if(isNew !== '') { filterMIDs.splice(isNew,1); }
		else { filterMIDs.push(MID); }
	}
	else if(CUST != '')
	{
		for(i=0;i<filterCUSTOMs.length;i++) { if(filterCUSTOMs[i] == CUST) { isNew = i; break; } }

		if(isNew !== '') { filterCUSTOMs.splice(isNew,1); }
		else { filterCUSTOMs.push(CUST); }
	}

	setPrc(0);
	setKeyw(0);
	getProds();
}

function setPrc(dogetprods)
{
	artol = document.getElementById('ART').value;
	arig = document.getElementById('ARI').value;

	if(artol >= 0 && isitNum(artol)) { fttol = artol; }
	else { fttol = ''; }

	if(arig >= 0 && isitNum(arig)) { ftig = arig; }
	else { ftig = ''; }

	if(dogetprods == 1)
	{
		getProds();
	}
}

function clearPrc(tol,ig,dogetprods)
{
	if(tol == 1) { document.getElementById('ART').value = ""; }
	if(ig == 1) { document.getElementById('ARI').value = ""; }

	if(dogetprods == 1)
	{
		setPrc(0);
		getProds();
	}
}

function setKeyw(dogetprods)
{
	filterKEYWORDs = document.getElementById('KEYW').value;
	if(dogetprods == 1)
	{
		getProds();
	}
}

function clearKeyw()
{
	filterKEYWORDs = '';
	document.getElementById('KEYW').value = '';
	setPrc(0);
	getProds();
}

function setTags(REgyids,REfids,REmids,REartol,REarig,REcustoms)
{
	REgyidArr = REgyids.split(",");

	for(i=0;i<tagGYIDs.length;i++)
	{
		tagged = 0;
		for(j=0;j<REgyidArr.length;j++)
		{
			if('G'+REgyidArr[j] == tagGYIDs[i])
			{
				document.getElementById(tagGYIDs[i]).className = 'cimkeOn';
				tagged = 1;

				break;
			}

		}

		if(tagged != 1)
		{
			document.getElementById(tagGYIDs[i]).className = 'cimke';
		}
	}

	REfidArr = REfids.split(",");

	for(i=0;i<tagIDs.length;i++)
	{
		tagged = 0;
		for(j=0;j<REfidArr.length;j++)
		{
			if(REfidArr[j] == tagIDs[i])
			{
				$("#"+tagIDs[i]).attr('class', 'cimkeOn');

				tagged = 1;
				break;
			}
		}
		if(tagged != 1)
		{ $("#"+tagIDs[i]).attr('class', 'cimke'); }
	}



	REmidArr = REmids.split(",");

	for(i=0;i<tagMIDs.length;i++)
	{
		tagged = 0;
		for(j=0;j<REmidArr.length;j++)
		{
			if(REmidArr[j] == tagMIDs[i])
			{
				$("#"+tagMIDs[i]).attr('class', 'cimkeOn');
				tagged = 1;
				break;
			}
		}
		if(tagged != 1)
		{ $("#"+tagMIDs[i]).attr('class', 'cimke'); }
	}

	if(REartol > 0 && isitNum(REartol))
	{
		fttol = REartol;
		document.getElementById('ART').value = REartol;
	}
	else
	{
		fttol = '';
		document.getElementById('ART').value = '';
	}

	if(REarig > 0 && isitNum(REarig))
	{
		ftig = REarig;
		document.getElementById('ARI').value = REarig;
	}
	else
	{
		ftig = '';
		document.getElementById('ARI').value = '';
	}

	REcusArr = REcustoms.split(",");

	for(i=0;i<tagCUSTs.length;i++)
	{
		tagged = 0;
		for(j=0;j<REcusArr.length;j++)
		{
			if('C'+REcusArr[j] == tagCUSTs[i])
			{
				document.getElementById(tagCUSTs[i]).className = 'cimkeOn';
				tagged = 1;
				break;
			}
		}
		if(tagged != 1)
		{ document.getElementById(tagCUSTs[i]).className = 'cimke'; }
	}

}
//]]>
</script>
<script type="text/javascript">
//<![CDATA[

	$(window).on("popstate", function(e)
	{
		if (e.originalEvent.state !== null)
		{
			top.location.href=location.href;
		}
	});

function getProds(turn)
{
	fids	= filterIDs.join(",");
	gyids	= filterGYIDs.join(",");
	mids	= filterMIDs.join(",");
	customs	= filterCUSTOMs.join(",");

	if(sumOnly == 1)
	{
		$("#loadingHldr").removeClass("dnone");
		$("#showMres a").addClass("dnone");
	}
	else
	{
		document.getElementById('prodsRes').innerHTML='<div style="background:#ffffff;text-align:center;margin-top:20px;"><img src="/scnt/ajax-bars.gif" width="180" height="40" alt="Feldolgozás folyamatban"></div>';
	}


	var posting = $.post("/", {
									mode: "getProducts",
									ckID: 0,
									fids: fids,
									gyids: gyids,
									mids: mids,
									artol: fttol,
									arig: ftig,
									customs: customs,
									keyw: filterKEYWORDs,
									turn: turn,
									sumOnly: sumOnly
								}, '', "json"
	);

	posting.done(function( retdata )
	{
		if(sumOnly == 0)
		{
        	document.getElementById('prodsRes').innerHTML = retdata.products;
        	$('#mobresCount').html( retdata.REcnt );

        	document.getElementById('axRes').innerHTML = retdata.REcnt+' db termék';
        	document.getElementById('axRes').style.display = 'block';
        	tmr = setTimeout("document.getElementById('axRes').style.display = 'none';",3000);
        	setTags(retdata.REgyids, retdata.REfids, retdata.REmids, retdata.REartol, retdata.REarig, retdata.REcustoms);

        		//alert($("#reboundURL").attr("href"));
        		history.pushState({}, '', retdata.REurl);

        	//setupZoom();

        	checkLeadHei();
		}
		else if(sumOnly == 1)
		{
			document.getElementById('prodsRes').innerHTML = retdata.REcnt;
			$('#mobresCount').html( retdata.REcnt );
			$("#loadingHldr").addClass("dnone");
			$("#showMres a").removeClass("dnone");
			setTags(retdata.REgyids, retdata.REfids, retdata.REmids, retdata.REartol, retdata.REarig, retdata.REcustoms);

       		history.pushState({}, '', retdata.REurl);

			$("#showMobres").attr("href",retdata.REurl);
		}
	});


}

function toggleSub(id)
{
	if(document.getElementById('tags'+id).className == 'tagSTon')
	{ document.getElementById('tags'+id).className = 'tagSToff'; document.getElementById('stgs'+id).style.display = "none"; }
	else
	{ document.getElementById('tags'+id).className = 'tagSTon'; document.getElementById('stgs'+id).style.display = 'block'; }
}

function isitNum(val)
{
   return (val - 0) == val && val.length > 0;
}

function buildURL()
{
	artol = document.getElementById('ART').value;
	arig = document.getElementById('ARI').value;

	if(artol >= 0 && isitNum(artol)) { fttol = artol; }
	else { fttol = ''; }

	if(arig >= 0 && isitNum(arig)) { ftig = arig; }
	else { ftig = ''; }

	tx = 'mode=useprm&tplDt=subcat|||&prmID=;;;;;;;';

	var tx1 = tx.substring(0,tx.lastIndexOf(";"));
	var tx2 = tx1.substring(0,tx1.lastIndexOf(";"));
	var tx3 = tx2.substring(0,tx2.lastIndexOf(";"));
	var tx4 = tx3.substring(0,tx3.lastIndexOf(";"));

	var footKeyw = tx.substring(tx.lastIndexOf(";")+1);
	var footCust = tx1.substring(tx1.lastIndexOf(";")+1);
	var footArig = tx2.substring(tx2.lastIndexOf(";")+1);
	var footArtol = tx3.substring(tx3.lastIndexOf(";")+1);

	top.location.href = "/index.php?"+tx4+";"+artol+";"+arig+";"+footCust+";"+footKeyw;

	//index.php?mode=useprm&tplDt=subcat|1||&prmID=3;;;;100000;300000;1;
	//index.php?mode=useprm&tplDt=subcat|1||&prmID=3;;;;10;1000000;1;int
}
//]]>
</script>



