            var currentLang = "en";
            var availableLangs = ['br', 'es', 'de', 'fr', 'it', 'ru', 'tr', 'cs', 'sk', 'pl', 'nl', 'ar'];


            var lang = window.navigator.languages ? window.navigator.languages[0] : null;
            lang = lang || window.navigator.language || window.navigator.browserLanguage || window.navigator.userLanguage;

            if (lang.indexOf('-') !== -1)
                lang = lang.split('-')[0];

            if (lang.indexOf('_') !== -1)
                lang = lang.split('_')[0];

            if (lang == "pt-br" || lang == "pt_br")
                lang = "br";

            if (availableLangs.indexOf(lang) >= 0 && typeof cookieLang == 'undefined' && currentLang != lang) {
                $('#langinfo').show();
                $('#langinfo__'+lang).slideDown();
                ga('send', 'event', { eventCategory: 'Language', eventAction: 'banner/display', eventLabel: currentLang + ' -> ' + lang });
            }

            $('.langinfo-ignore').click(function (e) {
                e.preventDefault();
                $('#langinfo').slideUp();
                Cookies.set('siteLang', currentLang, { expires: 7 });
                ga('send', 'event', { eventCategory: 'Language', eventAction: 'banner/ignore', eventLabel: currentLang + ' -> ' + $(this).data('lang') });
            });

            $('.langinfo-set').click(function (e) {
                e.preventDefault();
                Cookies.set('siteLang', $(this).data('lang'), { expires: 7 });
                window.location.href = $(this).attr("href");
                ga('send', 'event', { eventCategory: 'Language', eventAction: 'banner/set', eventLabel: currentLang + ' -> ' + $(this).data('lang') });
            });

