        $('[data-b="cookie"]').show();
        $('[data-b="cookie"]').find('[data-el="close"]').on('click', function() {
            $('[data-b="cookie"]').slideUp();
            setCookie('bm_cookie_disclaimer', 1, 30);
        });
    } else {
        $('[data-b="cookie"]').hide();
    }
});
</script>

