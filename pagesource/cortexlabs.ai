<script>
    function setFontSize() {
        var winWidth = $(window).width();
        var size = (winWidth / 750) * 100;
        document.documentElement.style.fontSize = (size < 100 ? size : 100) + 'px';
    }
    window.onload = function () {
        $(".cortex").css('height', $(window).height());
        setFontSize();
        if (localStorage.getItem('ifAgree') == 'true') {
            $(".notChina").css('display', 'block');
            setTimeout(function () {
                scrollRuleTab();
            }, 200)
        } else {
            $.ajax({
                'url': 'http://api.ip138.com/query/',
                'data': {            //默认自动添加callback参数
                    'ip': '', //为空即为当前iP地址
                    'oid': '14256,',
                    'mid': '77200',
                    'token': 'cafe5c577a6928459e132f33ee40ca21'  //不安全，请定期刷新token，建议进行文件压缩
                },
                'dataType': 'jsonp',
                'success': function (json) {
                    if (json.data[0] == '中国') {
                        $('.china').css('display', 'block');
                        $(".notChina").css('display', 'none');
                    } else {
                        $(".notChina").css('display', 'block');
                        setTimeout(function () {
                            scrollRuleTab();
                        }, 200)
                    }
                }
            });
        }
        $(".sure").click(function () {
            $('.china').css('display', 'none');
            $(".notChina").css('display', 'block');
            setTimeout(function () {
                scrollRuleTab();
            }, 200);
            localStorage.setItem('ifAgree', 'true');
        });
        bgmove();
    };
    $(window).resize(function () {
        setFontSize();
        $(".cortex").css('height', $(window).height());
    });
    function scrollRuleTab() {
        var top = document.documentElement.scrollTop || window.pageYOffset || document.body.scrollTop;
        top = top + 3;
        var introduction = $("#introduction").offset().top;
        var techniques = $("#solutions").offset().top;
        var team = $("#team").offset().top;
        var advisors = $("#advisors").offset().top;
        var partnerships = $("#partnerships").offset().top;
        var investors = $("#investors").offset().top;
        if (top < introduction) {
            $(".menu li").eq(0).addClass("active").siblings().removeClass("active");
        } else if (top >= introduction && top < techniques) {
            $(".menu li").eq(1).addClass("active").siblings().removeClass("active");
        } else if (top >= techniques && top < team) {
            $(".menu li").eq(2).addClass("active").siblings().removeClass("active");
        } else if (top >= team && top < advisors) {
            $(".menu li").eq(4).addClass("active").siblings().removeClass("active");
        } else if (top >= advisors && top < partnerships) {
            $(".menu li").eq(5).addClass("active").siblings().removeClass("active");
        } else if (top >= partnerships && top < investors) {
            $(".menu li").eq(6).addClass("active").siblings().removeClass("active");
        } else if (top >= investors) {
            $(".menu li").eq(7).addClass("active").siblings().removeClass("active");
        }
        if ($(document).height() == $(window).height() + $(window).scrollTop()) {
            $(".menu li").eq(7).addClass("active").siblings().removeClass("active");
        }
    }
    function menu(id) {
        var position = $("#" + id).offset().top;
        $("body,html").animate({scrollTop: position + 3}, 500);
    }
