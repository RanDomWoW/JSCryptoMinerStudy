function ShowImgChange(index){
    var i=index-1;
    $("#ChangeImg div").removeClass("selected");
    $("#ChangeLink a").removeClass("selected");
    $("#ChangeImg div:eq(" + i + ")").addClass("selected");
    $("#ChangeLink a:eq(" + i + ")").addClass("selected");
}
var _h = 0;
var _c = 1;
function play(){
    _h = setInterval("auto()", 3000);
}
function auto(){
    _c = _c > 4 ? 1 : _c + 1;
    ShowImgChange(_c);
}
</script>
