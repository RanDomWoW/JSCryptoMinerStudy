<style type="text/css">
.mylivechat_poweredby {display: none;}
.mylivechat_main {height: 480px !important;}
  .cbp-hrmenu {margin-top: 0;}
.carousel-caption {left: 0; right: 0;}
.cbp-hrmenu .cbp-hrsub-inner > div {width: 20%;}
.cbp-hrmenu > ul {max-width: 100em; padding: 0;}
.cbp-hrmenu > ul > li > a {padding: 0.6em 1.4em;}
.modal-dialog {width: fit-content;}
.modal-header .close {
  margin: 0;
  position: absolute;
  right: 0;
  background-color: #000;
  width: 25px;
  height: 25px;
  z-index: 7;
  color: #fff;
  border-radius: 5px;
}
.carousel-control.right, .carousel-control.left {background-image: none; width: 5%;}
.carousel-control.right span, .carousel-control.left span {
  font-size: 30px;
    position: absolute;
    top: 45%;
    width: 35px;
    height: 35px;
    background-color: #ffffff;
    color: #000;
}
.carousel-control.right span {right: 0;}
.carousel-control.left span {left: 0;}
#inquiryForm {
  position: fixed;
  right: 20px;
  bottom: 0;
  background-color: rgba(26, 64, 120, 0.95);
  padding: 20px 15px 10px;
  max-width: 315px;
  width: 100%;
}
#inquiryForm button {
  background: transparent;
  width: 100%;
  border: 0px;
  margin-left: 0px;
  margin-bottom: 10px;
  overflow: visible;
}
#inquiryForm button:focus {outline: none;}
#inquiryForm button h5 {
  display: inline-block;
  line-height: 1;
  margin: 0;
  padding: 0;
  color: #ffffff;
  font-size: 16px;
  letter-spacing: 0.2px;
}
#inquiryForm button.collapsed:after {
  content: "\f106";
}
#inquiryForm button:after {
  font-family: 'FontAwesome';
  float: right;
  content: "\f107";
  font-size: 18px;
  color: #dcdcdc;
  line-height: 1;
  border: 1px solid #dcdcdc;
  width: 22px;
  height: 22px;
  margin-top: -4px;
  border-radius: 20px;
}
#inquiryForm p, #inquiryForm td {padding: 4px 0px; color: #dedede;}
#inquiryForm b {line-height: 1.5; font-weight: normal;}
#inquiryForm input, #inquiryForm select {
  padding: 0px 10px;
  border-radius: 4px;
  border: 0px;
  width: 100%;
  color: #000;
  background-color: #dedede;
}
#inquiryForm select {padding: 3px 10px;}
#inquiryForm input[type="submit"] {
  background-color: #132c52;
  width: 80px;
  margin: 5px 0 0 auto;
  display: block;
  color: #fff;
  text-transform: uppercase;
  letter-spacing: 1px;
  padding: 3px 10px;
}
#inquiryForm hr {margin: 5px 0; border-top: 1px solid #8e969e;}
.formError {
  position: relative!important;
  top: 0px!important;
  left: 0px!important;
  margin-top: 0px!important;
  color: red;
  text-align: left;
}
.formError .formErrorContent {
  color: #ff6d6d;
  font-size: 12px;
  line-height: 1;
  letter-spacing: 1px;
}
