                    '<div class="slider-button" style="'+sliderTexts[number][3]+'">' +
                    '<p>'+sliderTexts[number][0]+'</p>' +
                    '<p class="slider-button-bold">'+sliderTexts[number][1]+'</p>' +
                    '</div>' +
                    '</a>' +
                    '</div>' +
                    '</li>';
        }
    }

    function initSlidesPosition() {

        slidePositions[0] = new Array('4', 0);
        slidePositions[1] = new Array('3', 1);        slidePositions[2] = new Array('5', 2);        slidePositions[3] = new Array('1', 3);        slidePositions[4] = new Array('2', 4);
        slidePositions.sort();
    }

    initSliderImageSizes();
    extendSliderInfo();
    setSlide(0);
    adjustSliderSize();

    function adjustSliderSize() {

        if (windowWidth < 768) {
            $('widget_slider').setStyle({
                width: windowWidth + "px",
                height: (Math.round(((imageSizeHeight * windowWidth) / imageSizeWidth)) + $$('.slider-button').first().getHeight()) + "px"
            });
            $$('#widget_slider .show li.slide').first().setStyle({
                width: windowWidth + "px",
                height: (Math.round(((imageSizeHeight * windowWidth) / imageSizeWidth)) + $$('.slider-button').first().getHeight()) + "px"
            });
            $$('#widget_slider .show img').each(function(element) {
                element.setStyle({
                    width: windowWidth + "px",
                    height: Math.round(((imageSizeHeight * windowWidth) / imageSizeWidth)) + "px"
                });
            });
        }
         else if (windowWidth < imageSizeWidth) {

            $('widget_slider').setStyle({
                width: windowWidth + "px",
                height: Math.round(((imageSizeHeight * windowWidth) / imageSizeWidth)) + "px"
            });

            $$('#widget_slider .show img').each(function(element) {
                element.setStyle({
                    width: windowWidth + "px",
                    height: Math.round(((imageSizeHeight * windowWidth) / imageSizeWidth)) + "px"
                });
            });

        } else {
            $('widget_slider').setStyle({'width': imageSizeWidth+'px', 'height': imageSizeHeight+'px'});
        }
    }

    function addSliderImages() {

        setSlide(1);
        setSlide(2);
        setSlide(3);
        setSlide(4);
        setSlide(5);
    }

    $('widget_slider').setStyle({'max-width': '1910px'});

    var sliderOptions = {
            captions: false,
            controls: false,
            transitionType: "fade",
            swipeEvents: false,
            interval : 5000,
            transitionTime: 1
        };


    Event.observe(window, 'load', function () {

        addSliderImages();
        adjustSliderSize();

        show = new protoShow('widget_slider', sliderOptions);
    });

    Event.observe(window, "resize", function() {

        if (windowWidth !== document.documentElement.clientWidth) {
            show.stop();
            initSliderImageSizes();
            $('widget_slider').innerHTML = original_slider_content;
            setSlide(0);
            addSliderImages();
            adjustSliderSize();
            delete show;

            show = new protoShow('widget_slider', sliderOptions);
        }
    });

</script>
