<script>
    Event.observe(window, 'load', function() {

        <!-- Datalayer Homepage -->
        var json = [];
        var cont = 1;
        $$('.product-item').each(function (name) {
            json[cont]= {
                'name' : name.readAttribute('data-name'),
                'id' : name.readAttribute('data-sku'),
                'price' : name.readAttribute('data-price'),
                'brand' : name.readAttribute('data-brand'),
                'category' : name.readAttribute('data-category'),
                'variant' : name.readAttribute('data-variant'),
                'position' : cont
            };
            cont++;
        });

        dataLayer.push({
            'event': 'productImpressions',
            'ecommerce': {
                'currencyCode': 'EUR',
                'impressions': json
            }
        });
        <!-- /Datalayer Homepage -->

        <!-- DataLayer Product click -->
        $$('.product-item-button').invoke('observe','click', function(event) {
            dataLayer.push({
                'event': 'productClick',
                'ecommerce': {
                    'click': {
                        'products': [{
                            'name': this.title,
                            'id': this.readAttribute('data-sku'),
                            'price': this.readAttribute('data-price'),
                            'brand': this.readAttribute('data-brand'),
                            'category' : this.readAttribute('data-category'),
                            'variant' : this.readAttribute('data-variant'),
                            'position' : this.readAttribute('data-position')
                        }]
                    }
                }
            });
        });
        $$('.product-item-image').invoke('observe','click', function(event) {
            dataLayer.push({
                'event': 'productClick',
                'ecommerce': {
                    'click': {
                        'products': [{
                            'name': this.title,
                            'id': this.readAttribute('data-sku'),
                            'price': this.readAttribute('data-price'),
                            'brand': this.readAttribute('data-brand'),
                            'category' : this.readAttribute('data-category'),
                            'variant' : this.readAttribute('data-variant'),
                            'position' : this.readAttribute('data-position')
                        }]
                    }
                }
            });
        });
        <!-- /DataLayer Product click -->

        <!-- DataLayer Banners -->
        setTimeout(jsonbanner, 1000);
        function jsonbanner() {
            var json = [];
            var cont = 1;
            $$('.show-window .show li').each(function (name) {
                json[cont]= {
                    id : 'banner_'+cont,
                    name: name.select('.slider-button').first().innerHTML,
                    creative : name.select('.slider-image').first().src,
                    position : 'slot'+cont
                };
                cont++;
            });
            dataLayer.push({
                'event': 'promoView',
                'ecommerce': {
                    'promoView': {
                        'promotions': json
                    }
                }
            });
        }
        <!-- /DataLayer Banners -->

        <!-- DataLayer Click over Banners -->
        setTimeout(clickbanner, 1000);
        function clickbanner() {
            $$('.slide').invoke('observe', 'click', function (event) {
                dataLayer.push({
                    'event': 'promotionClick',
                    'ecommerce': {
                        'promoClick': {
                            'promotions': [{
                                id : 'banner_' + (this.previousSiblings().size()+1),
                                name: this.select('.slider-button').first().innerHTML.innerHTML,
                                creative : this.select('.slider-image').first().src,
                                position : 'slot'+ (this.previousSiblings().size()+1)
                            }]
                        }
                    }
                });
            });
        }
        <!-- /DataLayer Click over Banners -->

    });
</script>

