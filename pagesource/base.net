<script type="text/javascript">
    // <![CDATA[
    var newsletterSubscriberFormPopup;

    function loadNewsletterPopup() {
        if (!Mage.Cookies.get('cookie_newsletter_viewed')) {
            
            $$('body').first().insert(
                '<div class="visor-newsletter-background" >' +
                    '<div class="visor visor_newsletter" id="visor_container">' +
                        '<div class="visor-wrapper">' +
                            '<div class="visor-controls">' +
                                '<a href="#" onclick="javascript:$$(\'.visor-newsletter-background\').first().remove();return false;" class="visor-close">Cerrar</a>' +
                            '</div>' +
                            '<div class="visor-content">' +
                                '<p><img alt="" src="https://cdn.base.net/media/wysiwyg/POP-UP-2018.jpg" /></p><p>Recibe nuestras mejores ofertas y promociones</p><h2>A QUE ESPERAS!!!</h2>' +
                                '<form action="https://www.base.net/newsletter/subscriber/new/" method="post" id="newsletter_popup_form"><div class="email"><input type="text" name="email" id="newsletter" placeholder="Escribe aquí tu email..." class="required-entry validate-email" /></div><div class="check"><input type="checkbox" name="newsletter_conditions" id="newsletter_conditions_popup" class="required-entry" /><label for="newsletter_conditions_popup"><span class="check"></span>Acepto el <a href="https://www.base.net/aviso-legal/" class="ajax-visor">aviso legal</a> y la <a href="https://www.base.net/politica-de-privacidad/" class="ajax-visor">política de privacidad</a></label></div><input type="submit" value="¡Quiero suscribirme!" /></form>' +
                                '<button onclick="javascript:newsletterSubscriberFormPopup.submit()">Recibir ofertas</button>' +
                            '</div>' +
                        '</div>' +
                    '</div>' +
                '</div>'
            );

            $$('.visor-newsletter-background').first().observe('click',function(e, el){
                if ( ! e.target.descendantOf('visor_container')) {
                    $$('.visor-newsletter-background').first().remove();return false;
                }
            });

            newsletterSubscriberFormPopup = new VarienForm('newsletter_popup_form');

            var d = new Date();
            d.setTime(d.getTime() + (365 * 24 * 60 * 60 * 1000));
            Mage.Cookies.set('cookie_newsletter_viewed', '1', d);

        }
    }
    //]]>
</script>
    <script type="text/javascript">
        //<![CDATA[
        if (typeof loadNewsletterPopup == 'function') {
            loadNewsletterPopup();
        }
        //]]>
    </script>
