				<script>
			var bannerChanging = false;
			var changeDelay = 5000;
			var changeSpeed = 800;
			var changeTime = null;
			function bannerChanger()
			{
				var thisButton = jQuery(this);
				if(bannerChanging == false){
					bannerChanging = true;
					var currentBanner = jQuery("#big-banners li.big-banner.active");
					if(thisButton.hasClass("banner-button")){
						var nextBannerclass = thisButton.attr('class').substring(0,12);
					}
					else{
						var nextBannernumber = (parseInt(currentBanner.attr('class').substring(11,12)) + 1);
						if(nextBannernumber>4){nextBannernumber = nextBannernumber - 4};
						var nextBannerclass = currentBanner.attr('class').substring(0,11) + nextBannernumber;
					}
					var nextBanner = jQuery("." + nextBannerclass + ".big-banner");
					if(!currentBanner.hasClass(nextBannerclass)){
						currentBanner.animate({opacity:'0'},changeSpeed);
						nextBanner.animate({opacity:'1'},changeSpeed);
						jQuery("#big-banners li").removeClass("active");
						jQuery("." + nextBannerclass).addClass("active");
					}
					setTimeout(function(){bannerChanging = false;},changeSpeed);
				}
				else {
					clearTimeout(changeTime);
					changeTime = setTimeout(function(){thisButton.mouseover();},changeSpeed);
				}
			}
			var bannerChange = setInterval(bannerChanger,changeDelay);
			jQuery(".banner-button").click(bannerChanger);
			jQuery(".banner-button").mouseover(bannerChanger);
			jQuery(".banner-button").mouseover(function(){clearInterval(bannerChange);});
			jQuery(".banner-button").mouseout(function(){bannerChange = setInterval(bannerChanger,changeDelay);clearTimeout(changeTime);});
		</script>
