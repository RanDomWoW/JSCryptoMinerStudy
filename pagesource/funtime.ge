	<script>
		
			$.fn.ajaxlink();
			$.fn.ajaxform();

		$(document).ready(function(){

			window.onscroll = function(ev) {
			    if ((window.innerHeight + window.scrollY) >= document.body.offsetHeight + document.getElementById('footer').clientHeight) {
			        if($('button#load_more').length) $('button#load_more').trigger("click");
			    }
			};

			// LIVE
			var dataString = '';

			
			$.ajax({
				type: "GET",
				url: "https://live.melon.ge/store/funtime/live",
				cache: false,
				data: dataString,
				success: function (html) {
				
				}
			});
			// END LIVE



			$(window).on('load', function(){
				//$.removeCookie('bann');
				//$.removeCookie('mobile-banner-funtime');
				if(!$.cookie('mobile-banner-funtime') && $(window).width() < 700 && $('.mobile-banner ins a').length){
					$('#revese-countdown')
					  .prop('number', 4)
					  .animateNumber(
					    {
					      number: 0,
					      numberStep: function(now, tween) {
					        var target = $(tween.elem),
					            rounded_now = Math.round(now);

					        target.text(now === tween.end ? blurOut() : rounded_now);
					      }
					    },
					    4000,
					    'linear'
				  	);

					blurIn();
					$('.mobile-banner').show();
					$.cookie('mobile-banner-funtime',1);
				} 
			});

			$('.close-slide-banner').click(function(){
				$('.slide-banner').css('left', '-400px');
				$('.slide-banner').remove();
				$.cookie('funtime_400_250', 1, { expires : 1 });
			});

			if($.cookie('live-embed') && $('#live-embed').length) {
			//$('#live-embed').remove();
			} else {
				$('#live-embed').fadeIn();
			}


			var scrolled = false;

			$(window).scroll(function(){
				if ($(this).scrollTop() > 100) {
					$('#scrollUp').fadeIn();
					$('.main-nav-bar').addClass('fixed');
				} else {
					$('#scrollUp').fadeOut();
					$('.main-nav-bar').removeClass('fixed');
				}

				if ($(this).scrollTop() > 300){
					if($('.slide-banner ins a').length || $('.slide-banner ins iframe').length) {
						if(!$.cookie('funtime_400_250')){
							$('.slide-banner').css('left', '15px');
						}
					}
				} else {
					$('.slide-banner').css('left', '-400px');
				}


				if($(this).scrollTop() > 100 && scrolled === false){
                    scrolled = true;
                    $('.verticals').addClass('opened');
                    $('.verticals').removeClass('closed');
                    $('#informer-block-toggle').find('span').html('<i class="fa fa-angle-down"></i>');
                    $('#informer-block-mobile-toggle').find('span').html('<i class="fa fa-angle-down"></i>');
                    //alert(scrolled);
                }else if($(this).scrollTop() < 100){
                    $('.verticals').addClass('closed');
                    $('.verticals').removeClass('opened');
                    $('#informer-block-toggle').find('span').html('<i class="fa fa-angle-up"></i>');
                    $('#informer-block-mobile-toggle').find('span').html('<i class="fa fa-angle-up"></i>');
                    scrolled = false;
                }
			});


			$("#informer-block-mobile-toggle").click(function() {
				if ($("#informer-block-mobile").hasClass('opened')) {
					$("#informer-block-mobile").addClass("closed");
					$("#informer-block-mobile").removeClass("opened");
					$("#informer-block-mobile").find("#informer-block-mobile-toggle span i").attr("class", "fa fa-angle-up");
				} else {
					$("#informer-block-mobile").addClass("opened");
					$("#informer-block-mobile").removeClass("closed");
					$("#informer-block-mobile").find("#informer-block-mobile-toggle span i").attr("class", "fa fa-angle-down");
				}
			});
			
			//Click event to scroll to top
			$('#scrollUp').click(function(){
				$('html, body').animate({scrollTop : 0},800);
				return false;
			});

			$('.hamburger-menu').on('click', function() {
				$('.bar').toggleClass('animate');
				$('.rubric-menu').toggleClass('menu-hidden');
				$('.rubric-menu').toggleClass('menu-show');
				$('.site-wrapper').toggleClass('move-left');
				$('.fl-banner').toggleClass('move-left-banner');
				$('.fr-banner').toggleClass('move-left-banner');
				$('.content-hover').toggleClass('content-hover-active');
			});

			$('.content-hover').click(function(){
				$('.bar').removeClass('animate');
				$('.rubric-menu').addClass('menu-hidden');
				$('.rubric-menu').removeClass('menu-show');
				$('.site-wrapper').removeClass('move-left');
				$('.fl-banner').removeClass('move-left-banner');
				$('.fr-banner').removeClass('move-left-banner');
				$('.content-hover').removeClass('content-hover-active');
			});


			  $('.zoom-pan')
			    // tile mouse actions
			    .on('mouseover', function(){
			      $(this).find('.photo').css({'transform': 'scale('+ $(this).attr('data-scale') +')'});
			    })
			    .on('mouseout', function(){
			      $(this).find('.photo').css({'transform': 'scale(1)'});
			    })
			    .on('mousemove', function(e){
			      $(this).find('.photo').css({'transform-origin': ((e.pageX - $(this).offset().left) / $(this).width()) * 100 + '% ' + ((e.pageY - $(this).offset().top) / $(this).height()) * 100 +'%'});
			    })
			    // tiles set up
			    .each(function(){
			      $(this)
			        // set up a background image for each tile based on data-image attribute
			        .find('.photo').css({'background-image': 'url('+ $(this).attr('data-image') +')'});
		       });

			    if ($(window).width() < 700) {
				   $('.desktop').remove();
				}

        });



	</script>

