<script>
  $(function() {
    function resetErrors(field) {
      $field = $(field);

      $field
        .removeClass('has-error')
        .find('.form_error')
        .remove();
    }

    function setError(field, error) {
      $field = $(field);
      $error = $(error);

      $field
        .addClass('has-error')
        .find('.error-container')
        .append($error);
    }

    function initCategorySearcher($categorySearcher)
    {
      searcher_inicialized = true;
      $categorySearcher.categorySearcher({
        alwaysSelectOne: true,
        extraOption: {
          name: "Ver categorias",
          redirectUrl: "https:\/\/www.habitissimo.com.br\/orcamentos\/selecionar_categoria",
        },
      });

      $categorySearcher.on('categorySearcher:error', function(ev) {
        $(this).categorySearcher('val', '');
        $(this).categorySearcher('keyVal', '');
      });

      $categorySearcher.on('categorySearcher:autocompleted', function(ev) {
        resetErrors($(this).closest('.form-group'));
      });
    }

    /**
     * Location typeahead
     */

    var $locationTypeahead = $('#location-name')

    var defaultTypeaheadConfig = {
      id_key: 'id',
      delay: 150,
      always_select_one: true,
      select_on_submit: false,
      hint: true,
      minLength: 2,
      autoselect: true
    };

    var amountLocationResults;
    var typedFirstLocation = null;

    var locationConfig = $.extend({}, defaultTypeaheadConfig, {
      source: '/p/api/autocomplete/location',
      input: '[name=location-id]',
      suggestion_template: function (value) {
        var template = $('<span>', {text: value.name});

        if (value.hint) {
          template.append(' ', $('<small>', {text: value.hint }));
        }

        return template;
      },
      prepare_request: function(query, settings) {
        settings.url = settings.url.replace('%QUERY', query) + '&';
        settings.url += $.param({ tree_level: [2]});

        return settings;
      },
      prepare_response: function (response) {
        amountLocationResults = response.length;
        if (amountLocationResults > 0) {
          typedFirstLocation = $(response).first()[0];
        }

        return response;
      }
    });

    $locationTypeahead.on('blur', function () {
      var $locationId = $("[name=location-id]");

      var locationIdValue     = $locationId.val().trim(),
          locationInputValue  = $locationTypeahead.val().trim();

      $field = $locationTypeahead.closest('.form-group');
      resetErrors($field);

      if (amountLocationResults === 0) {
        var $error = $('<span>', {
          class: 'form_error',
          text: "N\u00e3o h\u00e1 coincid\u00eancias. Indique outra regi\u00e3o"        });
        setError($field, $error);
      } else if (locationIdValue.length === 0 && locationInputValue.length > 0) {
        $locationTypeahead.typeahead('val', typedFirstLocation.name);
        $locationId.val(typedFirstLocation.id);
      }
    });

    typeaheadAutocomplete($locationTypeahead, locationConfig);

    /**
     * Category searcher plugin
     */


    var $categorySearcher = $('#category-searcher');
    var searcher_inicialized = false;
    $categorySearcher.focus();

    $categorySearcher.keypress(function() {
      if (!searcher_inicialized) {
        initCategorySearcher($(this));
        $(this).focus();
      }
    });

    $categorySearcher.focusout(function() {
      if (!searcher_inicialized) {
        initCategorySearcher($(this));
      }
    });

    /**
     * @todo Clean this mess of validations!
     */
    $('.home-searcher a.btn').on('click', function(ev) {
      ev.preventDefault();

      var params = {},
          hasErrors = false;

      /**
       * Location validation
       */
      var $typeaheadValue = $('[name=location-id]'),
          $field = $typeaheadValue.closest('.form-group');

      resetErrors($field);

      if ($typeaheadValue.val().length !== 0 && $typeaheadValue.val() != -1) {
        params[$typeaheadValue.attr('name')] = $typeaheadValue.val()
      } else {
        var $error = $('<span>', {
          class: 'form_error',
          text: "N\u00e3o h\u00e1 coincid\u00eancias. Indique outra regi\u00e3o"        });

        setError($field, $error);
        hasErrors = true;
      }

      /**
       * Category validation
       */
      $field = $categorySearcher.closest('.form-group');

      resetErrors($field);

      if ($categorySearcher.categorySearcher('isValid')) {
        params['unified-category-id'] = $categorySearcher.categorySearcher('keyVal')
      } else {
        var $error = $('<span>', {
          class: 'form_error',
          text: "N\u00e3o h\u00e1 coincid\u00eancias. "        });

        $error.append($('<a>', {
          class: 'warranty-error raquo text-bold',
          href: "https:\/\/www.habitissimo.com.br\/orcamentos\/selecionar_categoria",
          text: "Ver categorias"        }));

        setError($field, $error)
        hasErrors = true
      }

      if (!hasErrors) {
        var uriHasParameters = (ev.target.href.indexOf('?') !== -1);
        location.href = ev.target.href + (uriHasParameters ? '&' : '?') + $.param(params);
      }
    });
  });
</script>
