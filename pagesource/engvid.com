function updateBrowseTopics(clicked_topic,prev_topic,topic_name,topic_count,topic_link) {
	if (!select_topics_firstrun) {
		$("li.cat-item-"+ prev_topic +" a").removeClass('selected'); //remove selection class from current topic list item. note variable here is internal
	} else {select_topics_firstrun = false;}
	
	//below: load clicked topic's lessons. when loaded, slide into visibility
	$("#lessonlinks_topics_content").hide("slide", { direction: "right" }, 550, function() {
		$("#lessonlinks_topics_content").load("/selector-results/", {topic: clicked_topic}, function() {
			$("#lessonlinks_topics_content").show("slide", { direction: "left" }, 650);
			$("#lessonlinks_topics_title").html(function() {
				var title_output = topic_name + ' (' + topic_count + ' lessons)'; //change title of list
				return title_output;
			});
			$("#lessonlinks_select_topics_more").attr("href", topic_link); //add link to button
			$("#lessonlinks_select_topics_more").html('all '+ topic_name +' lessons');//add link to button
			previous_topic = clicked_topic; //set the PERSISTENT previous_topic to the current topic, for the next time this runs.
			jQuery(document).trigger("scroll"); // for lazy load to load images without user actually having to scroll.
		});
	}); //hide the current lesson links for existing topic
}

function updateBrowseTeachers(clicked_teacher,prev_teacher,teacher_name,teacher_link) {
	if (!select_teachers_firstrun) {
		$("a.browser_topics_select_listitem-"+ prev_teacher).removeClass('selected'); //remove selection class from current topic list item. note variable here is internal
	} else {select_teachers_firstrun = false;}
	
	//below: hide current lessons, then load clicked topic's lessons. when loaded, slide into visibility
	$("#lessonlinks_teachers_content").hide("slide", { direction: "right" }, 550, function() {
		$("#lessonlinks_teachers_content").load("/selector-results/", {teacher: clicked_teacher}, function() {
			$("#lessonlinks_teachers_content").show("slide", { direction: "left" }, 650);
			$("#lessonlinks_teachers_title").html('lessons by '+ teacher_name);
			$("#lessonlinks_select_teachers_more").attr("href", teacher_link); //add link to button
			$("#lessonlinks_select_teachers_more").html('all lessons by '+ teacher_name); //add link to button
			previous_teacher = clicked_teacher; //set the PERSISTENT previous_teacher to the current teacher, for the next time this runs.
			// jQuery(document).trigger("scroll"); // for lazy load to load images without user actually having to scroll.
		});
	}); 
