</div>

<script type="text/javascript">
    
    //Show Modal
    function showEmail() {
        $("#EmailMsg").removeClass();
        $('#MainEmail').foundation('reveal', 'open');
    }

    //Cleanup
     function EmailCleanup () {
            var eDiv = $("#MainEmail");
            var eForm = $("#EmailForm");
            eForm.trigger('reset');
            eForm.find("input[type=text], textarea").removeClass("input-validation-error").addClass("valid");
            eForm.find("div.validation-summary-errors").removeClass().addClass("validation-summary-valid").find("ul li").hide();
            eDiv.find("#EmailResultText").html("");
        }

    $(document).ready(function () {
        //Link Click Logic
        $("a.email-trigger").click(function (e) {
            e.preventDefault();
            $.getJSON($(this).attr("href"), function (data) {
                var name = data["recipName"];
                var user = data["recipUser"];
                var domain = data["recipDomain"];

                if (!name) { name = data.person.FirstName + " " + data.person.LastName; }
                if (!user) { user = data.person.Username; }
                if (!domain) { domain = "1"; }

                $("#EmailSendingTo").html(name);
                $("#EmailRecipName").val(name);
                $("#EmailRecipUser").val(user);
                $("#EmailRecipDomain").val(domain);
                showEmail();
            });
        });
        
        //Email Form Submit Logic
        $("#EmailForm").on("submit", function (event) {
            event.preventDefault();
            if ($(this).valid()) {
                console.log($(this).serialize());
                $.post("/umbraco/api/Email/Post", $(this).serialize())
                    .success(function () {
                        $("#EmailMsg").removeClass().addClass("alert-box success");
                        $("#EmailResultText").html("Mail Sent");
                    })
                    .error(function () {
                        $("#EmailMsg").removeClass().addClass("alert-box alert");
                        $("#EmailResultText").html("Failed to Send Mail");
                    });
            }
            return false;
        });
    });
</script>

