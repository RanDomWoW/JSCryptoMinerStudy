<script type="text/javascript">

    //Show Modal
    function showContact() {
        $("#ContactMsg").removeClass();
        $('#MainContact').foundation('reveal', 'open');
    }
    
    //Cleanup
    function ContactCleanup () {
        var cDiv = $("#MainContact");
        var cForm = $("#ContactForm");
        cForm.trigger('reset');
        cForm.find("input[type=text], textarea").removeClass("input-validation-error").addClass("valid");
        cForm.find("div.validation-summary-errors").removeClass().addClass("validation-summary-valid").find("ul li").hide();
        cDiv.find("#ContactResultText").html("");
    }
    
    $(document).ready(function () {
        //Contact Form Submit Logic
        $("#ContactForm").on("submit", function(event) {
            event.preventDefault();
            if ($(this).valid()) {

                console.log($(this).serialize());
                $.post("/umbraco/api/Contact/Post", $(this).serialize())
                    .success(function() {
                        $("#ContactMsg").removeClass().addClass("alert-box success");
                        $("#ContactResultText").html("Mail Sent");
                    })
                    .error(function() {
                        $("#ContactMsg").removeClass().addClass("alert-box alert");
                        $("#ContactResultText").html("Failed to Send Mail");
                    });
            }
            return false;
        });
    });
</script>


        

