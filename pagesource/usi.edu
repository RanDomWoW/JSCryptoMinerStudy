    <script>
        var activeSlide;

        $(document).ready(function() {
            $("#FeatureSlider").on("orbit:ready", function(event) {
                ShowActiveCaption();
            });

            $("#FeatureSlider").on("orbit:after-slide-change", function(event, orbit) {
                ShowActiveCaption();
            });

            $("#FeatureSlider").on("orbit:timer-stopped", function(event, orbit) {
                CaptionPaused();
            });
        });

        function ShowActiveCaption() {
            activeSlide = $("#FeatureSlider li.active .float-caption");
            if (activeSlide.hasClass("caption-right")) {
                activeSlide.animate({
                    opacity: 1,
                    right: '+=50',
                }, 1000, "linear").fadeTo(5000, 1).animate({
                    opacity: 0,
                    right: '+=300',
                }, 500, "linear", function() {
                    $(this).css("right", "20px");
                });
            } else {
                activeSlide.animate({
                    opacity: 1,
                    left: '+=50',
                }, 1000, "linear").fadeTo(5000, 1).animate({
                    opacity: 0,
                    left: '+=300',
                }, 500, "linear", function() {
                    $(this).css("left", "20px");
                });
            }
        }

        function CaptionPaused() {
            if (activeSlide != 'undefined') {
                jQuery.queue(activeSlide[0], "fx", []);
                activeSlide.stop();
                activeSlide.css("opacity", "1");
                if (activeSlide.hasClass("caption-right")) {
                    activeSlide.css("right", "70px");
                } else {
                    activeSlide.css("left", "70px");
                }
            }
        }

    </script>
    
