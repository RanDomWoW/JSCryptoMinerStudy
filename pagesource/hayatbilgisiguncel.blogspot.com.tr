background: #23A839!important;
}
/*==============================
Rightside Wrapper
==============================*/
#rightside-wrapper {
float: left;
background: #FFF;
width: 300px;
padding: 20px;
}
.sidebar {
color: #374142;
font-size: 13px;
margin: 0 0 10px;
}
.sidebar h2 {
background: #23A839;
color: #FFF;
height: 28px;
border-bottom: 4px solid #444;
border-radius: 12px;
font-size: 18px;
font-family: Segoe Print, sans-serif;
font-weight: 700;
text-transform: capitalize;
text-align: center;
margin-left: -1px;
padding-top: 10px;
}
.sidebar .widget {
color: #374142;
font-size: 13px;
margin: 5px 0 30px;
padding: 0;
}
.sidebar a {
color: #5F5F5F;
text-decoration: none;
}
.sidebar a:hover {
color: #797979;
text-decoration: none;
}
.sidebar ul {
width: 100%;
list-style-type: none;
list-style: none;
margin: 0;
padding: 0;
}
.sidebar ul li {
margin: 0 0 8px;
padding: 0 0 9px;
}
/*==============================
Rightside Search
==============================*/
#search {
padding: 15px 0;
}
#submitsearch input {
float: left;
width: 210px;
border: 1px solid #AAA;
font-family: 'Lora', serif;
font-size: 14px;
font-weight: 700;
margin: 0;
outline: 0;
padding: 9px 5px;
}
#search button {
background: #23A839;
color: #FFF;
height: 38px;
font-family: 'Lora', serif;
font-size: 14px;
font-weight: 700;
position: relative;
cursor: pointer;
overflow: hidden;
border: none;
outline: 0;
margin-left: 8px;
}
/*==============================
Popular Posts
==============================*/
.PopularPosts .widget-content ul li {
padding: 20px 0 15px;
border-bottom: solid 0px #A2A2A2;
margin: 0;
padding: 9px 0;
}
.PopularPosts img {
width: 100px;
height: 85px;
}
.PopularPosts .item-title a {
color: #3D5054;
font-family: 'Lora', serif;
font-size: 18px;
line-height: 25px;
font-weight: normal;
text-transform: capitalize;
}
.item-snippet {
display: none;
}
/*==============================
List Style Labels
==============================*/
body .list-label-widget-content li {
display: inline-block;
float: left;
color: #FFF;
background: #49B3DA;
font-size: 15px;
font-weight: 700;
line-height: 1.2em;
border-radius: 2px;
overflow: hidden;
text-decoration: none;
white-space: nowrap;
text-transform: uppercase;
padding: 10px 12px;
margin: 0 6px 6px 0;
}
body .list-label-widget-content li a {
color: #FFF;
text-transform: capitalize;
font-weight: 700;
}
body .list-label-widget-content li a:hover {
color: #222;
}
/*==============================
Lower Section
==============================*/
#footer {
background: #FF8800;
width: 100%;
border-top: 6px solid #50C3DD;
}
#lower-container {
display: block;
width: 1020px;
margin: 0 auto;
}
#lower-wrapper {
display: block;
width: 100%;
margin: 0 auto;
padding: 20px 0;
}
#lowerbar-box {
float: left;
color: #DDD;
width: 33.3%;
font-family: Arial, Verdana;
font-size: 14px;
line-height: 1.8em;
font-weight: 700;
text-align: justify;
word-wrap: break-word;
overflow: hidden;
padding-bottom: 20px;
}
.lowerbar {
margin: 0 0 -15px;
padding: 0;
}
.lowerbar .widget {
margin: 0;
padding: 10px 20px 0;
}
.lowerbar h2 {
color: #363B3F;
width: 100%;
border-bottom: 1px solid #888;
font-family: 'Lora', serif;
font-size: 18px;
text-transform: none;
font-weight: 700;
position: relative;
margin-bottom: 25px;
padding: 5px 0 15px;
}
.lowerbar ul {
color: #FFF;
list-style-type: none;
margin: 0 auto;
padding: 0;
}
.lowerbar li {
display: block;
color: #FFF;
line-height: 1.8em;
list-style-type: none;
padding: 7px 0;
}
.lowerbar li a {
color: #FFF;
font-size: 14px;
text-decoration: none;
}
.lowerbar li a:hover {
text-decoration: underline;
}
/*==============================
Credits
==============================*/
#credit {
display: block;
background: #333;
color: #FFF;
width: 100%;
height: 85px;
font-family: Roboto Slab, serif;
font-size: 15px;
border-top: 1px solid #50C3DD;
margin: 0 auto;
}
#credit-wrapper {
width: 1020px;
margin: 0 auto;
}
.credit-text {
text-align: center;
}
.credit-text a {
color: #53C6F0;
padding: 0 7px;
}
.credit-text a:hover {
color: #BAECFF;
}
/*==============================
Responsive Design
==============================*/
@media all and (min-width: 240px) and (max-width:767px) {
#outer-wrapper, .quotation-block {
width: 365px;
}
#featured-quotation {
height: 330px;
}
.readmore {
margin-right: 30px;
}
#related-posts {
height: 900px;
}
.related-body {
padding: 72px 72px 0!important;
}
#rightside-wrapper {
padding: 0 32px;
}
#lower-container, #credit-wrapper {
width: 365px;
}
#lowerbar-box {
width: 100%;
}
}
@media all and (min-width: 768px) and (max-width:1023px) {
#outer-wrapper, .quotation-block {
width: 695px;
}
#featured-quotation {
height: 260px;
}
.related-body {
padding: 17.5px!important;
}
#rightside-wrapper {
padding: 0 197px;
}
#lower-container, #credit-wrapper {
width: 695px;
}
#lowerbar-box {
padding-bottom: 30px;
width: 50%;
}
}
@media all and (min-width: 240px) and (max-width:1023px) {
#main-header, #header {
width: 100%;
}
#Header1_headerimg {
max-width: 280px;
}
.titlewrapper {
width: 100%;
}
#main-wrapper {
width: 100%!important;
border-bottom: 2px solid #999;
border: 0;
}
#left-ad, #bm-social, #author-box {
display: none;
}
#navigation {
background-color: transparent;
height: auto;
}
#navigation ul {
display: none;
float: none;
width: 100%;
height :auto;
overflow: hidden;
}
#navigation li {
float: none;
margin: 0;
}
#navigation li a {
color: #555;
font-weight: 700;
border-top: 1px solid #F5F5F5;
border-bottom: 1px solid #C5C5C5;
padding: 10px;
}
.home {
background: none;
}
#navigation .home a {
width: auto;
text-indent: 0;
}
.active {
border-bottom: 0;
}
#navigation li a:hover {
background-color: #F7F7F7;
}
#navigation .home a:hover {
background-image: none;
background-color: #F7F7F7;
}
#navigation li.sub-indicator::after {
border-color: transparent;
}
#navigation ul.sub-menu {
display: block;
width: 100%;
position: static;
box-shadow: none;
-moz-box-shadow: none;
-webkit-box-shadow: none;
-khtml-opacity: 1;
-moz-opacity: 1;
opacity: 1;
visibility: visible;
}
#navigation ul.sub-menu li {
border-bottom: 1px solid #888;
}
#navigation ul.sub-menu a {
color: #FFF;
font-weight: 700;
padding-left: 45px;
border: 0;
}
#navigation li a, #navigation .home a, #navigation ul.sub-menu a, #navigation li.sub-indicator a {
background-image: url(http://4.bp.blogspot.com/-8JN3CC3DRqI/UIeVS92mK7I/AAAAAAAAGKI/1yWTcqwwoeY/s1600/rightarrow.png);
background-repeat: no-repeat;
background-position: 95% 50%;
}
#navigation li a:hover, #navigation .home a:hover, #navigation ul.sub-menu a:hover, #navigation li.sub-indicator a:hover {
background-image: url(http://4.bp.blogspot.com/-8JN3CC3DRqI/UIeVS92mK7I/AAAAAAAAGKI/1yWTcqwwoeY/s1600/rightarrow.png)!important;
background-repeat: no-repeat;
background-position: 98% 50%;
}
#mobi-navigation, #navigation-mobi {
display: block;
}
#mobi-navigation {
color: #FFF;
background-color: #333;
background-image: url(https://4.bp.blogspot.com/-AEMtJse1TuY/WChBK-wEjEI/AAAAAAAABiY/jXmlNh1EU5EQ88SttTfkDKiMkAvYKhNIwCLcB/s1600/logo2.png);
background-repeat:no-repeat;
background-position:97% 50%;
height: auto;
padding: 13px;
}
}
/*==============================
Status Message
==============================*/
.status-msg-wrap {
width: 96%;
}
.status-msg-body {
background: #FAFAFA;
font-size: 17px;
line-height: 30px;
border: 1px solid #AAA;
position: relative;
margin-top: 20px;
}
.status-msg-border {
display: none;
}
/*==============================
Feed Links
==============================*/
.feed-links {
display: none;
}
/*==============================
Selection & Scroll Bar
==============================*/
::selection {
background: #49B3DA;
color: #FFF;
}
::-moz-selection {
