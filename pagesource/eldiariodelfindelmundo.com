<script>
      
        
        // Chequeo si clickean un item que tiene hijos
        $(".li_items_menu_principal.hijos a.enlaces_principales").click(function(event){
            event.preventDefault();
            event.stopPropagation();
            
            /*
            if(!$(this).parent().parent().hasClass("mobile")){
                cerrar_items_abiertos_menu_principal();    
            }
            */
            
            var ul_hijos = $(this).parent().find(".ul_items_hijos_menu_principal");
            
           
            // si YA esta desplegado
            if(ul_hijos.hasClass("desplegado")){
                
                ul_hijos.removeClass("desplegado");
                ul_hijos.css("height","0px");
                if($("#menu_principal").hasClass("mobile")){
                    $("#menu_principal").css("height",alto_menu_principal_desplegado+"px");
                }
                
            
            // si NO esta desplegado
            }else{
                
                
                
                ul_hijos.addClass("desplegado");    
                
                var alto_total = 0;
                $(this).parent().find(".ul_items_hijos_menu_principal .li_items_hijos_menu_principal").each(function(e){
                    var item = $(".li_items_hijos_menu_principal").eq(e);
                    alto_total = parseInt(alto_total) + parseInt(item.css("height"));                     
                }); 
                
                
                if(ul_hijos.parent().hasClass("buscador")){
                    alto_total = 45;
                    $("#buscar").focus();
                }
                
                if(ul_hijos.hasClass("menu_largo") && $("#menu_principal").hasClass("mobile")){
                    alto_total = parseInt(ul_hijos.css("max-height"));
                }
                
                ul_hijos.css("height",alto_total+"px");
                
                if($("#menu_principal").hasClass("mobile")){
                    var alto_menu_principal = parseInt($("#menu_principal").css("height"));
                    $("#menu_principal").css("height",(alto_menu_principal+alto_total)+"px");
                }
                
               
            }
            
        });
        
        // Accion de cerrar el menu hijo al clickear afuera
        $("html").click(function(){ 
           cerrar_items_abiertos_menu_principal();            
        })
        
        $("#buscar").click(function(event){
            event.preventDefault();
            event.stopPropagation();
        })
       
        
        function cerrar_items_abiertos_menu_principal(){
            
            $(".ul_items_hijos_menu_principal.desplegado").each(function(){
                $(this).parent().find(".enlaces_principales").click();
            });
            
        }
        
        
        // Declaro la variable global del ancho minimo del menu
        var ancho_minimo_menu_principal = 0;
        var alto_menu_principal_desplegado = 0;
    
        // Chequeo la seccion actual para mostrar visualmente en el menu
        var seccion_actual = 'portada';
        $(".ul_items_menu_principal li").removeClass("actual");
        $(".ul_items_menu_principal li[seccion="+seccion_actual+"]").addClass("actual");
        
        // Accion de colapsar y desplegar el menu en mobile
