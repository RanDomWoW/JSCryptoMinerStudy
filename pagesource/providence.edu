	<!-- Responsive Nav -->
	<script>
		(function(){
			$(document).ready(function () {
				var siteUrl = window.location.protocol + '//' + window.location.host;
				var byTheNumbersRequest = new Rest.Requests.Request(siteUrl);
				var homepageRequest = new Rest.Requests.Request(siteUrl + '/marketing-communications');				
				
				var fImage = 'FeatureImage', fBodyText = 'FeatureBody', fNavUrl ='FeatureNavigationUrl';				
	
				var byTheNumberOptions = {
					htmlTemplate: '<li class="col-1-3" data-clickurl="{{LinkURL}}"><i class="{{IconType}}"></i><h4>{{Title}}</h4></li>',
					listName: 'By The Numbers',
					callback: function (html) {
						document.querySelector('#list-byTheNumbers').innerHTML = html;
						var list = document.querySelectorAll('#list-byTheNumbers li');
						Array.prototype.forEach.call(list, function (el, i) {
							var listUrl = el.getAttribute('data-clickurl');
							if (listUrl !== '') {
								el.style.cursor = 'pointer';
								el.title = listUrl;
								el.addEventListener('click', function () {
									window.location.href = listUrl;
								});
							}
						});
	
					},
					fields: ['Title', 'IconType', 'LinkURL']
				};
				
				var homepageOptions = { 
					listName: 'Homepage Content',
					callback: function(data) {
						
						if(data.length > 0) {
							var item = data[0];
							document.querySelector('#feature-container').style.backgroundImage = 'url(' + item[fImage].Url + ')';
							document.querySelector('#feature-header').textContent = item['Title'];
							document.querySelector('#feature-body').textContent = item[fBodyText];
							document.querySelector('#feature-link').href = item[fNavUrl].Url;
						}
					 },
					 fields: ['Title', fImage, fBodyText, fNavUrl]
					
				};
	
				byTheNumbersRequest.process(byTheNumberOptions);
								
				homepageRequest.process(homepageOptions);
			});
			
		})();
		
	</script>
