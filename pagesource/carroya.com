									 $("#linkUno").attr("href",""+json_[0].enlace);

									 $("#imgArticulo").attr("src",jsonimg_.thumbnail);
									 $("#imgArticulo").attr("alt",jsonimg_.titulo);

									 $("#linkTitulo").attr("href",json_[0].enlace);
									 $("#linkTitulo").attr("title",jsonimg_.titulo);
									 $("#linkTitulo").html(json_[0].titulo);

									 $("#linkLead").attr("href",json_[0].enlace);
									 $("#linkLead").html(json_[0].lead);

									 $("#linkContinuar").attr("href",json_[0].enlace);


									 $("#refTempNews").addClass("viewNews");
									 $("#seccionNoticias").show();
