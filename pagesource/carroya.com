function cargarPautasHome(){
	$.ajax({
		url:'/pautas/ajax/home.do',
		type:'POST',
		dataType:'json',
		success:function(rpa){
			if(rpa!=0){
				$.each(rpa, function(k,v){
					if($("#seccionIdPauta").lenght>0 && $("#seccionIdPauta").val()!="" && $("#seccionIdPauta").val()!=undefined){
						seccion=$("#seccionIdPauta").val();
						putScriptPauta(v.seccion,v.formato,v.dispositivo=="m",seccion);
					}else{
						putScriptPauta(v.seccion,v.formato,v.dispositivo=="m");
					}
						
				});		
			}
		}
	});
}
