$(document).ready(function(){
	function isScrolledIntoView(elem)
	{
	    var offSetPosElement = $(elem).offset().top
	    var posTopScroll = $(document).scrollTop();

	    var diffPos = offSetPosElement - posTopScroll;
	    return (diffPos <= 500 );
	}
	$(document).on('scroll', function(event){
	      event.stopPropagation();
	      if(!$("#refTempNews").is(".viewNews")){
		      if(isScrolledIntoView($("#refTempNews"))){
