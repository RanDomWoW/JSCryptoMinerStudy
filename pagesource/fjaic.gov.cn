<script type="text/javascript">
var $div,$last,$first,intV;
function LunBoDiv(){
  if($div.attr("id") == $last.attr("id")){
    $div.hide();
    $div = $first;
    $div.fadeIn();
  }else{
    $div.hide();
    $div.next().fadeIn();
    $div = $div.next();
  }
  chnPic($div.index());
}

function backBoDiv(){
  if($div.attr("id") == $first.attr("id")){
    $div.hide();
    $div = $last;
    $div.fadeIn();
  }else{
    $div.hide();
    $div.prev().fadeIn();
    $div = $div.prev();
  } 
  chnPic($div.index());
}

