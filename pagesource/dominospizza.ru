    <div class="clear"></div>
    <div class="basket-content">
        <noindex>
            <div class="column column-data">
                <div class="no-items" ng-show="!cartItems.length">Ваша корзина пуста</div>
                <div ng-repeat="item in cartItems | orderBy: 'common.productId'" class="item">
                    <div class="name pizza" ng-if="item.PizzaId">{{ pizzaCartDescription(item) }}</div>
                    <div class="name {{findCategory(common.findProduct('Other', item.ProductId).groupId).id | lowercase}}" ng-if="item.ProductId && !item.PizzaId">
                        {{ common.findProduct("Other", item.ProductId).name.ru }}
                    </div>
                    <div class="name coupon" ng-if="item.ReferenceId">{{ common.findCoupon(item.ReferenceId).name.ru }}</div>
                    <div class="price">{{ common.formatMenuPrice(item.Price) }} <s class="rub"></s> <s ng-show="item.ReferenceId">{{ common.formatMenuPrice(item.DiscountlessPrice) }} <s class="rub"></s></s></div>
                    <a href="javascript:;" title="Удалить из корзины" class="delete" ng-click="showRemoveConfirm = true">&times;</a>
                    <noindex>
                    <div class="confirm-popup" ng-show="showRemoveConfirm">
                        Удалить
                        <span ng-if="item.ReferenceId" class="remove-name">{{ common.findCoupon(item.ReferenceId).name.ru }}</span>
                        <span ng-if="item.ProductId && !item.PizzaId" class="remove-name">{{ common.findProduct("Other", item.ProductId).name.ru }}</span>
                        <span ng-if="item.PizzaId" class="remove-name">{{pizzaCartDescription(item)}}</span>
                        из корзины?
                        <div class="remove-btns">
                            <a href="javascript:;" class="remove-yes" ng-click="cartRemove($event, common.categoryByItem(item), common.productId(item), item.SizeId, item.OrderOptions)">Да</a>
                            <a href="javascript:;" class="remove-no" ng-click="showRemoveConfirm = false">Нет</a>
                        </div>
                    </div>
                    </noindex>
                    <div class="quantity" ng-class="{ quantity_one: item.Quantity == 1 }">
                        
                        <input type="text" maxlength="2" name="quantity" ng-model="item.Quantity" ng-change="cartQuantityChanged(item, {{ item.Quantity }})" disabled="disabled" />
                        <a href="javascript:;" class="increase">&plus;</a><a href="javascript:;" class="decrease" ng-click="item.Quantity == 1 ? showRemoveConfirm = true : showRemoveConfirm = false">&minus;</a>
                    </div>
                    <noindex>
                    <div class="hidden cart-ext-coupon">
                        <table class="table-cart-ext">
                            <tr ng-repeat="couponItem in (item.OrderItems || [item])">
                                <td ng-show="item.PizzaId">{{ couponItem.Quantity }} пицца<br /></td>
                                <td class="table-cart-ext__quantity" ng-show="item.ReferenceId">({{ couponItem.Quantity }}) </td>
                                <td>{{ couponItem.PizzaId ? pizzaCartDescription(couponItem) : common.findProduct("Other", couponItem.ProductId).name.ru }}</td>
                                <td>
                                    <span ng-repeat="option in addedOptions(couponItem) | orderBy: 'OptionId'">
                                        {{ findById(products.pizzaOptions, option.OptionId).name.ru }}
                                    </span>
                                    <span ng-repeat="option in removedOptions(couponItem)" class="canceled-option">
                                        {{ findById(products.pizzaOptions, option.OptionId).name.ru }}
                                    </span>
                                </td>
                                <td class="price" ng-show="item.ReferenceId">
                                    {{ common.formatMenuPrice(couponItem.Price) }} <s class="rub"></s>
                                    <span class="discountless_price">(<s>{{ common.formatMenuPrice(couponItem.DiscountlessPrice) }}</s> <s class="rub"></s>)</span>
                                </td>
                            </tr>
                        </table>
                    </div>
                    </noindex>
                </div>
            </div><!--column-data-->
            <div class="column column-action">
                <div class="total">Итого:<span>{{ common.formatMenuPriceDetails(cartPrice) }}</span> <s class="rub"></s></div>
                <div class="action-buttons">
                    <div class="buttons" ng-show="cartItems.length">
                        <a class="button add-favorites" ng-class="{ added: cart.favoriteId }" href="javascript:;" ng-click="showAddFavoriteOrder(cart.Order, true)">{{ cart.favoriteId ? "Удалить из избранного" : "Добавить в избранное" }}</a>
                        <a href="javascript:;" class="button empty-basket" ng-click="clearCart()">Очистить корзину</a>
                    </div>
                    <a href="javascript:;" ng-click="cartItems.length && orderNow()" class="button make-order" ng-class="{ 'disable': !cartItems.length }">Оформить заказ</a>
                </div>
            </div><!--column-action-->
        </noindex><!--basket-content-->
    </div><!--basket-mini-->
