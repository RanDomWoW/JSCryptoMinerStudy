    				$('body').addClass('active-tab-menu');
    
    				var $activeTab = $tabFlyout.find('div' + $tabName);
    				$activeTab.siblings(':visible').fadeOut();
    				$activeTab.fadeIn('fast', function(){
    				    if($tabName == '#academic'){
    				        setTimeout(function(){ window.dispatchEvent(new Event('resize')); }, 1000);
    				        initialize_owl($('.academics-slider'));
    				    }
    					jQuery('#noeffects').hide();
    				});
    			} else{
    				jQuery('body').removeClass('active-tab-menu');
    
    				$tabFlyout.slideUp(function () {
    					$tabFlyout.children('.wrapper > div').hide();
    					/*if ($(".owl-carousel")[0]){
                            destroy_owl($('.academics-slider'));
                        } */
    					jQuery('#noeffects').show();
    				});
    			}
    		} else {
    			jQuery('body').addClass('active-tab-menu');
    			var $activeTab = $tabFlyout.find('div' + $tabName);
    			$activeTab.show().siblings().hide().end().end().slideDown('fast', function () {
    				if($tabName == '#academic'){
    			        initialize_owl($('.academics-slider'));
    			    }
    				jQuery('#noeffects').hide();
    			});
    		}
    		function initialize_owl(el) {
                el.owlCarousel({
                    loop: true,
    				nav: true,
    				dots: false,
    				lazyLoad: true,
    				margin:20,
    				smartSpeed: 500,
    				autoplay: true,
    				//navText: ['<span class="fa fa-angle-left"></span>', '<span class="fa fa-angle-right"></span>'],
                    responsive: {
                    0: {
                        items: 1
                    },
                    600: {
                        items: 2
                    },
                    800: {
                        items: 4
                    },
                    1024: {
                        items: 4
                    },
                    1200: {
                        items: 4
                    }
                }
                });
            }
    	});
        var lastindex = window.location.href.substr(window.location.href.lastIndexOf('/') + 1);
        if (lastindex == '#schools') {
            jQuery(".tab-menu li > a[href^='#schools']").trigger('click');
        } else if (lastindex == '#programs') {
            jQuery(".tab-menu li > a[href^='#programs']").trigger('click');
        } else if (lastindex == '#academic') {
            jQuery(".tab-menu li > a[href^='#academic']").trigger('click');
        }
        jQuery(document).ready(function($) {
            
            if ( $(window).width() > 739) { 
             setTimeout(function() {
        jQuery("#phplive_btn_1479248363").trigger('click');
    }, 10000);
  
            } 
			var socialSource = document.getElementById('social_tab');
			socialSource.addEventListener('click', process_socials);
			function process_socials() {
				var data = {
					action: 'process_shortcode_on_tab_click'
				}
				$.post('https://www.viu.edu/wp-admin/admin-ajax.php',data).done(function(response) {
					$('#show_board').html(response);
					socialSource.removeEventListener('click', process_socials);
					setTimeout(function() {
    					window.dispatchEvent(new Event('resize'));
    				}, 1000);
				});
			}
		});
    });
</script>
