            str+='<div class="project"><a href="'+data[i].href+'" title="'+data[i].name+'" target="_blank"><img src="'+data[i].url+'" alt="'+data[i].name+'"/></a></div>';
        }
        str+=str;
        $(".project-detail").css("width",(data.length+1)*100+"%").append('<div class="project"><a href="'+data[0].href+'" target="_blank" title="'+data[0].name+'"><img src="'+data[0].url+'" alt="'+data[0].name+'"/></a></div>');
        $(".strip-l").append(str);
        $(".strip-r").append(str);
        function bannerAnimate() {
            if(parseInt($("body").css("width"))>=1366){
                $(".strip-l").css({"width":1070*data.length+"px","margin-right":-((data.length-1)*535+187)+"px"});
                $(".strip-r").css({"width":1070*data.length+"px","margin-left":"-185px"});
                c=parseInt($(".project-detail .project").css("width"));
                l=$(".strip-l").css("margin-right");
                r=$(".strip-r").css("margin-left");
                setTimeA();
            }else{
                $(".strip-l").css({"width":1030*data.length+"px","margin-right":-((data.length-1)*535+218)+"px"});
                $(".strip-r").css({"width":1030*data.length+"px","margin-left":"-216px"});
                c=parseInt($(".project-detail .project").css("width"));
                l=$(".strip-l").css("margin-right");
                r=$(".strip-r").css("margin-left");
                setTimeB();
            }
        }
        bannerAnimate();
        //banner
        function setTimeA() {
            window.clearTimeout(time);
            time=window.setTimeout(function () {
                $(".project-detail").animate({marginLeft:'-=670px'},700,"easeInOutExpo");
                $(".strip-l").animate({marginRight:'+=535px'},700,"easeInOutExpo");
                $(".strip-r").animate({marginLeft:'-=535px'},700,"easeInOutExpo",function () {
                    if($(".project-detail").css("margin-left")==-data.length*c+"px"){
                        $(".project-detail").css("margin-left","0px");
                        $(".strip-l").css("margin-right",l);
                        $(".strip-r").css("margin-left",r);
                    }
                });
                setTimeA();
            },2000)
        }
        function setTimeB() {
            window.clearTimeout(time);
            time=window.setTimeout(function () {
                $(".project-detail").animate({marginLeft:'-=580px'},700,"easeInOutExpo");
                $(".strip-l").animate({marginRight:'+=515px'},700,"easeInOutExpo");
                $(".strip-r").animate({marginLeft:'-=515px'},700,"easeInOutExpo",function () {
                    if($(".project-detail").css("margin-left")==-data.length*c+"px"){
                        $(".project-detail").css("margin-left","0px");
                        $(".strip-l").css("margin-right",l);
                        $(".strip-r").css("margin-left",r);
                    }
                });
                setTimeB();
            },2000)
        }
        //滚动
        var h=window.innerHeight;
        window.onresize=function () {
            h=window.innerHeight;
            $(".fullPage").css("height",h+"px");
            window.clearTimeout(time);
            $(".project-detail").css("margin-left","0px");
            $(".strip-l").css("margin-right",l);
            $(".strip-r").css("margin-left",r);
            bannerAnimate();
        };
        $(".fullPage").css("height",h+"px");
        var i=1,f=0;
        var scrollTime;
        function scrollBody(f) {
            $(document.body).stop(true,true).animate({scrollTop:f*h+'px'},1000,"easeInOutExpo",function () {
                //i=1;
            });
            $(document.documentElement).stop(true,true).animate({scrollTop:f*h+'px'},1000,"easeInOutExpo",function () {
                //i=1;
            });
        }
        //锚点
        $(".dots ul li").eq(0).addClass("dlActive");
        $(".dots ul li").on("click",function(){
            var index=$(this).index();
            f=index;
            $("body,html").animate({scrollTop:f*h+'px'},500);
        });
        var scrollFunc=function(e){
            var e=e || window.event;
            e.preventDefault();
            if(e.wheelDelta && i){
                i=0;
                if(e.wheelDelta<0){
                    f=f==5 ? 5 : f+1;
                    //f++;
                    scrollBody(f);
                }else {
                    f=f==0 ? 0 : f-1;
                    //f--;
                    scrollBody(f);
                }
            }else if(e.detail && i){
                i=0;
                if(e.detail>0){
                    f=f==5 ? 5 : f+1;
                    scrollBody(f);
                }else {
                    f=f==0 ? 0 : f-1;
                    scrollBody(f);
                }
            }
            scrollTime=window.setTimeout(function () {
                i=1;
                window.clearTimeout(scrollTime);
            },500)
        };
        //事件监听及兼容
        function addEventHandler(type,func){
            if(document.addEventListener){
                //监听IE9，谷歌和火狐
                document.addEventListener(type, func, false);
            }else if(document.attachEvent){
                document.attachEvent("on" + type, func);
            }else{
                document["on" + type] = func;
            }
        }
        addEventHandler('DOMMouseScroll',scrollFunc);
        addEventHandler('mousewheel',scrollFunc);
        addEventHandler('keydown',function (e) {
            var e=e||window.event;
            var i=e.keyCode;
            if(i==40 || i==34 || i==32){
                e.preventDefault();
                f=f==5 ? 5 : f+1;
                scrollBody(f);
            }else if(i==38 || i==33){
                e.preventDefault();
                f=f==0 ? 0 : f-1;
                scrollBody(f);
            }else if(i==36){
                e.preventDefault();
                f=0;
                scrollBody(f);
            }else if(i==35){
                e.preventDefault();
                f=5;
                scrollBody(f);
            }
        });
    })
</script>
