    <script>
        /* Feature Case Study Slider */
        var $gallery = $('.single-item');
        var slideCount = null;
        function setSlideCount() {
            var $el = $('.cs_count').find('.total');
            $el.text(slideCount);
        }
        function setCurrentSlideNumber(currentSlide) {
            var $el = $('.cs_count').find('.current');
            $el.text(currentSlide + 1);
        }
        $(document).ready(function () {
            $gallery.slick({
                appendArrows: $(".controls"),
                prevArrow: '<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 12.469 16.07" class="arrow"><path d="M47.886,1993.38l-7.2-8.1,7.2-7.97H42.621l-7.2,7.97,7.2,8.1h5.265Z" transform="translate(-35.406 -1977.31)" /></svg>',
                nextArrow: '<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 12.469 16.07" class="arrow"><path d="M238.886,1985.28l-7.2-7.97h-5.265l7.2,7.97-7.2,8.1h5.265Z" transform="translate(-226.406 -1977.31)" /></svg>'
            });

            // On before slide change
            $gallery.on('afterChange', function (event, slick, currentSlide, nextSlide)
            {
                var currentLink = $(slick.$slides.get(currentSlide)).data('link');
                var openinnewwindow = $(slick.$slides.get(currentSlide)).data('openinnewwindow');

                $("#featureLink").attr("href", currentLink);
                if (openinnewwindow == "True")
                {
                    $("#featureLink").attr("target", "_blank");
                }
                else
                {
                    $("#featureLink").removeAttr("target");
                }
            });
        });
        $gallery.on('init', function (event, slick) {
            slideCount = slick.slideCount;
            setSlideCount();
            setCurrentSlideNumber(slick.currentSlide);
            var currentLink = $(slick.$slides.get(slick.currentSlide)).data('link');
            var openinnewwindow = $(slick.$slides.get(slick.currentSlide)).data('openinnewwindow');

            $("#featureLink").attr("href", currentLink);
            if (openinnewwindow == "True")
            {
                $("#featureLink").attr("target", "_blank");
            }
            else
            {
                $("#featureLink").removeAttr("target");
            }
        });
        $gallery.on('beforeChange', function (event, slick, currentSlide, nextSlide) {
            setCurrentSlideNumber(nextSlide);
        });

        /* Technologies Slider */
        $('.technologies').slick({
            /* Desktop Up  */
            centerMode: false,
            slidesToShow: 4,
            arrows: true,
            prevArrow: '<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16.969 28.313" class="previous"><path d="M0.863,15.037L15.006,29.181l2.807-2.807L6.491,15.051,17.84,3.7,15.019,0.881Z" transform="translate(-0.875 -0.875)"/></svg>',
