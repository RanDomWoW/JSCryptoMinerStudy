            responsive: [
              /* Tablet Large Up */
              {
                  breakpoint: 1200,
                  settings: {
                      slidesToShow: 3
                  }
              },
              /* Tablet Small Up */
              {
                  breakpoint: 984,
                  settings: {
                      slidesToShow: 2
                  }
              },
              /* Mobile Up */
              {
                  breakpoint: 768,
                  settings: {
                      slidesToShow: 1
                  }
              }
            ]
        });

