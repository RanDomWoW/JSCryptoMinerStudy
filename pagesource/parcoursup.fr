		<script>
			function hoverIpad(idElementClick)
			{
				var iOS = !!navigator.platform && /iPad|iPhone|iPod/.test(navigator.platform);
				if(iOS)
				{
					if(document.getElementById(idElementClick).style.display == "" || document.getElementById(idElementClick).style.display == "none")
					{
						document.getElementById(idElementClick).style.display = "block";
					}
					else
					{
						document.getElementById(idElementClick).style.display = "none";
					}
				}
			}

			// Gestion du menu hoover au clavier
			var menuItems = document.querySelectorAll('.topMenu li.has-submenu');
			var timer, timer2;
			Array.prototype.forEach.call(menuItems, function(el, i)
			{
				el.addEventListener(
					"mouseover", function(event)
					{
						this.className = "has-submenu open";
						this.firstChild.nextElementSibling.setAttribute('aria-expanded', "true");
						clearTimeout(timer);
					}
				);
				el.addEventListener(
					"mouseout", function(event)
					{
						timer = setTimeout(function(event)
							{
								var listeMenu = document.querySelectorAll(".topMenu .has-submenu.open");
								Array.prototype.forEach.call(listeMenu, function(el, i)
								{
									el.className = "has-submenu";
									el.firstChild.nextElementSibling.setAttribute('aria-expanded', "false");
								})
							}, 1000
						);
					}
				);
				el.querySelector('a').addEventListener(
					"click",  function(event)
					{
						if (this.parentNode.className == "has-submenu")
						{
							this.parentNode.className = "has-submenu open";
							this.setAttribute('aria-expanded', "true");
						}
						else
						{
							this.parentNode.className = "has-submenu";
							this.setAttribute('aria-expanded', "false");
						}
						event.preventDefault();
					}
				);
				var links = el.querySelectorAll('a');
				Array.prototype.forEach.call(links, function(el, i){
					el.addEventListener("focus", function() {
						if (timer2) {
							clearTimeout(timer2);
							timer2 = null;
						}
					});
					el.addEventListener("blur", function(event) {
						timer2 = setTimeout(function () {
							var listeMenu2 = document.querySelectorAll(".topMenu .has-submenu.open");
							if (listeMenu2)
							{
								Array.prototype.forEach.call(listeMenu2, function(el, i)
								{
									el.className = "has-submenu";
									el.firstChild.nextElementSibling.setAttribute('aria-expanded', "false");
								})
							}
						}, 10);
					});
				});
			});

			// Gestion du pliage/depliage au clavier des section via label
			var labelList = document.querySelectorAll('label');
			var elementTmp;
			Array.prototype.forEach.call(labelList, function(el, i)
			{
				el.parentNode.addEventListener(
					"click",  function(event)
					{
						if (document.getElementById(el.htmlFor).checked)
						{
							document.getElementById(el.htmlFor).checked = false;
							el.firstChild.setAttribute('aria-expanded', "false");
							elementTmp=el.parentNode.nextElementSibling;
							while (elementTmp != null)
							{
								elementTmp.setAttribute('aria-hidden', "true");
								elementTmp=elementTmp.nextElementSibling;
							}
						}
						else
						{
							document.getElementById(el.htmlFor).checked = true;
							el.firstChild.setAttribute('aria-expanded', "true");
							elementTmp=el.parentNode.nextElementSibling;
							while (elementTmp != null)
							{
								elementTmp.setAttribute('aria-hidden', "false");
								elementTmp=elementTmp.nextElementSibling;
							}

							/*var regexp = /[A-E]/gi;
							document.getElementById("sel.htmlFor.substring(0).match(regexp)).setAttribute('aria-hidden', "false");*/
						}
						event.preventDefault();
					}
				);
			});
		</script>
