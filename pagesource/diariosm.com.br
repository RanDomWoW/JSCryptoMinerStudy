    //* Funcão para mover as colunas aleatoriamente de um divisor de colunas
    function fncDivisorColunasAleatorio()
    {

        $(".suita-elementos-divisor-colunas").each(
            function(index)
            {

                var $colunas = $(this).find("div[suita-tipo='coluna']");

                for(var i = 0; i < $colunas.length; i++){
                    var posicao1 = Math.floor(Math.random() * $colunas.length -1) + 1;
                    var posicao2 = Math.floor(Math.random() * $colunas.length -1) +1;

                     $colunas.eq(posicao1).before($colunas.eq(posicao2));

                 }

            }
        );

    }

    function carregarEdicaoDestaque()
    {

      $(".suita-elementos-destaque .destaque-item-artigo .cartola-materia,.suita-elementos-destaque .destaque-item-artigo .titulo-materia,.suita-elementos-destaque .destaque-item-artigo .resumo-materia").each(
          function(index)
          {

            //* Define os atributos de cada elemento que pode ser editado
            $(this).attr("contenteditable",true);
            $(this).attr("destaqueid",$(this).closest(".suita-elementos-destaque").attr("suita-contentid"));
            $(this).attr("destaqueartigoid",$(this).closest(".destaque-item-artigo").attr("suita-contentid-artigo"));
            $(this).parent().click(function () {return false;});

            if($(this).hasClass("cartola-materia"))
            {
              $(this).attr("destaqueelemento","cartola");
            }

            if($(this).hasClass("titulo-materia"))
            {
              $(this).attr("destaqueelemento","titulo");
            }

            if($(this).hasClass("resumo-materia"))
            {
              $(this).attr("destaqueelemento","resumo");
            }

            $(this).attr("destaqueconteudoanterior",$(this).text());

            $(this).bind("DOMSubtreeModified",function(){

              var $novoArtigo = false;
              var $destaqueArtigo;

              //* Define os atributos do elemento editado
              var $destaqueId = $(this).attr("destaqueid");
              var $destaqueArtigoId = $(this).attr("destaqueartigoid");
              var $destaqueElemento = $(this).attr("destaqueelemento");

              //* Carrega os novos elementos do artigo
              var $elementCartola = $(this).closest(".destaque-item-artigo").find(".cartola-materia");
              var $elementTitulo = $(this).closest(".destaque-item-artigo").find(".titulo-materia");
              var $elementResumo = $(this).closest(".destaque-item-artigo").find(".resumo-materia");

              //* Busca o elemento do destaque caso já esteja alterado anteriormente
              $destaqueArtigo = $("#edicaoDestaque").find("div[destaqueid='" + $destaqueId + "'][destaqueartigoid='" + $destaqueArtigoId + "']");

              //* Verifica se não encontrou nenhum elemento
              if($destaqueArtigo.length == 0)
              {

                $novoArtigo = true;

                //* Insere o artigo na lista de itens modificados
                $destaqueArtigo = $("<div class='destaqueItem'></div>");
                $destaqueArtigo.attr("destaqueid",$destaqueId);
                $destaqueArtigo.attr("destaqueartigoid",$destaqueArtigoId);

                var $novoArtigoHtml = "<div class='row'><div class='col col-md-12'><div class='pull-right'><h6><b>Destaque:</b> " + $destaqueId + " <b>Artigo:</b>" + $destaqueArtigoId + "</h6></div></div></div>" +
                "<div style='border-left: green 3px solid;padding-left: 15px;margin-bottom:2px'><div class='row itemArtigo' style='display:none'><div class='col col-md-12'><h5><u class='conteudoCartolaNovo'></u></h5></div></div>" +
                  "<div class='row itemArtigo' style='display:none'><div class='col col-md-12'><h5><b class='conteudoTituloNovo'></b></h5></div></div>" +
                  "<div class='row itemArtigo' style='display:none'><div class='col col-md-12'><h5 class='conteudoResumoNovo'></h5></div></div></div>" +
                  "<div style='border-left: gray 3px solid;padding-left: 15px;'><div class='row itemArtigo' style='display:none'><div class='col col-md-12'><h5><u class='conteudoCartolaAnterior'></u></h5></div></div>" +
                    "<div class='row itemArtigo' style='display:none'><div class='col col-md-12'><h5><b class='conteudoTituloAnterior'></b></h5></div></div>" +
                    "<div class='row itemArtigo' style='display:none'><div class='col col-md-12'><h5 class='conteudoResumoAnterior'></h5></div></div></div>";

                $destaqueArtigo.html($novoArtigoHtml);

                if($elementCartola.length > 0)
                {
                  var $conteudoCartolaAnterior = $destaqueArtigo.find(".conteudoCartolaAnterior");
                  var $cartolaAnterior = $elementCartola.attr("destaqueconteudoanterior");
                  $conteudoCartolaAnterior.html($cartolaAnterior);
                  $destaqueArtigo.find(".conteudoCartolaNovo").closest(".itemArtigo").show();
                  $conteudoCartolaAnterior.closest(".itemArtigo").show();
                }

                if($elementTitulo.length > 0)
                {
                  var $conteudoTituloAnterior = $destaqueArtigo.find(".conteudoTituloAnterior");
                  var $tituloAnterior = $elementTitulo.attr("destaqueconteudoanterior");
                  $conteudoTituloAnterior.html($tituloAnterior);
                  $destaqueArtigo.find(".conteudoTituloNovo").closest(".itemArtigo").show();
                  $conteudoTituloAnterior.closest(".itemArtigo").show();
                }

                if($elementResumo.length > 0)
                {
                  var $conteudoResumoAnterior = $destaqueArtigo.find(".conteudoResumoAnterior");
                  var $resumoAnterior = $elementResumo.attr("destaqueconteudoanterior");
                  $conteudoResumoAnterior.html($resumoAnterior);
                  $destaqueArtigo.find(".conteudoResumoNovo").closest(".itemArtigo").show();
                  $conteudoResumoAnterior.closest(".itemArtigo").show();
                }

              }

              var $conteudoCartolaNovo = $destaqueArtigo.find(".conteudoCartolaNovo");
              var $conteudoTituloNovo = $destaqueArtigo.find(".conteudoTituloNovo");
              var $conteudoResumoNovo = $destaqueArtigo.find(".conteudoResumoNovo");

              var $cartolaNovo = $elementCartola.html();
              var $tituloNovo = $elementTitulo.html();
              var $resumoNovo = $elementResumo.html();

              $conteudoCartolaNovo.html($cartolaNovo);
              $conteudoTituloNovo.html($tituloNovo);
              $conteudoResumoNovo.html($resumoNovo);

              //* Verifica se é um novo item
              if($novoArtigo)
              {

                //* Insere a nova alteração
                $("#edicaoDestaque").append($destaqueArtigo);

              }

            });

          }
      );

      $("body").append($("<button type='button' class='btn btn-primary' onclick='abrirSalvarEdicaoDestaque();' style='position: fixed;top: 10px;right: 10px;'>Salvar</button>"));

      $(".suita-elementos-destaque .destaque-item-artigo .cartola-materia, .suita-elementos-destaque .destaque-item-artigo .titulo-materia,.suita-elementos-destaque .destaque-item-artigo .resumo-materia").on('click',
          function(index)
          {



          }
      );

    }

    function salvarEdicaoDestaque()
    {

      var arrArtigosEditados = new Array();

      $("#edicaoDestaque").find(".destaqueItem").each(function (index, listItem) {

          var arrArtigoEditado = {
              destaqueId: $(listItem).attr("destaqueid"),
              destaqueArtigoId: $(listItem).attr("destaqueartigoid"),
              cartola: $(listItem).find(".conteudoCartolaNovo").text().trim(),
              titulo: $(listItem).find(".conteudoTituloNovo").text().trim(),
              resumo: $(listItem).find(".conteudoResumoNovo").text().trim()
          };

          arrArtigosEditados[index] = arrArtigoEditado;

      });

      $(".edicaoDestaquePainel").hide();
      $(".edicaoDestaqueSalvar").hide();
      $(".edicaoDestaqueCancelar").hide();
      $(".edicaoDestaqueMensagem").show();
      $(".edicaoDestaqueLegenda").hide();

      $.ajax({
           url: "http://web.suita.com.br/destaque-acao.php?editar-pagina",
           type: "POST",
           crossDomain: true,
           data: {destaques: JSON.stringify(arrArtigosEditados)},
           dataType: "jsonp"
       });

    }

    function salvarEdicaoDestaqueRetorno(retorno)
    {

      $(".edicaoDestaqueMensagem").html("<h4 class='text-center'>Alteração concluída!</h4>");
      $(".edicaoDestaqueRecarregar").show();

    }

    function abrirSalvarEdicaoDestaque()
    {

      if($("#edicaoDestaque").find(".destaqueItem").length == 0)
      {

        $(".edicaoDestaquePainel").hide();
        $(".edicaoDestaqueSalvar").hide();
        $(".edicaoDestaqueCancelar").html("Fechar");
        $(".edicaoDestaqueMensagem").show();
        $(".edicaoDestaqueMensagem").html("<h4 class='text-center'>Nenhuma modificação foi encontrada</h4>");
        $(".edicaoDestaqueLegenda").hide();

      }else{

        $(".edicaoDestaquePainel").show();
        $(".edicaoDestaqueSalvar").show();
        $(".edicaoDestaqueCancelar").html("Cancelar");
        $(".edicaoDestaqueMensagem").hide();
        $(".edicaoDestaqueMensagem").html("<h4 class='text-center'>Enviando modificações...</h4>");
        $(".edicaoDestaqueLegenda").show();

      }

      $("#modalEdicaoDestaque").modal("show");

    }

    
