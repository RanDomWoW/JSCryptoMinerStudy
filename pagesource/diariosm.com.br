        }

        function abreProximaPagina(contentId,click)
        {

            var indicePaginaSelecionada = $(".pagina[contentId='" + contentId + "']:visible").attr("indice");
            var quantidadePaginas = $(".pagina[contentId='" + contentId + "']").length;
