        function abrePagina(indicePagina,contentId,click)
        {

            $(".pagina[contentId='" + contentId + "']").hide();
            $("#pagina" + indicePagina + "[contentId='" + contentId + "']").show();

            if(click)
            {
                var idIntervalo = $("#paginacaoIntervalo[contentId='" + contentId + "']").val();

