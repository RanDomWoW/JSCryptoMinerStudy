<script type="text/javascript">
	$('html').addClass("js");
	//determines page language
	var LANG_COOKIE = 'LG_GAIDHLIG_PREFERRED';
	var GD_TRUE = 'TRUE_1';
	var GD_FALSE = 'FALSE_1';
	var isGd = false;
	var lang;
	var engFormPlacehold = 'search the site...';
	
	$(document).ready ( function() {
		
		
		
		//store form input placeholder for future ref
		engFormPlacehold = $('.forminput').attr('placeholder');
		
		// show and hide menu on smaller screens
		$('#menutoggle').click(function(e) {
		
			$('.icon-menuhide').removeClass('sometimeshidden');
			
			if ($('#nav').hasClass('sometimeshidden')) {
				$('#nav').removeClass('sometimeshidden');
				//swap the menu icon from open to close (addclass/removeclass doesn't work on svgs)
				$('.icon-menushow').attr('class','iconsvg icon-menushow sometimeshidden');
				$('.icon-menuhide').attr('class','iconsvg icon-menuhide');
			}
			else  {
				$('#nav').addClass('sometimeshidden');
				
				$('.icon-menuhide').attr('class','iconsvg icon-menuhide sometimeshidden');
				$('.icon-menushow').attr('class','iconsvg icon-menushow');
			}
			
			$('.hassub').removeClass('minusd');
			$('.hassub').addClass('plusd');
			if (e != null) e.preventDefault();
		});
			
		
		
		$('.hassub').click(function(e) {
			var submenu = $(this).parent().children('ul.submenu');
			
			if (submenu.is(':visible')) {
				submenu.hide();
				$(this).removeClass('minusd');
				$(this).addClass('plusd');
				
				// let users go to the portal page if they want...
				//but if they click the minus sign, close the menu
				if (e!=null && e.target )
				{ 
					var isSpanEn = $(e.target).hasClass('en');
					var isSpanGd = $(e.target).hasClass('gd');
					if ( !isSpanEn && !isSpanGd)
						e.preventDefault();
				}
				
			} else {
				submenu.show();
				$(this).removeClass('plusd');
				$(this).addClass('minusd');
				if (e!=null) e.preventDefault();

			}
		});//hide submenu if we click outside it
		$('ul.submenu').outside('click', function() {
			
		    $(this).hide();
		});
		
		
		
		/**** ====== language switch code ================= *****/
		//check if GET param exists, it overrides default to show English version
		var uri = URI();
		var langCookieVal = Cookies.get(LANG_COOKIE); //also check cookie
		// check if parameter has a given value
		if (uri.hasQuery("lang", "gd") === true) {
			lang='gd';
			toGd(null);
		} else if(typeof langCookieVal != 'undefined') {
			if (langCookieVal == GD_TRUE) {
				lang='gd';
				toGd(null);
			}
	 	 } 
		 $('html').removeClass('js-fouc');

		//handle click on swap languages button
		$(".en_gd").click(function(event) {
			if (!isGd) {
				toGd(event);
				Cookies.remove(LANG_COOKIE);
				Cookies.set(LANG_COOKIE, GD_TRUE, { expires: 150 });
			}
			else {
				toEn(event);
				Cookies.remove(LANG_COOKIE);
				Cookies.set(LANG_COOKIE, GD_FALSE, { expires: 150 });
			}
		});


	});
	
	
	
	/**
	 * Turn the page into Gaelic.
	 */
	function toGd(event)
	{
		// block or what?
		$(".en").hide();
		$(".gd").fadeIn("fast");

		$('.forminput').attr('placeholder','lorg...');
		$('#theTypeahead').attr('placeholder','lorg...');
		$('#lookinput').attr('placeholder','lorg an seo...');
		$('.smNav input:first-child').attr('placeholder','lorg an seo...');
	
		isGd = true;

	
		$("a").attr("href", 
			function (index, href) 
			{
				if (!href) return;
				if (href.length == 0) return;

				if (href.indexOf("http:") != -1)
					return href; //dont do external urls
				if (href.indexOf("https:") != -1)
					return href; //dont do external urls
				if (href.indexOf("mailto:") != -1)
					return href; // or emails
				if (href.indexOf("javascript:") != -1)
					return href; // or js
				if (href.charAt(0) == "#")
					return href; // or named internal links
				

				
				var newHref = URI(href);
				// set data map:
				newHref.addSearch({ lang: "gd"});
				return newHref.toString();
			});
	
	
		if (window.location.pathname.toString().indexOf("/thesaurus") != -1)
				try { langChange(); } catch (err) {  } //ignore
		if (event != null) event.preventDefault();
	}

	/**
	 * Turn the page into English.
	 */
	function toEn(event)
	{
	
		$(".gd").hide();
		$(".en").fadeIn("fast");

		$('.forminput').attr('placeholder',engFormPlacehold);
		$('#theTypeahead').attr('placeholder','search...');
		$('#lookinput').attr('placeholder','search in Look@LearnGaelic...');
	

		isGd = false;
	
		//change internal URLs, removing lang=gd parameter

			$("a").attr("href", 
				function (index, href) 
				{
					if (!href) return;
					if (href.length == 0) return;
					
					if (href.indexOf("http:") != -1)
						return href; //dont do external urls
					if (href.indexOf("mailto:") !=-1)
						return href;
					if (href.indexOf("https://") != -1)
						return href;
					if (href.indexOf("javascript:") != -1)
						return href;
					var newHref = URI(href);
					// set data map:
					newHref.removeSearch('lang');
					return newHref.toString();
				});
	
		if (window.location.pathname.toString().indexOf("/thesaurus") != -1)
				try { langChange(); } catch (err) { console.log(err); } //ignore
		if (event != null) event.preventDefault();

	}
	
	
	

	
	
	/* plugin to make the submenus vanish when you click off them */
	(function($){
	    $.fn.outside = function(ename, cb){
	        return this.each(function(){
	            var $this = $(this),
	                self = this;

	            $(document).bind(ename, function tempo(e){
					
					var li = $(e.target).parents('li');
					var inLi = false;
					
					// we hide submenu if we're not clicking on it OR we're not
					// clicking on its ancestor li element
					if (li && li[0]) {
						if($.contains(li[0], self)) inLi=true;
					}
					if(e.target !== self &&  !$.contains(self, e.target) && !inLi) {

							cb.apply(self, [e]);
							if(!self.parentNode) $(document.body).unbind(ename, tempo);
							
	                }
	            });
	        });
	    };
	}(jQuery));
	
	


	
		
	//jQuery extension to get params from URL - 
	// thanks to http://jquery-howto.blogspot.com/2009/09/get-url-parameters-values-with-jquery.html
	/*$.extend({
	  getUrlVars: function(){
	    var vars = [], hash;
