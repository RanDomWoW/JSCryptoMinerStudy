        width: 100%;
        position: relative;
        display: block;
    }
    
    .screenshot img {
        position: absolute;
        left: 6.7%;
        width: 86.6%;
        top: 12.4%;    
    }
    
    h2 {
        margin-top: 0px;
    }
    
    h3 {
        margin: 0px;
    }
    
    .slide1 {
        animation:fade1 12s infinite;
    }
    .slide2 {
        animation:fade2 12s infinite;
    }
    .slide3 {
        animation:fade3 12s infinite;
    }
    
    .header-pic.slide2 {
        right: 20px;
    }

    .header-pic.slide3 {
        right: 160px;
    }
    
    .event-name {
        padding: 14px 10px 0px 15px;
