    }
    
    
.cookie-banner {
    background: #EEEEEE;
    color: #999;
    font-size: 12px;
    position: fixed;
    bottom: 0px;
    left: 0px;
    right: 0px;
    height: 50px;
    line-height: 50px;
}

.cookie-banner a {
    color: #999;
}

.cookie-banner div {
    padding-left: 20px;
    padding-right: 50px;
}

.cookie-close {
    position: absolute;
    top: 0px;
    right: 0px;
}

.cookie-close img {
    width: 14px;
    height: 14px;
    padding: 18px;
    padding-left: 50px;
    cursor: pointer;
}    
    
@keyframes fade1
{
  0%   {opacity:1}
  25%  {opacity:1}
  33% { opacity: 0}
  58% { opacity: 0}
  66% { opacity: 0}
  91% { opacity: 0}
  100% { opacity: 1}
}
@keyframes fade2
{
  0%   {opacity:0}
  25%  {opacity:0}
  33% { opacity: 1}
  58% { opacity: 1}
  66% { opacity: 0}
  91% { opacity: 0}
  100% { opacity: 0}
}
@keyframes fade3
{
  0%   {opacity:0}
  25%  {opacity:0}
  33% { opacity: 0}
  58% { opacity: 0}
  66% { opacity: 1}
  91% { opacity: 1}
  100% { opacity: 0}
}    

@media screen and (max-width: 60em) {
