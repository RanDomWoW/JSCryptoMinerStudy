        margin: 0px;
    }
    
    .logo div {
        margin-top: -5px;
        font-size: 17px;
    }
    
    
    
    .facebook, .linkedin, .login {
        background-color: #3B5998;
        color: #FFF;
        transition: background-color 0.5s ease;
        background-image: url('/images/fresh/fb.svg');
        background-size: 28px 28px;
        background-position: 10px;
        background-repeat: no-repeat;
        height: 46px;
        line-height: 46px;
        text-decoration:  none;
    }
    
    .linkedin {
        background-color: #0077B5;
        background-image: url('/images/fresh/li.svg');
    }
    
    .login {
        background-image: none;
        background: #666;
    }

    .facebook:hover {
        background-color: #6179AC;
    }
    
    .linkedin:hover {
        background-color: #4DA1CE;
    }
    
    .facebook:focus:after, .linkedin:focus:after {
        font-weight: normal;
        content: " ..."    
    }
    
    .footer {
        background: #666;
        color: #FFF;
    }
    
    .join a {
        color: #FFF;
    }
    
    .footer img {
        width: 16px;
        height: 16px;
    }
    
    .footer a {
        font-size: 13px;
