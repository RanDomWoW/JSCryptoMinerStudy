          }
          // tablets at android
          else if (/Android/.test(navigator.userAgent)) {
            alert('Нажмите иконку звездочки в адресной строке для добавления страницы в избранное.');
          }
          else {
            // Unknown && Chrome
            alert('Нажмите CTRL-D, для добавления страницы в избранное.');
          }
        }
      }
    }
    return false;
  }
