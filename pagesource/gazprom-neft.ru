  function bookmark(a) {
    var url = window.document.location;
    var title = window.document.title;
    try {
      // Internet Explorer
      window.external.AddFavorite(url, title);
    }
    catch (e) {
      try {
        // Mozilla
        window.sidebar.addPanel(title, url, "");
      }
      catch (e) {
        // Opera и Firefox 23+
        if ( typeof(opera) == "object" || window.sidebar ) {
          a.href  = url;
          a.rel   = "sidebar";
          a.title = title;
          a.url   = url;
          return true;
        }
        else {
          // Mobile devices
          if (/Mobile/.test(navigator.userAgent)) {
            if (/Windows Phone/.test(navigator.userAgent)) {
              alert('Нажмите иконку с тремя горизонтальными точками рядом с адресной строкой, затем добавить в закладки, для добавления страницы в избранное.');
