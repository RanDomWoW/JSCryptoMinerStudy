<script type="text/javascript">

    $(document).ready(function() {
        'use strict';
        $('.pop-close').click(function(e) {
            $('.fancybox-chat').css('display', 'none');
            e.preventDefault();
        });
        $('.chatPop').fancybox({
            openEffect  : 'elastic',
            closeEffect : 'elastic',
            openSpeed : 400,
            closeBtn : false,
            width: 320,
            height: 460,
            aspectRatio: false,
            autoSize: false,
            // minHeight: 350,
            margin: 0,
            padding: 0,
            wrapCSS: 'fancybox-chat',
            helpers: {
                overlay: null
            }
        });
    });

    var chatConfig = {};
    function createChatConfig(){
        'use strict'
        var firstName = document.getElementById('firstName').value;
        var customerEmail = document.getElementById('customerEmail').value;
        var queueSelect = document.getElementById('queueSelect').value;

        chatConfig = {
            "webchatAppUrl": "https://apps.mypurecloud.com/webchat",
            "webchatServiceUrl": "https://realtime.mypurecloud.com:443",
            "orgId": 15979,
            "orgName": "Shipt",
            "priority": 5,
            "queueName" : queueSelect,
            "logLevel": "WARN",
            "locale": "en",
            "skills": ["Chats"],
            // "reconnectEnabled": true,
            // "reconnectOrigins": ['https://shipt.com', 'http://help.shipt.com', 'https://www.shipt.com', 'https://shipt.staging.wpengine.com/'],

            // Data that will be included with interaction
            "data": {
                "firstName": firstName,
                "customerId": customerEmail
            },

            // Logo used at the top of the chat window
            "companyLogo": {
                "width": 197,
                "height": 75,
                "url": "https://www.shipt.com/wp-content/uploads/2017/06/shipt-logo.png"
            },
            "companyLogoSmall": {
                "width": 75,
                "height": 75,
                "url": "https://www.shipt.com/wp-content/uploads/2015/04/Shipt-Logomark-Screen-Color-RGB-11-e1433303918490.png"
            },
            "agentAvatar": {
               "width": 75,
               "height": 75,
               "url": "https://www.shipt.com/wp-content/uploads/2015/04/Shipt-Logomark-Screen-Color-RGB-11-e1433303918490.png"
            },
            "welcomeMessage": "Please hold for the next available Experience Team member. We look forward to chatting with you!",
            "cssClass": "webchat-frame",
            "css": {
                "width": "100%",
                "position": "absolute",
                "top": "0",
                "left": "0",
                "height": "460px"
            }
        };
    }

    function startEmbeddedChat(event) {
      'use strict';
    	event.preventDefault();
        createChatConfig();
        ININ.webchat.create(chatConfig, function(err, webchat) {
            if (err) {
                throw err;
            }

            // Render to frame
            webchat.renderFrame({
                containerEl: 'chatContain'
            });

            webchat.chatStarted = function () {
              //Code to run when the chat is started
            };

            webchat.chatConnectedToAgent = function () {
              //Code to run when the chat is connected to an agent
            };

            webchat.chatEnded = function () {
              $('.fancybox-chat').css('display', 'none');
            };
        });
    }
</script>

