      triggerRestEvent(createRestEvent(eventType, eventMetaJson));
    }

    // Functions for creating and procing events should be available everywhere as this
    // JSP is present in every footer
    function createRestEvent(eventType, eventMetaJson) {
      updateCookieSession = false;
      var application_name = "sun.basket_web";
      var jsonData = {
        event_type: eventType,
        application_name: application_name,
        application_version: "1.166.1",
        session_id: "d258c0a7-65ab-4402-86b7-3d355b4208f2",
        cookie_id: "",
        ip_address: "172.68.65.95",
        context_id: window.location.href,
        event_meta_json: eventMetaJson,
      };

      if("PRODUCTION" != "") {
        jsonData.environment = "PRODUCTION";
      }

      var lastUpdate = getCookie("eventCookieTimestamp");

      if (lastUpdate === '' || Math.floor((Date.now() - parseInt(lastUpdate)) / 1000 / 60) > 12) {
          setCookie("eventCookieTimestamp", Date.now(), 10000);
          updateCookieSession = true;
      }

      if(getCookie("eventCookieID")) {
        jsonData.cookie_id = getCookie("eventCookieID");
      } else {
        setCookie("eventCookieID", "63526c67-053b-4efb-ba77-3140c903cb15", 10000);
        jsonData.cookie_id = "63526c67-053b-4efb-ba77-3140c903cb15";
        updateCookieSession = true;
      }

      if (getCookie("lastSessionId") !== "d258c0a7-65ab-4402-86b7-3d355b4208f2") {
        setCookie("lastSessionId", "d258c0a7-65ab-4402-86b7-3d355b4208f2", 10000);
        updateCookieSession = true;
      }

      if(updateCookieSession) {
        updateCookieSessionCall(getCookie("eventCookieID"), getCookie("lastSessionId"));
      }

      return jsonData;
    }

