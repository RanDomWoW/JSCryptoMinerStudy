<script type="text/javascript">
	// Timer
	var play = null;	

	$j(document).ready(function() {
		changeArticle($j(".splash-nav a:first"));	
		slideSwitch();		
	
		$j(".splash-nav a").hover(function() {
			clearInterval(play);
			changeArticle(this);		
		}, function(){
			slideSwitch();
		});

		$j(".splash-content").hover(function() {
			clearInterval(play);		
		}, function(){
			slideSwitch();
		});
	});

	function changeArticle(element)
	{
		$j(".splash-nav a").removeClass("selected");
		$j(element).addClass("selected");
		$j(".selected-summary").text($j(".recipe-summary", element).text());
    		$j('#splash-difficulty').attr('class', '');
    		$j('#splash-difficulty').attr('class', 'cook-difficulty difficulty-'+$j(".recipe-difficulty", element).text());
    		$j(".splash-content .cook-time").text($j(".recipe-cook-time", element).text());
    		$j('.splash .splash-image').css('background-image', 'url('+$j(".recipe-big-image img", element).attr('src')+')');
    		$j('#splash-recipe').attr('href', $j(element).attr('href'));

    		if ( $j(element).hasClass("article") ) {
	    		$j("#splash-recipe").addClass("article");
    		} else {
    			$j("#splash-recipe").removeClass("article");
    		}
	}

	function slideSwitch() {
		clearInterval(play);
		play = setInterval(function(){
    			var $active = $j('.splash-nav a.selected');
    			if ( $active.length == 0 ) $active = $j('.splash-nav a:last');
    			var $next =  $active.next().length ? $active.next() : $j('.splash-nav a:first');
      			changeArticle($next);
		}, 5000);
	}
</script>

