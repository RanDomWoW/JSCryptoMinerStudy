<script type="text/javascript">
$j(document).ready(function(){
	// weather
	//get cookie
	var location = $j.cookie('weather_location');
	if(location){
		var element = $j("#location_" + location);
		element.addClass("active");
		$j("#change-weather").html($j("#location_"+location).html());
		$j("#change-weather .city_short").html(element.attr('name'));
		element.addClass("active");
	}
	// change data in brandline
	$j(".allcity").click(function(ev){
		location = $j(this).attr('loc');
		$j.cookie('weather_location', location, { expires: 365 });
		var element = $j("#location_" + location);
		$j("#change-weather").html($j("#location_"+location).html());
		$j("#change-weather .city_short").html(element.attr('name'));
		$j(".allcity").removeClass("active");
		$j(this).addClass("active");
	});

	$j('#crm_login_button').on('click', function(e)
	{
		e.preventDefault();
  	      crm_login.show(this, null, this);
	});

	$j("#crm-settings-button").on("click", function(e) 
	{
		e.stopPropagation();
		e.preventDefault();
		$j("body").off("click.settings-dropdown-closer"); // remove event that waits for click
		
		$j("#crm-settings-dropdown").toggle();

		// add event that hides settings dropdown when user clicks something else
		if ($j("#crm-settings-dropdown").is(":visible"))
		{
			$j('body').on("click.settings-dropdown-closer", function(event) 
			{
				if (!$j(event.target).closest('#crm-settings-dropdown').length) 
				{
					$j('#crm-settings-dropdown').hide();
				};
			});
		}



	});

});
</script>
