<script type="text/javascript">
	window.addEvent('domready', function() {
		// check if vote cookie is set and display results
		if (Cookie.get('myvote9077')) {
			$('poll_answers').setStyle('display', 'none');
			$('poll_results').setStyle('display', 'block');
		}
	});
	
	voteDone=false;
	separateVote=false;

	function doVote(answer_id) {
		voteDone=true;

		$$('.poll-options .option').removeClass('selected');
		$('option_'+answer_id).addClass('selected');
		$('answer_value').value=answer_id;

		if (!separateVote)
			doSubmitVote('unisex');
	}

	function doSubmitVote(sex) {
		if (!voteDone) {
			alert('Prosim, najprej izberite željeni odgovor.');
			return false;
		}
		document.getElementById('votesex').value=sex;
		
		// display ajax loader to user
		$('poll_answers').setStyle('display', 'none');
		$('ajax_loader').setStyle('display', 'block');
						
		// submit vote via ajax request
		$('voteForm').send({
			onComplete: function(data) {
				data = data.toString();

				if (data == "ok") {
					$('ajax_loader').setStyle('display', 'none');
					$('poll_results').setStyle('display', 'block');
				}
				else {
					// user already voted or more than 10 votes from ip
					if (data == 1 || data == 2) {
						alert('Za to anketo ste že glasovali!');
						$('poll_results').setStyle('display', 'block');
						$('ajax_loader').setStyle('display', 'none');
					}
					// wrong parameters, archived or non-existent poll
					else if (data > 3) {
						alert('Napaka pri oddaji glasu!');
						$('poll_answers').setStyle('display', 'block');
						$('ajax_loader').setStyle('display', 'none');
					}
				}
			}
		});
		return false;
	}

	function switch_sex_sub(selected_sex) {
		for (sex=0;sex<sexes.length;sex++) {
			if (sexes[sex] == selected_sex) {
				try {
					$('result_'+sexes[sex]).style.display='block';
					$('link_'+sexes[sex]).style.color='#319212';
				} catch(error) {
					return false;
				}
			}
			else {
				try {
					$('result_'+sexes[sex]).style.display='none';
					$('link_'+sexes[sex]).style.color='';
				} catch(error) {
					return false;
				}
			}
		}
	}
	sexes = new Array('male','female','sum'); 


	function openTab(openDiv, closeDiv){
		$j("#" + openDiv + '_tab').removeClass('normal');
