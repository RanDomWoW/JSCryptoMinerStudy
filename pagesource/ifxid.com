    <script>
        var numberScrollTop = 0;

        function openMobileMenu(e) {
            e.preventDefault();
            numberScrollTop = window.scrollY;
            document.getElementById('menu_mobile_block').style.cssText = 'display: block; top:' + numberScrollTop + 'px; height:' + window.innerHeight + 'px;';
            document.body.setAttribute('class', document.body.getAttribute('class') + ' menu_mobile_active');
            document.body.style.cssText = 'width:' + window.innerWidth + 'px; height:' + window.innerHeight + 'px; margin-top:' + (-(numberScrollTop)) + 'px;';
            setTimeout(function () {
                document.body.style.height = window.innerHeight + 'px';
                document.getElementById('menu_mobile_block_background').style.opacity = '.7';
                document.getElementById('menu_mobile').style.left = '-5px';
            }, 50);
        }

        function resizeMobileMenu() {
            if (document.body.getAttribute('class').search(/menu_mobile_active/i) !== -1) {
                document.getElementById('menu_mobile_block').style.height = window.innerHeight + 'px';
                document.body.style.width = window.innerWidth + 'px';
                document.body.style.height = window.innerHeight + 'px';
            }
        }

        function closeMobileMenu(e) {
            e.preventDefault();
            var body = document.body;
            var menu_mobile_block_background = document.getElementById('menu_mobile_block_background');
            var menu_mobile = document.getElementById('menu_mobile');
            var classBody = document.body.getAttribute('class').replace(/(\s{2}|(\s*menu_mobile_active)|^\s|\s$)/g, '');
            var style = getComputedStyle(menu_mobile_block_background);

            body.style.position = 'fixed';
            body.setAttribute('class', classBody);
            menu_mobile.style.left = '-280px';
            menu_mobile_block_background.style.opacity = '0';

            setTimeout(function () {
                document.getElementById('menu_mobile_block').setAttribute('style', '');
                document.body.setAttribute('style', '');
                window.scrollTo(0, numberScrollTop);
            }, parseFloat(style.transitionDuration) * 1000);
        }

        function readyToStartMenu() {
            $('#mobile_menu_button').on('click', function (e) {
                openMobileMenu(e);
            });

            $('#menu_mobile_block_background').on('click', function (e) {
                closeMobileMenu(e);
            });

            $('#menu_mobile').on('click', '.menu_parent > a', function (e) {
                e.preventDefault();
                var arrayLinks = document.querySelectorAll('#menu_mobile > ul > li.menu_parent:not(.menu_important)');
                var $target = $(e.target);
                var $parent = $target.parent();
                var has = $parent.hasClass('active');

                for (var i = 0, size = arrayLinks.length; i < size; i++) {
                    if ($(arrayLinks[i]).hasClass('active')) {
                        $(arrayLinks[i]).removeClass('active').find('>div').eq(0).slideUp("fast", function () {
                            // Animation complete.
                        });
                    }
                }
                if (has) {
                    $parent.removeClass('active').find('>div').eq(0).slideUp("fast", function () {
                        // Animation complete.
                    });
                } else {
                    $parent.addClass('active').find('>div').eq(0).slideDown("fast", function () {
                        // Animation complete.
                    });
                }
            });
        }

        document.addEventListener('DOMContentLoaded', readyToStartMenu);
        window.addEventListener('resize', resizeMobileMenu);

        document.addEventListener('touchstart', handleTouchStart, false);
        document.addEventListener('touchend', handleTouchStop, false);

        var xTouch = null;
        var yTouch = null;

        function handleTouchStart(e) {
            xTouch = e.touches[0].clientX;
            yTouch = e.touches[0].clientY;
        }

        function handleTouchStop(e) {
            if (!xTouch || !yTouch) {
                return;
            }

            var xUp = e.changedTouches[0].clientX;
            var yUp = e.changedTouches[0].clientY;

            var xDiff = xTouch - xUp;
            var yDiff = yTouch - yUp;

            if (Math.abs(xDiff) > Math.abs(yDiff)) {/*most significant*/
                if (xDiff > 0) {
                    /* left swipe */
                    if ($('body').hasClass('menu_mobile_active') && Math.abs(xDiff) > document.documentElement.clientWidth / 5 && Math.abs(yDiff) < 100) {
                        closeMobileMenu(e);
                    }
                } else {
                    /* right swipe */
                    if (!$('body').hasClass('menu_mobile_active') && Math.abs(xDiff) > document.documentElement.clientWidth / 2 && Math.abs(yDiff) < 100) {
                        openMobileMenu(e);
                    }
                }
            } else {
                if (yDiff > 0) {
                    /* up swipe */
