              <script type="text/javascript">
$(function(){

  var access_date = accessDate();
  function accessDate()
  {
    var url = decodeURIComponent(location.href);
    var parameters = url.split("?");
    var params = [];
    if(parameters.length > 1){
      if(parameters[1]){
        params = parameters[1].split("&");
      }
    }

    var paramsArray = [];
    for (i = 0; i < params.length; i++) {
      var tmp = params[i].split("=");
      paramsArray.push(tmp[0]);
      paramsArray[tmp[0]] = tmp[1];
    }

    var access_date = new Date();
    if ('test_date' in paramsArray) {
      var test_date = paramsArray["test_date"];
      access_date = new Date(test_date);
    } else {
      access_date = new Date();
    }
    return access_date;
  }

  var start_camp_bar = new Date("2018/03/01 10:00");
  if(start_camp_bar <= access_date){
    // 切替日付後
    $(".memohide").css("display", "");
  }else{
    // 切替日付前
    $(".memohide").css("display", "none");
  }
});
</script>

