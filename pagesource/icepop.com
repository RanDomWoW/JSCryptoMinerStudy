                    ga('send', 'event', 'Adblock', 'Yes', {'nonInteraction': 1});
                } else if (typeof _gaq !== 'undefined') {
                    // Log a non-interactive event in old Google Analytics
                    _gaq.push(['_trackEvent', 'Adblock', 'Yes', undefined, undefined, true]);
                }
            }
        }, 5000); // Run ad block detection 5 seconds after page load
    };
</script>
<script>
    function StickToTop() {
        var currentTopOfScreen = (window.pageYOffset || document.documentElement.scrollTop) - (document.documentElement.clientTop || 0);

        if (jQuery('#bottom_rightrail_sticky_location').offset().top > adTop) {
            adTop = jQuery('#bottom_rightrail_sticky_location').offset().top;
        }

        if (currentTopOfScreen >= (adTop - 60)) {
            if (!jQuery('#bottom_rightrail_ad').hasClass('sticky-rightrail-ad')) {
                jQuery('#bottom_rightrail_ad').addClass('sticky-rightrail-ad');
            }
        } else {
            if (jQuery('#bottom_rightrail_ad').hasClass('sticky-rightrail-ad')) {
                jQuery('#bottom_rightrail_ad').removeClass('sticky-rightrail-ad');
            }
        }
    }

    var adTop = 0;
    if (typeof(document.getElementById('bottom_rightrail_ad')) !== 'undefined'
        && document.getElementById('bottom_rightrail_ad') != null) {
        adTop = jQuery('#bottom_rightrail_sticky_location').offset().top;
        window.addEventListener("scroll", function () {
            StickToTop();
        });
    }
</script>

