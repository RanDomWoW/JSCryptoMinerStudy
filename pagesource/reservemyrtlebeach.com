<script type="text/javascript">
	var bsAutoCycled = true;

	$(document).ready(function () {

		REX.analytics.pageView.sendProductListImpressions('.site-promo', 'Home Page Featured Promos', null);

		REX.analytics.pageView.sendSpotLightImpressions();

		//bootstrap carousel (Main Promo Banner)
		$('.carousel').carousel({
			interval: 10000
		});

		ProcessSlides('.carousel-inner > .item', 'active', 'Main Promo Banners', bsAutoCycled);

		$('.left.carousel-control').click(function (e) {
			e.preventDefault();
			bsAutoCycled = false;
			$('.carousel').carousel('prev');
		});

		$('.right.carousel-control').click(function (e) {
			e.preventDefault();
			bsAutoCycled = false;
			$('.carousel').carousel('next');
		});

		$('.carousel').on('slid.bs.carousel', function () {
			ProcessSlides('.carousel-inner > .item', 'active', 'Main Promo Banners', bsAutoCycled);
		});
		//

		//slick carousel (Videos, Spotlights)
		$('.slick-carousel').slick({
			dots: false,
			infinite: true,
			speed: 300,
			slidesToShow: 3,
			slidesToScroll: 3,
			responsive: [
				{
					breakpoint: 991,
					settings: {
						slidesToShow: 2,
						slidesToScroll: 2
					}
				},
				{
					breakpoint: 560,
					settings: {
						slidesToShow: 1,
						slidesToScroll: 1
					}
				}
			]
		});
	});

	//$('.slick-carousel').on('init', function (event, slick) {
	//	ProcessSlides('.insight-video', 'slick-active', 'Insight Videos', true);
	//});

	//$('.slick-carousel').on('afterChange', function (event, slick, currentSlide) {
	//	ProcessSlides('.insight-video', 'slick-active', 'Insight Videos', false);
	//});

	//$('.spotlight-carousel').on('init', function (event, slick) {
	//	ProcessSlides('.spotlight-product', 'slick-active', 'Spotlights', true);
	//});

	//$('.spotlight-carousel').on('afterChange', function (event, slick, currentSlide) {
	//	ProcessSlides('.spotlight-product', 'slick-active', 'Spotlights', false);
	//});
	//

	function ProcessSlides(slideSelector, activeClass, list, nonInteraction) {
		$(slideSelector).each(function () {
			if ($(this).hasClass(activeClass)) {

				var obj = $(this).data('obj');
				if (obj != null) {
					var position = $(this).data('position');

					REX.analytics.ec.impression.add(obj.id, obj.name, list, null, obj.type, "Home", position, null);
				}
			}
		});

		if (nonInteraction != true) {
			nonInteraction = false;
		}

		//if this was called for the bootstrap carousel (activeClass == 'active')
		if (activeClass == 'active') {
			//reset flag for auto cycle (used for the GA event's nonInteraction value)
			bsAutoCycled = true;
		}

		REX.analytics.event.send('EE Generic', 'Click', list, null, nonInteraction);
	}

	function SendProductData(obj, position, list) {

		if (obj != null) {
			var actionData = { 'list': list };

			REX.analytics.ec.product.add(obj.id, obj.name, obj.type, position, 'Home');
			REX.analytics.ec.action.click(actionData);

			//send event
			REX.analytics.event.send(list, 'Click-M', obj.name);
		}
	}
</script>


