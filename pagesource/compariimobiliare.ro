                if ($('#add-search-areas').hasClass("visible")) {
                    var areas = [],
                        cities = [],
                        poi = [];

                    $("input[name='zona[]']:checked").each(function () {
                        areas.push($.trim($(this).closest('label').text()));
                    });

                    $("input[name='city[]']:checked").each(function () {
                        cities.push($.trim($(this).closest('label').text()));
                    });

                    $("input[name='poi[]']:checked").each(function () {
                        poi.push($.trim($(this).closest('label').text()));
                    });

                    if (areas.length>0 && cities.length==0 && poi.length==0)
                    {
                        $('.nav-tabs a[href="#tab_areas"]').tab('show');
                    }
                    if (areas.length==0 && cities.length>0 && poi.length==0)
                    {
                        $('.nav-tabs a[href="#tab_cities"]').tab('show');
                    }
                    if (areas.length==0 && cities.length==0 && poi.length>0)
                    {
                        $('.nav-tabs a[href="#tab_points_of_interest"]').tab('show');
                    }

                    //$('body, html').addClass('navactive'); - !!!! THIS IS FREEZING SOME BROWSERS (android Chrome for example)
                }
            });

            $('.close-areas, .close-areas-bottom').click(function (e) {
                // clicked to close dropdown
                //$('body, html').removeClass('navactive');
            });

            $('input:checkbox').on('change', function(){
                if($(this).is(":checked")) {
                    $(this).parents('label').addClass('checked');
                } else {
                    $(this).parents('label').removeClass('checked');
                }

                // sync selections
                clearTimeout(window.zoneRefreshTimeout);
                window.zoneRefreshTimeout = setTimeout(function () {
                    $('#zone-selector').trigger('refresh');
                }, 200);
            });

            // ENABLE AND DISABLE ALL
            $('.select-all').on('click', function() {
                var status = $(this).find(':checkbox').is(':checked');
                $(this).closest('.columns-wrapper').find('.column').find(':checkbox').prop('checked', status).trigger('change');
                $('#zone-selector').trigger('refresh');
            });

            $('#zone-selector').on('autoclose', function () {
                $(this).slideUp();
                //$('body, html').removeClass('navactive');
            });
            $('#zone-selector').on('refresh', function() {
                var selected_areas = [];

                $('#zone-selector .column input[type=checkbox]:checked').each(function () {
                    selected_areas.push($.trim($(this).closest('label').text()));
                });
                $('#zone-selector').trigger('changed', selected_areas.join(', '));
                $("input[data-role=tagsinput]").tagsinput('removeAll');
                $("input[data-role=tagsinput]").tagsinput('add', selected_areas.join(','));
                $('#areaTagsInput .bootstrap-tagsinput').addClass('hasData');
            });

            // remove item from preview selections
            $("input[data-role=tagsinput]").on('itemRemoved', function(event) {
                item = event.item;
                $("label:textEquals("+item+")").click();
            });

            $('input.area.zona').click(function () {
                var is_area = $(this).parents('#tab_areas').length ? true : false,
                    is_city = $(this).parents('#tab_cities').length ? true : false,
                    is_poi = $(this).parents('#tab_points_of_interest').length ? true : false;

                // if an area or point of interest is selected, deselect all cities;
                // if a city is selected, deselect areas & points of interest
                if ($(this).is(':checked')) {
                    if (is_area || is_poi) {
                        $('#tab_cities input:checked').prop('checked', false);
                        $('#tab_cities label').removeClass('checked');
                    }

                    if (is_city) {
                        $('#tab_areas input:checked').prop('checked', false);
                        $('#tab_areas label').removeClass('checked');
                        $('#tab_points_of_interest input:checked').prop('checked', false);
                        $('#tab_points_of_interest label').removeClass('checked');
                    }
                }
            });

            // list options live search filter
            $('input.listSearchInput').on('keyup paste input', function () {
                var input = $(this);
                clearTimeout(window.filterListTimeout);
                window.filterListTimeout = setTimeout(function () {
                    var searchText = input.val();
                    var targetList = $(".nav-tabs li a.active").attr('aria-controls');
                    filterTab('#' + targetList, searchText);
                }, 100);

            });

            $('.nav-tabs li').on('click', function () {
                $('input.listSearchInput').val('');
                filterTab('div.tab-pane', '');
            });
        });

        // list options live search filter
        function filterTab(tabSelector, searchText) {
            $(tabSelector).scrollLeft(0);
            if (searchText) {
                searchText = searchText.toLowerCase().trim();
                $(tabSelector + ' label').each(function() {
                    var value = $(this).text().toLowerCase().trim();
                    $(this).css('display', value.indexOf(searchText) > -1 ? 'block' : 'none');
                });
            } else {
                $(tabSelector + ' label').each(function() {
                    $(this).css('display', 'block');
                });
            }
        }

        $.expr[':'].textEquals = function(a, i, m) {
            return $.trim($(a).text().replace(/[^a-zA-Z 0-9]+/g, '')).match("^" + m[3] + "$");
        };
        //
    </script>
    <script>
        jQuery(document).ready(function($) {
            $('.add-locations').on('click', function() {
               if ($(this).hasClass('expanded')) {
                   $('#areaTagsInput .bootstrap-tagsinput').addClass('hasData');
               } else if (typeof $('#areaTagsInput .bootstrap-tagsinput span') == 'undefined') {
                    $('#areaTagsInput .bootstrap-tagsinput').removeClass('hasData');
               }
            });

            $('#searchForm .form-check label').on('click', function() {
                $('#searchForm .estate-type .list-inline-item').removeClass('selected');
                $(this).parents('.list-inline-item').addClass('selected');
                $(this).parents('.estate-type').removeClass('open');
            });
        });
    </script>
<script>
    $.compare = compariImobiliare.init({url: 'https://compariimobiliare.ro/compara'});

    $(document).ready(function () {
        $('.radio-wrap a').click(function (e) {
            e.preventDefault();

            $(this).closest('.row').find('.radio-wrap a.active').removeClass('active');

            $(this).addClass('active');
        });

        $('#state-select').change(function () {
            showMask();
            Cookies.set('location', $('#state-select').val(), {expires: 365});
            location.reload();
        });

        $('#zone-input :submit, .search-btn').click(function () {
            var mainLocationName = $("#state-select option[value='" + $('#state-select').val() + "']").text(),
                areas = [],
                cities = [],
                poi = [],
                type = $("[name='estate-type']:checked").attr('data-id'),
                category = $("#saleRent").find(':selected').attr('data-id');

            $("input[name='zona[]']:checked").each(function () {
                areas.push($.trim($(this).closest('label').text()));
            });

            $("input[name='city[]']:checked").each(function () {
                cities.push($.trim($(this).closest('label').text()));
            });

            $("input[name='poi[]']:checked").each(function () {
                poi.push($.trim($(this).closest('label').text()));
            });

            $.url = urlHelper.init({
                state: 'cluj'
            });

            window.location = $.url.generate(mainLocationName, category, type, areas, cities, poi);
        });
    });
</script>
