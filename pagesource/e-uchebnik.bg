<script>          
$(function(){
	$('.dot-holder').width(0);
	$('.dot').each(function(){
		$('.dot-holder').width(parseInt($('.dot-holder').width())+$(this).width()+parseInt($(this).css('marginLeft')));
	});
	$('.dot').click(function(){                                                  
        startSlider($(this).index());
	});       
    
    startSlider(0);
});                 
var slideTime = 0;
var slideInterval;
function startSlider(index)
{    
        var nextIndex = index;
        var prevIndex = $('.active-dot').index();                                                 
                          
        $('.active-dot').addClass('passive-dot');
        $('.active-dot').removeClass('active-dot');
        $('.dot:eq('+index+')').addClass('active-dot');
        $('.dot:eq('+index+')').removeClass('passive-dot'); 
        $('.slide-holder').addClass('dispno');
        $('.slide-holder:eq('+(prevIndex)+')').html($('.slide-holder:eq('+(prevIndex)+')').html()); 
        $('.slide-holder:eq('+(index)+')').removeClass('dispno'); 
         
        var i = 0;
        $('.dot-holder .dot').each(function(){
            if($(this).hasClass('active-dot'))
                nextIndex = i+1;
            i++;
        });
        if(nextIndex >= i)                   
            nextIndex = 0;                  
        
        if($('.slide-holder:eq('+(index)+') .slide-time').length > 0)
            slideTime = $('.slide-holder:eq('+(index)+') .slide-time').val();
        else
            slideTime = 3;
            
        clearInterval(slideInterval);
        slideInterval = setInterval(function(){
            startSlider(nextIndex);
        }, slideTime*1000); 
}
</script>

					<script>   

var hText = 0;
var timeout;
var ajaxRdy = 0;
function getNextResources(startRow, sliderGroup, container, limit)
{              
	$(container).children('.product-slider-box-content').fadeOut();                   
    $.get("/main/ajaxGetNextResources/"+startRow+"/"+sliderGroup+"/"+limit, function(data){
    	
        $(container).html(data);
		$(container).find('.product-slider-box-content').fadeIn(1000);
        
        ajaxRdy++;
        
        if(ajaxRdy == 2)  
            resizeProductHolders();
            
        if(ajaxRdy > 1)
            resizeProductSliderElements(parseInt(limit/2))
        
        
        /*
        $('.box-item-text').each(function(){
            if(hText < $(this).height())
                hText = $(this).height();
        });
        $('.box-item-text').each(function(){
            $(this).css('margin-top' , parseInt(hText)-parseInt($(this).height()));
        });
        */ 
    });
}   
function resizeProductSliderElements(rowLimit)
{                       
    $('.inner-box-middle').each(function(){ 
        var hTextBox = new Array();
        hTextBox[0] = 0;  
         
        var key = 0;
        $(this).find('.box-item').each(function(i){ 
            if(!hTextBox[key] || hTextBox[key] < $(this).height())
            {                           
                hTextBox[key] = $(this).height();        
            }
            if((i+1)%rowLimit == 0)
            {                             
                key++;                      
            }
        });  
        
        key = 0;
        $(this).find('.box-item').each(function(i){                         
            var boxHeight = parseInt(hTextBox[key]);
            var imgHeight = parseInt($(this).find('img').height());
            var textHeight = $(this).find('.box-item-text').height() + 10;
                                             
            $(this).css('height', boxHeight);                             
            var margin = boxHeight - textHeight;
            if(!isNaN(imgHeight))
                margin -= imgHeight;
