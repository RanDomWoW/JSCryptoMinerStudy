<script>
$(function(){
	$(document).click(function(e){ 
        if( $(e.target).attr('id') != 'dropdownGrade' && $(e.target).parent().attr('id') != 'dropdownGrade' && 
            $(e.target).attr('id') != 'dropdownSubject' && $(e.target).parent().attr('id') != 'dropdownSubject' && 
            $(e.target).attr('id') != 'dropdownType' && $(e.target).parent().attr('id') != 'dropdownType') // $(e.target).attr('id') != 'dropdownGrade' && $(e.target).parent().attr('id') != 'dropdownGrade' - repeat this pattern for other elements
        {   
            $('.catalogue-box.dropdown-drop-box').addClass('dispno');
        }
	});                                                                                                                                                                                    
    $('#dropdownGrade').click(function(){if(!$('#dropdownDropGrade').hasClass('dispno')) $('#dropdownDropGrade').addClass('dispno'); else $('#dropdownDropGrade').removeClass('dispno');});
    $('#dropdownSubject').click(function(){if(!$('#dropdownDropSubject').hasClass('dispno')) $('#dropdownDropSubject').addClass('dispno'); else $('#dropdownDropSubject').removeClass('dispno');});
    $('#dropdownType').click(function(){if(!$('#dropdownDropType').hasClass('dispno')) $('#dropdownDropType').addClass('dispno'); else $('#dropdownDropType').removeClass('dispno');});
});
</script>
																
