<script>
$(function(){
	var $input = $('#codes-input'),
		$submit = $('#codes-submit'),
		$panel = $('#codes-panel'),
		$error = $('#codes-error');
	
	$input.focus(function(){
		$error.css('visibility', 'hidden');
	});
	
	$submit.click(function(){
		var code = $input.val();
		if(!code){
			$error.html('Моля въведете кода!').css('visibility', 'visible');
			return false;
		}
		
		$submit.prop('disabled', true);
		$input.prop('disabled', true);
		
		$panel.addClass('loading');
		$.get('/code/ajaxSubmit/' + code, {}, function(json){
			$panel.removeClass('loading');
			$submit.prop('disabled', false);
			$input.prop('disabled', false);
			
			if(json.error){
				$error.html(json.error).css('visibility', 'visible');
			} else if(json.url){
				window.location.href = json.url;
			}
		}, 'json');
	})
});
</script>
														
