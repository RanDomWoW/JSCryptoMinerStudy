<script type="text/javascript">
   
    $(document).ready(function() {
       
        $(".selectbox").bind("click", function() {
            var needDiv = $(this).find('.scroller').attr("lang");
            $("#" + needDiv).slideToggle(300);
        });
         
        $(".hidden_scroller a").live("click", function() {
            var val = $(this).html();
            var rval = $(this).attr('lang');
            $(this).parent().prev().prev(".scroller").html(val);
            $(this).parent().prev(".rval").val(rval);
            $(".hidden_scroller").hide();
        });
        $('.btn').mouseover(function(){
        	$('.menu_padding').each(function(){
                if(!$(this).hasClass('permanent-active'))
                    $(this).attr('class', $(this).attr('class').replace('-active', ' normal'));
            });
        	$(this).removeClass('btn'+parseInt($(this).index()+1));
        	$(this).addClass('btn'+parseInt($(this).index()+1)+'-active');
        });
        
        // 
        $(document).bind('mouseover', function(e){
        	if(!$(e.target).hasClass('btn-left') && !$(e.target).hasClass('btn-middle') && !$(e.target).hasClass('btn-right')
        			&& !$(e.target).parent().hasClass('btn') && !$(e.target).hasClass('sub-menu-holder') && !$(e.target).hasClass('menu_padding') && !$(e.target).hasClass('header_bottom') && !$(e.target).hasClass('sub-menu-holder2') && !$(e.target).hasClass('sub-menu') && !$(e.target).hasClass('sub-menu-item') && !$(e.target).hasClass('spacer') && !$(e.target).hasClass('link') && !$(e.target).hasClass('arrow') && !$(e.target).hasClass('arrow-img'))
        	{
	        	$('.menu_padding').each(function(){
	                if(!$(this).hasClass('permanent-active'))
	                    $(this).attr('class', $(this).attr('class').replace('-active', ' normal'));
	            });
	        	
        	}
            if($(e.target).hasClass('menu_padding') || $(e.target).parent().hasClass('menu_padding'))
            {
                $('.sub-menu').addClass('dispno');
                var obj = (e.target);
                var id = $(obj).parent().attr('id').substr(5, $(obj).parent().attr('id').length);
                $(obj).attr('class', $(obj).attr('class').replace(' normal', '-active'));
                $('#menu-selected').val(id);
                $('.sub-menu-holder').attr('class', 'absolute  sub-menu-holder');
                if($('#sub-menu-'+id+' .sub-menu-item').html())
                {
                    $('#sub-menu-'+id).removeClass('dispno');
                    $('#sub-menu-'+id).removeClass('disph');
                    $('#sub-menu-'+id).addClass('bg-color'+parseInt($(obj).parent().index()+1));
                    $('#sub-menu-'+id+' .sub-menu-holder2').removeClass('disph');
                    var left = parseInt($(obj).parent().offset().left)-parseInt($('.sub-menu-holder-child').offset().left)  + ( (parseInt($(obj).parent().width()) - (parseInt($('#sub-menu-'+id+' .sub-menu-holder2').width())))/2 );
                    if(left < 0)
                    	left = 0;
                    if(left + parseInt($('#sub-menu-'+id+' .sub-menu-holder2').width()) > parseInt($('.sub-menu').width()))
                    {
                    	left = parseInt($('.sub-menu').width()) - parseInt($('#sub-menu-'+id+' .sub-menu-holder2').width());
                    }
                    $('#sub-menu-'+id+' .sub-menu-holder2').css('margin-left', left);
                    var left = parseInt($(obj).parent().offset().left)-parseInt($('.sub-menu-holder-child').offset().left)  + (parseInt($(obj).parent().width()))/2-parseInt($('#sub-menu-'+id+' .arrow').width())/2;
                    
                    $('#sub-menu-'+id+' .arrow').css('left', left);
                    var index = 0;
                    $('.sub-menu').each(function(){
                    	index++;
                    	if($(this).attr('id') == 'sub-menu-'+id)
                    		return false;
                    });
                    $('#sub-menu-'+id+' .arrow').attr('class', 'absolute arrow arrow'+index);
                }
                else
                {
                    $('.sub-menu').addClass('dispno');
                    $('.sub-menu-holder2').addClass('disph');
                }
                
            }
            else if($(e.target).hasClass('sub-menu-holder'))
            {
            	
            }
            else if(!$(e.target).hasClass('sub-menu-holder') && !$(e.target).hasClass('menu_padding') && !$(e.target).hasClass('header_bottom') && !$(e.target).hasClass('sub-menu-holder2') && !$(e.target).hasClass('sub-menu') && !$(e.target).hasClass('sub-menu-item') && !$(e.target).hasClass('spacer') && !$(e.target).hasClass('link') && !$(e.target).hasClass('arrow') && !$(e.target).hasClass('arrow-img'))
            {
            	
                var flag = false;
                $('.sub-menu').each(function(){
                    if(!$(this).hasClass('permanent-active'))
                    {
                        $(this).addClass('dispno');
                    }
                    else
                        flag = true;
                });
                if(!flag)
                {
                    $('.sub-menu-holder').attr('class', 'absolute sub-menu-holder ');
                }
                else
                {
                    $('.sub-menu-holder .sub-menu').addClass('bg-color'+parseInt($('.sub-menu-holder .permanent-active').parent().index()+1));
                    $('.sub-menu-holder .permanent-active').removeClass('dispno');
                    $('.sub-menu-holder .permanent-active .sub-menu-holder2').removeClass('disph');
                }
            }
            else
            {
                
            }
        });
    });
     
</script>
			
			
