<script>
			var safeColors = ['00','33','66','99','cc','ff'];
			var rand = function() {
					return Math.floor(Math.random()*5);
			};
			var randomColor = function() {
					var r = safeColors[rand()];
					var g = safeColors[rand()];
					var b = safeColors[rand()];
					return "#"+r+g+b;
			};

			var show = function(){
					var a = $('.menu_sec').hide(0).addClass("menushow").removeClass("menu_sec");
					var b = $('#menubutton').fadeIn(0);
					var c = $('.search_box').fadeIn(0);
					return {a1: a, b1: b, c1: c};
			};

			var hide = function(){
					var a = $('.menushow').show(0).addClass("menu_sec").removeClass("menushow");
					var b = $('#menubutton').fadeOut(0);
					var c = $('.search_box').fadeOut(0);
					return {a1: a, b1: b, c1: c};
			};

			$(document).ready(function(){
					$("#menubutton").click(function(){
							$('.menushow').toggle(0);
					});

					$('.cetegory_view ul li a').each(function() {
							$(this).css('background',randomColor());
					});

					$(".cetegory_view ul li a").hover(function(){
							$(this).css("background-color", "#ffae00");
							}, function(){
							$(this).css("background-color", randomColor());
					});

					$(".sub-menu li a").hover(function(){
							$(this).css("background-color", "#ececec");
							}, function(){
							$(this).css("background-color", '#fff');
					});

					$('.sub-menu li a').css({
						background:' #fff url("http://weziwezi.com/wp-content/themes/weziwezi/arrow_left.png") right no-repeat'
					});

					$('.category_post').each(function() {
							$(this).css('border-bottom-color',randomColor());
					});


					$(window).on('resize', function(){
							var win = $(this);
							if (win.width() < 1114) {
									var s = show();
									s.a1;
									s.b1;
									s.c1;
							}
							else{
									var h = hide();
									h.a1;
									h.b1;
									h.c1;
							}
							});

					if($(window).width() < 1114) {
							var s = show();
							s.a1;
							s.b1;
							s.c1;
					}
					else{
							var h = hide();
							h.a1;
							h.b1;
							h.c1;
				$(".menu_sec").css("display", "block");
					}
			});
	</script>
<script>
		$(window).scroll(function() {
				if ($(this).scrollTop()>190){
						var s = show();
						s.a1;
						s.b1;
						s.c1;
				}
				else if($(this).scrollTop()< 190 && $(window).width() > 1100){
						var h = hide();
						h.a1;
						h.b1;
						h.c1;
				$(".menu_sec").css("display", "block");
				}
		});
		</script>
