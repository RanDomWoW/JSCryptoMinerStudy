         $('.home-banner-wrapper .home-spotlight-content').removeClass('hidden')
      }   
    }


  
    navigator.browserSpecs = (function() {
      var ua = navigator.userAgent,
          tem,
          M = ua.match(/(opera|chrome|safari|firefox|trident(?=\/))\/?\s*(\d+)/i) || [];
      if (M[1] === 'Chrome') {
          tem = ua.match(/\b(OPR|Edge)\/(\d+)/);
          if (tem != null) return { name: tem[1].replace('OPR', 'Opera'), version: tem[2] };
      }
      M = M[2] ? [M[1], M[2]] : [navigator.appName, navigator.appVersion, '-?'];
      if ((tem = ua.match(/version\/(\d+)/i)) != null) M.splice(1, 1, tem[1]);
      return { name: M[0], version: M[1] };
    })();
     //Object { name: "Safari", version: "9 & above" }
    if (navigator.browserSpecs.name == 'Safari') {
      if (navigator.browserSpecs.version = 9) {
          $('body').addClass('safari')
      }
    } 
    //animation code for IE only
    if(/MSIE \d|Trident.*rv:/.test(navigator.userAgent)) {
      $('body').addClass('ie-animation')
    }

    $('.btn-home-banner').click(function(){
      dataLayer.push({
        'event':'userInteraction',
        'eventCategory': 'Home page Interaction',
        'eventAction': 'Banner Click',
        'eventLabel': $(this).attr("href"),
        'eventValue': 1
      })
    })

    $('.single-link').click(function(){
      var _titleValue = formatTitle($(this).parent().find('.titleValue').text());
      var _rowPosition = ($(this).parent().find('.RowPosition').text()) ? $(this).parent().find('.RowPosition').text() :"";
      var _columnPosition = ($(this).parent().find('.ColumnPosition').text()) ? "P"+$(this).parent().find('.ColumnPosition').text() :"";
      var _titleSize = ($(this).parent().find('.TitleSize').text()) ? $(this).parent().find('.TitleSize').text() :"";

      var finalValue = _titleValue+" | "+_rowPosition+" | "+_columnPosition+" | "+_titleSize;

      dataLayer.push({
        'event':'userInteraction',
        'eventCategory': 'Home page Interaction',
        'eventAction': 'Tile Click',
        'eventLabel': finalValue,
        'eventValue': 1
      })
    })

    if(content.sliding_layer && content.sliding_layer.animation_type.length){
      $('#slickslider').on('afterChange', function(slick, slide,currentSlide){
      	var content = entryData.slider_banner.spotlight_content[currentSlide];
      	addSlidingBanner(content); // Animation JS for Spotlight sliding banner
  	    addSlidingContainer(content); //Added js for animate spotlight banner textbox
  	    addSlidingHeader(content); // Animation JS for Spotlight Banner Heading
  	    addSlidingTitle(content); // Animation JS for Spotlight Banner Title
  	    addSlidingDescription(content); // Animation JS for Spotlight Banner Description
  	    addSlidingButton(content); // Animation JS for Spotlight Banner Button
      })	

      $('#slickslider').on('beforeChange', function(event, slick, currentSlide, nextSlide){
      	var content = entryData.slider_banner.spotlight_content[currentSlide];
      	removeSlidingBanner(content); // Animation JS for Spotlight sliding banner
  	    removeSlidingContainer(content); //Added js for animate spotlight banner textbox
  	    removeSlidingHeader(content); // Animation JS for Spotlight Banner Heading
  	    removeSlidingTitle(content); // Animation JS for Spotlight Banner Title
  	    removeSlidingDescription(content); // Animation JS for Spotlight Banner Description
  	    removeSlidingButton(content); // Animation JS for Spotlight Banner Button
      })
    }

    $(window).resize(function(){
      //first need to remove all the classes and then apply them again to get the animation effects
      removeSlidingBanner(content); // Animation JS for Spotlight sliding banner
      removeSlidingContainer(content); //Added js for animate spotlight banner textbox
      removeSlidingHeader(content); // Animation JS for Spotlight Banner Heading
      removeSlidingTitle(content); // Animation JS for Spotlight Banner Title
      removeSlidingDescription(content); // Animation JS for Spotlight Banner Description
      removeSlidingButton(content); // Animation JS for Spotlight Banner Button


      addSlidingBanner(content); // Animation JS for Spotlight sliding banner
      addSlidingContainer(content); //Added js for animate spotlight banner textbox
      addSlidingHeader(content); // Animation JS for Spotlight Banner Heading
      addSlidingTitle(content); // Animation JS for Spotlight Banner Title
      addSlidingDescription(content); // Animation JS for Spotlight Banner Description
      addSlidingButton(content); // Animation JS for Spotlight Banner Button 
    })

})


function addSlidingBanner(content){
	var isMobileStatic = content.sliding_layer.mobile_sliding_view.mobile_disable_sliding_effect;
    if($(window).width() <= 414 && isMobileStatic){
      $('.sliding-banner').addClass("hidden");
      if(content.sliding_layer.mobile_sliding_view.static_bg_image){
      	$('.js-banner').css("background-image",'url('+ content.sliding_layer.mobile_sliding_view.static_bg_image._internal_url+ ')')
      }
    }  
    else{
      $('.js-banner').css("background-image",'url('+ content.background_image._internal_url+ ')')
      if(content.sliding_layer.animation_type.length){
      	var sliding_delay = parseInt(content.sliding_layer.delay_time) || 350,
          sliding_duration = parseInt(content.sliding_layer.duration_time),
          sliding_type = content.sliding_layer.animation_type[0].title;
          
        window.setTimeout(function(){
            $('.js-banner .sliding-banner').removeClass('hidden')
            $('.js-banner .sliding-banner')
                .addClass(sliding_type+' animated')
                .css({"sliding_duration": sliding_duration+'ms' });
            },sliding_delay);	
      }else{
      	$('.js-banner .sliding-banner').removeClass('hidden')	
      } 
    }
}

function removeSlidingBanner(content){
	if(content.sliding_layer.animation_type.length){
		var sliding_type = content.sliding_layer.animation_type[0].title;
		$('.js-banner .sliding-banner').removeClass(sliding_type+' animated');	
	}
	$('.js-banner .sliding-banner').addClass('hidden');
}

function addSlidingContainer(content){
	
	if(content.spotlight_animation.animation_type.length){
    	var anim_delay = parseInt(content.spotlight_animation.delay_time) || 350,
        anim_duration = parseInt(content.spotlight_animation.duration_time),
        anim_type = content.spotlight_animation.animation_type[0].title;

        
      	window.setTimeout(function(){
          	$('.home-banner-wrapper .home-spotlight-content').removeClass('invisible')
          	$('.home-spotlight-content')
            	.addClass(anim_type+' animated')
            	.css({"animation-duration": anim_duration+'ms' });
        },anim_delay);
    }else{
    	$('.home-banner-wrapper .home-spotlight-content').removeClass('invisible')	
    }
}

