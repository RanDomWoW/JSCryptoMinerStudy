<script>
var clicks = 0;
var type = null;
$('button.newer, button.older').click(function() {
	
	type = $(this).val();
		
	if (type == "older") {
		clicks -= 1;
		if (clicks < 1) {
			$('button.older').prop('disabled', true);
		} if (clicks < 99) {
			$('button.newer').prop('disabled', false);
		}
	} else if (type == "newer") {
		clicks += 1;
		if (clicks >= 1) {
			$('button.older').prop('disabled', false);
		} if (clicks >= 99) {
			$('button.newer').prop('disabled', true);
		}
	}

	$.get("uploads.php", {oldal: clicks}).done(function(data){
		$("#uploads").html(data);
	});

});






