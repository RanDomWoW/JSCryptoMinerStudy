        function getNext(name) {
          var url = '/toaster/next';
          if (name) {
            url = '/toaster/next?name=' + name;
          }
          $.ajaxSetup({
            headers: {
              'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
            }
          });
          $.ajax({
            type: 'GET',
            url: url,
            dataType: 'json',
          }).done(function(data, status, jqXHR) {
            if(data != null){
              var name = data.name;
              //for toasters that are supposed to only show once
              if(data.priority == 0){
                $("#toaster").addClass("alert-success-a");
              } else if(data.priority >= 10){
                $("#toaster").addClass("alert-info-a");
              } 

              if (getCookie('toaster_'+name) != 'showed') {
                $("#close-toaster").data('name', data.name);
                $("#toaster-content").html(data.content);
                $("#general-toaster").attr('data-type', data.priority).attr('data-name', data.name).show();
              }

              if(data.priority == 10){
                setCookie("toaster_"+name, "showed", "10");
              } 

            } 
          }).fail(function(jqXHR, status, err) {
            console.log(err);
          });
        }
        $(document).ready(function () {
          if($('#top_toast').text()){
              $("#promo-toaster").show();
          }
          if($("#promo-toaster").length == 0) {
            getNext('');
          }

          $("#close-promo").click(function() {
            getNext('');
          });

          $("#close-toaster").click(function() {
            var name = $(this).data('name');
            setCookie("toaster_"+name, "showed", "10");
            getNext(name);
          });

          $("#new-feature-tour").click(function() {
            var name = $(this).data('name');
            getNext(name);
          });
        });
      </script>

