<script type="text/javascript" src="https://www.jobscan.co/script/bootstrap-notify-3.1.3/bootstrap-notify.min.js"></script>
<script>
  function showNotify(message, type, specificTitle) {
    var style_type = "pastel-info";
    var delay = 5000;
    var title = "Success";
    if (!type) {
      type = "info";
    }

    switch(type) {
      case "error" : 
        enteranimate = "animated bounceInDown";
        exitanimate = "animated bounceOutUp";
        delay = 10000;
        style_type = "pastel-danger";
        title = specificTitle || "Error";
        break;
      case "warning" : 
        enteranimate = "animated bounceInDown";
        exitanimate = "animated bounceOutUp";
        style_type = "pastel-warning";
        title = specificTitle || "Warning";
        break;
      case "success" : 
        enteranimate = "animated bounceInDown";
        exitanimate = "animated bounceOutUp";
        style_type = "pastel-success";
        title = specificTitle || "";
        break;
      default : 
        type = "info";
        enteranimate = 'animated fadeInRight';
        exitanimate = 'animated fadeOutRight';
        style_type = "pastel-info";
        title = specificTitle || "Info";
        break;
    }

    var rootNotify = $.notify({
      title: title,
      message: message,
    }, {
      type: style_type,
      allow_dismiss: true,
      // newest_on_top: true,
      placement: {
        from: "top",
        align: "center"
      },
      delay: 5000,
      timer: 1000,
      url_target: '_blank',
      mouse_over: "pause",
      animate: {
        enter: enteranimate,
        exit: exitanimate
      }
    });
  }

  $(document).ready(function(){
    // $('.alert-info').each
    $('.alert-info').each(function() {
      if ($(this).text().trim() != "") {
        $(this).hide();
        showNotify($(this).text(), "info");
      }
    });
    $('.alert-warning').each(function() {
      if ($(this).text().trim() != "") {
        $(this).hide();
        showNotify($(this).text(), "warning");
      }
    });
    $('.alert-danger').each(function() {
      if ($(this).text().trim() != "") {
        $(this).hide();
        showNotify($(this).text(), "error");
      }
    });
    $('.alert-success').each(function() {
      if ($(this).text().trim() != "") {
        $(this).hide();
        showNotify($(this).text(), "success");
      }
    });
  });
