<script>
    // a little trick to prevent page from jumping on load
    var target = window.location.hash;
    target = target.replace('#', '');
    window.location.hash = "";

    function checkMasterCvChange() {
      if ($("#cv").val() != $('#cv_hidden_master').val()) {
        $('#master_cv').prop('checked', false);
        $('#master_cv').attr('checked', false);
      }
    }

    $(document).ready(function() {

      
      $('#allowCoach').click(function(){
          mixpanel.track("[Main Page] - Allow To Upgrade To Premuim Plus", {
              "id": "0",
              "type": "guest"
          });
          sendMessageToCoachForUpgrade(true, $(this).val());
      });

      $('#denyCoach').click(function(){
          mixpanel.track("[Main Page] - Deny To Upgrade To Premuim Plus", {
              "id": "0",
              "type": "guest"
          });
          sendMessageToCoachForUpgrade(false, $(this).val());
      });
      function sendMessageToCoachForUpgrade(upgrade, coachEmail){
          $('#close-promo').click();
          $.ajaxSetup({
              headers: {
                  'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
              }
          });
          $.ajax({
              type: 'GET',
              url: '/acceptedUpgrade',
              data: {
                  upgrade: upgrade,
                  coachEmail: coachEmail,
              },
              success: function (msg) {
                  console.log("Message to Career Coach was sent");
              },
              error: function (msg) {
                  console.log("Message to Career Coach was not sent:" + msg);
              }
          })
      }

      var userId = "0";
      var client_id = localStorage.getItem("client-id");
      $('#client-id').val(client_id);

      var jsonResult = null;
      
      
      restoreCvJd();
      $('#clearCv').click(function() {
        sessionStorage.cv = '';
        $('#cv').val('');
        reportGaEvent('click', 'clear-cv');
        mixpanel.track("[Main Page] - Clear Resume", {
            "id": "0",
            "type": "guest"
        });
        return false;
      });
      $('#clearJd').click(function() {
        sessionStorage.jd = '';
        $('#jd').val('');
        reportGaEvent('click', 'clear-jd');
        mixpanel.track("[Main Page] - Clear Job Description", {
            "id": "0",
            "type": "guest"
        });
        return false;
      });

      $("#btnAnalyze").click(function() {
        $("#step1-title-error").fadeOut('slow');
        checkMasterCvChange();
        // $('#cvErr').text('');
        // $('#jdErr').text('');
        if (($("#cv").val().length < 100) || ($("#jd").val().length < 100)) {
          if ($("#cv").val().length < 100) {
            $("#step1-title").html('Please enter a resume before scanning.');
            // $('#cvErr').text('Please enter a resume before scanning');
            $("#step1-title-error").fadeIn('slow');
          }
          else if ($("#jd").val().length < 100) {
            $("#step1-title").html('Please enter a job description before scanning.');
            // $('#jdErr').text('Please enter a job description before scanning');
            $("#step1-title-error").fadeIn('slow');
          }
          return false;
        }
        storeCvJd();
        mixpanel.track("[Main Page] - Scanned Resume", {
            "click-source": "",
            "id": "0",
            "type": "guest"
        });
        $("#btnAnalyze").html("Scanning...")
      });
      
      
      $('#trySampleCta').click(function() {
        mixpanel.track("[Main Page] - Try Sample Resume", {
          "id": "0",
          "click-source": 'topCTA',
          "type": "guest"
        });

        trySample(false);
      });
      $('#trySample').click(function() {
        mixpanel.track("[Main Page] - Try Sample Resume", {
          "id": "0",
          "click-source": 'bottomCTA',
          "type": "guest"
        });

        trySample(false);
      });
      $('.todo-link').click(function() {
        mixpanel.track("[Main Page] - To Do List Click", {
          "id": "0",
          "click-source": $(this).text(),
          "type": "guest"
        });
      });
      $('.blog-nav-bar').click(function() {
        mixpanel.track("[Main Page] - Blog Page List Click", {
          "id": "0",
          "click-source": $(this).text(),
          "type": "guest"
        });
      });
      $('.scan-item-link').click(function() {
        mixpanel.track("[Main Page] - Recent Scan Click", {
          "id": "0",
          "link-type" : $(this).data('linktype'),
          "type": "guest"
        });
      });
      $( "#a-la-carte" ).load(function() {
        mixpanel.track("[Main Page] - A La Carte Loaded", {
