<script type="text/javascript">
    var nfrm = $('#news-form');
    nfrm.submit(function (ev) {
        var em = $("#news-form #email").val();
        if (IsEmptyString(em) || checkemail(em)) {
            $("#news-form #email").focus();
            $('<span class="error-msg">Neispravna e-mail adresa</span>').appendTo('#news-form');
            $('#news-form').parent('.form-group').addClass('error');
            setTimeout(function () {
                $('.error-msg').remove();
            }, 5000);
            $(document).click(function () { $('.error-msg').remove(); });
            return false;
        }
        $.ajax({
            type: nfrm.attr('method'),
            url: nfrm.attr('action'),
            data: nfrm.serialize(),
            dataType: 'json',
            success: function (data) {
                if (data.status != "Success") {
                    $('#news-form .alert').css("display", "block");
                    $('#news-form .alert').html('Greška prilikom prijave').delay(5000).fadeOut();
                    $('#news-form').parent('.form-group').removeClass('error');
                }
                else {
                    $('#news-form .alert').css("display", "block");
                    $('#news-form .alert').html('Hvala Vam na prijavi!').delay(10000).fadeOut();
                    $('#news-form').parent('.form-group').removeClass('error');
                }
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                $('#news-form .alert').css("display", "block");
                $('#news-form .alert').html('Greška prilikom prijave').delay(5000).fadeOut();
                $('#news-form').parent('.form-group').removeClass('error');
        }
        });
        ev.preventDefault();
    });
    function IsEmptyString(str) {
        return str.replace(/^\s+|\s+$/g, '').length == 0;
    }
    function checkemail(str) {
        return str.search(/\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/) == -1;
    }
</script>




