$(document).on('click','.btn_mobile_slideMenu', function() {
	$('body').append("<div id='mobile_background_disabled'></div>");
	$('#mobile_topMenuZone').show().css({left:'-200px'}).animate({ left:'0' }, 200);
});
$(document).on('click','.btn_mobile_slideMenu_hide', function() {
	$('#mobile_topMenuZone').animate({ left:'-300px' }, 200,function() {
		$(this).hide();
		$('#mobile_background_disabled').remove();
	});

});
$(document).on('click','#mobile_topMenuZone .submenu_title',function() {

	if( $('.mobile_menu_sub > ul', $(this).parent()).is('.on') != true ) {

		$('.mobile_menu_sub > ul:visible').slideUp().removeClass('on');
		$('.mobile_menu_sub > ul', $(this).parent()).slideDown().addClass('on');

	}
});


