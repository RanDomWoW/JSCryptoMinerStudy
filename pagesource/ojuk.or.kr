	<script type='text/javascript'>
	var dz_userDevice;
	$(document).ready(function() {
		dz_screen_wrap = dz_screen_wrap_reset();

		var user_Agent=navigator.userAgent.toLowerCase();
		var Ret_1=user_Agent.search("android");
		var Ret_2=user_Agent.search("iphone");
		dz_userDevice = (Ret_1>-1||Ret_2>-1) ? 'mobile' : 'desktop';


		if(!xURL_String('xch')) {
			$('body').backstretch(['/images/main/maincover_1.jpg','/images/main/maincover_2.jpg','/images/main/maincover_3.jpg','/images/main/maincover_4.jpg'], { fade: 2000, duration: 7000 } );

		}

		$('#global_menu > ul > li').mouseenter(function(i) {

			var idx = $(this).index();
			var idxcolor = ['black','blue','red','yellow','white'];
			$('#global_menu > ul > li .progressline').stop().animate({ width: '0' },200, function() {
				$(this).remove();
			});

			$(this).prepend("<div class='progressline "+idxcolor[idx]+"'></div>");
			$('.progressline',$(this)).stop().animate({ width: '100%' });

			var obj_nav = $('nav', $(this));
			if( $('.global_menu_sub:visible').length>0) {
				var obj_width = $('.global_menu_sub:visible').width();

				$('.global_menu_sub:visible > *').hide();
				$('.global_menu_sub:visible').css({ width:0 }).hide();
				if(obj_width == obj_nav.attr('width')) {
					obj_nav.css('width', obj_width+'px' ).show().css({ width: obj_nav.attr('data-width')+'px' });
				} else	obj_nav.css('width', obj_width+'px' ).show().stop().animate({ width: obj_nav.attr('data-width')+'px' });
				$('nav > *', $(this)).fadeIn();

			} else {
				$('nav:hidden',$(this)).show().stop().animate({ width: obj_nav.attr('data-width')+'px' }, 200, function() {
					$('*', $(this)).fadeIn();
				});
			}

		}).mouseleave(function() {
			$('#global_menu > ul > li .progressline').stop().animate({ width: '0' },100, function() {
				$(this).remove();
			});

			$('nav > *',$(this)).hide();
			$('nav', $(this)).stop().animate({ width:0 }, 300, function() {
				$(this).hide();
			});
		});
	});
/*
	$(document).scroll(function() {
		if( $(document).scrollTop() > 130 ) {
			$('#subContents').addClass('fixedtop');
		} else	$('#subContents').removeClass('fixedtop');
	});
*/
	var dz_screen_wrap,dz_device_wrap;
	$(window).resize(function() {
		dz_screen_wrap = dz_screen_wrap_reset();
	});
	function dz_screen_wrap_reset() {
		var this_screen;
		var screensize = $('body').width();
		if(screensize <= 1200) {
			this_screen = 'edge_normal';
			if(screensize <= 700) {
				dz_device_wrap = 'mobile';
			}
		} else
		if(screensize < 1400) {
			this_screen = 'edge_general';
		} else
		if(screensize>=1400) {
			this_screen = 'edge_wide';
		}
		if(dz_screen_wrap!=this_screen) {
			$('body').removeClass('edge_normal edge_general edge_wide').addClass(this_screen);
			return this_screen;
		} else	return dz_screen_wrap;
	}











function dz_swalalert(param,func) {
	param.width= param.width ? param.width : 500;
	param.padding= param.padding ? param.padding : 20;
	param.type=param.type?param.type:'error';
	param.confirmButtonText=param.confirmButtonText?param.confirmButtonText:'확인';
	param.animation=param.animation?param.animation:true;

	param.confirmButtonClass=param.confirmButtonClass?param.confirmButtonClass:'btn btn-lg btn-theme ph50 w100p';
	param.buttonsStyling=param.buttonsStyling?param.buttonsStyling:false;

	if(dz_device_wrap=='mobile') {
		param.type=false;
	}

	if(func) {
		swal(param).then(function () {
			func();
		});
	} else	swal(param);
	return true;
}


