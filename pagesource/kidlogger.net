    if(! defaultTz)
        defaultTz = moment.tz.guess();

    Cookies.set('tz', defaultTz);

            function s4() {
            return Math.floor((1 + Math.random()) * 0x10000)
                .toString(16)
                .substring(1);
        }

        function guid() {
            return s4() + s4() + '-' + s4() + '-' + s4() + '-' +
                s4() + '-' + s4() + s4() + s4();
        }

        if(! getCookie('cookie-guid'))
            setCookie('cookie-guid', guid(), {
                expires: 3600 * 24 * 30 * 12 * 10
            });
    </script>

</body>
