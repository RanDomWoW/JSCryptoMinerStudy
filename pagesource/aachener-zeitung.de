}

/* links */
.fupa_widget > div a {
    color: #278bcf;
    text-decoration: none;
}

.fupa_widget > div a:visited {
    color: #278bcf;
}

.fupa_widget > div a:focus {
    color: #278bcf;
}

.fupa_widget > div a:hover {
    color: #278bcf;
}

.fupa_widget > div a:active {
    color: #278bcf;
}

.fupa_widget > div a:hover {
    text-decoration: underline;
}

.fupa_widget > div a:hover {
    text-decoration: none;
    color: #61ade1;
}

.fupa_widget > div a.more {
    margin-left: 0.1em;
    margin-right: 0.3em;
}

.fupa_widget > div a.featured {
    font-size: 14px;
    font-weight: bold;
}


/* liveticker */

.fupa_widget > div table.widget_liveticker tr.widget_liveticker_klicks td {
    font-size: 15px;
    padding-bottom: 10px;
}

.widget_team_widget_liveticker .navileiste_zusatz {
    margin-top: 10px;
}

.widget_team_widget_liveticker .aktualisieren {
    -webkit-font-smoothing: antialiased;
    font-size: 16px;
    font-family: Roboto,Helvetica,Arial,sans-serif;
    text-transform: none;
    font-weight: bold;
    border: none;
    margin-bottom: 2px;
    margin-top: 20px;
    width: auto;
    color: #FFF;
    background: #0085c7;
    background: -owg(linear-gradient(to bottom, #008fd6, #007bb8));
    background: -webkit(linear-gradient(to bottom, #008fd6, #007bb8));
    background: -moz(linear-gradient(to bottom, #008fd6, #007bb8));
    background: -o(linear-gradient(to bottom, #008fd6, #007bb8));
    background: -webkit-linear-gradient(top, #008fd6, #007bb8);
    background: linear-gradient(to bottom, #008fd6, #007bb8);
    -webkit-box-shadow: none;
    -moz-box-shadow: none;
    box-shadow: none;
    border: none;
    text-shadow: none;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    -ms-border-radius: 3px;
    -o-border-radius: 3px;
    border-radius: 3px;
}


/* tabelle */

.fupa_widget a.btn-small {
    font-size: 12px;
    font-family: Roboto,Helvetica,Arial,sans-serif;
    text-transform: none;
    font-weight: bold;
    border: none;
    margin-bottom: 2px;
    margin-top: 20px;
    width: auto;
    color: #333;
}

.fupa_widget .widget_team_widget_tabelle tr.tabelle_farbe_meister td:first-child {
    background: #2174ab;
    color: white;
}

.fupa_widget .widget_team_widget_tabelle tr.tabelle_farbe_aufsteiger_r td:first-child {
    background: #2991d6;
    color: white;
}

/* kader statistik */ 

.fupa_widget .widget_team_widget_statistik,
.fupa_widget .widget_team_widget_statistik a
 {
	font-size:14px;
}
.fupa_widget .content_table_std td {
    padding: 0.1rem 0.75rem;
    padding-left:0px;
    padding-right:0px;
}

.w-fupa-l h4 {
background-color: #f3f3f3;
    line-height: 16px;
    padding: 0.5rem;
    margin-bottom: 0.2rem;
}

@media (min-width: 480px) {
.w-fupa-l {
    height: 320px !important;
}
.w-fupa-l .widget_team_widget_liveticker .aktualisieren {
    margin-top:56px;
}
}

/* spielplan */
.fupa_widget .fupa_match {
	width:30px;
}
.fupa_widget .widget_team_widget_spielplan a.fupa_table_entry > div:last-child .icon {
    position: absolute;
    right: 5px;
    margin-top: 8px;
}
.fupa_table_entry .short {
	display:none;
}
.fupa_table_entry .long {
	display:none;
}
.widget_team_widget_spielplan > h3 {
	margin-bottom:10px;
	font-family: Roboto,Helvetica,Arial,sans-serif;
    color: #333;
    font-size: 16px;
    font-weight: 700;
    text-transform: uppercase;
    background-color: #ffffff;
}
.widget_team_widget_spielplan > h3 + div {
	margin-bottom:30px;
}
.widget_team_widget_spielplan a.fupa_table_entry > div.spieltag { 
	line-height:40px;
}

