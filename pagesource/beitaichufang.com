<script>
(function(){
var baiduTimer = window.setTimeout(function(){}) - 1;
var banner = $('ins[data-baidu-clb-type="top-down"]');
var makeSpan = function(text) {
    return $('<span style="z-index:1000; background-color:#fff; position:absolute; right:0; bottom:0; cursor:pointer;"></span>').text(text).click(function() {
        $(this).parent().hide().siblings().show(function(){
            var $this = $(this);
            // <ins> is inline element
            $this.parent().height($this.css('display', 'block').height());
			status.css('top',banner.height()+'px');
        });
        // remove timer if there is still any, a.k.a, closed before impressionTime is over
        window.clearTimeout(baiduTimer);
        return false;
    });
};
banner.children().first().append(makeSpan('关闭')).end().last().append(makeSpan('展开'));
banner.insertBefore($('#pane')).css('z-index', '8000');
var x=0;
var status=$("#status");
window.onscroll=function(){
	status.css('top','0px');
	x=(document.body.scrollTop||document.documentElement.scrollTop);
	if (x <= 15&&banner.html()!=null){
		status.css('top',banner.height()+'px');
	}
};
window.onresize=function(){
	status.css('top','0px');
	x=(document.body.scrollTop||document.documentElement.scrollTop);
	if (x <= 15&&banner.html()!=null){
		status.css('top',banner.height()+'px');
	}};
})();
</script>


