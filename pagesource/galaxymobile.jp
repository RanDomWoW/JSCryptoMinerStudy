		 */
		var momentLoadFlag = false;
		var sec = $('.section-main'), len = sec.length, momentCon = $('.main-instargram');
		win.on("scroll.main", function(){
			var sp = win.scrollTop(), iw = window.__getInnerHeight(),count=0;
			sec.each(function(){
				var top = $(this).offset().top;
				if( sp + iw > top && $(this).data("visible") == undefined){
					$(this).data("visible", "visible");
					len--;
					if($(this).hasClass('main-instargram')){
						//보고후 적용
						momentLoad();
					}
					$(this).animate({'opacity':1},600);
				};
				if(len == 0) win.off("scroll.main");
			});
			if(!momentLoadFlag && momentCon.offset().top- momentCon.height()/2 < sp + iw){
				momentLoad();
			}

		});
		function momentLoad(){
			if(momentLoadFlag) return false;
			momentLoadFlag = true;
			GALAXYJP.net.send("momentsMedia", moments, function(res){
				var momentsMediaData = JSON.parse(res.responseText).response.resultData.mediaList;
				for(var i=0;i<momentsMediaData.length;i++){
					momentsMediaData[i] = $.extend({}, cdata, momentsMediaData[i]);
				};
				setMoments(momentsMediaData);
			});
		};

		if(window.GALAXYJP.imgRoadCom) $(window).scroll();
		window.itemShow = function(t){
			//보고후 적용 dom 생성시 opa 0 처리 함께
			$(t).animate({'opacity':1},400);
		};

		setTimeout(function(){
			if(!momentLoadFlag){
				momentLoad();
			}
		},3000);
		function setMoments(data){
			var i =1, len = data.length, l;
			if(len < 7){
				$('.sec-instargram').remove();
				return false;
			}
			momentsP= '<div class="slide-inner">'
					+	'<div class="slide d-slider">'
					+			'<ul class="d-item">'
					+				'<li class="big"><a href="/discover/moment/?cd='+data[0].insgId+'" data-omni-action="home:mmt:thumb:page-to:dsc^mmt^list"><div class="center"><figure><img src="'+data[0].standUrl+'" alt="'+data[0].text+'" class="'+((data[0].standWidth == data[0].standHeight)? "" : (data[0].standWidth > data[0].standHeight)? "landscape" : "portrait")+'" style="opacity:0;" onload="itemShow(this)"/></figure></div></a></li>'
					+				'<li class="set">'
					+					'<ul>';
			for(i=1;i<7;i++){
				l = data[i];
				momentsP += 					'<li><a href="/discover/moment/?cd='+l.insgId+'" data-omni-action="home:mmt:thumb:page-to:dsc^mmt^list"><div class="center"><figure><img src="'+l.standUrl+'" alt="'+l.text+'" class="'+((l.standWidth == l.standHeight)? "" : (l.standWidth > l.standHeight)? "landscape" : "portrait")+'" style="opacity:0;" onload="itemShow(this)"/></div></figure></a></li>'
			}
			momentsP+=					'</ul>'
					+				'</li>'
					+			'</ul>'
