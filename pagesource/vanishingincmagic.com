	<script type="text/javascript">	
		$( document ).ready(function()
		{
			$('.carousel').carousel();
			
			var boolAllowedToPlay = true;
			var boolManuallyPaused = true;
			
            
			$('#homepage-video').waypoint(function(direction) {
				if (!boolAllowedToPlay)
					return;
					
				var f = $('#homepage-video iframe');
	    		url = f.attr('src').split('?')[0];
	    			
				if (direction == "down")
				{			
	    			f[0].contentWindow.postMessage(JSON.stringify({method:'setVolume', value:'0.4' }), url);	
		            f[0].contentWindow.postMessage(JSON.stringify({method:'play' }), url);	
				}
				else
				{
					boolManuallyPaused = false;
					f[0].contentWindow.postMessage(JSON.stringify({method:'pause' }), url);
				}
			},
			{ offset: '50%' });
			
			var player = $('#homepage-video iframe');
		    var url = window.location.protocol + player.attr('src').split('?')[0];
		
		    // Listen for messages from the player
		    var iframe = $('#homepage-video iframe')[0];
		    var player = $f(iframe);
		
		    player.addEvent('ready', function() {
		        player.addEvent('pause', function() { 
		        	if (boolManuallyPaused)
		        		boolAllowedToPlay = false;
		        });
		        
		        player.addEvent('play', function() { 
		        	boolManuallyPaused = true;
		        	boolAllowedToPlay = true;
		        });
		    });

            
		});
		
		function sendToNewsletterSignup()
		{
			location.href = "/account/signup/?h=28&email_address=" + $("#ctl00_ContentPlaceHolder1_EmailAddress").val();
		}
		
		$("ctl00_ContentPlaceHolder1_EmailAddress").keyup(function(event){
		  if(event.keyCode == 13){
		  	sendToNewsletterSignup();
		  	return false;
		  }
		});
		
		$("#popup-email").keyup(function(event){
		  if(event.keyCode == 13){
		  	location.href = "/account/signup/?h=29&email_address=" + $("#popup-email").val();
		  	return false; 
		  }
		});	
		
		$("#popup-button").click(function(event){
			if ($("#popup-email").val() == '')
				$("#popup-email").shake();
			else
		  		location.href = "/account/signup/?h=29&email_address=" + $("#popup-email").val();
		});

		jQuery.fn.shake = function(intShakes, intDistance, intDuration) {
		    intShakes = intShakes || 10;
		    intDistance = intDistance || 2;
		    intDuration = intDuration || 1000;
		  
		    this.each(function() {
		        $(this).css("position","relative"); 
		        for (var x=1; x<=intShakes; x++) {
		        $(this).animate({left:(intDistance*-1)}, (((intDuration/intShakes)/4)))
		    .animate({left:intDistance}, ((intDuration/intShakes)/2))
		    .animate({left:0}, (((intDuration/intShakes)/4)));
		    }
		  });
		return this;
		};

		$('.close').click(function (e) {

		    $.cookie('alert-box', 'closed', { path: '/' });

		});
	</script>
	
