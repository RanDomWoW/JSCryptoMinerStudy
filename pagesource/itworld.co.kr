<script>
if(checkDevice()=='desktop'){
	$('head').append('<link rel="stylesheet" type="text/css" href="/_css/core/_web_fonts.css" />');
	$('head').append('<link rel="stylesheet" type="text/css" href="/_css/core/_desktop.css" />');
