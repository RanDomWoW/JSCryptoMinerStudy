						break;
						case('springboaard'):
							ex_count_springboard();
						break;
						case('billboard'):
							ex_count_billboard();
						break;
						case('mobile_welcome'):
							ex_count_mobile_welcome();
						break;
						case('leaderboard'):
							ex_count_leaderboard();
						break;
						default:
							//ex_count_top();
						break;
					}
					$('#nano_div').css({'display':'block'});
				}
				openFlg = 1;
			}else{
				$('#nano_div').css({'display':'none'});
				openFlg=0;
			};
		};

