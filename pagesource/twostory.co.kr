<script type="text/javascript">
var display_option_stock = function (opt_type) {
    var _form = document.detailform;
    var option_stock_display = 'NO';
    if (option_stock_display == 'NO') { return; }

    switch (opt_type) {
        case 'NL':
            if (typeof _form.spcode != 'undefined' && _form.spcode.value.length > 0) {     // spcode ∞° º±≈√¿Ã µ«æ˙¿ª∂ß∏∏ Ω««‡
                if (typeof _form.spcode2 != 'undefined' && _form.spcode2.type != 'hidden') {
                    for (i = 0; i < _form.spcode2.length; i++) {
                        _opt = _form.spcode2.options[i];
                        if (_opt.value.length == 0) {
                            continue;
                        }
                        _opt.text = ori_opt_text[i - 2];     // √ ±‚»≠
                        opt_stock = num[((i-2) * 10) + (_form.spcode.selectedIndex - 2)];
                        if (opt_stock == 100000) {
                            if (option_stock_display == 'DISPLAY') {
                                _opt.text += " (¿Á∞Ìºˆ∑Æ : π´¡¶«—)";
                            }
                        } else if (opt_stock > 0) {
                            _opt.text = _opt.text + " (¿Á∞Ìºˆ∑Æ : " + opt_stock + " ∞≥)";
                        }
                    }
                }
            }
            break;
        case 'PS':
            if (Object.keys(stockInfo).length > 0) {        // ¿Á∞Ì ¡§∫∏∞° ¡∏¿Á«“∞ÊøÏ∏∏ Ω««‡
                var select_elements = document.getElementsByName('optionlist[]');     // object or collection return µ 
                
                // 1. « ºˆ¿Œ ø…º« element∏∏ ªÃ¥¬¥Ÿ.
                var _mandatory_sel = []
                  , _mandatory_sel_idx = []
                  , _stock_key = [];
                for (var i = 0, sel_cnt = select_elements.length; i < sel_cnt; i++) {
                    var _sel = select_elements[i];
                    if (_sel.getAttribute('mandatory') == 'Y') {
                        _mandatory_sel.push(_sel);
                        _mandatory_sel_idx.push(i);
                    }
                }

                // 2. stock key∏¶ ªÃ¥¬¥Ÿ. (« ºˆ ø…º«¿« ∞πºˆ - 1 ¿∫ ∏∂¡ˆ∏∑ « ºˆø…º« ¿Ã¿¸±Ó¡ˆ¿« ø…º«µÈ)
                for (var i = 0, _idx_cnt = _mandatory_sel_idx.length - 1; i < _idx_cnt; i++) {
                    if (select_elements[_mandatory_sel_idx[i]].value.length > 0) {
                        _stock_key.push(select_elements[_mandatory_sel_idx[i]].options.selectedIndex - 1);
                    }
                }

                // « ºˆø…º«¿Ã 1∞≥ ¿Ã∏È ø©±‚º≠ ∞¸∑√∫Øºˆ ∫Ø∞Ê
                if (_mandatory_sel_idx.length == 1) {
                    _stock_key.push(0);
                }


                // 3. stock key∞° ¿÷¿∏∏È ∏∂¡ˆ∏∑ ø…º«ø° options∏¶ ºˆ¡§«ÿ¡ÿ¥Ÿ
                if (_stock_key.length > 0) {
                    for (var i = 0; i < _mandatory_sel[_idx_cnt].length; i++) {      // ∏∂¡ˆ∏∑ element
                        var _opt = _mandatory_sel[_idx_cnt].options[i];
                        if (_opt.value.length > 0) {
                            _opt.text = _opt.getAttribute('origin');                    // √ ±‚»≠
                            _real_stock_key = _mandatory_sel_idx.length == 1 ? (i - 1) : _stock_key.join(',') + ',' + (i - 1);

                            if (option_stock_display == 'DISPLAY' && stockInfo[_real_stock_key] == -1) {
                                _opt.text += " (¿Á∞Ìºˆ∑Æ : π´¡¶«—)";
                            }
                            if (stockInfo[_real_stock_key] > 0) {
                                _opt.text += " (¿Á∞Ìºˆ∑Æ : " + stockInfo[_real_stock_key] + " ∞≥)";
                            }
                        }
                    }
                }
            }
            break;
    }
};
</script>
