    jQuery(document).ready(function ($) {
        // initialize FlexNav
        $(".flexnav").flexNav();

        var $btnAumentar = $(".btnAumentar");
        var $btnDiminuir = $(".btnDiminuir");
        var $elemento = $("body #pagina").find("*"); //encontra todos os elementos dentro do #content
        var fonts = [];

        function obterTamanhoFonte() {
            for (var i = 0; i < $elemento.length; i++) {
                fonts.push(parseFloat($elemento.eq(i).css('font-size')));
            }
        }

        obterTamanhoFonte();
        $btnAumentar.on('click', function () {
            console.log($elemento);
            for (var i = 0; i < $elemento.length; i++) {
                ++fonts[i];
                $elemento.eq(i).css('font-size', fonts[i]);
            }
        });

        $btnDiminuir.on('click', function () {
            for (var i = 0; i < $elemento.length; i++) {
                --fonts[i];
                $elemento.eq(i).css('font-size', fonts[i]);
            }
        });

        $(function () {
            var height = screen.height - 130;
            $('.menu-height').height(height);
        });
    });

    $('.botoes-acessibilidade').find('a').not('.btnDiminuir').not('.btnAumentar').click(function () {
        var classeAtiva = $(this).data('class');
        console.log("classeAtiva", classeAtiva);
        var possibilidades = ['black', 'yellow', 'blue', 'white'];
        possibilidades.forEach(function (elemento) {
            if (classeAtiva == elemento) {
                $("html").addClass(elemento);
                Cookies.set('acessibilidade', elemento, { expires: 7});
            } else {
                $("html").removeClass(elemento);
            }
        });

    });

    var cor = Cookies.get('acessibilidade');
    if (cor) {
        $("html").addClass(cor);
    }

    var d = new Date(new Date().getTime() + 1000 * 120 * 120 * 2000);

    // default example
    simplyCountdown('.simply-countdown-one', {
        year: d.getFullYear(),
        month: d.getMonth() + 1,
        day: d.getDate()
    });

    //jQuery example
    $('#simply-countdown-losange').simplyCountdown({
        year: d.getFullYear(),
        month: d.getMonth() + 1,
        day: d.getDate(),
        enableUtc: false
    });


    var trateClickMenu = function (e) {
        if ($(".side-menu-wrapper").hasClass("menuaberto")) {
            $('html').addClass('body-overflow')
        } else {
            $('html').removeClass('body-overflow')
        }
    };
