                    items: 3,
                    nav: true
                },
                1000: {
                    items: 4,
                    nav: true,
                    loop: false,
                    margin: 20
                }
            }
        })
    });


    $(function () {
        var nav = $('#menuHeader');
        $(window).scroll(function () {
            var cursosAberto = ($("#li-cursos").hasClass("cbp-hropen"));
            var campusAberto = ($("#li-campus").hasClass("cbp-hropen"));
            if ( (($(this).scrollTop() > 179) && !(cursosAberto || campusAberto))  ) {
                nav.addClass("menu-fixo");
            } else {
                nav.removeClass("menu-fixo");
            }
        });
    });

    var slidebar_width = 350; //slidebar width + padding size
    var slide_bar = $(".side-menu-wrapper"); //slidebar
    var slide_open_btn = $(".slide-menu-open"); //slidebar close btn
    var slide_close_btn = $(".menu-close"); //slidebar close btn
    var overlay = $(".side-menu-overlay"); //slidebar close btn

    slide_open_btn.click(function (e) {
        e.preventDefault();
        slide_bar.addClass("menuaberto");
        slide_bar.css({"left": "0px"}); //change to "right" for right positioned menu
        overlay.css({"opacity": "1", "width": "100%"});
    });
    slide_close_btn.click(function (e) {
        e.preventDefault();
        slide_bar.removeClass("menuaberto");
        slide_bar.css({"left": "-" + slidebar_width + "px"}); //change to "right" for right positioned menu
        overlay.css({"opacity": "0", "width": "0"});
        trateClickMenu();
    });


