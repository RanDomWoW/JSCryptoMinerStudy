	var slideshowIsPaused = false;
	function createSlideshow(slide){
		$('#carousel').css('overflow','hidden').height('430px'); // Hardcode height: used to be dynamic, but Firefox 25.0 had race condition

		// fetch the carousel div slide by tab_id and get id index
		var id = $('div[tab-id="' + slide + '"]').attr('id');
		if(id) {
			starting_slide = id.slice(-1);
		} else {
			starting_slide = 0;
		}
		
		setTimeout(function() {
			$('#carousel').css('overflow', 'auto');
            $('#carousel').css('margin-left', '-800px');
            $('#carousel').css('left', '50%');
			$('#carousel').cycle({
				//activePagerClass: 'activeSlide',
				fx: 'scrollHorz',
				timeout: 0,
				//manualTrump: false,
				//pager: '#nav_ss',
				//pauseOnPagerHover: true,
				//startingSlide: starting_slide,
				//sync: true,
				//pagerAnchorBuilder: pagerFactory,
                next: '#carousel-next',
                prev: '#carousel-prev',
                width: 1600,
                fit: false
			});
			$('#carousel, #nav_ss').click(function() {
				if(!slideshowIsPaused){
					$('#carousel').cycle('toggle');
					slideshowIsPaused=true;
				}
			});
		},100);
	}

	function pagerFactory(idx, slide) { // this gets run VERY LAST for some reason!
		var last = '';
		if($('#carousel div.home_page_module').size() == idx + 1) {
			last = ' class="last"';
		}
		var new_title = $(slide).find('.banner_image').attr('title');
		$(slide).find('.banner_image').removeAttr('title');
		var anchor = $(slide).attr('tab-id');
		if(!anchor) anchor = '';
		return '<li' + last + '><a class="asset" href="#' + anchor + '">' + new_title + '</a></li>';
	};

</script>
