<script>
    $('#search form').submit(function () {
        return ($('#q').val() != '') ? true : false;
    });

    function animatertbs() {
        $('#t_rtb_first').delay(1000).animate({color: '#f7c03e'}, 500);
        $('#t_rtb_first').delay(2000).animate({color: '#7c601f'}, 500, function () {

            $('#t_rtb_second').animate({color: '#f7c03e'}, 500);
            $('#t_rtb_second').delay(2000).animate({color: '#7c601f'}, 500, function () {

                $('#t_rtb_third').animate({color: '#f7c03e'}, 500);
                $('#t_rtb_third').delay(2000).animate({color: '#7c601f'}, 500);
            });
        });
    }

    function loginErrors(json, stark_json) {
        if (stark_json['acctVerify']) {
            $('#acct-login').hide();
            $('#verify-account').show();
            if (stark_json['1800']) {
                //console.log($('#login_type_returning').val());
                $('#verify_pass_email').val($('#signin_flyout__ret_username').val());
                $('#verify-text').text("Please enter your email address to reset your password.");
            }
            //$('#verify_pass_email').attr('value', $('#signin_flyout__ret_username').val() );
            //$('#verify_pass_email').focus();
        } else {
            $('.signin form').errorfy();
        }
    }

    function closeLoginOK(url, stark_json) {
        var val = $('input[name="login_type"]').filter(':checked').val();
        window.location.reload();
    }

    //favorites
    var fave_count = 0;
    var fave_list = [];
    var menuFavorites = document.getElementById('favorites_menu');
    var presentationFavorites = document.getElementById('favorites');
    var timeout = null;

    function countFavorite(downUp, change) {
        if (change) {
            if (downUp) {
                fave_count++;
            } else {
                fave_count--;
            }
        }
        //GLDP-724 vluongo
        try {
            var cont = document.getElementById('favorite_count');
            cont.innerHTML = fave_count;
        } catch (e) {
            cont.innerHTML = '0';
        }
    }

    function showFavorites() {
        //GLDP-724 vluongo
        try {

            while (menuFavorites.firstChild) {
                menuFavorites.removeChild(menuFavorites.firstChild);
            }
            for (var i = 0; i < Math.min(2, fave_list.length); i++) {

                var elemColor = document.createElement("div");
                elemColor.classList.add("color_favorite");
                elemColor.innerHTML = fave_list[i].prod_color_description;

                var elemPrice = document.createElement("div");
                elemPrice.classList.add("price_favorite");
                elemPrice.innerHTML = "$ " + parseInt(fave_list[i].prod_price);

                var elemDeleteFavorite = document.createElement("div");
                elemDeleteFavorite.classList.add("delete_favorite");
                elemDeleteFavorite.id = "delete_favorite_" + i;
                elemDeleteFavorite.innerHTML = "X";
                elemDeleteFavorite.onclick = function (e) {
                    removeFavorite(fave_list[e.target.id.substring(e.target.id.length - 1)].prod_id);
                };

                var elemImage = document.createElement("img");
                elemImage.setAttribute("src", "https://s7d5.scene7.com/is/image/Glasses/productpage?src=is{Glasses/" + fave_list[i].prod_sku + "_shad_fr?$2GlassesProduct$}");

                var elemA = document.createElement("a");
                elemA.innerHTML = fave_list[i].prod_name;
                elemA.setAttribute("href", fave_list[i].prod_url);

                var elemDiv = document.createElement("div");
                elemDiv.appendChild(elemImage);
                elemDiv.appendChild(elemA);
                elemDiv.appendChild(elemDeleteFavorite);
                elemDiv.appendChild(elemPrice);
                elemDiv.appendChild(elemColor);

                //create li
                var elem = document.createElement("li");
                elem.appendChild(elemDiv);

                //append everything to the fly-out menu
                menuFavorites.appendChild(elem);
            }
            if (fave_list !== null && fave_list.length !== 0) {
                var elemA = document.createElement("a");
                elemA.innerHTML = "VIEW FAVORITES";
                elemA.setAttribute("href", "/favorites");

                var elem = document.createElement("li");
                elem.appendChild(elemA);

                menuFavorites.appendChild(elem);
            }
        } catch (e) {
            console.log(e);
        }
    }

    function updateAddFavorites(results, id) {
        var index = searchIndex(results, id);
        if (index != null) {
            fave_list.unshift({
                "prod_id": results[index].prod_id,
                "prod_sku": results[index].active_sku.upc,
                "prod_name": results[index].product_name,
                "prod_url": results[index].base_product_url[0],
                "prod_price": results[index].active_sku.price,
                "prod_color_description": results[index].active_sku.color_description,
            });
            showFavorites();
            menuFavorites.classList.add("showMenu");
            presentationFavorites.classList.add("open");
            if (timeout != null) {
                clearTimeout(timeout);
            }
            timeout = setTimeout(function () {
                menuFavorites.classList.remove("showMenu");
                presentationFavorites.classList.remove("open");
            }, 3000);
        }
    }

    function updateAddFavorite(product, selected_sku) {
        fave_list.unshift({
            "prod_id": product.product.prod_id,
            "prod_sku": selected_sku.upc,
            "prod_name": product.product.product_name,
            "prod_url": product.default_pdp_page_url,
            "prod_price": selected_sku.price,
            "prod_color_description": selected_sku.color_name,
        });
        showFavorites();
        menuFavorites.classList.add("showMenu");
        presentationFavorites.classList.add("open");
        if (timeout != null) {
            clearTimeout(timeout);
        }
        timeout = setTimeout(function () {
            menuFavorites.classList.remove("showMenu");
            presentationFavorites.classList.remove("open");
        }, 3000);
    }

    function updateRemoveFavorite(id) {
        var index = searchIndex(fave_list, id);
        if (index != null) {
            fave_list.splice(index, 1);
            showFavorites();
        }
    }

    function searchIndex(results, id) {
        for (var i = 0; i < results.length; i++) {
            if (results[i].prod_id == id) {
                return i;
            }
            ;
        }
        return null;
    }

    function searchIndexFavorites(results, id) {
        for (var i = 0; i < results.length; i++) {
            if (results[i].product.prod_id == id) {
                return i;
            }
            ;
        }
        return null;
    }

    function removeFavorite(prod_id) {
        try {
            if (angular.element(document.getElementById('cardstack')).scope() != undefined) {
                var scope = angular.element(document.getElementById('cardstack')).scope();
                var index = scope.favorite_items.indexOf(prod_id);
                scope.removeFavorite(prod_id, index);
                updateRemoveFavorite(prod_id);
            } else if (angular.element(document.getElementById('favorite-body')).scope() != undefined) {
                var scope = angular.element(document.getElementById('favorite-body')).scope();
                var index = searchIndexFavorites(scope.favorite_items, prod_id);
                scope.removeFavorite(prod_id, index);
            } else if (angular.element(document.getElementById('product-detail-ng-app')).scope() != undefined) {
                var scope = angular.element(document.getElementById('product-detail-ng-app')).scope();
                scope.favoriteAction("remove");
            } else {
                var data = "{\"prod_id\":\"" + prod_id + "\"}";
                return $.post('/favorites/remove-favorite', data,
                        function () {
                            //location.reload();
                            countFavorite(false, true);
                            updateRemoveFavorite(prod_id);
                        }, "json"
                        );
            }
        } catch (err) {
            var data = "{\"prod_id\":\"" + prod_id + "\"}";

            return $.post('/favorites/remove-favorite', data,
                    function () {
                        //location.reload();
                        countFavorite(false, true);
                        updateRemoveFavorite(prod_id);
                    }, "json"
                    );
        }
    }
    ;

    //cart
    var cart_list =[];
    var menuCart = document.getElementById('cart_menu');
    var presentationCart = document.getElementById('cart');

    function addCountCart(showDropdown) {

        var cont = document.getElementById('headerCartCount');
        cont.innerHTML = cart_list.length;

        if (showDropdown == undefined || showDropdown != false)
        {
            menuCart.classList.add("showMenu");
            showCart();
        }
    }

    function showCart() {
        while (menuCart.firstChild) {
            menuCart.removeChild(menuCart.firstChild);
        }
        var subtotal = 0;
        for (var i = 0; i < cart_list.length; i++) {
            subtotal += parseFloat(cart_list[i].prod_price);
        }
        for (var i = 0; i < Math.min(2, cart_list.length); i++) {
            var prod_info = cart_list[i];
            var elemColor = document.createElement("div");
            elemColor.classList.add("color_cart");
            elemColor.innerHTML = prod_info.prod_color_description;

            var elemPrice = document.createElement("div");
            elemPrice.classList.add("price_cart");
            elemPrice.innerHTML = "$ " + parseInt(prod_info.prod_price);


            var elemImage = document.createElement("img");
//            elemImage.setAttribute("src","https://s7d5.scene7.com/is/image/Glasses/productpage?src=is{Glasses/"+prod_info.prod_sku+"_shad_fr?$2GlassesProduct$}");
            elemImage.setAttribute("src", prod_info.prod_image_url);

            var elemA = document.createElement("a");
            elemA.innerHTML = prod_info.prod_name;
            elemA.setAttribute("href", prod_info.prod_url);

            var elemDiv = document.createElement("div");
            elemDiv.appendChild(elemImage);
            elemDiv.appendChild(elemA);
            elemDiv.appendChild(elemPrice);
            elemDiv.appendChild(elemColor);

            //create li
            var elem = document.createElement("li");
            elem.appendChild(elemDiv);

            //append everything to the fly-out menu
            menuCart.appendChild(elem);
        }
        if (cart_list !== null && cart_list.length !== 0) {

            var elemSubtotalDiv = document.createElement("div");
            elemSubtotalDiv.classList.add('subtotal_div_cart');

            var elemSubtotalLabel = document.createElement("div");
            elemSubtotalLabel.classList.add('subtotal_div_label_cart');
            elemSubtotalLabel.innerHTML = "SUBTOTAL";
            elemSubtotalDiv.appendChild(elemSubtotalLabel);

            var elemSubtotalValue = document.createElement("div");
            elemSubtotalValue.classList.add('subtotal_div_value_cart');
            elemSubtotalValue.innerHTML = "$ " + parseInt(subtotal);
            elemSubtotalDiv.appendChild(elemSubtotalValue);

            var elemLiCart = document.createElement("li");
            elemLiCart.appendChild(elemSubtotalDiv);

            menuCart.appendChild(elemLiCart);

            var elemA = document.createElement("a");
            elemA.innerHTML = "VIEW ALL CART";
            elemA.setAttribute("href", "/cart");

            var elem = document.createElement("li");
            elem.appendChild(elemA);

            menuCart.appendChild(elem);
        }
    }

    function updateAddCart(data, showDropdown) {
        cart_list = data;
        addCountCart(showDropdown);

        if (showDropdown == undefined || showDropdown == true)
        {
            showCart();
            menuCart.classList.add("showMenu");
            presentationCart.classList.add("open");
            if (timeout != null) {
                clearTimeout(timeout);
            }
            timeout = setTimeout(function () {
                menuCart.classList.remove("showMenu");
                presentationCart.classList.remove("open");
            }, 3000);
        }
    }

    $(document).ready(function () {
        countFavorite(false, false);
        showFavorites();
        showCart();
    })

</script>

