<script type="text/javascript">
$(function(){
    var imgNum = 2;     //画像の枚数
    var imgSize = 650;  //画像のサイズ
    var time = 6000;    //切り替えのタイミング
    var current = 1;    //現在の画像
    setInterval(function(){
        if( current < imgNum ) {
            current++;
            $("#b_banner ul").animate({
                marginLeft : parseInt($("#b_banner ul").css("margin-left"))-imgSize+"px"
            },"fast");
        //最後の画像にきたら最初に戻る
        } else {
             $("#b_banner ul").animate({
                marginLeft : parseInt($("#b_banner ul").css("margin-left"))+(imgSize * (imgNum-1))+"px"
            },"fast");
            current = 1;
        }
    }, time);
});

$(function () {
  $(window).scroll(function(){
  var scrollTop = $(this).scrollTop();
	  if (scrollTop > 800 ) {
		  $('.bbs_bt').insertAfter('.bbs_content');
	  }else if (scrollTop < 800 ) {
		  $('.bbs_bt').insertBefore('.bbs_content');
	  }	
  });
  $('.bbs_bt li').click(function() {
	var index = $('.bbs_bt li').index(this);
	$('.bbs_content dd').css('display','none');
	$('.bbs_content dd').eq(index).css('display','block');
	$('.bbs_bt li').removeClass('select');
	$(this).addClass('select');
	$('html,body').animate({scrollTop:540}, 500, 'swing');
  });
});
</script>



