	         if(url){
	            var hasQueryString = url.indexOf("?") > -1;
	           
		    if(hasQueryString){
		       url += "&" + queryStrings;
		    }
		    else{
		       url += "?" + queryStrings;
		    }
		    buttons[i].href = url;
