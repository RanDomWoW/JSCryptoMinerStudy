        if ($(".divRegistro form").valid() == false || $.Rut.validar($("#UserName").val())==false) {
            $("#AlertaRegistro").html("<strong>Por favor completa o rectifica los datos solicitados en rojo.</strong>");
            $("#AlertaRegistro").show();
            $("#AlertaRut").hide();
            $("#errorEmailRegistro").hide();
            $("#errorClaveRegistro").hide();
            $("#registro_cancelar").removeAttr("disabled");
            return false;
        } else {
            $("#AlertaRegistro").hide();
        }

        try {
            if ($("#TerminosCondiciones").prop("checked") == false) {
                acepta = false;
            }
        } catch (err) {
            acepta = true;
        }

        if (
            $("#Nombre").val() != ""
                && $("#Apellido").val() != ""
                && $("#UserName").val() != ""
                && $("#EMail").val() != ""
                && $("#Clave").val() != ""
                && $("#ReingreseClave").val() != ""
                && $("#Captcha").val() != ""
                && acepta == false) {
            $("#registro_cancelar").removeAttr("disabled");
            $("#AlertaRegistro").html("<strong>Para registarse, debes aceptar los términos y condiciones.</strong>");
            $("#AlertaRegistro").show();
            return false;
        }
        try {
            if (acepta == false) {
                $("#registro_cancelar").removeAttr("disabled");
            }
        } catch (Exce) { }
        return true;
    }
</script>

