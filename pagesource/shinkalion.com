<script>
$(function(){
//―――――――――――――――――――
// tickerローテーション
//―――――――――――――――――――
	var _tickerLength = $('#ticker span').length;
	$('#ticker').append($('#ticker span:first-child').clone());
	var _nowTick = 0;
	
	//イベントGA設置:href置き換え＆onclick追加
	setTimeout(function(){
		$('#ticker span a').each(function(_e){
			var _url = $(this).attr('href');
			var _txt = $(this).text();
			$(this).attr('href','javascript:void(0)\;').attr({
				'data-url':_url,
				'onClick':'_sendTickerEventGA("'+_url+'","'+_txt+'")'
			});
		});
	},300);
	
	setInterval(function(){
		_nowTick += 1;
		if(_nowTick == _tickerLength){ setTimeout(function(){ $('#ticker').css({'transition':'none','left':'0'}); },1500); }
		if(_nowTick > _tickerLength){ _nowTick = 0; }
		$('#ticker').css({'transition':'','left':-100*_nowTick+'%'});
	},6000);
});

//―――――――――――――――――
// GA送信＆遷移
//―――――――――――――――――
function _sendTickerEventGA(_url,_txt){
	try{
		gtag('event',_txt,{
			'event_category': 'ticker_click',
			'event_label': 'ticker_'+_url,
			'value': 1
		});
		setTimeout(function(){ window.location.href = _url; },100)
	}catch(e){
		console.log(e);
	}
};
</script>

