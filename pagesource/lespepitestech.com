  		$(".tinder-button-center").append(votedanime);
  		$(".tinder-button-center").children().hide();
  		$(".tinder-button-center").children().addClass("fa fa-times fa-5x");
  		$(".tinder-button-center").children().fadeIn("fast");
  	  $("#edit-submit-tinder-des-startups-french-tech").trigger('click');
}
function tinderVoteNo(){
   console.log('nope sorry');
   $('#ModalloginvotetinderLink').click();
}
function tinderPerm(){
  
  if($("body.logged-in").length){
      return true;
