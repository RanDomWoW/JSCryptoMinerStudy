(function($){
         
  

//tinder animation

  $(".page-tinder-des-startups-french-tech").on('click', '.tinder-next', function(){
      if (tinderPerm()){  tinderVoteUp(); }
      else {  tinderVoteNo(); }
    }); 
    
  $(".page-tinder-des-startups-french-tech").on('click', '.tinder-prev', function(){
      if (tinderPerm()){  tinderVoteDown(); }
      else {  tinderVoteNo(); }      
    });         

  $(".page-tinder-des-startups-french-tech").keydown(function(e) {
        switch(e.which) {
            case 37: // left
              if (tinderPerm()){  tinderVoteDown(); }
              else {  tinderVoteNo(); }   
            break;
    
            case 39: // right
              if (tinderPerm()){  tinderVoteUp(); }
              else {  tinderVoteNo(); }   
            break;
    
            default: return; // exit this handler for other keys
        }
        e.preventDefault(); // prevent the default action (scroll / move caret)
    });
    
  $(".thumbnail").on('swipeleft', function(e) { 
      if (tinderPerm()){  tinderVoteDown(); }
      else {  tinderVoteNo(); }     
  });
    
  $(".thumbnail").on('swiperight', function(e) {
      if (tinderPerm()){  tinderVoteUp(); }
      else {  tinderVoteNo(); }
  });
    
  $(".thumbnail").on('movestart', function(e) {
      // If the movestart is heading off in an upwards or downwards
      // direction, prevent it so that the browser scrolls normally.
      if ((e.distX > e.distY && e.distX < -e.distY) ||
        (e.distX < e.distY && e.distX > -e.distY)) {
        e.preventDefault();
      }
    });


var tinderVoteCount = 0;
function tinderVoteUp(){
      tinderVoteCount++; 
      console.log('tinderVoteCount = '+tinderVoteCount);
      // alert = function(){};
      $(".tinder-pepite-vote").find(".up-inactive").trigger('click');
      $(".thumbnail").addClass('rotate-left').delay(500).fadeOut(1);
  		// var tinderinfosmall = $(".tinder-info-small");
  		var votedanime = $("<i>");
			$(".tinder-button-center").children().fadeOut("fast");
  		$(".tinder-button-center").empty();	
  		$(".tinder-button-center").append(votedanime);
  		$(".tinder-button-center").children().hide();
  		$(".tinder-button-center").children().addClass("fa fa-heart fa-5x");
  		$(".tinder-button-center").children().fadeIn("fast");
      $("#edit-submit-tinder-des-startups-french-tech").trigger('click');  		
}
function tinderVoteDown(){
      tinderVoteCount++; 
      console.log('tinderVoteCount = '+tinderVoteCount);  
  	 // alert = function(){};
  	  $(".flag-ne-plus-voir").find(".flag-action").trigger('click');
