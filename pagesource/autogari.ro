<script>

	$(function() {
		$( "#plecare" ).datepicker({  altField:'rplecare', altFormat:'yyyy-mm-dd', dateFormat:'DD, d M yy', minDate:0, showAnim: 'fold', numberOfMonths: 1});
		$( "#plecare" ).datepicker( "setDate" , -1 );
		

		$("#pasageri input").change(function() { if(isNaN(this.value)) this.value=0; this.style.color = (this.value>0?'#000':'#CCC'); });
	    $("#pasageri div").append('<div><button class="inc">+</button><button class="dec">-</button></div>');

		$( "#pasageri button.inc" )
			.button( {text: false,icons: {primary: "ui-icon-plusthick"}})
			.removeClass("ui-corner-all" ).addClass("ui-corner-tr")
			.click(function() {
				vi = $(this).parent().parent().find("input");
				if(vi.val()<23) vi.val(Number(vi.val())+1).change(); else vi.val(0).change();
			})
			.next()
				.button( {text: false,icons: {primary: "ui-icon-minusthick"}})
				.removeClass("ui-corner-all" ).addClass("ui-corner-br").css('border-top','none')
				.click(function() {
					vi = $(this).parent().parent().find("input");
					if(vi.val()>0) vi.val(vi.val()-1).change();	else vi.val(0).change();
				}); 
		
		if($.cookie('psg'))
		{
			var psg = $.cookie('psg').split('-');
			$('#adulti').val(psg[0]).change();
			$('#pensionari').val(psg[1]).change();
			$('#studenti').val(psg[2]).change();
			$('#elevi').val(psg[3]).change();
			$('#copii').val(psg[4]).change();
		}
		else
		{
			$('#pasageri input').change();
		}
		
	});
	
	function checkData1()
	{
		var ret = true;
		stopTutorial(0);
		if($('#loc_plecare').val()=='' || $('#loc_plecare').val()==$('#loc_plecare').attr('default')){
			tutorialTimer.push( setTimeout("showBallon($('#loc_plecare'), 'Localitatea de plecare este obligatorie.', true);", getTutorialTime()) );
			ret = false;
		}
		else if($('#loc_plecare_id').val()=='') {
			tutorialTimer.push( setTimeout("showBallon($('#loc_plecare'), 'Selectarea unei localiati din lista afisata este obligatorie.', true);", getTutorialTime()) );
			$('#loc_plecare').val("");
			ret = false;
		}
		
		if($('#loc_sosire').val()=='' || $('#loc_sosire').val()==$('#loc_sosire').attr('default')){
			tutorialTimer.push( setTimeout("showBallon($('#loc_sosire'), 'Localitatea de destinatie este obligatorie.', true);", getTutorialTime()) );
			ret = false;
		}
		else if($('#loc_sosire_id').val()=='') {
			tutorialTimer.push( setTimeout("showBallon($('#loc_sosire'), 'Selectarea unei localitati din lista afisata este obligatorie.', true);", getTutorialTime()) );
			$('#loc_sosire').val("");
			ret = false;
		}		

		if(Number($("#adulti").val())+Number($("#copii").val())+Number($("#elevi").val())+Number($("#studenti").val())+Number($("#pensionari").val())==0){
			tutorialTimer.push( setTimeout("showBallon($('#adulti'), 'Selectati cel putin un tip de calator.', true);", getTutorialTime()) );
			ret = false;
		}
		
		if(!ret)
			tutorialTimer.push( setTimeout("hideBallon();", getTutorialTime()) );
		
		return ret;
	}		
</script>

