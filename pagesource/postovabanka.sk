                    <script>
                        $(function () {
                            var isInGetRates = false;

                            function GetChangeCss(change) {
                                return (change < 0) ? "arr-r-small-down" : (change > 0) ? "arr-g-small-up" : "dash-gray middle";
                            }

                            function GetRates(date) {
                                if (isInGetRates) return;
                                isInGetRates = true;

                                $.ajax({
                                    url: "https://www.postovabanka.sk/svc/banka/GetRates",
                                    type: "GET",
                                    data: {date: date, isCz: false},
                                    success: function (rates) {
                                        for (var i = 0; i < rates.length; i++) {
                                            var rate = rates[i];

                                            var buyCss = GetChangeCss(rate.BankNoteBuyChange);
                                            var sellCss = GetChangeCss(rate.BankNoteSellChange);
                                            
                                            $('#BankNoteBuy_' + rate.Currency).html((rate.BankNoteBuy - 0).toFixed(3));
                                            $('#BankNoteBuyChange_' + rate.Currency).attr("class", buyCss);

                                            $('#BankNoteSell_' + rate.Currency).html((rate.BankNoteSell - 0).toFixed(3));
                                            $('#BankNoteSellChange_' + rate.Currency).attr("class", sellCss);
                                        }
                                    },
                                    error: function (xhr) {
                                        var error = "GetRates Error (" + xhr.status + "): " + xhr.responseText;
                                    },
                                    complete: function () {
                                        isInGetRates = false;
                                    }
                                });
                            }

                            GetRates('20.03.2018');
                        })
</script>   
