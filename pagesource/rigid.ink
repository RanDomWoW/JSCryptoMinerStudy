  function cart_load(){
      var url = "/cart.js";
      $(".items-wrapper").empty();
      $.getJSON(url, function (results) {
          var shop_money_format = "£{{amount}} GBP";
          //var total_price = results.total_price;
          var len = results.items.length;
          var total_price = 0;
          $.each(results.items, function (i, item) {
              var product_title = item.product_title;
              var product_variant = item.title.replace(product_title,"");
              var product_image = item.image;
              product_image = product_image.replace(/(\.jpg|\.png|\.jpeg|\.gif)/, '_small$1');
              var product_quantity = item.quantity;
              var product_price = item.price;
              var frequency_num = '';
              if(item.properties){
                  if(item.properties.discounted_price){
                      product_price = item.properties.discounted_price;
                      product_price = product_price.match(/\d/g);
                      product_price = product_price.join("");
                  }
                  if(item.properties.frequency_num){
                      frequency_num = '(Every '+ item.properties.frequency_num +' Days)'
                  }
              }
              product_price = parseInt(product_price);
              if ( product_title != 'Gift Card' ){
                  var tax = "0.2";
                  product_price = (product_price * tax) + product_price;
              }
              total_price = total_price + product_price * product_quantity;
              product_price = theme.Currency.formatMoney(product_price, shop_money_format);
              var new_cart = '<div class="item"><div class="image"><img src="'+product_image+'" alt="'+product_title+'"></div><div class="item-info"><div class="title">'+item.title+'</div><div class="quantity">'+product_quantity+' x '+product_price+' <span class="frequency">'+ frequency_num +'</span></div></div></div><div class="clearfix"></div>';
              $(new_cart).appendTo(".items-wrapper");
              if (i == len - 1) {
                  $('html, body').animate({scrollTop : 0},800);
                  $("#minicart").removeClass("hidden");
                  setTimeout(function(){ $("#minicart").addClass("hidden"); }, 3000);
              }
          });
          total_price = theme.Currency.formatMoney(total_price, shop_money_format);
          //$("#cart-total span.money, #cart-total-mobile span.money").html(total_price);
          //$("#cart-total-small span.money").html(total_price);
          $("#items-count").html('<span>'+results.item_count+'</span>');
          $(".cart-link .items-count").html('('+results.item_count+')');
          if ($(window).width() <= 768) {
              setTimeout(function () {$("#shopify-section-header").find(".cart_open").slideDown(); }, 1000);
          }
          $(".items-count").addClass('show-cart');
//       $(".cart-count-mobile #items-count").html(results.item_count);
//       $(".cart-count-desktop #items-count").html(results.item_count);
      });
