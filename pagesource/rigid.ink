  function currencyFlags() {
      var currentCurrency = $("#currencies option:selected").val();
      //console.log('11',currentCurrency);
      if (currentCurrency == "GBP") {
          $('#currencies').removeClass('flag-usd').removeClass('flag-cad').removeClass('flag-eur').addClass('flag-gbp');
      } else if (currentCurrency == "USD") {
          $('#currencies').removeClass('flag-cad').removeClass('flag-gbp').removeClass('flag-eur').addClass('flag-usd');
      } else if (currentCurrency == "CAD") {
          $('#currencies').removeClass('flag-usd').removeClass('flag-gbp').removeClass('flag-eur').addClass('flag-cad');
      } else if (currentCurrency == "EUR") {
          $('#currencies').removeClass('flag-usd').removeClass('flag-gbp').removeClass('flag-cad').addClass('flag-eur');
      }
  }

  function changeCurrencyFlag() {
      $('#currencies').change(function() {
          currencyFlags();
      });
  }
