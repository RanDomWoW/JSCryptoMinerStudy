		$( "#provincia" ).change( function () {
			src = $( '#provincia' ).val();
			myAutocomplete( "p_" + src, src_s );
		} );
		$( "#settore" ).change( function () {
			src_s = $( '#settore' ).val();
			myAutocomplete( src, src_s );
		} );
		myAutocomplete( src, src_s );

	} );


