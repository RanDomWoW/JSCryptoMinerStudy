                //$('body').css({'position':'static'});
                $('html').removeClass("menu-burger__html-overlay");
            }
            //Hide mobile menu
            function showMobileEnterMenu() {
                //$('.header__mobile-menu .enter-page .menu').show( "slide", { direction: "right" }, 150 );
                $('.enter-page').fadeIn("fast");
                $('.header__enter-btn').addClass("menu-on");
                $('.enter-page').addClass("active active_visible");
                $('.header__mobile-menu .menu').addClass("active");
                //$('body').css({'position':'fixed'});
                $('html').addClass("menu-burger__html-overlay");
            }
            enterMenu.click(function (e) {

                //disable all events
                e.stopPropagation();
                // when clickd if visible then open
                if($('.header__mobile-menu .enter-page').is(":visible")){
                    hideMobileEnterMenu();
                } else {
                    // when clickd if not visible then hide
                    showMobileEnterMenu();
                }
            });

            $('.enter-page__close').click(function (e) {
                
                if (!$(e.target).closest('.header__mobile-menu .menu').length && !$(e.target).is('.header__mobile-menu .menu')) {
                    hideMobileEnterMenu();
                }
            });

            $(".header__search-ico").click(function (e) {

                //disable all events
                e.stopPropagation();
                // when clickd if visible then open
                if($('.header__mobile-menu .header__search-page').is(":visible")){
                    hideMobileSearch();
                } else {
                    // when clickd if not visible then hide
                    showMobileSearch();
                }
            });
            //Show mobile menu
            function hideMobileSearch() {
                $('.header__search-page-wrap').fadeOut("fast");
                $('.header__enter-btn').removeClass("menu-on");
                $('.header__search-page-wrap').removeClass("active active_visible");
                $('.header__search-page').removeClass("active");
                $('html').removeClass("menu-burger__html-overlay");
            }
            //Hide mobile menu
            function showMobileSearch() {
                $('.header__search-page-wrap').fadeIn("fast");
                $('.header__enter-btn').addClass("menu-on");
                $('.header__search-page-wrap').addClass("active active_visible");
                $('.header__search-page').addClass("active");
                $('.search-form-mini-mob #srchFrm-mini').focus();
                $('html').addClass("menu-burger__html-overlay");
