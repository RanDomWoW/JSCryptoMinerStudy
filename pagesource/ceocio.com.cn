<script>
$(function(){
    var index = 0;
    var timer = null;
    var slideBox = $(".Rightbar .Slide .slideBox");
    var ulheight = $(".Rightbar .Slide .slideBox ul").height();
    $(".Rightbar .Slide dt a").eq(0).addClass("tabActive");
    $(slideBox).css("top",-index*ulheight);
    var aGroup = $(".Rightbar .Slide dt a");
    //初始启动
      go();
    //鼠标进入tab导航 滚动暂停
    $(".Rightbar .Slide dt a").mouseover(function(){
          stop(this);
    });
     $(".Rightbar .Slide .slideBox ul").mouseover(function(){
          stop(this);
    });
    //鼠标离开tab导航 滚动开始
    $(".Rightbar .Slide dt a").mouseout(function(){
          go();
    });
     $(".Rightbar .Slide .slideBox ul").mouseout(function(){
           go();
    });
   //启动轮播图
   function go(){
       timer = setInterval(function(){
            index++;
            if(index == aGroup.length){
            index = 0;
            $(aGroup).eq(index).addClass("tabActive").siblings().removeClass("tabActive");
            $(slideBox).css("top",-index*ulheight);
            }else{
            $(aGroup).eq(index).addClass("tabActive").siblings().removeClass("tabActive");
            $(slideBox).css("top",-index*ulheight);
            }
         },1500);
      }
   //暂停轮播图
   function stop(dom){
         clearInterval(timer);
         index = $(dom).index();
         $(aGroup).eq(index).addClass("tabActive").siblings().removeClass("tabActive");
         $(slideBox).css("top",-index*ulheight);
   }
});
</script>
