$(function() {
	var weeks;
	$.getJSON('/skin/default/data/week.json?1447933637', function(data) {
		weeks = data;
	});
	var holidays;
	$.getJSON('/skin/default/data/holidays.json?1447933637', function(data) {
		holidays = data;
	});
	var weekt = ['일', '월', '화', '수', '목', '금', '토'];
	var weekb = [1, 2, 4, 8, 16, 32, 64];
	
	try {
		var interval = Date.parse('Sat, 24 Mar 2018 13:28:58 +0900') * 1 - Date.now();
		var datefail = false;
	} catch(e) {
		var interval = new Date('Sat, 24 Mar 2018 13:28:58 +0900') * 1 - new Date() * 1;
		var datefail = true;
	}

	function setSchedule(obj, ffname) {
		try {
			var week = weeks[ffname], holiday = holidays[ffname];
		} catch(e) {
			return;
		}
		if(datefail) {
			var now = new Date(new Date() * 1 + interval + (week.calibrate - 32400) * 1000);
		} else {
			var now = new Date(Date.now() + interval + (week.calibrate - 32400) * 1000);
		}
		var mm = (now.getMonth() + 1).strpad(2);
		var dd = now.getDate().strpad(2);
		var date = now.getFullYear() + '년 ' + mm + '월' + dd + '일 ' + weekt[now.getDay()] + ' ';
		var hh = now.getHours();
		var time = hh.strpad(2) + ':' + now.getMinutes().strpad(2);
		var apm = 'AM ';
		if(hh > 12) {
			hh -= 12;
			apm = 'PM ';
		}
		var timet = apm + hh.strpad(2) + ':' + now.getMinutes().strpad(2) + ':' + now.getSeconds().strpad(2);
		var mmdd = mm + '-' + dd;
		
		var text = week.end_text;
		var work = '<i class="fa fa-moon-o"></i> ';
		
		if(holiday && holiday[mmdd]) {
			text = holiday[mmdd].type;
		} else {
			var day = now.getDay();
			if(week.off & weekb[day]) {
				text = week.off_text;
			} else {
				if(week.workstart_time[day] && week.workend_time[day]) {
					if(week.workstart_time[day] <= time && week.workend_time[day] >= time) {
						text = week.work_text;
						work = '<i class="fa fa-sun-o"></i> ';
					}
				}
			}
		}
		
		text = work + text;
		
		$('span.today_date:first', obj).html(date);
		$('span.today_time:first', obj).html(timet); // + ':' + now.getSeconds().strpad(2)
		$('span.p_status:first', obj).each(function() {
			if(this.innerHTML != text) {
				this.innerHTML = text;
			}
		});
	}
	
	$('#scheduleBox_kr').everyTime(1000, function() {
		setSchedule(this, 'seoul');
	});	
	$('#scheduleBox_jp').everyTime(1000, function() {
		setSchedule(this, 'osaka');
	});
	$('#scheduleBox_ca').everyTime(1000, function() {
		setSchedule(this, 'california');
	});	
	$('#scheduleBox_nj').everyTime(1000, function() {
		setSchedule(this, 'newjersey');
	});
	$('#scheduleBox_or').everyTime(1000, function() {
		setSchedule(this, 'oregon');
	});				
	$('#scheduleBox_ch').everyTime(1000, function() {
		setSchedule(this, 'shanghai');
	});

});
