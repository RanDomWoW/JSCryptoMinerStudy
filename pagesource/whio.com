<script>
<!-- GIVE WINDOW ACCESS TO SWIPER FUNCTIONS -->
	var swipersGlobal = swipersGlobal || {};
	$(document)
			.ready(
					function() {
						(function() {
							var changeActive;
							var activeZeroIndex = 0;
							var galleryTop;
							var numPriorSlides = 0;
							var clickedLeft;
							var thisSwiperId = "#swiper_934326";
							var numSlides = $(thisSwiperId)
									.find('.rotator-top').find(
											$('.swiper-slide')).length;
							
							var numThumbsShowing = 4;
							if ($( window ).width() <= 992) {
								numThumbsShowing = 3;
								if ($( window ).width() <= 768) {
									numThumbsShowing = 2;
								}
							}

							changeActive = function() {
								$(thisSwiperId+ ' .rotator-thumbs .swiper-slide').siblings().find('img').removeClass('active');

								$(thisSwiperId + ' .rotator-thumbs .swiper-slide img').each(function(index){
									if (index == activeZeroIndex) {
										$(this).addClass('active');
									}
								});
							}
							
							galleryTop = new Swiper(
									thisSwiperId + " .rotator-top",
									{
										loop : false,
										autoplay : 5000,
										autoplayStopOnLast: false,
										autoplayDisableOnInteraction: false,
										spaceBetween : 10,
										speed : 600,
										slideToClickedSlide : true,
										autoHeight : false,
										loopAdditionalSlides : 0,
										nextButton : thisSwiperId
												+ " .rotator-top .next",
										prevButton : thisSwiperId
												+ " .rotator-top .prev",
										onSlideChangeStart : function(swiper,event) {
											var ourLastIndex = activeZeroIndex;
											activeZeroIndex = swiper.activeIndex;

											if (galleryThumbs) {
												thumbIndex = galleryThumbs.activeIndex;
												console.log('num thumbs', numThumbsShowing);

												// object holding currently visible thumbs
												var currentThumbs = {};
												for (var i = 0; i < numThumbsShowing; i++) {
													currentThumbs[(thumbIndex + i)] = true;
												}

												// if the activeSlide isn't in thumbs we need to move thumbs
												if (!currentThumbs[activeZeroIndex]) {
													galleryThumbs.slideTo((activeZeroIndex),600, true);
												}
												changeActive();
											}
											
											$(thisSwiperId).find('.swiper-slide').each(function(index,element){
												if (index == ourLastIndex){
													var lastVideo = $(element).find($('iframe'));
													if (lastVideo){
														var lastVideoId = $(lastVideo).attr('id');
														if(anvp[lastVideoId]){
															anvp[lastVideoId].getState(function(e){
																if (e.indexOf('playing') > -1) {
																	console.log('the rotator is pausing',lastVideoId);
																	anvp[lastVideoId].pause(function(){
																		console.log('the rotator is removing the header of',lastVideoId);
																		$('#'+lastVideoId).closest('.swiper-slide').find('.content-holder').slideUp();
																	})
																}
															});
														}
													}
												} else {
													var lastVideo = $(element).find($('iframe'));
													if (lastVideo){
														var lastVideoId = $(lastVideo).attr('id');
														if(anvp[lastVideoId]){
															anvp[lastVideoId].pause();
														}
													}
												}
											})
										},
									});
							
							var hoverIn = function(){
								galleryTop.stopAutoplay();
								/* $('body').css('background-color', 'red'); */
							};
							
							var hoverOut = function(){
								galleryTop.startAutoplay();
								/* $('body').css('background-color', 'green'); */
							}
							
							$(thisSwiperId).on({
								mouseenter: hoverIn,
								mouseleave: hoverOut
							})
							
 							$('.swiper-container .rotator-top').mouseup(function(){
									if($( window ).width()<= 768){
										galleryTop.startAutoplay();
									}
							})
							
/* 							$('.rotator-thumbs').mouseup(function(){
									if($( window ).width()<= 825){
										// stop and start autoplay when clicking on mobile thumbs
										galleryTop.stopAutoplay();
										galleryTop.startAutoplay();
									}
							}) */

							var thumbsByBreakpoint = {
									768: 2,
									992: 3,
									max: 4
							}
							
							console.log(thisSwiperId + " .rotator-thumbs")


							var galleryThumbs = new Swiper(thisSwiperId
									+ " .rotator-thumbs", {
								spaceBetween : 10,
								loop : false,
							    slidesPerView: 4,
							    slidesPerGroup : 4,
								touchRatio : 0.2,
								slideToClickedSlide : false,
								loopAdditionalSlides : 0,
							    breakpoints: {
							    	769: {
							    	      slidesPerView: 2,
							    	      slidesPerGroup: 2,
							    	      spaceBetweenSlides: 20
							    	    },
						    	    993: {
										slidesPerView: 3,
										slidesPerGroup: 3,
										spaceBetweenSlides: 20
						    	      }
							    },
								nextButton : thisSwiperId
										+ " .rotator-thumbs .next",
								prevButton : thisSwiperId
										+ " .rotator-thumbs .prev",
								autoplayDisableOnInteraction: false,
							});


							// set numPriorSlides
							var foundZero = false
							$('.rotator-thumbs .swiper-slide').each(
									function(a, b) {
										var dataIndex = $(b).attr(
												'data-swiper-slide-index');
										if (!foundZero && dataIndex != 0) {
											numPriorSlides++;
										} else {
											foundZero = true;
										}
									});

							changeActive();
							

							if (numSlides <= numThumbsShowing) {
								galleryThumbs.lockSwipes();
								$(thisSwiperId).find('.rotator-thumbs a').remove();
							}
							
							while (numSlides < numThumbsShowing) {
								console.log('adding a blank slide')
								galleryThumbs.appendSlide('<div class="swiper-slide"></div>');
								numThumbsShowing--;
							}
							
							$(thisSwiperId + ' .rotator-thumbs .swiper-slide')
								.each(function(index, element){
									$(element).on('click',function(){
										activeZeroIndex = index-1;
										galleryTop.slideTo(activeZeroIndex + 1,600, true);
									})
								})
/* 							.on('click',function() {
										activeZeroIndex = parseInt($(this).attr('data-swiper-slide-index'));

										// remove active classes and activate clicked one
										$(this).siblings().find('img')
												.removeClass('active');
										$(this).find("img").addClass(
												'active');
										galleryTop.slideTo(
												activeZeroIndex + 1,
												600, true)
									}); */

							swipersGlobal[thisSwiperId] = galleryTop;
						})
						();
						

					})
