		<script>		
	membercenter.loadSDK({
		janrainSettings: {
		    social: {
		      providers: [
		        'facebook',
		        'twitter',
		        'native-googleplus',
		        'email-googleplus',
		        'email-mailto'
		      ],
		      orientation: 'horizontal',
		      formFactor: 'bar',
		      shareCountMode: 'none'
		    }
		}
	}).then(function(sdk) {
		membercenter.sdk.events.on('user.logout').subscribe(function(x) {		
			/* reset the login text*/
			document.querySelector('.member-center-login-name').innerHTML = 'Log in';
			
			/* we check to see if there's a user avatar and remove it if so*/
			let userAvatar = document.querySelector('.member-center-login-avatar img');
			if(document.body.contains(userAvatar)){
				document.querySelector('.member-center-login-avatar img').remove();
			}
			
			/* show the login avatar again */
			$('.member-center-login-avatar svg').attr('class', '');
			
			/* remove the logged in ul dropdown */
			$('#member-center-menu .dropdown-menu .content').removeClass('hide');
			$('#member-center-menu .dropdown-menu').removeClass('logged-in');
			document.querySelector('#member-center-menu .dropdown-menu > ul').remove();
			document.querySelector('#member-center-menu .dropdown-menu > a').remove();
		})

		function updateMenu(authN, profile, links){
			if(authN){
				console.log('boop three profile ' + profile.displayName);
				console.log('boop four link ' + links[0].link);
				console.log('boop four avatar: ', profile.avatar);
				
				if(profile.avatar == ''){
		
				} else {
					/* header */
					$('.member-center-login-avatar svg').attr('class', 'hide');
					let userImageHeader = document.createElement('img');
					userImageHeader.src = profile.avatar;
					document.querySelector('.member-center-login-avatar-header').innerHTML = '';
					document.querySelector('.member-center-login-avatar-header').appendChild(userImageHeader);
					
					/* mobile */
					let userImageMobile = document.createElement('img');
					userImageMobile.src = profile.avatar;
					document.querySelector('.member-center-login-avatar-mobile').innerHTML = '';
					document.querySelector('.member-center-login-avatar-mobile').appendChild(userImageMobile);
					
					/* desktop */
					let userImage = document.createElement('img');
					userImage.src = profile.avatar;
					
					document.querySelector('.member-center-login-avatar').appendChild(userImage);
				}
				
				/* header */
				document.querySelector('#member-center-menu-header a').href = links[0].link;
				
				/* mobile */
				document.querySelector('.member-center-login-name-mobile').innerHTML = profile.displayName;
				document.querySelector('#member-center-menu-mobile a').href = links[0].link;
				
				/* desktop */
				document.querySelector('.member-center-login-name').innerHTML = profile.displayName;
				let list = document.createElement('ul');
				$('#member-center-menu .dropdown-menu .content').addClass('hide');
				document.querySelector('#member-center-menu .dropdown-menu').appendChild(list);
				
				links.forEach(function(item) {
					let listItem = document.createElement('li');
					let elem = document.createElement('a');
					elem.href = item.link;
					
					let svg = document.createElement('img');
					svg.className = 'member-center-icon icon-' + item.icon;
					svg.src = '//www.whio.com/assets/images/membercenter/' + item.icon + '.svg';
					
					let text = document.createElement('div');
					text.className = 'icon-text';
					text.innerText = item.text;
					
					listItem.appendChild(elem);
					elem.appendChild(svg);
					elem.appendChild(text);

					document.querySelector('#member-center-menu .dropdown-menu > ul').appendChild(listItem);
				});
				
				let logoutLinkElem = document.createElement('a');
				logoutLinkElem.href = 'javascript:membercenter.sdk.redirectToLogout()';
				let logoutButton = document.createElement('div');
				logoutButton.className = 'btn-logout';
				logoutButton.innerText = 'Log Out';
				logoutLinkElem.appendChild(logoutButton);
				
				document.querySelector('#member-center-menu .dropdown-menu').appendChild(logoutLinkElem);
				$('#member-center-menu .dropdown-menu').addClass('logged-in');
			}
			$('#member-center-menu').removeClass('hide');
			$('#member-center-menu-header').removeClass('hide');
			$('#member-center-menu-header').addClass('logged-in');
			$('#member-center-menu-mobile').removeClass('hide');
		};

		Promise.all([
			sdk.isAuthenticated(),
			sdk.getUserProfile(),
			sdk.getNavLinks(),
		]).then(function(x){
			var authN = x[0];
			var profile = x[1];
			var links = x[2];
			updateMenu(authN, profile, links);
      	});
	});
