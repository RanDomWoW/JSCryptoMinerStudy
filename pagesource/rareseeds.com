<script type="text/javascript">
    // Used for making sure scroll event doesn't keep hitting when we are at the footer
    var isLoadingTiles = false;
    // All groups sorted
    var Groups = JSON.parse('[99,12,108,73,114,101,124,44,36,85,53,63,49,77,8,83,69,28,55,61,40,59,119,129]')
    // loading div
    var $tilesLoading = $('.tilesLoading');
    // # of groups to load per page
    var paginationCount = parseInt('9');
    var pageCount = 0;

    // load initial groups
    $(function () {
        isLoadingTiles = true;
        $tilesLoading.show();
        loadTiles(onComplete);
    });

    // send next {x} groups to load from server
    function loadTiles(completeCallback) {
        var postData = {
            F: 'LoadTiles',
            TileGroups: Groups.splice(0, paginationCount).join(',')
        };

        //console.log(postData);
        $.ajax({
            type: 'POST',
            dataType: 'json',
            data: postData,
            url: '/ajax/store/ajax.aspx',
            success: function (o) {
                // debugger
                for (idx in o.Groups) {
                    var group = o.Groups[idx];
                    HomeFreeWall.appendBlock(group);
                }
            },
            complete: completeCallback
        });
    };

    // debounce used for performance
    // check if we are at footer and we still have groups to load plus we are not already loading groups of tiles.
    $(document).scroll(debounce(function () {
        if ($(window).scrollTop() + $(window).height() > $('.footer').offset().top && Groups.length > 0 && !isLoadingTiles) {
            if (pageCount === 0) {
                var opts = getFreewallOptions();
                // opts.delay = 500;
                //opts.animate = true;
                HomeFreeWall.reset(opts);
                HomeFreeWall.fitWidth();
            }

            pageCount++;

            $tilesLoading.show();
            isLoadingTiles = true;
            loadTiles(onComplete);
        }
    }, 250));
	
	// handle window resize and wall disappears
	$(window).resize(debounce(function () {
		console.log("reload tiles");
		$tilesLoading.show();
		isLoadingTiles = true;
		loadTiles(onComplete);
	}, 250));

    // on complete, reset is loading and hide the loading div
    // also reset image tiles and register youtube videos
    function onComplete() {
        isLoadingTiles = false;
        setTimeout(function () { $tilesLoading.hide(); }, 2000);

        $(".media-wall-img").each(function () {
            var imageSrc = $(this).attr("src");
            $(this).parents(".brick-item:not(.brick-video)").css("background", "url('" + imageSrc + "') no-repeat center center / cover");
        });

        $(".brick-video-youtube").each(function () {
            $this = $(this),
            id = $this.find('div').attr('data-youtubecode');
            if (id) {
                if (!players['youtubeVid_' + id]) {
                    players['youtubeVid_' + id] = new YT.Player('youtubeVid_' + id, {
                        height: '482',
                        width: '482',
                        videoId: id,  // youtube video id
                        playerVars: {
                            'autoplay': 0,
                            'rel': 0,
                            'showinfo': 0
                        },
                        events: {
                            'onStateChange': onPlayerStateChange
                        }
                    });

                    var p = document.getElementById("youtubeVid_" + id);
                    $(p).hide();

                    var imageSrc = "http://img.youtube.com/vi/0XrvLRiZVos/maxresdefault.jpg";
                    $this.css("background", "url('" + imageSrc + "') no-repeat center center / cover");

                    $this.on("click", function () {
                        var id = $(this).find('iframe').attr('data-youtubecode');
                        $('#youtubeVid_' + id).parent().removeClass('not-playing');
                        $("#youtubeVid_" + id).show();
                        players['youtubeVid_' + id].playVideo();
                    });
                }
            }
        });
    };
</script>




