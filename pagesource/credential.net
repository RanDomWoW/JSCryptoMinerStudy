  <meta property="og:site_name" content="Accredible" />
  <meta property="og:title" content="{{ $ctrl.meta.title }}" />
  <meta property="og:description" content="{{ $ctrl.meta.description }}" />
  <meta property="og:url" content="{{ $ctrl.meta.url }}" />
  <meta property="og:image" content="{{ $ctrl.meta.image }}" />
  <meta property="og:image:width" content="{{ $ctrl.meta.image_width }}" ng-if="$ctrl.meta.image_width" />
  <meta property="og:image:height" content="{{ $ctrl.meta.image_height }}" ng-if="$ctrl.meta.image_height" />
  <!-- Twitter -->
  <meta name="twitter:card" content="{{ $ctrl.meta.twitter_card }}">
  <meta name="twitter:site" content="@{{ $ctrl.meta.twitter_site }}">
  <meta name="twitter:creator" content="@{{ $ctrl.meta.twitter_username }}">
  <meta name="twitter:title" content="{{ $ctrl.meta.title }}">
  <meta name="twitter:description" content="{{ $ctrl.meta.twitter_description || $ctrl.meta.description }}">
  <meta name="twitter:image" content="{{ $ctrl.meta.image }}">
  <!-- Style -->
  <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico" />
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/spectrum/1.8.0/spectrum.min.css">
  <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.0/themes/smoothness/jquery-ui.min.css">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,300i,400,400,600,600">
  <link rel="stylesheet" href="/app/style.css?v=1.24.2-1521794904676">
