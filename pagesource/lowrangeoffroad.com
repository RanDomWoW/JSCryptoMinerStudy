    $zopim.livechat.setOnStatus(updateStatus);
  }); 

 function updateStatus(s) {

 	var chts = document.getElementById("chat_status");
 	var chtstyle = document.getElementById("chat_styles");
 	var cbtn = document.getElementById("chat_btn_color");
 	var cbtnIcon = document.getElementById("chat_icon");

 	
		chts.innerHTML = s;


	if (s == 'online') //This is what runs the APIs below when chat is Online
		{
		chtstyle.style.color = "#ffffff";
		cbtn.className = "btn btn-success";
		cbtnIcon.className = "btn btn-success";
		
		}
	else if (s == 'away') //This is what runs the APIs below when chat is Away
		{
		chtstyle.style.color = "#FFDEA1";
		cbtn.className = "btn btn-warning";
		cbtnIcon.className = "btn btn-warning";
		
		}
    else if (s == 'offline') //This is what runs the APIs below when chat is Offline
		{
		chtstyle.style.color = "#B8B8B8";
		cbtn.className = "btn btn-primary";
		cbtnIcon.className = "btn btn-primary";
			
		}

  }   

