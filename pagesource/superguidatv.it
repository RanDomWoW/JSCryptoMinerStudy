<script>

  var sectionSelected ="night";

  var pageEveningName = 'serata';
  var pageOnAirName = 'ora-in-onda';
  var pageFilmviewName = 'film-in-tv';
  var arrBouquet = {"1":"Sky","2":"Premium","3":"Nazionali","4":"RSI"};
  var arrSubBouquet = {"1":"Intrattenimento","2":"Sport","3":"Cinema","4":"Doc e Lifestyle","5":"News","6":"Bambini","7":"Musica","8":"Primafila"};
  var siteBaseUrl = 'https://www.superguidatv.it/';
  var pathOutputHtml = '/wp-content/themes/SGTV-Newspaper/sgtv-static-output/';
  
  var selectorVar = debounce(dynamicResizeSelector, 250);

  function debounce(func, wait, immediate) {
    var timeout;
    return function() {
      var context = this, args = arguments;
      var later = function() {
        timeout = null;
        if (!immediate) func.apply(context, args);
      };
      var callNow = immediate && !timeout;
      clearTimeout(timeout);
      timeout = setTimeout(later, wait);
      if (callNow) func.apply(context, args);
    };
  };

  window.addEventListener("resize", selectorVar);

  function createUrlSuperguidaApp(section, plt, bq) {
    
    var url = siteBaseUrl;

    switch (section){
      case "night":
        url = url+pageEveningName;
        if (plt != 3){
          url = url+'/oggi';
        }
        break;

      case "onair":
        url = url+pageOnAirName;
        break;

      case "filmview":
        url = url+pageFilmviewName;
        if (plt != 3){
          url = url+'/oggi';
        }
        break;   
    }
  
    if (plt != 3){
      url=url.concat("/"+arrBouquet[parseInt(plt)].toLowerCase());
    }
    if (bq !== undefined) {
      url=url.concat("-"+arrSubBouquet[parseInt(bq)].toLowerCase());
    }

    return url;              
  }

  function setFooterButtonUrlSuperguidaApp(section) {

      var buttonFooterDtt = document.getElementById("buttonFooterDtt");
      var buttonFooterPremium = document.getElementById("buttonFooterPremium");
      var buttonFooterSky = document.getElementById("buttonFooterSky");
      var buttonFooterRsi = document.getElementById("buttonFooterRsi");  

      buttonFooterDtt.href=createUrlSuperguidaApp(section,"3");
      buttonFooterPremium.href=createUrlSuperguidaApp(section,"2");
      buttonFooterSky.href=createUrlSuperguidaApp(section,"1","3");
      buttonFooterRsi.href=createUrlSuperguidaApp(section,"4");

      return true;
  }

  function selectPage(section) {
      var selector = document.getElementById("tvguide_selector");

      var sectionNight = document.getElementById("night");
      var sectionNow = document.getElementById("now");
      var sectionMovie = document.getElementById("movie");

      switch (section){
          case "night":
              sectionSelected="night";
              selector.style.left=0 + "px";

              sectionNight.style.display='block';
              sectionNight.style.opacity=1;

              sectionNow.style.display='none';
              sectionNow.style.opacity=0;

              sectionMovie.style.display='none';
              sectionMovie.style.opacity=0;

              setFooterButtonUrlSuperguidaApp("night");
              break;

          case "now":
              jQuery("#now").load("https://www.superguidatv.it/wp-content/themes/SGTV-Newspaper/sgtv-static-output/sgtv-home-onair-widget.html", function(responseTxt, statusTxt, xhr){
                  if(statusTxt == "error")
                    jQuery("#now").html('<div style="min-height: 200px;text-align: center;padding-top: 20px;font-size: 22px;cursor: pointer;line-height: 56px;">Unable to connect to server</div>');
              });
              
              sectionSelected="now";
              selector.style.left=(selector.offsetWidth + 7 + 14) + "px";

              sectionNight.style.display='none';
              sectionNight.style.opacity=0;

              sectionNow.style.display='block';
              sectionNow.style.opacity=1;

