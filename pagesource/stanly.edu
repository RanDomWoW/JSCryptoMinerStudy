<script language="JavaScript" type="text/javascript">
    var slide_wait = 0;
    var current_slide = 1;
    var max_slides = 1;


    jQuery(document).ready(function () {

        jQuery('.minidot').click(function () {

            var slide_class = jQuery(this).attr('id');
            var slide_class_trunc = slide_class.substr(3, slide_class.length - 1);
            current_slide = slide_class_trunc;
            slide_wait = 1;
            jQuery(".minidot").css("background-color", "#124678");
            jQuery(this).css("background-color", "#FFCC00");
            jQuery('.glsection').addClass('hide');
            jQuery('#glsection' + slide_class_trunc).removeClass('hide');
  
            timer();
        });
        
        function timer(){
          clearTimeout(timeout);
          timeout = setTimeout(forward_slide, 5000);   
        }

        function forward_slide() {

            if (slide_wait == 1) {
                slide_wait = 0;
                clearTimeout(timeout);
                timeout = setTimeout(forward_slide, 5000);
            } else {
                jQuery('.glsection').addClass('hide');
                if (current_slide < max_slides) {
                    current_slide++;
                } else {
                    current_slide = 1;
                }

                jQuery('#glsection' + current_slide).removeClass('hide');
                jQuery(".minidot").css("background-color", "#124678");
                jQuery('#dot' + current_slide).css("background-color", "#FFCC00");
                timer();
            }

        }

       var timeout =  setTimeout(forward_slide, 5000);

    });



</script>

