<script type="text/template" id="template_company">
	<div class="per_{{category}} ib personalized_item hide_ele " data-type="{{category}}">
		{{if personalized_data && personalized_data.length}}
		<ul class="company_wrapper">
			{{each personalized_data as value i}}
			{{if i<=3}}
			<li class="link_wrap">
				<a class=" personalized_link_company " href="/company/{{value['id']}}" data-index="{{i}}"
				   target="_blank">
					<div class="logo_wrap personalized_logo_wrap ">
						{{if value.logo && value.logo.length}}
						<img class="logo personalized" src="https://ss.intsig.net/yemai/vip/camfs/qxb/{{value.logo + '.t160u'}}">
						{{else}}
						{{if value.logo_cname && value.logo_cname.length < 5}}
						{{if 2==value.logo_cname.length || 1==value.logo_cname.length}}
						<div class="c_core_logo length_three_logo">{{value.logo_cname}}</div>
						{{/if}}
						{{if 3==value.logo_cname.length}}
						<div class="c_core_logo length_three_logo">{{value.logo_cname}}</div>
						{{/if}}
						{{if 4==value.logo_cname.length}}
						<div class="c_core_logo">{{value.logo_cname}}</div>
						{{/if}}
						{{else}}
						<i class="icon_company personalized"></i>
						{{/if}}
						{{/if}}
					</div>
					<div class="company_name hide_long">{{value.name}}</div>
					<div class="{{ !value['business'] || !value['business'].length ?'':'businesses'}} ib ">
						{{if value.business &&value.business.length}}
						<div class="business_title">主营业务：</div>
						{{each value.business as busi m}}
						<div class="business">{{busi}}</div>
						{{/each}}
						{{/if}}
					</div>
					<div class="sr">
						{{if value.start_date}}
						<div class="start_time">成立时间：{{value.start_date}}</div>
						{{/if}}
						{{if value.reg_capi}}
						<div class="{{value.start_date?'register_capi' : ''}}">
							注册资本：{{value.reg_capi}}
						</div>
						{{/if}}
					</div>
					<div class="contact ib">
						{{ if value['employee'] && value['employee'] != 0}}
						<i class="icon_member"></i>
						{{ if value['contact_post'] }}
						<div class="employee">{{ value['contact_post'] }}
							等{{ value['employee'] }}人在早稻
						</div>
						{{else}}
						<div class="employee">等{{ value['employee'] }}员工在早稻</div>
						{{/if}}
						{{/if }}
						{{ if value['contact_status'] && value['contact_status'] == 1 }}
						<div class="{{ value['employee'] ? 'contact_status' : '' }}">
							<i class="icon_call"></i>
							可联系
						</div>
						{{/if}}
					</div>
					{{if is_show_company}}
					<div class="feedback_wrap" data-index="{{i}}">
						<div class="feedback_inner">
							<i class="icon_ellipsis feedback"></i>
						</div>
					</div>
					{{/if}}
				</a>
			</li>
			{{/if}}
			{{/each}}
			{{/if}}
		</ul>
	</div>
</script>
<script type="text/template" id="template_person">
