<script type="text/javascript">
    var g_yourjob_cate	= 0;
    
        get_vlnb(1, '57');
    


    function get_vlnb(page, category)
    {
        var country	= '';
        $('input[name=jobs_option]').each(function(){
            if(this.checked){
                country	= this.value;
            }
        });
        if(country)
        {
            if(category){
                g_yourjob_cate	= category;
            }

            $.getJSON(
                    HOST + '/index/yourjob?callback=?',
                    {
                        'ci'	: g_yourjob_cate,
                        'p'		: page
                    },
                    function(data)
                    {
                        $('#resultVLNB_'+country).html(data.html);
                        show_vlnb(country);
                    }
            );
        }
    }

    function show_vlnb(country)
    {
        $('#resultVLNB_vi').hide();
        $('#resultVLNB_ja').hide();
        $('#resultVLNB_engineer').hide();

        $('#resultVLNB_'+country).show();
    }
</script>
