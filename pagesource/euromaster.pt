    // dimension
    url += section;
    url += "-" + profile;
    url += "-" + rim;
    if (!isBlank(speed)) { url += "-" + speed; }
    if (!isBlank(load)) { url += "-" + load; }

    // specificities
    var seasonList = "";
    if (seasons != null && seasons.length > 0) {
      for (var i = 0 ; i < seasons.length ; i++) {
        if (seasons[i] == "SUMMER") { seasonList += "summer" + (i<seasons.length-1 ? ":" : ""); }
        if (seasons[i] == "WINTER") { seasonList += "cold-weather" + (i<seasons.length-1 ? ":" : ""); }
        if (seasons[i] == "ALL") { seasonList += "all-seasons" + (i<seasons.length-1 ? ":" : ""); }
      }
    }
    if (xl || rf || wt || seasonList.length > 0) {
      url += "--";
