                600: {
                    items: 2
                },
                992: {
                    items: 3
                },
                1200: {
                    items: 4
                },
            },
            dots: false,
            responsiveRefreshRate: 300
        });
        owlArray.each(function () {
            var $this = $(this)
            $this.siblings().find('.prev').click(function (e) {
                e.preventDefault();
                $this.trigger('prev.owl.carousel');
            })
            $this.siblings().find('.next').click(function (e) {
                e.preventDefault();
                $this.trigger('next.owl.carousel');
            })
        });
        owlArray.on('mousewheel', '.owl-stage', function (e) {
            if (e.deltaY>0) {
                $(this).trigger('next.owl.carousel');
            } else {
                $(this).trigger('prev.owl.carousel');
            }
            e.preventDefault();
        });
    });
</script>

