    <script type="text/javascript">
    // Set default plans - monthly
    var defaultPlans = function() {

        $('.freelance span.price').text(37);
        $('.professional span.price').text(67);
        $('.consultant span.price').text(97);

        $('.unit').text('per month');
        $('.monthSpan').removeClass('hide');
        $('.annualSpan').addClass('hide');

        $('input[name="planType"]').val('month');
        $('input[name=payment_type]').val('month');

        $('.freelance .change-plan-btn').attr('onclick', "changePlan('BasicPlan')");
        $('.professional .change-plan-btn').attr('onclick', "changePlan('ProPlan')");
        $('.consultant .change-plan-btn').attr('onclick', "changePlan('UnlimitedMonthly5Sub')");

        $('.freelance .downgrade-plan').attr('onclick', "downgradePlan('BasicPlan')");
        $('.professional .downgrade-plan').attr('onclick', "downgradePlan('ProPlan')");

    }

    // Switch plans function
    var switchPlans = function() {
        // If monthly, switch to annual
        if($('input[name="planType"]').val() == 'month') {

            $('.freelance span.price').text(360);
            $('.professional span.price').text(600);
            $('.consultant span.price').text(959);

            $('.unit').text('annually');
            $('.monthSpan').addClass('hide');
            $('.annualSpan').removeClass('hide');

            $('input[name=payment_type]').val('annually');
            $('input[name="planType"]').val('annual');

            $('.freelance .change-plan-btn').attr('onclick', "changePlan('AnnualDiscountBasic')");
            $('.professional .change-plan-btn').attr('onclick', "changePlan('AnnualDiscountPro')");
            $('.consultant .change-plan-btn').attr('onclick', "changePlan('UnlimitedAnnual5Sub')");

            $('.freelance .downgrade-plan').attr('onclick', "downgradePlan('AnnualDiscountBasic')");
            $('.professional .downgrade-plan').attr('onclick', "downgradePlan('AnnualDiscountPro')");

        } else {
            // Switch to default - monthly plans
            defaultPlans();

        }
    }

    defaultPlans();

    $('a.switchPlans').click(function(e) {

        e.preventDefault();
        switchPlans();

    });
</script>

