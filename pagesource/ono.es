	 GeocodeNexus_generic_c2c.prototype.init_generic_c2c=function(){
		 try{
			 var self=this;
				//iniciamos el input como un autocomplete
				var autocomplete_generic_c2c=$(".profile-addr_generic_c2c").autocomplete({
					minLength: 3,
					delay:0,
					messages: {
						noResults: '',
						results: function() {}
					},	
					source: function (request, response) {
						jQuery.ajax({
							url: self.urlRequest,
							data:{address:request.term}, //escapem l'address perque arribi be al proxy
							dataType: "json",
							type: "POST",
							success: function (dataResponse) {
								self.urlEnCurso=false;
								if(dataResponse.results && dataResponse.results.length>0){
									response(jQuery.map(dataResponse.results, function(item){
										return self.getAddress_generic_c2c(item);
									}));
								}else{
									response([]);
								}
							},
							error:function(data){
								self.urlEnCurso=false;
								response([]);
							},
							beforeSend: function( xhr ) {
								if(self.urlEnCurso)return false; //No ejecutamos la peticion mientras haya otra que se esta ejecutando
								else self.urlEnCurso=true; //Si no hay ninguna ejecutando, marcamos que se va a ejecutar una peticion
							}
						})
					}
				});

				autocomplete_generic_c2c.data( "ui-autocomplete" )._renderMenu = function( ul, items ) {
					var that = this;
					ul.addClass("nexus-container");
					jQuery.each( items, function( index, item ) {
						that._renderItem( ul, item );
					});
				};

				autocomplete_generic_c2c.data( "ui-autocomplete" )._renderItem = function( ul, item ) {
					var direccionBuscada=jQuery(".profile-addr_generic_c2c").val();
					var indice=self.normalizar_generic_c2c(item.direccion).indexOf(self.normalizar_generic_c2c(direccionBuscada));
					var direccionNegro=item.direccion;
					var direccionBlancoInicial="";
					var direccionBlancoFinal="";

					if(indice>=0){
						direccionNegro=item.direccion.substr(indice,direccionBuscada.length);
						direccionBlancoInicial=item.direccion.substring(0,indice);
						direccionBlancoFinal=item.direccion.substring(indice+direccionBuscada.length);
					}

					return jQuery('<li class="nexus-item"></li>')
						.data("ui-autocomplete-item", item)
						.append('<a><span>'+direccionBlancoInicial+'</span><span class="nexus-item-black">'+direccionNegro +'</span><span>' + direccionBlancoFinal+
						' </span><span class="nexus-item-smaller">'+item.localidadYPais +'</span></a>')
						//.append('<a>' + item.label + '</a>')
						.hover(function(){
							jQuery( this ).addClass("nexus-item-hover");
						},function(){
							jQuery( this ).removeClass("nexus-item-hover");
						})
						.appendTo(ul);
				};
			 
		 }catch(ex){
			alert("Error a GeocodeNexus.init:"+ex.message);
		 }
	 };

	 /**
	 * Devuelve un objeto con las propiedades a mirar para hacer las negritas y tamano mas pequeno en la localidad + pais igual que google
	 **/
	 GeocodeNexus_generic_c2c.prototype.getAddress_generic_c2c=function(item){
	 	var direccion="";
	 	var localidad="";
	 	var numero="";
	 	var pais="";
	 	
	 	for(var i=0; i < item.address_components.length;i++){
	 		if(direccion!="" && localidad !="" && numero !="" && pais != ""){
	 			break;
	 		}
	 		if(item.address_components[i].types[0]=="locality"){
	 			localidad=item.address_components[i].long_name;
	 		}
	 		else if(item.address_components[i].types[0]=="route"){
	 			direccion=item.address_components[i].long_name;
	 		}
	 		else if(item.address_components[i].types[0]=="street_number"){
	 			numero=item.address_components[i].long_name;
	 		}
	 		else if(item.address_components[i].types[0]=="country"){
	 			pais=item.address_components[i].long_name;
	 		}
	 	}
	 	
	 	var label="";
	 	var localidadYPais="";
	 	if(numero!="" && direccion !=""){
	 		direccion=direccion+ ", "+ numero;
	 	}
	 	
	 	if(direccion !="") localidadYPais=", ";
		if(localidad !="") localidadYPais=localidadYPais +localidad;
		if(pais != "") localidadYPais=localidadYPais + ", "+ pais;
		if(localidadYPais != "") label=direccion + localidadYPais;
		

	 	return {
	 		direccion:direccion,
	 		localidadYPais:localidadYPais,
	 		label:label,
	 		value:label
	 	}
	 };

	 /**
	 *Devuelve el string pasado por parametro sin acentos para poder comparar sin tener en cuenta estos acentos
	 **/
	 GeocodeNexus_generic_c2c.prototype.normalizar_generic_c2c=function(s){

	 	var nombre=s.toLowerCase();
	 	var r=s.toLowerCase();
	 	r = r.replace(new RegExp("[àáâãäå]", 'g'),"a");
	 	r = r.replace(new RegExp("æ", 'g'),"ae");
	 	r = r.replace(new RegExp("ç", 'g'),"c");
	 	r = r.replace(new RegExp("[èéêë]", 'g'),"e");
	 	r = r.replace(new RegExp("[ìíîï]", 'g'),"i");
	 	r = r.replace(new RegExp("ñ", 'g'),"n");
	 	r = r.replace(new RegExp("[òóôõö]", 'g'),"o");
	 	r = r.replace(new RegExp("œ", 'g'),"oe");
	 	r = r.replace(new RegExp("[ùúûü]", 'g'),"u");
	 	r = r.replace(new RegExp("[ýÿ]", 'g'),"y");
	 	return r;
	 };
	 
	 	
