	<script type="text/javascript">

         $(document).ready(function() {
		       // -- Sub menu store lavazza -- 
		      var inside_find = 0;
		      var timer_find;
		
		            $('.store').mouseenter(function(){
		                $('.sub-menu-store').show();
		            });
		            $('.store').mouseleave(function(){
		                timer_find = setTimeout(function(){
		                    if(inside_find == 0) {
		                        $('.sub-menu-store').hide(); 
		                        clearInterval(timer_find);
		                    }
		                }, 200);
		            });
		            
		            $('.sub-menu-store').mouseenter(function(){
		                inside_find = 1;
		            });
		            
		            $('.sub-menu-store').mouseleave(function(){
		                $(this).hide();
		                clearInterval(timer_find);
		                inside_find = 0;
		            });
		            // -- END Sub menu store lavazza -- 
		     
		   
		    
		  // -- Sub menu search lavazza -- 
		  var inside_find = 0;
		  var timer_find;

				$('.search').mouseenter(function(){
					$('.sub-menu-search').show();
				});
				$('.search').mouseleave(function(){
					timer_find = setTimeout(function(){
						if(inside_find == 0) {
							$('.sub-menu-search').hide(); 
							clearInterval(timer_find);
						}
					}, 200);
				});

				$('.sub-menu-search').mouseenter(function(){
					inside_find = 1;
				});
				
				$('.sub-menu-search').mouseleave(function(){
					$(this).hide();
					clearInterval(timer_find);
					inside_find = 0;
				});
				// -- END Sub menu search lavazza -- 
	 
	 
   });     //fine dom

 </script>







