    <script data-cfasync="false" src="/cdn-cgi/scripts/d07b1474/cloudflare-static/email-decode.min.js"></script><script>
        var form = $(".signup"),
            submitButton = form.find("button");

        function showLoading() {
            submitButton.text("Please wait...");
        }

        function hideLoading() {
            submitButton.text("Create an account");
        }

        form.submit(function(event) {
            event.preventDefault();

            // Start loading
            showLoading()

            // Start loading
            jQuery.ajax({
                url: "https://gab.ai/auth/register",
                type: "POST",
                data: form.serialize(),
                dataType: "JSON",
                success: function(response) {
                    // Clear error state
                    form.find(".form-group").removeClass("form-group--error");

                    // Redirect
                    window.location.href = "https://gab.ai/home?onboarding=true";
                },
                error: function(response) {
                    // Clear error state
                    form.find(".form-group").removeClass("form-group--error");

                    // Get errors
                    var errors = response.responseJSON.errors;

                    for (key in errors) {
                        var formGroup = form.find("[data-key='" + key + "']");
                        formGroup.addClass("form-group--error");
                        formGroup.find(".form-group__message span").text(errors[key]);
                    }

                    // Reload captcha
                    $('input#captcha').val('');
                    $('[data-key="captcha"] img:first').each(function() {
                        $(this).attr('src', $(this).attr('src') + (new Date));
                    });

                    // End loading
                    hideLoading()
                }
            });
        });
    </script>

