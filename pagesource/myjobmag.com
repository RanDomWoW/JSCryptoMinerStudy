<script>
				var expanded = false;
				function showCheckboxes(box, chk) {
					var checkboxes = document.getElementById(box);
					if (!expanded) {
						checkboxes.style.display = "block";
						expanded = true;
					} else {
						checkboxes.style.display = "none";
						expanded = false;
					}
					/* document.getElementById('wrapper').onclick = function(e) {
						if(e.target != document.getElementById('filterjobs')) {
							checkboxes.style.display = "none";         
						}
					} */					
				}
				
				function triggerOption(a)
				{
					var b=document.getElementsByName(a);
					for (var i=0; i<b.length; i++)
					{
						b[i].onclick = function () 
						{  
							var c=''; var g=document.getElementsByName(this.name);
							for (var i=0; i<g.length; i++){  if(g[i].checked){ c+= g[i].id.substring(3)+', ';}	}
							var temp=this.name.length-2; 
							if(c==''){ c='- Select '+ capitalizeFirstLetter(this.name.substring(0,temp)) +' -' }
							var chk='select-'+this.name.substring(0,temp);
							document.getElementById(chk).options[0].text=c;
						}	
						
						var c=''; var g=document.getElementsByName(b[i].name);
						for (var j=0; j<g.length; j++){  if(g[j].checked){ c+= g[j].id.substring(3)+', ';}	}
						var temp=b[i].name.length-2; 
						if(c==''){ c='- Select '+ capitalizeFirstLetter(b[i].name.substring(0,temp)) +' -' }
						var chk='select-'+b[i].name.substring(0,temp);
						//document.getElementById('xhrt').value = chk;
						
						document.getElementById(chk).options[0].text=c;

					}
				}
				
				function capitalizeFirstLetter(string) { return string.charAt(0).toUpperCase() + string.slice(1);}
				
				triggerOption('states[]'); triggerOption('industry[]'); triggerOption('field[]'); triggerOption('education[]'); 
				
				var filterdivwrap=document.getElementById('filterjobs');
					document.body.onclick = function(e) {
						var crtElem = e.target;
						/*while (crtElem != null) {
							crtElem = crtElem.parentNode;
						}*/
						
						var chkbks=document.getElementsByClassName('selectBox'); 
						if ((crtElem == null || crtElem.className != 'overSelect') && !(crtElem != null && crtElem.className == 'dummyclass' )) {
							var chkbkState=document.getElementById('checkboxes-state');
							if(chkbkState != null){chkbkState.style.display = "none";}
							var chkbkField=document.getElementById('checkboxes-field');
							if(chkbkField != null){chkbkField.style.display = "none";}
							var chkbkEducation=document.getElementById('checkboxes-education');
							if(chkbkEducation != null){chkbkEducation.style.display = "none";}
							var chkbkIndustry=document.getElementById('checkboxes-industry');
							if(chkbkIndustry != null){chkbkIndustry.style.display = "none";}
						}
					}
					
