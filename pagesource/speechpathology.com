$(function extendMobileNav() {
    if ($(window).width() < 767) {
        var windowH = $(window).height();
        var wrapperH = $('#navbar-main').height();
        if(windowH > wrapperH) {
            $('#navbar-main').css({'height':($(window).height())+'px'});
        }
        $(window).resize(function(){
            var windowH = $(window).height();
            var wrapperH = $('#navbar-main').height();
            var differenceH = windowH - wrapperH;
            var newH = wrapperH + differenceH;
            var truecontentH = $('#navbar').height();
            if(windowH > truecontentH) {
                $('#navbar-main').css('height', (newH)+'px');
            }
        });
    }
});
</script>


