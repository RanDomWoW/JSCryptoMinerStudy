	function fmt_day(time) {
        t = new Date();
        t.setTime(time*1000);
        m = t.getMonth()+1;
        d = t.getDate();
        return p(m)+'-'+p(d);
    }

    function fmt_time(time) {
      t = new Date();
      t.setTime(time*1000);
      h = t.getHours();
      i = t.getMinutes();

      return p(h)+':'+p(i);
    }
    function p(s) {
      return s < 10 ? '0' + s: s;
