        function checkMagentoRegistration(uid) {
            jQuery.ajax({
                type: "POST",
                url: "https://www.recyclebank.com/customer/account/loginpostfb/",
                data: ({
                    fb_id: uid,
                    referer: ''
                }),
                success: function (response) {
                    switch (response) {
                        case '0':
                            redirectToCreateWithFb();
                            break;
                        case 'inactive':
                            redirectToLogin();
                            break;
                        case '':
                        case undefined:
                            var href = window.location.href;
                            var replace = href.match(/(___store=\w+&___from_store=\w+)/);
                            if (replace != null && replace != '') {
                                href = href.replace(replace[0], "");
                            }
                            window.location.href = href;
                            break;
                        default:
                            window.location.href = response;
                            break;
                    }
                },
                error: function (response) {
                }
            })
        }

        function redirectToCreateWithFb() {
            window.location.href = 'https://www.recyclebank.com/customer/account/createwithfb/' + '?success_url=' + registerRedirectUrl;
        }

        function redirectToLogin() {
            window.location.href = 'https://www.recyclebank.com/customer/account/login/';
        }

        function login_user() {
            var self = this;
            if (!self.fbLoginBusy) {
                self.fbLoginBusy = true;
            } else {
                return;
            }
            fbEnsureInit(function() {
                FB.login(function (response) {
                    self.fbLoginBusy = false;
                    if (typeof(jQuery('.lightbox-loader')) == 'object' && response.status == 'unknown') {
                        jQuery('.lightbox-loader').hide();
                    }
                    if (response.authResponse) {
                        checkMagentoRegistration(response.authResponse.userID);
                    }
                }, {scope: 'email'});
            });
        }
