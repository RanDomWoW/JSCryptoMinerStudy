                var categorias = [];
                if(data.estatus) {
                    var counter = data.categorias.length;
                    counter = (counter > 2) ? 2 : counter;
                    if(counter > 0){
                        $("#selectCategoriasMenu").empty();
                        setSearchCombo(null);
                        for(var i = 0; i < counter; i++){
                            var tmp = [];
                            tmp['id'] = data.categorias[i].id;
                            tmp['clase']= "categoriaItem";
                            tmp['cadena'] = data.cadena;
                            tmp['nombre'] = "<em>" + data.cadena + "</em>" + ' en ' + "<em>" + data.categorias[i].nombre + "</em>";
                            setSearchCombo(data.categorias[i]);
                            productos.unshift(tmp);
                        }
                        var tmp = [];
                        tmp['id']     = "";
                        tmp['clase']  = "categoriaItem";
                        tmp['cadena'] = data.cadena;
                        tmp['nombre'] = "<em>" + data.cadena + "</em>" + ' en ' + "<em>Todas las categorias</em>";
                        productos.unshift(tmp);
                    }

                }
                response(productos);
            }).fail(function (error){
                console.log("Error de Red");
            });
        }
    });
