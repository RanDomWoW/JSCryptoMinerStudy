    function buscaPaginado(elemento)
    {
        $('#search').val($(elemento).attr('title'));
        var host = document.location.origin;
        var action = $(elemento).attr('class');
        $("#frmBuscador").attr('action',host + action);
        $("#frmBuscador").submit();
    }

    /**
     * Funcion encargada de enviar el
     * formulario de busqueda
     * */
    function sendForm()
    {
        setTimeout( function(){
            $("#btnSendForm").click();
        },300);
    }

    /**
     * Funcion encargada de insertar el filtro de categorias
     * */
    function setSearchCombo(categoria)
    {
        if(categoria === null){
            var combo = '<option value="">Todas las Categorias</option>';
        } else {
            var combo = '<option value="'+categoria.id+'">' + categoria.nombre + '</option>';
        }
        $(combo).appendTo($("#selectCategoriasMenu"));
    }

    /**
     * Funcion encargada de desaparecer
     * el panel de busquedas recientes.
     **/
    $( "#search" ).focusout(function() {
