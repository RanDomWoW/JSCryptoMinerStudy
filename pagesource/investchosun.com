    <script>
	getCookiePop('PopCookie010');

	/* by hyemi */
	$(".main_img:first-child").show();

	var main_img = 0;

	function banner_rolling(img_num) {
		main_img = parseInt(img_num) + 1;

		var size = $(".main_img").length;

		if (size > 1) {
			if (main_img > size - 1) {
				main_img = 0;
			}

			for (var i=0; i<size; i++) {
				$(".main_img").eq(i).fadeOut('slow');
			}
			$(".main_img").eq(main_img).fadeIn('slow');
		}
	}

	setInterval(function() {
		banner_rolling(main_img);
	}, 5000);

	/* end */

	function goDetailNotice() {
		location.href = "/notice?uid=29&mod=document";
	}

	function onlyCloseModal() {
		$('.overlay').fadeOut();
		$('.modal').hide();
	}

    	function closeModal() {
    		if (document.getElementById("PopCookie010").checked){
				setCookie("PopCookie010", "done", 1); // 쿠키값 체크
			} 
			
        	$('.overlay').fadeOut();
		$('.modal').hide();
    	}

    	function getCookiePop(Name) { // 쿠키 불러오는 함수
    		var search = Name + "=";
    		if (document.cookie.length > 0) { // if there are any cookies
    			offset = document.cookie.indexOf(search);
    			if (offset != -1) { // if cookie exists
    				offset += search.length; // set index of beginning of value
    				end = document.cookie.indexOf(";", offset); // set index of end of cookie value
    				if (end == -1)
    					end = document.cookie.length;
    				return unescape(document.cookie.substring(offset, end));
    			}else {
				$('.modal').show();
			}
    		}
    	}

    	function setCookie(name, value, expiredays) {
			var todayDate = new Date();
			todayDate.setDate(todayDate.getDate() + expiredays);
			document.cookie = name + "=" + escape(value) + "; path=/; expires="
					+ todayDate.toGMTString() + ";"
		}
    </script>	
