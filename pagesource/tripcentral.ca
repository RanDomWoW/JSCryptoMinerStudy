	$("a.mobile-dropdown").click(function(e) {
		//	Using width was unreliable because the scrollbars get subtracted from it. So instead we check for a couple of
		//		classes that are only applied on the mobile version... Oh, and Matchmaker, apparently, so we add a second check for that.
		if ($("body").hasClass("open") && !$("body").hasClass("finder-styles")) {
			e.preventDefault();

			var clickedElement = $(this);
			$("a.mobile-dropdown").each(function() {
				if($(this).attr("href") === clickedElement.attr("href")) {
					$(this).parent("li").toggleClass("selected");
				} else {
					$(this).parent("li").removeClass("selected");
				}
			});
		}
	});
});




