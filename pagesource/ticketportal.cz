            HTML.push('        <p class="panel-title">Toto představen&#237;&#233; lze zakoupit pouze ze str&#225;nek partnera přednostn&#237;ho n&#225;kupu. ' + zozWeb + '</p>');
            HTML.push('    </div>');
            HTML.push('</div>');
        }

