	<script type ="text/javascript">
	    function SearchDocClick() {
	        if (document.getElementById('txtDocNumber').value == "") {
	            alert("GIVE DOCUMENT NUMBER....");
	            document.getElementById('txtDocNumber').focus();
	            return false;
	        }
	        window.open('Doc_TrackingMulti.aspx?No=' + document.getElementById('txtDocNumber').value + '&Tmp=' + (new Date()).getTime(), '_blank', '', false);
	        return false;
	    }
	    function SearchNetworkClick() {
	        if (document.getElementById('txtDocNumber').value == "") {
	            alert("GIVE CENTER NAME....");
	            document.getElementById('txtDocNumber').focus();
	            return false;
	        }
	        window.open('NetworkSearch.aspx?Center=' + document.getElementById('txtDocNumber').value + '&Tmp=' + (new Date()).getTime(), '_blank', '', false);
	        return false;
	    }
	    function SearchPincodeClick() {
	        if (document.getElementById('txtDocNumber').value == ""
                || document.getElementById('txtDocNumber').value.length != 6
                || isNaN(Number(document.getElementById('txtDocNumber').value)) == true) {
	            alert("GIVE PROPER 6 DIGIT PINCODE NUMBER....");
	            document.getElementById('txtDocNumber').focus();
	            return false;
	        }

	        window.open('PincodeLocator.aspx?Pincode=' + Number(document.getElementById('txtDocNumber').value) + '&Tmp=' + (new Date()).getTime(), '_blank', '', false);
	        return false;
	    } 
	</script>

