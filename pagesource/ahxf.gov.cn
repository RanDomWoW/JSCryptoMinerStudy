    <script type="text/javascript">

var mh = 40; //最小高度
var step1 = 20; //每次变化的px量
var ms = 10; //每隔多久循环一次
//折叠速度的设置方法
function toggle(o,type){

  if (!o.tid)o.tid = "_" + Math.random() * 100;
  if (!window.toggler)window.toggler = {};
  if (!window.toggler[o.tid]){
    window.toggler[o.tid]={
      obj:o,
      maxHeight:o.offsetHeight,
      minHeight:mh,
      timer:null,
      action:1
    };  }
  o.style.height = o.offsetHeight + "px";
  if (window.toggler[o.tid].timer)clearTimeout(window.toggler[o.tid].timer);
  window.toggler[o.tid].action *= -1;
  window.toggler[o.tid].timer = setTimeout("anim('"+o.tid+"')",ms );//注意计时器的用法
  if(type=="up")
	{
		document.getElementById("down").style.display = "block";
		document.getElementById("up").style.display = "none";
		document.getElementById("_Node").style.display = "none";
	}else
	{
		document.getElementById("down").style.display = "none";
		document.getElementById("up").style.display = "block";
		document.getElementById("_Node").style.display = "block";
	}
}
//通过对象的最小高度和最大高度，判断折叠是否停止
function anim(id){
  var t = window.toggler[id];
  var o = window.toggler[id].obj;
  if (t.action < 0){
    if (o.offsetHeight <= t.minHeight){
      clearTimeout(t.timer);
      return;
    }
  }
  else{
    if (o.offsetHeight >= t.maxHeight){
      clearTimeout(t.timer);
      return;
    }
  }
  o.style.height = (parseInt(o.style.height, 10) + t.action * step1) + "px";
  window.toggler[id].timer = setTimeout("anim('"+id+"')",ms );
}
            var t = 0;
                function show() {
                    if (t > 0) {
                        clearInterval(time);
                    }
                    else {
                        if (document.getElementById("down").style.display != "block") {
                            toggle(_Node, 'up');
                        }
                    }
                    t++;
                }
                var time = setInterval("show()", 3000);
    </script>
