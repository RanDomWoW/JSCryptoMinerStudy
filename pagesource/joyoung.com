                pro_animate(img_one, img_two, strong_one, strong_two, em_one, em_two);
                $(".font_item:eq(" + item_num + ")").addClass("show").siblings().removeClass("show");
                $('.pro_left').find('a').attr('href', a_href);
                $('.pro_right').find('a').attr('href', b_href);
            })
        }
        function btnClick() {
            $(".color_btn").bind("click", function () {
                $(".console_item,.color_btn").unbind("click");
                if ($(this).hasClass("next")) {
                    if (pro_num < list_num) {
                        var img1 = pro_list.find("img").eq(pro_num).attr("data-url");
                        var img2 = pro_list.find("img").eq(pro_num + 1).attr("data-url");
                        var strong1 = pro_list.find("strong").eq(pro_num).html();
                        var strong2 = pro_list.find("strong").eq(pro_num + 1).html();
                        var em1 = pro_list.find("em").eq(pro_num).html();
                        var em2 = pro_list.find("em").eq(pro_num + 1).html();
                        var a_href = pro_list.find("a").eq(pro_num).attr("href");
                        var b_href = pro_list.find("a").eq(pro_num + 1).attr("href");
                        pro_num += 2;
                    } else {
                        pro_num = 0;
                        var img1 = pro_list.find("img").eq(pro_num).attr("data-url");
                        var img2 = pro_list.find("img").eq(pro_num + 1).attr("data-url");
                        var strong1 = pro_list.find("strong").eq(pro_num).html();
                        var strong2 = pro_list.find("strong").eq(pro_num + 1).html();
                        var em1 = pro_list.find("em").eq(pro_num).html();
                        var em2 = pro_list.find("em").eq(pro_num + 1).html();
                        var a_href = pro_list.find("a").eq(pro_num).attr("href");
                        var b_href = pro_list.find("a").eq(pro_num + 1).attr("href");
                        pro_num += 2;
                    }
                } else {
                    if (pro_num > 2) {
                        var img1 = pro_list.find("img").eq(pro_num - 4).attr("data-url");
                        var img2 = pro_list.find("img").eq(pro_num - 3).attr("data-url");
                        var strong1 = pro_list.find("strong").eq(pro_num - 4).html();
                        var strong2 = pro_list.find("strong").eq(pro_num - 3).html();
                        var em1 = pro_list.find("em").eq(pro_num - 4).html();
                        var em2 = pro_list.find("em").eq(pro_num - 3).html();
                        var a_href = pro_list.find("a").eq(pro_num - 4).attr("href");
                        var b_href = pro_list.find("a").eq(pro_num - 3).attr("href");
                        pro_num -= 2;
                    } else {
                        pro_num = list_num + 2;
                        var img1 = pro_list.find("img").eq(pro_num - 4).attr("data-url");
                        var img2 = pro_list.find("img").eq(pro_num - 3).attr("data-url");
                        var strong1 = pro_list.find("strong").eq(pro_num - 4).html();
                        var strong2 = pro_list.find("strong").eq(pro_num - 3).html();
                        var em1 = pro_list.find("em").eq(pro_num - 4).html();
                        var em2 = pro_list.find("em").eq(pro_num - 3).html();
                        var a_href = pro_list.find("a").eq(pro_num - 4).attr("href");
                        var b_href = pro_list.find("a").eq(pro_num - 3).attr("href");
                        pro_num -= 2;
                    }
                }
                pro_animate(img1, img2,strong1,strong2,em1,em2);
                $('.pro_left').find('a').attr('href', a_href);
                $('.pro_right').find('a').attr('href', b_href);
            })
        }
        function pro_animate(src1, src2, strong1, strong2, em1, em2) {
            $(".pro_left,.pro_right").addClass("run");
            setTimeout(function () {
                $(".pro_left .img_box img").attr("src", src1).attr("data-url",src1);
                $(".pro_right .img_box img").attr("src", src2).attr("data-url",src2);
                $(".pro_left .img_box .img_text strong").html(strong1);
                $(".pro_right .img_box .img_text strong").html(strong2);
                $(".pro_left .img_box .img_text em").html(em1);
                $(".pro_right .img_box .img_text em").html(em2);
            }, 800);
            setTimeout(function () {
                $(".pro_left,.pro_right").removeClass("run");
                itemClick();
                btnClick();
            }, 1600);
        }
