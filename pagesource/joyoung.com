        function itemClick()
        {
            $(".console_item").bind("click", function () {
                pro_num = 2;
                var item_num = $(this).index();
                pro_list = $(".img_cont .img_list:eq(" + item_num + ")");
                list_num = pro_list.find("img").length;
                var a_href = pro_list.find("a").eq(pro_num - 2).attr("href");
                var b_href = pro_list.find("a").eq(pro_num - 1).attr("href");
                var img_one = pro_list.find("img").eq(pro_num - 2).attr("data-url");
                var img_two = pro_list.find("img").eq(pro_num - 1).attr("data-url");
                var strong_one = pro_list.find("strong").eq(pro_num - 2).html();
                var strong_two = pro_list.find("strong").eq(pro_num - 1).html();
                var em_one = pro_list.find("em").eq(pro_num - 2).html();
                var em_two = pro_list.find("em").eq(pro_num - 1).html();
