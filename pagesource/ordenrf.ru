<script>
   // var $ = jQuery = jquery_1_5_1;
    $('#show_more').css({ 'visibility' : '', 'visible' : '' });
    $('#show_more').hide(0);

    function showMore(){ /* событие OnClick по кнопке "раскрыть" */

        var more_btn = $('#show_more');
        if (more_btn.attr('class')=='more_links'){;
            //alert('i start');
            $('.left_menu_div .active_ul li').show(0);
            $('.active_ul').removeClass('ul_max_size');
            $('.active_ul').hide().show(0);
            more_btn.removeClass('more_links');
            more_btn.addClass('no_more_links');
            more_btn.parent().attr('href','#first_invisible_li');
            //alert('i did');
        } else {
            $('.active_ul').addClass('ul_max_size');
            $('.active_ul').hide().show(0);
            checkOuterLinks();
            more_btn.removeClass('no_more_links');
            more_btn.addClass('more_links');
            more_btn.parent().attr('href','#');

        }

    }

    /* обёртка для старых трёхстолбовых таблиц */
    function isColTable(table){
        var found_p = table.find('.col');
        return (found_p.length>=2);
    }
    function transformColTable(table){
        table.wrap( "<div class='col3_table'></div>" );
    }
    $('.central_column table').each( function(){
        if (isColTable($(this))){
            transformColTable($(this));
        }
    });


    /* обёртка для старых левых и правых картинок */
    $('.central_column img[align = "left"]').each( function(){
        $(this).wrap( "<div class='left_picture'></div>" );
    });

    $('.central_column img[align = "right"]').each( function(){
        $(this).wrap( "<div class='right_picture'></div>" );
    });

    /* убираем лишние html элементы в правых и левых картинках*/
    $('.central_column img[align = "left"],.central_column img[align = "right"]').each( function(){
        //console.log($(this).parent()[0].tagName);
        if ($(this).parent()[0].tagName == 'P'){
            $(this).unwrap();
        }
        $(this).removeAttr('width');
        $(this).removeAttr('height');
        $(this).removeAttr('align');
        $(this).removeAttr('hspace');
        $(this).removeAttr('vspace');
    });




    /* возвращает true, если есть элементы, выходящие за границы левого нижнего меню */
    function checkOuterLinks(){
        $('.left_menu_div .active_ul li').show(0);
        var res=false;
        var left_menu_div = $('.active_ul');
        if (left_menu_div.length>0){
            var max_top = left_menu_div.offset().top+left_menu_div.innerHeight() - 4;
            var max_left = left_menu_div.offset().left+left_menu_div.innerWidth();
            //console.log(max_left);
            var first = true;
            $('.left_menu_div .active_ul li').each( function(){
                var el = $(this);
                var top = $(this).offset().top + $(this).innerHeight() ;
                var left = $(this).position().left;
                if (  (left>max_left) || (top>max_top) ) {
                    if (first){
                        $(this).attr('id','first_invisible_li');
                        first = false;
                    }
                    $(this).hide();
                    res = true;
                    /*return false;*/
                }
            });
        }
        return res;
    }

    function loadResizeEvent(){
        var btn = $('#show_more');
        if ( (!checkOuterLinks()) && (btn.attr('class')=='more_links')){
            btn.hide(0);
            //console.log('hide');
        } else {
            btn.show(0);
        }
    }

    $(window).resize(function(){
        loadResizeEvent();
    });


    $(document).ready(function(){
        //alert('i start');
        loadResizeEvent();
        //alert('i ready');
    });
    console.log('executed');
</script>

