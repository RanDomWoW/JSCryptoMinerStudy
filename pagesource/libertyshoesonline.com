jQuery(document).ready(function(){

    // show cart item count in minicart on page load
    jQuery('.header_cart_loader').show(); 
    var AjaxURL = 'https://www.libertyshoesonline.com/' + 'minicart/index/showCustomMinicart';
    jQuery.ajax({
        dataType: 'json',
        type: "POST",
        url: AjaxURL,
        data: {},
        success: function (result) {
            jQuery('.header_cart_loader').hide();                                     
            var response_qty = parseInt(result.qty);
            if(response_qty > 0){                        
                jQuery('.cart_link .count').text(response_qty);                        
            }else{                        
                jQuery('.cart_link .count').text('0');                    
            }                
        }
    });

    // show cart item count with cart item in minicart on hover of header cart icon
    jQuery('.cart_link').on('mouseenter', function(){
        jQuery('.header_cart_loader').show(); 
        var AjaxURL = 'https://www.libertyshoesonline.com/' + 'minicart/index/showCustomMinicart';
        jQuery.ajax({
            dataType: 'json',
            type: "POST",
            url: AjaxURL,
            data: {},
            success: function (result) {
                jQuery('.header_cart_loader').hide();                                     
                var response_qty = parseInt(result.qty);
                if(response_qty > 0){                        
                    jQuery('.cart_link .count').text(response_qty);                        
                }else{                        
                    jQuery('.cart_link .count').text('0');                    
                }
                jQuery('.header_minicart').html('');
                jQuery('.header_minicart').append(result.html);
                jQuery('.header_minicart').addClass('skip-active');
            }
        });               
    });

    // hide minicart on clicking of close link in minicart
    jQuery(document).on("click", ".skip-link-close" , function() {    
        jQuery('.header_minicart').removeClass('skip-active');
    }); 

     
});    
