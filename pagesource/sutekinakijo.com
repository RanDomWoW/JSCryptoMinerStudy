<script type="text/javascript">
(function($){
  
    $(document).ready(function(){floatingAd();});
  
    function floatingAd(){
        var a,ah,b,bh,c,cb,ch,l,p,t,w,wt,wl,wh,pa,pt,pl,nf,s,r,pt,ar,bw,lc,bm,cp,d;
  
        //edit start
        a=$('#gad-fixed-r');
        b=$('.right-container');
        c=$('.main-container2');
        pt=0;
        bm=10;
        //edit end
 
        if(pt===2) d=$('#container');
 
        bw=b.width();
        ar = a.closest('.plugin-memo');
        ar.css({'overflow':'hidden'});
        cp = $('#container').css('position');
        p='static';
  
        w=$(window);
        l=b.offset().left;
        lc=c.offset().left;
        t=ar.offset().top;
  
        s=function(){
            bh=b.outerHeight();
            ch=c.outerHeight();
            ah=ar.outerHeight();
            cb=c.offset().top+ch;
  
            wt=w.scrollTop();
            wl=w.scrollLeft();
            wh=w.height();
            nf=(wt>t)&&(ch>bh)?true:false;
            p=!nf?'static':(wt+ah)>cb?'absolute':'fixed';
  
            if(p==='fixed'){
                b.css({
                    position:p,
                    top:'',
                    bottom:wh-ah-bm,
                    left:l-wl,
                    'margin-left':(pt===1)?0:''
                });
            }
            else if(p==='absolute'){
                b.css({
                    position:p,
                    top:cb-bh,
                    bottom:'',
                    left:(cp=='relative')?(pt===2)?l-d.offset().left:l-lc:l
                });
            }
            else{
                b.css({
                    position:p,
                    'margin-left':(pt===1)?'-' +bw+'px':''
                });
            }
        };
  
        r=function(){
            b.css({
                position:'static',
                'margin-left':(pt===1)?'-' +bw+'px':''
            });
            wl=w.scrollLeft();
            l=b.offset().left+0;
            lc=c.offset().left;
            if(p==='fixed'){
                b.css({
                    position:p,
                    left:l-wl,
                    'margin-left':(pt===1)?0:''
                });
            }
            else if(p==='absolute'){
                b.css({
                    position:p,
                    left:(cp=='relative')?(pt===2)?l-d.offset().left:l-lc+wl:l,
                    'margin-left':(pt===1)?0:''
                });
            }
        };
  
        w.on('load',s);
        w.on('scroll',s);
        w.on('resize',r);
    }
  
})(jQuery);
</script>




