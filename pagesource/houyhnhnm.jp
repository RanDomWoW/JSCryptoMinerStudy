$(document).ready(function()
{
    var $searchTrigger = $('[data-ic-class="search-trigger"]'),
        $searchInput = $('[data-ic-class="search-input"]'),
        $searchClear = $('[data-ic-class="search-clear"]');

    $searchTrigger.click(function()
    {
        var $this = $('[data-ic-class="search-trigger"]');
        $this.addClass('active');
        $searchInput.focus();
    });

    $searchInput.blur(function()
    {
      if($searchInput.val().length > 0)
      {
        return false;
      }
      else
      {
        $searchTrigger.removeClass('active');
      }
    });

    $searchClear.click(function()
    {
      $searchInput.val('');
    });

    $searchInput.focus(function()
    {
      $searchTrigger.addClass('active');
    });
});
</script>
<script src="http://www.houyhnhnm.jp/wp-content/themes/houyhnhnm/js/vendor/jquery.autopager-1.0.0.js"></script>
<script>
var maxpage = 0;

$('.module--type_loading, .autoload a, .autoload .page-numbers').css('display', 'none');
$('.autoload a.next').css('display', 'block');
$.autopager({
    content: '.module--type_autoWrap',// 読み込むコンテンツ
    link: '.autoload a.next', // 次ページへのリンク
    autoLoad: false,// スクロールの自動読込み解除
 
    start: function(current, next){
      $('.module--type_loading').css('display', 'block');
      $('.autoload a.next').css('display', 'none');
    },
 
    load: function(current, next){
        $('.module--type_loading').css('display', 'none');
        $('.autoload a.next').css('display', 'block');
		// getShareCounts($(this));
        var path = $(location).attr('pathname'),
            ua = navigator.userAgent;
        if (ua.search(/iPhone/) != -1 || ua.search(/iPod/) != -1 || ua.search(/Android/) != -1)
        {
          $('.module--type_autoWrap .col').removeClass('col').addClass('col');
        }
        if( current.page >= maxpage ){ //最後のページ
            $('.autoload a.next').hide(); //次ページのリンクを隠す
        }
        
          if (window.matchMedia('(max-width: 767px)').matches)
          {
            $('.content--category_whatsnew__body').removeClass('grid');
          }
    }
});
 
$('.autoload a.next').click(function(){
    $.autopager('load');
    return false;
});
</script>

