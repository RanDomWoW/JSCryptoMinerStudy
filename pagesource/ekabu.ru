.t-social-right {
    float: right;
    height: 40px;
    line-height: 40px;
}

.centered {overflow: hidden;}
.f-social-container {
    position: absolute;
    width: 50px;
    top: 10px;
    background-color: rgba(0,0,0,.5);
    border-radius: 5px;
    right: -50px;
    transition: right .28s ease;
}
.centered:hover .f-social-container {
  right: 10px;
}
.f-social-button:hover {
    opacity: .7;
}
.f-social-button {
    font-size: 18px;
    line-height: 50px;
    display: inline-block;
    box-sizing: border-box;
    height: 50px;
    padding-left: 0px;
    width: 50px;
    vertical-align: bottom;
    text-decoration: none;
    color: #666;
    border-radius: 4px;
    background: none no-repeat transparent;
    background-size: auto 32px;
    cursor: pointer;
    transition: opacity .28s ease;
}
