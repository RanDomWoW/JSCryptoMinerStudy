     <script>

        

         app.controller('MyCtrl', function ($scope, $http, $window) {
             $scope.Email = "";
             $scope.Mobile = "";

             function ValidateEmail(email) {
                 var expr = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
                 return expr.test(email);
             };

             $scope.OnClientClicked = function () {

                 if ($scope.EmailId == "" || $scope.EmailId == undefined || $scope.EmailId == null) {
                     $scope.divValidationMsg = true;
                     $scope.divSuccessMsg = false;
                     $scope.ValidationMsg = "Please Fill Email";
                 }
                 else if (!ValidateEmail($scope.EmailId)) {
                     $scope.divValidationMsg = true;
                     $scope.divSuccessMsg = false;
                     $scope.ValidationMsg = "Please Fill a Email Address.";
                 }
                 else if ($scope.Mobile == "" || $scope.Mobile == undefined || $scope.Mobile == null) {
                     $scope.divValidationMsg = true;
                     $scope.divSuccessMsg = false;
                     $scope.ValidationMsg = "Please Fill Mobile No.";
                 }
                 else if ($scope.Mobile.length < 10 || $scope.Mobile == 0000000000) {
                     $scope.divValidationMsg = true;
                     $scope.divSuccessMsg = false;
                     $scope.ValidationMsg = "Please Fill a valid Mobile No.";
                 }
                 else {
                     $window.open('PermotionThanks.aspx?Email=' + $scope.EmailId + '&Mobile=' + $scope.Mobile + '', '_top');
                 }
             }

         });

    </script>
