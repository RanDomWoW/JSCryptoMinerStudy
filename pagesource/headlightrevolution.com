    function openEmailModalWindow () {         
        $('#email-modal').delay(800).fadeIn(600, function() {
           $('#email-modal .modal-window').fadeIn(600);
        });
    }
    
    if ($.cookie('emailSubcribeModal') != 'closed') {
        openEmailModalWindow();
    };
     
    //
