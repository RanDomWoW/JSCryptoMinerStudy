<script>
	// $( '#search-action' ).slideDown();

	$( '#search-open' ).on( 'click', function(e) {
		$( '#search-input' ).val("");
      $( '.btn-group.btn-block' ).removeClass("open");
		$( '#search' ).toggleClass( 'open' ).find( 'input' ).focus();
		$( '#search-open i' ).toggleClass( 'fa-search' ).toggleClass( 'fa-times' );
		$( '#search-action i' ).toggleClass( 'fa-search' ).toggleClass( 'fa-times' );
		$( this ).css( 'display', 28 );

		e.stopPropagation();
	});

	$( '#search input' ).click( function(e) {
		e.stopPropagation();
	});

   $( '#resp-search-open' ).on( 'click', function(e) {
      $( '.btn-group.btn-block' ).removeClass("open");
      $( '#resp-search-input' ).val("");
		$( '#resp-search-input' ).toggleClass( 'open' ).focus();
      $( '#resp-search-open i' ).toggleClass( 'fa-search' ).toggleClass( 'fa-times' );
		$( '#resp-search-action i' ).toggleClass( 'fa-search' ).toggleClass( 'fa-times' );
      $( this ).css( 'display', 28 );
		e.stopPropagation();
	});

	$( document ).click( function() {
		$( '#search' ).removeClass( 'open' );
		$( '#search-open i' ).addClass( 'fa-search' ).removeClass( 'fa-times' );
		$( '#search-action i' ).addClass( 'fa-search' ).removeClass( 'fa-times' );
		$( '#search input' ).attr( 'value', '' );
	});

   $('html').click(function(e) {
		if ( ! $(e.target).is('input#resp-search-input') && ! $(e.target).is('#search') && ! $(e.target).is('.fa.fa-search') && ! $(e.target).is('button')) {
			$( '#resp-search-input' ).removeClass( 'open');
         $( '#resp-search-open i' ).addClass( 'fa-search' ).removeClass( 'fa-times' );
   		$( '#resp-search-action i' ).addClass( 'fa-search' ).removeClass( 'fa-times' );
		}
	});

	if ( $( '#search input' ).attr( 'value' ).length > 0 )
		$( '#search input' ).attr( 'value', '' );
</script>
<script>
	$(document).ready(function () {
  /*Handle Escape Key*/
  $(document).keyup(function(e) {
      var keycode = (e.keyCode ? e.keyCode : e.which);
      if (keycode == 27) {
        if ($('.search-box  input[type="search"]').length > 0) {
          var q = $.trim($('.search-box input[type="search"]').val());
          if (q != undefined && q == '') {
            $('[data-toggle="popover"]').popover('hide');
          }
        }
      }
  });

  /*Configure Search Popover*/
  $('.search-box-trigger[data-toggle="popover"]').popover(
  {
    trigger: 'manual',
    container: 'body',
    html: true,
    template: '<div class="popover search-popover" role="tooltip"><div class="arrow"></div><h3 class="popover-title"></h3><div class="popover-content"></div></div>',
    content: function () {
        return $('#search-box-container').html();
    }
  }).click(function (e) {
    e.preventDefault();
    var self = $(this);

    self.popover('toggle');
    $('.popover').css({'left': '0px', 'width': '100%'});
    $('.search-box input[type="search"]').focus();

    /* Event handlers*/
    $('.search-box input[type="search"]').focusin(function() {
        $('.search-box .search-close').show();
    });

    $('.search-box input[type="search"]').keypress(function(e) {
        var keycode = (e.keyCode ? e.keyCode : e.which);
        if (keycode == 13) {
          var q = $.trim($(this).val());
          if (q != undefined && q != '') {
            $('.search-box input[type="search"]').blur();
            self.popover('toggle');
            $('#search-result').html('You searched: <br><b>' + q + '</b>');
          }
        }
    });

    $('.search-box .search-close').click(function() {
        self.popover('toggle');
    });

  });
});
</script>

