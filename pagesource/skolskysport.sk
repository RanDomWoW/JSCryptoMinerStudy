<script type="text/javascript">
//<![CDATA[

$(document).ready(function(){

	$("#CompanyTitle").keyup( function() {
	    $("#CompanyTitle").autocomplete({
	        source: function(request, response) {
	            $.ajax({
	                    url: "CompanyAutocomplete.cfm",
		                contentType: "application/json; charset=utf-8",
		                dataType: "json",
	                    data: request,
	                    success: function( data ) {
							items = new Array();
	                        for(itemData in data["DATA"]) 
								{ 
									 var ItemTMP = new Array();
									 ItemTMP = data["DATA"][itemData];
									 items.push( ItemTMP[3] );
								}
	                        response( items );

	                    }
	            });
	        } // ajax end
			
	    }, { 
			minLength: 2, 
			autoFill: false, 	
				select: function(event, ui) {
					$('#CompanyID').val(ui.item.CompanyID);
					$('#CompanyTitle').val(ui.item.label);
					
					return false;
				}
		});
	});

});



//]]>
</script>



