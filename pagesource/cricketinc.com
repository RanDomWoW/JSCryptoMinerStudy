<script>
    $(function () {
        $(".showLeague").on('click', function (evt) {
            evt.preventDefault();
            //    var logval=$('#ctl00_ctl00_PlaceHolderPageContent_PlaceHolderSubPageContent_hdnmUserCode').val();    
            //    if(logval=="")
            //    {
            //     $('#loginpop').modal('show');
            //     return false;
            //    }

            var btn = $(this);
            var aa = ($(this).attr('rel'));
            var btntext = $(this).html();
            if ($(this).html() == 'Hide') {
                $(this).html('Show')
                $("#div_P2P_" + aa).hide();
                return false;
            }
            else if ($(this).html() == 'Show') {
                $(this).html('Hide')
                $("#div_P2P_" + aa).show();
                return false;
            }
            $.blockUI({ message: '<br><br><img  src="/images/Loading.gif" />&nbsp;&nbsp;<b class="loading"><font color="#888888" size="+1" >Loading Leagues...</font></b><br><br>' });
            var pUserCode = $('#ctl00_ctl00_PlaceHolderHeader_Header1_UClLI_hdnmUserCode').val();
            $.ajax({
                type: "POST",
                url: "../HttpHandlers/LoadP2PLeague.ashx?strFantasyMatchCode=" + aa + "&strUSerTeamCode=" + pUserCode + "&MethodName=LoadLeague",
                dataType: 'html',
                async: true,
                success: function (data) {
                    $(".divP2PLeauge").empty();
                    $(".leaguediv").removeClass('fullwidth');
                    $('.showLeague').show();
                    $('.showFutureMLeague').show();
                    //$("#div_P2P_" + aa).parent().parent().addClass('fullwidth');
                    $("#div_P2P_" + aa).empty();
                    $("#div_P2P_" + aa).append(data);
                    $("#div_P2P_" + aa).show();
                    $('.showLeague').each(function () {
                        var txt = $(this).attr('btntext');
                        $(this).html(txt);
                    });
                    $(btn).hide();
                    $.unblockUI();
                    //SortDropdown(); 
                    //CustomFilter(); 
                    // ReplaceRsToSymbol();                  
                },
                error: function (requestObject, error, errorThrown) {
                    $.unblockUI();
                    $(".divP2PLeauge").empty();
                    $(".leaguediv").removeClass('fullwidth');
                    $(btn).show();
                    alert('error');

                }
            });
        });
    });
    </script>
        	
