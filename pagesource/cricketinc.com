//var alpha_num_char = /^[a-zA-Z0-9\'\s\-\,\.\(\)]+$/;
var alpha_num_char = /^[a-zA-Z0-9\@\-\_\.]+$/;

    if ((pTeamName=='')||(pTeamName==null))
    { pErrMsg = pErrMsg +'Please enter a team name.'+'\n'; $(".login-popup-inner input:text").eq(0).focus();  blnErr=false; }
    else
    {   var pTeamNameVal = pTeamName;
       if (/\'/.test(pTeamNameVal)) {
            pErrMsg = pErrMsg + 'No apostrophes allowed in team name.'+'\n';blnErr = false;
            }
//        if(!alpha_num_char.test(pTeamNameVal))
//	    {   alert('Invalid characters in login name\n'); pTeamName.focus(); blnErr = false;    }
    }
     
    if ((pPassword=='')||(pPassword==null))
    { pErrMsg = pErrMsg +'Please enter a Password.'+'\n'; $(".login-popup-inner input:password").eq(1).focus();   blnErr=false; }
    else {
        if (/\'/.test(pPassword)) {
            pErrMsg = pErrMsg + 'No apostrophes allowed in password.'+'\n'; blnErr = false;
            }
    }
    if (blnErr==false)
    {
    alert(pErrMsg);
        return false;
    }
    else
    {       
        return true;
    }
}
