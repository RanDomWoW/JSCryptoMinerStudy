<script>
    $(document).ready(function () {
        var hdnstatus = $("#ctl00_ctl00_PlaceHolderHeader_Header1_UClNL_hdnpaystatus").val();
        if (hdnstatus == "paysuccess") {
            $("#depositstatussuccess").modal({
                backdrop: 'static',
                keyboard: false
            });
            $('#depositstatussuccess').modal('show');
        } else if (hdnstatus == "payfail") {
            $("#failtext").html('sorry your deposit was not successful.');
            $('#depositstatusfail').modal('show');
        } else if (hdnstatus == "pending" || hdnstatus == "open") {
            $("#failtext").html('sorry your deposit is ' + hdnstatus + ' from PayTm.');
            $('#depositstatusfail').modal('show');
        }

        $("#depositamount").on('click', function () {
            $('#depositstatusfail').modal('hide');
            $('.selectdeposit').show();
            $("#depositpopup").modal('show');
        });

        $("#depositmethod").on('click', function () {
            var d_amt = $("#ctl00_ctl00_PlaceHolderHeader_Header1_UClLI_UserDeposit1_hndPrizefee").val();
            if (d_amt != 'Undefined') {
                document.getElementById('ctl00_ctl00_PlaceHolderHeader_Header1_UClLI_UserDeposit1_lblpayamount').innerHTML = '₹' + d_amt;
            }
            $('#depositstatusfail').modal('hide');
            $('.selectdeposit').hide();
            $("#amountdeposit").show();
            $("#depositpopup").modal('show');
        });

        $(".clsclose").click(function () {
            $("#depositstatussuccess").modal('hide');            
            var FMcode = $('#ctl00_ctl00_PlaceHolderHeader_Header1_UClNL_hdnpay_FmCode').val();
            if (parseInt(FMcode) > 0) {
                var aa = FMcode;
                $('.showLeague').each(function () {
                    var txtrel = $(this).attr('rel');
                    if (txtrel == FMcode) {
                        $(this).hide();
                    }
                });
                var btntext = "Play";
                if ($(FMcode).html() == 'Hide') {
                    $(FMcode).html('Show')
                    $("#div_P2P_" + aa).hide();
                    return false;
                }
                else if ($(FMcode).html() == 'Show') {
                    $(FMcode).html('Hide')
                    $("#div_P2P_" + aa).show();
                    return false;
                }
                var pUserCode = $('#ctl00_ctl00_PlaceHolderHeader_Header1_UClLI_UserDeposit1_hdnmUserCode').val();
                //Loading P2P league by Fm code
                var tpage_name = $('#ctl00_ctl00_PlaceHolderPageContent_PlaceHolderSubPageContent_htnFTPage').val();
                if (tpage_name == '' || tpage_name == undefined) {
                    tpage_name = $('#ctl00_ctl00_PlaceHolderPageContent_PlaceHolderSubPageContent_uc_FMScoreCard_FmDetail_hdnTPage_Name').val();
                }
                if (tpage_name == '' || tpage_name == undefined) {
                    tpage_name = $('#ctl00_ctl00_PlaceHolderPageContent_PlaceHolderSubPageContent_uc_teamSelection_FmDetail_hdnTPage_Name').val();
                }
                var pathname = window.location.pathname;
                if (pathname.indexOf('index') >= 0) {
                    BindP2PLeague(aa, pUserCode, '', tpage_name, "");
                } else {
                    OpenConfirmPOpupAfterSucessDeposit();
                }
            }
        });
    });

    
</script>
