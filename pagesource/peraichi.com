<script>

var _chaq = _chaq || [];

var __jQuery = false;
if(typeof jQuery !== 'undefined') {
    __jQuery = jQuery;
}

if(!!!__jQuery) {
    for(var obj in document) {
        if ( obj.toString().match(/^jQuery/)) {
            __jQuery = obj;
            break;
        }
    }
}

(function($){
    if(localStorage && localStorage.status) {
        localStorage.clear();
    }
    _chaq['_accountID'] = 2781;
    (function(D,s){
        var ca = D.createElement(s)
        ,ss = D.getElementsByTagName(s)[0];
        ca.type = 'text/javascript';
        ca.async = !0;
        ca.setAttribute('charset','utf-8');
        var sr = 'https://v1.chamo-chat.com/chamovps.js';
        ca.src = sr + '?' + parseInt((new Date)/60000);
        ss.parentNode.insertBefore(ca, ss);
    })(document,'script');

    var MENU = 'ご訪問ありがとうございます。お聞きになりたいことはございますか？[+]使い方を知りたい[+]どんなページが作れるのか知りたい[+]集客の仕方を学びたい[+]プラン・料金[+]設定や操作で困っている[+]迷惑行為をするなど悪質なページを通報したい[+]上記いずれでもない';
    var BACK_MENU = '[+]メニューに戻る';

    _chaq.onChamoLoad = function() {
        $chamo.api.user.set({
            visitorName       : '',
            visitorEmail      : '',
            visitorCustomerid : ''
        });
        $chamo.api.event.onUserSend(function(mes) {
            switch(mes) {
                case '直接問い合わせする':
                    // 送信ボタンの表示
                    $("#chamo_style").remove();
                    break;
                                case 'プラン変更・解約':
                    $chamo.api.operator.sendMessage('プランの変更・解約についてはこちらをご覧ください。\nhttps://peraichi.com/support/category/contracts' + BACK_MENU);
                    break;
                                case '使い方を知りたい':
                    $chamo.api.operator.sendMessage('▼こちらに情報がございます！\nhttps://peraichi.com/support:blank\nリンクの貼り方やドメイン設定も。\n\n▼初めての方はこちらをどうぞ！\nhttps://peraichi.com/pages/tutorial:blank' + BACK_MENU);
                    break;
                                case 'どんなページが作れるのか知りたい':
                    $chamo.api.operator.sendMessage('ぺライチではこのようなページを作成できます。\nぜひ参考にしてください！\n\nペライチ｜制作事例\nhttps://peraichi.com/pages/showcase:blank' + BACK_MENU);
                    break;
                                case '集客の仕方を学びたい':
                    $chamo.api.operator.sendMessage('ペライチの活用方法・成功パターンを、動画で何度でも学べる講座『ペラ子屋ONLINE』がございます。\n\n短期間の利用も可能ですので、是非お気軽にご活用ください。\nhttps://perakoya.com:blank' + BACK_MENU);
                    break;
                                case '上記いずれでもない':
                    $chamo.api.operator.sendMessage('お手数をおかけしますが、こちらのフォームから直接ご連絡ください。\n\n[+]直接問い合わせする' + BACK_MENU);
                    break;
                                case 'エンタープライズプランの詳細':
                    $chamo.api.operator.sendMessage('  エンタープライズプランは、公開ページ数やPV数、機能などに応じてカスタマイズ可能なプランです。\n\n詳細は下記をご覧ください。\nhttps://peraichi.com/landing_pages/view/contact-enterprise:blank\n\n[+]直接問い合わせする' + BACK_MENU);
                    break;
                                case '公開ページ数を増やす方法':
                    $chamo.api.operator.sendMessage('「有料プラン」や「ページ追加オプション」にお申込みいただくと、公開ページ上限数を増やすことができます。\n\n詳細はこちらをご覧くださいませ。\nhttps://peraichi.com/pages/pricing:blank\n\n[+]直接問い合わせする' + BACK_MENU);
                    break;
                                case 'プランの種類と内容':
                    $chamo.api.operator.sendMessage('料金プランについてはこちらをご覧くださいませ。\nhttps://peraichi.com/pages/pricing:blank' + BACK_MENU);
                    break;
                                case 'プラン・料金':
                    $chamo.api.operator.sendMessage('どのような内容でしょうか？\n\n[+]プランの種類と内容\n[+]公開ページ数を増やす方法\n[+]エンタープライズプランの詳細\n[+]プラン変更・解約\n[+]上記いずれでもない' + BACK_MENU);
                    break;
                                case '設定や操作で困っている':
                    $chamo.api.operator.sendMessage('具体的にどのようなことでお困りでしょうか？\n\n[+]ログインできない\n[+]メールアドレスの認証メールが届かない\n[+]画像がアップロードできない\n[+]操作がエラーになる\n[+]ドメインの設定がうまくいかない\n[+]OGP設定がうまくいかない\n[+]検索結果にページが表示されない\n[+]いずれでもない' + BACK_MENU);
                    break;
                                case 'いずれでもない':
                    $chamo.api.operator.sendMessage('大変申し訳ございません。一度こちらをご確認ください。\n\nペライチ｜ヘルプ\nhttps://peraichi.com/support:blank\n\n[+]直接問い合わせする' + BACK_MENU);
                    break;
                                case '検索結果にページが表示されない':
                    $chamo.api.operator.sendMessage('ご不便おかけしております。一度こちらをご確認いただけますでしょうか。\nhttps://peraichi.com/support/entry/149:blank\n\n[+]直接問い合わせする' + BACK_MENU);
                    break;
                                case 'OGP設定がうまくいかない':
                    $chamo.api.operator.sendMessage('ご不便おかけしております。一度こちらをご確認いただけますでしょうか。\nhttps://peraichi.com/support/entry/54:blank\n\n[+]直接問い合わせする' + BACK_MENU);
                    break;
                                case 'ドメインの設定がうまくいかない':
                    $chamo.api.operator.sendMessage('ご不便おかけしております。一度こちらをご確認いただけますでしょうか。\nhttps://peraichi.com/support/category/domain:blank\n\n[+]直接問い合わせする' + BACK_MENU);
                    break;
                                case 'ログインできない':
                    $chamo.api.operator.sendMessage('ご不便おかけしております。一度こちらをご確認いただけますでしょうか。\nhttps://peraichi.com/support/entry/25:blank\n\n[+]直接問い合わせする' + BACK_MENU);
                    break;
                                case '操作がエラーになる':
                    $chamo.api.operator.sendMessage('ご不便おかけしております。一度こちらをご確認いただけますでしょうか。\nhttps://peraichi.com/support/entry/24:blank\n\n[+]直接問い合わせする' + BACK_MENU);
                    break;
                                case 'メールアドレスの認証メールが届かない':
                    $chamo.api.operator.sendMessage('ご不便おかけしております。一度こちらをご確認いただけますでしょうか。\nhttps://peraiti.com/support/entry/245:blank\n\n[+]直接問い合わせする' + BACK_MENU);
                    break;
                                case '迷惑行為をするなど悪質なページを通報したい':
                    $chamo.api.operator.sendMessage('こちらからご通報ください　https://peraichi.com/landing_pages/view/fuseiriyou:blank' + BACK_MENU);
                    break;
                                case '上記いずれでもない':
                    $chamo.api.operator.sendMessage('こちらに様々な情報がのっています！\n\nペライチ｜ヘルプ\nhttps://peraichi.com/support:blank' + BACK_MENU);
                    break;
                                case '画像がアップロードできない':
                    $chamo.api.operator.sendMessage('ご不便おかけしております。一度こちらをご確認ください。\nhttps://peraichi.com/support/entry/212:blank\n\n[+]直接問い合わせする' + BACK_MENU);
                    break;
                 
                case 'メニューに戻る':
                    $chamo.api.operator.sendMessage(MENU);
                    break;
            }
        },'myEvent');
        initChamo();

        function initChamo(){
            $chamo.api.widget.hide();
            // 最初にメッセージ送信して最小化しようとすると二重送信されるバグがあるので防止
            setTimeout(function(){$chamo.api.operator.sendMessage(MENU);}, 3000);
            setTimeout(function(){
                $chamo.api.widget.minimize();
                // 料金表ページでは最初からポップアップさせる
                // ただしスマホUIのブレークポイント以下であれば適用しない
                var SP_BREAK_POINT = 768;
                if (location.href.match('/pages/pricing') && SP_BREAK_POINT < $(window).width()) {
                    $chamo.api.widget.maximize();
                }
            },3200);
        }
    };

})(__jQuery);
