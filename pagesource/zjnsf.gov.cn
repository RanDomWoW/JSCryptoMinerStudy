    $(function () {
      $('#tbNews').jdNewsScroll({ divWidth: 355, divHeight: 300, fontSize: '9pt', lineHeight: '150%', liPadding: '4px 10px' });
    });
    var _irW = 380;
    var _irH = 250;
    var _irBig, _irSmall
    var _irTitle = $("#h2title");
    var _irCurId = 0;
    var _irPic = $("<img />").width(_irW).height(_irH).css("border", "none");
    var _irPicLink = $("<a target='_blank'></a>").append(_irPic);
    var _irImg = [];
    var _irSmallImg = [];
    var _irSmallArr = [];
    var _irAutoTimer
    function FocusImg() {
      _irBig = $("#con_1").width(_irW).height(_irH).css("overflow", "hidden");
      _irSmall = $("#con_2");
      if (_irImg.length == 0) return false;
      for (var i = 0; i < _irImg.length; i++) {
        var slspan = $("<div></div>").addClass("small_div");
        var slimglink = $("<a target='_blank'></a>").attr("href", _irImg[i].url);
        slimglink.append($("<img />").attr("src", _irImg[i].bigimg));
        slimglink.append($("<span></span>").html(_irImg[i].title));
        slspan.append(slimglink);
        _irSmallArr.push(slspan);
        (function () {
          var itemid = i;
          slspan.onmouseover = function () {
            t.doPic(itemid);
            t.curId = itemid;
            clearInterval(_irAutoTimer);
            t.autoPlay();
          }
        })();
        _irSmall.append(slspan);
      }
      _irBig.append($("<div></div>").append(_irPicLink));
      _irDoPic(0);
      _irAutoTimer = setInterval(function () {
        _irCurId++;
        if (_irCurId >= _irImg.length) _irCurId = 0;
        _irDoPic(_irCurId);
      }, 6000);
    }
    function _irDoPic(id) {
      //try { clearInterval(_irAutoTimer) } catch (e) { };
      try {
        if (document.all) {
          _irPic.get(0).filters.revealTrans.stop();
          _irPic.get(0).filters.revealTrans.Transition = 23;
          _irPic.get(0).filters.revealTrans.apply();
          _irPic.attr("src", _irImg[id].bigimg);
          _irPic.get(0).filters.revealTrans.play();
        } else
          _irPic.attr("src", _irImg[id].bigimg);
      } catch (e) { };
      _irTitle.html(_irImg[id].title);
      _irPicLink.attr("href", _irImg[id].url);
      for (i = 0; i < _irSmallArr.length; i++) {
        _irSmallArr[i].removeClass("selected");
      }
      _irSmallArr[id].addClass("selected");
    }
  </script>

