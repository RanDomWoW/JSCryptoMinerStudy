<script type="text/javascript">
    var fbTools;

    //  FB SDK as a Singleton
    //  Usage :
    //  var myObject = FBSDK.getInstance();
    //
    //  returns a new instance if none exists, or the existing instance
    var FBSDK = (function(w, $, undefined){
        var _friends = [],
            _instance,
            _scopeFacebook = 'public_profile,email,user_birthday',
            _scopeFacebookList = _scopeFacebook.split(','),
            _permissions = [];

        function init() { // Singleton
            FB.init({
                appId                   : '311570458934147',
                status                  : true,
                cookie                  : true,
                xfbml                   : true,
                oauth                   : true,
                frictionlessRequests    : true,
                version                 : 'v2.9'
            });

            
            return {
                checkLoginStatus: function() {
                    return getLoginStatus();
                },
                login: function() {
                    return doLogin();
                },
                getFriends: function() {
                    return _friends;
                },
                share: function($fb_link){
                    fb_share($fb_link);
                },
                getPermissions: function() {
                    return rerequestPermissions(_permissions);
                }
            };


        };

        function getLoginStatus() {
            $(w).trigger('fb.login.start');
            FB.getLoginStatus(function(response) {
                if (response.status === 'connected') {
                    FB.api('/me/permissions', function(perms_response) {
                        var scopesPermissions =  [];

                        $.each(perms_response.data, function(k, v) {
                            if(v.status === 'declined') {
                                scopesPermissions[v.permission] = false;
                            }
                            else {
                                scopesPermissions[v.permission] = true;
                            }
                        });

                        checkPermissions(scopesPermissions);

                    });
                } else if (response.status === 'not_authorized') {
                    doLogin();
                } else {
                    doLogin();
                }
            });
        };

        function checkPermissions(permissions) {  //Log in if all ƒpermissions are granted.
            var permitted = true;
            for(var scope in permissions) {
                if(!permissions[scope]) {
                    permitted = false;
                }
            }

            if(permitted) {
                handleUrlStuff();
            } else {
                _permissions = permissions;
                showFacebookPermsModal();
            }
        };

        function checkScopes(response) { //Get scopes and check all permissions
            var grantedScopes = response.authResponse.grantedScopes.split(','),
                scopesPermissions = {};

            for(var s in _scopeFacebookList) {
                scopesPermissions[_scopeFacebookList[s]] = $.inArray(_scopeFacebookList[s], grantedScopes) >= 0 ? true : false;
            }

            checkPermissions(scopesPermissions);
        };

        function rerequestPermissions(permissions) { //Rerequest refused permissions
            var rerequestScope = [];
            for(var p in permissions) {
                if(!permissions[p]) {
                    rerequestScope.push(p);
                }
            }
            rerequestScope = rerequestScope.join();

             FB.login(function(response) {
               checkScopes(response);
             },
             {
                scope: rerequestScope,
                return_scopes: true,
                auth_type: 'rerequest',
            });
        };

        function doLogin() {
            FB.login(function(response) {
                if (response.authResponse) {
                    checkScopes(response);
                } else {
                    whenLoginFailed();
                }
            }, { scope : _scopeFacebook, return_scopes: true });

        };

        function whenLoginFailed() {
            $(w).trigger('fb.login.failed');

                            $('#popup-register').modal('show');
                    };

        function handleUrlStuff() {
                        w.location = 'https://www.adoteumcara.com.br/facebookHandler/connect_handler';
                    };

        function fb_share($fb_link){
            FB.ui({
                method: 'share',
                href: $fb_link.data('href'),
            }, function(response){});
        };

        return {
            // Get the Singleton instance if one exists or create one if none
            getInstance: function () {
                if ( !_instance ) {
                    _instance = init();
                }
                return _instance;
            }
        };
    })(window, jQuery); // FBSDK

    // Charge le SDK Facebook
    // API 2.2
    (function(d, s, id){
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) {return;}
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/pt_BR/sdk.js";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));

    // Quand le SDK FB est chargé, initialise FB et place les events
    window.fbAsyncInit = function() {
        fbTools = FBSDK.getInstance();

        $('.fb-login').click(function(e){
            e.preventDefault();
            fbTools.checkLoginStatus();
        });

        $('.aum_fb_share_button').click(function(e){
            e.preventDefault();
            fbTools.share($(this));
        });
    };
</script>
