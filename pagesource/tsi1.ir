function search_products()
{
	val = $('.clist_view_search_input').val();
	href ='/جستجو';
	href_arr = href.split("?");
	if(href_arr.length>1)
	{
		final_href = "";
		href_arr1 = href_arr[1].split("&");
		exist =0;
		href_arr1.forEach(function(href_get) {
			href_get_arr = href_get.split("=");
			if(href_get_arr[0]=="Product[title]")
			{
				exist = 1;
				href_get_arr[1]=val;
			}
			final_href += href_get_arr[0]+'='+href_get_arr[1]+'&';
		});
		if(exist==0)
			final_href += "Product[title]="+val;
		else
			final_href = final_href.substring(0, final_href.length - 1);
	}
	else
		final_href = "Product[title]="+val;
	if(href_arr[0].substr(href_arr[0].length - 1)=='/')
		href_arr[0] = href_arr[0].substring(0, href_arr[0].length - 1);
	href = href_arr[0]+'?'+final_href;
	window.location.href=href;
}
