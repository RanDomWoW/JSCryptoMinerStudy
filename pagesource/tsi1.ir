	$("#newsletter_validate_div").slideUp();

	email = $("#newsletter_email_field").val();

	fullname = $("#newsletter_fullname_field").val();

	tel = $("#newsletter_tel_field").val();

	if (isNumeric(tel))

	{

		if(isValidEmailAddress(email))

		{

			$("#newsletter_validate_div").html('<div style="width:16px;margin:0 auto;"><img src="/images/loading.gif"></div>');

			$("#newsletter_validate_div").slideDown();

			$.post(

				'/newsletter/newsletter_default/register_email',

				{email:email,fullname:fullname,tel:tel},

				function(data)

				{

					if(data=='1')

					{

						$("#newsletter_validate_div").removeClass("text-danger");

						$("#newsletter_validate_div").addClass("text-success");

						$("#newsletter_validate_div").html("مشخصات شما با موفقیت در خبر نامه ثبت گردید");

						$("#newsletter_email_field").val("");

						$("#newsletter_fullname_field").val("");

						$("#newsletter_tel_field").val("");

					}

					else if(data=='2')

					{

						$("#newsletter_validate_div").addClass("text-danger");

						$("#newsletter_validate_div").removeClass("text-success");

						$("#newsletter_validate_div").html("متاسفانه مشکلی در ثبت اطلاعات مورد نظر شما به وجود آمده است.<br>لطفا دقایقی دیگر مجددا تلاش کنید.<br>اگر مجددا این پیام را مشاهده کردید لطفا با مدیر سایت تماس بگیرید.");



					}

					else if(data=='3')

					{

						$("#newsletter_validate_div").addClass("text-danger");

						$("#newsletter_validate_div").removeClass("text-success");

						$("#newsletter_validate_div").html("شما قبلا در خبرنامه ثبت نام کرده اید");

					}

				}

			);

		} 

		else

		{

			$("#newsletter_validate_div").addClass("text-danger");

			$("#newsletter_validate_div").removeClass("text-success");

			$("#newsletter_validate_div").html("ایمیل وارد شده غیر مجاز است.<br/>");

			$("#newsletter_validate_div").slideDown();

		}

	}

	else

	{

		$("#newsletter_validate_div").addClass("text-danger");

		$("#newsletter_validate_div").removeClass("text-success");

		$("#newsletter_validate_div").html("شماره تلفن باید به صورت عددی وارد شود.");

		$("#newsletter_validate_div").slideDown();

	}

}

function isValidEmailAddress(emailAddress)

{

    var pattern = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);

    return pattern.test(emailAddress);

};

function isNumeric(telephone)

{

	var pattern=new RegExp(/^\d+/);

	return pattern.test(telephone);

}

