<script>
$(document).ready(function(){

	// hide #back-top first
	$("#back-top").hide();
	
	// fade in #back-top
	$(function () {
		$(window).scroll(function () {
			if ($(this).scrollTop() > 100) {$('#back-top').fadeIn();} else {$('#back-top').fadeOut();}
		});

		// scroll body to 0px on click
		$('#back-top a').click(function () {
			$('body,html').animate({
				scrollTop: 0
			}, 800);
			return false;
		});
	});

});
</script>

<script type="text/javascript">
$(document).ready(function() {
	var hideWidth = '-308px'; //width that will be hidden
	var collapsibleEl = $('.collapsible'); //collapsible element
	var buttonEl =  $(".collapsible button"); //button inside element

	collapsibleEl.css({'margin-left': hideWidth}); //on page load we'll move and hide part of elements
	
	$(buttonEl).click(function()
	{
		var curwidth = $(this).parent().offset(); //get offset value of the element
		if(curwidth.left>0) //compare margin-left value
		{
			//animate margin-left value to -490px
			$(this).parent().animate({marginLeft: hideWidth}, 300 );
			$(this).html('&raquo;'); //change text of button
		}else{
			//animate margin-left value 0px
			$(this).parent().animate({marginLeft: "0"}, 300 );	
			$(this).html('&laquo;'); //change text of button
		}
	});

});


$(document).click(function(event) {
			$('div').click(function() {
				var theClass = this.className;  // "this" is the element clicked
				var hideWidth = '-308px'; //width that will be hidden
				var collapsibleEl = $('.collapsible'); //collapsible element
				var buttonEl =  $(".collapsible button"); //button inside element
			
				if(theClass=="collapsible" || theClass=="clr")
				{
					return false();
				}
				else if(theClass=='')
				{
					return false();
				}
				else
				{
					$(".collapsible button").parent().animate({marginLeft: hideWidth}, 300 );
					$(".collapsible button").html('&laquo;'); //change text of button
				}

			});
				
});

$("ul#ticker01").liScroll({travelocity: 0.05});

</script>



