<script type="text/javascript">
$(document).ready(function(){
	// slide
	var currentSlide = 1;
	var slideNumber = 0;
	var maxSlideNumber = 3;
	function doSlide(slideNumberIndex,slideCommand){
		if(slideNumber == slideNumberIndex){
			var previousSlide = currentSlide;
			if(slideCommand == "next"){
				if(currentSlide < maxSlideNumber){
					currentSlide++;
				}else{
					currentSlide = 1;
				}
			}else if(slideCommand == "pre"){
				if(currentSlide > 1){
					currentSlide--;
				}else{
					currentSlide = maxSlideNumber;
				}
			}else{
				currentSlide = slideCommand;
			}
			$(".slide-navigator-item").removeClass("active");
			$("#slide-navigator-item_"+currentSlide).addClass("active");
			$("#slide-item_"+previousSlide).fadeOut(function(){
				$("#slide-item_"+currentSlide).find("h2").hide();
				$("#slide-item_"+currentSlide).find("p").hide();
				$("#slide-item_"+currentSlide).find("a").hide();
				$("#slide-item_"+currentSlide).show();
				$("#slide-item_"+currentSlide).find("h2").fadeIn("slow",function(){
					$("#slide-item_"+currentSlide).find("p").fadeIn("slow",function(){
						$("#slide-item_"+currentSlide).find("a").fadeIn("slow");
					});
				});
				
			});
			setTimeout(function() {
				doSlide(slideNumberIndex,"next");
			}, 10000);
		}
	}
	doSlide(0,1);
		
	$(".slide-navigator-item").click(function(){
		var thisItemId = $(this).attr("id");
		thisItemId = thisItemId.replace("slide-navigator-item_","");
		slideNumber++;
		doSlide(slideNumber,thisItemId);
	});
		
	$("#next-button").click(function(){
		slideNumber++;
		doSlide(slideNumber,"next");
	});
		
	$("#pre-button").click(function(){
		slideNumber++;
		doSlide(slideNumber,"pre");
	});
});
</script>
