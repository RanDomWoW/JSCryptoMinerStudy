		<script type="text/javascript">
		jQuery(document).ready(function($){
			$('#insider-form').submit(function(event) {

		    	event.preventDefault();

		    	if($('#insider-form input[name="privacy"]').is(':checked')) {
			        if (isValidEmailAddress($('#insider-form input[name=email]').val())) {

			        	var successText = '<div class="thank-you col-md-12"><h3>Almost There!</h3><br/>We have sent you an email with a link to verify your account and become a fully-fledged member.</div>';
			        	var failText    = '<div class="thank-you">Something went wrong! Please try again.</div>';

			            $.ajax({
			                url: '/sites/all/modules/_custom/insider/mc_subs.php',
			                data: $('#insider-form').serialize(),
			                type: 'POST',
			                success: function(msg) {
			                    if (msg == "success") {
			                        $("footer div.form").empty();
			                        $("footer div.form").append().html(successText);

			                    } else {
			                        $("footer div.form").append().html(failText);
			                    }
			                }
			            });
			        } else {
			            $('footer div.form input[name=email]').addClass('error');
			            $('footer div.form p.error-message').show();
			            
			        }
		        } else {
			        $('footer div.form p.error-checkbox').show();
		        }

		        return false;
		    });

		    function isValidEmailAddress(emailAddress) {
			    var pattern = /^([a-z\d!#$%&'*+\-\/=?^_`{|}~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]+(\.[a-z\d!#$%&'*+\-\/=?^_`{|}~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]+)*|"((([ \t]*\r\n)?[ \t]+)?([\x01-\x08\x0b\x0c\x0e-\x1f\x7f\x21\x23-\x5b\x5d-\x7e\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]|\\[\x01-\x09\x0b\x0c\x0d-\x7f\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))*(([ \t]*\r\n)?[ \t]+)?")@(([a-z\d\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]|[a-z\d\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF][a-z\d\-._~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]*[a-z\d\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])\.)+([a-z\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]|[a-z\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF][a-z\d\-._~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]*[a-z\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])\.?$/i;
			    return pattern.test(emailAddress);
					};

				$(window).scroll(function() {              
			      	$("#sticky-footer").toggle($(document).scrollTop() > 100);
			    });

				$(window).scroll(function() {
				   if($(window).scrollTop() + $(window).height() == $(document).height()) {
				       $("#sticky-footer").css('display', 'none');
				   }
				});

				$('#sidr').show();
			});
