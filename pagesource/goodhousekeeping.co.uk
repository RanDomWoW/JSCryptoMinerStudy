    <script type="text/javascript">
        function fixCookTimes(){
            if($('#cookTime').length>0) $('#cookTime').text( convertToHours($('#cookTime').text()));
            if($('#prepTime').length>0) $('#prepTime').text( convertToHours($('#prepTime').text()));
            function convertToHours(mins){
                var h=Math.floor(parseInt(mins)/60);
                var m=parseInt(mins)%60;var time_string="";
                if(h!==0){if(h==1){time_string+=h+" hr ";}else{time_string+=h+" hrs ";}}
                if(m!==0){if(m==1){time_string+=m+" min";}else{time_string+=m+" mins";}}
                return time_string;
            }
        }
    </script>

