$(window).on('scroll resize load click',function() {
    var wH = $(window).scrollTop();
    var wH = $(window).scrollTop();
    var wl = $(window).scrollLeft();
    var wid = $(window).width();
	var servise_H=wid/4;
    var wd = wid-1100;
    var wd = wd/2;
	
	$(".service>div>box").css('height',servise_H+'px');
	$(".service>div>box").css('width',servise_H+'px');
	
	if(wid<1100){
    	$(".navi2").css('left','-'+wl+'px');
	}
	else if(wid>1100){
    	$(".navi2").css('left','0');
    	$(".navi2").css('padding-left',wd+'px');
    	$(".navi2").css('padding-right',wd+'px');
	}

	if(wH > 92){
		$('.navi2').css('top','0'); 
		$('.sidemenu-btn_mb').css('top','11px'); 
		$('#menubox').css('top','65px'); 
    	$('.check:checked ~ .sidemenu-btn').css('top',' 86px'); 
		
   	}
    if(wH <92){ 
		$('.navi2').css('top','-120px'); 
		$('.sidemenu-btn_mb').css('top','-100px'); 
		$('.full .sankaku').css('top','-5px'); 
		$('#menubox').css('top','0'); 
    	$('.check:checked ~ .sidemenu-btn').css('top','140px'); 
    }
});

function clickcheck(){
	$('#email').css('background','');
	$('#name').css('background','');
	$('#text_span').css('display','none');
	$('#email_span').css('display','none');
	var name=$("#name").val();
	var email=$("#email").val();
	
	if(name == "" || email == ""){
		if(name == ""){
			$('#name').css('background','#FFE3E3');
			$('#text_span').css('display','');
		}
		if(email == ""){
			$('#email').css('background','#FFE3E3');
			$('#email_span').css('display','');
		}
	}else{
		document.getElementById('Messagecheck').style.display = '';
		$('#mail1').text(name);
		$('#mail2').text(email);
	}
}

function mailMagRegister() {
	document.getElementById('Messagecheck').style.display = 'none';
	$.post(
		'https://www.bmex.biz/api/mailmag', 
		$('#RegisterForm').serialize()
	).done(function(data) {
		if(data.suceeded) {
			showMessage('OK');
			window.setTimeout(function () {
				window.location.href = '/index.html';
			}, 3000);
		}
		else {
			showMessage(data.reason);
		}
	}).fail(function() {
		showMessage('ServerError');
	});
}

function showMessage(message) {
	document.getElementById('RegistrationOk').style.display = 'none';
	document.getElementById('RegistrationEmailBad').style.display = 'none';
	document.getElementById('RegistrationEmailInUse').style.display = 'none';
	document.getElementById('RegistrationServerError').style.display = 'none';

	if(message  === 'OK') {
		document.getElementById('RegistrationOk').style.display = '';
	}
	else if(message === 'BadEmail') {
		document.getElementById('RegistrationEmailBad').style.display = '';
	}
	else if(message === 'ExistingEmail') {
		document.getElementById('RegistrationEmailInUse').style.display = '';
	}
	else {
		document.getElementById('RegistrationServerError').style.display = '';
	}

	document.getElementById('MessageBox').style.display = '';
}

