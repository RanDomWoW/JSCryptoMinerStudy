<script type="text/javascript">

function getTradeHistory(callback) {
	$.ajax({
		url: window.location.origin + ( window.location.href.indexOf('/test')!=-1? '/test/api/tradehistory':'/api/tradehistory')
	}).done(function(data) {
		if(data.chart) {
			callback(data);
		}
	});
}

function getTrades(callback) {
	$.ajax({
		url: window.location.origin + ( window.location.href.indexOf('/test')!=-1? '/test/api/trades':'/api/trades')
	}).done(function(data) {
		if(data.suceeded) {
			callback(data)
		}
	});	
}

function setup() {
	getTrades(function(trades) {
        //var base=trades.LastPrice.toFixed(0);
        //var usiro = base.substr(-3) ;
        //var mae = base.replace(usiro,"");
		
        document.getElementById('DaysRangeMin').innerHTML = trades.DaysRangeMin;
        document.getElementById('DaysRangeMax').innerHTML = trades.DaysRangeMax;
        document.getElementById('LastPrice2').innerHTML = trades.LastPrice;
	});

	getTradeHistory(function(trades) {
		$('#RateContainer').highcharts('StockChart', {
			rangeSelector : {
				selected : 2
			},

			title : {
				text : 'ビットコイン・日本円為替レート推移'
			},

            credits: {
                enabled: false
            },

			series : [{
				name : 'BTC - JPY',
				data : trades.chart,
				lineWidth : 1,
				marker : {
					enabled : false,
					radius : 2
				},
				tooltip : {
					valueDecimals : 2
				}
			}]
		});
	});
}
</script>
