      $(document).ready(function(){
         
        var menu = $('nav');
        var origMenusetY = menu.offset().top;

        function scroll() {    
        if ($(window).scrollTop() >= origMenusetY) {
            $('nav').addClass('navbarscroll').removeClass('container');
            $('header').addClass('sticky');
            $('.nav.navbar-nav').addClass('container');
            $('.navbarscroll .logo img').attr('src','/images/foot-transfert-madeinfoot-sticky.png');
        } else {
            $('nav').removeClass('navbarscroll').addClass('container');
            $('header').removeClass('sticky');
            $('.nav.navbar-nav').removeClass('container');
            $('nav .logo img').attr('src','/images/foot-transfert-madeinfoot.png');
        }}
        document.onscroll = scroll;

        var divHeight;
        var obj = document.getElementById('pubtop');

        if(obj.offsetHeight){
          divHeight=obj.offsetHeight;
        }
        else if(obj.style.pixelHeight){
          divHeight=obj.style.pixelHeight;
        }

        if(divHeight==0){
          $('#pubtop').css('display','none');
        }
      });

      $( "#scan_pa + ul li" ).mouseover(
        function() {
          var longueur = $( this ).children( ".titre_scan" ).children( "a" ).outerWidth();
          
          if ( longueur > 250 ){
            var margegauche = 250 - longueur;
            $( this ).children( ".titre_scan" ).children( "time" ).css( "opacity", "0" );
            $( this ).children( ".titre_scan" ).children( "a" ).css( "margin-left", margegauche );
          }         
        }
      )
      $( "#scan_pa + ul li" ).mouseout(
        function() {
          $( this ).children( ".titre_scan" ).children( "time" ).css( "opacity", "1" );
          $( this ).children( ".titre_scan" ).children( "a" ).css( "margin-left", "0" );
        }
      )
      $('.carousel').carousel({
        pause: true,
        interval: false
      });
