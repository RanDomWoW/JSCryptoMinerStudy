<script type="text/javascript">
$(document).ready(function() {
	
	if ($('#slidertop>a').length > 1) {
		var slidertop = $('#slidertop').bxSlider({
			touchEnabled: false, responsive: true, controls:false, auto:true, autoHover:true
		});
		$('#slidertop_next').click(function() {slidertop.goToNextSlide();});
		$('#slidertop_prev').click(function() {slidertop.goToPrevSlide();});
	}
		
	if ($('#slidervol>a').length > 1) {
		var slidervol = $('#slidervol').bxSlider({
			touchEnabled: false, responsive: false, controls:false,
			onSlideBefore: function(el, prev, next) {
				$('#prodotti_cont'+prev).fadeOut(300, function() {
					$('#prodotti_cont'+next).fadeIn(300);
				});
			}
		});
		$('#slidervol_next').click(function() {slidervol.goToNextSlide();});
		$('#slidervol_prev').click(function() {slidervol.goToPrevSlide();});
	}
});
</script>
