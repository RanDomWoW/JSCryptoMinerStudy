		url: "http://azinsanat.com/cart",
		data: { data: "delete_product", id: item},
		success: function(responses) {
			eval (responses);
			$("#post_id_" + item).slideUp("slow",  function() { $(this).remove();});
			loading(0);
		}
	});
});

