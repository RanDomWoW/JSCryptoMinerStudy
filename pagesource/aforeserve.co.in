<div id="footer">
    <script>

        function ValidateRequest() {
            var objName, objContactNo, objEmailID, objProduct, objDescription;
            objName = document.getElementById('footer1_txtName');
            objEmailID = document.getElementById('footer1_txtEmailID');
            objPhone = document.getElementById('footer1_txtContactNumber');
            objFeedback = document.getElementById('footer1_txtQuery');

            if (objName.value == "") {
                alert("Enter Name");
                objName.focus();
                return false;
            }


            if (objPhone.value == "") {
                alert("Enter Contact Number ");
                objPhone.focus();
                return false;
            }

            if (objEmailID.value == "") {
                alert("Enter Email");
                objEmailID.focus();
                return false;
            }
            else {

                if (objEmailID.value != "") {

                    if (echeck(objEmailID.value) == false) {
                        objEmailID.focus();
                        return false;
                    }

                }

            }


            


            if (objFeedback.value == "") {
                alert("Enter  Query ");
                objFeedback.focus();
                return false;
            }


            return true;

        }


        function echeck(str) {

            var at = "@"
            var dot = "."
            var lat = str.indexOf(at)
            var lstr = str.length
            var ldot = str.indexOf(dot)
            if (str.indexOf(at) == -1) {
                alert("Invalid E-mail ID")
                return false
            }

            if (str.indexOf(at) == -1 || str.indexOf(at) == 0 || str.indexOf(at) == lstr) {
                alert("Invalid E-mail ID")
                return false
            }

            if (str.indexOf(dot) == -1 || str.indexOf(dot) == 0 || str.indexOf(dot) == lstr) {
                alert("Invalid E-mail ID")
                return false
            }

            if (str.indexOf(at, (lat + 1)) != -1) {
                alert("Invalid E-mail ID")
                return false
            }

            if (str.substring(lat - 1, lat) == dot || str.substring(lat + 1, lat + 2) == dot) {
                alert("Invalid E-mail ID")
                return false
            }

            if (str.indexOf(dot, (lat + 2)) == -1) {
                alert("Invalid E-mail ID")
                return false
            }

            if (str.indexOf(" ") != -1) {
                alert("Invalid E-mail ID")
                return false
            }

            return true
        }
    </script>


