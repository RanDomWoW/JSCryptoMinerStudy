<script>

    var blogDetailsPageName="";
    var splitpage="";
    var currentPageUrl=window.location.href;
    var currentPageDecodedUrl=decodeURI(decodeURI(currentPageUrl));
    var blogTitleFromUrl=currentPageDecodedUrl.split("/");
    for(var i=0; i<blogTitleFromUrl.length; i++){
        var subTopic=blogTitleFromUrl[i];
        var matchedSubTopic=subTopic.match(".topic.html");
        if(matchedSubTopic==".topic.html"){
          blogDetailsPageName=blogTitleFromUrl[blogTitleFromUrl.length-1];
        }
    }
    var pagepath = document.getElementById('pagepath').value;
    var exactpage = pagepath.replace('/content/ap/en/','');
    if(blogDetailsPageName){
        splitpage = exactpage.replace(/\//g,':')+":"+blogDetailsPageName;
    }else{
        splitpage = exactpage.replace(/\//g,':');
    }
    var customErrorPage = document.getElementById('customErrorPage');
    if(customErrorPage){
        var digitalData = {
            page :{  'customError' : '500:Internal Server Error',
                     'pageName' : '500:'+window.location.href
            }
        }
    }else{
        if(window.authManager.getBpNumber() != null){
            var crmid = window.authManager.getBpNumber();
            var digitalData = {
               page :{  'pageName' : 'in:'+splitpage },
               user :{ 'crmid' : crmid }
            }
        }else{
            var digitalData = {
               page :{  'pageName' : 'in:'+splitpage }
            }
        }
    }
</script>

    
    
        
