            var title = document.querySelectorAll('.image-dropdown-preview-title');
            var img = document.querySelectorAll('.nav-image-large');
            var previewImg = document.querySelectorAll('.image-dropdown-preview-img');

            for (var i = box.length - 1; i >= 0; i--) {
                box[i].style.display = 'block';
                title[i].innerHTML = element.getAttribute('title');

                var previewImgSrc = element.getAttribute('data-preview-img');
                if (previewImgSrc) {
                    for (var j = previewImg.length - 1; j >= 0; j--) {
                        previewImg[j].src = previewImgSrc;
                    }
                } else {
                    for (var k = img.length - 1; k >= 0; k--) {
                        var spriteName = element.getAttribute('data-sprite-name');
                        var spriteIndex = element.getAttribute('data-sprite-index');
                        
                        img[k].className = 'nav-image-large ' + spriteName + '-' + spriteIndex;
                    }
                }
            }
        }

        function stop_preview_img() {
            var box = document.querySelectorAll('.image-dropdown-preview-box');
            for (var i = box.length - 1; i >= 0; i--) {
                box[i].style.display = 'none';
            }
        }
    </script>

