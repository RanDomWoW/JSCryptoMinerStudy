		function deixaNumeros(valor){
			while( valor.match(/[^d]/)!=null ){
				valor = valor.replace(/[^d]/, '');
			}
			return valor
		}
		function preencheComZero(valor, total){
			while( valor.length < total ){
				valor = '0'+ valor;
			}
			return valor;
		}
