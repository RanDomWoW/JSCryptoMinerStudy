					var field = $('#city_list').find('option');

					field.click(function(e){
						var t = $(this).text();
						$(".popup_new a.city-item").each(function(){
							if ($(this).text() == t)
							{
								$("#city_list").hide();
								$(this).trigger('click');
								return true;
							}
						});
					});

					$("#city_list").bind('keyup', function(e){
						console.log(e.keyCode);
						if (e.keyCode != 13)
							return true;
						var t = $(this).val();
						$(".popup_new a.city-item").each(function(){
							if ($(this).text() == t)
							{
								$("#city_list").hide();
								$(this).trigger('click');
								return true;
							}
						});
					});

					$("#city_list").hide();
					$("#city_list_clear").hide();

					$("#city_list_clear").click(function(){
						$("#city_list").hide();
						$("#city_list_clear").hide();
						$('#searchInput').val('').focus();
					});

					$('#searchInput').bind('keyup', function(e) {

						if ($(this).val().length <= 0)
						{
							$("#city_list_clear").hide();
							$("#city_list").hide();
							return true;
						}

						$("#city_list_clear").show();

					    var q = new RegExp($(this).val(), 'ig');
					    var flag = false;

						$("#city_list").hide();
					    for (var i = 0, l = field.length; i < l; i += 1) {
					        var option = $(field[i]),
					            parent = option.parent();

					        if ($(field[i]).text().match(q)) {
					            $("#city_list").show();
					            flag = true;
					            if (parent.is('span')) {
					                option.show();
					                parent.replaceWith(option);
					            }
					        } else {
					            if (option.is('option') && (!parent.is('span'))) {
					                option.wrap('<span>').hide();
					            }
					        }
					    }

					    if (e.keyCode == 40 && flag)
					        $("#city_list").focus();

					});

				</script>

