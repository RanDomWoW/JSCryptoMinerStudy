				if(selected=='WILD CARD')
				{
					jQuery('#walltype_repeat_div_cms').html('HALF REPEAT 0.52 m');
				}
				else
				{
					jQuery('#walltype_repeat_div_cms').html('0.52 m');
				}	
			}
		}
		else
		{
			jQuery('#walltype_div_cms').html("");
			jQuery('#walltype_width_div_cms').html('');
			jQuery('#walltype_length_div_cms').html('');
			jQuery('#walltype_repeat_div_cms').html('');
		}
		
	});
	
	jQuery('#wallpaper_calc_cms').submit(function(){
		if (jQuery("#wallpapertypecms option:selected").val() <= 0)
		{
			alert('Please select Wallpaper collection');
			return false;
		}
	
		var wallval = jQuery("#wallpapertypecms option:selected").val();
		
		var measurement = jQuery('input[name=feetcms]:checked').val();
			
		var listParams = new Array('wall1height_cms','wall1width_cms','wall2width_cms','wall3width_cms','wall4width_cms');
		for(i=0;i<listParams.length;i++){
		   
			var str = jQuery('#'+listParams[i]+'').val();
			
			if(str!='')
			{
				var res = str.split(" "); 
				if(isNaN(res[0]))
				{
					alert('Please enter only numeric value');
					jQuery('#'+listParams[i]+'').focus();
					var rolls = '... rolls needed';
					jQuery('#result-quantity-cms').html(rolls);
					return false;
				}
				
				eval("var "+listParams[i]+" = (isNaN("+res[0]+"))?0:"+res[0]+"");
			}
			else
			{
				eval("var "+listParams[i]+" = (isNaN(jQuery('#"+listParams[i]+"').val()))?0:jQuery('#"+listParams[i]+"').val();");
			}	
			
		}
		
		if(measurement=='Centimeters')
		{
			var wall_height = (wall1height_cms/100);
			
			var length1 = (wall1width_cms/100);
			var length2 = (wall2width_cms/100);
			var length3 = (wall3width_cms/100);
			var length4 = (wall4width_cms/100);
			
		}
		else if(measurement=='Inch')
		{
			var wall_height = (wall1height_cms/39.37);
			
			var length1 = (wall1width_cms/39.37);
			var length2 = (wall2width_cms/39.37);
			var length3 = (wall3width_cms/39.37);
			var length4 = (wall4width_cms/39.37);
			
		}
		else if(measurement=='Feet')
		{
			var wall_height = (wall1height_cms/3.28);
			
			var length1 = (wall1width_cms/3.28);
			var length2 = (wall2width_cms/3.28);
			var length3 = (wall3width_cms/3.28);
			var length4 = (wall4width_cms/3.28);
			
		}
		else
		{
			var wall_height =wall1height_cms;
			
			var length1 = (wall1width_cms);
			var length2 = (wall2width_cms);
			var length3 = (wall3width_cms);
			var length4 = (wall4width_cms);
		}
			
		if(wallval == 'PAMPAS' || wallval == 'FLAMBOYANCE' || wallval == 'HYACINTH' || wallval == 'ACANTHUS' || wallval == 'LONDON ROSE' || wallval == 'DALSTON ROSE' || wallval == 'BIDJAR' || wallval == 'TULIPA' || wallval == 'OSWEGO' || wallval == 'EQUUS' || wallval == 'FLORIKA' || wallval == 'SUMATRA' || wallval == 'BALEANA' || wallval == 'ORSMAN STRIPE')
		{
			
			if(wall_height > 3)
			{
				var rolls = '... rolls needed';
				jQuery('#result-quantity-cms').html(rolls);
				jQuery("#orderdivcms").show();
				jQuery("#focus_cms").focus();
				return false;
			}
			
			var totallength = Math.ceil(((length1 + length2 + length3 + length4)/0.45));
			totallength = totallength/3;
			
		}
		else if(wallval == 'HACKNEY EMPIRE' || wallval == 'FLIGHTS OF FANCY' || wallval == 'PALMERAL' || wallval == 'POPPIUM' || wallval == 'MIDNIGHT GARDEN' || wallval == 'MEY MEH' || wallval == 'PEONEDEN' || wallval == 'CASTANEA' || wallval == 'TAROVINE' || wallval == 'INFERNO' || wallval == 'MONO STRIPE' || wallval == 'BABYLON' || wallval == 'OPIA' || wallval == 'ETERIS' || wallval == 'LIMERENCE' || wallval == 'TROOP' || wallval == 'BLACKTHORN' || wallval == 'PLUMA' || wallval == 'ZANJAN' || wallval == 'ZEUS' || wallval == 'SERPENTIS' || wallval == 'MAMOUNIA' || wallval == 'MAJORELLE' || wallval == 'DAISY STRIPE')
		{
			
			if(wall_height > 3)
			{
				var rolls = '... rolls needed';
				jQuery('#result-quantity-cms').html(rolls);
				jQuery("#orderdivcms").show();
				jQuery("#focus_cms").focus();
				return false;
			}
			
			var totallength = Math.ceil(((length1 + length2 + length3 + length4)/0.45));
			totallength = totallength/4;
			
		}
		else if(wallval == 'WILD CARD')
		{
			
			if(wall_height > 10)
			{
				var rolls = '... rolls needed';
				jQuery('#result-quantity-cms').html(rolls);
				jQuery("#orderdivcms").show();
				jQuery("#focus_cms").focus();
				return false;
			}
			
			
			if(wallval == 'DALSTON ROSE' || wallval =='LEONINE')
			{
				var no_of_cuts_needed = Math.ceil(((length1 + length2 + length3 + length4)/0.52));
				
				var lenth_of_each_cut=Math.ceil(wall_height/0.52)*0.52;
				
				var total_length_required=no_of_cuts_needed*lenth_of_each_cut;
				
				var totallength=total_length_required/10.05;
				
			}
			else
			{
				var no_of_cuts_needed = Math.ceil(((length1 + length2 + length3 + length4)/0.52));
				
				var lenth_of_each_cut=(Math.ceil(wall_height/0.52)*0.52)+(0.52/2);
				
				var total_length_required=no_of_cuts_needed*lenth_of_each_cut;
				
				var totallength=total_length_required/10.05;
			}
			
			
			
		}
		
		if((length1==0)&&(length2==0)&&(length3==0)&&(length4==0)){
			alert("Please specify the Height and Width of your walls");
			var rolls = '... rolls needed';
			jQuery('#result-quantity-cms').html(rolls);
			return false;						
		}
		
		if(totallength<=0){
			alert("Type in a valid wall area");
			var rolls = '... rolls needed';
			jQuery('#result-quantity-cms').html(rolls);
			return false;						
		}					
		
		var quantity = Math.ceil( totallength );

		jQuery("#orderdivcms").hide();
		var quantity1 = quantity + ' rolls needed';
		jQuery('#result-quantity-cms').html(quantity1);		
		
		
	
	});

});

</script>



