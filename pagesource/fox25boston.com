  this.newsletters = ko.observableArray(newsletters);
        this.zipcodeValidation = ko.observable('');
  this.emailValidation = ko.observable('');
  this.updateNNewslettersChecked = function() {
   var n = 0;
   for (var i = 0; i < this.newsletters().length; i++) {
    if (this.newsletters()[i].checked) {
     n++;
    }
   }
   this.nNewslettersChecked(n);
  };
  this.signupClicked = function() {
   var formValid = true;
   this.clearFormErrors();
            var isEmailValid = isEmail(this.email());
            var isZipcodeValid = isZipcode(this.zipcode());
   if (!isEmailValid && isZipcodeValid) {
    this.displayEmailErrorMsg('Please enter a valid email address');
    formValid = false;
   }
   if (!isZipcodeValid && isEmailValid) {
                this.validateEmailWithInvalidZipcode();
                formValid = false;
   }
            if (!isZipcodeValid && !isEmailValid) {
                this.displayZipcodeAndEmailErrorMsg('Please enter a valid email address and zip code');
                formValid = false;
            }
   if (formValid) {
    this.validateEmailAndSignup();
   }
  };
        
        this.validateEmailWithInvalidZipcode = function() {
