                            } else {
                                that.displayEmailErrorMsg('You are already subscribed to this list');
                            }
                        }
                    }
                    that.notProcessing(true);
                },
    error: function() {
     that.displayEmailErrorMsg('A problem was encountered. Please try again later.');
     that.notProcessing(true);
    }
   });
        };
        
  this.clearFormErrors = function() {
   this.emailErrorMsg('');
   this.zipcodeErrorMsg('');
   this.zipcodeValidation('');
   this.emailValidation('');
  };
  this.zipcodeErrorMsg = ko.observable('');
  this.displayZipcodeErrorMsg = function(msg) {
   this.zipcodeErrorMsg(msg);
            this.emailValidation('valid');
   this.zipcodeValidation('invalid');
  };
  this.emailErrorMsg = ko.observable('');
  this.displayEmailErrorMsg = function(msg) {
   this.emailErrorMsg(msg);
            if (this.zipcode()) {
                this.zipcodeValidation('valid');
            } else {
                this.zipcodeValidation('');
