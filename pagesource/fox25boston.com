   this.notProcessing(false);
   $.ajax({
    type: 'POST',
    url: '/cmg-thirdparty-portlet/restful/newsletter/signup',
    data: data,
    success: function(r) {
                    if (r.stat == "error" && r.emailValidation == "invalid") {
                        that.displayEmailErrorMsg('Please enter a valid email address');
                    } else {
                        if (r.stat == "ok" && r.subscribed.length) {
                            that.formVisible(false);
