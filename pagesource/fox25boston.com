        this.displayZipcodeAndEmailErrorMsg = function(msg) {
            this.zipcodeErrorMsg(msg);
            this.emailValidation('invalid');
            this.zipcodeValidation('invalid');
        };
  this.showOptions = ko.observable(false);
 };
 var newsletterViewModel = new NewsletterViewModel('', '', newsletters);
