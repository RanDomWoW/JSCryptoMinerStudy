    <script type="text/javascript">
        $(document).ready(function () {
            if ($("#nextArticles").css("display") == "none") {
                articleOffset = 5;
            }
        });

        var articleOffset = 16;
        var popularOffset = 19;
        function nextShowArticles(btn) {
            $.ajax({
                type: 'POST',
                url: '/getNextArticles?fromHomePage=true',
                data: {offset: articleOffset, },
                success: function (response) {
                    var parent;
                    if (btn != null) {
                        parent = $(btn).parents('.box');
                        $(btn).parent().remove();
                    } else {
                        parent = $("#getNextArticlesButton").parents('.box');
                        $("#getNextArticlesButton").remove();
                    }
                    //$("#dynamic-articles").append( response );
                    parent.append(response);

                    resetShareCount();

                }});
            $.ajax({
                type: 'POST',
                url: '/getNextPopular',
                data: {offset: popularOffset},
                success: function (response) {

                    $("#dynamic-popular").append(response);

                }});
            articleOffset = articleOffset + 10;
            popularOffset = popularOffset + 18;
            return false;
        }

        $(function () {
            var a = function () {
                var a = $("#affiliate");
                var b = $(window).scrollTop();
                var c = $("#affiliate_old").offset().top + $("#affiliate_old").height();
                var d = $("#affiliate-anchor").offset().top + 70;
                var f = $("#footer").offset().top;

                if ((a.offset().top - 130) < c) {
                    $("#affiliate").css({position: "relative", top: "initial"});
                    $("#affiliate-anchor").css({position: "relative", top: 0});
                } else if ((b + a.height()) >= (f - 70)) {
                    var top = f - d + 70 - a.height();
                    $("#affiliate").css({position: "absolute", top: top});
                } else if (b > d && (b + a.height()) < (f)) {
                    $("#affiliate").css({position: "fixed", top: "120px"});
                    $("#affiliate-anchor").css({position: "absolute", top: 0});
                } else {
                    $("#affiliate").css({position: "relative", top: "initial"});
                    $("#affiliate-anchor").css({position: "relative", top: 0});
                }
            };
            $(window).scroll(a);
            a();
        });
