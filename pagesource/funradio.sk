        function fixBackground() {
            var position = $("#header").offset();
            var leftWidth = $("#backgroundLeft").width();
            $("#backgroundLeft").css("left", (position.left - leftWidth - 10) + 'px').css("visibility", "visible");
            $("#backgroundRight").css("left", (position.left + 1070) + 'px').css("visibility", "visible");
        }
        $(document).ready(function () {
            fixBackground();

            $('#ticker #news').bind('beforeStarting', function () {
                $('#ticker #news').css('visibility', 'visible');
                $('#ticker_l').css('display', 'block');
                $('#ticker_r').css('display', 'block');
            }).marquee({
                speed: 15000,
                gap: 0,
                delayBeforeStart: 1000,
                direction: 'left',
                duplicated: true,
                pauseOnHover: true
            });

        });
        $(window).resize(function () {
            fixBackground();
        });
