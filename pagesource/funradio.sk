    <script type="text/javascript">
        var articleOffset = 16;

        function openPlayer(id) {
            window.open("/play/?select=" + id, "Fun rádio player", "width=1000, height=620", true);
            return false;
        }
        function getPlayerInfo() {
            $.ajax({
                url: '/sepia/sepia_data_live.xml',
                type: 'GET',
                dataType: 'xml',
                cache: false,
                success: function (xml) {
                    var playing = $(xml).find('item1');

                    var interpret = $(playing).find('interpret').text() || null;
                    var song = $(playing).find('skladba').text() || null;

                    if (typeof assetVideo == 'function') {
                        var video = $(playing).find('video').text();
                        assetVideo(video);
                    }

                    var full_song_name = '';
                    var full_song_name2 = '';

                    if (interpret)
                    {
                        full_song_name = full_song_name.concat(interpret, ' - ', song);
                        full_song_name2 = '<span style="font-weight:bold; font-size: 22px;">' + interpret + '</span><br /><br />' + song;
                    }

                    $('.song-info').html(full_song_name);
                    $('.songNameInVideo').html(full_song_name2);
                }
            });
        }

        $(document).ready(function () {
            getPlayerInfo();
            setInterval(getPlayerInfo, 30000);  //after 10s reload player info

            var x = function () {
                var b = $(window).scrollTop();
                var d = $("#menu-anchor").offset().top;
                var x = $("#affiliate");
                if (b > d && (b + x.height())) {
                    $('#header').addClass("closed");
                    $('#player').addClass("embeded");
                    $("#pollInMenu").css("display", "none");
                            } else {
                        $('#header').removeClass("closed");
                        $('#player').removeClass("embeded");
                        $("#pollInMenu").css("display", "block");
                            }
                };
                $(window).scroll(x);
                x()

        
                $("#menu-icon").click(function () {
                    $("#main-menu").toggleClass("visible");
                    $("#menu-icon").toggleClass("selected");
                });

                $('#main-menu li').mouseenter(function () {
                    $(".submenu", this).css('left', '-25px');
                }).mouseleave(function () {
                    $(".submenu", this).css('left', '-999em');
                });


                $('#big-submenu > li').mouseenter(function () {
                    $("#main-menu .submenu .default").css('left', '-999em');
                    $(".column-list", this).css('left', '0px');
                }).mouseleave(function () {
                    $("#main-menu .column-list").css('left', '-999em');
                    $("#main-menu .submenu .default").css('left', '0px');

                });

        
                if (($(".skyscraper img").height() == 0 || $(".skyscraper img").height() == null) &&
                        ($(".skyscraper object").parent().height() == 0 || $(".skyscraper object").parent().height() == null)) {
                    $(".skyscraper").css("display", "none");
                }

                $("img[title]").titleBlock();

            });
            function resetShareCount() {
                $(".count").baitshare();
            }
    </script>

