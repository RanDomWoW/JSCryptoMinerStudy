				<input id='pREmail' type='text' placeholder='введите имейл' />
				<button class='submit' onclick="
					var footerMail = document.getElementById('pREmail').value;
					toldot.blockIsDownloading(event,true,'pR')
					toldot.ajax(
						{action:'/work-zone/subscriptions/mailing/',method:'post'},
						{email:footerMail,sub:1125,type:'subscribe'},
						function(response){
						toldot.blockIsDownloading(false,false,'pR')
							response = JSON.parse(response);
							if( response.code == 160 /*успешно*/ || response.code == 170 /*уже подписан*/ ){ 
								toldot.showWindow( event,'Вы подписаны','Вы подписаны на нашу еженедельную рассылку!<br/><br/>Она выходит по средам и содержит в себе анонсы лучших материалов Толдота за неделю. Мы с радостью <a href=\'/about/send-email/\'>примем ваши замечания и предложения</a> по улучшению рассылки.')
							} else if(response.code == 175){ // отписан, ссылка на ручную подписку
								toldot.showWindow( event, 'Не получилось', 'У нас не получилось подписать вас автоматически.<br/><br/>Ранее вы были подписаны на эту рассылку и отписались от нее. Для повторной подписки заполните, пожалуйста, форму: <a href=' + response.answerHTML  + '  target=_blank>'+response.answerHTML+'</a>. Далее следуйте инструкциям в ней.' );
							} else if(response.code == 10){ // Не указан email
								toldot.showWindow( event, 'Не получилось', 'Не указан email.' ); 
							} else if(response.code == 180){ // неизвестная ошибка
								toldot.showWindow( event, 'Возникла ошибка', 'У нас не получилось подписать вас автоматически. Возникла ошибка <br><i>' + response.answerHTML + '</i><br><i>'  + response.mchimpStatus + '</i><br>' + 'Скопируйте описание ошибки, приведенное выше, и <a href=https://toldot.ru/about/send-email/>пришлите нам</a>. Мы с радостью поможем.' ); 
							} else if(response.code == 190){ // имейл забанили
								toldot.showWindow( event, 'Возникла ошибка', 'У нас не получилось подписать вас автоматически. <a href=https://toldot.ru/about/send-email/>пришлите нам ваш имейл</a>. Мы с радостью подпишем Вам вручную.' ); 
							} else {
								toldot.showWindow( event, 'Неизвестный ответ сервера',JSON.stringify( response ) )
							}
						}
