			var app = {};
			app.opt = {
				parent : $('.b-counter__holder')
			}

			app.num_stop = 146859;
			app.counter = app.num_stop - 10;

			app.odometer = app.opt.parent.jOdometer({
				increment: 1,
				counterStart: app.counter,
				delayTime: 10000
			});

			app.get_random = function(from, to) {
				return Math.floor(Math.random() * (to - from) + from);
			}

			app.go = function() {
				if (app.counter < app.num_stop) {
					app.counter += 1;
					app.odometer.goToNumber(app.counter);

					setTimeout(app.go, app.get_random(5000, 15000));
				}
			}

			app.go();
		});
