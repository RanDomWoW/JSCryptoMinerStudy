<script>
// A $( document ).ready() block.
$.ajax({
//url for loading .txt file for search terms
url: "templates/postoffice/LP/resources/pop-search-links.txt",
dataType: "text",
success: function(data) {
		//split text file into array at each new line
		var autoCompleteData = data.split('\n');
		
		var array = autoCompleteData;
		var newHTML = [];
		//generate each <a> and <span> tag for the search terms
		$.each(array, function(index, value) {
			newHTML.push('<a href="/search/' + value + '/" title="' + value + '">' + '<span>' + value + '</span></a>');
		});
		//insert the above into the <p> with ID of searchHolder 
		$("#searchHolder").html(newHTML.join(""));
}
});
</script>
 
 

