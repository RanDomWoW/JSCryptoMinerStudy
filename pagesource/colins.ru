<script type="text/javascript">

    function RefreshCartInfo() {
        var counterContainer = $('.timeCounter');
        var countContainer = $(".cart-item-count");
        var counterWrapper = $(".module-time-counter");

        $.ajax({
            cache: false,
            url: '/ShoppingCart/GetCartInfo',
            type: 'get',
            success: function (result) {
                $(".module-time-counter").hide();

                if (result.ExpireTime.length > 0 && "False" == "True") {
                    counterContainer.countdown({
                        date: result.ExpireTime,
                        onEnd: function () {
                            alert("Common.BasketTimeoutMessage");
                            window.location = '/ShoppingCart/ClearBasket';
                        }
                    });

                    $(".module-time-counter").show();
                }

                $('#hiddenShoppingCount').val(result.Count);
                countContainer.text("( " + result.Count + " )");

                if (result.Count > 0) {
                    counterWrapper.show();
                }

                if (result.Messages.length > 0) {
                    var messageListHtml = "";
                    for (var i = 0; i < result.Messages.length; i++) {
                        messageListHtml += result.Messages[i].Message + "<br />";
                    }

                    //alert or use notification
                    alert(messageListHtml);

                    $.ajax({
                        cache: false,
                        url: '/ShoppingCart/UpdateWaitingCustomerMessages',
                        type: 'post',
                        data: 'IdList=' + result.MessageIdList,
                        success: function (result) {
                        }
                    });
                }
            }
        });
    }

    $("document").ready(function () {
        RefreshCartInfo();
    });
</script>
