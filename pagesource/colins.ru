<script type="text/javascript">
    $(document).ready(function () {
        $(".bultenBtn").prop('disabled', true);

        $("#gizlilikCheckbox").change(function () {

            if ($(this).is(":checked")) {

                $(".bultenBtn").prop('disabled', false);
            } else {

                $(".bultenBtn").prop('disabled', true);

            }
        })
    });
   

    function Subscribe() {

        var subscribe = 'true';
        if ($('#newsletter-unsubscribe').is(':checked')) {
            subscribe = 'false';
        }
        var email = $("#Email").val();

        if (email.length > 0) {
            var subscribeProgress = $("#subscribe-loading-progress");
            subscribeProgress.show();
            $.ajax({
                cache: false,
                type: "POST",
                url: "/Newsletter/SubscribeNewsletter",
                data: "subscribe=" + subscribe + "&email=" + email,
                success: function (data) {
                    subscribeProgress.hide();
                    $("#pnl-newsletter-result div").html(data.Result);
                    ShowBsModal(data.Result);
                    $("#Email", ".newsletterbox").val("E-mail");
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert('Failed to subscribe.');
                    subscribeProgress.hide();
                }
            });

            return false;
        }
    }

    $(".newsletter-email").keydown(function (event) {
        if (event.keyCode == 13) {
            Subscribe();
            return false;
        }
    });

</script>

