function couponDemand(flag){
	var param = "";
	if(flag == '1'){
		if($("#cu_id").val() == ""){
			alert('아이디를 입력해주세요.');
			$("#cu_id").focus();
			return;
		}
		if($("#cu_pwd").val() == ""){
			alert('비밀번호를 입력해주세요.');
			$("#cu_pwd").focus();
			return;
		}
		param = "type=1&id=" + $("#cu_id").val() + "&pwd=" + $("#cu_pwd").val();
	}else if(flag == '2'){
		if($("#cpn_vip").val() == ""){
			alert('쿠폰번호를 입력해주세요.');
			$("#cpn_vip").focus();
			return;
		}	
		param = "type=2&cpn=" + $("#cpn_vip").val();
	}else if(flag == '3'){
		if($("#cpn_fest").val() == ""){
			alert('쿠폰번호를 입력해주세요.');
			$("#cpn_fest").focus();
			return;
		}
		param = "type=3&cpn=" + $("#cpn_fest").val();
	}else if(flag == '4'){
		if($("#cpn_gift").val() == ""){
			alert('쿠폰번호를 입력해주세요.');
			$("#cpn_gift").focus();
			return;
		}
		param = "type=4&cpn=" + $("#cpn_gift").val();
	}else if(flag == '5'){
		if($("#cpn_hana").val() == ""){
			alert('쿠폰번호를 입력해주세요.');
			$("#cpn_hana").focus();
			return;
		}
		param = "type=5&cpn=" + $("#cpn_hana").val();
	}else if(flag == '6'){
		if($("#palm_id").val() == ""){
			alert('아이디를 입력해주세요.');
			$("#palm_id").focus();
			return;
		}
		if($("#palm_pwd").val() == ""){
			alert('비밀번호를 입력해주세요.');
			$("#palm_pwd").focus();
			return;
		}
		param = "type=6&id=" + $("#palm_id").val() + "&pwd=" + $("#palm_pwd").val();
	}

	$.ajax({
			url:'/_common/exec/part_coupon.php',
			type:'POST',
			data:param,
			dataType:'html',
			timeout:3000, 
			beforeSend:function(){},
			success:function(data){
				if(data == 'fail_login'){
					//$("#resultCpn").attr("src","/img/top_13_1.jpg");
					alert('제휴사 쿠폰번호가 다릅니다.\r\n발급받으신 제휴사 또는 고객센터로 문의해주세요.');
				}else if(data == 'same'){
					//$("#resultCpn").attr("src","/img/top_13_2.jpg");
					//alert('쿠폰및 상품권이 이미 발급되었습니다.');
					alert('쿠폰 및 상품권이 이미 발급되었습니다.\r\n마이페이지 - 쿠폰보관함을 확인해주세요.');
				}else if(data == 'success'){
					//$("#resultCpn").attr("src","/img/top_13.jpg");
					alert('청첩장 결제시 사용가능한 쿠폰이 마이페이지에 등록되었습니다.');
				}else if(data == 'memreg'){
					alert('사용가능한 쿠폰입니다.\r\n회원가입하시면 마이페이지에 할인쿠폰을 담아드리겠습니다.');
					window.document.location.href="/member/join.php";
				}else if(data == 'end'){
					alert('만료된 쿠폰입니다.');
					return;
				}
				initField(flag);
			},
			error:ajax_error
	});
}
function initField(flag){
	if(flag == '1'){
		$("#cu_id").val("");
		$("#cu_pwd").val("");
		$("#cu_id").addClass("bg_id");
		$("#cu_pwd").addClass("bg_pwd");
	}else if(flag == '2'){
		$("#cpn_vip").val("");
		$("#cpn_vip").addClass("bg_id");
	}else if(flag == '3'){
		$("#cpn_fest").val("");
		$("#cpn_fest").addClass("bg_coupon");
	}else if(flag == '4'){
		$("#cpn_gift").val("");
		$("#cpn_gift").addClass("bg_coupon");
	}
}
