            <!-- ko if: TipoTransacao() !== "Resgate" -->
            <td width="70" class="val" data-bind="text: ValorCredito().toFixed(2)"></td>
            <!-- /ko -->
            <td class="val" data-bind="text: moment.utc(Vencimento()).format('DD.MM.YYYY') == '01.01.0001'?'': moment.utc(Vencimento()).format('DD.MM.YYYY')"></td>
        </tr>
        </tbody>
    </table>
    <table class="tb-extrato footer">
        <tfoot>
