</div>
            <div class="bx-extrato" id="dv-extrato">
    <h2>
        Seu <b>Extrato</b>
        <button class="btn" data-bind="click: OnShowModal">Show Modal</button>
    </h2>

    <fieldset>
        <input type="hidden" id="hdnContaCorrenteExtrato" data-bind="value: IdContaCorrente" />
        <table class="tb-extrato">
            <thead>
                <tr>
                    <td colspan="2">
                        <div class="fm-campo fm-ddl fst">
                            <div class="ct-select">
                                <select name="ddlTipoTransacao" id="ddlTipoTransacao" data-bind="value: TipoTransacao, event: {change: TipoTransacaoChange}">
                                    <option value="">Todos</option>
                                    <option value="A">Acúmulo</option>
                                    <option value="R">Resgate</option>
                                </select>
                            </div>
                        </div>
                        <div class="fm-campo fm-ddl">
                            <div class="ct-select">
                                <select name="ddlPeriodoTransacao" id="ddlPeriodoTransacao" data-bind="value: Periodo, event: {change: PeriodoChange}">
                                    <option value="0">Selecionar período</option>
                                    <option value="99" selected="selected">Últimas 5 transações</option>
                                    <option value="7">Últimos 7 dias</option>
                                    <option value="15">Últimos 15 dias</option>
                                    <option value="30">Últimos 30 dias</option>
                                    <option value="60">Últimos 60 dias</option>
                                    <option value="90">Últimos 90 dias</option>
                                </select>
                            </div>
                        </div>
                    </td>
                </tr>
                <tr id="trPeriodo" style="display: none;">
                    <td colspan="2">
                        <div class="fm-campo" id="boxPeriodo">
                            <input type="text" id="periodoInicial" class="w91 periodo next calendario data required numeric" name="PeriodoInicial" maxlength="10" data-bind="value: PeriodoInicial" />
                            <input type="text" id="periodoFinal" class="w91 periodo next calendario data required numeric" name="PeriodoFinal" maxlength="10" data-bind="value: PeriodoFinal" />
                        </div>
                        <div class="buttons">
                            <button class="btn btn-vermelho" data-bind="click: OnGetExtrato">Buscar</button>
                        </div>
                    </td>
                </tr>
            </thead>
            <tbody data-bind="foreach: ListaTransacao">
                <tr>
                    <td>
                        <!-- ko if: TipoTransacao() === "Resgate" -->
                        <span class="status status3"></span>
                        <!-- /ko -->
                        <!-- ko if: TipoTransacao() !== "Resgate" -->
                        <span class="status status1"></span>
                        <!-- /ko -->
                        <b class="data" data-bind="text: moment.utc(DateTimeTransacao()).format('DD.MM.YYYY')"></b>
                        <!-- ko if: IdTipoTransacao() !== 14 -->
                        <p data-bind="text: Descricao"></p>
                        <!-- /ko -->
                        <!-- ko if: TipoTransacao() === "Acúmulo" -->
                        <p data-bind="text: NumeroPedido"></p>
                        <!-- /ko -->
                        <!-- ko if: IdTipoTransacao() === 14 -->
                        <p data-bind="text: OrigemTransacao"></p>
                        <!-- /ko -->
                    </td>
                    <!-- ko if: TipoTransacao() === "Resgate" -->
                    <td class="val" data-bind="text: ValorDebito().toFixed(2)"></td>
                    <!-- /ko -->
                    <!-- ko if: TipoTransacao() !== "Resgate" -->
                    <td class="val" data-bind="text: ValorCredito().toFixed(2)"></td>
                    <!-- /ko -->
                </tr>
            </tbody>
            <tfoot>
                <tr>
                    <td>Total de combustível creditado no período</td>
