                    <td></td>
                </tr>
                <tr>
                    <td>Total resgatado no período</td>
                    <td class="val"><span data-bind="text: TotalDebitadoResumoFormatado"></span></td>
                    <td></td>
                </tr>
                <tr>
                    <td>Saldo de Shell Resgate Fácil</td>
                    <td class="val"><span data-bind="text: SaldoFormatado" /></td>
                    <td></td>
                </tr>
                
                <tr>
                    <td colspan="3" class="buttons">
                        <button class="btn btn-vermelho" data-bind="click: GerarPDF">Salvar</button>
                        <button class="btn btn-vermelho" id="btnImprimirExtrato" name="btnImprimirExtrato" onclick="window.print();">Imprimir</button>
                    </td>
                </tr>
            </tfoot>
        </table>

    </fieldset>
</div>
<div class="bx-extrato modal" id="bx-extrato-modal">
    <h2 class="extrato-title">
        Seu <b>Extrato</b>
        <button class="btn-close" data-bind="click: OnCloseModal">Close Modal</button>
    </h2>
    <table id="tb-modal-extrato" class="tb-extrato tb-modal-extrato">
        <tr>
            <th></th>
            <th>Valor</th>
            <th>Vencimento</th>
        </tr>
        <tbody data-bind="foreach: ListaTransacaoDetalhada">
        <tr>
            <td width="380">
                <!-- ko if: TipoTransacao() === "Resgate" -->
                <span class="status status3"></span>
                <!-- /ko -->
                <!-- ko if: TipoTransacao() !== "Resgate" -->
                <span class="status status1"></span>
                <!-- /ko -->
                <b class="data" data-bind="text: moment.utc(DateTimeTransacao()).format('DD.MM.YYYY')"></b>
                <!-- ko if: IdTipoTransacao() !== 14 -->
                <p data-bind="text: Descricao"></p>
                <!-- /ko -->
                <!-- ko if: TipoTransacao() === "Acúmulo" -->
                <p data-bind="text: NumeroPedido"></p>
                <!-- /ko -->
                <!-- ko if: IdTipoTransacao() === 14 -->
                <p data-bind="text: OrigemTransacao"></p>
                <!-- /ko -->
            </td>
            <!-- ko if: TipoTransacao() === "Resgate" -->
            <td width="60" class="val" data-bind="text: ValorDebito().toFixed(2)"></td>
