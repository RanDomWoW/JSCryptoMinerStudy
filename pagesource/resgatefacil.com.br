                //vmMapaRevenda.Init('fmLocalizarPostos');
                ShellCSVP.Main.vmDeslogado.MapaRevenda = vmMapaRevenda;
            });


            ShellCSVP.Main.vmUtils.AjaxRequest('API/Login', 'GET', null, null, null, function (data) {
                if (data == 200) {
                    window.location.href = ShellCSVP.Main.vmUtils.baseURL + 'Home/Logada';
                }
            });

            if (''.toLowerCase() == '1anodetanquecheio') {
                bradesco = true;
                $("#btnPromocaoCaixa").click();
