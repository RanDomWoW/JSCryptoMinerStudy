	<script type="text/javascript">
        // 2017.01.23 cho yong dong append for mobile move
        var uAgent = navigator.userAgent.toLowerCase();
        //< 모바일 기기 추가시 등록
        var mobilePhones = new Array('iphone', 'ipod', 'android', 'blackberry', 'windows ce', 'nokia', 'webos', 'opera mini', 'sonyericsson', 'opera mobi', 'iemobile', 'windows phone');
        var isMobile = false;
        for( var i = 0 ; i < mobilePhones.length ; ++i )
        {
            if( uAgent.indexOf(mobilePhones[i]) > -1)
            {
                isMobile = true;
                break;
            }
        }

        //< 모바일인 경우
        // 쿠키에서 pc_check = OK 가 아니면 모바일로 보낸다.
        if( isMobile )
        {
            var vPCOk = getCook("pc_check");
            if(vPCOk!="OK"){
                //alert("mobile 페이지로 이동 합니다.");
                location.href = "http://www.foodsafetykorea.go.kr/portalmobile/main.do";
            }
        }


        // 2017.02.01 cho yong dong append
        function getCook(vName){
            //alert("all cookie="+document.cookie);
            var y;
            var ca = document.cookie.split(';');
            for(var i=0; i< ca.length;i++){
                var c = ca[i].substr(0,ca[i].indexOf("="));
                y=ca[i].substr(ca[i].indexOf("=")+1);
                c=c.replace(/^\s+|\s+$/g,"");
                if(c==vName){
                    return unescape(y);
                }

            }

            return null;
        }

	</script>
