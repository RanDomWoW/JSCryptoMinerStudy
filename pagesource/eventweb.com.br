<!-- INICIO FEEDBACK -->

 <script type="text/javascript">
       $(function() {

            var feedbackTab = {

                speed:300,
                containerWidth:$('.feedback-panel').outerWidth(),
                containerHeight:$('.feedback-panel').outerHeight(),
                tabWidth:$('.feedback-tab').outerWidth(),

                init:function(){
                    $('.feedback-panel').css('height',feedbackTab.containerHeight + 'px');

                    $('a.feedback-tab').click(function(event){

                        if ($('.feedback-panel').hasClass('aberto')) {
                            $('.feedback-panel').animate({left:'-' + feedbackTab.containerWidth}, feedbackTab.speed)
                            .removeClass('aberto');
                        } else {
                            $('.feedback-panel').animate({left:'0'},  feedbackTab.speed)
                            .addClass('aberto');
                        }
                        event.preventDefault();
                    });
                }
            };

            feedbackTab.init();

            $(".button").click(function() {
                if ($("#form-feedback").valid()) {
                    var destino = $("select#destino").val();
                    var ip = $("input#ip").val();
                    var email_user = $("input#email_user").val();
                    var browser = $("input#browser").val();
                    var message = $("textarea#message").val();
                    var path = $("input#path").val();
                    var response_message = "Obrigado por enviar seu comentário";
                    var palavra = $("input#palavra").val();

                    var dataString = 'destino=' + destino + '&email_user=' + email_user + '&message=' + message + '&ip=' + ip + '&browser=' + browser + '&path=' + path + '&palavra=' + palavra;
                    $.ajax({
                        type: "POST",
//                  url: "http://www.eventweb.com.br//home-event/feedback-action.php",
                        url: "//home-event/feedback-action.php",
                        contentType: "application/x-www-form-urlencoded; charset=ISO-8859-1",
                        data: dataString,
                        beforeSend: function() {
                            $('.feedback-panel').animate({left:'-' + feedbackTab.containerWidth}, feedbackTab.speed)
                                .removeClass('open');
                            popupMsg('Enviando comentário', '<img src="/css-default/imgs/ajax-loader.gif">');
                        },

                        success: function(resultado) {
                            if (resultado==1)
                                popupMsg('Comentário enviado', 'Obrigado por enviar seu comentário');
                            else if (resultado==2)
                                popupMsg('Erro no envio do comentário', 'código de verificação incorreto.');
                            else
                                popupMsg('Erro no envio do comentário', 'comentário não pode estar vazio');

                            $('#message').val('');
                            $('#palavra').val('');
                                                        $('#email_user').val('');
                                                    }
                    });
                }
                return false;
            });
			// Prevenir duplo click em botï¿½o submit
		/*	$('form').submit(function(){
				$(':submit', this).click(function() {
					if ($(this).attr("class") == "anti_duplo_clique") {
						$(this).val('Please wait...');
						return false;
					}
				});
			}); */
        });
    </script>



    <script>
	var numero_real_ctrl = false;

    //permitir somente numeros usar atributo tipo="somente_numero"
    $(document).ready(function(){


    $("input").keydown(function(event) {

       if ($(this).attr("tipo") == "somente_numero"){
           if(event.shiftKey)
                event.preventDefault();
           if (event.keyCode == 46 || event.keyCode == 8 || event.keyCode == 9) {
           }
           else {
                if (event.keyCode < 95) {
                  if (event.keyCode < 48 || event.keyCode > 57) {
                        event.preventDefault();
                  }
                }
                else {
                      if (event.keyCode < 96 || event.keyCode > 105) {
                          event.preventDefault();
                      }
                }
              }
        }

	if ($(this).attr("tipo") == "numero_real"){
		if(event.keyCode == 17) {
			numero_real_ctrl = true;
		} else if (numero_real_ctrl == true && event.keyCode == 86) {
			numero_real_ctrl = false;
		} else {
			if(event.shiftKey)
			event.preventDefault();
			if((event.keyCode == 188 || event.keyCode == 190) && $(this).val().length >= 1){
				var controle = $(this).val();
				if(controle.indexOf(",") == -1 && controle.indexOf(".") == -1){
					$(this).val($(this).val()+event);
				}
			}
			if (event.keyCode == 46 || event.keyCode == 8 || event.keyCode == 9) {
			} else {
				if (event.keyCode < 95) {
					if (event.keyCode < 48 || event.keyCode > 57) {
						event.preventDefault();
					}
				} else {
					if (event.keyCode < 96 || event.keyCode > 105) {
						event.preventDefault();
					}
				}
			}
		}
	}

        if ($(this).attr("tipo") == "numero_ponto"){
           if(event.shiftKey)
                event.preventDefault();

           if((event.keyCode == 190) && $(this).val().length >= 1){
                var controle = $(this).val();
                if(controle.indexOf(".") == -1){
                    $(this).val($(this).val()+event);
                }
           }
           if (event.keyCode == 46 || event.keyCode == 8 || event.keyCode == 9) {
           } else {
                if (event.keyCode < 95) {
                  if (event.keyCode < 48 || event.keyCode > 57) {
                        event.preventDefault();
                  }
                }
                else {
                      if (event.keyCode < 96 || event.keyCode > 105) {
                          event.preventDefault();
                      }
                }
              }
        }
      });
    });


    </script>

    <script type="text/javascript">
        var CheckMaximo = 3;


    // limita a seleção de checkbox (topicos), neste caso em 3 em caso da multi seleção estiver ativa.
        function verificar() {
            var Marcados = 1;
            var objCheck = document.forms['submission-step1'].elements['topicos_multi'];

            //Percorrendo os checks para ver quantos foram selecionados:
            for (var iLoop = 0; iLoop<objCheck.length; iLoop++) {
                //Se o número máximo de checkboxes ainda não tiver sido atingido, continua a verificação:
                if (Marcados <= CheckMaximo) {
                    if (objCheck[iLoop].checked) {
                        Marcados++;
                    }
                    //Habilitando todos os checkboxes, pois o máximo ainda não foi alcançado.
                    for (var i=0; i<objCheck.length; i++) {
                        objCheck[i].disabled = false;
                    }
                    //Caso contrário, desabilitar o checkbox;
                    //Nesse caso, é necessário percorrer todas as opções novamente, desabilitando as não checadas;
                } else {
                    for (var i=0; i<objCheck.length; i++) {
                        if(objCheck[i].checked == false) {
                            objCheck[i].disabled = true;
                        }
                    }
                }
            }
        }
    </script>

