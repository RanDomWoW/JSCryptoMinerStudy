  <script>
    var landing_products_array = [];
    
    
    
    
      
    
      
    
      
    

    var selectCallback = function(variant, selector) {

      var $stickerContainer = $('.sticker-container').empty();

      timber.productPage({
        money_format: "${{amount}}",
        variant: variant,
        selector: selector
      });

      for (var i=0; i<landing_products_array.length;i++) {
        if ( variant.title === landing_products_array[i].title && landing_products_array[i].sticker_front != '' ) {
          var $sticker = $('<div>', {
            class: 'sticker'
          }),
          $stickerFront = $('<div>', {
            class: 'sticker-front',
            html: '<img src="' + landing_products_array[i].sticker_front + '" />',
            click: function() {
              $(this).parent().addClass('flipped');
            }
          }),
          $backImageWithLink = $('<a>', {
            href: landing_products_array[i].link,
            html: '<img src="' + landing_products_array[i].sticker_back + '" />'
          }),
          $stickerBack = $('<div>', {
            class: 'sticker-back'
          });

          $stickerBack.append($backImageWithLink);
          $sticker.append($stickerFront, $stickerBack);
          $stickerContainer.append($sticker);
          return;
        } else {
          $stickerContainer.empty();
        }
      }

    };
    
