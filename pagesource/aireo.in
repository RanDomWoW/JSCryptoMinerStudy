	function ycal(txtid,cal1,contin,mdate,mdtid)
	{
			var calendar = new YAHOO.widget.CalendarGroup(cal1,contin, { pages:3, title:"Please select date", close:true,mindate: mdate } );
			calendar.render();

			calendar.myview=function()
			{
				if (mdtid!="")
				{
					var txtDate = document.getElementById(mdtid).value; 

	        var dateParts = txtDate.split("/");
					var cdstr = parseInt(dateParts[1],10) + "/" + parseInt(dateParts[0],10) + "/" + parseInt(dateParts[2],10);
					var pgstr = parseInt(dateParts[1],10) + "/" + parseInt(dateParts[2],10);
	        
					calendar.cfg.setProperty("pagedate",pgstr,false);
					calendar.cfg.setProperty("mindate",cdstr,false);
					calendar.render();
				} 
				calendar.show();
			};

			// Listener to show the 2 page Calendar when the button is clicked
			YAHOO.util.Event.addListener(txtid, "focus", calendar.myview, calendar, true);

			calendar.selectEvent.subscribe(function() 
			{
	        if (calendar.getSelectedDates().length > 0) 
					{
	          var selDate = calendar.getSelectedDates()[0];
						var txtDate = document.getElementById(txtid); 
	          txtDate.value = (selDate.getDate()<10 ? '0' : '' ) + selDate.getDate() + "/" + (selDate.getMonth()<9 ? '0' : '' ) + (selDate.getMonth()+1) + "/" + selDate.getFullYear();
	        } 
	        calendar.hide();
			});
	}

	</script>
