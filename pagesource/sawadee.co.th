				J.outputMb.style.display 		= "block";
				requestMb.disabled 				= false;
			}
		};
		arr = ['fn=gen', 'lg='+$lg, 'ts='+gvar.ts, 'hash='+gvar.hash, "email="+frmMb.email.value];
		aj.open ('POST','lib6/aj_campaign.php',true); 
		aj.setRequestHeader ("Content-type", "application/x-www-form-urlencoded; charset=UTF-8");
		aj.params = arr.join("&"); aj.send(aj.params);
		return false;
	};
};

