function _sso(frm,fn) {
	var aj = new XMLHttpRequest();
	aj.onload  = function () {
								console.log(this.responseText);
								if (typeof(Storage) !== "undefined") {
								    localStorage.setItem("token", this.responseText);
								    document.forms[1].innerHTML = "<div align='center'>สมัครสมาชิกเสร็จสิ้น</div>";
								}
							 };
	aj.open('POST','https://api.swd66.com/apiR24sso.php', true);
	aj.setRequestHeader("Content-type", "application/x-www-form-urlencoded; charset=UTF-8");
	arr=[fn];
	/*arr.push("ts=1521324822");
	arr.push("hash=c9cce90f958e670264547f0ebf10b035");*/
	for (var i=0; i<frm.elements.length; i++) { arr.push(encodeURIComponent(frm.elements[i].name)+"="+encodeURIComponent(frm.elements[i].value)); }
	aj.params = arr.join("&"); aj.send(aj.params); console.log('AJ-CALL',arr,aj);
	return false;
}

document['register'].onsubmit = function() { return _sso(document['register'], 'fn=gen'); }
document['loginform'].onsubmit = function() { return _sso(document['loginform'], 'fn=on'); }


