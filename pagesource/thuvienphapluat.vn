<script type="text/javascript">
    $(document).ready(function () {
        $(".xcHeader span").click(function myfunction() {
            $(".xinchao").remove();
        });

        $('.xcbodyBottomMS').click(function (event) {
            if ($(".xctxtEmail").val() == "") {
                $(".xcdivEmail").show();
                $(".xctxtHoten").show();
            }
        });

        $('.xcbodyBottomMS, .xctxtEmail, .xctxtHoten').keypress(function (event) {
            if (event.keyCode == 13) {
                if ($(".xctxtHoten").val() == "") {
                    $(".xctxtHoten").focus();
                    $(".xctxtHoten").addClass("xcdivEmailactive");
                    return false;
                }
                else if ($(".xctxtEmail").val() == "") {
                    $(".xctxtEmail").focus();
                    $(".xcdivEmail").addClass("xcdivEmailactive");
                    return false;
                }
                if ($(".xcbodyBottomMS").val() == "") {
                    $(".xcbodyBottomMS").focus();
                    return false;
                }
                if ($(".xctxtEmail").attr("islg") == "false") {
                    $.ajax({ 'url': '/page/ajaxcontroler.aspx',
                        'data': "xctxtEmail=" + $(".xctxtEmail").val() + '&action=CheckEmailOrPhone',
                        'type': 'POST',
                        success: function (response) {
                            if (response == "error") {
                                $(".mserror").html("Nhập đúng Email của bạn");
                                $(".mserror").show();
                                $(".xctxtEmail").focus();
                                return false;
                            }
                            else {
                                var ms = $(".xcbodyBottomMS").val();
                                $zopim.livechat.setName($(".xctxtHoten").val());
                                $zopim.livechat.setEmail($(".xctxtEmail").val());
                                $(".xinchao").remove();
                                $zopim.livechat.say(ms);
                                return false;
                            }
                        },
                        error: function (response) {
                            return false;
                        }
                    });
                }
                else {
                    var ms = $(".xcbodyBottomMS").val();
                    $zopim.livechat.setName($(".xctxtHoten").val());
                    $zopim.livechat.setEmail($(".xctxtEmail").val());
                    $(".xinchao").remove();
                    $zopim.livechat.say(ms);
                    return false;
                }
                return false;
            }
        });
    });
</script>

