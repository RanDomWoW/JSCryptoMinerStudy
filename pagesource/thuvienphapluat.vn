    <script type="text/javascript">
        $(document).ready(function () {
            $("#changeMobile-dialog-form").dialog({
                autoOpen: false,
                height: "auto",
                width: 300,
                resizable: false,
                modal: true,
                buttons: {
                    "Cập nhật": function () {
                        var bValid = true;
                        $("#cpPhone").removeClass("ui-state-error");
                        var params = $("#FrchangeMobile").serialize();
                        if (bValid) {

                            $.ajax({ 'url': '/page/ajaxcontroler.aspx',
                                'data': params + '&action=ChangeMobile',
                                'type': 'POST',
                                success: fnSend,
                                error: function (response) {
                                    alert("Quá trình cập nhật gặp sự cố. Mời bạn thử lại trong giây lát!")
                                }
                            });

                        }
                    },
                    "Thoát": function () {

                        var d = new Date();
                        $.cookies.set("changeMobile", "1", { expiresAt: new Date(d.getFullYear(), d.getMonth(), d.getDate() + 1) });
                        $(this).dialog("close");
                    }
                },
                close: function () {
                    var d = new Date();
                    $.cookies.set("changeMobile", "1", { expiresAt: new Date(d.getFullYear(), d.getMonth(), d.getDate() + 1) });
                    $("#cpPhone").removeClass("ui-state-error");
                }
            });

            if ($.cookies.get("changeMobile") != '1' && $.cookies.get("changeMobile") != '2') {
                $("#changeMobile-dialog-form").dialog("open");
            }
            else if ($.cookies.get("changeMobile") == '2') {
                $(".IsExpire_login").hide();
            }
            $("#changeMobileClick").click(function () {
                $("#changeMobile-dialog-form").dialog("open");
                return false;
            });


        });

        function fnSend(response) {
            if (response == "<ok>" || response == "") {
                alert("Cập nhật thành công.");
                $("#cpPhone").val("");
                $("#changeMobile-dialog-form").dialog("close");
                $(".IsExpire_login").hide();
                var d = new Date();
                $.cookies.set("changeMobile", "2", { expiresAt: new Date(d.getFullYear(), d.getMonth(), d.getDate() + 1) });
            }
            else {
                alert(response);
                var d = new Date();
                $.cookies.set("changeMobile", "1", { expiresAt: new Date(d.getFullYear(), d.getMonth(), d.getDate() + 1) });
            }
        }



         
       

       

    </script>
