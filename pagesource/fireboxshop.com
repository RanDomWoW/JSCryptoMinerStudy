});
$(document).ready(function(){
	fixed_nav();
	$(window).scroll(function() {
		if(window.innerWidth >= 992){
            fixed_nav();
        }
	});
});
function fixed_nav(){
	var top = $(document).scrollTop();
	if($(".bl-msg_club--wrapper").length && $(".bl-action_timer--wrapper").length)
		var limit_top = 188;
	else if($(".bl-msg_club--wrapper").length)
		var limit_top = 138;
	else if($(".bl-action_timer--wrapper").length)
		var limit_top = 104;
	else
		var limit_top = 64;
	if((top > limit_top)&&(window.innerWidth >= 1200)){
		if($("header").hasClass("fixed") === false){
			if($(".bl-sub_menu--wrapper").hasClass("active") === true && $(".bl-sub_menu--wrapper").hasClass("over") === false){
				var mtop = 164;
			}
			else
				var mtop = 114;

			$(".bl-sub_menu--wrapper").removeClass("active");

			if(mtop == 114){
				$('div#content').addClass('mt114');
				$('div#main').addClass('mt114')
			}else{
				$('div#content').addClass('mt164');
				$('div#main').addClass('mt164')
			}
		}
		$("header").addClass("fixed");
	}else{
		$(".bl-main_menu-wrapper nav div.active").trigger("click");
		$('div#content').removeClass('mt164').removeClass('mt114');
		$('div#main').removeClass('mt164').removeClass('mt114');
		$("header").removeClass("fixed");
	}
}
</script>
