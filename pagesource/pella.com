leadSubmit: function (leadSource) {
pella.cpleadgen.leadSource = leadSource;
var userEmail = $('#leadGenEmailBox1343851622437').val(),
userZip = $('#leadGenZipBox1343851622437').val(),
isValid = pella.cpleadgen.validateEmail(userEmail),
isZipValid = pella.cpleadgen.validateZip(userZip);
if (isValid && isZipValid) {
$('.leadGenEmailError').css('display', 'none');
$('.leadGenEmailError').css('visibility', 'hidden');
//hide the submit form
$('.leadgen-form .middle').hide();
//show the sending email message
$('.sendingEmailMsg').css('display', 'block');
$('.sendingEmailMsg').css('visibility', 'visible');
//dataLayer.push({ 'event': 'definedEvent', 'eventCategory': 'Pre-Lead Footer', 'eventAction': 'Email Sign Up', 'eventLabel': 'Footer' });
//submit email
pella.cpleadgen.submit('#leadGenEmailBox1343851622437', '#leadGenZipBox1343851622437');
} else {
if (isValid){
$('#leadGenZipBox1343851622437').css('border', 'red 1px solid');
$('#leadGenEmailBox1343851622437').css('border', '#ccc 1px solid');
console.log("here1");
}
else if (isZipValid){
$('#leadGenEmailBox1343851622437').css('border', 'red 1px solid');
$('#leadGenZipBox1343851622437').css('border', '#ccc 1px solid');
console.log("here2");
}
else {
$('#leadGenEmailBox1343851622437').css('border', 'red 1px solid');
$('#leadGenZipBox1343851622437').css('border', 'red 1px solid');
console.log("here3");
}
$('.leadGenEmailError').css('display', 'block');
$('.leadGenEmailError').css('visibility', 'visible');
// $('.leadGenEmailError').css('margin', '0');
// $('.leadGenEmailError').css('padding', '4px 50px 6px 600px');
return false;
}
},
submit: function (textSelector, zipSelector) {
try {
var url = pella.cpleadgen.service;
if(zipSelector === ''){
url += "?e=" + $(textSelector).val();
if (pella.zCookie.getCookie("PRESHUSS") != null){
url += "&p=" + eval(pella.zCookie.getCookie("PRESHUSS"))[0].PostalCode;
}
else{
url += "&p=";
}
url += "&s=" + pella.cpleadgen.leadSource;
}
else{
url += "?e=" + $(textSelector).val() + "&p=" + $(zipSelector).val() + "&s=" + pella.cpleadgen.leadSource;
}
$.get(url+"&"+pella.cpleadgen.queryString, function (data) {
if (data.success) {
pella.cpleadgen.hasSignedUp = true;
pella.zCookie.setCookie("prelead_hasSignedUp", true, 500);
// GATracker('Main', 'Click', 'Email Signup');
switch (pella.cpleadgen.leadSource) {
case "Footer":
//$(".sendingEmailMsg").hide();
$('.sendingEmailMsg').css('display', 'none');
$('.sendingEmailMsg').css('visibility', 'hidden');
$(".leadgen-form .success").show();
break;
default:
break;
}
return true;
}
else {
//handle for error messaging
switch (pella.cpleadgen.leadSource) {
case "Footer":
$('.leadgen-form #leadGenEmailBox1343851622437').css('border', 'red 1px solid');
$(".leadgen-form .middle").show();
break;
default:
break;
}
return true;
}
});
}
catch (ex) {
//alert(ex);
}
return false;
}
}
// END pre-lead email
});
