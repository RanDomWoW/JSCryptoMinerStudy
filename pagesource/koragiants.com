/* Main Menu */
#cssmenu,#cssmenu #menu-button,#cssmenu ul,#cssmenu ul li,#cssmenu ul li a{margin:0;padding:0;border:0;list-style:none;display:block;position:relative}
#cssmenu:after,#cssmenu > ul:after{content:".";display:block;clear:both;visibility:hidden;line-height:0;height:0}
#cssmenu #menu-button{display:none}
#cssmenu{margin:auto;width:100%;font-family:'Droid Arabic Kufi'}
.cssmenu-ul{background:#fff}
#cssmenu ul{margin:auto;padding:0}
#cssmenu > ul > li{float:right}
#cssmenu.align-center > ul{font-size:0;text-align:center}
#cssmenu.align-center > ul > li{display:inline-block;float:none}
#cssmenu.align-center ul ul{text-align:right}
#cssmenu.align-right > ul > li{float:right}
#cssmenu > ul > li > a{padding:0 20px;font-size:13px;text-decoration:none;color:RGB(51,51,51);text-transform:uppercase;transition:all .3s;height:60px;line-height:60px;letter-spacing:-0.5px;font-weight:700}
#cssmenu > ul > li > a.actv{background:#00BE2E;color:#fff}
#cssmenu > ul > li:hover > a{color:#00BE2E}
#cssmenu > ul > li:hover > a.actv{background:#333;color:#fff}
#cssmenu > ul > li.has-sub > a{padding-left:30px}
#cssmenu > ul > li.has-sub > a:after{content:'';position:absolute;top:31px;left:11px;width:8px;height:2px;display:block;background:#333}
#cssmenu > ul > li.has-sub > a:before{content:'';position:absolute;top:28px;left:14px;display:block;width:2px;height:8px;background:#333;transition:all .25s ease}
#cssmenu > ul > li.has-sub:hover > a:before{top:23px;height:0}
#cssmenu ul ul{position:absolute;left:-9999px;z-index:2}
#cssmenu.align-right ul ul{text-align:right}
#cssmenu ul ul li{background:#333;height:0;transition:all .25s ease}
#cssmenu li:hover > ul{left:auto}
#cssmenu.align-right li:hover > ul{left:auto;right:0}
#cssmenu li:hover > ul > li{height:60px}
#cssmenu ul ul ul{margin-right:100%;top:0}
#cssmenu.align-right ul ul ul{margin-left:0;margin-right:100%}
#cssmenu ul ul li a {
border-bottom: 1px solid rgba(150,150,150,.15);
padding: 10px 15px;
width: 170px;
font-size: 12px;
text-decoration: none;
color: #ddd;
font-weight: 400;
background: #333;
}
#cssmenu ul ul li a:hover{color:#fff}
#cssmenu ul ul li:last-child > a,#cssmenu ul ul li.last-item > a{border-bottom:0}
#cssmenu ul ul li:hover > a,#cssmenu ul ul li a:hover{color:#00BE2E}
#cssmenu ul ul li.has-sub > a:after{content:'';position:absolute;top:25px;left:15px;width:8px;height:2px;display:block;background:#ddd}
#cssmenu.align-right ul ul li.has-sub > a:after{right:auto;left:11px}
#cssmenu ul ul li.has-sub > a:before{content:'';position:absolute;top:22px;left:18px;display:block;width:2px;height:8px;background:#ddd;transition:all .25s ease}
#cssmenu.align-right ul ul li.has-sub > a:before{right:auto;left:14px}
#cssmenu ul ul > li.has-sub:hover > a:before{top:17px;height:0}
