 <script>
     $('#spot').carousel({
         interval: 6000 // in milliseconds  
     });

     var currentwidth = screenwidth();

     function screenwidth() {
         pwidth = $(window).width();
         if (pwidth >= 930) {
             return 2;
         }
         else if (pwidth >= 740) {
             return 1;
         }
         else {
             return 0;
         };
     }

     function navigation(pwidth) {
         if (pwidth == 2) {
             $('#mainmenu').on({
                 mouseenter: function () {
                     $('#mainmenu>li ul').stop(true).animate({ height: 450 }, 250);
                 },
                 mouseleave: function () {
                     $('#mainmenu>li ul').stop(true).animate({ height: 0 }, 250);
                 }
             });
         }
         else if (pwidth == 1) {
             $('.drop-down-toggle').on("click", function () {
                 $('.drop-down-toggle').css('display', 'none');
                 $('#mainmenu>li ul').stop(true).animate({ height: 480 }, 250);
             });

             $(document.body).on("click", function (e) {
                 if (e.target.id !== '#mainmenu' && $('#mainmenu ul').height() > 0) {
                     $('#mainmenu>li ul').stop(true).animate({ height: 0 }, 250);
                     $('.drop-down-toggle').removeAttr('style');
                 };
             });

         }
         else {
             $('.nav-toggle').on("click", function () {
                 if ($('#mainmenu').height() > 0) {
                     $('.selected').removeClass("selected");
                     $('.drop-down-toggle').removeAttr('style');
                     $('#mainmenu ul').removeAttr('style');
                 };
                 $('#mainmenu').slideToggle(250);
                 return false;
             });

             $('.drop-down-toggle').on("click", function () {
                 $('.selected').children('ul').slideToggle(250);
                 $('.selected').children('.drop-down-toggle').removeAttr('style');
                 $('.selected').removeClass("selected");
                 $(this).parent().children('ul').slideToggle(250);
                 if (!$(this).parent().hasClass("selected")) {
                     $(this).parent().addClass("selected");
                     $(this).css('display', 'none');
                 };
             });

             $(document.body).on("click", function (e) {
                 var $box = $('#mainmenu');
                 if ($(window).width() < 740 || $(document).width() < 740) {
                     if ((e.target.id !== '#mainmenu') && (e.target.id !== '.nav-toggle') && (!$.contains($box[0], e.target)) && ($('#mainmenu').css('display') != 'none')) {
                         $('#mainmenu').slideToggle(250);
                     };
                 };
             });
         };

     }


     $(window).resize(function () {
         if (screenwidth() == 2 || screenwidth() == 1) {
             $('#mainmenu').removeAttr('style');
         };
         if (currentwidth != screenwidth()) {
             $('#mainmenu').off();              //unbind eventhandlers
             $('#mainmenu ul').off();           //unbind eventhandlers
             $('#mainmenu>li ul').off();        //unbind eventhandlers
             $(document.body).off();            //unbind eventhandlers
             $('.drop-down-toggle').off();      //unbind eventhandlers
             $('.nav-toggle').off();            //unbind eventhandlers

             navigation(screenwidth());
             currentwidth = screenwidth();
             $('#mainmenu ul').removeAttr('style');
         };
     });


     $(document).ready(function () {
         $('#mainmenu li').each(function () {
             if ($(this).children('ul').length)
                 $(this).append('<span class="drop-down-toggle"></span>');
         });
         navigation(screenwidth());
         currentwidth = screenwidth();
     });
</script>

