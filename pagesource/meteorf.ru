    // выравнивает левую колонку по высоте с правой
    var refreshLeftColumn = function() {
        if ($('.mainRow .banners').length && $('.mainRow .newsroom').length) {
            var leftColumnHeight = $('.mainRow .banners').offset().top + $('.mainRow .banners').outerHeight();
            var rightColumnHeight = $('.info-board').height() + $('.info-board').offset().top,
                offset = Math.abs(leftColumnHeight - rightColumnHeight),
                newsRoom = $('.mainRow .newsroom'),
                marginDelta = parseInt($(".banners .row:last").css("margin-bottom"));

            if (rightColumnHeight >= leftColumnHeight) {
                newsRoom.height(newsRoom.height() + offset + marginDelta);
            }
            else {
                newsRoom.height(newsRoom.height() - offset + marginDelta);
            }
        }
    };

