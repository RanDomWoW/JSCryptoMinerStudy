    $live.on("click", ".tab-mode-nav li a", function(event) {
        event.preventDefault();
        var c = $(this).attr("href");
        var _this = this;
        var ntall = $(c).siblings();
        ntall.css("display", "none");
        $(c).css("display", "block");
        $(".nav-tabs-link").removeClass("selected");
        $(_this).addClass("selected");
       
        if ($(c).children(".xinzx-tiles").length == 0 || $(c).children(".xinzx-tiles").html() == "") {
            if (c == "#j_case") {
                $.ajax({
                    url: "anli.html",
