                        $(c).html(m);
                      //  navScroll(_this, c);
                    }
                });
            } else {
               // navScroll(this, c);
                // if($live.offset().top - $document.scrollTop() < 0) {
                // $document.scrollTop($(c).offset().top - 93);
                // }
            }
        } else {
            navScroll(this, c);
        }
    });
    $document.on("scroll", function() {
        var st = $live.offset().top - $document.scrollTop();
        if (st >= 0) {
            $navTabsFixed.hide();
            $navTabsFixed.children().appendTo($navTabs);
        } else {
            $navTabsFixed.show();
            $navTabs.children().appendTo($navTabsFixed);
        }
    });
    KISSY.use("node,button, dd,switchable, resizable,ua,event,overlay,json,ajax", function(S, Node, Button, DD, Switchable, Resizable, UA, Event, Overlay, JSON, Ajax) {
        var DDM = DD.DDM, DOM = S.DOM;
        S.ready(function() {
            S.all('.J_TWidget').each(function(v, k) {
                try {
                    var t = v.attr('data-widget-type'), c = v.attr('data-widget-config'), cfg = c ? S.JSON.parse(c.replace(/'/ig, '"')) : 0;
                    if (cfg) {
                        cfg.srcNode = v;
                        if (t == 'Slide') new Switchable.Slide(v, cfg);
                        else if (t == 'Carousel') new Switchable.Carousel(v, cfg);
                        else if (t == 'Popup') new Overlay.Popup(cfg);
                        else if (t == 'Tabs') new Switchable.Tabs(v, cfg);
                        else if (t == 'Accordion') new Switchable.Accordion(v, cfg);
                        else if (t == 'Countdown') new EasyCountdown(v, cfg.endTime, cfg);
                    }
                } catch (ex) {
                    console && console.error(ex);
                } finally {
                }
            });
        });
    });
</script>
