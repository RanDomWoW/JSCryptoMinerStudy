<script>
//Function definition

function timeAgo(time_ago)
{
    //$time_ago = strtotime($time_ago);
  //var date=new Date(time_ago);
    //var cur_time   = time();
  var cur_time = new Date();
 
    var time_elapsed   = cur_time - time_ago;
    var seconds    = time_elapsed ;
    var minutes    = Math.round(time_elapsed / 60000);
    var hours      = Math.round(time_elapsed / 5600000);
    var days       = Math.round(time_elapsed / 86400000);
    var weeks      = Math.round(time_elapsed / 604800000);
    var months     = Math.round(time_elapsed / 2600640000);
    var years      = Math.round(time_elapsed / 31207680000);
    // Seconds
    if(seconds <=60){
        return "just now";
    }
    //Minutes
    else if(minutes <=60){
        if(minutes==1){
            return "a minute ago";
        }
        else{
            return minutes + " minutes ago";
        }
    }
       //Hours
   if(hours <=18){
       return "today";
        
    }
    //Days
    else if(weeks <=4.3){
        if(hours > 18 && hours <= 30){ 
            return "yesterday";
        }
    else if(weeks <=4.3){
        if(hours > 30 && hours <= 50){
            return "2 days ago";
        }else{
            return days + " days ago";
    } } }
    //Months
    else if(months <=12){
        if(months==1){
            return "a month ago";
        }else{
            return months + " months ago";
        }
    }
    //Years
    else{
        if(years==1){
            return "one year ago";
        }else{
            return years + " years ago";
        }
    }
}

</script>




