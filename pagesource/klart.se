<script>
  (function() {
    var serializedState = localStorage.getItem('klart-app-state')
    var state = serializedState
      ? JSON.parse(serializedState)
      : {}

    if(state.position && state.position.place) {
      document.body.classList.add('current-position')
    }

    if(!state.favorites) {
      return
    }

    var favoritePlaces = state.favorites.items.filter(function (favorite) {
      return favorite.type !== 'coastalRegion'
    })

    if(favoritePlaces.length > 0) {
      document.body.classList.add('has-favorites')
    }

    var favorite = state.favorites.items.findIndex(function(favorite) {
        return favorite.id === klart.place.id
    }) !== -1

    if(favorite) {
      document.body.classList.add('favorite-position')
    }

  })()
</script>

  
  
    
    
      
