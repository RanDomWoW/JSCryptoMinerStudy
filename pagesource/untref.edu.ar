        function getEvents(date,dateLocal,day,month){
            $('.calendar-header').html(day+' '+month);
            var settings = {
                "async": true,
                url: 'http://untref.edu.ar/api/get-events-by-date',
                "method": "POST",
                "data": {date:date}
            };
            $.ajax(settings).success(function (response) {
                console.log(response);
                $('.calendar-event').html('');
                var str = '';
                var len = JSON.parse(response);
                if(len == 0){
                    str = "No se registran eventos para esta fecha";
                }else{


                $.each(JSON.parse(response),function(index,item){
                    var daytime = item.start_time.split(' ');
                    var time = '';
                    if(daytime[1].substring(0, 4) != "00:0"){
                    time = daytime[1]+'hs';
                    }
                     str += '<div class="calendar-event">'+
                        '<a href="http://untref.edu.ar/events/'+item.slug+'" target="_blank"><h4>'+item.title+'</h4></a>'+
                        '<p> '+time.substring(0, 5)+'</p>'+
                         '<hr class="post-separator">';
                        $('.next-events-container').html('');
                        $('.next-calendar-event').html('');

                });
                }
                $('.calendar-event').append(str);

            });

        }

