            var TodayDate = '2018-03-17 00:00:00';
            var splitdatespace = TodayDate.split(' ');
            var splitdate = splitdatespace[0].split('-');

            var months = ['Ene','Feb','Mar','Abr','May','Jun','Jul','Ago','Sep','Oct','Nov','Dic'];

            console.log(splitdate[0]);
            getEvents('2018-03-17 00:00:00',splitdate[2]+'-'+splitdate[1]+'-'+splitdate[0],splitdate[2],months[parseInt(splitdate[1])-1]);
