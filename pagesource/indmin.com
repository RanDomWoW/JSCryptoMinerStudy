<script type="text/javascript">
	function RegisterTopStoriesTicker(newsId, pagerId)
	{
		$(newsId).cycle({
			fx:				'scrollHorz',
			speed:			400,
			timeout:		11000,
			pager:			pagerId,
			pagerEvent:		'click',
			fastOnEvent:	true,
			pause:				1,
