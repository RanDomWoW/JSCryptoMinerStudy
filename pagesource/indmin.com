				interactionObj.customer_id = "";
				interactionObj.subscription_id = "";
			}

			if (categoryName) {
				interactionObj.category_name = categoryName;
			}

			if (subCategoryName) {
				interactionObj.sub_category_name = subCategoryName;
			}

