		LogInteraction: function (interactionName, interactionObj) {
			interactionObj = interactionObj || {};
			var date = new Date();
			date.setHours(date.getHours() + date.getTimezoneOffset() / 60);
			interactionObj.event = 'visitor interaction';
			interactionObj.interaction_name = interactionName;
			window.dataLayer = window.dataLayer || [];
			dataLayer.push(interactionObj);
		},

		LogCustomAction: function (actionName, fields) {
			var interactionObj = {};

			for (var i = 1, l = arguments.length; i < l; i++) {
				var keyValuePair = arguments[i].split(':');
				interactionObj[keyValuePair[0]] = keyValuePair[1];
			}

			this.LogInteraction(actionName, interactionObj);
		},

		LogSubscriptionClickEvent: function (sender, redirectLocation) {
			var subClick = {};
			subClick.subscription_button = sender;
			this.LogInteraction('subscription click', subClick);

			if (redirectLocation) {
				setTimeout(function () { window.location = redirectLocation; }, 300);
			}

			return true;
		},

		LogPriceBookResult: function (action, currentTab, priceName) {
			var priceBookResult = {};
			priceBookResult.pb_results_tab = currentTab;
			priceBookResult.pb_results_action = action;
			priceBookResult.pb_results_description = priceName;
			this.LogInteraction('price book results', priceBookResult);
		},


		LogVirtualPageView: function (pageType, pageName, categoryName, subCategoryName) {
			var interactionObj = {}

			interactionObj.event = "virtual page view";
			interactionObj.page_type = pageType;
			interactionObj.page_name = pageName;

			interactionObj.logged_in = "not logged in";
