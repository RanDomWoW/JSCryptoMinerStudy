		window.GoogleTagFormTracker = {
			InitializeFormTracking: function () {
				var inputs = $('.ga-tracking-form input[type="text"], .ga-tracking-form input[type="email"]');
				var selects = $('.ga-tracking-form select');
				var checkboxes = $('.ga-tracking-form input[type="checkbox"], .ga-tracking-form input[type="radio"]');
				var submit = $('.submit-form');
				this.currentForm = $('.ga-tracking-form').prop('id');

				inputs.focusout(function () {
					var fieldName = this.id.substr(this.id.lastIndexOf('_') + 1);
					var fieldVal = this.value;
					GoogleTagFormTracker.LogTracking(fieldName, fieldVal);
				});

				selects.change(function () {
					var fieldName = this.id.split('_');
					fieldName = fieldName[fieldName.length - 2];
					var fieldVal = $('#' + this.id + ' option:selected').text();

					if (this.multiple) {
						fieldVal = '';
						$('#' + this.id + ' option:selected').each(function () {
							fieldVal += $(this).text() + "|";
						});

						fieldVal = fieldVal.substring(0, fieldVal.length - 1);
					}

					GoogleTagFormTracker.LogTracking(fieldName, fieldVal);
				});

				checkboxes.change(function () {
					var fieldName;

					if (this.id.indexOf('selectAll') < 0) {
						fieldName = $('label[for="' + this.id + '"').text();
					} else {
						fieldName = this.id;
					}

					var fieldVal = this.checked;
					GoogleTagFormTracker.LogTracking(fieldName, fieldVal);
				});

				submit.click(function () {
					GoogleTagManagerLogger.LogCustomAction('form submit', 'form_track_form:' + GoogleTagFormTracker.currentForm);

					setTimeout(function () {
						var validationMessages = [];
						$('.validator').filter(function (index) { return $(this).css('visibility') !== 'hidden' && $(this).css('display') !== 'none' }).each(function () { validationMessages.push('* ' + $(this).text() + '\n|' + this.id); });

						$('.submit-details.fail .messages').empty();
						$(validationMessages).first(function () {
							var name = this.split('|')[1];
							var msg = this.split('|')[0];

							name = name.substring(name.lastIndexOf('_') + 1);
							GoogleTagManagerLogger.LogCustomAction('form error', 'val_error_form:' + GoogleTagFormTracker.currentForm, 'val_error_field:' + name, 'val_error_message:' + msg);
						});
					}, 500);
				});
			},

			LogTracking: function (field, value) {
				GoogleTagManagerLogger.LogCustomAction('form tracking', 'form_track_form:' + GoogleTagFormTracker.currentForm, 'form_track_field:' + field, 'form_track_value:' + value);
			}
		}
