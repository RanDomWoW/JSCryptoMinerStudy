    function redrawCity(val){
        var val = parseInt(val);
        document.getElementById("cityPanel").innerHTML='<span><a href="'+dataArr[val].href+'" target="_blank">'+dataArr[val].city+'</a></span>';
        $('.dsxw_con').css("display","none");
		$('.ttl_jn').css("backgroundImage","url(/static/2016/images/"+dataArr[val].bg+".jpg)");
        $('.dsxw_con:eq('+val+')').css("display","block");
    }
    $("#cities").change(function(){
        redrawCity($(this).children('option:selected').val());
    });
    function detectCity(cityInfo){
        var city = cityInfo.city;
        var flag = -1;
        for(var i = 0;i<dataArr.length;i++){
            if(dataArr[i].city==city){
                flag = i;
            }
        }
        if(flag != -1){
            $("#cities").get(0).selectedIndex = flag;
            redrawCity(flag);
        }
        
    }
