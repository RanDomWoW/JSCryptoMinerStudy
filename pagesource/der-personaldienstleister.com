			// TODO dblclick muss ersetzt werden weil dies in moile geraete nicht funktioniert
			$(".dropdown-toggle").dblclick(function() {
				var a_href = $(this).attr("href");
				window.location.href = a_href;
			});

			if ($(window).width() > 750) {
				$(".dropdown").mouseover(function() {

					$(this).addClass("open"); // add active class to clicked menu item

				});

				$(".dropdown").mouseleave(function() {
					$(this).removeClass("open"); // add active class to clicked menu item    
				});
			}
			$(".dropdown-toggle").click(function() {

				var a_href = $(this).attr("href");
				if ($(this).parent("li").hasClass("open")) {
					window.location.href = a_href;
				}

			});
		});
	});
