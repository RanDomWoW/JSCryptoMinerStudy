<!--career analytics code-->
<script type="text/javascript">

$(function() {
	
	var adhocReferrer = 0;
	
	var arr = document.URL.toLowerCase().match(/adhoc_referrer=([0-9]+)/);
	if(arr != null) {
		adhocReferrer = arr[1];			
	}
	
	if (adhocReferrer > 0) {				
		$('a').each(function () {
			var href = $(this).attr('href');
			if (this.host == location.host || (href !== undefined && href.indexOf('recruitmentservices.applicationtrack') > 0)) {				
				$(this).attr('href', LinkAmender.Amend($(this).attr('href'), adhocReferrer));
			}
		});
	}
		
});

var LinkAmender = {
	Amend: function (originalHref, adhocReferrer) {
		
		if (originalHref.toLowerCase().indexOf('adhoc_referrer') > 0 || originalHref.toLowerCase().indexOf('.pdf') > 0 || originalHref === '' || originalHref === '#') {
        	return originalHref;
        }	                
		
		var queryBegins = originalHref.indexOf('?') > 0 ? 'adhoc_referrer=' : '?adhoc_referrer=';
		
		var insertLocation = originalHref.indexOf('#');
		
		if (insertLocation > 0) {
            return originalHref = originalHref.substring(0, insertLocation) + queryBegins + adhocReferrer + originalHref.substring(insertLocation);
		}
		
        return originalHref = originalHref + queryBegins + adhocReferrer;
    }
};

</script>
