      googleInterval = setInterval(googleAdsFunction, 500);
      
      function googleAdsFunction() {
        if( window.googletag && googletag.apiReady ) {
          
          googleInterval && clearInterval(googleInterval);
          
          var windowWidth = $(window).width();
          var isDesktop   = windowWidth >= adsBreakpoint;
          var isMobile    = !isDesktop;
          
          loadedState     = isMobile ? 'mobile' : 'desktop';
          
          for(var i = 0; i < googleAds.length; i++) {
            
            var currentAd = googleAds[i];
            
            if(!currentAd.google) {
              continue;
            }
            
            if ( ((isDesktop && currentAd.desktop) || (isMobile && currentAd.mobile)) && (currentAd.width <= windowWidth) ) {
              
              $('#' + currentAd.id).closest('.banner').show();
              
              googletag.cmd.push((function(ad) {
                googletag.display(ad.id);
              })(currentAd));
              //console.log('push ' + currentAd.id);
            }
            else {
              $('#' + currentAd.id).closest('.banner').remove();
              //console.log('remove ' + currentAd.id);
            }
          }
        }
      }
      
      function onResizeEnd() {
        var windowWidth     = $(window).width();
        var isDesktop       = windowWidth >= adsBreakpoint;
        var isMobile        = !isDesktop;
        
        var currentState    = isMobile ? 'mobile' : 'desktop';
        
        if(loadedState != currentState) {
          window.location.reload();
        }
      }
      
      
      $(window).on('resize', function() {
        resizeFinished && clearTimeout(resizeFinished);
        
        resizeFinished = setTimeout(function() {
          onResizeEnd();
        }, 1000);
      });
      
      var windowWidth = $(window).width();
      var isDesktop   = windowWidth >= adsBreakpoint;
      var isMobile    = !isDesktop;
      
      loadedState     = isMobile ? 'mobile' : 'desktop';
      
      for(var i = 0; i < genericAds.length; i++) {
