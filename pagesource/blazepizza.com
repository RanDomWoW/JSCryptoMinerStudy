function validateEmail(emailAddress) {       /* NOTE:  This regular expression is identical to the one used by Enterprise for e-mail address validation */ 
	var regExp = /^\s*[a-zA-Z\d][a-zA-Z\d\.!#$%&'*+\-\/=?^_`{|}~]*@([a-zA-Z0-9]([a-zA-Z0-9\-]{0,61}[a-zA-Z0-9])?\.)+[a-zA-Z]{2,6}\s*$/; 
	if (emailAddress.length <= 200 && regExp.test(emailAddress)) { 
		return true; 
	} else { 
	 alert("Invalid e-mail address"); 
	  return false; 
	} 
}
function validatePhone(MobilePhone)
   {
   	if(MobilePhone == '') {
   		return true;
   	}
     var PhoneNumber = /^\(?([0-9]{3})\)?[-]?([0-9]{3})[-]?([0-9]{4})$/;
     if(PhoneNumber.test(MobilePhone))
     {
       return true;
	}  else  {
		 alert("Invalid Phone Number");
		 return false;
		 }
	} 

</script>
