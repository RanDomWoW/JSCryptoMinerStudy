<script language="javascript" type="text/javascript">
$(window).load(function(){
    
	$(window).scroll(function(){
        if($(window).height() >= $('#bannerLeft').height()){
		
			if($(window).scrollTop()>30){
				$('#bannerLeft, #bannerRight').css('position', 'fixed');
				$('#bannerLeft, #bannerRight').css('top', 0);
			} else {
				$('#bannerLeft, #bannerRight').css('position', 'absolute');
				$('#bannerLeft, #bannerRight').css('top', 30);
			}
			}else{
				if($(window).scrollTop()>(30+$('#bannerLeft').height()-$(window).height())){
					$('#bannerLeft, #bannerRight').css('position', 'fixed');
					$('#bannerLeft, #bannerRight').css('top', -$('#bannerLeft').height()+$(window).height());
			} else {
				$('#bannerLeft, #bannerRight').css('position', 'absolute');
				$('#bannerLeft, #bannerRight').css('top', 30);
			}
		}
    });
	
	SetBannerPos ();
	$(window).resize(function () {
		SetBannerPos ();
	});
});

function SetBannerPos () {
	$('#bannerLeft').css('left', (parseInt($("#content").offset().left)-parseInt($('#bannerLeft').css('width'))-5) + 'px');	
	$('#bannerRight').css('left', (parseInt($("#content").offset().left)+parseInt($("#content").width())+38) + 'px');
	$('#bannerLeft').show();
	$('#bannerRight').show();
}
</script>

