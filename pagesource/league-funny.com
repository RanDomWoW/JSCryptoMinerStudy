<script>
var f_rpt;
var report_fid;

$( "#report_feed_rect .closeBox" ).click(function() {
	$("#report_feed_rect").hide();
	f_rpt = $( "#report_feed_rect" ).detach();
	return false;
});

f_rpt = $( "#report_feed_rect" ).detach();

$(".report").click(function() {

	reportFid($(this).attr("fid"));
	return false;
});

$(".card .up, .card .down").click(function() {

	var box_parent = $(this).closest(".card");
	var fid = box_parent.attr("fid");
	var act;

	if ($(this).hasClass("down")) {
		act = "down";
	} else if($(this).hasClass("up")) {
		act = "up";
	} else {
		return;
	}


	$.ajax({
		type: "post",
		url: "ajax/up_down.php",
		data :{fid: fid , act: act},
		dataType: "jsonp",
		success: function(json) {
			if (json.error) {

				if (json.error == 'login') {
					login();
				} else {
					halert(json.error, "error");
				}
			} else {

				box_parent.find(".up ,.down").removeClass("add");
				box_parent.find(".data span").html(json.up_down_info['f_up_down']);
				box_parent.find(".data").attr("class", "data");

				if (json['status']) {

					box_parent.find(".data").addClass(act);
					box_parent.find("."+act).addClass("add");
				}


				if (json.getcoinstatus) {

					halert(json.msg,json.getcoinstatus, '', 1);

					setTimeout(function() {
						location.href = "article-"+fid;
					}, 4000);
				}
			}
		},
		error: function() {
			console.log('fail');
		}
	});

	return false;
});

$(".delete").click(function() {
	var fid = $(this).attr('fid');
	if (fid) {
		del_post(fid);
	}

	return false;
});

$(".dissticky").click(function() {
	var fid = $(this).attr('fid');
	if (fid) {
		dissticky_post(fid);
	}
	return false;
});

$(".tosticky").click(function() {
	var fid = $(this).attr('fid');
	if (fid) {
		tosticky_post(fid);
	}
	return false;
});

$(".favorite").click(function() {
	var fid = $(this).attr('rel');
	var this_favorite = $(this);
	var act;

	if ($(this).hasClass("add")) {
		act = "";
	} else {
		act = "add";
	}

	$.ajax({
		type: "post",
		url: "ajax/collect.php",
		data: {fid: fid, act: act},
		dataType: "jsonp",
		success: function(json) {
			if (json.error) {
				if (json.error=='login') {
					login();
				} else {
					halert(json.error,"error");
				}
			} else {
				if (act == 'add') {
					this_favorite.addClass("add");
				} else {
					this_favorite.removeClass("add");
				}
			}
		},
		error: function() {
			console.log('fail');
		}
	});

	return false;
});

function reportFid(fid){

	if (f_rpt) {

		report_fid = fid;

		f_rpt.appendTo( ".report[fid="+fid+"]");

		$( "#report_feed_rect").show();

		f_rpt = null;	

	}
}
function report_feed(fid, reason) {

	if (fid == 0) {
		fid = report_fid;
	}

	$.ajax({
		type: "post",
		url: "ajax/report.php",
		data :{fid : fid , reason : reason},
		dataType: "jsonp",
		success: function(json) {

			if (json.error) {

				if (json.error == 'login')
					login();
				else
					halert(json.error, "error");
			} else {

				halert("檢舉成功");
			}
			f_rpt = $( "#report_feed_rect" ).detach();
			
		},
		error: function() {
			console.log('fail');
		}
	});

	return false;
}
</script>
