//twitch follow or pid follow	
var follow_act ;
var follow_twitchid ;
var token = "";

$(".follow[pid]").click(function () {

	follow_twitchid = $(this).attr("twitchid");
	follow_act = $(this).hasClass("unfollow") ? "unfollow" : "follow";
	var pid = $(this).attr("pid");

	if (follow_twitchid) {
		if (token) {
			twitch_follow_unfollow(follow_act,follow_twitchid);
		} else {
			twitchlogin();
		}
	} else {
		follow_unfollow(follow_act,pid);
	}
		
	return false;
});

function twitch_follow_unfollow () {

	if (follow_act=='follow') {
		$(".follow[twitchid="+follow_twitchid+"]").addClass("unfollow");
	} else if (follow_act=='unfollow') {
		$(".follow[twitchid="+follow_twitchid+"]").removeClass("unfollow");
	}

	$.ajax({
		type: "POST",
		dataType: "json",
		url: "ajax/twitch_follow_unfollow.php",
		data: {channel:follow_twitchid,act:follow_act},
		success: function( response ) { 
			
			if (response.error) {
				halert(response.error,"error");
			}
		}		
	});
}
// if not twitch
function follow_unfollow(act,pid){

	if(act=='follow'){
		$(".follow[pid="+pid+"]").addClass("unfollow");
	}else if(act=='unfollow'){
		$(".follow[pid="+pid+"]").removeClass("unfollow");
	}

	$.ajax({
		type: "POST",
		dataType: "json",
		url: "ajax/pid_follow_unfollow.php",
		data: {pid:pid,act:act},
		success: function( response ) {
			if(response.error){
				halert(response.error,"error");
			}
		}
	});
}

function twitchlogin(){
	window.open("twitch_login.php", "遊戲大亂鬥 Twitch 登入", "width=400, height=600");
}

function twitchislogin(b){

	token = b;

	if(follow_twitchid && follow_act){

		twitch_follow_unfollow();

	}else{

		halert("串接twitch完成");

		setTimeout(function(){
			parent.location.reload();
		},1000);
	}
}

