			$(".review.back").click();
			$("#mail_pic").click();
		}
	}

	}).children(".close_photo").click(function(){
		$("#mail_photo .img").hide();
		$("#mail_photo .close_photo").hide();
		$("#mail_photo .img img").attr("src","img/loading_spinner.gif");
		$("#mail_photo .img img").attr("_src","");
	});

	$('#mail_pic').change(function() { 
		// select the form and submit
		$('#mail_pic_form').submit(); 
		$('#mail_pic').val('');
	});  

	$("#mail_pic_form").on('submit',(function(e) {
		e.preventDefault();
		$("#mail_photo .img").show();
		$("#mail_photo .close_photo").show();
			
		$.ajax({
		url: "ajax/upload_photo_imgur.php",
		type: "POST",
		data:  new FormData(this),
		contentType: false,
		cache: false,
		processData:false,
		dataType: "json",
		success: function(data){
			console.log(data);
			if (data.error) {
				if(data.error=='login'){
					login();
				}else{
					halert(data.error,"error");
				}

			} else {

				var caretpos =  caretPos("mail_content");
				var strBegin = $('#mail_content').val().substring(0, caretpos);
				var strEnd   = $('#mail_content').val().substring(caretpos);
						
				if ($('#mail_content').val()=='') {

					$('#mail_content').val(data.attach['small']);

				} else {
						
					if (strBegin) {

						if (strEnd) {

							$('#mail_content').val( strBegin + "\r\n" + data.attach['small'] + "\r\n" + strEnd);

						} else {

							$('#mail_content').val( strBegin + "\r\n" + data.attach['small']);

						}

					} else {
						if (strEnd) {
							$('#mail_content').val(data.attach['small'] + "\r\n" + strEnd);
						}else{
							$('#mail_content').val(data.attach['small']);
						}
					}
				}
						
				$("#mail_photo .img").hide();
				$("#mail_photo .close_photo").hide();

			}
		},
		error: function() {}           
	});
}));
