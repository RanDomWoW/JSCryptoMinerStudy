#cb-previous-next-links #cb-previous-link a, #cb-previous-next-links #cb-next-link a, #cb-previous-next-links .cb-empty a{font-family:Sahel}

#cb-nav-bar.cb-dark-menu #cb-main-menu .main-nav li a{
    color: #ffffff;
    font-family: Sahel !important;
    font-size: 14px !important;
}

.entry-content blockquote{border-right: 3px solid #8f8f8f;color: #000000; font-weight: 100; background: #f7f7f7;border-radius: 9px 0px 0px 9px;}
.entry-content blockquote p { font-size: 15px;font-style:normal;color: #3e3e3e}

.entry-content, .dataTables_wrapper, .tablepress, #cb-footer, .cb-excerpt, .cb-module-header p {
    font-family: Sahel ;
    font-size: 15px;
}



.cb-fis .cb-single-title{
    font-family: Sahel !important; 
    border-bottom: 1px solid #d0cdcd;
    margin-bottom: 3px;
    font-size: 18px;
}

.apss-theme-6 .apss-single-icon a:before{border-radius: 40px;transform: rotate(0deg) !important;}

h1, h2, h3, h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6, #cb-nav-bar #cb-main-menu ul li > a, .cb-author-posts-count, .cb-author-title, .cb-author-position, .search .s, .cb-review-box .cb-bar, .cb-review-box .cb-score-box, .cb-review-box .cb-title, #cb-review-title, .cb-title-subtle, #cb-top-menu a, .tabbernav, #cb-next-link a, #cb-previous-link a, .cb-review-ext-box .cb-score, .tipper-positioner, .cb-caption, .cb-button, #wp-calendar caption, .forum-titles, .bbp-submit-wrapper button, #bbpress-forums li.bbp-header, #bbpress-forums fieldset.bbp-form .bbp-the-content-wrapper input, #bbpress-forums .bbp-forum-title, #bbpress-forums .bbp-topic-permalink, .widget_display_stats dl dt, .cb-lwa-profile .cb-block, #buddypress #members-list .cb-member-list-box .item .item-title, #buddypress div.item-list-tabs ul li, #buddypress .activity-list li.load-more, #buddypress a.activity-time-since, #buddypress ul#groups-list li div.meta, .widget.buddypress div.item-options, .cb-activity-stream #buddypress .activity-header .time-since, .cb-font-header, .woocommerce table.shop_table th, .woocommerce-page table.shop_table th, .cb-infinite-scroll a, .cb-no-more-posts{font-family: Sahel !important;}

.cb-grid-block .cb-l h2{font-size: 22px;}
.cb-grid-block .cb-s h2{font-size: 16px !important;}




a.dl-b {
    width: 65%;
    background: #2b7ef1;
    padding: 1%;
    display: block;
    margin-top: 20px;
    color: #fff !important;
    text-decoration: none;
    border-radius: 8px;margin-bottom: 4px;
    transition: .3s;
}
a.dl-b:hover {
    background: #3e3e3e;
    transition: .3s;
}

/******  under head   *******/
a.grda {
    color: #3e3e3e !important;;
    background: #ffab2a;
    border-radius: 4px;
    font-size: 14px;
}

a.grda:hover{color:#fff !important;}

a.grdab{
    color: #3e3e3e !important;;
    background: #00a1ff;
    border-radius: 4px;
    font-size: 14px;
}
a.grdab:hover{color:#fff !important;}

a.grdas {
    background: #da0000;
    color: #fff !important;;
    border-radius: 4px;
    font-size: 14px;
}

a.grdas:hover{color:#222 !important;}
/*********  brand  *************/
.cars {
    width: 11.11%;
    float: right;
    padding: 0 0 29px;
    height: 110px;
    position: relative;
}
.cars img {
    margin: 0 auto;
    display: block;
}
.cars span {
    font-size: 14px;
    color: #3e3e3e;
    padding: 0 0 20px;
    position: absolute;
    text-align: center;
    transition: all .2s ease 0s;
    width: 100%;
    bottom: 0;
}

.so {
    font-size: 28px;
}

#socialbut {
    text-align: right;
    font-size:15px;
    font-weight:600;
    color:#999999;
    border: 1px solid #e9e9e9;
    border-bottom: 3px solid #e9e9e9;
    border-radius: 5px;margin-top: 50px;
    padding:20px;
    width: 100%;
}
#socialbut .shares {
    font-size: 13px;
    display: inline-block;
    color: gray;
    float: left;
}
.entry-content {
    margin: 0 0 10px 0 !important;
}

#cb-previous-next-links {
    background:#ffffff;
    border: 1px solid #e9e9e9;
    border-bottom: 3px solid #e9e9e9;
    border-radius: 5px;
}
.cb-tags a, .bbp-topic-tags a{border: 1px solid #e9e9e9;}

#cb-previous-next-links #cb-previous-link {
    border-left: 3px solid #e9e9e9;
}
#cb-previous-next-links #cb-next-link {
    border-right: 2px solid #e9e9e9;
}
.cb-sidebar-widget .cb-light {
    background: #e6e6e6;
    border: 1px solid #b5b5b5;
    border-bottom: 3px solid #b5b5b5;
    border-radius: 10px;
}
.cb-sidebar-widget .cb-sidebar-widget-title{
    margin: 0 0 13px 0;
    border: 1px solid #b5b5b5;
    border-radius: 10px;
}
.edd-submit.button.white {
    color: #ffffff;
    background: #3db90a;
    border-color: #323332;
    font-size: 15px;
    font-weight: 600;
}

p.error {
    display: none;
}
#buddypress div.item-list-tabs {
    text-align: center;
    border-radius: 10px;
    margin-bottom: 3px;
    background: #f2f2f2;
}
#buddypress div#item-header{border-radius: 15px;}

#item-header-avatar a img{border-radius: 150px;}
#buddypress div#item-header #item-header-avatar a img:hover{
    -webkit-transform: 0 !important;
    -moz-transform: 0 !important;
    -ms-transform: 0 !important;
    -o-transform: 0 !important;
    transform: initial !important;
}
#buddypress div#item-header {
    background: url(http://f1iran.com/wp-content/uploads/2017/05/f1iran1.jpg);
    margin-bottom: 5px;box-shadow: 0 11px 100px #000 inset;
}

#buddypress span.activity{color:#fff;}
.cb-modal-dark .cb-modal{border-radius: 10px;}
.lwa .cb-form-body i, .cb-join-modal .cb-form-body i{border-radius: 30px;background: #eb9812;}
.lwa .cb-form-body input[type="password"], .lwa .cb-form-body input[type="text"], .cb-join-modal .cb-form-body input[type="password"], .cb-join-modal .cb-form-body input[type="text"]{
    border-radius: 50px;
}
#respond form .comment-notes {
    color: #ace100;padding:5px;
    font-family: Sahel;
    background: #4d4d4d;
}
label {font-family: Sahel;}
.label {color: #2048a5;}
#respond form #submit{font-family: Sahel;}
#respond form textarea {height: 120px;}

img.avatar {border-radius: 100px;} 
#buddypress .standard-form p.description {
    color: #fe0000;
    font-size: 95%;
    margin: 5px 0;
    font-weight: 600;
}
#buddypress table tr.alt td {background: #fff;}
.must-log-in {
    font-size: 19px;
    font-family: Sahel;
    background: #ffea00;
}
.SocialTop {
    background: #edf9ff;
    padding: 15px 0 5px;
    margin-bottom: 15px;
    border: 1px solid #0093e9;
    border-radius:15px;
}

.text-center {
    text-align: center;
}
.SocialTop p {
    font-size: 15px;
    font-weight: 700;
    margin-bottom: 15px;
}

.cb-meta p {margin: 0 2em 0 0;}
