    $("document").ready(function() {
        $('.button_save #add-collection').click(function() {
            var status = false;
            status = validateForm('form-add-collection');
            if(status == true) {
                return true;
            }
            $("#title").val('');
            $("#desc").val('');
            return false;
        });

        $('#cover_image_upload').on('change', function () {
            var maxSize = 1024 * 1024 * 2; // 2 MB
            var files = this.files || [];
            if (!files.length || !window.FileReader) return;
            if (files[0].size > maxSize || !/^image/.test(files[0].type)) {
                $(this).val('');
                $('#cover-img').attr('src', '');
                alert('Hanya gambar berukuran kurang dari 2 MB yang diperbolehkan');
                return false;
            }

            var reader = new FileReader();
            reader.onload = function () {
                var dataURL  = reader.result;
                $('#cover-img').attr('src', dataURL);
                $('#image-browse-preset').val(dataURL);
            }
            reader.readAsDataURL(files[0]);
        });
    });
</script>
