    <script language="javascript">
		$(document).ready(function() {
	    	var jsContextRoot = "";
	    	if(location.host == "twww.jejuair.net"){
	    		jsContextRoot = "https://twww.jejuair.net/jejuair/";
	    	}else if(location.host == "tm.jejuair.net"){
	    		jsContextRoot = "https://tm.jejuair.net/jejuair/";
	    	}else{
	    		jsContextRoot = "https://www.jejuair.net/jejuair/"; 
	    	}
			var tgURL = "";
			
			var type = navigator.appName
				var lang;
				var lang2;
				var lang_country;
				var lang_type;
				var skin_type;
				if(type == "Netscape") {
					lang = navigator.language;
				}else{
					lang = navigator.userLanguage;
				}
				
				//alert(lang);
				
				lang2 = lang;
				lang = lang.substr(0, 2);
				
				//alert(lang);
				
				
				if ( "ko" == lang ) {
					lang_country = "KOR";
					lang_type    = "KR";
					skin_type	 = "KR";
				} else if ( "ja" == lang ) {
				    lang_country = "JPN";
				    lang_type    = "JP";
					skin_type	 = "JP";
				} else if ( "zh" == lang ) {
					if( "zh-HK" == lang2){
						lang_country = "HONG";
						lang_type    = "HK";
						skin_type	 = "HK";
					}else if( "zh-TW" == lang2){
						lang_country = "TWN";
						lang_type    = "TW";
						skin_type	 = "HK";
					}else{
						lang_country = "CHN";
						lang_type    = "CN";
						skin_type	 = "CN";
					}
				} else if ( "th" == lang ) {
						lang_country = "THAI";
				    lang_type    = "EN";
					skin_type	 = "TH";
				}else if ( "fi" == lang ) {
						lang_country = "PHIL";
				    lang_type    = "EN";
					skin_type	 = "EN";
				}else if ( "vi" == lang ) {
						lang_country = "VNM";
				    lang_type    = "EN";
				}else if ( "ms" == lang ) {
						lang_country = "MYS";
				    lang_type    = "EN";
				}else {
				    lang_country = "ENG";
				    lang_type    = "EN";
				}
			
	        if(   navigator.userAgent.match(/Android/i) || navigator.userAgent.match(/webOS/i)
	           || navigator.userAgent.match(/iPhone/i)  || navigator.userAgent.match(/iPad/i)
	           || navigator.userAgent.match(/iPod/i)    || navigator.userAgent.match(/BlackBerry/i)
	           || navigator.userAgent.match(/Windows Phone/i)
	        ) {
						//tgURL = jsContextRoot + "mobile/intro.jsp";
						//jQuery('<form action="'+tgURL+'" method="post" target="_self"></form>').appendTo('body').submit().remove();
						//NetFunnel_Action({service_id:"service_5",action_id:"act_1",use_mobile_ui:"true"},function(ev,ret){
						NetFunnel_Action({},function(ev,ret){
							tgURL = jsContextRoot + "common/language_session.jsp?LANGUAGE_COUNTRY=" + lang_country + "&LANGUAGE_TYPE=" + lang_type + "&LANGUAGE_INIT=INDEX" + "&targetUrl=mobile";
							jQuery('<form action="'+tgURL+'" method="post" target="_self"></form>').appendTo('body').submit().remove();
						});
			    } else {
						NetFunnel_Action({},function(ev,ret){
							tgURL = jsContextRoot + "common/language_session.jsp?LANGUAGE_COUNTRY=" + lang_country + "&LANGUAGE_TYPE=" + lang_type + "&LANGUAGE_INIT=INDEX" + "&targetUrl=main";
							jQuery('<form action="'+tgURL+'" method="post" target="_self"></form>').appendTo('body').submit().remove();
						});
	        }
		});
    </script>
