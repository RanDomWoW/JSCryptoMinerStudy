      var ppz000 = new _PPZ();
      ppz000.cid = 11069;
      ppz000.rid = 0;
      ppz000.item_id = _item_param;
      ppz000.cb = 'ppz_draw_dummy';
      ppz000.request();
      function ppz_draw_dummy(info){
        return "";
      }
    }
  });

