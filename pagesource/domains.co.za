	<script type="text/javascript">
		function submitForm() {
			if ($.trim($('#g-recaptcha-response').val()).length == 0) {
				showMsg('Please complete the form security check', 'findOutMore', 'error');
			} else {
				$.post('/custom/newCap.php', { response : $('#g-recaptcha-response').val() }, function(data){							
					if ($.trim(data) == 'success') {
						$('#message').val('Contact: '+$('#number').val()+' Message: '+$('#inputTxt').val());
						
						$("#findOutMoreDiv").hide();
						$('#findOutMoreProcessing').fadeIn();
						
						$.post('/custom/openTicket.php', $('#findOutMoreForm').serialize(), function(data) {
							var res = $.parseJSON(data);
							
							if (res.result == 'success') {
								var str = ''+
									'<div class="center80">'+
										'<div class="alert alert-success textcenter">'+
											'<p class="fontsize3" style="padding:10px;"><strong>Ticket Created <a href="viewticket.php?tid='+res.tid+'&amp;c='+res.c+'" style="color:#000000;">#382773</a></strong></p>'+
										'</div>'+
										'<div class="center80">'+
											'<p class="txtCenter">Your ticket has been successfully created. An email has been sent to your address with the ticket information. If you would like to view this ticket now you can do so.</p>'+
										'</div>'+
									'</div>';
								$('#findOutMoreProcessing').html(str);
							} else {
								window.location.href = '/submit-ticket/support';
							}
						});
					} else {
						showMsg('Form security check failed, please retry.', 'findOutMore', 'error');
					}
				});
			}	
		}
		$(document).ready(function(e) {
			$(document.body).on('click', '.sbHeadBannerSection', function(e){
				window.open('http://builder.domains.co.za/en/brand/22060/', '_blank');
			});
      $('.offeringBlock').click(function(e) {
        if ($(this).attr('rel')) {
					window.location.href = $(this).attr('rel');
				}
      });
			
			$(document.body).on('click', '#loginFrmBttn', function(e){
				e.preventDefault();
				
				$('#loginemail, #loginpw').removeClass('inputError');
				if ($.trim($('#loginemail').val()).length == 0) {
					showMsg('Please fill in your email address', 'findOutMore', 'error');
					$('#loginemail').addClass('inputError');
				} else if ($.trim($('#loginpw').val()).length == 0) {
					showMsg('Please fill in your password', 'findOutMore', 'error');
					$('#loginpw').addClass('inputError');
				} else if (!isEmail($('#loginemail').val())) {
					showMsg('Invalid email address', 'findOutMore', 'error');
					$('#loginemail').addClass('inputError');
				} else {
					$.post('/custom/validateUser.php?login', { userEmail : $('#loginemail').val(), userPass : $('#loginpw').val(), sessMsg : $('#inputTxt').val() }, function(data) {
						var res = $.parseJSON(data);
						if ($.trim(res.result) == 'success') {	
							window.location.reload();
						} else {
							showMsg('Email or password invalid', 'findOutMore', 'error');
							$('#loginemail, #loginpw').addClass('inputError');
						}
					});
				}
			});
			
			$('#findOutMoreSubmitBttn').click(function(e) {
        e.preventDefault();
				var inputError = false;
				$('#findOutMoreForm input[type="text"], #findOutMoreForm textarea').removeClass('inputError');
				
				$('#findOutMoreForm input[type="text"]').each(function(index, element) {
					if ($('#loginFrmDiv').css('display') != 'none') {
						if ($.trim($(this).val()).length == 0) {
							$(this).addClass('inputError');
							inputError = true;
						}		
					} else {
						if (!($(this).attr('id') == 'loginemail' || $(this).attr('id') == 'loginpw')) {
							if ($.trim($(this).val()).length == 0) {
								$(this).addClass('inputError');
								inputError = true;
							}
						}
					}
        });
				
				if ($.trim($('#inputTxt').val()).length == 0) {
					$('#inputTxt').addClass('inputError');
					inputError = true;
				}
				
				if (inputError) {
					showMsg('Please complete all form inputs', 'findOutMore', 'error');
				} else if (!isEmail($('#email').val())) {
					showMsg('Invalid email address', 'findOutMore', 'error');
					$('#email').addClass('inputError');
				} else {
					var loggedin = '';
					if (!loggedin) {
						$.post('/custom/validateUser.php?checkEmail', { userEmail : $.trim($('#email').val()) }, function(data){
							if ($.trim(data) == 'Existing User') {
								showMsg('We see you have an account with us, please login to continue.', 'findOutMore', 'error');
								$('#loginFrmDiv').slideDown('fast');
								$('#loginemail').val($('#email').val());
							}	else {
								submitForm();
							}
						});
					} else {
						submitForm();
					}
				}
      });
    });
	</script>


