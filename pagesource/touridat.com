<script>

	$(function () {
	
		$('#logo-flyout-link').popover({
			trigger: "focus",
			html: true,
			content: '<div><p>Damit Ihr Urlaub immer ein unvergesslich schönes Erlebnis wird, werden alle Angebote auf touriDat nur von ausgewählten Gastgebern angeboten. Zusätzlich sichert touriDat Ihren Kauf mit der touriDays-Garantie gegen finanzielle Verluste z. B. bei Insolvenz des Gastgebers oder Betriebsschließung zu 100% ab.</p><a id="popover-touridays-more" style="color:#1a4e17;text-decoration:underline;font-size:12px;" href="https://www.touridat.com/shop/touridays/">mehr erfahren</a></div>'
		}).parent().delegate('a#popover-touridays-more', 'click', function(e) {
			e.preventDefault();
		
			// mehr erfahren im Popove
			var link = $('#popover-touridays-more').attr('href');
			
			ga('send', 'event', {
			    eventCategory: 'MasterNavigation',
			    eventAction: 'open-more',
			    eventLabel: link,
			    hitCallback: function() {
			      window.location.href = link;
			    }
			});
		});
		
	});
	
	$('#logo-flyout-link').on('show.bs.popover', function (e) {
		// flyout / popover
		
		ga('send', 'event', {
		    eventCategory: 'MasterNavigation',
		    eventAction: 'open-popover',
		    eventLabel: 'open-popover'
		});
	});

	$('#touridays_globallink_wrapper').on('click', function (e) {
		e.preventDefault();
	
		// sponsoring seite
		var link = $('#touridays_globallink_wrapper').attr('href');
		
		ga('send', 'event', {
		    eventCategory: 'MasterNavigation',
		    eventAction: 'open-sponsoring',
		    eventLabel: link,
		    hitCallback: function() {
		      window.location.href = link; 
		    }
		});
	});

	$('#trustami_globallink_wrapper').on('click', function (e) {
		e.preventDefault();
	
		// trustami
		var link = $('#trustami_globallink_wrapper').attr('href');
		
		ga('send', 'event', {
		    eventCategory: 'MasterNavigation',
		    eventAction: 'open-trustami',
		    eventLabel: link,
		    hitCallback: function() {
		    }
		});
		var _window = window.open(link, '_blank');
  		_window.focus();
	});


	$('.GAeventLink').on('click',function(event){
		event.preventDefault();
		var link = $(this).data('href');
		var newTab = $(this).data('blank') == '1';
		ga('send', 'event', {
		    eventCategory: 'MasterNavigation',
		    eventAction: $(this).data('ga-action'),
		    eventLabel: link,
		    hitCallback: function() {
		    	if (!newTab) {
		      		window.location.href = link;
		      	}
		    }
		});
		
		setTimeout(function() {
			if (newTab) {
				var _window = window.open(link, '_blank');
  				_window.focus();
			}
		}, 100);
		
		return false;
	});
	
</script>


