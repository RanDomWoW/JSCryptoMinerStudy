<script>
	$(document).ready(function () {
		// wa-nl-promo-ga-js-listener
		$('.wa-nl-promo-ga-js-listener').on('click', function (e) {
			e.preventDefault();	

			var link = $(this).attr('href');

			ga('send', 'event', {
			    eventCategory: 'whatsapp',
			    eventAction: 'open-mobile-nl',
			    eventLabel: link,
			    hitCallback: function() {
			      window.location.href = link;
			    }
			});
		});
	});


	
	$(function() {
	
		$('.iosSlider').iosSlider({
			desktopClickDrag: true,
			snapToChildren: true,
			infiniteSlider:true,
			responsiveSlideContainer:true,
			responsiveSlides:true,
			onSlideComplete: slideComplete,
			navNextSelector: $('.next'),
			navPrevSelector: $('.prev'), 
			autoSlide: true,
			autoSlideTimer: 3500
		});
		
	});
	
	$( "div[class*='category_tile']" ).on('click',function (e){
		var currentId = $(this).attr('id');
		if (!((currentId === 'category_touriPics') || (currentId === 'category_TopTen') || (currentId === 'category_Deluxe'))) {
			window.location.href=$(this).find('.category_tile_btn').attr('href');
		} else { 
			var href = $(this).find('.category_tile_btn').attr('href');
			window.open(href,'_blank');
			e.preventDefault();
		}
	});
	$('.category_tile').hover(function () {
	 //mouse in
		$(this).find('.hover_category').fadeIn();
	}, function () {
	 //mouse out
	 $(this).find('.hover_category').fadeOut();
	});
	$('#welcome-slider-content-inner').hover(function () {
	 //mouse in
		$(this).find('.hover_welcome').fadeIn();
	}, function () {
	 //mouse out
	 $(this).find('.hover_welcome').fadeOut();
	});
	//Animation
	$(document).ready(function(){
		
		$(window).on('scroll', function () {
			const elems = document.querySelectorAll('[data-td-object="fadeIn"]');
			if (elems !== undefined) {
				elems.forEach(function (elem, index) {
					const isAlreadyVisible = elem.className.indexOf('scrollVisible');
					
					if ((isAlreadyVisible === -1) && (checkVisible(elem))) {
						// element is not visible and animation hasn't run yet
						elem.className = 'scrollVisible animated fadeInDown';
						
						// newsletter edge case
						if (elem.innerHTML === 'Newsletter') {
							elem.className = elem.className + ' visible-xs';
						}
					}
				});
			}
		});
		
		function checkVisible(elm) {
		  var rect = elm.getBoundingClientRect();
		  var viewHeight = Math.max(document.documentElement.clientHeight, window.innerHeight);
		  return !(rect.bottom < 0 || rect.top - viewHeight >= 0);
		}		
		
		/*
		if($('[data-td-object="fadeIn"]').length > 0)
		{
			$('[data-td-object="fadeIn"]').addClass("scrollHidden").viewportChecker({
		        classToAdd: 'scrollVisible animated fadeInDown', // Class to add to the elements when they are visible,
				classToRemove: 'scrollHidden',
		        offset: '10%'
			});
		}*/
	});
</script>
 


