	function populate(x){
if( x == "DS"){
	cacheobj.disabled=true
	//console.log(x);
}
else
{
	cacheobj.disabled=false
}
for (m=cacheobj.options.length-1;m>0;m--)
cacheobj.options[m]=null
selectedarray=eval(x)
for (i=0;i<selectedarray.length;i++)
cacheobj.options[i]=new Option(selectedarray[i].text,selectedarray[i].value)
cacheobj.options[0].selected=true
calculate()
}

function calculate()
{
	preis=cacheobj.options[cacheobj.selectedIndex].value	
	//document.dynamiccombo.preis.value=preis
	if (preis == "" || preis == null || preis == "DS" )
	{
		preis = "0";
		//alert("preis ist ")
	}
	preis1=preis + " €"
	preis1=preis1.replace(",",".")
	document.getElementById("preis1").innerHTML = preis1
	
	gewicht=document.dynamiccombo.gewicht.value
	gewicht1=gewicht.replace(",",".")
	apreis=document.dynamiccombo.apreis.value
	apreis1=apreis.replace(",",".")
	//console.log(apreis1);
	if(isNaN(gewicht1)==true || isNaN(apreis1)==true)
	{
		document.getElementById("meldung").innerHTML = "Пожалуйста в заносите в поля <Соимость> и <Вес> только числа в формате 00.00"
	}
	else
	{
		
		gewicht1=gewicht.replace(",",".")
		preis=preis.replace(",",".")
		apreis=apreis.replace(",",".")
		preis= parseFloat(preis)
		apreis= parseFloat(apreis)
		gewicht1= parseFloat(gewicht1)
		gesamt=(preis*gewicht1).toFixed(2)
		agesamt=((preis*gewicht1)+apreis).toFixed(2)
		//agesamt=agesamt.toFixed(2)
		document.getElementById("gesamt").innerHTML = gesamt + " €"
		document.getElementById("gesamtpreis").innerHTML = agesamt + " €"
		document.getElementById("meldung").innerHTML = " "
		
	}
	//document.dynamiccombo.gesamt.value=gesamt
}

