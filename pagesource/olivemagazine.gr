<script type="text/javascript">
function scrollFunction() {
    var clientHeight = document.getElementById('topAdvert').clientHeight;
    var ipsos = clientHeight + 30;
    
    //console.log(ipsos);
    var scrollPos = $(document).scrollTop();

    if (scrollPos > clientHeight) {
        document.getElementById("skin-olv").style.position = "fixed";
        document.getElementById("skin-olv").style.top = "0px";
    } else {
        document.getElementById("skin-olv").style.position = "absolute";
        document.getElementById("skin-olv").style.top = ""+ipsos+"px";
    }
} 
window.onscroll = scrollFunction;
</script>


