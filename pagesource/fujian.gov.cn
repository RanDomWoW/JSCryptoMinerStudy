		//右侧微信浮窗
		function rigScroll(){
			var winWidth=$(window).width();
			if(winWidth<1024){
				$(".right_aside").css({"right":12});
				$(".left_aside").css({"right":12})
			}else{
				var winRig=(winWidth-1005)/2-120;
				var winLef=(winWidth-1005)/2-120;
				$(".right_aside").css({"right":winRig})
				$(".left_aside").css({"left":winLef})
			}
			$(".leftclose").click(function(){
				$(".left_aside").hide();
			});
			$(".rightclose").click(function(){
				$(".right_aside").hide();
			})
		}
		rigScroll();

	})
