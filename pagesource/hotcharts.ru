	Inline.add('main', function ()
	{
		var $user_menu = $('.b_user_menu:first');

		if ($user_menu.data('init')) return;

		$user_menu.data('init', true);

		$user_menu.find('a.ajax').click(function(e)
		{
			var a = $(e.currentTarget),
				url = a.attr('href');

			var form_login_selector = '.b_form_login';

			var $form_login = $('body').find(form_login_selector);

			if ($form_login.length > 0)
			{
				// возврат к первой вкладке
				$form_login.find('.nav li:first a').click();

				window.scrollTo(0, 0);

				$form_login.show();

			} else {
				CA.Ajix.get(url, [], function (response)
				{
					if ( ! response.isSuccess()) {
						window.location.href = url;
						return;
					}

					$form_login = $(response.getData());

					window.scrollTo(0, 0);

					$('body').append($form_login);

					Inline.exec('main');
				});
			}

			return false;
		});
	});
