<script>
    $(function(){
        $(".chzn-container").removeAttr("style"); // hack  что бы не заменял стандарные стили
        Selector.OnChangeName();
    });
    var Selector = {
        Init: function() {
            this.name = $('.name').chosen();
            this.number = $('.number');
            this.make = $('.make').chosen();
            this.model = $('.model').chosen();
            this.condition = $('.condition').chosen();
        },
        OnSetMyCar: function(el) {
            this.Load(this.make, ['type=0'].join('/'), 10, el);

        },
        OnChangeMake: function(el) {
            this.Load(this.model, ['make='+this.make.val()].join('/'),2);
            this.name.html('<option></option>').trigger("chosen:updated");
        },
        OnChangeModel: function(el) {
            this.Load(this.name, ['make='+this.make.val(), 'model='+this.model.val()].join('/'),3);
        },
        OnChangeName: function(el) {
            this.ButtonTrigger();
        },
        OnChangeNumber: function(el, value) {
            this.ButtonTrigger();
        },
        Load: function(el, url, cur, sel) {
            var that = this;

            $.ajax({
                url: '/options/'+url+'/receiver='+el.attr('name'),
                dataType: 'html',
                success: function(r) {
                    el.html(r);
                    el.trigger("chosen:updated");

                    if(cur==1){
                        that.OnChangeMake(el);
                    }
                    if(cur==2){
                        that.OnChangeModel(el);
                    }
                    if(cur==3){
                        that.OnChangeName(el);
                    }
                    if(cur==10){
                        var mark = $(sel).data("mark");
                        that.make.val(mark);
                        that.make.trigger('chosen:updated');
                        that.Load(that.model, ['type='+$(sel).data("type"), 'make='+mark].join('/'),11, sel);
                    }
                    if (cur==11) {
                        var model = $(sel).data("model");
                        that.model.val(model);
                        that.model.trigger('chosen:updated');
                        that.Load(that.name, ['make='+that.make.val()   , 'model='+model].join('/'),3);
                    }
                },
                failure: function() {

                }
            })
        },
        Submit: function() {
            var url = '';

            if (this.make.val()) {
                url += this.make.val()+'/';
            }

            if (this.model.val()) {
                url += this.model.val()+'/';
            }

            if (this.name.val()) {
                url += this.name.val()+'/';
            }
            /*
            if (this.number.val()) {
                url.push('number='+this.number.val());
            }
            if (this.condition.val()) {
                url.push('condition='+this.condition.val());
            }
            */

            if (url.length > 0) {
                location.href = '/parts/'+url;
            }
        },
        Done: function() {

        },
        ButtonTrigger: function() {
            var name = this.name.find(':selected');

            if (name.val()){
                this.ButtonOn();
            } else {
                this.ButtonOff();
            }
        },
        ButtonOn: function() {
            $('#submitbutton').removeAttr('disabled');
        },
        ButtonOff: function() {
            $('#submitbutton').attr('disabled', 'disabled');
        }
    }
    Selector.Init();
</script>
