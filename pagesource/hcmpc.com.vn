       function startclock() {
           var curTime = new Date();
           var nhours = curTime.getHours();
           var nmins = curTime.getMinutes();
           var nsecn = curTime.getSeconds();
           var nday = curTime.getDay();
           var nmonth = curTime.getMonth();
           var ntoday = curTime.getDate();
           var nyear = curTime.getYear();
           var AMorPM = "AM";
           if (nhours >= 12)
               AMorPM = "PM";
           else
               AMorPM = "AM";

           if (nhours >= 13)
               nhours -= 12;
           if (nhours == 0)
               nhours = 12;
           if (nsecn < 10)
               nsecn = "0" + nsecn;
           if (nmins < 10)
               nmins = "0" + nmins;
           if (nday == 0)
               nday = "Ch&#7911; Nh&#7853;t";
           if (nday == 1)
               nday = "Th&#7913; Hai";
           if (nday == 2)
               nday = "Th&#7913; Ba";
           if (nday == 3)
               nday = "Th&#7913; T&#432;";
           if (nday == 4)
               nday = "Th&#7913; N&#259;m";
           if (nday == 5)
               nday = "Th&#7913; S&#225;u";
           if (nday == 6)
               nday = "Th&#7913; B&#7843;y";
           nmonth += 1;
           if (nyear <= 99)
               nyear = "19" + nyear;
           if ((nyear > 99) && (nyear < 2000))
               nyear += 1900;
           var d;
           var Str0 = "";
           if (nhours > 9) Str0 = "";
           else Str0 = "0";
           d = document.getElementById("theClock");
           //d.innerHTML = " <span>" + Str0 + nhours + ":" + nmins + ":" + nsecn + " " + AMorPM + "</span>" + nday + ", " + ntoday + "/" + nmonth + "/" + nyear;
           d.innerHTML = " <span>" + Str0 + nhours + ":" + nmins + " " + AMorPM + " - " + "</span>" + ntoday + "/" + nmonth + "/" + nyear;
           setTimeout('startclock()', 1000);
       }


       function start() {
           //modal.style.display = "block";
          // $('#myModal').modal({ show: true });
           $('#bootstrap-touch-slider').bsTouchSlider();
           startclock();
       }



       window.onload = start;

    </script>

      
    
    <script>
        $(document).ready(function () {
            $('.carousel').on('slid.bs.carousel', function () {
                var carouselData = $(this).data('bs.carousel');
                var currentIndex = carouselData.getItemIndex(carouselData.$element.find('.item.active'));
                var total = carouselData.$items.length;
                var slide = (currentIndex + 1);
                var items = carouselData.$items[currentIndex];
                var item = $(items).find(".carousel-caption-more").eq(0);
                var noidung = $(item).html();
                // do something depending on the slide number..
                //$("#tag-of-imageslide").html(noidung);
            });

            $('.dropdown-accordion').on('click', 'a[data-toggle="collapse"]', function (event) {
                event.preventDefault();
                event.stopPropagation();
                $($(this).data('parent')).find('.panel-collapse.in').collapse('hide');
                $($(this).attr('href')).collapse('show');
            });

            $('.row-equal-heights').each(function () {
                var highestBox = 0;
                $(this).children('[class*="col-"]').each(function (index, el) {
                    if ($(el).height() > highestBox)
                        highestBox = $(el).height();
                });
                $(this).children('[class*="col-"]').css('height', highestBox);
            });

            var highestImgInVideo = 0;
            var highestImg = 0;
            var ShortestImg = 0;
            $(function () {
                GetHighestImageInVideo();
                GetHighestImage();
                GetShortestImage();
                GetShortestImageTrangChu();

                // gán ảnh đại diện video chiều cao của cái cao nhất
                $(".img-equal-height").each(function () {
                    $(this).css('height', highestImgInVideo);
                });

              
                
                // hiệu chỉnh div parent theo kịch thước thực tế
                $('.equal-heights').each(function () {
                    var highestBox = 0;
                    $(this).children('[class*="col-"]').each(function (index, el) {
                        if ($(el).height() > highestBox)
                            highestBox = $(el).height();
                    });
                    $(this).children('[class*="col-"]').css('height', highestBox);
                });
            });

            function GetHighestImageInVideo() {
                $(".img-equal-height").each(function () {
                    var height = $(this).height();
                    if (height > highestImgInVideo)
                        highestImgInVideo = height;
                });  
            }

            function GetHighestImage() {
                $(".img-equal-height-smallest").each(function () {
                    var height = $(this).height();
                    if (height > highestImg)
                        highestImg = height;
                });
            }

            function GetShortestImage() {
                ShortestImg = 0;
                $(".img-equal-height-smallest").each(function () {
                    var height = $(this).height();
                    if (height > ShortestImg)
                        ShortestImg = height;
                });               

                // gán ảnh đại diện của phóng sự ảnh/ảnh EVNHCMC bằng chiều cao nhỏ nhất
                //if ($(this).width() > 768) {
                    $(".img-equal-height-smallest").each(function () {
                        $(this).css('height', ShortestImg);
                    });
                //}
                //else {
                //    $(".img-equal-height-smallest").each(function () {
                //        $(this).css('height', "auto");
                //    });
                //}

            }

            function GetShortestImageTrangChu() {
                height = 0;
                  if ($(this).width() > 750) {
                      height = $("#tuyentruyen").height();
                }
                else
                      height = "auto";

                $(".img-equal-height-smallest-TrangChu").each(function () {
                    $(this).css('height', height);
                });             

            }

            $(window).resize(function () { //Fires when window is resized
                GetHighestImageInVideo();
                GetHighestImage();
                GetShortestImage();
                GetShortestImageTrangChu();
            });

            // Instantiate the Bootstrap carousel danh sach don vi FOOTER
            $('.multi-item-carousel').carousel({
                interval: 3000
            });

            // for every slide in carousel, copy the next slide's item in the slide.
            // Do the same for the next, next item.
            // danh sach don vi FOOTER
            $('.multi-item-carousel .item').each(function () {
                var next = $(this).next();
                if (!next.length) {
                    next = $(this).siblings(':first');
                }
                next.children(':first-child').clone().appendTo($(this));

                if (next.next().length > 0) {
                    next.next().children(':first-child').clone().appendTo($(this));
                } else {
                    $(this).siblings(':first').children(':first-child').clone().appendTo($(this));
                }
            });
            //# sourceURL=pen.js

        });

    </script>
