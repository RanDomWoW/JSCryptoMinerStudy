                    var slide = $('.' + this.wrapperClass).find('.swiper-slide')[index+1];
                    return '<span class="pag-shoes ' + className + '"><i class="pag-shoes-bull"></i><span class="pag-shoes-text">' + $(slide).data('name') + '</span></span>';
                }
            });
        });
