<script>
	var _app = {};
	// (action, form selector[, callback])
	_app.accountAction = function(a, s)
	{
		var c, popup=$(s), form=$(s).find('form'),
			callback = arguments[2] === 'function' ? arguments[2] : null;

		if ((c = this.actionRoute(a)) === null) {
			return;
		}

		$.post(c, form.serialize(), function(data){
			var result = data.split('::');
			if (result[0] == 'OK') {
				if (callback != null) {
					callback(result[1]);
				} else {
					document.location.reload();
				}
			} else {
				popup.find('.error').html(result[1]);
			}
		});
	}

	_app.actionRoute = function(action)
	{
		var route = {
			login: 'http://udoktora.net/account/login/ajax/1/',
			forgot: 'http://udoktora.net/account/forgot/ajax/1/'
		};

		if (typeof route[action] !== 'undefined') {
			return route[action];
		}

		return null;
	}

	function showhide(selector)
	{
		var e = $(selector);
		arguments[1] ? e.css({ left:arguments[1]+'px'}) : false;
		arguments[2] ? e.css({ top:arguments[2]+'px'}) : false;
		arguments[3] ? $(arguments[3]).hide() : false;
		e.is(':hidden') ? e.show() : e.hide();
	}

	$(function(){
		
		$('#form-login form').submit(function(){ _app.accountAction('login', '#form-login'); return false;});
		$('#form-forgot form').submit(function(){ _app.accountAction('forgot', '#form-forgot'); return false; });

		
		//$('#button-login').click(function() { showhide('#form-login',   '#form-forgot'); return false; })
		$('#button-checker').click(function() { showhide('#form-checker'); return false; })
		$('#button-region').click(function() { showhide('#checker-region'); return false; })
		$('.dpopup a.close').click(function() { $(this).closest('.dpopup').hide(); return false; })
		$('#form-checker a.close').click(function() { $(this).closest('#form-checker').hide(); return false; })
		$('#checker-region a.close').click(function() { $(this).closest('#checker-region').hide(); return false; })
		$('.dpopup [name="iforgot-link"]').click(function() { showhide('#form-forgot', -75, 45, '#form-login'); return false; })
		// $('#button-login').click(function() { showhide('#form-login'); return false; })
	});

</script>



