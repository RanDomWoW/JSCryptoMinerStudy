	background: #aef9ea;
}


#accordion a:hover{
	background: #aef9ea;
}	

/*===============================
  12.ヘッダーナビドロップダウン背景色
===============================*/
#main-navigation li ul li a{
	background: #12105e;
}
#main-navigation li ul li ul li a {
	background: #12105e;
}
/*===============================
  13.見出し色(H2,H3)
===============================*/
.post h2{background: #a9a9a9;}
h2 {background: #a9a9a9;}
.post .fukidasi h2{background: #a9a9a9;}
.post .fukidasi h2:after{border-top: 15px solid #a9a9a9;}
.post .shadow h2{background: #a9a9a9;}
.post .ribbon h2{background: #a9a9a9;}
.post .ribbon h2::before,
.post .ribbon h2::after{border-top: 5px solid #a9a9a9;}
.post .ribbon h2::before{border-left: 5px solid #a9a9a9;}
.post .ribbon h2::after{border-right: 5px solid #a9a9a9;}
.post .under-gradline h2{color:#a9a9a9;}
.post .under-gradline h2:after{
	background: -moz-linear-gradient(to right, #a9a9a9, transparent);
	background: -webkit-linear-gradient(to right, #a9a9a9, transparent);
	background: linear-gradient(to right, #a9a9a9, transparent);
}
.post .first-big h2{color:#1a1a1a;}
.post .first-big h2:first-letter {
	color:#a9a9a9;
	border-bottom:solid 3px #a9a9a9;
}
.post .hamiwaku h2{
	border-top: solid 1px #a9a9a9;
	border-bottom: solid 1px #a9a9a9;
	color:#a9a9a9;
}
.post .hamiwaku h2:before,.post .hamiwaku h2:after{
	background-color: #a9a9a9;
}
.post .chapter h2{
	border:1px solid #a9a9a9;
	color:#a9a9a9;
}
.post .chapter h2::after{color: #a9a9a9;}
.post .icon h2 {background:#a9a9a9;}
.post h3 {
	border-color: #a9a9a9;
	color:#a9a9a9;
}
.post .h3underline h3 {
	border-color: #a9a9a9;
	color:#a9a9a9;
}
.post .thin-underline h3 {
	border-color: #a9a9a9;
	color:#a9a9a9;
}
.post .lh3 h3{
	border-color: #a9a9a9;
	color:#a9a9a9;
}
.post .stripes h3:after {
background: -webkit-repeating-linear-gradient(-45deg, #a9a9a9, #a9a9a9 0px, #fff 2px, #fff 2px);
background: repeating-linear-gradient(-45deg, #a9a9a9, #a9a9a9 0px, #fff 2px, #fff 2px);
}
.post .box2 h3{color: #a9a9a9;}
.post .box2 h3::before{color: #a9a9a9;}
.post .box2 h3::after{color: #A1B8D1;}
.post h4{	color:#a9a9a9;}
.post .trans-color h4{	color:#a9a9a9;}
.post .cube-icon h4{	color:#a9a9a9;}
.post .left-border h4{border-color: #a9a9a9;}
.post .left-thin-border h4{border-color: #a9a9a9;}
.textwidget h3{
	border-color: #a9a9a9;
	color:#a9a9a9;
}
#newposts h4.widgettitle {
background: #a9a9a9;
}
.sankaku { border-top: 30px solid #a9a9a9;}


