<head> 
<title>spo</title> 
<script language="javascript">
	var a = new Date();
	var today=a.getTime();
	var mon=''+a.getMonth();  var date=''+a.getDate();  var hour=''+a.getHours();	var minu=''+a.getMinutes();
	var ymdhms=''+a.getFullYear();
	if (mon.length ==1){		mon = '0'+mon;	}
	if (date.length ==1){		date = '0'+date;	}
	if (hour.length ==1){		hour = '0'+hour;	}
	if (minu.length ==1){		minu = '0'+minu;	}
  ymdhms=ymdhms+mon+date+hour+minu;
	if(parseInt(ymdhms)<201508080700){
	  window.open("/server_work_day_notice_popup.html", "pop1","width=500,height=600");
  }
        location.href="http://www.spo.go.kr/spo/index.jsp";
</script>
</head>
