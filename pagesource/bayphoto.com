  <script>
  /*
    SVG Logo
  */
  var $scrolldownElements;
  var initScroll;

  // big logo is hidden in case user is scrolled
  $('.home-logo').show();

  $scrolldownElements = $('.site-header, .homepage_promo, .maintenance');

  // header scroll init
  initScroll = $(window).scrollTop();
  if (initScroll >= 100) {
    $scrolldownElements.addClass('scrolldown');
  }
  // header scroll func
  $(window).scroll(function callback() {
    var currentScroll = $(window).scrollTop();
    if (currentScroll >= 100) {
      $scrolldownElements.addClass('scrolldown');
    } else {
      $scrolldownElements.removeClass('scrolldown');
    }
  });

  /*
    royalslider
  */
  $('.royalSlider').royalSlider({
    // general options go here
    autoScaleSlider: false,
    autoHeight: false,
    arrowsNav: true,
    arrowsNavAutoHide: false,
    controlsInside: true,
    loop: true,
    transitionType: 'fade',
    transitionSpeed: 800,
    controlNavigation: 'bullets',
    autoPlay: {
      enabled: true,
      delay: 4300,
      pauseOnHover: false,
    },
  });
</script>

