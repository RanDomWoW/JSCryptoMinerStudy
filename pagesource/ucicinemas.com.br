	$('#splash').bind('click', function (evt) {
		if (evt.target.id == 'splashImagemBg') {
			fecharSplash();
		}
	});
	
  
	function fecharSplash()	{
		pauseVideo();
		fade(document.getElementById('splash'));
			
		var date = new Date();
		var minutes = 60;
		date.setTime(date.getTime() + (minutes * 60 * 1000));
			
		$.cookie("splashUCIExpire", "UCI", { expires: date });
	}
	
    function fade(element) {
    var op = 1;  // initial opacity
    var timer = setInterval(function () {
        if (op <= 0.1){
            clearInterval(timer);
          
            element.style.display = 'none';
        }
        element.style.opacity = op;
        element.style.filter = 'alpha(opacity=' + op * 100 + ")";
        op -= op * 0.1;
    }, 50);
	}
	
  setTimeout(function(){ 
			
            if(typeof(Storage) !== "undefined")
			{
         
				console.log("Already shown " + $.cookie("splashUCIExpire"));

				if ( $.cookie("splashUCIExpire") == "" || $.cookie("splashUCIExpire") === undefined)
				{
					if ($(window).width() > 800) {
						console.log("Already shown IF " + $.cookie("splashUCIExpire"));
						//comentar
						document.getElementById('splash') .style.display = 'inline';
						//comentar
					}
					//comentar
					//Display splash
					setTimeout(function(){   

					 fade(document.getElementById('splash'));
					  
					 sessionStorage.setItem('splashShown1', true  );
					}
					, 33000);
					//comentar
                } else {
                    
                     //Display Main Content
                      document.getElementById('splash') .style.display = 'none'
                      console.log("Already shown ELSE " + $.cookie("splashUCIExpire"));
                }
            }
            else
			{
				//document.getElementById("result").innerHTML = "Sorry, your browser does not support web storage...";
            }
            
		}, 0);

  	</script>
