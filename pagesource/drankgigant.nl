<script>
    require(['jquery'], function($) {
        $(document).ready(function() {
            $('.nav-sections').css('top', '32px');

            var storageName = 'shopMessage';
            var storageValue = localStorage.getItem(storageName);

            if(storageValue == null) {
                $('.above-header-bl').show();
                $('.nav-sections').css('top', '75px');
            } else {
                var object = JSON.parse(localStorage.getItem(storageName)),
                dateString = object.timestamp,
                now = new Date().getTime().toString();
                timeDif = now - dateString;
                expireTime = 8640000000; // 100 days

                if (timeDif > expireTime) {
                    localStorage.removeItem(storageName);
                    $('.above-header-bl').show();
                    $('.nav-sections').css('top','75px');
                }
            }

            $('.above-header__close').on('click', function() {
                $('.above-header-bl').hide();
                $('.nav-sections').css('top', '32px');
                $('#maincontent').css("margin-top", "0px");
                var object = {value: 1, timestamp: new Date().getTime()};
                localStorage.setItem(storageName, JSON.stringify(object));
            });

            if($('body .above-header-bl').css('display') == 'block')
            {
                $("#maincontent").css("margin-top", "40px");
            }

        });

    });

</script>

