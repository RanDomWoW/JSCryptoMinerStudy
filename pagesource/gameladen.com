<script type="text/javascript">
	const PRODUCT_NOTIFICATION = {
		delivery_Time: 'Dieses Produkt hat eine erhöhte Lieferzeit und wird nicht sofort versendet. Möchtest du den Artikel trotzdem bestellen?',
		product_notice: 'Möchtest du dieses Produkt zum Warenkorb hinzufügen?',
		version: '<p><strong>HINWEIS:</strong></p><p>Sie kaufen hier einen NEUEN, vollkommen unbenutzten Steam-Account, KEINEN CODE ZUM EINLÖSEN.Das Spiel kann NICHT zu Ihrem eigenen Steam-Account hinzugefügt werden.Die Lieferzeit kann bis zu 24 Stunden betragen. Die meisten Bestellungen liefern wir aber wesentlich schneller. Wenn Sie mit der Lieferung eines neuen Steam-Accounts nicht einverstanden sind, kaufen Sie dieses Produkt bitte nicht. Sie erhalten nach dem Kauf weitere Informationen zu ihrem neuen Steam-Account. </p><p>&nbsp;</p><p>Sind Sie damit einverstanden?</p><p>&nbsp;</p><p>Sollten Sie nicht einverstanden sein, finden Sie über unsere Suchfunktion gleiche oder andere Produkte mit Key-Aktivierung</p>',
		version_next: '<p>Sie kaufen ein Produkt, das über einen VPN-Tunnel einmalig aktiviert werden muss. Eine Anleitung dazu erhalten Sie beim Kauf per Email. Die Installation und Nutzung des Spiels sind jedoch ohne diesen VPN-Tunnel möglich. </p><p>&nbsp;</p><p>Sind Sie damit einverstanden?</p><p>&nbsp;</p><p>Sollten Sie nicht einverstanden sein, finden Sie über unsere Suchfunktion gleiche oder andere Produkte ohne VPN-Aktivierung.</p>',
	};
	
	function add_to_cart(el){
		el = jQuery(el);
		let delivery_time = el.attr('data-product-notification-delivery');
		let notice = el.attr('data-product-notification-notice');
		let version = el.attr('data-product-notification-version');
		
		let yesFunction = function(el) {
			if(el.is("a")) {
				setLocation(el.attr('href'));
			}else if (el.is("button")) {
				el.parents('form:first').submit();
			} else {
				console.log('Unknown Product Notification Popup Handler')
			}
		};
		
		// Proof if VPN notification
		if(version ==61 ||version == 39 ||version == 41 ||version == 37){
			if(version == 61){    
				el.GLNPopup({
					buttons: 'choice',
					afterYes: yesFunction,
					title: 'VPN-Produkt',
					type: 'warning',
					content: PRODUCT_NOTIFICATION['version'],
				});
			}else{
				el.GLNPopup({
					buttons: 'choice',
					afterYes: yesFunction,
					title: 'VPN-Produkt',
					type: 'warning',
					content: PRODUCT_NOTIFICATION['version_next'],
				});
			}
		}
		
		// Proof if product notice notification
		else if (notice.trim() !== null && notice.trim() !== ''){
			el.GLNPopup({
				buttons: 'choice',
				afterYes: yesFunction,
				title: 'Wichtige Info',
				type: 'warning',
				content: notice + PRODUCT_NOTIFICATION['product_notice'],
			});
		}
		
		// Proof if deliver time notification
		else if (delivery_time.trim() !== null && delivery_time.trim() !== ''){
			
			el.GLNPopup({
				buttons: 'choice',
				afterYes: yesFunction,
				title: delivery_time,
				type: 'Wichtige Informationen',
				content: PRODUCT_NOTIFICATION['delivery_Time'],
			});
		}
		
		// No notification
		else {
			yesFunction(el);
		}
		
		return false;
	}
</script>		


