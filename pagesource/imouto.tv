		<script>
      $(function(){
        var timerID;

        $(".special01").click(function(){
          changeImage("special01");
        });
        $(".special02").click(function(){
          changeImage("special02");
        });
        $(".special03").click(function(){
          changeImage("special03");
        });

        timerID = setInterval(slideShow,10000);
        
        function changeImage(myclass){
          myclass = (myclass=='') ? false : myclass;
          var now = $("p.show");
          
          if(!myclass) return false;
          if(now.attr('id')==myclass) return false;
          
          next = $("#" + myclass);
          now.fadeOut(0,function(){next.fadeIn(1000);});
      
          now.removeClass('show');
          next.addClass('show');
          
          clearInterval(timerID);
          timerID = setInterval(slideShow,10000);
        }
        
        function slideShow(){
          var now = $('p.show').attr('id');
          var next;
          if(now == "special01") next = "special02";
          if(now == "special02") next = "special03";
          if(now == "special03") next = "special01";
          changeImage(next);
        }
        
      });
    </script>
    
