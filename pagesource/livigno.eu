function menu(){
    var saveBase;
    saveBase = $('.current');
    var dimSubmenu = [];
    $('.list-menu').each(function(){
        altezzaMenu = $(this).height();
        dimSubmenu.push(altezzaMenu);
    });
    $('.submenu').css({
        'display':'none',
        'visibility':'visible'
    });
    $('.menu-site').find('.sub').each(function(){
        $(this).click(function(){   
            if($(this).find('a').hasClass('temp')) {
                $('.sub').find('a').removeClass('current');
                $('.sub').find('a').removeClass('temp');
                saveBase.addClass('current');
                $('.submenu').slideUp(500);
                $('.list-menu').slideUp(500);
                
                /* RESET ALTEZZE SPOSTAMENTO CONTENUTO */
                $('.wrapper').animate({'padding-top':0},500);
                $('.fix-home').animate({'top':80},500);
                $('.fix-home').css({'position':'fixed'});
                $('.submenu').removeClass('aperto');
                removeEvent(window, "scroll", checkFullScreenBG);
            }else{
                $('.sub').find('a').removeClass('temp');
                $(this).find('a').addClass('temp');
                $('.sub').find('a').removeClass('current');
                $(this).find('a').addClass('current');
                submenu = $(this).data('sub');
                var res = submenu.substring(3);
                /*console.log(dimSubmenu[res-1]);*/
                $('.submenu').show();
                $('.list-menu').hide();
                if($('.submenu').is('.aperto')){
                    $('#'+submenu).show();
                }else{
                    $('#'+submenu).slideDown(500);
                }
                
                /* SET ALTEZZE SPOSTAMENTO CONTENUTO */
                addEvent(window, "scroll", checkFullScreenBG);
                var h_body = $(window).scrollTop();
                if(h_body==0){
                    h_scroll = dimSubmenu[res-1];
                    $('.wrapper').animate({'padding-top':h_scroll+60},500);
                    $('.fix-home').animate({'top':(h_scroll+30+110)},500);
                }
                $('.submenu').addClass('aperto');
            }
        })
    });

    $('body').off('click').click(function(e){
        if($(e.target).closest('.submenu').length == 0 && $(e.target).closest('.menu-site').length == 0 && !isMobile)
        {
            $('.sub').find('a').removeClass('current');
            $('.sub').find('a').removeClass('temp');
            saveBase.addClass('current');
            $('.submenu').slideUp(500);
            $('.list-menu').slideUp(500);
            /* RESET ALTEZZE SPOSTAMENTO CONTENUTO */
            $('.wrapper').animate({'padding-top':0},500);
            $('.fix-home').animate({'top':80},500);
            $('.fix-home').css({'position':'fixed'});
            $('.submenu').removeClass('aperto');
            removeEvent(window, "scroll", checkFullScreenBG);
        }
        if($(e.target).closest('.content-sidebar-left').length == 0 && $(e.target).closest('.contenuti-box').length == 0 && $(e.target).closest('.ui-corner-all').length == 0)
        {
            $( "#content-ajax" ).html('');
            $('.single-box-sidebar').removeClass('selected');
