                var n_adult = $(this).val();

                $( "input.adult-age" ).remove();

                for (var i = 0; i < n_adult; i++) {
                    $('<input>').attr({
                        type: 'hidden',
                        class: 'adult-age',
                        name: 'age[a]['+i+']',
                        value: '18'
                    }).appendTo('form');
                }
            })

            $("form").submit(function(e) {
                e.preventDefault();
                var n_adult = $('#num-adult').val();
                $( "input.adult-age" ).remove();

                for (var i = 0; i < n_adult; i++) {
                    $('<input>').attr({
                        type: 'hidden',
                        class: 'adult-age',
                        name: 'age[0][a]['+i+']',
                        value: '18'
                    }).appendTo('form');
                }

                $("select[name=adult]").prop('disabled', true);
                $("select[name=children]").prop('disabled', true);
                $('#loading').show();
                this.submit();

                return false;
            });

            function print_child(n){
                var childs_age_array = { }
                
                if (n<1) {
                    $('#eta-bambini').hide();
                    $('#age-child').html('');
                }else{
                    $('#eta-bambini').css({
                        'display':'inline-block'
                    });
                    $('#age-child').html('');
                    for (var i = 0; i < n; i++) {
                    var eta_bimbo = '<select name="age[0][c]['+(i+1)+']" id="eta_'+(i+1)+'" class="form-control">'+
                        '<option value="" >-</option>';
                        for(var j=0; j<18; j++){
                            if (j == childs_age_array[i]) {
                                eta_bimbo +='<option value="'+j+'" selected="selected">'+j+'</option>';
                            } else {
                                eta_bimbo +='<option value="'+j+'" >'+j+'</option>';
                            }

                        }
                        eta_bimbo +='</select>';
                        $('#age-child').append(eta_bimbo);
                    }
                }
            }

            /*
             * gestione click bottone cerca
             * */
            $( "#cerca_disponibilita" ).click(function() {
                var n_child = $('#num-bimbi').val();
                var age_child_empty = false;

                if(n_child >= 1){
                    for (var i = 1; i <= n_child; i++) {
                        var e = $('#eta_b' + i).val();
                        if (!$.isNumeric(e)) {
                            age_child_empty = true;
                            break;
                        }
                    }
                }

                if(age_child_empty){
                    modalOpen("Attenzione", 'Compilare tutti i campi età');
                    return false;
                }else {
                    $("#pianifica_vacanza").submit();
                }
            });


