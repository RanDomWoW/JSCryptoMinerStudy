function prenotaHome() {
    var e = "";
    var t = new Date;
        $.datepicker.setDefaults( $.datepicker.regional[lang_site] );
    $(function() {
        var e = $("#from").datepicker({
            minDate: t,
            changeMonth: true,
            changeYear: true,
            yearRange: "2017:2020",
            onClose: function() {
                update_days()
            },
            onSelect: function(t) {
                $(".ui-state-default").attr("href","javascript:void(0)");
                var n = this.id == "from" ? "minDate" : "maxDate",
                    r = $(this).data("datepicker"),
                    i = $.datepicker.parseDate(r.settings.dateFormat || $.datepicker._defaults.dateFormat, t, r.settings);
                e.not(this).datepicker("option", n, i);
                if (this.id == "from") {
                    from = $(this).datepicker("getDate");
                    if (typeof(to) != "undefined" && !(to == "")) {
                        update_days()
                    }
                }
                if (this.id == "to") {
                    to = $(this).datepicker("getDate");
                    update_days()
                }
                var from_set = $('#from').datepicker('getDate'); 
                from_set.setDate(from_set.getDate()+7); 
                $('#to').datepicker('setDate', from_set);
            }
        });
        
        var e = $("#to").datepicker({
            minDate: t,
            changeMonth: true,
            changeYear: true,
            numberOfMonths: 1,
            yearRange: "2017:2020",
            onClose: function() {
                update_days()
            },
            onSelect: function(t) {
                $(".ui-state-default").attr("href","javascript:void(0)");
                var n = this.id == "from" ? "minDate" : "maxDate",
                    r = $(this).data("datepicker"),
                    i = $.datepicker.parseDate(r.settings.dateFormat || $.datepicker._defaults.dateFormat, t, r.settings);
                e.not(this).datepicker("option", n, i);
                if (this.id == "from") {
                    from = $(this).datepicker("getDate");
                    if (!(to == "")) {
                        update_days()
                    }
                }
                if (this.id == "to") {
                    to = $(this).datepicker("getDate");
                    update_days()
                }
            },
            beforeShow: function() {
                var dateMin = $('#from').datepicker('getDate');
                dateMin.setDate(dateMin.getDate() + 1);

                $( "#to" ).datepicker( "option", "minDate", dateMin );
            }
        });

        if ($("#from").data("from") != "") {
            var n = $("#from").data("from");
            $("#from").datepicker("setDate", n)
        }else{
            $("#from").datepicker("setDate", t);
        }
        if ($("#to").data("to") != "") {
            var r = $("#to").data("to");
            $("#to").datepicker("setDate", r);
            $("#to").datepicker({
                minDate: $("#from").val()
            })
        }else{
            $("#to").datepicker("setDate", "+7");
        }

        update_days();

    });
    $(".cal").click(function() {
        $(this).find('input').datepicker("show");
    });/*
    $("#cal_to").click(function() {
        $("#to").datepicker("show")
    })*/
}

function update_days() {
    setTimeout(function(){
        var from = $("#from").datepicker("getDate");
        var to = $("#to").datepicker("getDate");
        //dayDiff = Math.ceil((to - from) / (1000 * 60 * 60 * 24));
        var dayDiff = '';
        if(!isNaN(to.valueOf()) && !isNaN(from.valueOf())){
            //dayDiff = Math.floor((to.getTime() - from.getTime()) / 86400000);
            dayDiff = Math.ceil((to.getTime() - from.getTime()) / 86400000);
        }
        if (dayDiff >= 0) {
            $("#n_notti").html("");
            $("#n_notti").html(dayDiff);
            if (dayDiff > 1) {
                $("#n_notti_label").html(LN_NOTTI);
            } else {
                $("#n_notti_label").html(LN_NOTTE);
            }

            $("#giorni_sogg").val(dayDiff);
        } else {
            $("#n_notti").html("0");
        }
    }, 100);
}

