                        $(".fix-home" + ' LI').removeClass('bgs-current');
                        next.addClass('bgs-current');
                        
                        next.fadeIn( 1000 );
                        current.fadeOut( 1000 );
                        
                        _bgStretcherTm = setTimeout('$.fn.bgStretcher.slideShow()', 1000);
                        stepSlide = i;
                    }
                });

                var divs = $('.fix-home'),
                    limit = $('.divertimento').offset().top - ($(window).height()/2);  /* scrolltop value when opacity should be 0 */

                $(window).on('scroll', function() {
                   var st = $(this).scrollTop();

                   /* avoid unnecessary call to jQuery function */
                   if(!isMobile || !($(window).width()<700)){
                       if (st <= limit) {
                          divs.css({ 'opacity' : (1 - st/limit) });
                       }else{
                        divs.css({ 'opacity' : 0 });
                       }
                   }
                });
            }); 
