function fixaFooter() {
    $("footer").css({
        position: "static"
    });
    var e = $(window).height();
    if ($("footer").offset()) {
        var t = $("footer").height() + $("footer").offset().top
    } else {
        var t = $("footer").height()
    }
    if (e > t) {
        $("footer").css({
            width: "100%",
            position: "fixed",
            bottom: "0px"
        })
    } else {
        $("footer").css({
            position: "static",
            bottom: "0px"
        })
    }
}

