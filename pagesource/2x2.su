                        });

                $(".like").click(function () {
                    var elem = $(this);
                    $.ajax({
                        url: "/likes/toggle/" + elem.attr("name"),
                        async: false,
                        dataType: "json",
                        type: "POST",
                        success: function (json) {
                            if (json.result == 'on') {
                                elem.attr("class", "like active");
