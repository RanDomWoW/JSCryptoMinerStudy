<script type="text/javascript">
$(function() {
	var loginMsg = '';
	var iframeMsg = "";
	var permissionMsg = '';
	var message = loginMsg || permissionMsg ||iframeMsg;
	if (message) {
		if (window.top !== window.self) {
		$("#_msg",window.top.document).val(message);	
		$("#_indexForm",window.top.document).submit();
		}else{
			window.top.$info(message);
		}
	}
	listenKeyEvent();
});
	function formCheck(obj){
		return $("#eiForm").form("validate");
	} 
	function login(){
		var istmp=$("#loginForm1").form("validate");
		if(!istmp) return false;
		$("#loginForm1").submit();
	}   
	function eiSq(obj) {
		window.location.href="/ei/Ei!show.action";
	}
	function wljyptSq(obj) {
		window.location.href="/tpweb/TradePlatWeb!applyDesc.action";
	}
	function reCaptcha() {
	    document.getElementById("captcha").src = "/websys/WebUser!captcha.action?r=" + Math.random();
	}
	
	//监听回车事件
	function listenKeyEvent(){
		var $inp = $('#loginForm1 input'); //所有的input元素
		$inp.keypress(
			function (e) { 
		 	    var key = e.which; //e.which是回车按键的值
		    	if (key == 13) {
		    	    login();
		   	 	}
			});
	}
	function eiSubmitClick(){
		$("#eiForm").submit();
	}
</script>

