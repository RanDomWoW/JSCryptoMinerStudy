<script type="text/javascript">
    $(function(){
        $.ajax({
            url:"/web/FriendLink!list.action",
            type:"POST",
            dataType:"json",
            success:function (response) {
                /*if (html) {
                    $('#yqlj').html(html);
                    $('#yqlj').marquee();
                }*/
				if(response.result){
					var htmlstr="";
					var list=response["custom-content"]["friendLinkList"];
					var rootPath=response["custom-content"]["rootPath"];
					var visitedNum=response["custom-content"]["visitedNum"];
					$("#visited").html(visitedNum);
					
					var linkSize=list.length,widths="",imgPath="";
					for(var i=0;i<list.length;i++){
						title=list[i].title;
						if(htmlstr!="") htmlstr+="&nbsp;"
						htmlstr+="<a href='"+list[i].url+"'";//<li>
						htmlstr+=" title='"+list[i].title+"' target='_blank'>";
						imgPath=list[i].imgPath;
						if(('ts'=='cz'||'ts'=='ts')&&imgPath!=""){
							htmlstr+="<img class='linkImg' height='37' src='"+rootPath+imgPath+"' onload='javascript:DrawImage(this)'/>";
							widths=256;
						}else{
							if(title.toString().length>13){
								htmlstr+=title.substr(0,11)+"...";	
							}else{
								htmlstr+=title;
							}
							widths=162;
						}
						htmlstr+="</a>";//</li>
					}
					if(imgPath==""){//没有图片，还是显示文字的友情链接
						$("#baDiv_link").css("height","20px");
						$("#baDiv_link").css("padding","0px 8px 8px 8px");
					}
					if(htmlstr){
						//htmlstr="<ul class='empty_list'>"+htmlstr+"</ul>";
						$('#baContent_link').html(htmlstr);
						//$('#yqlj').marquee();
						if((('ts'=='cz'||'ts'=='ts')&&linkSize>=4) || ('ts'!='cz'&&'ts'!='ts'&&linkSize>=6)){
							var linkWidth=(widths*linkSize<=975?975:widths*linkSize);
							$("#baBox_link").css("width",linkWidth);
							$("#baContent_link").css("width",linkWidth+"px");
							$("#baShadow_link").css("width",linkWidth+"px");
							$("#baDiv_link").css("width",2*linkWidth+"px");							
							new MarQuee_link("baBox_link","baContent_link","baShadow_link",10); 
						}
					}
				} else {
					$error("访问出错，错误信息：" + response.msg );
				}
            }
        });
    });

	function MarQuee_link() {
		this.conBox = document.getElementById(arguments[0]);
		this.conContent = document.getElementById(arguments[1]);
		this.conShadow = document.getElementById(arguments[2]);
		this.speed = arguments[3];
		var mqobj = this;
		this.conShadow.innerHTML = this.conContent.innerHTML;
		this.Start = function() {
			mqobj.StartFun = function() {
				if (mqobj.conShadow.offsetWidth - mqobj.conBox.scrollLeft <= 0)
					mqobj.conBox.scrollLeft -= mqobj.conContent.offsetWidth;
				else
					mqobj.conBox.scrollLeft++;
			}

			mqobj.conBox.onmouseover = function() {
				clearInterval(mqobj.interVal);
			}
			mqobj.conBox.onmouseout = function() {
				mqobj.interVal = setInterval(mqobj.StartFun, mqobj.speed);
			}
			mqobj.interVal = setInterval(mqobj.StartFun, mqobj.speed);
		};
		this.Start();
	}
    function DrawImage(p){
    	if(navigator.userAgent.indexOf("MSIE 6.0")>=0){ //IE6需要用JS来判断图片缩放
    		if(p.width>250) p.width="250";
    	}
    }	
</script>




