<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<script>
var $j = jQuery.noConflict();

var json_data = ""; //データベースから取ってきたデータを格納する配列。
var personal_data = {}; //ユーザーのこのページ内の行動を記録する連想配列。

var html = []; //rem-banner-box以下に挿入するHTMLを格納する配列。
var result_box = [];
var img_width = []; //画像のwidthを格納する配列。
var img_path = []; //画像のパスを格納する配列。
var w_height = window.innerHeight;
var rm_banner_position_y = [];
var view_flg = []; //リマケバナーがスクリーン内にあるかどうかを判定するフラグ。

var item_parent  = ".rem-banner-box";
var item         = item_parent + " > .item";
var item_html    = []; //liのHTMLを一行ずつ格納していく配列。
var item_active  = []; //表示させる画像の配列（item_html）のキーを格納しておく配列。
var item_next    = []; //activeより1大きい数を格納しておく配列。
var item_before  = []; //item_activeの値を更新する前にactiveを保存しておくための変数。setTimeout関数を呼び出すときに使用。
var item_max_num = []; //全画像数を格納する配列。

/*=================スライド関数(start)=================*/
//基本的にはnext_show()とitem_slide()がお互いを呼び合って回ることでスライドショーを実装しています。

//次のアイテムを出力する
function next_show(rm_number) {
  item_before[rm_number] = item_active[rm_number];

  if(item_active[rm_number] == item_max_num[rm_number]){
    //配列のキーが最大になったとき、初期化。
    item_next[rm_number] = 0;
    item_active[rm_number] = 0;
  } else {
    item_next[rm_number] = item_active[rm_number] + 1;
    item_active[rm_number]++;
  }
  $j(item_html[rm_number][item_next[rm_number]]).addClass("next").appendTo(result_box[rm_number]).css("opacity",0).css("display","none");
  setTimeout("item_slide(" + rm_number + ")", json_data[item_before[rm_number]]["ms_show_time"]);
}

//アイテムをスライドさせる
function item_slide(rm_number) {
  if (view_flg[rm_number] == true) {//画面に表示されているか
    personal_data[item_active[rm_number]]["view_count"]++;
  }

  $j(result_box[rm_number]).children('.active').animate({//activeを消す時のアニメーション

    "left":"-50px",
    "opacity":0
  }, 500, function(){
    $j(result_box[rm_number]).children('.active').remove();//activeを完全に削除

      $j(result_box[rm_number]).children('.next').animate({//nextを入れる時のアニメーション
      "display" :"none",
      "left":"0",
      "opacity":1
    }, 500).removeClass("next").addClass("active").css("display","block");
    next_show(rm_number);
  });
}
/*=================スライド関数(end)=================*/

function jump() {
  personal_data[item_before]["click_count"]++;

  $j.ajax({
    url: 'https://www.toshin.com/remarketing/updateCount.php',
    type: 'POST',
    data: {data : JSON.stringify(personal_data)},
    timeout: 5000,
  }).done(function(data){
      //personal_dataの初期化。
    $j(json_data).each(function(i){
      personal_data[i]["banner_id"] = json_data[i]["banner_id"];
      personal_data[i]["click_count"] = 0;
      personal_data[i]["view_count"]  = 0;
    });
    }).fail(function(){
      // 通信失敗時の処理を記述
    });
  window.open(json_data[item_before].link, '_blank');
}

$j(window).on("beforeunload", function() {//ページ離脱時の処理
  $j.ajax({
    url: 'https://www.toshin.com/remarketing/updateCount.php',
    type: 'POST',
    data: {data : JSON.stringify(personal_data)},
    timeout: 5000,
  }).done(function(data){
      //personal_dataの初期化。
    $j(json_data).each(function(i){
      personal_data[i]["banner_id"] = json_data[i]["banner_id"];
      personal_data[i]["click_count"] = 0;
      personal_data[i]["view_count"]  = 0;
    });
    }).fail(function(){
      // 通信失敗時の処理を記述
    });
});

function parseCookie(data) {
  json_data = data;
  result_box = document.getElementsByClassName("rem-banner-box");
  Object.keys(result_box).forEach(function (rm_number) {//ページ内にあるリマケバナーの数(rem-banner-boxクラスの数)で回す。

    /*リマケバナーを同一ページに複数埋め込む場合にそれぞれ初期化する必要のある変数*/
    max_height   = 0;
    html[rm_number] = "";
    img_width[rm_number] = ""; //画像のwidthの初期値。
    img_path[rm_number] = 12; //画像のパスの初期値。
    view_flg[rm_number] = false; //フラグの初期値。

    item_html[rm_number] = [];
    item_active[rm_number]  = 0;
    item_next[rm_number]    = 1;
    item_before[rm_number]  = 0;
    item_max_num[rm_number] = -1; //回して加算していく時に配列のキーと同期させるために初期値は-1に設定しておく。

    if($j(this[rm_number]).attr("data-width")){
      img_width[rm_number] = $j(this[rm_number]).attr("data-width");
      img_width[rm_number]  = img_width[rm_number].slice(1); //img_widthに"w数字"のような値が入っている。"w"だけを除外してwidthのパーセントを取得する。
    }
    if($j(this[rm_number]).attr("data-path")){
      img_path[rm_number] = $j(this[rm_number]).attr("data-path");
      img_path[rm_number]  = img_path[rm_number].slice(1); //img_pathに"p数字"のような値が入っている。"p"だけを除外してpathの指定数値を取得する。
    }

    //json_dataからHTMLの要素を作成して配列に一個ずつ入れていく。
    Object.keys(json_data).forEach(function (key) {
      item_html[rm_number][key] = "<li class=\"item\"><a onclick=\"jump(); return false;\" href=\"" + this[key]["link"] + "\"><img class=\"imgClass\" style=\"width: " + img_width[rm_number] + "%;\" src=\"" + this[key]["path_" + img_path[rm_number]] + "\" alt=\"" + this[key]["alt_name"] + "\"></a></li>";
      item_max_num[rm_number]++; //ついでに要素の最大個数もカウントする。
    }, json_data);

    //HTMLの要素すべてが入っている配列の作成。
    Object.keys(item_html[rm_number]).forEach(function (i) {
      html[rm_number] += item_html[rm_number][i];

/*      console.log($j(this).clientHeight);*/
      //ついでに各要素の高さを取得し、最大値をmax_heightに代入
/*      this_height = $j(this).height();
      if(max_height < this_height){
        max_height = this_height;
      }*/
    }, item_html);
    //スライドの大枠の高さを、アイテムの高さの最大値に合わせる。
/*    $j(this).css("height", max_height);*/

    //result_boxがあればinnerHTMLを更新。
    if (result_box[rm_number] != null) {
      result_box[rm_number].innerHTML = html[rm_number];
      var img = new Image();
      img.src = $j(this).find('.imgClass')[0].src;

      img.onload = function() {
        $j(result_box[rm_number]).css("height", $j(result_box[rm_number]).find('.imgClass')[0].height);
      }
    }

    //画面に表示されているかの判定(view_flgの生成)
    if ( $j('.rem-banner-box').length ) { //offset().topを使う前に存在を判定しておく。
      $j(".rem-banner-box").each(function(i){
        rm_banner_position_y[i] = $j(this).offset().top;
      });
    }
    if (rm_banner_position_y[rm_number] < w_height) { //ロード時にすでに画面内にあるかどうかの判定。
      view_flg[rm_number] = true;
    }
    $j(window).scroll(function(){ //あとはスクロールするごとに判定していく。
      if ( ($j(window).scrollTop() < rm_banner_position_y[rm_number]) && (rm_banner_position_y[rm_number] < $j(window).scrollTop() + w_height) ) {
        view_flg[rm_number] = true;
      } else {
        view_flg[rm_number] = false;
      }
    });

  }, result_box);

  //personal_dataの初期化。
  $j(json_data).each(function(i){
    personal_data[i] = {};
    personal_data[i]["banner_id"] = json_data[i]["banner_id"];
    personal_data[i]["click_count"] = 0;
    personal_data[i]["view_count"]  = 0;
  });

  //最初の処理
  $j(item).addClass("active");
  Object.keys(result_box).forEach(function (rm_number) {
    $j(result_box[rm_number]).children('li').not(":first").remove();
    next_show(rm_number); //スライド開始

  }, result_box);
}

</script>

