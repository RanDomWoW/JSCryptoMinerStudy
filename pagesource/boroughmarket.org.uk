<script type="text/javascript">
    $(function(){
        $('.box--newsletter__form').submit(function(){

            $.ajax({
                type: "POST",
                url: '/base/cms/newsletter/add-subscriber',
                data: {email: $('.email_form_field').val()}
            }).done(function(data) {
                if (data.success) {
                    $('#news-box-heading').text("Success");
                    $('#news-box-text').text("Thank you for signing up to our newsletter");

                    $.magnificPopup.open({
                      items: {
                        src: '#newsletter-success-popup'
                      },
                      type: 'inline'
                    });

                    $('.email_form_field').val("");
                } else {
                    $('#news-box-heading').text("Whoops...");
                    $('#news-box-text').text(data.error);

                    $.magnificPopup.open({
                        items: {
                            src: '#newsletter-success-popup'
                        },
                        type: 'inline'
                    });

                    $('.email_form_field').val("");
                }
            });

            return false;
        });
    });
</script>

