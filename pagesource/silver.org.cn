    <script type="text/javascript">
		$(function(){
			var fix_box = $(".fix_bottom_position");
			var fix_small = $(".fixbottom_wrap_small");
			if($.cookie("isClose") != 'yes'){
				fix_small.css("left","-140px");
				fix_box.css("left","0");
				fix_box.show();
				$.cookie("isClose",'yes',{ expires:1/144});
			}
			else{
				fix_box.hide();
				fix_box.css("left","-100%");
				fix_small.css("left","0");
			}
			
			ttm = setTimeout(gb_fn,10000);
			function gb_fn(){
				fix_box.animate({"left":"-100%"},500,function(){
					fix_box.hide();
				})
				fix_small.delay(500).animate({"left":"0"},300);
			}
			$(".fix_bottom_position .close").click(function(){
				gb_fn()
				clearTimeout(ttm);
				//十分钟
				$.cookie("isClose",'yes',{ expires:1/144}); 
			})
			
			fix_small.click(function(){
				$(this).animate({"left":"-140px"},300,function(){
					fix_box.show();
					fix_box.animate({"left":"0"},500);
				});
			})
		})
