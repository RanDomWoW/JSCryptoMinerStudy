<script type="text/javascript">

	var index = 0;

	var items = $( '.stagemain' );

			

	function selectItem( i ) {

		$( '.stagemain a' ).removeClass( 'sn-active' );

		$( '.stagesub a' ).removeClass( 'sn-active' );

		$( '.stagemain ul' ).hide();							

		$( i ).find( '.stagesub a' ).addClass( 'sn-active' );

		$( i ).children( 'ul' ).show();

		var target = $( i ).children( 'a' ).addClass( 'sn-active' ).attr( 'data-blurb' );

		$( '.stageitem' ).hide();

		$( '#stage-' + target ).show();

	}

			

	function showNext() {

		++index;

		if ( index > items.length - 1 )

			index = 0;

		selectItem( items[ index ] );

	}

			

	var timer = setInterval( 'showNext()', 4000 );

	

	$( document ).ready(

		function() {



			$( '.item' ).removeClass( 'item' );

			$( '.stageitem' ).removeClass( 'active' );

			$( '#stage-respondus' ).show();

			$( '.stagemain' ).unbind()

				.mouseenter(

					function( e ) {				

						clearInterval( timer );		

						selectItem( $( this ) );

						for ( index = 0; index < items.length && items[index] != $( this )[0]; index++ );

					}

				)

				.mouseleave(

					function() {

						timer = setInterval( 'showNext()', 4000 );

					}

				);

			$( '.stage' ).unbind()

				.mouseenter(

					function( e ) {				

						clearInterval( timer );		

					}

				)

				.mouseleave(

					function() {

						timer = setInterval( 'showNext()', 4000 );

					}

				);

			$( '.topmain' ).unbind()

			.mouseenter(

				function() {

					$( this ).children( 'ul' ).show();

					$( this ).children( '.tn-primary' ).addClass( 'tn-active' );

				}

			)

			.mouseleave(

				function() {

					$( this ).children( 'ul' ).hide();	

					$( this ).children( '.tn-primary' ).removeClass( 'tn-active' );

				}

			);

		}

	);

</script>



