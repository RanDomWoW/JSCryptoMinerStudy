<script type="text/javascript">
    function globalNOP() { return false; }

    $('a').live('click', function(event) {
        $(event.target).click(globalNOP);
        setTimeout(function(){
            $(event.target).unbind('click', globalNOP);
        }, 1000);
    });

    $('input[type="submit"], button[type="submit"]').live('click', function(event) {
        $(event.target).click(globalNOP);
        setTimeout(function(){
            $(event.target).unbind('click', globalNOP);
        }, 1000);
    });

	$.currentScript="frontpage.pl";
	$.dsPreload={};
	$.autoSaveEnabled=0;
	
</script>

