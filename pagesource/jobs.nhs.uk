<script type='text/javascript'>if ( !($.browser.msie && parseInt($.browser.version) <= 7)) {

var typeaheadConfig = {
    justChanged: false,
    formatRemove: function ($remove, $item) {
        var itemTitle = $item.attr('title'),
            removeTitle = 'Remove ' + itemTitle + ' from your search';
        $remove.attr('title', removeTitle);
        return 'x';
    },
    onAdd: function (data, $item, initial) {
        this.closest('.searchField, .formField').children('.mp_label').addClass('hidden');
    },
    onChange: function (type, data, $item) {
        typeaheadConfig.justChanged = true;
        var currentInput = $('input', $item),
            currentVal = currentInput.val(),
            currentTitle = $item.attr('title'),
            itemNum = this.manifest('values').length,
            itemList = this.prev();
        if (itemNum){
            if (type === 'add'){
                itemList.children('.justAdded').removeClass('justAdded');
                $item.addClass('justAdded');
            } else {
                itemList.children('.justAdded').removeClass('justAdded');
            };
            var existingItems = itemList.children().not('.justAdded');
            $(existingItems).each(function(index) {
                var itemVal = $('input', this).val(),
                    itemTitle = $(this).attr('title');
                if (type === 'add'){
                    if (currentVal === itemVal){
                        currentInput.val('');
                        return false;
                    };
                } else {
                    if (currentTitle === itemTitle && currentVal != ''){
                        $('input', this).val(currentVal);
                        return false;
                    };
                };
            });
        };
    },
    marcoPolo: {
        url: '/typeahead/',
        delay: 250,
        minChars: 1,
        highlight: false,
        formatData: function (data) {
            return data.response.docs;
        },
        formatItem: function (data, $item) {
            //This is where we can try to highlight terms in the dropdown
            return data.term;
        },
        formatNoResults: function (q, $item) {
            return false;
        },
        formatError: function ($item, jqXHR, textStatus, errorThrown) {
            return "<em>Typeahead not currently available</em>";
        },
        formatMinChars: function (minChars, $item) {
            return '<em>Your search must be at least <strong>' + minChars + '</strong> characters</em>';
        },
        onBlur: function () {
            var widgetItem = this.parent().find('.mf_item').length;
            if (widgetItem === 0) {
                this.closest('.searchField, .formField').children('.mp_label').removeClass('hidden');
            };
        },
        onChange: function (q) {
            var input = this;
            setTimeout(function(){
                if ($('.mp_list').is(':visible')) {
                    input.parent().addClass('noRadius');
                } else {
                    input.parent().removeClass('noRadius');
                };
            },300);
        },
        onRequestBefore: function(){
            //Useful for showing a loading spinner if the request is going to take some time
        },
        onRequestAfter: function(jqXHR, textStatus){
           //Useful for hiding a loading spinner that's shown in onRequestBefore
        },
        onSelect: function (data, $item, initial) {
            this.val(data.term);
        }
    }
};

$('#typeahead_key').manifest(typeaheadConfig, 'option', {
    valuesName: 'keyword',
    formatDisplay: function (data, $item, $mpItem) {
        if ($mpItem) {
            var term = $mpItem.text(),
                displayName = term.split(' - ')[0];
            $item.attr('title', term);
            return displayName;
        } else {
            $item.attr('title', data);
            return data;
        };
    },
    formatValue: function (data, $value, $item, $mpItem) {
        if ($mpItem) {
            return data.term;
        } else {
            return data;
        };
    }
}).marcoPolo({
    data: {
        fq: 'type:NHTERM',
        rows: '10'
    },
    label: $('<span>e.g. physiotherapist, nurse, SFP-MED-340</span>').appendTo('.keyField')
}).removeAttr('placeholder');

$('#typeahead_loc').manifest(typeaheadConfig, 'option', {
    valuesName: 'loc_id',
    formatDisplay: function (data, $item, $mpItem) {
        if ($mpItem) {
            var term = $mpItem.text(),
                displayName = term.split(' - ')[0];
            $item.attr('title', term);
            return displayName;
        } else {
            data = data.toLowerCase().replace(/\b[a-z]/g, function(letter){
                return letter.toUpperCase();
            });
            data = data.replace(/&[A-Z]{1,1}/g, function(letter){
                return letter.toLowerCase();
            });
            $item.attr('title', data);

            return data;
        };
    },
    formatValue: function (data, $value, $item, $mpItem) {
        if ($mpItem) {
            return data.id + '|' + data.term;
        } else {
            data = data.toLowerCase().replace(/\b[a-z]/g, function(letter){
                return letter.toUpperCase();
            });

            return data;
        };
    }
}).marcoPolo({
    data: {
        fq: 'type:NHSUKNC',
        rows: '10',
        mode: 'location'
    },
    label: $('<span>e.g. Sunderland, BS8 1RL</span>').appendTo('.locField')
}).removeAttr('placeholder');

$('#employerName').marcoPolo({
    url: '/xi/agency_search/?',
    data: {
        rows: '10',
        type: 'json'
    },
    delay: 250,
    minChars: 3,
    submitOnEnter: true,
    formatData: function (data) {
        return data.response.docs;
    },
    formatItem: function (data, $item) {
        return data.term;
    },
    formatNoResults: function (q, $item) {
        return '<em>No results for <strong>' + q + '</strong></em>';
    },
    formatError: function ($item, jqXHR, textStatus, errorThrown) {
        return "<em>Typeahead not currently available</em>";
    },
    formatMinChars: function (minChars, $item) {
        return '<em>Your search must be at least <strong>' + minChars + '</strong> characters</em>';
    },
    onChange: function (q) {
        var input = this,
            strLength = q.length;
        if (strLength == 0){
            $('#client_id').remove();
        };
        setTimeout(function(){
            if ($('.mp_list').is(':visible')) {
                input.addClass('noRadius');
            } else {
                input.removeClass('noRadius');
            };
        },300);
    },
    onSelect: function (data, $item, initial) {
        if (initial === false){
            this.val(data.term).removeClass('noRadius');
            if ($('#client_id').length) {
                $('#client_id').val(data.id);
            } else {
                $('<input type="hidden" name="client_id" id="client_id" value="' + data.id + '" />').insertAfter(this);
            };
        };
    }
});

$('.mp_label').click(function(){
    $(this).hide().parent().find('input').focus();
});

$('.mf_item').each(function(index) {
    $(this).closest('.formField').children('.mp_label').addClass('hidden');
});

$('.mf_input').on('focus', function(){
    if (typeaheadConfig.justChanged == true) {
        typeaheadConfig.justChanged = false;
    };
}).keyup(function(e){
    var list = $(this).closest('div').next(),
        keycode = e.which;
    if (keycode == '13'){
        if (($(this).val() == '') && ($(this).manifest('values') == '')) {
            $(this).closest('form').submit();
        } else {
            if (typeaheadConfig.justChanged == true) {
                typeaheadConfig.justChanged = false;
            } else {
                $(this).closest('form').submit();
            };
        };
    } else {
        typeaheadConfig.justChanged = false;
    };
});

var locObj = $('#loc_id');
if (locObj.length){
    var locData = locObj.val(),
        locIds = locData.split('#');
    $('.mf_value[name="loc_id"]').each(function(i){
        $(this).val(locIds[i]);
    });
    locObj.remove();
};

$('.jsTypeahead').closest('.formField').find('label').addClass('alignMid');
}
</script>


	


