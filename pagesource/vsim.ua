<script type="text/javascript">
    $(function(){
        $(document).on('click', '.js_pdf_download', function(e){
            var pdfId = $(this).data('pdf');
            var oImg = document.createElement("img");
            oImg.setAttribute('src', '/pdf/download?pdf_id=' + pdfId);
