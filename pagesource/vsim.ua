    $(function () {
        var counter = 1;
        var interval = setInterval(function(){
            changeBanners();

            if(counter >= 7) {
                clearInterval(interval);
            }
            counter++;
        }, 30000);

        function getChangeableZoneIds()
        {
            var changeableZonesIds = [];
            $('*[data-changeable="1"]').each(function(){
                var zoneId = parseInt( $(this).attr('id') );

                if( !$(this).children().filter(function(){
                            return $(this).length ? true : false;
                        }).length ) {
                    return;
                }

                changeableZonesIds.push(zoneId);
            });

            return changeableZonesIds;
        }

        function changeBanners()
        {
            var changeableZonesIds = getChangeableZoneIds();

            banners.cmd.push(function(){
                if(changeableZonesIds.length > 1) {
                    // if mor then fix site_callback
                    return;
                }

                $.each(changeableZonesIds, function(index, zoneId) {
                    if( typeof banners.displayedZones[zoneId] != 'undefined' ) {
                        banners.displayedZones[zoneId].is_display = false;
                        banners.defineSlot(zoneId);
                    }
                });
                banners.request();
                $.each(changeableZonesIds, function(index, zoneId) {
                    var zone = $('#zone_' + zoneId);

                    if( zone.find('*[data-adsense="0"]').length ) {
                        var back = zone.parent().children().filter(function () {
                            return $(this).attr('id') ? false : true;
                        });
                        back.attr('id', zone.attr('id'));
                        zone.removeAttr('id');

                        banners.site_callback = function(zoneId) {
                            zoneId = parseInt(zoneId);
                            var zone = $('#zone_' + zoneId);

                            if( changeableZonesIds.indexOf(zoneId) == -1 ) {
                                return;
                            }

                            zone.parent().toggleClass('flipped');

                            setTimeout(function () {
                                zone.parent().children().filter(function(){
                                    return $(this).attr('id') ? false : true;
                                }).html('');
                            }, 1000);
                        };
                    } else {
                        banners.site_callback = function(zoneId) {}
                    }

                    banners.display(zoneId);
                });
            });
        }
    });




    (function(){
        var banners = window.banners;

        var zones;
        if($(window).width() >= 768) {
            zones = window.banners_zones;
        } else {
            zones = window.banners_zones_mobile;
        }

        banners.cmd.push(function(){
            $.each(zones, function(index, zone_id) {
                if( window.TwentyMinutUa.data.banners['ZONE_HEAD_1200x3000']
                                .concat(window.TwentyMinutUa.data.banners['ZONE_MOB_300x250_1'])
                                .indexOf(parseInt(zone_id)) == -1 ) {
                    if($('#zone_' + zone_id).length) {
                        banners.display(zone_id);
                    }
                }
            });
        });
    })();




