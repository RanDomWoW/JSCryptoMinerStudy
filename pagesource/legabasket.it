<script>
    function showYoutubeLightBox(url) {
        $('#youtube_lightbox iframe').attr('src', "https://www.youtube.com/embed/"+url+"?autoplay=1");
        $("#youtube_lightbox").fadeIn(200);
        $(".dark_overlay").show(0);
    }
    
    function closeLightbox() {
        $('#youtube_lightbox iframe').attr('src', "");
        $("#youtube_lightbox").fadeOut(200);
        $(".dark_overlay").hide(0);
    }
    
    $(document).keyup(function(e) {
        if (e.keyCode == 27) {
             $('#youtube_lightbox iframe').attr('src', "");
             $("#youtube_lightbox").fadeOut(200);
             $(".dark_overlay").hide(0);
        }
    });

</script>


