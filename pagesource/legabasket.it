<script>
    $(document).ready(function () {
        $("#sign-in").click(function () {
           $("#login, .wrapper").show(); 
        });
        
        $(".x-exit, .wrapper").click(function () {
           $("#login, .wrapper").hide(); 
        });
    });
    
    function user_login() {
        var username = $("#username").val();
        var password = $("#password").val();
        
        $("#message").val("");
        $("#login-message").hide();
        
        $.post("/php/login.php",
        {
            username: username,
            password: password
        },
        function(data, status){
            if(data == "invalid") {
                $("#message").text("Username or Password is invalid");
                $("#login-message").show();
                return;
            }
            location.reload(); 
        });
        
    }
