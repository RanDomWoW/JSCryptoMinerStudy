function checkforavailability1(str)
{
$('.loader').show();
 $.ajax({
    url : 'emailchecksub.php',              //Declaration of file, in which we will send the data
    data:"email="+str,
        success : function(data){
            window.setTimeout(function(){
            if(data=='no_desirename')
            {
                $('#negative').css("display","");
                $('#positive').css("display","none");
                $('#btn').attr('disabled',true);
                
            }
            else if(data=='yes_desirename')
            {
                $('#positive').css("display","");
                $('#negative').css("display","none");
                $('#btn').removeAttr('disabled');
                
            }
            
            else
            {
                $('#positive').css("display","none");  
                $('#negative').css("display","none");
            }
            

            
        });
            $('.loader').hide();
        }
    });
}
</script>