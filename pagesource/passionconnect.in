        // custom function written to setPosition. 
        $(window).resize(setPosition());
        function setPosition() {
            var containerWidth = parseInt($('.container').outerWidth());
            //alert(containerWidth);
            var topHeightArray = [];
            var divInRow = 0;
            var divInRow = 0;
            if (containerWidth >= 970) {
                divInRow = 3;
                topHeightArray[1] = 0;
                topHeightArray[2] = 0;
                topHeightArray[3] = 0;
            } else if (containerWidth >= 750 && containerWidth < 970) {
                divInRow = 2;
                topHeightArray[1] = 0;
                topHeightArray[2] = 0;
            }
            var counterIncrementor = 1;

            $('.article_chunck').each(function () {
                if (divInRow == 2) {
                    $(this).css('position', 'absolute');
                    if (counterIncrementor > 2) {

                        if (counterIncrementor % 2 == 0) {
                            $(this).css('top', topHeightArray[2] + 60 + 'px');
                            $(this).css('left', 360 + 'px');
                            //alert('4=>'+topHeightArray[2]);
                        } else {
                            $(this).css('top', topHeightArray[1] + 'px');
                            $(this).css('left', '0px');
                            //alert('3=>'+topHeightArray[1]);
                        }
                    } else {
                        if (counterIncrementor == 1) {
                            $(this).css('top', '50px');
                            $(this).css('left', '0px');
                        } else {
                            $(this).css('top', '50px');
                            $(this).css('left', '360px');
                        }


                    }
                    if (counterIncrementor % 2 == 0) {
                        topHeightArray[2] = topHeightArray[2] + $(this).outerHeight();
                        //alert('even=>'+topHeightArray[2]);
                    } else {
                        topHeightArray[1] = topHeightArray[1] + $(this).outerHeight();
                        //alert('odd=>'+topHeightArray[1]);
                    }
                    counterIncrementor = counterIncrementor + 1;
                    $('.article_list').css('position', 'relative');
                    $('.article_list').css('height', $('.article_list').outerHeight() + 20 + 'px');
                }
                if (divInRow == 3) {
                    $(this).css('position', 'absolute');
                    if (counterIncrementor > 3) {

                        if (counterIncrementor % 3 == 0) {
                            $(this).css('top', topHeightArray[3] + 40 + 'px');
                            $(this).css('left', '760px');
                            //alert('4=>'+topHeightArray[2]);
                        } else if (counterIncrementor % 3 == 1) {
                            $(this).css('top', topHeightArray[1] + 40 + 'px');
                            $(this).css('left', +'0px');
                        } else if (counterIncrementor % 3 == 2) {
                            $(this).css('top', topHeightArray[2] + 40 + 'px');
                            $(this).css('left', '380px');
                        }
                    } else {
                        if (counterIncrementor == 1) {
                            $(this).css('top', '50px');
                            $(this).css('left', '0px');
                        } else if (counterIncrementor == 2) {
                            $(this).css('top', '50px');
                            $(this).css('left', '380px');
                        } else {
                            $(this).css('top', '50px');
                            $(this).css('left', '760px');
                        }


                    }
                    if (counterIncrementor % 3 == 0) {
                        topHeightArray[3] = topHeightArray[3] + $(this).outerHeight();
