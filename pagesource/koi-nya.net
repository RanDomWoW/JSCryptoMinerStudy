	<script type="text/javascript">
	//<![CDATA[
		/** Descubre u oculta un spoiler a partir de la cabecera spoiler. */
	function spoiler(cabecera) {
		// Putada: dependiendo de que haya espacios, puede estar en posición last, 1, 3, ¡a saber! Lo buscaremos entre todos
	  var total = cabecera.parentNode.childNodes.length;
	  var n;
	  for (var i=0; i<total; i++) {
		  n = cabecera.parentNode.childNodes[i];
		  if (n.nodeType==3) // Es un puto salto de línea jodevidas, lo ignoro
		  	continue;
		  if (n.getAttribute("class") == 'spoiler_content')
		  	break;
	  }
	  if (n.nodeType!=3) {
		  if(n.style.display == 'block') {
			  n.style.display = 'none';
		  } else {
			  n.style.display = 'block';
		  }
	  }
	  return false;
	}
	
	jQuery(document).ready(function(){
		jQuery("div.spoiler_title").click(function () {
			spoiler(this);
		});
	});
	//]]>
