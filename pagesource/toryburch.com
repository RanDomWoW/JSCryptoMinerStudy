        var submitTrigger = (triggerSubmit != undefined && triggerSubmit == true)?true:false;
        var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;

