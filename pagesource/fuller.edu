select[multiple] {
    height: 100px !important;
}

/* 404 report button */
.report-404-button,
.report-404-button:hover,
.report-404-button:focus {
    padding: 0;
    margin: 0;
    background: none;
    border: 0;
    color: #CA8A35;
    font-weight: 600;
}

.report-404-button:hover {
    color: #d68844;
}
article.fl-404 {
    padding-top: 0;
}		</style>
