			/* MOVED FROM CHILD THEME EDITOR CSS FILE */

h2 {
	text-transform: capitalize!important;	
}
h4 {
	color: #6d6d6d;
}

a.fl-button, a.fl-button:hover {
border: 0;
}


/*
// Lumpkin's Stuff
*/

/* FORM ASSEMBLY STUFF */

label {
font-family:"Sentinel A", "Sentinel B";
font-size: 18px;
    font-style: normal;
    font-weight: 400;
    letter-spacing: 0.4px;
    color: #0a0a0a;
}
input.primaryAction {
    font-family: "alternate-gothic-no-3-d"!important;
    font-size: 18px!important;;
    letter-spacing: 1px!important;
    color: #ffffff!important;
    text-transform: uppercase;
    padding: 10px 20px 8px!important;
    border: 0!important;
    -webkit-border-radius: 4px!important;
    background: #CA8A35!important;
}
.supportInfo {
display: none;
}

/*END FORM ASSEMBLY STUFF */

.fl-module-content p,
.fl-module-content ul,
.fl-module-content ol {
    font-family: "Sentinel A", "Sentinel B";
	font-size: 18px;
    font-style: normal;
    font-weight: 400;
    letter-spacing: 0.4px;
	color: #0a0a0a;
}

.fl-module-content ol li {
	margin-bottom: 1em;
}

.fl-module-rich-text ul {
	
}

.center {
    margin: auto;
    text-align: center;
}

.callout, h1.callout {
    font-family: "alternate-gothic-no-3-d";
    font-size: 1.3em;
    margin: 1em 0;
    color: #322D60;
    clear: both;
    line-height: 1.3;
}

h1 {
    font-family: "alternate-gothic-no-3-d";
    /*color: #16163d; */
    font-size: 43px;
    text-transform: uppercase;
    color: #322D60;
    line-height: 1.1;
    margin-top: .5em;
    letter-spacing: 4px;
}

h2 {
    font-size: 31px;
    font-family: "alternate-gothic-no-3-d";
    text-transform: uppercase !important;
    color: #0a0a0a;
    margin-bottom: .5em;
    line-height: 1.3;
    margin-top: .5em;
    letter-spacing: .5px;
}

h3 {
    font-family: "Sentinel A", "Sentinel B";
    font-size: 28px;
    /*font-style: italic;*/
    font-weight: 700;
    line-height: 1.1;
    /*text-transform: uppercase; */
    color: #272727;
}

.main h3 {
    color: #272727;
}

h4 {
    font-family: "alternate-gothic-no-3-d";
    font-size: 19px;
    color: #6d6d6d;
    /*font-weight: normal;*/
    margin-top: 0;
text-transform: uppercase;
}

h4.bottom-border {
    border-bottom: 1px solid rgb(204, 204, 204);
    margin-bottom: 0.75em; padding-bottom: 0.5em;
}

h4 a {
	color: #CA8A35;
}

h5 {
    font-family: "alternate-gothic-no-3-d";
    font-size: 15px;
}

.georgia-caps{
    font-family: Georgia, serif;
    font-size: 3.5em;
    font-style: italic;
    text-transform: uppercase;
    vertical-align: middle;
    display: table-cell;
    padding-left: 0.5em;
}

a {
    font-weight: bold;
    color: #c3832e;
    text-decoration: none;
}

a:hover, a:focus {
    text-decoration: none;
}

.bg {
    background:#F2F2F2;
}

.inset {
    padding: 1em 1em 0 1em;
}
.bg-inset {
    background:#F2F2F2;
    padding: 1em 1em 0 1em;
}

.secondary-content {
    color: #ffffff !important;
}

.fl-module-rich-text ul li {
    letter-spacing:1px;
    line-height: 1.5em;
    margin-bottom:.75em;
}

/* [REMOVED SUB NAV STYLE] */

/*
// Buttons
*/

a.button {
    font-family: "alternate-gothic-no-3-d";
    font-size: 18px;
    display: inline-block;
    padding: 8px 20px 5px;
    text-decoration: none;
    text-transform: uppercase;
    letter-spacing: 1px;
    -webkit-transition: all .15s ease-in-out;
    -moz-transition: all .15s ease-in-out;
    -o-transition: all .15s ease-in-out;
    -ms-transition: all .15s ease-in-out;
    transition: all .15s ease-in-out;
    color: #FFFFFF;
    margin-bottom: .5em;
    border-radius: 3px;
}

a.button-primary {
    background-color: #CA8A35;
}

a.button-primary:hover {
    background-color: #d68844;
}

.fl-builder-content .fl-module-content .fl-button-wrap a.fl-button {
    padding: 10px 20px 8px;
	border: 0;
}

.fl-module-content .fl-button-wrap a.fl-button span.fl-button-text {
	font-family: "alternate-gothic-no-3-d";
    font-size: 18px;
    letter-spacing: 1px;
	color: #ffffff;
	text-transform: uppercase;
}

.fl-module-content a.fl-button span.fl-button-text:hover {
	color: #ffffff;
}

.button-secondary,
.button-secondary:link {
    font-size: 15px;
    text-transform: uppercase;
    margin: 1em 0;
    display: inline-block;
    padding: 8px 25px 7px;
    color: #ffffff;
    background: #6d6d6d;
    border-radius: 3px;
    text-align: center;
    border: 0;
    letter-spacing:1px;
}

a.button-secondary,
a.button-secondary:link {
    text-decoration:none;
}

a.button-secondary:hover {
    background-color: #0a0a0a;
}

/*
// End Buttons
*/

blockquote {
    padding: 10px 20px;
    margin: 0 0 20px;
    font-size: 17.5px;
    border-left: 0px solid #eee;
}

blockquote.pull-quote {
    margin: 1em 0;
    padding: 0 3em 0 0;
    font-family: inherit;
    font-style: normal;
    font-size: 1em;
    color: #272729 !important;
}

.cite {
    margin: .25em 0 1em;
    color: #676767 !important;
    font-weight: normal;
    text-transform: uppercase;
    font-size: .8em !important;
    display: block;
    clear: both;
    -webkit-font-smoothing: antialiased;
}

.bqstart {
    float: left;
    height: 45px;
    line-height: 45px;
    margin-top: -8px;
    padding-top: 0;
    margin-bottom: -20px;
    font-size: 200%;
    color: #c07511;
    margin-right: 3px;
}

.bqend {
    float: none;
    height: 5px;
    line-height: 10px;
    margin-top: 0;
    padding-top: 3px;
    padding-left: 5px;
    font-size: 200%;
    color: #c07511;
    position: relative;
    top: 10px;
}

.uppercase {
    text-transform: uppercase !important;
}
.caption-wrapper  {
    background: rgba(0,0,0,0.4);
    border: 0;
    opacity: 1;
    bottom: 0;
    width: 100%;
    left: 0;
    padding: 1em 2em;
}

.figcaption h1 {
    margin-bottom: 0;
    color: #fff;
    text-shadow: 0 2px 5px rgba(0,0,0,0.5);
    text-transform: uppercase;
    font-weight: 600;
    font-family: "proxima-nova","Arial Narrow",sans-serif;
    font-size: 2.5em;
    background: 0;
    padding: 0;
    display: table-cell;
    vertical-align: middle;
}

.purple-bg {
    background-color: #16163d;
}

.callout {
    font-size: 1.3em;
    margin: 1em 0;
    color: #322D60;
    clear: both;
    line-height: 1.3;
    font-family: "proxima-nova-condensed","Arial Narrow",sans-serif;
}


/* FORM ASSEMBLY STUFF */
.wFormContainer label
{color: inherit;
text-align: inherit;
font-weight: inherit;
font-size: inherit;
line-height: inherit;
border-radius: inherit;
padding-left: 0;

}

/* /end Lumpkin Stuff */

/* Role Based Navigation */
div.fl-page-bar.role-based {
	background-color: #e1e1e1;
    border-top: 0;
    padding-top: 5px;
    padding-bottom: 5px;
    display:none;
	border-bottom: 0;
}

ul#role-student,
ul#role-staff,
ul#role-alumni {
    display: none;
}

ul.role-menu li:first-child a {
    background-color: #CA8A35;
    color: #fff;
    border-radius: 3px;
}

ul.role-menu li:first-child a:hover {
    background-color: #d68844;
    color: #fff;
}

/*
// Header
*/

div.fl-page-bar {
    border-top: 3px solid #CA8A35;
    border-bottom: 1px solid #fff;
}

div.fl-page-header-wrap {
    border-top: 2px solid #f2f2f2
}

img.fl-logo-img {
	max-height: 40px;
}

img.fl-logo-img.large {
	max-height: 60px;
}

div.fl-page-header-container {
    padding-bottom: 0;
}

@media (max-device-width: 480px) {
	div.fl-page-bar {
		display: none;
	}
}


/*
// Main Navigation
*/

div.fl-page-nav-wrap {
    border: 0;
}

div.navbar-collapse ul.menu li a {
	font-family: "alternate-gothic-no-3-d";
	font-size: 1.15em;
}

.fl-page-nav ul.sub-menu {
    border-top: 0;
    border-bottom: 3px solid #CA8A35;
}

.fl-page-nav ul.sub-menu li a {
	transition: 0.3s;
}

.fl-page-nav ul.sub-menu li a:hover {
    background-color: #f2f2f2;
    transition: 0.3s;
}

/*
// Page Layout
*/

div.fl-page-content {
    padding-top: 20px;
}



/* FOOTER */


.fl-page-footer-widgets-container,
.fl-page-footer-widgets-container container a {
    letter-spacing: 1px!important;
/*    font-weight: 100!important; */
}
.vlist li, ul.vlist li {
    margin-bottom: 1em;
}
.fl-page-footer-widgets-container .fl-button {
    font-size: 12px;
    line-height: 5em;
    padding: 10px 20px;
    border-radius: 4px;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    background: #ca8a35;
}

.fl-page-footer-widgets-container a {
    text-decoration: none;
/*    font-weight:100!important; */
}

.fl-page-footer-widgets-container a,
.fl-page-footer-widgets-container  {
    font-size: 12px;
    line-height:1.5em;
}

.fl-page-footer-widgets-container ul li {
    margin-bottom: 1em!important;
}

.campus-list {
    text-transform: uppercase;
}

div.fl-page-footer-widgets-container i {
    font-size: 2.5em;
    padding-right:10px;
    color:#ffffff;
}

div.fl-page-footer-widgets-container i:hover {
    color:#ffffff;
}

div.fl-page-footer-widgets-container i:visited {
    color:#ffffff;
}

/* Faculty */

div.fl-builder-content-2358 * {
    color: #4d4d4d;
}

.fac-profile-container {
    position: relative;
}

.fac-profile-image img {
    padding-right: 20px;
}
.fac-school {
    position: absolute;
    bottom: -40px;
    right: -10px;
    width: 100px;
    height: 100px;
    border-radius: 50px;
    font-size: 15px;
    padding-top: 1.7em;
    text-align: center;
    background: #CA8A35;
}

.fac-school.school-sis {
    width: 110px;
    height: 110px;
    border-radius: 105px;
}

.fac-school p {
    color: #fff;
    margin-bottom: 2px;
}

p.fac-school-of {
    font-style: italic;
    font-weight: 100;
}

div.fac-social {
    background-color: #4d4d4d;
    display: inline-block;
    width: 36px;
    height: 36px;
    border-radius: 18px;
    text-align: center;
    margin-right: 5px;
}

div.fac-social:hover {
    background-color: #737373;
}

div.fac-social i.fa {
    font-size: 1.3em;
    color: #fff;
    line-height: 2em;
}

p.fac-school-name {
    font-family: "alternate-gothic-no-3-d";
    text-transform: uppercase;
}

p.fac-school-name.school-sot {
    font-size: 1.3em;
}

p.fac-school-name.school-sop {
    font-size: 1.1em;
}

p.fac-school-name.school-sis {
    font-size: 1.1em;
    line-height: 1em;
}

.fac-phone p,
.fac-email p {
    font-family: "alternate-gothic-no-3-d";
    margin-bottom: 0;
}

.fac-email a {
    font-weight: normal;
}

.fac-name h1 {
    font-family: “Sentinel A”, “Sentinel B”;
    text-transform: initial;
    font-style: italic;
	font-size: 70px;
	letter-spacing: 0px;
}

.fac-title h3,
.fac-education p {
    font-family: "alternate-gothic-no-3-d";
    font-style: normal;
    text-transform: uppercase;
}

.fac-expertise p,
.fac-courses p,
.fac-affiliations p,
.fac-awards p,
.fac-quote-attribution p {
    font-family: "Arial Narrow";
    font-size: 1em;
}

.fac-expertise p {
    line-height: 1.5em;
}

.fac-current {
    font-family: "alternate-gothic-no-3-d";
	font-size: 1.1em;
    text-transform: uppercase;
}

.fac-current-field {
    font-family: "Arial Narrow";
    font-size: 1em;
}

.fac-title h3 {
    font-size: 2.7em;
}
.fac-education p {
    font-size: 1.8em;
}

.fac-affiliations a {
	color: #CA8A35;
	font-weight: 100;
}

/* Quote */

.fac-quote-body p {
    font-style: italic;
    font-size: 2em;
    font-weight: 300;
    line-height: 1.2em;
}

span.fac-quote-plus {
	color: #CA8A35;
	font-weight: 600;
}

.fac-quote-attribution p {
    font-size: 1em;
    display: inline;
    font-style: italic;
}

.fac-quote-attribution a {
	color: #CA8A35;
	font-weight: 100;
}

/* Bio */

.fac-bio p {
    font-weight: 100;
}

.fac-bio a {
    color: #CA8A35;
    font-weight: 100;
}

/* Books */

div.fac-publications-item {
    margin-bottom: 2em;
}

/* Find Faculty on Fuller Studio */

h4.find-on-fs {
    font-family: "Sentinel A", "Sentinel B";
    text-transform: initial;
    font-size: 35px;
    position: relative;
    z-index: 1;
    margin-bottom: .5em;
}

h4.find-on-fs:before {
    border-top: 2px solid #CA8A35;
    content:"";
    margin: 0 auto; /* this centers the line to the full width specified */
    position: absolute; /* positioning must be absolute here, and relative positioning must be applied to the parent */
    top: 50%; left: 0; right: 0; bottom: 0;
    width: 100%;
    z-index: -1;
}

h4.find-on-fs span {
    background-color: #fff;
    margin-right: .2em;
}

section#fs-posts {
    width: 100%;
    position: relative;
}

.fs-post-single-box {
    position: relative;
    width: 22%;
    float: left;
    margin-right: 3%;
}

.fs-post-single-box:before {
    content: "";
    display: block;
    padding-top: 70%;
}

div.fs-post-single-content {
    background-size: auto 100%;
    background-position: center;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    padding: 25px;
}

/* Color overlay */
div.fs-post-single-content:before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: rgba(68,54,19,0.3);
    transition: .5s;
}

div.fs-post-single-content:hover:before {
    background-color: rgba(17,17,17,.7);
    transition: .5s;
}

div.fs-post-title {
    height: 100%;
    display: table;
    width: 100%;
    text-align: center;
}

div.fs-post-title h6 {
    color: #fff;
    font-family: "Sentinel A", "Sentinel B";
    font-size: 1.8em;
		font-weight: 100;
		font-style: italic;
    position: relative;
    line-height: 1.5em;
    display: table-cell;
    vertical-align: middle;
    text-shadow:
   -1px -1px 5px rgba(0,0,0,0.3),  
    1px -1px 5px rgba(0,0,0,0.3),
    -1px 1px 5px rgba(0,0,0,0.3),
     1px 1px 5px rgba(0,0,0,0.3);
}

@media (max-width: 1100px) {
    div.fs-post-title h6 {
        font-size: 1.1em;
    }
}

@media (max-width: 850px) {
    .fs-post-single-box {
        width: 100%;
        margin-bottom: 1em;
    }
    div.fs-post-title h6 {
        font-size: 2em;
    }
}

