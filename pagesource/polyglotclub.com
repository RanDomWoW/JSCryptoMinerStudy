						var str = '';
						
						if (state.s_title && (state.s_title != '0')){	
							var s_title = ' title="' + state.s_title + '" ';
						}						
						
						if (state.b_br == 1){
							str += '<hr />';
							//str += '<div class="pg_no_select">test</div>';
						}
						if (state.souscat_b_flag && (state.souscat_b_flag != '0')){
							str += "<img " + s_title + " class='flag' src = '/image/flag/lang/Language_" + $.trim(state.id) + ".gif?180219' /> ";
						}
						else if (state.souscat_scope != 'Macrolanguage'){
							str += "<span " + s_title + " class='pg_flag'>" + state.souscat_iso + "</span>";
						}
						if (state.souscat_scope == 'Macrolanguage'){
							str += '<span ' + s_title + ' class="bold font-medium">';
						}
						else{
							str += '<span ' + s_title + ' class="margin-left">';
						}
						
						str += state.text;
						
						str += '</span>';
						
						if (state.s_text2 && (state.s_text2 != '0')){
							str += ' (' + state.s_text2 + ') ';
						}
						
						if (state.souscat_nb && (state.souscat_nb != '0')){
							str += "<div class='margin-left font-small'>" + state.souscat_nb + " Members</div>";
						}
						return str;
					}
