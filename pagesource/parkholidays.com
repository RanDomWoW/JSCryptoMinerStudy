<script>

var chatTitle = "Have a question?";
var chatText = "Our friendly support advisors are online ready to answer all your questions.";

/*
$(function() {
    handleWebChatWidget($(window).width(),$(window).height());
});
*/

//Code to allow the live chat window to slide around like a DALEK
function handleWebChatWidget(windowWidth,windowHeight)
{
    //Declaring the screensize breakpoints
    var mobileWidth = 473;
    var dockedWidth = 673;
    var fullWidth = 768;

    var chatPanelDefaultWidth = 251; //The default width of the non-fixed panel
    var callMeHeight = 53; //The height of the dynamically created 'Call me' section of the panel


    if(windowWidth < mobileWidth + 1) //Mobile only - no floating widget, put it back into its default DOM position?
    {

        $(".chat").removeClass("chatFloat");
        $(".chat").css("width",chatPanelDefaultWidth);

        $("#chatTabEdge").css({"width":0,"left":0});
        $(".chat").attr("state","hidden");
        $(".chat").attr("prevState","mobile"); //Allows us to keep track of changes in screen size

    } else if (windowWidth < dockedWidth + 1) { //Docked, floating widget that is hid by default (click to activate)

        $(".chat").addClass("chatFloat");
        $(".chat").css("width",chatPanelDefaultWidth);
        $(".chat").css("bottom",(windowHeight/2)-(($(".chat").height()+callMeHeight)/2));

        var panelState = $(".chat").attr("state"); //Depending on state of panel, show or hide
        if(panelState=="shown")
        {
            $(".chat").css("right","0");
        } else if(panelState=="hidden") {
            var chatPanelWidth = $(".chat").width()+24;
            $(".chat").css("right",(0-chatPanelWidth)+"px");
        }

        //If the screen size has become docked, de-activate the pop-out
        if($(".chat").attr("prevState")!="docked" && panelState=="shown")
            chatTabClick();

        $("#chatTabEdge").css({"width":"20px","left":"-20px"});
        $(".chat").attr("prevState","docked"); //Allows us to keep track of changes in screen size

    } else  {

        $(".chat").addClass("chatFloat");
        $(".chat").css("width",chatPanelDefaultWidth);
        $(".chat").css("bottom",(windowHeight/2)-(($(".chat").height()+callMeHeight)/2));

        var panelState = $(".chat").attr("state"); //Depending on state of panel, show or hide
        if(panelState=="shown")
        {
            $(".chat").css("right","0");
        } else if(panelState=="hidden") {
            var chatPanelWidth = $(".chat").width()+24;
            $(".chat").css("right",(0-chatPanelWidth)+"px");
        }

        //If the screen size has become fullscreen, activate the pop-out
        if($(".chat").attr("prevState")!="full" && panelState=="hidden")
            chatTabClick();

        $("#chatTabEdge").css({"width":"20px","left":"-20px"});
        $(".chat").attr("prevState","full"); //Allows us to keep track of changes in screen size

    }

}

function chatTabClick() //When the tab on the left is clicked
{
    var chatPanelWidth = $(".chat").width()+24;

    if($(".chat").css("right")=="0px") //If the panel is showing
    {
        $(".chat").animate({
            right:"-"+chatPanelWidth+"px"
        },800,function() {
            $(".chat").attr("state","hidden");
        });
    } else if ($(".chat").css("right")==(0-chatPanelWidth)+"px") { //Else if the panel is hidden
        $(".chat").animate({
            right:0
        },800,function(){
            $(".chat").attr("state","shown");
        });
    }

}

</script>

