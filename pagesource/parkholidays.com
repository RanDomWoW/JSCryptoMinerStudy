                $("#newsletterSignUp").find("#Address1").val("");
                $("#newsletterSignUp").find("#Address2").val("");
                $("#newsletterSignUp").find("#City").val("");
            } else {
                var selectElement = $("#newsletterSignUp").find('#drpWinAddresses');
                if (selectElement != null) {
                    selectElement.show();
                    selectElement.empty();
                    for (var i = 0; i < response.length; i++) {
                        selectElement.append($("<option/>", {
                            text:response[i].StreetAddress+", "+response[i].Place,
                            value:response[i].Id
                        }));
                    }
                }
            }
        }
    };

    winPostcodeRetrieve = function (key, id, callBackFunction) {
        var scriptTag = $("#newsletterSignUp").find("#PCAa73f9bc2b60d4e4cbd595512478a3294");
        var headTag = document.getElementsByTagName('head').item(0);
        var strUrl = "";

        //Build the url
        strUrl = "http://services.postcodeanywhere.co.uk/PostcodeAnywhere/Interactive/RetrieveById/v1.10/json.ws?";
        strUrl += "&Key=" + encodeURI(key);
        strUrl += "&Id=" + encodeURI(id);
        strUrl += "&PreferredLanguage=";
        strUrl += "&UserName=";
        strUrl += "&CallbackFunction=" + callBackFunction;

        //Make the request
        if (scriptTag) {
            try {
                headTag.removeChild(scriptTag);
            }
            catch (e) {
                //Ignore
            }
        }
        scriptTag = document.createElement("script");
        scriptTag.src = strUrl;
        scriptTag.type = "text/javascript";
        scriptTag.id = "PCAa73f9bc2b60d4e4cbd595512478a3294";
        headTag.appendChild(scriptTag);
    };

    winPostcodeRetrieveCallback = function (response) {
        //Test for an error
        if (response.length == 1 && typeof (response[0].Error) != 'undefined') {
            //Show the error message
            $("#newsletterSignUp").find("#winAddressLookupErrorMessage").html(response[0].Description);
            $("#newsletterSignUp").find("#winAddressLookupErrorMessage").show();
        }
        else {
            //Check if there were any items found
            if (response.length == 0) {
                $("#newsletterSignUp").find("#winAddressLookupErrorMessage").html("Sorry, no matching items found");
                $("#newsletterSignUp").find("#winAddressLookupErrorMessage").show();

                $("#newsletterSignUp").find("#winAddress-selector").hide();
            }
            else {
                $("#newsletterSignUp").find("#Address1").val(response[0].Line1);
                $("#newsletterSignUp").find("#Address2").val(response[0].Line2);
                $("#newsletterSignUp").find("#City").val(response[0].PostTown);
            }
        }
    };

</script>


