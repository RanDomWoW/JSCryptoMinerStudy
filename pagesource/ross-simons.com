<script language="javascript">
var xmlHttp;
var countryCode = 'US';
var currencyCountry = 'US';

function linkLocalize(){
	checkCountryCookie();
	var url = this.href;
	countryCode = readCookie('CC');
	if(countryCode != 'US'){
		currencyCountry = readCookie('CURRENCY_COUNTRY');
		anchors = document.links;
		for(i=0;i<anchors.length;i++){
			// Remove from main nav
			if(anchors[i].className != 'linkMain' && anchors[i].href.indexOf('javascript') < 0  && anchors[i].href.indexOf('#') < 0 && anchors[i].className != 'homeDetails'){
				ele = anchors[i];
				if (window.attachEvent){
					ele.attachEvent('onclick', linkInterrupt);
				} else if (window.addEventListener) {
					ele.addEventListener('click',linkInterrupt,true);
				}
			}
		}
		//Check Welcome Mat
		checkWelcomeMat();	
	}
}

function checkCountryCookie() {
	// Handle caching
	urlCountryCode = getUrlVars()["cc"];
	if (typeof urlCountryCode != 'undefined'){
		makeCookie('CC',urlCountryCode,99);
		//document.write('Set via URL');
	}
	urlCurrencyCountry = getUrlVars()["curr"];
	if (typeof urlCurrencyCountry != 'undefined'){
		makeCookie('CURRENCY_COUNTRY',urlCurrencyCountry,99);
	}
	makeCookie('CURRENCY_SYMBOL','$',99);
	if (document.cookie.indexOf('CC=') < 0 && document.cookie.indexOf('cc=') < 0) {
		createXMLHttpRequest();
		xmlHttp.onreadystatechange = handleStateChange;
		xmlHttp.open("GET", "/localization/ajaxGetCountry.cfm?r=" + Math.random(), true);
		xmlHttp.send(null);
		//document.write('Set via IP');
	} else if (typeof urlCountryCode == 'undefined' && window.location.pathname == '/'){
		curr = readCookie('CURRENCY_COUNTRY');
		cc = readCookie('CC');
		if(cc != 'US'){
			thisLink = document.location.toString();
			if(thisLink.indexOf('?') > 0){
				if(thisLink.indexOf('cc=') < 0){
					newURL = '&cc=' + cc + '&curr=' + curr;	
					fullURL = thisLink + newURL;
				} else {
					newURL = 'cc=' + cc + '&curr=' + curr;	
					baseURL = thisLink.split('?')[0];
					fullURL = baseURL + "?" + newURL;
				}
			} else {
				var fullURL = thisLink + '?cc=' + cc + '&curr=' + curr;	 
			}
			document.location.href = fullURL;
		}
		//document.write('Set via Cookie');
	}
}

function createXMLHttpRequest() {
	if (window.ActiveXObject) {
		xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
	}
	else if (window.XMLHttpRequest) {
		xmlHttp = new XMLHttpRequest();
	}
}

function getUrlVars() {
	var vars = {};
	var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
		vars[key] = value;
	});
	return vars;
}

function makeCookie(c_name,value,exdays){
	var exdate=new Date();
	exdate.setDate(exdate.getDate() + exdays);
	var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString()+"; path=/; domain=.ross-simons.com");
	document.cookie=c_name + "=" + c_value;
}

function handleStateChange() {
	if(xmlHttp.readyState == 4) {
		if(xmlHttp.status == 200) {
			parseResults();
		}
	}
}

function parseResults() {
	var exdate=new Date();
	exdays = 365;
	exdate.setDate(exdate.getDate() + exdays);
	var c_value=escape(xmlHttp.responseText);// + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
	makeCookie('CURRENCY_COUNTRY',c_value,99);
	makeCookie('CC',c_value,99);		
	if(xmlHttp.responseText != 'US'){	
		document.location.href = '?cc=' + xmlHttp.responseText + '&curr=' + xmlHttp.responseText;
	}
}

