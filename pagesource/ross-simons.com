function linkInterrupt(e){
	if (!e){
	  e = window.event;
	}
	
	//IE9 & Other Browsers
	if (e.stopPropagation) {
	  e.stopPropagation();
	}
	//IE8 and Lower
	else {
	  e.cancelBubble = true;
	}
	thisLink = e.currentTarget;
	if (typeof thisLink === 'undefined'){
		thisLink = e.srcElement;	
	}
	if(thisLink == '[object HTMLPhraseElement]' || thisLink == '[object HTMLImageElement]'){
		thisLink = e.srcElement.parentElement;
	}
	thisLink = thisLink.toString();
	
	if(thisLink.indexOf('?') > 0){
		if(thisLink.indexOf('cc=') < 0){
			newURL = '&cc=' + countryCode + '&curr=' + currencyCountry;	
			fullURL = thisLink + newURL;
		} else {
			newURL = 'cc=' + countryCode + '&curr=' + currencyCountry;	
			baseURL = thisLink.split('?')[0];
			fullURL = baseURL + "?" + newURL;
		}
	} else {
		var fullURL = thisLink + '?cc=' + countryCode + '&curr=' + currencyCountry;	 
	}				
	e.returnValue = false;
	if (e.stopPropagation) {
		e.stopPropagation();
		e.preventDefault();
	}
	if(thisLink.substr(0,4) == 'http'){
		document.location.href = fullURL;
	}
	
}
