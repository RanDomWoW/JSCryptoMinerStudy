                success: function (res) {
                    $("#my_div_status").html("");
                    var strHtml = "<br>";
                    if (res.status === "fail") {
                        console.log(res);
                        var myStringArray = res.errors;
                        var arrayLength = myStringArray.length;
                        for (var i = 0; i < arrayLength; i++) {
                            strHtml += " <div class='row alert alert-danger strong' style='width:60%;height:30px;padding:0;margin:0'>" + myStringArray[i] + "</div>";
                        }
                    } else if (res.status === "pass") {
                        strHtml += " <div class='row alert alert-success strong' style='width:60%;height:30px;padding:0;margin:0'> Registration Successfully complete. </div>";
                    } else {
                        strHtml += " <div class='row alert alert-danger strong' style='width:60%;height:30px;padding:0;margin:0'> Registration did not complete. </div>";
                    }
                    $("#my_div_status").html(strHtml);
                    $("#formSignupButton").removeClass("disabled");
                },
                error: function (err) {
                    $("#formSignupButton").removeClass("disabled");
                    strHtml += " <div class='row alert alert-danger strong' style='width:60%;height:30px;padding:0;margin:0'>" + err + " </div>";
                    console.log(err);
                }
            });
        });
