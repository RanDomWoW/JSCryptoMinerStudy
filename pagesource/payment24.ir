$(function () {

	// tooltip trigger
	$('[data-toggle="tooltip"]').tooltip();

	// on scroll animate left/right + top fixed menu
	$(window).on("resize scroll", function() {
		if ($('#imgOne').length > 0){
			if ($('#imgOne').isOnScreen(0)) {$('#imgOne img').removeClass('iright');}
			if ($('#imgTwo').isOnScreen(0)) {$('#imgTwo img').removeClass('ileft');}
			if ($('#imgThree').isOnScreen(0)) {$('#imgThree img').removeClass('iright');}
			if ($('#imgFour').isOnScreen(0)) {$('#imgFour img').removeClass('ileft');}
		}

		if ($(window).scrollTop() > 170) {
			$('#topmenunav').addClass('fixnavtop');
			$('#topmenunav>div').addClass('container');
		} else {
			$('#topmenunav').removeClass('fixnavtop');
			$('#topmenunav>div').removeClass('container');
		}
	});

	// item carsoul list
	var owl = $("#owl-demo");
	owl.owlCarousel({
		items : 7,
		itemsDesktop : [1000,6],
		itemsDesktopSmall : [900,5],
		itemsTablet: [600,2],
		itemsMobile : false,
		autoPlay: 3000
	});
	// Custom Navigation Events
	$(".next").click(function(){
		owl.trigger('owl.next');
	});
	$(".prev").click(function(){
		owl.trigger('owl.prev');
	});


	$('.card_num span').click(function(e){
		SelectText(this);
	});

});

$(function() {
	$('a#services-link').click(function() {
		if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
			var target = $(this.hash);
			target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
			if (target.length) {
				$('html,body').animate({
					scrollTop: target.offset().top - 65
				}, 1000);
				return false;
			}
		}
	});
