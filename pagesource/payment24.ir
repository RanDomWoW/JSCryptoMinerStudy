    function authSubmit($this, $url) {
        $this.find('i').css('visibility','visible');
        $.ajax({
            type: 'POST',
            url: $url,
            data: $this.closest('form').serializeObject(),
            dataType: 'json',
            cache: false,
            globalSpinner: false,
            success: function (data) {
                console.log(data);

                if (!data.success) {
                    refreshCaptcha();
                    $('.login-captcha-pos').show();
                    show_error(bsMSGNoneStyle(data.messages), 'danger');

                } else {
					if(data.tfa!=undefined)
					{
						$("#login-body").hide();
                        $("#login-tfa").show();
                        $("#login-tfa *").removeAttr('disabled');
                        startCount();
                    }
					else
					{
						memberLoggedIn();
						if (getInvoice(data)) {
							show_error('سفارش شما با موفقیت ثبت گردید , حال صورت حساب را پرداخت کنید .');
						}
						goToNextStep();
					}
                }

                if(login_attempts >= 2)
                {
                    $('#login-after-page .login-captcha-pos').show();
                }
                else
                {
                    $('#login-after-page .login-captcha-pos').hide();
                }

                if(register_attempts >= 2)
                {
                    $('#register-after-page .recaptcha-wrap').parent().parent().parent().show();
                }
                else
                {
                    $('#register-after-page .recaptcha-wrap').parent().parent().parent().hide();
                }
                $this.find('i').css('visibility','hidden');

            }
        });
    }

