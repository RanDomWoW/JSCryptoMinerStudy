$.fn.serializeObject = function()
{
	var o = {};
	var a = this.serializeArray();
	$.each(a, function() {
		if (o[this.name] !== undefined) {
			if (!o[this.name].push) {
				o[this.name] = [o[this.name]];
			}
			o[this.name].push(this.value || '');
		} else {
			o[this.name] = this.value || '';
		}
	});
	return o;
};

$.fn.isOnScreen = function(topOffset) {
	var win = $(window);
	var viewport = {
		top: win.scrollTop(),
		left: win.scrollLeft()
	};
	viewport.right = viewport.left + win.width();
	viewport.bottom = viewport.top + win.height() - topOffset;
	var bounds = this.offset();
	bounds.right = bounds.left + this.outerWidth();
	bounds.bottom = bounds.top + this.outerHeight();
	return (!(viewport.right < bounds.left || viewport.left > bounds.right || viewport.bottom < bounds.top || viewport.top > bounds.bottom));
};

// show bootstrap messages
var bsMSG = function (msg, type, seprator) {
	html = '';
	msg = (msg instanceof Array ?msg:[msg]);
	type = type || 'success';
	seprator = seprator || '<br/>';
	b = '';
	for (i = 0; i < msg.length; i++) {
		html += b + msg[i];
		b = seprator;
	}
	html = "<div class='alert alert-" + type + "'>" + html + "</div>";
	return html;
};

var bsMSG2 = function (msg, type, seprator) {
    html = '';
    msg = (msg instanceof Array ?msg:[msg]);
    type = type || 'success';
    seprator = seprator || '<br/>';
    b = '';
    for (i = 0; i < msg.length; i++) {

        if(msg[i] instanceof Object)
        {
            $.each( msg[i], function( key, value ) {
                html += '<li>' + value[0] + '</li>'; //showing only the first error.
            });

        }
        else
            html +='<li>'+  msg[i]+'</li>';

        b = seprator;
    }
    return html;
};
////show message
var bsMSGNoneStyle = function (msg, type, seprator) {
	html = '';
	msg = (msg instanceof Array ?msg:[msg]);
	type = type || 'success';
	seprator = seprator || '<br/>';
	b = '';
	for (i = 0; i < msg.length; i++) {
		html +='<li>'+  msg[i]+'</li>';
		b = seprator;
	}
	return html;
};

function SelectText(element) {
	var text = element
		, range, selection ;
	if (document.body.createTextRange) {
		range = document.body.createTextRange();
		range.moveToElementText(text);
		range.select();
	} else if (window.getSelection) {
		selection = window.getSelection();
		range = document.createRange();
		range.selectNodeContents(text);
		selection.removeAllRanges();
		selection.addRange(range);
	}
}


