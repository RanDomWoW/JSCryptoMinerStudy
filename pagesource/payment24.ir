window.ci=0;
setInterval(function () {
	// تولید ایندکس برای کپچا ها
	$('.recaptcha-wrap').each(function(){
		attr=$(this).data('pindex');
		if (typeof attr == 'undefined' && $(this).is(':visible')) {
			$(this).data('pindex', window.ci++);
		}
	});
	// پیدا کردن بزرگترین ایندکس
	window.maxCaptchaIndex=0;
	$('.recaptcha-wrap').each(function(){
		if($(this).is(':visible') && $(this).data('pindex')>window.maxCaptchaIndex)
			window.maxCaptchaIndex= $(this).data('pindex');
	});

	$('.recaptcha-wrap').each(function(){

		if($(this).data('pindex')==window.maxCaptchaIndex){
			if($(this).find('.captcha-container').length==0) {
				$(this).append($('.captcha-container'));
				//refreshCaptcha();
			}
			return false;
		}
	});

}, 300);
