            (function($){

                $('[data-field="limit-decimal"]').each(function(){
                    var value=$(this).val();
                    var limitDecimal=(value).substr(0,10);
                     $(this).val(limitDecimal);
                });

                $(document).ready(function(){

                    $('#cssmenu li.active').addClass('open').children('ul').show();
                    $('#cssmenu li.has-sub>a').on('click', function(){
                        $(this).removeAttr('href');
                        var element = $(this).parent('li');
                        if (element.hasClass('open')) {
                            element.removeClass('open');
                            element.find('li').removeClass('open');
                            element.find('ul').slideUp(200);
                        }
                        else {
                            element.addClass('open');
                            element.children('ul').slideDown(200);
                            element.siblings('li').children('ul').slideUp(200);
                            element.siblings('li').removeClass('open');
                            element.siblings('li').find('li').removeClass('open');
                            element.siblings('li').find('ul').slideUp(200);
                        }
                    });

                });
            })(jQuery);


            $(window).load(function() {

                $('body img:not(".manually,.image-captcha")').prepend(function(index){
                    var newImg = new Image();
                    var imgSrc=$(this).attr('src');
                    newImg.src = imgSrc;
                    var curHeight = newImg.height;
                    var curWidth = newImg.width;
                    $(this).attr('height', curHeight);
                    $(this).attr('width', curWidth);
                    $(this).closest('.captcha').find('img').removeAttr('height');
                    $(this).closest('.captcha').find('img').removeAttr('width');
                });
            });

            $('.refresh').click(function(){
               refreshCaptcha();
            });


        //    $(".closePop").click(function(){
        //        $.cookie('hasAlreadySeenNewsLetter', '1',{ expires: 365 });
        //    });

            function refreshCaptcha(){
               $('.refresh').addClass('fa-spin fa-fw');
               $.ajax({
                   url: "/refereshcapcha",
                   type: 'get',
                   dataType: 'html',
                   success: function (json) {
                       $('.refresh').closest('.refereshrecapcha').find('.captcha').html(json);
                       $('.refresh').removeClass('fa-spin fa-fw');
                   },
                   error: function (data) {
                       alert('Try Again.');
                       $('.refresh').removeClass('fa-spin fa-fw');
                   }
               });
           }

            function show_error(error,type,timeOut)
            {
                timeOut=timeOut|3000;
                var typeName="success";
                if(type=='danger')
                     typeName="danger";

                if(Array.isArray(error))
                {
                    error=bsMSGNoneStyle(error);
                }

                $.notify({
                    // options
                    message: error
                },{
                    // settings
                    type:typeName,
                    z_index: 100000000000,
                    timer: timeOut,

                    template: '<div data-notify="container" style="padding: 10px 20px" class="col-xs-11 col-sm-3 alert alert-{0}" role="alert">' +
                    '<button type="button" aria-hidden="true" style="position: fixed;top:7px;" class="close" data-notify="dismiss">×</button>' +
                    '<span data-notify="icon"></span> ' +
                    '<span data-notify="title">{1}</span> ' +
                    '<span data-notify="message">{2}</span>' +
                    '<div class="progress" data-notify="progressbar">' +
                    '<div class="progress-bar progress-bar-{0}" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;"></div>' +
                    '</div>' +
                    '<a href="{3}" target="{4}" data-notify="url"></a>' +
                    '</div>'
                });
                $('.alert .close').removeAttr('style');
            }

            function submit_form($this,$url){
                $.ajax({
                    type: 'POST',
                    url: $url,
                    data: $($this).serializeObject(),
                    dataType: 'json',
                    cache: false,
                    globalSpinner:false,
                    success: function (response) {
                        // do something with ajax data
                        if (!response.success) {
                            show_error(bsMSGNoneStyle(response.msg),'danger');
                            refreshCaptcha();
                            $('.login-captcha-pos').show();

                        } else {
                            // redirect to dashboard
                            show_error(bsMSGNoneStyle(response.msg));
                            document.location.href = response.success_url;
                            refreshCaptcha();
                        }
                    }
                });
                return false;
            }




            $(function () {

                // ajax register form submission
                $("form.ajaxregisterform").submit(function (event) {
                    $this = $(this);
                    submit_form($this,'https://payment24.ir/auth/register');
                    return false;
                });

                // ajax login form submission
                $("form.ajaxloginform").submit(function (event) {
                    $this = $(this);
                    submit_form($this,'https://payment24.ir/auth/login');
                    return false;
                });

                $('[data-toggle="popover"]').popover();


            })
            </script>
