    function requestGateWay(gateway) {
        data = $('form#payoutOrderFrom').serializeObject();
        $.ajax({
            url: "https://payment24.ir/api/order/payout",
            dataType: 'json',
            type: 'POST',
            data: $.extend(data, {gateway: gateway, kind_pay: 'online'}),
            success: function (d) {
                if (d.success) {
                    show_error("در حال اتصال به درگاه", 'success', 10000);
                    var form = document.createElement("form");

                    form.setAttribute("method", "POST");
                    form.setAttribute("action", d.post_url);
                    form.setAttribute("target", "_self");

                    if (d.hasOwnProperty('post_data')) {
                        $.each(d.post_data, function (k, v) {
                            var hiddenField = document.createElement("input");
                            hiddenField.setAttribute("name", k);
                            hiddenField.setAttribute("value", v);
                            form.appendChild(hiddenField);
                        });
                    }

                    document.body.appendChild(form);
                    form.submit();
                } else {
                    show_error(bsMSGNoneStyle(d.messages), 'danger');
                }

                $('#payoutOrderFrom .pay_order').find('.spinner').remove();

            }
        });
    }

