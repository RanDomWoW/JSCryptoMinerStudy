<script>function roundTooman(price, by, up)
{
	up=up||true;
	by=by||1;
	if (up)
		price = Math.ceil(price / by);
	else
		price = Math.floor(price / by);
	return parseInt(price * by);
}


function calculatePrice(){
	$('.stotal span').html('0');
	service = $('select[name=service]').val();
	amount = parseFloat( $('input[name=sprice]').val().toString().replace(',',''));
	currency = $('input[name=scurrency]:checked').val();
	//disable other currencies for Webmoney
	if( service=='perfectMoney:charge' || service=='perfectMoney:buy' || service=='btce:charge') {
		$('input[value=GBP]').parent().fadeOut();
		$('input[value=EUR]').parent().fadeOut();
		$('input[value=USD]').trigger('click');
	}else if(service=='WebMoney:buy' || service=='WebMoney:charge'  || service=='Skrill:charge' || service=='Skrill:buy'){
		$('input[value=GBP]').parent().fadeOut();
		$('input[value=EUR]').parent().fadeOut();
		$('input[value=USD]').trigger('click');
	}else if(service=='BtcController:buy' || service=='BtcController:charge' ){
        $('input[value=GBP]').parent().fadeOut();
        $('input[value=EUR]').parent().fadeOut();
        $('input[value=USD]').parent().fadeOut();
        currency ='USD';

    }else{
		$('input[value=GBP]').parent().fadeIn();
		$('input[value=EUR]').parent().fadeIn();
	}

	if(!(amount>0))
		return;
	if(service.length==0)
		return;
	// Currency rates
	ref_rate = 4885;
	rates=[];
	rates['USD'] = 1;
	rates['EUR'] =1.11;
	rates['GBP'] = 1.287;

	if (service == 'PayPal:buy' || service == 'PayPal:charge') {
		rates['USD'] = 1.06;
		rates['EUR'] = 1.06 * 1.2163;
		rates['GBP'] = 1.06 * 1.414;
	}

	if (service == 'VisaMaster:buy' ) {
		rates['EUR'] = 1.2163;
		rates['GBP'] = 1.4140;
	}
    if (service == 'VisaMaster:buy' ) {
        rates['EUR'] = 1.2163;
        rates['GBP'] = 1.4140;
    }
	service_rates=[];
	service_rates['PayPal:buy']=1;
	service_rates['PayPal:charge']=1;
	service_rates['WebMoney:buy']=1.05;
	service_rates['WebMoney:charge']=1.05;
