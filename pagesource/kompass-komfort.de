			function checkBtnDisabled() {
				var i = 0;
				$('[rel=btn]').each(function () {
					i += $.trim($(this).val()) ? 1 : 0;
				});
				$('#btnSearch').prop('disabled', i == 0);

				if ($.trim(datepicker_elements.val()) == '' || $.trim(datepicker_elements.val()) == datepicker_elements.siblings('img.ui-datepicker-trigger').attr('alt')) {
					$('#btnSearch').prop('disabled', true);
				}

				$('#btnSearch').attr('disabled') ? $('#btnSearch').addClass('disabled') : $('#btnSearch').removeClass('disabled');

			}

			$('[rel=btn]').change(checkBtnDisabled);
			checkBtnDisabled();

		});

		function form_reset() {
			$form = $('#cat-form');

			$('input', $form).val('');
			$('textarea', $form).val('');
			$('.valid_error').text('');
		}

		$(document).ready(function () {

            $('.datepick').datepicker({
                yearRange: "-93:-0",
                showOn: "button",
                buttonImage: "public/kompass/img/calendar.gif",
                buttonImageOnly: true,
                buttonText: 'Выбрать дату...',
                changeMonth: true,
                changeYear: true,
                numberOfMonths: 2,
                //maxDate: '-30d',
                showButtonPanel: true
            });

            $('.datepick').on('click', function(){
                $(this).datepicker('show');
            });

			$(document).on('click', '.send-query', function () {

				$.post('http://kompass-komfort.de/fuhrpark', $("#cat-form").serialize(), function (data) {

					if (data == 1) {
						form_reset();
						$('#fancybox-transport-order .fancybox-close').hide().parent().parent().hide().siblings('#fancybox-overlay').hide();
						alert('Ваш запрос успешно отправлен!');
					}

					var obj = jQuery.parseJSON(data);

					var count = 0;
					var show_error_text = '';
					for (var key in obj) {
						if (obj[key] != '') {

							$('.' + key + '_error').html(obj[key]);
							$('.' + key + '_error').show();

						} else {

							$('.' + key + '_error').hide();
						}
					}

				});
				return false;

			});
		});
