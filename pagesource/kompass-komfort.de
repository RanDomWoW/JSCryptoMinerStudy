		<script type="text/javascript">
			var $bus = $('#bus');

			function rotate_bus() {
				$bus.rotate({animateTo: 0.7});
			}

			function rotate_bus_back() {
				$bus.rotate({animateTo: -0.7});
			}

			function rotate_bus_norm() {
				$bus.rotate({animateTo: 0});
			}

			function bus_up(){
				$bus.animate({
					marginTop: -3
				}, { duration: 250, queue: true});
			}

			function bus_down(){
				$bus.animate({
					marginTop: 0
				}, { duration: 250, queue: true});
			}




			function go_all() {
				rotate_bus_back();

				setTimeout(bus_up, 30);

				setTimeout(rotate_bus, 200);

				setTimeout(bus_down, 30);

				setTimeout(rotate_bus_norm, 200);

			}

			function bus_go() {
				setTimeout(go_all, 700);
				setTimeout(go_all, 450);
			}


				$('#header-image-slider').anythingSlider({
										mode: "horiz",
					onSlideBegin: function() {
						bus_go();
					},
										resizeContents: false,
					buildArrows: false,      // If true, builds the forwards and backwards buttons
					buildNavigation: false,      // If true, builds a list of anchor links to link to each panel
					buildStartStop: false,
					autoPlay: true,
					delay: 5000,
					animationTime: 2000,
					pauseOnHover: false,
					hashTags: false
				});

			</script>

