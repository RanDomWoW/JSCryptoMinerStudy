                if (action.indexOf(".") == -1) {
                    var queryIndex = action.indexOf("?");
                    if (queryIndex > -1) {
                        if (action.substring(queryIndex - 1, 1) != "/")
                            action = action.substring(0, queryIndex) + "/" + action.substring(queryIndex)
                    } else {
                        if (action.substr(-1) != "/") 
                            action = action + "/";
                    }
                    $(this).attr("action", action);
