    <script type="text/javascript">
    $(document).ready(function () {
        $('#quoteWidgetSubmit').click(function () {
            return WidgetProcessQuote('quoteWidgetForm');
        });
    });

    function WidgetProcessQuote(widgetFormId) {
        postalCode = $('#' + widgetFormId + ' .postalCode').val();
        businessLine = $('#' + widgetFormId + ' .businessLine').val();

        if (checkPostalCode(postalCode)) {
            $("#quoteError").css('display', 'none');
            //_gaq.push(['_trackEvent', 'Insurance', 'Get Insurance Button Click', 'Content', window.location.href])
            if (checkIfOntarioPostalCode(postalCode) && businessLine == 1) {
                $('#quoteWidgetForm').attr("action", "https://www.lowestrates.ca/landing/icandrive/");
            } else {
                $('#quoteWidgetForm').attr("action", "http://partner.insurancehotline.com/default.aspx");
            }

            document.forms['quoteWidgetForm'].submit();
            return false;
        } else {
            if ($('#quoteWidgetPostalCode').val() == '') {
                $('#quoteWidgetError').html("Please enter your postalcode.");
            } else {
                $('#quoteWidgetError').html("Invalid postal code. Please correct and try again.");
            }
            $("#quoteError").css('display', 'block');
            return false;
        }
        return false;
    }

