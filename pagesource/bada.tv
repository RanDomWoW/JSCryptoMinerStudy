	$('body').addClass("");	// white: #e5e9ec, //default: #4b4b4b

	$(document).ready(function () {
		// masonry setting -----------------------------------------------------------
		var $container = $('.king-part-q-list');
		$container.masonry({
			isAnimated: false,
			itemSelector : '.box',
			isFitWidth: true,
			columnWidth: 160,
			gutter: 10
		});
		$container.show();
		// -- masonry setting -----------------------------------------------------------
		
		//border top change due to notification status
		if( $('.notification').is(':visible') )	{
			$('#custom_menu_border').css('top','120px');
		} else {
			$('#custom_menu_border').css('top','70px');
		}
		
		//intial values
		var flagPrev = false;
		var flagNext = true;
		
		// page navigation show
		var startPage = parseInt($('#page').val());

		$('#nextPage').not('.unavailable').on('click',function() {
			changePage(startPage+1);
		});
		
		$('#previousPage').not('.unavailable').on('click', function() {
			changePage(startPage-1);
		});
		
		// 다음 버튼 숨기기
		// 안됨.. 왜죠? 
		if (startPage>=parseInt($('#totalPage').val())) {
			$('#nextPage').addClass('unavailable');
			$('#nextPage').off();
			flagNext = false;
			//$('#nextPage').hide();
		} else if ( flagNext == false ) {
			$('#nextPage').on('click',function() {
				changePage(startPage+1);
			});
			flagNext = true;
		} 

		// 이전 버튼 숨기기
		if (startPage<2) {
			$('#previousPage').addClass('unavailable');
			$('#previousPage').off();
			flagPrev = false;
			//$('#previousPage').hide();
		} else if ( flagPrev == false ) {
				$('.loadMorePrev').on('click', function() {
				changePage(startPage-1);
			});
			flagPrev = true;
		} 

		// 바로 점프
		var onLetterWidth = 8;
		var totalPage = parseInt($('#totalPage').val());
		$('.pageInputBox').keyup(function(e) {
			$(this).width(onLetterWidth * $(this).val().length);
			if(e.keyCode == 13){
				if (!$.isNumeric($(this).val())) {
					alert('숫자만 입력하실 수 있습니다.');
					$(this).focus();
					$(this).select();
					return;
				}
				// 1보다 작으면 1페이지로
				if (parseInt($(this).val())<1)	{
					$('.pageInputBox').val('1');
				}
				// totalPage보다 크면 totalPage로
				if (parseInt($(this).val())>totalPage)	{
					$('.pageInputBox').val(totalPage);
				}
				changePage($('.pageInputBox').val());
			}
		});
		$('.pageInputBox').width(onLetterWidth * $('.pageInputBox').val().length);

