		// Google Map location autocomplete
		function initializeLoc() {
			var input = $("#location_search")[0];
			var input_mobile = $("#mobile_location_search")[0];
			var autocomplete = new google.maps.places.Autocomplete(input);
			var autocomplete_mobile = new google.maps.places.Autocomplete(input_mobile);

			google.maps.event.addListener(autocomplete, 'place_changed', function() {
				var place = autocomplete.getPlace();
				setLocationInfos(place);
			});
			google.maps.event.addListener(autocomplete_mobile, 'place_changed', function() {
				var place = autocomplete.getPlace();
				setLocationInfos(place);
			});

			// 위치 정보 셋팅
			function setLocationInfos(place) {
				var latitude = place.geometry.location.lat();
				var longitude = place.geometry.location.lng();
				var country = findComponent(place, 'country');
				var state = findComponent(place, 'administrative_area_level_1');
				var city = findComponent(place, 'administrative_area_level_3') || findComponent(place, 'locality');
				$('#mv_latitude').val(latitude);
				$('#mv_longitude').val(longitude);
				$('#mv_country').val(country);
				$('#mv_state').val(state);
				$('#mv_city').val(city);
			}

			function findComponent(place, type) {
				var components = place.address_components;
				for (i=0; i<components.length; i++) {
					var ithcomptypes = components[i].types;
					for (j=0; j<ithcomptypes.length; j++) {
						var typeString = ithcomptypes[j];
						if (typeString.localeCompare(type) == 0) {
							return components[i].long_name;
						}
					}
				}
			}

			google.maps.event.addDomListener(input, 'keydown', function(e) {
				if (e.keyCode == 13) {
					e.preventDefault();
				}
			});

			google.maps.event.addDomListener(input_mobile, 'keydown', function(e) {
				if (e.keyCode == 13) {
					e.preventDefault();
				}
			});
		}

		function loadScript() {
			var script = document.createElement('script');
			script.type = 'text/javascript';
			script.src = 'https://maps.googleapis.com/maps/api/js?key=AIzaSyAnV2TKji-DKnLVmtPH7IAV76P7d_ifhzQ&v=3.exp&language=en&region=us&libraries=places&callback=initializeLoc';
			document.body.appendChild(script);
		}

