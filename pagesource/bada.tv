		// url 얻기
		function getUrl() {
			// mypage 여부
			var s = $("#s").val();
			if (s!='') s+=' ';
			s+=$(".form_search #s").val();

			//var url = "/";			// POST방식
			var url = "/?";		// 쿠키방식

			var param = $("#page").val();
			param = param + "_" + $("#mv_cate0").val();
			param = param + "_" + $("#distance").val();
			param = param + "_" + $("#priceTo").val();
			param = param + "_" + $("#orderBy").val();
			param = param + "_" + $("#subscription").val();
			param = param + "_" + $("#bookmark").val();
			param = param + "_" + $("#messageFlag").val();
			param = param + "_" + $("#mypageFlag").val();

			if (s!='') {
				param = param + "_s=" + s;
				param = param.replace(/ /g, '+');
