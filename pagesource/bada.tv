			var thumbnailRootPath = "/files/thumbnail";	// 썸네일 base Path

			$(".context_inputbox.form-control").on("keyup", function(event) {
				if ( event.keyCode === $.ui.keyCode.TAB && $( this ).autocomplete( "instance" ).menu.active ) {
					event.preventDefault();
				}
			}).autocomplete({
				delay: 100,
				minLength: 1,
				source: function( request, resp ) {
					// 맨 뒤 단어만 검색
					var newtags = '';
          /*
           var tags = request.term;
           var tagsArray = tags.split(" ");
           for (var i = 0; i < tagsArray.length; i++) {
           var trimmedTag = tagsArray[i].trim();
           if (trimmedTag.length > 0 && trimmedTag.charAt(0) != ' ') {
           newtags = trimmedTag;
           }
           }
           */
					// 태그만 자르고 검색
					var tags = request.term;
					var tagsArray = tags.split(" ");
					for (var i = 0; i < tagsArray.length; i++) {
						var trimmedTag = tagsArray[i].trim();
						if (trimmedTag.length > 0 && trimmedTag.charAt(0) != '#') {
							if (newtags!='') newtags = newtags + ' ';
							newtags = newtags + trimmedTag;
						}
					}
					var availableTags = [];
					$.ajax({
						type: "POST",
						data: {
							csrf_token_bada_2015:"",
							tagKeyword: newtags
						},
						url: "/bada/ajaxIF/getTags/",
						success: function (response) {
							availableTags = [];
							json = $.parseJSON(response);
							var titleCount = 0;
							var tagCount = 0;
							$.each(json, function(index, value) {
								var mv_no = value.mv_no;
								var tag = value.TAG;
								if (tag.length>35) tag = tag.substring(0, 30) + '..';		// 25자 이상 자르기
								var regexp = new RegExp(newtags, "gi");
								tag = tag.replace(regexp, '<span class="ui-autocomplete-underline">$&</span>' );					// 검색어와 동일하면 underline...
								if (value.TAG.indexOf('#')<0) {
									var html = '<a href="/?m=' + mv_no + '" style="text-align:left; padding: 0 40px;"><span class="ui-autocomplete-a">' + tag + '</span>';	// href 태그 달기
									var mv_thumb = getThumbnailFileName(thumbnailRootPath, value.mv_thumb);							// 썸네일 풀 path 가져오기
									html+='<img class="ui-autocomplete_thumbnail" src="' + mv_thumb + '">';							// 표시될 html에 썸네일 추가
									html+='</a>';
									if (titleCount==0) {
										var searchDiv = "<div id='tagSearch'>Search</div>";
										availableTags.push(searchDiv);
									}
									availableTags.push(html);
									//console.error(html);
									titleCount++;																																	// 끝
								} else {
									if (tagCount==0) {
										var searchDiv = "<div id='tagSearch'>Tag Search</div>";
										availableTags.push(searchDiv);
									}
									tagCount++;
									availableTags.push(tag);		// 태그면 그대로 출력
								}
							});
							//resp( $.ui.autocomplete.filter(availableTags, newtags));
							resp( availableTags);
							tagFlag = false;
						}
					});
				},
				focus: function( event, ui ) {
					return false;
				},
				select: function(event, ui) {
					var value = ui.item.value;
					value = value.replace(/(<([^>]+)>)/gi, "");
					if (value.indexOf('#')<0) return false;
					// 찾은 아이템 리스트 중에 선택 시 -> 태그검색..................................
					var terms = [];
					var originalValue = this.value;
					var str = '';
					for (var i=0; i<originalValue.length; i++) {
						var chr = originalValue[i];
						if (chr==',') {
							terms.push(str.trim());
							str = '';
						} else {
							str += chr;
						}
					}
					terms.push(value);

					var originalSearchText = $("#s").val();
					if (originalSearchText!='') originalSearchText+=' ';
					this.value = originalSearchText + terms;
					submitForm(false);
					return false;
				}
			}).focus(function(event, ui) {
				event.preventDefault();
				return false;
			});

			$.extend( $.ui.autocomplete.prototype, {
				_renderItem: function( ul, item ) {
					var term = newtags;
					var b = "";
					if (item.label.indexOf('#')>=0) {
						b="font-weight:bold; height: 40px; line-height: 40px; text-align:left; padding: 0 40px;";
						tagFlag = true;
					}
					if (item.label.indexOf('tagSearch')>=0)
					{
						return $("<span></span>").data( "item.autocomplete", item )
							.append( item.label )
							.appendTo(ul);
					} else {
						return $( "<li style='"+b+"'></li>" )
							.data( "item.autocomplete", item )
							.append( item.label )
							.appendTo(ul);
					}
				}
			});
