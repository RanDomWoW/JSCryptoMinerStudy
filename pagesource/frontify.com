<style>
    /* Needs to be inline due to internal id urls (e.g. url(#drops-gradient-blue-ltr)) */
    
    /* Variant 1 - Orange Front / Blue Back */
    .m-drops--variant-1 .m-drops__drop--second {
        opacity: 0.9;
    }

    .m-drops--variant-1 .m-drops__drop--first svg {
        fill: url(#drops-gradient-blue-ltr);
    }

    .m-drops--variant-1 .m-drops__drop--second svg {
        fill: url(#drops-gradient-orange-rtl);
    }

    /* Variant 2 - Variant 1 mirrored */
    .m-drops--variant-2 .m-drops__drop--first {
        opacity: 0.9;
        z-index: 1;
    }
    
    .m-drops--variant-2 .m-drops__drop--first svg {
        fill: url(#drops-gradient-blue-rtl);
    }

    .m-drops--variant-2 .m-drops__drop--second svg {
        fill: url(#drops-gradient-orange-ltr);
    }

    /* Variant 3 - Orange Front / Orange Back */
    .m-drops--variant-3 .m-drops__drop--second {
        opacity: 0.9;
    }

    .m-drops--variant-3 .m-drops__drop--first svg {
        fill: url(#drops-gradient-orange-rtl);
    }

    .m-drops--variant-3 .m-drops__drop--second svg {
        fill: url(#drops-gradient-orange-ltr);
    }

    /* Variant 4 - Blue Front / Blue Back */
    .m-drops--variant-4 .m-drops__drop--second {
        opacity: 0.9;
    }

    .m-drops--variant-4 .m-drops__drop--first svg {
        fill: url(#drops-gradient-blue-rtl);
    }

    .m-drops--variant-4 .m-drops__drop--second svg {
        fill: url(#drops-gradient-blue-ltr);
    }

    /* Variant 5 - Variant 3 mirrored */
    .m-drops--variant-5 .m-drops__drop--first {
        opacity: 0.9;
        z-index: 1;
    }

    .m-drops--variant-5 .m-drops__drop--first svg {
        fill: url(#drops-gradient-orange-rtl);
    }

    .m-drops--variant-5 .m-drops__drop--second svg {
        fill: url(#drops-gradient-orange-ltr);
    }
    
    /* Variant header  */
    .m-drops--variant-header {
        z-index: 0;
        padding-bottom: 65%;
        bottom: -4.9305555555%;
    }
    
    .m-drops--variant-header .m-drops__drop--first {
        transform: scale(2.5);
        -moz-transform: scale(2.5);
        padding-bottom: 22.9305555555%;
        bottom: 4.9305555555%;
        z-index: 1;
        transform-origin: center bottom;
       
    }
    
    .m-drops--variant-header .m-drops__drop--second {
        transform: scale(2.5);
        -moz-transform: scale(2.5);
        padding-bottom: 38%;
        bottom: 4.9305555555%;
        transform-origin: 63% center;
    }
    
    .m-drops--variant-header .m-drops__drop--first svg {
        fill: #f8f8f8;
        transform: rotate(240deg);
    }

    .m-drops--variant-header .m-drops__drop--second svg {
        fill: #fff;
        transform: rotate(-220deg);
    }

    .m-drops--variant-header .m-drops__drop-cover {
        padding-bottom: 4.9305555555%;
        z-index: 1;
    }

    .m-drops--variant-header-condensed .m-drops__drop--first {
        padding-bottom: 20.5%;
    }

    .m-drops--variant-header-condensed .m-drops__drop--second {
        padding-bottom: 33%;
    }

    .m-drops--variant-header-condensed .m-drops__drop--second svg {
        transform: rotate(-225deg);
    }
</style>
