                    var updatesHtml = '';
                    $.each(updates.payload, function (idx, item) {
                        updatesHtml += '<article class="site-update-item">' +
                            '<span class="pull-right site-update-date">' + moment(item.published_at).calendar() + '</span>' +
                            '<h4>' + item.title + '</h4>' +
                            '<div class="site-update-content">' + item.contents + '</div>' +
                            '</article>';
					})
                    $('.site-updates-content').html(updatesHtml);
				}
			});
		});

