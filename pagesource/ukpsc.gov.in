<script type="text/javascript">
	$(document).ready(function (){
		$("#marquee1").marquee();
	});
	
	function pause()
	{
		$('#marquee1').marquee('pause');
		$('#marquee_stop').hide();
		$('#marquee_resume').show();
		$('#marquee1 li').addClass('stoped');
		document.getElementById("marquee1").style.overflow="scroll";
		document.getElementById("marquee1").style.padding='0 0 12px 0';
	}
	function play()
	{
		$('#marquee1').marquee('resume');
		$('#marquee_resume').hide();
		$('#marquee_stop').show();
		$('#marquee1 li').removeClass('stoped');
		document.getElementById("marquee1").style.overflow="hidden";
		document.getElementById("marquee1").style.padding='0 0';
	}
</script>
