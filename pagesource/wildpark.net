        function imgPopup(el){
            console.log($('html').scrollTop());
            $('body').append(
                '<div id="img-popup" style="cursor: pointer; display:flex; align-items:center; justify-content:center; position: absolute; top: '+($('html').scrollTop())+
                'px; left:0; width: 100%; height:100%; line-height:100%; background: rgba(0,0,0,0.8); z-index: 999;" onclick="$(this).remove()">'+
                '<img style="max-width: 95%; max-height: 95%;" src="'+($(el).attr('src'))+'">'+
                '</div>'
            );
            $('#img-popup').on('scroll touchmove mousewheel', function(e){
                e.preventDefault();
                e.stopPropagation();
                return false;
            });
        }

        $('.img-thumbnail').on('click', function(e){
            console.log(e);
            imgPopup(e.currentTarget);
        });
        
    };
</script>
