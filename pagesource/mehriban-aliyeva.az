<script type="text/javascript">
    var silderNum = 0;
    var silderLeftNum = 0;
    var silderRightNum = 2;
    var sliderImgCount = 0;
    
    var leftRight = 0;
    var isClick = false;
    var animationEnd = true;
    function mousedownPreview(){
        //leftRight = _leftRight;
        isClick = true;
        silderNum = leftRight;
        if(silderNum == sliderImgCount*1){
            silderLeftNum = sliderImgCount*1-1;
            silderRightNum = 1;
        }else if(silderNum == 1){
            silderLeftNum = sliderImgCount*1;
            silderRightNum = silderNum+1;
        }else{
            silderLeftNum = silderNum-1;
            silderRightNum = silderNum+1;
        }
    }
	jQuery(document).ready(function () {
	  
      sliderImgCount = $(".carousel-feature[attrData='cf-1']").attr('attrCount');
      //window.setTimeout('window.location.href="'+language+'/site/front"', sliderImgCount*1*5*2*1000);

      $(".carousel-feature").on('mousedown', function(e){
          if(e.buttons == 1){
            leftRight = $(this).attr('attrNum')*1;
            isClick = true;
            silderNum = leftRight;
            if(silderNum == sliderImgCount*1){
                silderLeftNum = sliderImgCount*1-1;
                silderRightNum = 1;
            }else if(silderNum == 1){
                silderLeftNum = sliderImgCount*1;
                silderRightNum = silderNum+1;
            }else{
                silderLeftNum = silderNum-1;
                silderRightNum = silderNum+1;
            }
          }
      });
      $("#left-image").on('mousedown', function(e){
          if(e.buttons == 1){
            if(animationEnd){
                animationEnd = false;
                leftRight = silderLeftNum;
                mousedownPreview();
            
            //console.log(['click', silderLeftNum, silderNum, silderRightNum]);
            
            }
          }
      });
      $("#right-image").on('mousedown', function(e){
          if(e.buttons == 1){
            if(animationEnd){
                animationEnd = false;
                leftRight = silderRightNum;
                mousedownPreview();
            
            //console.log(['click', silderLeftNum, silderNum, silderRightNum]);
            
            }
          }
      });
      //silderLeftNum = sliderImgCount*1;
	  jQuery("#carousel").featureCarousel({
		smallFeatureWidth:450,
		smallFeatureHeight:620,
		topPadding:0,
		sidePadding:0,
		smallFeatureOffset:0,
		pauseOnHover:true,
		trackerIndividual:false,
		trackerSummation:false,
		pauseOnHover:true,
        leftButtonTag: '#left-image',
        rightButtonTag: '#right-image',
		movedToCenter:function($feature){
		    $feature.addClass( " active-image" );
            animationEnd = true;
		},
		leavingCenter:function($feature){
		    $feature.removeClass().addClass( "carousel-feature" );
		    
		    if(!isClick){
			silderNum++;
			if(silderNum > sliderImgCount*1){
			    silderNum = 1;
			}
			if(silderNum == sliderImgCount*1){
			    silderLeftNum = sliderImgCount*1-1;
			    silderRightNum = 1;
			}else if(silderNum == 1){
			    silderLeftNum = sliderImgCount*1;
			    silderRightNum = silderNum+1;
			}else{
			    silderLeftNum = silderNum-1;
			    silderRightNum = silderNum+1;
			}
		    }
		    
		    isClick = false;
            animationEnd = false;
		    //console.log(['leavingCenter', silderLeftNum, silderNum, silderRightNum]);
		    
		    $(".carousel-feature[attrData='cf-"+silderNum+"'] img").animate({'margin-left':'0'}, 1000);
		    
		    $(".carousel-feature[attrData='cf-"+silderLeftNum+"'] img").animate({'margin-left':$(".carousel-feature[attrData='cf-"+silderLeftNum+"']").attr('attrDrag')+'px'}, 1000);
		    
		    $(".carousel-feature[attrData='cf-"+silderRightNum+"'] img").animate({'margin-left':$(".carousel-feature[attrData='cf-"+silderRightNum+"']").attr('attrDrag')+'px'}, 1000);
		    
		    
		},
	  });
	  //$(".carousel-feature[attrData='cf-2'] img").css({'margin-left':'-400px'});
	});
</script>
