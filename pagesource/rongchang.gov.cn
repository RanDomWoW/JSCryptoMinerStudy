    function fn_createGWYWJDiv(messageCount, wordCount, callback) {
        $.ajax({
            url: 'http://www.cq.gov.cn/gwywj/pushinfo.js',
            dataType: "jsonp",
            jsonp: "pushInfoJsonpCallBack",
            jsonpCallback: "pushInfoJsonpCallBack",
            success: function (data) {
                if (typeof(callback) == 'function') {
                    callback();
                }
                var count = 0;
                $.each(data, function (i, json) {
                    var allName = json.title;		//全称
                    var shortName = "";				//简称
                    if (allName.length > wordCount) {
                        shortName = allName.substr(0, wordCount) + "...";
                    } else {
                        shortName = allName;
                    }
                    var dataa = json.pubDate.slice(5, 10);
                    var temp = "<li title='" + allName + "'><a target='_blank' href='" + json.link + "'>" + shortName + "<span class='publish-time'>" + dataa + "</span></a></li>";
                    $("#tablist_gwywj2").append(temp);
                    count++;
                    if (count == messageCount) {
                        return false;
                    }
                });
            },
            error: function () {
                $("#tablist_gwywj2").append('加载中...');
            }
        });
    }

    function fn_createGWYWJDiv1(messageCount, wordCount) {
        $.ajax({
            url: 'http://www.gov.cn/pushinfo/v150203/pushinfo.jsonp',
            dataType: "jsonp",
            jsonp: "pushInfoJsonpCallBack",
            jsonpCallback: "pushInfoJsonpCallBack",
            success: function (data) {
                var count = 0;
                $.each(data, function (i, json) {
                    var allName = json.title;		//全称
                    var shortName = "";				//简称
                    if (allName.length > wordCount) {
                        shortName = allName.substr(0, wordCount) + "...";
                    } else {
                        shortName = allName;
                    }
                    var dataa = json.pubDate.slice(5, 10);
                    var temp = "<li title='" + allName + "'><a target='_blank'  href='" + json.link + "'>" + shortName + "<span class='publish-time'>" + dataa + "</span></a></li>";
                    $("#tablist_gwywj").append(temp);
                    count++;
                    if (count == messageCount) {
                        return false;
                    }
                });
            },
            error: function () {
                $("#tablist_gwywj").append('加载中...');
            }

        });
    }

    fn_createGWYWJDiv(8, 14, function () {
        fn_createGWYWJDiv1(8, 14)
