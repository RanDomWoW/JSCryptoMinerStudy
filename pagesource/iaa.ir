function GetDestinationAirports2(sourceCombo, destinationCombo) {
	var destinationChosen = $("#" + $(destinationCombo).prop("id") + "_chosen").find('span');
	$(destinationChosen).css("background-image", "url(/Content/Images/busy-indicator.gif)")
	$(destinationChosen).css("background-size", "26px 26px");
	$(destinationChosen).css("background-repeat", "no-repeat");
	$(destinationChosen).css("background-position", "0 0");
    $(destinationCombo).html("");

    $.post("/Flight/GetDestinationAirports", { id: $(sourceCombo).val() }, function (result) {
        var html = "";
        var lastGroup = null;
        $.each(result, function () {
            if (this.Group.Name != lastGroup)
            {
                if (lastGroup != null)
                    html += "</optgroup>";
                lastGroup = this.Group.Name;
                html += "<optgroup label='" + this.Group.Name + "'>";
            }

            html += "<option value='" + this.Value + "'>" + this.Text + "</option>";
        });
        if (html.length > 0)
            html += "</optgroup>";

        $(destinationCombo).html(html);
        $(destinationCombo).trigger("chosen:updated");
		$(destinationChosen).css("background-image", "");
    });
}

        function validateFlightForm(frm) {
            var objFrom = document.getElementsByName('leg1');
            var objTo = document.getElementsByName('leg2');
            var Sdepart = 0;
            var Sreturn = 0;
            for (var i = 0; i < objFrom.length; i++) {
                if (objFrom[i].checked == true) {
                    Sdepart = 1;
                    break;
                }
            }

            for (var j = 0; j < objTo.length; j++) {
                if (objTo[j].checked == true) {
                    Sreturn = 1;
                    break;
                }
            }

            if (Sdepart == 0) {
                alert('لطفا پرواز رفت خود را انتخاب نماييد');
                return false;
            }


            var SSreurn = '';
            for (var i = 0; i < objFrom.length; i++) {
                if (objFrom[i].checked == true)
                    break;
            }
            for (var j = 0; j < objTo.length; j++) {
                if (objTo[j].checked == true)
                    SSreurn = objTo[j].value;
            }
            var url = "http://91.212.16.51/DetailD.aspx?";
            url += "DEPART=" + objFrom[i].value;
            url += "&RETURN=" + SSreurn;
            url += "&Pax=" + document.getElementById('ddlAdt').value + document.getElementById('ddlChd').value + document.getElementById('ddlInf').value;
            //url+= "&CHD="+session("CHD");
            //url+= "&INF="+session("INF");
            location.href = url;

            return true;
        };



