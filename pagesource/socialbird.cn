            is_submit = true;
            $.ajax({
                type: "POST",
                dataType: "json",
                url: "/portal/form/add_msg.html",
                method: 'post',
                data: {
                    username: uname, phone: uphone, email: uemail, company: ucompany,
                    companytype: utype, prod1: uprod1, prod2: uprod2, prod3: uprod3, prod4: uprod4,
                    memo: umemo, source_page: "Home"
                },
                success: function (data) {
                    if(data.status == 1){
                        // showMessage("您的信息我们已收到，我们会尽快安排人员与您取得联系"); 修改by dengzhw 20170523
//                        $('#contactForm input').val('');
//                        $('textarea').val('');
//                        $('#prod1').prop('checked', false);
//                        $('#prod2').prop('checked', false);
