                        var message = '';
                        message += '产业类别：' + utype + '; 咨询产品：';
                        message += uprod1 + ' ' + uprod2 + ' ' + uprod3 + ' ' + uprod4 + '; 咨询内容：' + umemo;
                        send_contact_form(uname, uphone, uemail, ucompany, message);
                    }else{
                        showMessage("您的信息未填写正确，请再次确认");
                    }
                },
                error: function(){
                    is_submit = false;
                }
            })
        }
        return false;
    });

    function showMessage(msg) {
        $('.message p').text(msg);
        $('.message').addClass('pre-show');
        setTimeout(function () {
            $('.message').addClass('animated bounceIn');
        }, 1);
        setTimeout(function () {
            $('.message').addClass('fadeOutDown');
        }, 2000);
        setTimeout(function () {
            $('.message').hide();
        }, 3000);
        $('.message').one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function () {
            if ($(this).hasClass('bounceIn')) {
                $(this).removeClass('bounceIn');
            } else if ($(this).hasClass('fadeOutDown')) {
                $(this).removeClass('pre-show animated fadeOutDown');
            }
        });
    }
    $('#uname').on('keyup',function(){
        $('#uname-error').hide();
    })
    function send_contact_form(name,phone,email,company_name,message){
        $('#contact_form_id').val(5021);
        $('#contact_name').val(name);
        $('#contact_name').val(name);
        $('#contact_phone').val(phone);
        $('#contact_email').val(email);
        $('#contact_company_name').val(company_name);
        $('#contact_message').val(message);

        $('#remote_form').attr('target', 'submit_form');
        var iframe = document.createElement('iframe');
        iframe.id = 'submit_form';
        iframe.name = 'submit_form';
        iframe.src = 'about:blank';
        iframe.style.display = 'none';
        $('#submit_form').remove();
        $(document.body).append(iframe);
        $(iframe).load(function(){
            window.location.href="/page/index/thankyou"; //完成表单后跳转到thankyou 页面
        });
        $('#remote_form').submit();
    }
