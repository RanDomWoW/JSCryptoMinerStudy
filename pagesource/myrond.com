<script language="javascript">
    jQuery(document).ready(function () {
            jQuery("#cmbdwn").change(function() {
                jQuery("#Number1").focus();
            });

            jQuery(".number").keydown(function (e) {
                var keyCode = e.keyCode || e.which;

                if (e.keyCode == 13) {
                    $("#SearchForm").submit();
                }

                if ((keyCode<47 || keyCode>58) && (keyCode<96 || keyCode>105) && keyCode!=8 && keyCode!=32 && keyCode !=39 && keyCode!= 37 && keyCode != 9 && keyCode!=46)
                    return false;
                if (keyCode==46) {
                    jQuery(this).val("");
                }

                if (keyCode==37) { // left arrow key moves backward to previous digit
                    jQuery(this).prev(".number").select().focus();
                    return false;
                }
                if (keyCode==32  || keyCode==39 ) { // space or right arrow key moves forward to next digit
                    jQuery(this).next(".number").select().focus();
                    return false;
                }
                return true;
            });

            jQuery(".number").keyup(function(e) {
                var keyCode = e.keyCode || e.which;

                if ((keyCode>=47 && e.keyCode<=58) || (keyCode>=96 && keyCode<=105) )
                    jQuery(this).next(".number").focus();
                if (keyCode==8)
                    jQuery(this).prev(".number").focus();
            });
            //jQuery(".number").first().focus();
        });


</script>
