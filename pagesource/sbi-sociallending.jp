			idName = anchor[i].id.split("_");
			if(idName[0] === "investmentApp"){
				document.getElementById("investmentApp").style.background = "url("+baseUrl+"assets/images/bg_menumain.png) 0 -46px";
			}
			else if(idName[0] === "investmentInfo"){
				document.getElementById("investmentInfo").style.background = "url("+baseUrl+"assets/images/bg_menumain.png) 0 -46px";
			}
			else if(idName[0] === "registrationInfo"){
				document.getElementById("registrationInfo").style.background = "url("+baseUrl+"assets/images/bg_menumain.png) 0 -46px";
			}
			anchor[i].style.background = "url("+baseUrl+"assets/images/bg_menumain.png) 0 -46px";
		}
	}
	// <END> 20160420 RM:

}
