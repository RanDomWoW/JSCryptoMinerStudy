    </div>
    <script type="text/javascript">
       $(document).ready(function(){
           $('#meiChatL').click(function(){
               _MEIQIA('showPanel');
           })
       })
        $('#home-callback-close').click(function(){
            $('#home-callback-dialog').fadeOut();
            $('body').removeClass('active-hide');
        })
        $('#home-button-close').click(function(){
            $('#home-callback-dialog').fadeOut();
            $('body').removeClass('active-hide');
        })
        $('.fixed-right-top').click(function() {
            $('body, html').animate({scrollTop: 0}, 400);
        })
    </script>
</div>


