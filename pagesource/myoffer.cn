<div class="my-shade" id="tips">
    <div class="my-tips">
        <img id="tips-true" src="https://img.myoffer.cn/images/i-tips-true.png">
        <img id="tips-false" src="https://img.myoffer.cn/images/i-tips-false.png">
        <div class="body" id="tips-body">这是提示</div>
    </div>
</div>
<script>
    function loginKey(){
        $(document).keydown(function (event) {

            var now=$("#login-form-enroll").css("display")
            if (event.keyCode != 13) {
                return
            }
            if (now=="none") {
                $("#login-form-login #login-login").click();
                return
            }
            $("#login-form-enroll #login-submit").click()
        });
    }
    function myTips(text, state) {
        if(state) {
            $('#tips-true').show();
            $('#tips-false').hide();

        }
        else {
            $('#tips-true').hide();
            $('#tips-false').show();
        }

        $('#tips-body').text(text);
        setTimeout(function(){
            $('#tips').fadeIn();
        },200);

        setTimeout(function(){
            $('#tips').fadeOut();
        },1200)
    }
</script>

<!-- fixed-right -->
