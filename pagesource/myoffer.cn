<script>
(function() {
	var searchType = document.querySelector('.js-nav-search-type');
	var searchList = document.querySelector('.js-nav-search-list');
	var searchContent = document.querySelector('.js-nav-search-content');
	var searchIcon = document.querySelector('.js-nav-search-icon');
	var type = 0;

	searchType.addEventListener('click', function(e) {
		e.stopPropagation();
		var display = searchList.style.display;
		if (display === 'block') {
			searchList.style.display = 'none';
		} else {
			searchList.style.display = 'block';
		}
	}, false);

	searchList.addEventListener('click', function(e) {
		var target = e.target;
		var data = target.dataset.val;
		if (data === undefined) return;
		type = data;
		searchType.value = target.innerText;
	}, false);

	searchContent.addEventListener('keydown', function(e) {
		var value = e.target.value;
		if (e.keyCode === 13) {
			_search(value);
		}
	}, false);

	searchIcon.addEventListener('click', function() {
		var value = searchContent.value;
		_search(value);
	}, false);

	document.addEventListener('click', function(e) {
		searchList.style.display = 'none';
	}, false);

	function _search(content) {
		if (type === '1') {
			window.open('/article/search.html?text=' + window.encodeURIComponent(content));
		} else {
			window.location.href = '/search/' + window.encodeURIComponent(content);
		}
	}
}())
</script>

