            var slideContorlWidth = $(".slide_control").width();
            $(".slide_control").css({
                marginLeft: -slideContorlWidth / 2
            })
        };
        this.slide = function() {
            $("#mtsBanner .slide_control a").lazyhover(function() {
                _this.j = $(this).text() - 1;
                _this.n = _this.j;
                if (_this.j >= _this.count) {
                    return
                }
                moveFun(_this.first_show, _this.j)
            },
