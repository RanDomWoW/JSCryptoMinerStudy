    var conTop = $(".floorList").offset().top - 100;
    var isGuestLike = true;
    $(window).scroll(function() {
        var scrt = $(window).scrollTop();
        if (scrt > conTop) {
            $(".fsFixedTopContent").show("fast",
            function() {
                $(".fsFixedTop").css({
                    "-webkit-transform": "scale(1)",
                    "-moz-transform": "scale(1)",
                    "transform": "scale(1)",
                    "opacity": "1"
                })
            }).css({
                "visibility": "visible"
            })
        } else {
            $(".fsFixedTop").css({
                "-webkit-transform": "scale(1.2)",
                "-moz-transform": "scale(1.2)",
                "transform": "scale(1.2)",
                "opacity": "0"
            });
            $(".fsFixedTopContent").css({
                "visibility": "hidden"
            })
        }
        setTab();
        if (scrt >= $(".floor:last").offset().top + 100) {
            $('.fsFixedTopContent').css('background', 'none');
            $('.fsFixedTopContent .togo').height(0)
        } else {
            $('.fsFixedTopContent').css('background', '');
            $('.fsFixedTopContent .togo').attr('style', '')
        }
        if (scrt >= $(".floor:last").offset().top - 300 && isGuestLike) {
            fnGuestLike()
        }
    });
    var arr2 = [],
    arr1 = [],
    fsOffset = 0;
    for (var i = 0; i < $(".floor").length; i++) {
        arr2.push(parseInt($(".floor").eq(i).offset().top) - 15);
        if (i < 4) {
            arr1.push(parseInt($(".floor").eq(i).offset().top) - 15)
        } else {
            arr1.push(parseInt($(".floor").eq(i).offset().top) - 120)
        }
    }
    $(".fsFixedTop a.smooth").on("click",
    function() {
        var _th = $(this);
        _th.blur();
        var index = $(".fsFixedTop a.smooth").index(this);
        if (index > 0) {
            fsOffset = 50
        }
        var hh = arr2[index];
        $("html,body").stop().animate({
            scrollTop: hh - fsOffset + "px"
        },
        400)
    });
    $(".fsFixedTop a.fsbacktotop").click(function() {
        $("html,body").stop().animate({
            scrollTop: 0
        },
        400)
    }) 
	function setTab() {
        var Objs = $(".floor");
        var textSt = $(window).scrollTop();
        $(".fsFixedTop a").removeClass("active");
        for (var i = Objs.length - 1; i >= 0; i--) {
            if (textSt >= arr1[i] - 200) {
                $(".fsFixedTop a").eq(i).addClass("active");
                return
            }
        }
    }
