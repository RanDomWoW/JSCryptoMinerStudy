            },
            200);
            oEm.stop().animate({
                'marginLeft': '0px'
            },
            250)
        })
    }
    $('.Jtime').each(function(index, element) {
        var _th = $(this),
        t = _th.attr('data-time').split(' '),
        d = t[0].split('-')[0] + '/' + t[0].split('-')[1] + '/' + t[0].split('-')[2] + ' ' + t[1];
        _th.dataUI({
            oDateEnd: d
        })
    });
    $(".floor01,.floor,#slideM1").on("mouseenter", "img",
    function() {
        $(this).stop().fadeTo("", 0.8,
        function() {
            $(this).stop().fadeTo("", 1)
        })
    });
    $(".floor01,.floor,#slideM1").on("mouseout", "img",
    function() {
        return false
    });
    
	$("#slideM1").ImgAct({
                        auto: true,
                        showNum: 5
                    });
	
