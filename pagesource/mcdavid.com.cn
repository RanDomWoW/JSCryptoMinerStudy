        b > $(".floorList").offset().top ? ($(".fixBackTop").css({
            visibility: "visible"
        }), $(".fixBackTop").addClass("backTopShow")) : ($(".fixBackTop").css({
            visibility: "hidden"
        }), $(".fixBackTop").removeClass("backTopShow")),
        b > $(".floorList").offset().top - 200 ? $(".searchBar").hasClass("fixedOn") || ($(".searchBar").addClass("fixedOn").fadeIn(), $(".navApps,.head_navBar").hide(), $("#keyword").unautocomplete()) : $(".searchBar").hasClass("fixedOn") && ($(".searchBar").removeClass("fixedOn").removeAttr("style"), $(".navApps,.head_navBar").show(), wdAutoCM()),
        b + a > $(".n-footer").offset().top && $(".n-footer .service").removeAttr("style"),
        $(".ac_results").hide()
