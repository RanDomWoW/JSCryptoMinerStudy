        var self = $(this);
        var flag = 1;
        var h;
        var handle = function(elm) {
            clearTimeout(h);
            if (!flag) {
                self.removeData("timer")
            }
            return flag ? fuc_on.apply(elm) : null
        };
        var time_on = de_on || 0;
        var time_out = de_out || 500;
        var timer = function(elm) {
            h && clearTimeout(h);
            h = setTimeout(function() {
                handle(elm)
            },
            flag ? time_on: time_out);
            self.data("timer", h)
        };
        self.hover(function() {
            flag = 1;
            timer(this)
        },
        function() {
            flag = 0;
            timer(this)
        })
    };
