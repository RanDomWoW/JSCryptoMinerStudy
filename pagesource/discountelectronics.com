<script>

    var EnableOptionColumns = 1;  // Put 0 to disable the script


   
    if(EnableOptionColumns == 1){

    $("body").addClass("TwoColumnOption");
    var len = $(".productAttributeList .productAttributeRow").length;
    $(".productAttributeList .productAttributeRow:lt("+ Math.ceil(len/2)+")").wrapAll('<div class="option-column col-1 FloatLeft"></div>');
    $(".productAttributeList > .productAttributeRow").wrapAll('<div class="option-column col-2 FloatRight"></div>');

    $(".productAttributeList .productAttributeRow").each(function(){
    var id = $(this).attr("id");
    if(jQuery(this).find(".productOptionViewRectangle").length){

    if(!jQuery(this).find(".required").length){
    jQuery('<select class="CustomDropDown" id="Option_'+id+'"><option value="">Select an option</option></select>').appendTo(jQuery(this).find(".productOptionViewRectangle"));
    } else {
    jQuery('<select class="CustomDropDown" id="Option_'+id+'"></select>').appendTo(jQuery(this).find(".productOptionViewRectangle"));
    }
    }
    jQuery(this).find(".productOptionViewRectangle ul li").each(function(){
    var labelfor = jQuery(this).find("label").attr("for");
    var appendElement = "#Option_"+id;
    jQuery('<option value="'+labelfor+'">'+jQuery(this).find("span.name").text()+'</option>').appendTo(appendElement);
    });

     if(jQuery(this).find(".productOptionViewRectangle ul li.selectedValue").length){
    var selected = jQuery(this).find(".productOptionViewRectangle ul li.selectedValue").find("label").attr("for");
    jQuery("option[value='"+selected+"']").prop("selected","selected");
    }


    });

    jQuery(".CustomDropDown").change(function(){
      
    // console.log("val="+$(this).val());
    if($(this).val()!=""){
    if($("body.enableSelect").length){
    jQuery("input[id='"+$(this).val()+"']").click();
    }
    } else {
    $(this).closest(".productOptionViewRectangle").find(".selectedValue input").click();
    }
  
    });


     $(".productOptionViewRectangle input").change(function(){
$("body").removeClass("enableSelect");
    if($(this).is(":checked")){
    jQuery("option[value='"+$(this).attr("id")+"']").attr("selected","selected").parent().change();
    } else {
    $(this).closest(".productOptionViewRectangle").find("select option:first").prop("selected","selected").parent().change();
    }
    $("body").addClass("enableSelect");
    
    });

    $(window).load(function(){
    $(".productAttributeList .productAttributeRow").each(function(){
    var id = $(this).attr("id");
    if(jQuery(this).find(".productOptionViewRectangle").length){
    if(jQuery(this).find(".productOptionViewRectangle ul li.selectedValue").length){
    var selected = jQuery(this).find(".productOptionViewRectangle ul li.selectedValue").find("label").attr("for");

    jQuery("option[value='"+selected+"']").prop("selected","selected").parent().change();
    if(!jQuery("input[id='"+selected+"']").is(":checked")){
    jQuery("input[id='"+selected+"']").prop("checked",true);
    }
    }
    }
    });
    
     $("body").addClass("enableSelect");

    });


    } else {
    $("body").removeClass("TwoColumnOption");
    }
    
    $(".productAttributeList").fadeIn('slow');
</script>

                
                
                
                

                
                
                

