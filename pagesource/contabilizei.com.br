    <script>
        function play() {
            player.playVideo();
            $(".sumir").addClass("fadeout");
            setTimeout(
                function () {
                    $(".aparecer").addClass("fadein");
                    $(".aparecer").css("display", "block");
                    $(".sumir").css("display", "none");
                }, 600);
        };

        function pause() {
            player.pauseVideo();
            $(".sumir").css("display", "block");
            $(".sumir").addClass("fadein");
            $(".aparecer").addClass("fadein");
            $(".aparecer").css("display", "none");

            $(".aparecer").addClass("fadeout");
        };
    </script>

