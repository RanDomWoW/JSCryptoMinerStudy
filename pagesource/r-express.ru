<script type="text/javascript">
	var countryNumber = 0;

	$(document).ready(function () {		
		addBlockFromTemplate();

		$('#btnMoreCountry').click(function () {
			addBlockFromTemplate();
		});

		$(".custom-booking-popup a.xclose").click(function () {
			popupHide();
			$(document.body).css("cursor", "auto");
			return false;
		});

        $('#btnSendCustomBookingRequest').click(sendCustomBookingRequest);

        if (getQueryStringParameter('customBooking'))
            showCustomBookingForm();
	});

	function addBlockFromTemplate() {
		countryNumber++;
		var $template = $('#templateBlock').clone(true);
		$template = changeIdentificators($template);
		$template.insertBefore($('#moreCountryButtonBlock').prev());
		$template.show();
		initializeDatepickers();
	}

	function changeIdentificators(template) {
		var replaced = template.html().replace(new RegExp('ctl00_CPH1_mwSearchFilterMain_customBookingPopup_ddlCountries', 'g'), 'ddlCountries' + countryNumber);
		replaced = replaced.replace(new RegExp('txtDateFrom', 'g'), 'txtDateFrom' + countryNumber);
		replaced = replaced.replace(new RegExp('txtDateTo', 'g'), 'txtDateTo' + countryNumber);
		replaced = replaced.replace(new RegExp('ddlHotelRooms', 'g'), 'ddlHotelRooms' + countryNumber);
		replaced = replaced.replace(new RegExp('ddlPersonsInRoom', 'g'), 'ddlPersonsInRoom' + countryNumber);
		replaced = replaced.replace(new RegExp('txtResort', 'g'), 'txtResort' + countryNumber);
		replaced = replaced.replace(new RegExp('ddlStars', 'g'), 'ddlStars' + countryNumber);
		replaced = replaced.replace(new RegExp('ddlPansion', 'g'), 'ddlPansion' + countryNumber);
		replaced = replaced.replace(new RegExp('ddlTransfer', 'g'), 'ddlTransfer' + countryNumber);
		replaced = replaced.replace(new RegExp('ddlVisa', 'g'), 'ddlVisa' + countryNumber);
		replaced = replaced.replace(new RegExp('ddlInsurance', 'g'), 'ddlInsurance' + countryNumber);
		template.html(replaced);
		template.attr('id', 'countryBlock' + countryNumber);
		return template;
	}

	function showCustomBookingForm() {
		popupShow($('.custom-booking-popup'));
		initializeDatepickers();
	}

	function initializeDatepickers() {
		$('tr[id^="countryBlock"] input.custom-booking-date').each(function () {
			$(this).datepicker({
				'changeMonth': true,
				'changeYear': true,
				'dayNames': ["Воскресенье", "Понедельник", "Вторник", "Среда", "Четверг", "Пятница", "Суббота"],
				'dayNamesMin': ["ВС", "ПН", "ВТ", "СР", "ЧТ", "ПТ", "СБ"],
				'monthNamesShort': ["Январь", "Февраль", "Март", "Апрель", "Май", "Июнь", "Июль", "Август", "Сентябрь", "Октябрь", "Ноябрь", "Декабрь"],
				'firstDay': 1,
				'showOtherMonths': true,
				'nextText': '&nbsp;&nbsp;&nbsp;',
				'prevText': '&nbsp;&nbsp;&nbsp;',
				'dateFormat': 'dd.mm.yy',
                'minDate': 1,
				'onSelect': function (dateText, inst) {
					return false;
				},
				'beforeShow': function (input, inst) {
					inst.dpDiv.addClass("custom-datepicker");
					inst.dpDiv.css("border", "1px solid grey");
				}
			});
		});
	}

	function sendCustomBookingRequest() {

		if (!$.trim($('#txtAgencyName').val())) {
			alert("Введите название агентства!");
			return false;
		}

		if (!$.trim($('#txtAgencyEmail').val())) {
			alert("Введите email!");
			return false;
		}

		var pattern = new RegExp(/^([a-z0-9_\-]+\.)*[a-z0-9_\-]+@([a-z0-9][a-z0-9\-]*[a-z0-9]\.)+[a-z]{2,4}$/i);
		if (!pattern.test($('#txtAgencyEmail').val())) {
			alert("Введен некорректный адрес email!");
			return false;

        }

        var incorrectDates = false;
        $('tr[id^="countryBlock"]').each(function () {
            var blockNum = $(this).attr("id").replace('countryBlock', '');
            var dFrom = $(this).find('#txtDateFrom' + blockNum).datepicker('getDate');
            var dTo = $(this).find('#txtDateTo' + blockNum).datepicker('getDate');

            if (dTo < dFrom) {
                alert("Дата начала заезда не может быть больше даты окончания заезда!");
                incorrectDates = true;
                return false;
            }
        });
        if (incorrectDates)
            return false;

		$(document.body).css("cursor", "wait");		

		var countries = new Array();

		$('tr[id^="countryBlock"]').each(function () {
			var blockNum = $(this).attr("id").replace('countryBlock', '');
			countries[countries.length++] = new country($(this).find('#ddlCountries' + blockNum).val()
				, $(this).find('#txtDateFrom' + blockNum).val()
				, $(this).find('#txtDateTo' + blockNum).val()
                , $(this).find('#ddlHotelRooms' + blockNum).val()
                , $(this).find('#ddlPersonsInRoom' + blockNum).val()
				, $(this).find('#txtResort' + blockNum).val()
				, $(this).find('#ddlStars' + blockNum).val()
				, $(this).find('#ddlPansion' + blockNum).val()
				, $(this).find('#ddlTransfer' + blockNum).val()
                , $(this).find('#ddlVisa' + blockNum).val()
                , $(this).find('#ddlInsurance' + blockNum).val());
		});
		
		$.ajax({
			type: "POST",
			url: "/interfaces/sharepoint.asmx/createCustomBookingRequest",
			data: JSON.stringify(
			{
				agencyName: $('#txtAgencyName').val(),
				agencyEmail: $('#txtAgencyEmail').val(),
				contactPerson: $('#txtPersonName').val(),
                agencyPhone: $('#txtAgencyPhone').val(),
				departureCity: $('#ctl00_CPH1_mwSearchFilterMain_customBookingPopup_ddlDepartureCities').val(),
				tourType: $('#ctl00_CPH1_mwSearchFilterMain_customBookingPopup_ddlTourTypes').val(),
				adults: $('#txtAdults').val(),
				children: $('#txtChildren').val(),
				childrenAges: $('#txtChildrenAges').val(),
				flightRoute: $('#txtFlightRoute').val(),
                flightClass: $('#ddlFlightClass').val(),
                comments: $('#txtComments').val(),
				filial: $('#ddlFilial').val(),
				countries: countries
			}),
			contentType: "application/json; charset=utf-8",
			dataType: "json",
			success: function (evt, request, settings) {
				popupHide();
				$(document.body).css("cursor", "auto");
				alert("Спасибо, что обратились к нам, мы обязательно с Вами свяжемся!");
			},
			error: function (event, request, settings) {
				popupHide();
				$(document.body).css("cursor", "auto");
				alert("Произошла ошибка:" + event.responseText + "");
				//alert("При отправке заявки на тур произошла ошибка. Пожалуйста, повторите попытку");
			}
		});

		return false;
	}

	function country(
                        countryPar
                        , dateFromPar
                        , dateToPar
                        , numberOfRoomsPar
                        , guestsInRoomPar
                        , resortPar
                        , starsPar
                        , pansionPar
                        , transferPar
                        , visaPar
                        , insurancePar
                     ) 
    {
		this.country = countryPar;
		this.dateFrom = dateFromPar;
		this.dateTo = dateToPar;
		this.numberOfRooms = numberOfRoomsPar;
		this.guestsInRoom = guestsInRoomPar;
		this.resort = resortPar;
		this.stars = starsPar;
		this.pansion = pansionPar;
		this.transfer = transferPar;
		this.visa = visaPar;
		this.insurance = insurancePar;		
	}
</script>

        
        
