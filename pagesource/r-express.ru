	<script type="text/javascript">
	    $(document).ready(function () {

	        $("#lnk_sos").click(function () {
	            popupShow($('.online-support-popup'));
	            return false;
	        });

	        $(".online-support-popup a.xclose").click(function () {
	            popupHide();
	            $(document.body).css("cursor", "auto");
	            return false;
	        });

	        $("#sos_type").change(function () {
	            if ($('#sos_type').find('option:selected').attr('emergency')) {
	                $('#sos_content').val("Свяжитесь с нами по телефону: (495) 925-66-99. Бесплатная линия для регионов: 8 800 200 66 99. Горячая линия, работающая круглосуточно: +7 926 511 66 99, +7 926 690 66 99.");
	                //$("button.fio").unbind('click', sendSosMessage);
	            }
	            else {
	                $('#sos_content').val('');
	                $("button.fio").bind('click', sendSosMessage);
	            }
	        });

	        $("button.fio").click(sendSosMessage);

	        function sendSosMessage() {

	            if (!$.trim($('#sos_fio').val())) {
	                alert("Введите имя!");
	                return false;
	            }

	            if (!$.trim($('#sos_email').val())) {
	                alert("Введите email!");
	                return false;
	            }

	            var pattern = new RegExp(/^([a-z0-9_\-]+\.)*[a-z0-9_\-]+@([a-z0-9][a-z0-9\-]*[a-z0-9]\.)+[a-z]{2,4}$/i);
	            if (!pattern.test($('#sos_email').val())) {
	                alert("Введен некорректный адрес email!");
	                return false;
	            }

	            if (!$.trim($('#sos_content').val())) {
	                alert("Укажите, в чем Ваша проблема!");
	                return false;
	            }

	            $(document.body).css("cursor", "wait");
	            $.ajax({
	                type: "POST",
	                url: "/interfaces/feedback.asmx/sendSOS",
	                data: JSON.stringify(
							{
							    fio: $('#sos_fio').val(),
							    email: $('#sos_email').val(),
							    problemType: $('#sos_type option:selected').text(),
							    problemContent: $('#sos_content').val(),
							    department: $('#sos_type').val()
							}),
	                contentType: "application/json; charset=utf-8",
	                dataType: "json",
	                success: function (evt, request, settings) {
	                    popupHide();
	                    $(document.body).css("cursor", "auto");
	                    alert("Спасибо, что обратились к нам, мы обязательно с Вами свяжемся!");
	                },
	                error: function (event, request, settings) {
	                    popupHide();
	                    $(document.body).css("cursor", "auto");
	                    alert("Произошла ошибка:" + event.responseText + "");
	                }
	            });

	            return false;
	        }
	    });

		
	</script>
