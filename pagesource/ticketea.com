  <!-- Add the template into a script with text/template type which id is 'event-card-template' -->
  <script type="text/template" id="event-card-template">
  <article class="card card--<%= template_size %>
            <% if (event.is_lowest_price) { %> ab-card-lowest-price<% } %>"
           <% if (next_session_dates.length >= 1 && event.type == 0) { %>
             itemscope itemtype="http://schema.org/Event"
           <% } %>
  >
    <% if (next_session_dates.length >= 1 && event.type == 0) { %>
      <meta itemprop="url" content="<%= event.full_url %>"/>
    <% } %>
    <div class="card__item">
      <a href="<%= event.full_url %><% if (typeof tktpap != 'undefined') { %>?<%= tktpap %><% } %>"
        <% if (typeof product_tracking != 'undefined') { %>
            data-space-id="<%= product_tracking.space_id %>"
            data-product-id="<%= product_tracking.product_id %>"
            data-product-index="<%= product_tracking.product_index %>"
            data-product-source="<%= product_tracking.product_source %>"
        <% } %>
        <% if (typeof card_index !== 'undefined' && typeof card_type !== 'undefined') { %>
          data-event-category="interaction" data-event-action="<%= card_type %>_cards" data-event-label="<%= card_type %>_cards_<%= card_index %>"
        <% } %>
        aria-label="<%= event.title %>"
        class="card__link<% if (typeof product_tracking != 'undefined') { %> t2strk <% } %>"
      >

        <% if(event.type == 1) { %>
          <div class="card__tag">
            <%= gettext('Season') %>
          </div>
        <% } %>
        <div class="picture-frame picture-frame--placeholder picture-frame--card">
          <% if(event.campaign_start && event.campaign_end && new Date(moment(event.campaign_start)).getTime() <= new Date().getTime() && new Date().getTime() <= new Date(moment(event.campaign_end)).getTime()) { %>
            <div class="mkt-campaign-stamp">
              <div class="mkt-campaign-stamp__title"><%= event.campaign_title %></div>
              <div class="mkt-campaign-stamp__text"><%= event.campaign_text %></div>
            </div>
          <% } %>
          <div class="picture-frame__image" role="img" aria-label="<%= event.title %>" style="background-image: url('<%= image_url %>');"></div>
          <img src="<%= image_url %>"
               class="card__image picture-frame__img-tag"
               alt="<%= event.title %>"
               width="336"
               height="184"
               <% if (next_session_dates.length >= 1 && event.type == 0) { %>
                 itemprop="image"
               <% } %>
          >
          <i class="picture-frame__placeholder"></i>
        </div>

        <div class="card__heading">
          <% if (event.is_lowest_price) { %>
            <div class="card-label card-label--over-image"><%= gettext('Lowest price guaranteed') %></div>
          <% } %>
          <h3 class="card__title"
              <% if (next_session_dates.length >= 1 && event.type == 0) { %>
                itemprop="name"
              <% } %>
          >
            <span class="clip-text"><%= event.title %></span>
          </h3>
        </div>
        <div class="card__info">
          <% if (event.type == 0) { %>
            <div class="date-module--<%= template_size %>">
              <meta itemprop="startDate" content="<%= TICKETEA.L10N.make_timezone_aware_date(event.start_date_time, event.timezone).format('YYYY-MM-DD') %>"/>
              <meta itemprop="endDate" content="<%= TICKETEA.L10N.make_timezone_aware_date(event.end_date_time, event.timezone).format('YYYY-MM-DD') %>"/>

              <% if (moment.utc().isAfter(event.end_date_time)) { %>
                <span class="date-module__month"></span>
                <span class="date-module__day-week">
                  <% if (event.type == 0) { %>
                    <%= gettext('Event') %>
                  <% } else { %>
                    <%= gettext('Season') %>
                  <% } %>
                </span>
                <span class="date-module__day--various"><%= gettext('Finished') %></span>
              <% } else if (event.is_short || event.starts_and_ends_same_day) { %>
                <abbr title="<%= TICKETEA.L10N.make_timezone_aware_date(moment(event.start_date_time).toISOString(), event.timezone).format('MMMM') %>" class="date-module__month">
                  <%= TICKETEA.L10N.make_timezone_aware_date(moment(event.start_date_time).toISOString(), event.timezone).format('MMM') %>
                </abbr>
                <abbr title="<%= moment(event.start_date_time).format('dddd') %>" class="date-module__day-week">
                  <%= TICKETEA.L10N.make_timezone_aware_date(moment(event.start_date_time).toISOString(), event.timezone).format('ddd') %>
                </abbr>
                <span class="date-module__day">
                      <%= TICKETEA.L10N.make_timezone_aware_date(moment(event.start_date_time).toISOString(), event.timezone).format('DD') %>
                </span>
              <% } else { %>
                <span class="date-module__month"></span>
                <span class="date-module__day-week"><%= gettext('multiple') %></span>
                <span class="date-module__day--various"><%= gettext('dates') %></span>
              <% } %>
            </div>
          <% } else { %>
            <div class="date-module--small">
              <span class="date-module__number"><%= event.num_events %></span>
              <span class="date-module__text"><%= gettext('events') %></span>
            </div>
          <% } %>
          <div class="card__more-info">
            <% if (event.type == 0) { %>
              <% if (typeof event.min_price !== 'undefined' && event.min_price != null) { %>
                <div class="price"
                     <% if (next_session_dates.length >= 1) { %>
                       itemprop="offers"
                       itemscope
                       itemtype="http://schema.org/AggregateOffer"
                     <% } %>
                >
                  <span class="card__icon faw-icon-currency"></span>
                  <div class="price__text">
                    <% if (next_session_dates.length >= 1) { %>
                      <meta itemprop="lowPrice" content="<%= event.min_price / 100 %>"/>
                      <meta itemprop="priceCurrency" content="<%= event.currency %>" />
                      <meta itemprop="url" content="<%= event.full_url %>"/>
                    <% } %>
                    <%= gettext('from') %>
                    <span data-currency="<%= event.currency %>" data-price="<%= event.min_price %>">
                      <%= min_price %>
                    </span>
                  </div>
                </div>
              <% } %>
              <div class="location"
                   <% if (next_session_dates.length >= 1) { %>
                      itemprop="location"
                      itemscope
                      itemtype="http://schema.org/Place"
                   <% } %>
              >
                <span class="card__icon faw-icon-map-marker"></span>
                <div class="location__text">
                  <span class="clip-text">
                    <span<% if (next_session_dates.length >= 1) { %> itemprop="name"<% } %>><%= event.venue_name %></span>,
                    <% if (next_session_dates.length >= 1) { %>
                      <span itemprop="address" itemscope itemtype="http://schema.org/PostalAddress">
                        <span itemprop="addressLocality">
                    <% } %>
                        <%= event.venue_city %>
                    <% if (next_session_dates.length >= 1) { %>
                        </span>
                      </span>
                    <% } %>
                  </span>
                </div>
              </div>
            <% } else { %>
              <div class="date">
                <span class="card__icon faw-icon-calendar"></span>
                <div class="date__text">
                  <%= gettext('from') %>
                  <strong>
                    <span>
                      <%= TICKETEA.L10N.make_timezone_aware_date(event.start_date_time, event.timezone).format('DD') %>
                    </span>
                    <abbr title="<%= TICKETEA.L10N.make_timezone_aware_date(event.start_date_time, event.timezone).format('MMMM') %>">
                      <%= TICKETEA.L10N.make_timezone_aware_date(event.start_date_time, event.timezone).format('MMM') %>
                    </abbr>
                  </strong>
                  <%= gettext('to') %>
                  <strong>
                    <span>
                      <%= TICKETEA.L10N.make_timezone_aware_date(event.end_date_time, event.timezone).format('DD') %>
                    </span>
                    <abbr title="<%= TICKETEA.L10N.make_timezone_aware_date(event.end_date_time, event.timezone).format('MMMM') %>">
                      <%= TICKETEA.L10N.make_timezone_aware_date(event.end_date_time, event.timezone).format('MMM') %>
                    </abbr>
                  </strong>
                </div>
              </div>
              <div class="location">
                <% if (event.organizer_name) { %>
                  <span class="card__icon faw-icon-organizer"></span>
                  <div class="location__text">
                    <span class="clip-text">
                      <%= event.organizer_name %>
                    </span>
