  $(document).ready(function () {
      var start = [];
      var end = [];
      var initRoundFixtures = [];
      var initRoundResults = [];
      var activeRoundFixtures = [];
      var activeRoundResults = [];
            start[5] = 1;
      end[5] = 10;
      initRoundFixtures[5] = $('#fixtures .table-switcher[data-competition="5"]').data('current');
      initRoundResults[5] = $('#results .table-switcher[data-competition="5"]').data('current');
      activeRoundFixtures[5] = $('#fixtures .table-switcher[data-competition="5"]').data('current');
