      initRoundResults[6] = $('#results .table-switcher[data-competition="6"]').data('current');
      activeRoundFixtures[6] = $('#fixtures .table-switcher[data-competition="6"]').data('current');
      activeRoundResults[6] = $('#results .table-switcher[data-competition="6"]').data('current');
      
    $('#fixtures .table-switcher .next').click(function () {

      $('#fixtures .table-switcher .prev').removeClass('disabled');

      var competition_id = $(this).parent().data('competition');
      var parentElement = $(this).parent();

      if (++activeRoundFixtures[competition_id] <= end[competition_id])
      {
        if (activeRoundFixtures[competition_id] == end[competition_id])
          $(this).addClass('disabled');

        $(this).parent().find('.curr-label').text(activeRoundFixtures[competition_id]);
        $.get("/v1/games?round="+activeRoundFixtures[competition_id]+"&competition_id="+competition_id, function( data ) {
          generateRoundGames(data, parentElement);
        });
      }
      else {
        $(this).addClass('disabled');
      }
    });

    $('#fixtures .table-switcher .prev').click(function () {

      $('#fixtures .table-switcher .next').removeClass('disabled');

      var competition_id = $(this).parent().data('competition');
      var parentElement = $(this).parent();

      if (--activeRoundFixtures[competition_id] >= initRoundFixtures[competition_id])
      {
        if (activeRoundFixtures[competition_id] == initRoundFixtures[competition_id])
          $(this).addClass('disabled');

        $(this).parent().find('.curr-label').text(activeRoundFixtures[competition_id]);
        $.get("/v1/games?round="+activeRoundFixtures[competition_id]+"&competition_id="+competition_id, function( data ) {
          generateRoundGames(data, parentElement);
        });
      }
      else {
        $(this).addClass('disabled');
      }
    });

    $('#results .table-switcher .next').click(function () {

      $('#results .table-switcher .prev').removeClass('disabled');

        var competition_id = $(this).parent().data('competition');
        var parentElement = $(this).parent();

      if (++activeRoundResults[competition_id] <= initRoundResults[competition_id])
      {
        if (activeRoundResults[competition_id] == initRoundResults[competition_id])
          $(this).addClass('disabled');

        $(this).parent().find('.curr-label').text(activeRoundResults[competition_id]);
        $.get("/v1/games?round="+activeRoundResults[competition_id]+"&competition_id="+competition_id, function( data ) {
          generateRoundGames(data, parentElement);
        });
      }
      else {
        $(this).addClass('disabled');
      }
    });

    $('#results .table-switcher .prev').click(function () {

      $('#results .table-switcher .next').removeClass('disabled');

        var competition_id = $(this).parent().data('competition');
        var parentElement = $(this).parent();

      if (--activeRoundResults[competition_id] >= start[competition_id])
      {
        if (activeRoundResults[competition_id] == start[competition_id])
          $(this).addClass('disabled');

        $(this).parent().find('.curr-label').text(activeRoundResults[competition_id]);
        $.get("/v1/games?round="+activeRoundResults[competition_id]+"&competition_id="+competition_id, function( data ) {
          generateRoundGames(data, parentElement);
        });
      }
      else {
        $(this).addClass('disabled');
      }
    });

    function generateRoundGames(games, parent) {
      var len = games.length;
      var html = '<table class="table table-striped"><tbody>';
      for (var i = 0; i < len; i++) {
        html += '<tr>';
        html += '<td class="text-center">' +
                games[i].kickoff_date +
                '<br>' +
                games[i].kickoff_time +
                '</td><td><img src="' + games[i].home_logo + '" alt=""> ' +
                games[i].home_team_name +
                '<br>' +
                '<img src="' + games[i].away_logo + '" alt=""> ' +
                games[i].away_team_name +
                '</td><td>' +
                (games[i].home_score ? games[i].home_score : '-') +
                '<br>' +
                (games[i].away_score ? games[i].away_score : '-') +
                '</td>';
        html += "</tr>";
      }
      html += '</tbody></table>';

      parent.next('table').replaceWith(html);
    }
  });
