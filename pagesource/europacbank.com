			  if (document.getElementById('remember-me').checked){
				$.cookie('disclaimer_accepted', 'yes', { expires: 180, path: '/' }); // set cookie
			  }
			  else {$.cookie('disclaimer_accepted', 'yes', {expires: 1, path: '/' }); }  // set 1 day cookie}
			   	$.fancybox.close();
			  	return false;
	  		});

			//if deny is clicked
			$('a#deny-dis').click(function () {
				if (history.length > 1) {
				    history.back();
