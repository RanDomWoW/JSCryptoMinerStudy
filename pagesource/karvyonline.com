        function IndicesChgColor(appColorClass) {

            $('.' + appColorClass + '').each(function () {

                if ($(this).text().indexOf('-') > -1) {
                    $(this).removeClass('grn');
                    $(this).addClass('rdtxt');
                    $(this).parent().find('.indexIndicesUpDownArrow').removeClass('fa fa-caret-up grn');
                    $(this).parent().find('.indexIndicesUpDownArrow').addClass('fa fa-caret-down rdtxt');

                } else if ($(this).text() == "0.00") {
                    $(this).removeClass('grn rdtxt');
                    $(this).addClass('blktxt');
                    $('.indexIndicesUpDownArrow').removeClass('fa fa-caret-up grn');
                    $('.indexIndicesUpDownArrow').removeClass('fa fa-caret-down rdtxt');
                } else {
                    $(this).removeClass('rdtxt');
                    $(this).addClass('grn');
                    $(this).parent().find('.indexIndicesUpDownArrow').removeClass('fa fa-caret-down rdtxt');
                    $(this).parent().find('.indexIndicesUpDownArrow').addClass('fa fa-caret-up grn');
                }
            });
        }
		
		
