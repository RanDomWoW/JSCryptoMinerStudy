		function setCookie(cname, cvalue, exdays) {
			var d = new Date();
			d.setTime(d.getTime() + (exdays*24*60*60*1000));
			var expires = "expires="+d.toUTCString();
			document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
		}

		function getCookie(cname) {
			var name = cname + "=";
			var ca = document.cookie.split(';');
			for(var i=0; i<ca.length; i++) {
				var c = ca[i];
				while (c.charAt(0)==' ') c = c.substring(1);
				if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
			}
			return "";
		}

		function checkCookie() {
			var user = getCookie("username");
			if (user != "") {
				alert("Welcome again " + user);
			} else {
				user = prompt("Please enter your name:", "");
				if (user != "" && user != null) {
					setCookie("username", user, 365);
				}
			}
		}

		function deleteCookie(cname) {
		  document.cookie = cname + '=; expires=Thu, 01 Jan 1970 00:00:01 GMT;';
		}

		function getQueryVariable(variable) {
		 // Grab and unescape the query string - appending an '&' keeps the RegExp simple
			// for the sake of this example.
			var queryStr = unescape(window.location.search) + '&';

			// Dynamic replacement RegExp
			var regex = new RegExp('.*?[&\\?]' + variable + '=(.*?)&.*');

			// Apply RegExp to the query string
			val = queryStr.replace(regex, "$1");

			// If the string is the same, we didn't find a match - return false
			return val == queryStr ? false : val;
		}

		
		/***************************************/
		
		if(getCookie("origem") == ""){
			setCookie("origem", "notset>indefinido>indefinido>indefinido>indefinido_XX", 30);
		}
		
		if(!document.referrer){			
			origem_temp = "direto>indefinido>indefinido>indefinido>indefinido_AA";
		}
		
		if(getQueryVariable('utm_source')){
			origem_temp = ""
			+ getQueryVariable('utm_source')
			+ ">"
			+ ""
			+ ">"
			+ (!getQueryVariable('utm_medium') ? 'indefinido' : getQueryVariable('utm_medium')) 
			+ ">"
			+ (!getQueryVariable('utm_content') ? 'indefinido' : getQueryVariable('utm_content'))
			+ ">"
			+ (!getQueryVariable('utm_campaign') ? 'indefinido_CC' : getQueryVariable('utm_campaign'))
			;
		}
		
		if(document.referrer){	
			
			referer = URI.parse(document.referrer);

			if((referer.scheme + '://' + referer.host).indexOf(location.host) != -1){
				origem_temp = "direto>indefinido>indefinido>indefinido>indefinido_BB"; //posso montar esse como o de campanhas, para caso de banners internos
			}else{
				origem_temp = ""
				+ referer.scheme + '://' + referer.host
				+ ">"
				+ referer.path
				+ ">"
				+ (!getQueryVariable('utm_medium') ? 'indefinido' : getQueryVariable('utm_medium')) 
				+ ">"
				+ (!getQueryVariable('utm_content') ? 'indefinido' : getQueryVariable('utm_content'))
				+ ">"
				+ (!getQueryVariable('utm_campaign') ? 'indefinido_CC' : getQueryVariable('utm_campaign'))
				;
			}
		
		}

			
		origem_candidada = origem_temp.split(">");
		origem_atual = getCookie("origem").split(">");

		
		trocar = 1;
		if(origem_atual[2] == 'cpc' && origem_candidada[2] != 'cpc'){
			trocar = 0;
		}
		if(origem_atual[0] != 'direto' && origem_atual[0] != 'notset' && origem_candidada[0] == 'direto'){
			trocar = 0;
		}
		
		if(trocar == 1){
			setCookie("origem", origem_temp, 30);
		}
		

		/*************/
		
		$.ajax({
            url: "includes/ajax_origem.php",
            data: {referrer_js: document.referrer, origem_cookie: getCookie("origem"), url_atual: document.URL, origem_rdstation: getCookie("__trf.src").split(">>")[0]},
            method: 'post',
            beforeSend: function(){
            },
            success: function( data ) {
				console.log("data: " + data);
            }
        });
		
		
		
	
	</script>
	
	

