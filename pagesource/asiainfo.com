<style>
    .bannerImage{display:none;}
    .mainContent{background-color:transparent!important; padding:0px!important; height:390px;}


/*and (min-width: 421px)*/
   @media (max-width: 900px)
    {
     .headerContent{width:768px; height:315px!important; }
     .slider{margin-top:-50px; }
