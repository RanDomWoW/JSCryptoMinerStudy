            $('html, body').animate({ scrollTop: 0 }, 'slow');
            $('#lightBoxBackground').fadeIn('slow');
            $('#lightBoxPopUp').fadeIn('slow');

            $('#lightBoxContent').html('<iframe width="100%" height="100%" src="' + $(this).attr('href') + '" frameborder="0"></iframe>');

