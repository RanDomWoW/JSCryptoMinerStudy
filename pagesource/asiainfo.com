<script type="text/javascript">

   var bannerArr = [];
   var pArr = [];
   var currentIndex = 0;
   var myTimer;

   $(document).ready(function () {
       $('.banner').each(function () {
           bannerArr.push($(this));
       });

       if ($.browser.msie && parseInt($.browser.version, 10) === 8) {
           $('.banner').each(function () {
               $(this).css('display', 'none');
           });

           //show the first elements only slide #1
           bannerArr[currentIndex].addClass('selectedBanner');

           pArr = [];
           $('.selectedBanner p').each(function () {
               pArr.push($(this));
           });

           for (var i = 0; i < pArr.length; i++) {
               pArr[i].css('marginTop', (i * 24 + 220));
           }

           $(".selectedBanner").fadeIn();


       } else {
           setUpAnimationBanners();
       }



   });

   function setUpAnimationBanners() {
      

       if (bannerArr.length > 0) {

           //now add navigation controls
           $('.slider').append("<div id='SliderNavBtn_Previous' class='sliderNavBtn'></div>")
           $('.slider').append("<div id='SliderNavBtn_Next' class='sliderNavBtn'></div>");
           $('.slider').append("<div id='SliderQuickLinkContainer'><div>");

           //and for each provide a quick link
           for (var i = 0; i < bannerArr.length; i++) {
               $('#SliderQuickLinkContainer').append("<div class='sliderQuickLink' alt='" + i + "'></div>");
           }

           animateBanner();

           //now register the slider navigation click handlers
           $('#SliderNavBtn_Previous').click(function (evt) {
               currentIndex--;
               animateBanner();
           });

           $('#SliderNavBtn_Next').click(function (evt) {
               currentIndex++;
               animateBanner();
           });

           $('.sliderQuickLink').click(function (evt) {
               currentIndex = $(this).attr('alt');
               animateBanner();
           });
       }


   }


   function determineNavDisplay() {

       if (currentIndex == 0) {
           $('#SliderNavBtn_Previous').hide();
       } else {
           $('#SliderNavBtn_Previous').show();
       }

       if (currentIndex == (bannerArr.length - 1)) {
           $('#SliderNavBtn_Next').hide();
       } else {
           $('#SliderNavBtn_Next').show();
       }

       //and now show a highlight on the currently selected slide link
       $('.sliderQuickLink').each(function (evt) {
           $(this).removeClass('highlightedQuickLink');
           if ($(this).attr('alt') == currentIndex) {
               $(this).addClass('highlightedQuickLink');
           }
       });

   }

   function resetBanner() {
       if ($('.selectedBanner .shadow').length > 0) {
           $('.selectedBanner .shadow').stop();
           $('.selectedBanner .shadow').animate({ width: '0px' }, 50);
       }
       if ($('.selectedBanner .logo').length > 0) {
           $('.selectedBanner .logo').stop();
           $('.selectedBanner .logo').animate({ marginTop: '0px', opacity: '0' }, 50);
       }
       if ($('.selectedBanner .veriscloudlogo').length > 0) {
           $('.selectedBanner .veriscloudlogo').stop();
           $('.selectedBanner .veriscloudlogo').animate({ marginTop: '30px', opacity: '0' }, 50);
       }
       if ($('.selectedBanner .brand').length > 0) {
           $('.selectedBanner .brand').stop();
           $('.selectedBanner .brand').animate({ opacity: '0' }, 50);
       }

       $(".selectedBanner h2").stop();
       $(".selectedBanner h2").animate({ marginLeft: '0px', opacity: '0' }, 50);

       for (var i = 0; i < pArr.length; i++) {
           pArr[i].stop();
           pArr[i].animate({ marginLeft: '0px', opacity: '0' }, 50);
       }

       $(".selectedBanner .actionLink").stop();
       //  $(".selectedBanner .actionLink").animate({ opacity: '0' },50);
       $(".selectedBanner .actionLink").css('display', 'none');

       $('.selectedBanner .bannerImage').fadeOut();
       $('.selectedBanner').removeClass('selectedBanner');
   }

   function animateBanner() {
       determineNavDisplay();

       //first off reset the currently selected banner

       resetBanner();

      
       bannerArr[currentIndex].addClass('selectedBanner');
       $('.selectedBanner .bannerImage').fadeIn();

       pArr = [];
       $('.selectedBanner p').each(function () {
           pArr.push($(this));
       });

 
       if ($('.selectedBanner .shadow').length > 0) {
           $('.selectedBanner .shadow').delay(1000).animate({ width: '+=50px' }, "fast").animate({ opacity: '-=0.3' }).animate({ opacity: '+=0.3' });
       }
       if ($('.selectedBanner .logo').length > 0) {
           $('.selectedBanner .logo').delay(1000).animate({ marginTop: '+=50px', opacity: '1' }).animate({ marginTop: '-=15px' }).animate({ marginTop: '+=10px' }).animate({ marginTop: '-=5px' }).animate({ marginTop: '+=2px' });
       }
       if ($('.selectedBanner .brand').length > 0) {
           $('.selectedBanner .brand').delay(1000).animate({ opacity: '1' }, 1000, showDetails);
       } else {
            showDetails();
       }
   }

    function showDetails() {
        //animate the h2
        $(".selectedBanner h2").animate({ marginLeft: '+=115px', opacity: '1' }, "slow", "swing", showAction);

        for (var i = 0; i < pArr.length; i++) {
            pArr[i].css('marginTop', (i * 24 + 230));
            pArr[i].delay(i * 500).animate({ marginLeft: '+=140px', opacity: '1' }).animate({ marginLeft: '-=25px' });
        }

    }

    function showAction() {
        $(".selectedBanner .actionLink").css('display', 'block').animate({ opacity: '1' }, 2000, animationFinished);
    }

   function animationFinished() {
       //  myTimer = setInterval(function(){myTimerInterval()},4000);
       $(".selectedBanner .actionLink").animate({ opacity: '1' }, 4000, myTimerInterval);
    }
 
    function myTimerInterval(){
     //    window.clearInterval(myTimer);
        if (currentIndex != (bannerArr.length - 1))
        {
            currentIndex ++;
        }else{
            currentIndex = 0;
        }
        animateBanner();
 
    }
</script>

