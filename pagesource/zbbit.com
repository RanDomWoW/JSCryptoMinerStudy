var nbxlastcookie = $.cookie('nbxlast_');
var nbxlast = nbxlastcookie==null?0:nbxlastcookie;
function win1(key)
{
	var winkey = key.replace(/-/g,'_');
	(window.open("/games/" + key, winkey, "width=750,height=450,resize=no,location=no,scrollbar=no,statusbar=no")).focus();
	return false;
}
function win2(key)
{
	var winkey = key.replace(/-/g,'_');
	(window.open("/games/" + key, winkey, "width=300,height=300,resize=no,location=no,scrollbar=no,statusbar=no")).focus();
	return false;
}
function auto_check_inbox() {
	$.getJSON('nbx.php',{nbxlast:nbxlast,r:(new Date()).getTime()},notify_inbox);
}
function notify_inbox(j) {
	if (j[0]>0)
	{
		nbxlast = j[1];
		$.cookie('nbxlast_',nbxlast,{path:'/'});
		$('#mybar_inbox_unread_a').attr('title',j[2]+' ข้อความใหม่');
		$('#mybar_inbox_unread_img').attr('alt',j[2]+' ข้อความใหม่');
		$('#nbxnum').html(j[0]);
		$('#nbxnotify').fadeIn('fast');
	} else {
		setTimeout(auto_check_inbox, 15000);
	}
	if (j[2]>0) {
		$('#mailicon').attr('src','/pic/newmail-icon.gif');
	} else {
		$('#mailicon').attr('src','/pic/mail-icon.gif');
	}
}
function close_notify() {
	$(this).fadeOut(100);
	setTimeout(auto_check_inbox, 30000);
}
