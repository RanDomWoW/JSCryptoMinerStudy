        function getSliderConfiguration() {
            var globalOptions = {
                animation: 'slide',
                slideshow: false,
                controlNav: false,
                animationLoop: false,
                selector: '.slides > .slide'
            };

            var desktopOptions = {
                itemWidth: 120,
                itemMargin: 0
            };
                        desktopOptions.maxItems = 4;
            
            var tabletOptions = {
                itemWidth: 250,
                itemMargin: 0,
                maxItems: 2,
                move: 2
            };

            var mobileOptions = {
                itemWidth: 200,
                itemMargin: 0,
                maxItems: 1,
                move: 1
            };


            var options = desktopOptions;
            if (typeof ResponsiveManager !== 'undefined') {
                var state = responsiveManagerInstance.getCurrentState();
                if (state === ResponsiveManager.MOBILE) {
                    options = mobileOptions;
                } else if (state === ResponsiveManager.TABLET) {
                    options = tabletOptions;
                }
            }

            options = jQuery.extend(globalOptions, options);

            return options;
        }
        function initProductSlider() {
            jQuery("#widget-5aae05d74baec").flexslider(getSliderConfiguration());
        }

        function refreshSlider() {
            var sliderContainer = jQuery("#widget-5aae05d74baec");
            var carousel = sliderContainer.data("flexslider");
            carousel.vars = jQuery.extend(carousel.vars, getSliderConfiguration());
            carousel.resize();
            sliderContainer.flexslider(0);
        }
