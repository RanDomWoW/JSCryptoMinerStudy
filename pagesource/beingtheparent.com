<style>
.stickysidebar, .postSidebar {
    position: absolute;
}
.stickysidebarfixed {
    position: fixed;
}
</style>
<script type="text/javascript">
jQuery(function () {
  var msie6 = jQuery.browser == 'msie' && jQuery.browser.version < 7;
  if (!msie6 && jQuery('.stickysidebar').offset()!=null) {
    var top = jQuery('.stickysidebar').offset().top - parseFloat(jQuery('.stickysidebar').css('margin-top').replace(/auto/, 0));
	var height = jQuery('.stickysidebar').height();
	var winHeight = jQuery(window).height();	
	var footerTop = jQuery('.mainfooter').offset().top - parseFloat(jQuery('.mainfooter').css('margin-top').replace(/auto/, 0));
	var gap = 7;
    jQuery(window).scroll(function (event) {
      // what the y position of the scroll is
      var y = jQuery(this).scrollTop();
      
      // whether that's below the form
      if (y+winHeight >= top+ height+gap && y+winHeight<=footerTop) {
        // if so, ad the fixed class
        jQuery('.stickysidebar').addClass('stickysidebarfixed').css('top',winHeight-height-gap +'px');
      } 
	  else if (y+winHeight>footerTop) {
        // if so, ad the fixed class
       jQuery('.stickysidebar').addClass('stickysidebarfixed').css('top',footerTop-height-y-gap+'px');
      } 
	  else 	  
	  {
        // otherwise remove it
        jQuery('.stickysidebar').removeClass('stickysidebarfixed').css('top','0px');
      }
    });
  }  
});

if(jQuery(window).width() >= '414px') {
	jQuery('.search-field.placeholder').attr('placeholder','Search');
	jQuery('.search-field.placeholder').val('');
}
</script>
