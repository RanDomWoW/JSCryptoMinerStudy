<script src="/owl2/owl.carousel.min.js"></script>
<script src="/owl2/owl.lazyload.js"></script>


<script>
	jQuery(document).ready(function($) {
	var options = {
		parent: '#wrapper',
		inner_scrolling: false,
		spacer: '.sticky-content-spacer',
		offset_top: 20	};
	var stickWidth = 768;
	if ($(window).width() > stickWidth) {
		$('#sidemenu > .inner').stick_in_parent(options);
	}
    $(window).resize(function () {
	   if ($(window).width() > stickWidth) {
        $('#sidemenu > .inner').stick_in_parent(options);
    	} else {
        $('#sidemenu > .inner').trigger("sticky_kit:detach");
        }
	});

	$("#carousel-picks").owlCarousel({
			loop: true,
			margin: 20,
			stagePadding:20,
			nav: false,
			responsiveClass:true,
		    responsive:{
		        0:{
		            items:2
		        },
		        600:{
		            items:3
		        },
		        900: {
			        items:4
		        }

		    },
		    navText : [
	      "<i class='fa fa-angle-left' aria-hidden='true'></i>",
	      "<i class='fa fa-angle-right' aria-hidden='true'></i>"
	      ],
  		});

  	function random(owlSelector){
		    owlSelector.children().sort(function(){
	        	return Math.round(Math.random()) - 0.5;
				}).each(function(){
				$(this).appendTo(owlSelector);
	    	});
		}
		var $owl = $( '#editors-picks' );
		$owl.on('initialize.owl.carousel', function(event){
		var selector = $('#editors-picks');
		random(selector);
		});

	$("#editors-picks").owlCarousel({
	    items : 1,
		lazyLoad:true,
		loop:true,
		nav: true,
		dot: false,
		mouseDrag: false,
	    navText : [
	      "<i class='fa fa-angle-left' aria-hidden='true'></i>",
	      "<i class='fa fa-angle-right' aria-hidden='true'></i>"
	      ],

		});

});
</script>


</body>
</html>

