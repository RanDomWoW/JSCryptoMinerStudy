    // Deal with the dropdown menu and it's delay
    var RI_TopMenuMouseOverTimer;
    var RI_TopMenuBackgroundTimer;
    var RI_TopMenuOpen = false;
    var RI_TopMenuOver;
    var RI_TopMenuWaitTime = 250; // Time in ms to wait on mouse over before showing menu
    // Mouse over a top menu item
    $('.top-menu').on('mouseenter', function () {
        RI_TopMenuMouseOverTimer = setTimeout(function () {
            // Say the top menu is now open
            RI_TopMenuOpen = true;
            // Open the drop-down that the mouse is over
            $(RI_TopMenuOver).addClass('hover');
            // Remove any existing background hider timer
            clearTimeout(RI_TopMenuBackgroundTimer);
            // Show the background display
            $('.top-menu-background-hider').removeClass('hidden');
            // Set the background to the hight of the current height of the page
            $('.top-menu-background-hider').height($('html').height() - $('.top-menu-background-hider').offset().top);
            // Enable the background hider
            $('.top-menu-background-hider').addClass('show');
        }, RI_TopMenuWaitTime);
    })
    $('.top-menu').on('mouseleave', function () {
        clearTimeout(RI_TopMenuMouseOverTimer);
        RI_TopMenuOpen = false;
        // Disable the background hider
        $('.top-menu-background-hider').removeClass('show');
        RI_TopMenuBackgroundTimer = setTimeout(function () {
            // Hide the grey background hider
            $('.top-menu-background-hider').addClass('hidden');
        }, 1000);
    })
    $('li', '.top-menu').on('mouseenter', function () {
        // Store the object that has been moused over
        RI_TopMenuOver = this;

        // Is the menu already open
        if (RI_TopMenuOpen) { // The top menu is open so no need to wait
            // Open the popup
            $(this).addClass('hover');
        }
    });
    // Mouse leaves the top menu item
    $('li', '.top-menu').on('mouseleave', function () {
        // Remove the hover class from all top-menu buttons
        $(this).parent().children().removeClass('hover');
    });

    // Ensures the top menu is the correct width
    (function ($) {
        $.fn.autowidth = function () {
            return this.each(function () {
                // Work out the average tab width
                var tabWidth = ($(this).width() / $('li', this).length);
                // Calculate the last one 
                var lastTabWidth = tabWidth
                $('li', this).css({ 'width': (tabWidth + 1) + 'px' });
            });
        };
    })(jQuery);
    $(document).ready(function () {
        $('.header-menu ul.top-menu').autowidth();
    });
</script>
