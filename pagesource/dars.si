                    mask.hide(1, function() { mnu.hide(1); $(".topTransRight").css("z-index",12) })
                    $(_mnuActive).next().removeClass("sel");
                    if (_mnuActiveDef)
                        $(_mnuActiveDef).next().addClass("sel");                    
                }
            }, 200);
        }
    }
