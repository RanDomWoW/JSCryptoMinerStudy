    function TopLinkClick(el) {
        el = $(el);
        _linkIndex = $(".topLinks [class^=lnk]").index(el);
        
        $(".topLinks [class^=lnk]").removeClass("active");        
        el.addClass("active");

        $(".topBox").css("background", "url('" + el.attr("img") + "') no-repeat");

        if (eval($(".topTransRightTitle").css("opacity")) < 1)
            $(".topTransRightTitle").css("opacity", 1);

        if (eval($(".topTransRightDesc").css("opacity")) < 1)
            $(".topTransRightDesc").css("opacity", 1);

