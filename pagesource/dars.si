			if (el.hasClass('lnk0'))
                window.open(el.attr("url"),'_blank')
            else
                window.location = el.attr("url")
		});
		
		var desc = "<div>" + el.attr("desc") + "</div><a href=\"" + el.attr("url") + "\">Več ...</a>";
        
        $(".topTransRightTitle").stop(true).fadeOut(1, function () { $(this).html(el.attr("title").toUpperCase()).fadeIn(1); });
        $(".topTransRightDesc").stop(true).fadeOut(1, function () { $(this).html(desc).fadeIn(1); });
        if (_linkIndex == 0)
            $(".topLinksButton" + _linkIndex).click();              
    }

    function TopLinkMove(direction) {
        if (direction == "next") {
            _linkIndex ++;
            
            if (_linkIndex == _linkCount)                        
                _linkIndex = 0;                
            
            $(".btnNext").click();
        }
        else {
            _linkIndex --;
            if (_linkIndex < 0)
                _linkIndex = 0;
        }                
        TopLinkClick($(".topLinks li.lnk" + _linkIndex).get(0));
    }
    function MoveTopLinks() {
        if (_move) {
            TopLinkMove('next');
        }
        
