	function CheckLoggingCookie() {                
        var regexp = new RegExp("(?:^LoggingCookie|;\\s*LoggingCookie)=(.*?)(?:;|$)", "g");
        var cookie = regexp.exec(document.cookie);
        if (cookie == null) {                               
            $("#LoggingCookie").slideDown(1000);
            return true;
        }
        else
            return eval(cookie[1]);
    }
	
