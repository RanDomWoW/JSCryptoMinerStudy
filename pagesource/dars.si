    var _trafficIndex = 0;
    var _trafficMove = true;

    function TrafficReportEntryGet(s) {
        var title = s[6];
        title = title.replace(title.substring(0,2),"<span class='mark'>"+title.substring(0,2)+"</span>");

        var html = "<div title=\"" + s[1] + "\" class=\"clear e\"><div class=\"signSmall\"><img src=\"/images/promet/"+TrafficReportSignGet(s[4],"")+"\"/></div><div onmouseover=\"style.backgroundColor='#4d4d4e';\" onmouseout=\"style.backgroundColor=''\" class=\"entry\">";
        html +="<div>"+title+"</div><div class=\"clear\"><div class=\"left date\">"+s[2]+"</div><div class=\"right date\">"+s[3]+"</div></div></div></div>";        
        return html;
    }

    function TrafficReportMove(i) {
        
        if ($(".trafficReport .hidden div").html() == null)
        {
            $(".trafficReport .event").html("Ni podatkov");
            return;
        }
        
        if (_trafficMove)
        {            
            var m, s, ss = "";
            /*var m = $($(".trafficReport .hidden div").get(_trafficIndex)).html().split(';');
            var s = $($(".trafficReport .hidden div").get(_trafficIndex + 1)).html().split(';');
            var ss = $($(".trafficReport .hidden div").get(_trafficIndex + 2)).html().split(';');*/            
            switch(i)            
            {                
                case 0:
                    m = 0;
                    s = 1;
                    ss = 2;
                    i++;
                    break;  
                case 1:
                    m = 1;
                    s = 2;
                    ss = 0;
                    i++;
                    break;  
                case 2:
                    m = 2;
                    s = 0;
                    ss = 1;
                    i = 0;
                    break;                            
            }
          
          m = $($(".trafficReport .hidden div").get(m)).html().split(';');
          s = $($(".trafficReport .hidden div").get(s)).html().split(';');
          ss = $($(".trafficReport .hidden div").get(ss)).html().split(';');

            $($(".trafficReport .e").get(0)).slideUp(_trafficIndex == 0 ? 0: 1000, function () {
                var list = TrafficReportEntryGet(s) + TrafficReportEntryGet(ss)
                $(".trafficReport .list").html(list);
            });

            $(".trafficReport .event").stop().fadeOut(_trafficIndex == 0 ? 0 : 1000, function () {           
                var title = m[6];
                title = title.replace(title.substring(0,2),"<span class='mark'>"+title.substring(0,2)+"</span>");

                $(".trafficReport .event").attr("title",m[1]);                
                $(".trafficReport .event .t").html(title).attr("title",m[1]);
                $(".trafficReport .event .t2").html(m[5]);
                $(".trafficReport .event .left").html(m[2]);
                $(".trafficReport .event .right").html(m[3]);
                $(".trafficReport .sign img").attr("src", "/images/promet/" + TrafficReportSignGet(m[4],"Big"));                

                $(this).fadeIn(_trafficIndex == 0 ? 0: 500);
            });
            
            _trafficIndex++;
        
            if (_trafficIndex + 2 == $(".trafficReport .hidden div").length)
                _trafficIndex = 0;            
        }

        window.setTimeout(function () { TrafficReportMove(i++) }, 6000);
    }
</script>

