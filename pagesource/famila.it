<script>

	jQuery( document ).ready( function( $ ) {
	
		$("#headerOptions input[type=text]").attr("autocomplete","off");

		var changePdv  = "";
		if(( changePdv == "true" )&&($("#registrationStep2").length<1)) {

			$( "#scopri_btn" ).trigger( "click" );

			setTimeout( function() {

				$( "#scopri_btn" ).trigger( "click" );

			}, 2000 );

		}

		$( document ).on( 'click', '#toggleMenu', function( e ) {

			$( "#top-menu-desktop, #top-menu" ).slideToggle();

		} );
	
		$( '.selectric' ).selectric( {
        
			disableOnMobile: false

    	} );

		$("#headerOptions li input").attr("tabindex","-1");
	
		// listen for changes on nucleoFamigliare
		$(".componentiNucleo").hide();
		$("#opt-li_nucleoFamigliare .sendHeaderBtn").attr("disabled", "disabled").css("opacity","0.5");
	
		var valNF;
		
		$('#opt-li_nucleoFamigliare select').on('change', function() {
	  
			valNF = $(this).val();
	  		if(valNF>=0){
	  		
	  			$("#opt-li_nucleoFamigliare .sendHeaderBtn").prop("disabled", false).css("opacity","1");
	  	
	  			if(valNF>0){
		  	
	  				$("body").on('click', '#opt-li_nucleoFamigliare .sendHeaderBtn', function(event) {
		  		
	  					event.preventDefault();
		  			
	  					$(".componentiNucleo").hide();
		  		
	  					for(i=0;i<valNF;i++){
		  			
	  						$(".componentiNucleo:eq("+i+")").show();
		  		
	  					}
	  		
	  				});
  		
	  			}
	  
	  		}
	
		});  

		// animale domestico scelto?
		$("#opt-li_animaleDomestico .sendHeaderBtn").attr("disabled", "disabled").css("opacity","0.5");
		$('#opt-li_animaleDomestico select').on('change', function() {
		
			$("#opt-li_animaleDomestico .sendHeaderBtn").prop("disabled", false).css("opacity","1");
	
		});

		// professionescelta?
		$("#opt-li_professione .sendHeaderBtn").attr("disabled", "disabled").css("opacity","0.5");
		$('#opt-li_professione select').on('change', function() {
		
			$("#opt-li_professione .sendHeaderBtn").prop("disabled", false).css("opacity","1");
	
		});

		// block submit input
		$("#headerOptions li .input_txt").focus(function() {
		
			if($("#headerOptions li .input_txt").is(":focus")){
			
				$('#headerOptions li .input_txt').keypress(function(event){
			   
					if (event.keyCode == 10 || event.keyCode == 13) 
			       
						event.preventDefault();
			 
				});			
		
			}
	
		});

		// double input
		$(".secondInput").keyup('change', function() {
		
			if($(this).val()==""){
			
				$(this).siblings('.sendHeaderBtn').attr("disabled", "disabled").css("opacity","0.5");
		
			} else{
			
				$(this).siblings('.sendHeaderBtn').prop("disabled", false).css("opacity","1");
		
			}
	
		});

	} );

	$(window).load(function() {
	
		// check if children of nucleo famigliare exists
		childs2Bfield = "";
		if (childs2Bfield>0){
		
			for (i=0;i<childs2Bfield;i++){$(".componentiNucleo").eq(i).show();}
	
		}

	});

</script>




