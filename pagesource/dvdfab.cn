<script src="https://dneux1ee0sm9x.cloudfront.net/templates/dvdfab10/script/jquery.cookie.js?1013"></script>
<script>
	// 显示提示
	var COOKIE_NAME = "notice";
	jQuery(function() {
		var source = getUrlParam("source");
		var notice = $.cookie(COOKIE_NAME);
		if( notice != 'hidden' && source === 'sothink'){
			$("#index-notice").show(1000, function() {
				setTimeout("ClearIndexHeadPopup()", '30000');
			});
		}else{
			$("#index-notice").hide();
		}
		$("#notice-close").click(function(){
			ClearIndexHeadPopup();
		})
	});
	function ClearIndexHeadPopup() {
		$('#index-notice').hide();
		$.cookie(COOKIE_NAME,"hidden", {path: '/', expires: 3650});
	}
	//获取url中的参数
	function getUrlParam(name) {
		var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)"); 
		var r = window.location.search.substr(1).match(reg); 
		if (r != null) return unescape(r[2]); return null;
	}
</script>
 
