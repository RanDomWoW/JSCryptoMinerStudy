<script type="text/javascript">

$(document).ready(function(){
	if ($.cookie('cookieOptin') == null )
	{
	    $('#cookie-banner').removeClass( "hide" );
	}	
	
	var referrer = document.referrer.split('/')[2];
	
	if ( ACC.config.onBoardingBanner === referrer )
	{
		$('#onboarding-layer').removeClass( "hide" );
	}
});

function hideBanner(){
	$('#onboarding-layer').addClass( "hide" );
}

function addCookie(){
	if (typeof ACC != "undefined" && typeof ACC.config != "undefined" && typeof ACC.config.cookieOptinTTL != "undefined"){
			$('#cookie-banner').addClass( "hide" );
			
					$.cookie("cookieOptin", true, { path: '/', expires : Math.round(ACC.config.cookieOptinTTL), secure: true });
				
	};
}
