		    function showModal(pContent, footerContent)
		    {
		        var modalBody = $('.modal-body');
		        $(modalBody).empty();

		        // place anchor at top and link at bottom to work around missing or misplaced html anchor links in document
		        // (i.g. anchor of Privacy Policy is under the header "Privacy Policy for LandmarkWorldwide.com." 
                // Course legal terms contains no anchor links at all 
		        pContent = "<a id='_top' style='display:none;'></a>" + pContent + "<a href='#_top' style='display:none;'></a>";
		        if (footerContent)
		            pContent = '<div class="ftContent" style="overflow-y:auto">' + pContent + "</div>" + footerContent;

		        $(modalBody).html(pContent);
		        $(modalBody).scrollTop(0);
		        
		        try
		        {   // scroll to top  
		            var elements = $(modalBody).find("[href='#_top']");
		            var a = $(elements).eq($(elements).length - 1);

		            if ($(a).length > 0)
		            {
		                $(a).click(function ()
		                {
		                    $(modalBody).animate({ scrollTop: 0 }, 'fast');
		                    return false;
		                });

		                if ($(a)[0].click)
		                {
		                    $(a)[0].click();
		                }
		                else
		                {   // for safari
		                    var event = document.createEvent('MouseEvent');
		                    event.initEvent('click', true, true);
		                    $(a)[0].dispatchEvent(event);
		                }
		            }
                    
		            var at = $(modalBody).find("a[id$=_top]");
		            if ($(at).length > 0)
		                $(at)[0].scrollIntoView(true);
		        }
		        catch (e) { }

		        var modalDiv = $('#modalDiv');

		        $(modalDiv).on('tap touchstart', handleTouch);
		        $(modalDiv).on('keyup click', handleMouseKey);
		        $(document).on('tap touchstart', handleTouch);
		        $(document).on('keyup', handleMouseKey);

		        $('#imgCloseModal').click(function () { closeModal() });
		        $(modalDiv)[0].style.display = 'block';

		        var __h = $(modalBody).height();
		        if (footerContent)
		            $('.ftContent').css('max-height', (__h - 140) + 'px');

		    }
		    
		    function handleTouch(e)
		    {
		        var target = e.target || e.srcElement;
		        if (target && $(target).parents('#modalContentDiv').length > 0)
		            return;
		        else
		            closeModal();
		    }

		    function handleMouseKey(e)
		    {
		        var target = e.target || e.srcElement;
		        var keyCode = e.keyCode || e.which;

		        if (keyCode == 27)
		        {
		            closeModal();
		        }
		        else if (target && $(target).parents('#modalContentDiv').length > 0)
		        {
		            return;
		        }
		        else if (keyCode == 1) 
		        {
		            closeModal();
		        }
		    }

            
