<script type="text/javascript">
	function CheckSelection(fieldID) {
		if ($("#" + fieldID).val() == "0") // no selection made
		{
			$("#" + fieldID).parent().addClass("has-error");
			$("#" + fieldID).removeClass("form-control2");
			return false;
		}
		else // valid selection
		{
			$("#" + fieldID).parent().removeClass("has-error");
			$("#" + fieldID).addClass("form-control2");
			return true;
		}
	}

	function LoanStep_Submit() {
		var FieldsPass = true;
		if (CheckSelection("LoanType") == false) { FieldsPass = false; }
		if (CheckSelection("CreditRatingID") == false) { FieldsPass = false; }
		if (FieldsPass == true) {
			$("#btnSubmit").button("loading");
			return true;
		}
		else { return false; }
	}
</script>


