var placehold = function( element ) {
	
	var self = this;
	
	this.element = element;

	this.onfocuses = 0;
	this.defaultText = '';
	
	this.element.on('focus', function(){

		self.onfocuses++;
		if( self.onfocuses === 1 ){
			self.defaultText = this.value;	
		}
		if( this.value === self.defaultText ){
			this.value = '';	
		}
	});
	
	this.element.on('blur', function(){
		if( this.value === '' ){
			this.value = self.defaultText;
		}
	});
	
}

