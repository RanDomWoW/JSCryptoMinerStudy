<script type="text/javascript">
			function gov2go($id) {
			$('#registerErrors').addClass('hidden');
			$('#registerForm').addClass('hidden');
			$('#registerSuccess').addClass('hidden');
			$('#registerProgress').removeClass('hidden');

			$.ajax({
				type: 'POST',
				url: 'https://microservice.getgov2go.com/api/v1/accounts',
				processData: false,
				headers: {
					"Content-Type": "application/json; charset=utf-8",
				},
				contentType: "application/json",
				data: JSON.stringify({
					"email": $($id).find('input[name="email"]').val(),
					"state": "AR",
					"language": "en"
				})
			})
			.done(function(data){completeRegister(data);})
			.fail(function(ajax) {
				if (ajax.status == 409) {completeRegister(ajax.responseJSON);} else {
					alert("Failed to register you. Please try again later.");
					$('#registerForm').removeClass('hidden');
					$('#registerProgress').addClass('hidden');
				}
			});
			}

			function completeRegister($res) {
				$('#registerProgress').addClass('hidden');
				if ($res['errors'].length > 0) {
				$('#registerErrors').html($res['errors'][0]);
				$('#registerForm').removeClass('hidden');

				$('#registerErrors').removeClass('hidden');
				//_gaq.push(['_trackEvent', 'Signup', 'Error Portal']);
				}
				else {
				//_gaq.push(['_trackEvent', 'Signup', 'Success', 'Portal']);
				$('#registerSuccess').removeClass('hidden');
				//window.location.href = 'http://app.getgov2go.com';

				}
  			}
			</script>
