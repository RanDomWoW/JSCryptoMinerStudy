        }).on(&amp;#039;hidden&amp;#039;, function () {
          utp_block.css(&amp;#039;width&amp;#039;, &amp;#039;700px&amp;#039;);
          // reset to standard
          utp_block.on(&amp;#039;show&amp;#039;, function () {
            utp_block.css(&amp;#039;width&amp;#039;, &amp;#039;700px&amp;#039;);
          });
          });
      }
      }

    /**
     * box slider gallery
     * */

    modal_block_gallery.length &amp;amp;&amp;amp; init();
    function init() {
      var self = this;
      var modal_slider = {
        &amp;#039;firstI&amp;#039;: modal_block_gallery.find(&amp;#039;.main-thumb.main&amp;#039;),
        &amp;#039;lis&amp;#039;: modal_block_gallery.find(&amp;#039;.boxes-slider__previews&amp;#039;).find(&amp;#039;li&amp;#039;),
        &amp;#039;touched&amp;#039;: modal_block_gallery.find(&amp;#039;.boxes-slider__preview&amp;#039;)
      };
      this.determine_and_change = function (data) {
        if (typeof data !== &amp;#039;object&amp;#039; || !data) return false;
        modal_block_gallery.attr(&amp;#039;style&amp;#039;, data.dummy ? &amp;#039;background-image:url(&amp;quot;&amp;#039; + data.dummy + &amp;#039;&amp;quot;);&amp;#039; : &amp;#039;&amp;#039;);
        if (data.main) {
          modal_slider.firstI.attr(&amp;#039;src&amp;#039;, &amp;#039;&amp;#039;).animate({opacity: 0}, 200, function () {
            modal_slider.firstI.attr(&amp;#039;src&amp;#039;, data.main).animate({opacity: 1}, 200);
            data.class_name &amp;amp;&amp;amp; modal_slider.firstI.parent().attr(&amp;#039;class&amp;#039;, &amp;#039;boxes-slider__thumb-img&amp;#039;).addClass(data.class_name)
          });
        }
      };
      this.touch = function () {
        var $self = $(this), data = {};
        if ($self.hasClass(&amp;#039;selected&amp;#039;)) return false;
        data.data_attr = $self.find(&amp;#039;.boxes-slider__preview&amp;#039;);
        if (!data.data_attr.length) return;
        var boxImg = data.data_attr.next();
        data.main = boxImg.data(&amp;#039;main&amp;#039;);
        if (!data.main) return false;
        data.class_name = boxImg.data(&amp;#039;box-price&amp;#039;);
        data.dummy = boxImg.data(&amp;#039;dummy&amp;#039;);
        self.determine_and_change(data);
        modal_slider.lis.removeClass(&amp;#039;selected&amp;#039;);
        $self.addClass(&amp;#039;selected&amp;#039;);
      };
      modal_slider.lis.length &amp;amp;&amp;amp; modal_slider.lis.on(&amp;#039;click&amp;#039;, this.touch);
    }

    var on_click_change_type = function () {
      var thisElement = $(this);
      var tabType = thisElement.data(&amp;#039;type&amp;#039;);
      var container = $(&amp;#039;.modal-boxes-slider&amp;#039;);
      container.find(&amp;#039; .changeTab&amp;#039;).removeClass(&amp;#039;disable&amp;#039;);
      container.find(&amp;#039;.holder-boxType&amp;#039;).hide();
      container.find(&amp;#039;.holder-boxType[data-type=&amp;quot;&amp;#039; + tabType + &amp;#039;&amp;quot;]&amp;#039;).show();
      container.find(&amp;#039;.changeTab[data-type=&amp;quot;&amp;#039; + tabType + &amp;#039;&amp;quot;]&amp;#039;).addClass(&amp;#039;disable&amp;#039;);
    };
    $(document).on(&amp;#039;click&amp;#039;, &amp;#039;.changeTab&amp;#039;, on_click_change_type);
  }());
