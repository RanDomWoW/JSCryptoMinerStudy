<script type="text/javascript">


function getUrlVars()
{
  var queries = {};
   url = window.location.href;

  if(url.indexOf('?') != -1)
  {
 $.each(document.location.search.substr(1).split('&'),function(c,q){
   var i = q.split('=');
   queries[i[0].toString()] = i[1].toString();
 });
}
return queries;
}

var events = (function(){
  var topics = {};
  var hOP = topics.hasOwnProperty;

  return {
    subscribe: function(topic, listener) {
      // Create the topic's object if not yet created
      if(!hOP.call(topics, topic)) topics[topic] = [];

      // Add the listener to queue
      var index = topics[topic].push(listener) -1;

      // Provide handle back for removal of topic
      return {
        remove: function() {
          delete topics[topic][index];
        }
      };
    },
    publish: function(topic, info) {
      // If the topic doesn't exist, or there's no listeners in queue, just leave
      if(!hOP.call(topics, topic)) return;

      // Cycle through topics queue, fire!
      topics[topic].forEach(function(item) {
          item(info != undefined ? info : {});
      });
    }
  };
})();

// Sending whole WE Object to Backendside
window.onload = function()
{
    if (window.jQuery)
    {
$(document).ready(function(){
  if($('.wpcf7-form').length) {
  webengage.onReady(function(){
    var queryParams = getUrlVars();
    var weInfo = webengage.util.getSessionCookie();
        weInfo['getAnonymousId'] = webengage.user.getAnonymousId();
        weInfo['queryParams'] = JSON.stringify(queryParams);
  $('input[name="we-ui-data"]').val(JSON.stringify(weInfo)) });

// Code for the Country CODE
        $('.contactwithflag').each(function(flags) {
        var currId = $(this).attr('id');
        $("#"+currId).intlTelInput({
                  preferredCountries: [ "in", "us", "gb" , "ae" ],
                  hiddenInput: 'hidden-'+currId,
                  nationalMode: true,
                    autoPlaceholder: "on",
                    dropdownContainer: "body",
                    separateDialCode: true,
                    geoIpLookup: $.get("https://ipinfo.io", function() {}, "jsonp").always(function(resp) {
                      var countryCode = (resp && resp.country) ? resp.country : "";
                      $("#"+currId).intlTelInput("setCountry", countryCode);
                  })
              });

        var hField = 'hidden-'+currId;
        var countryData = $("#"+currId).intlTelInput("getSelectedCountryData");
        $('input[name="'+hField+'"]').val(countryData.dialCode) ;
});
        // Code for the Country CODE

}
});

}

}
