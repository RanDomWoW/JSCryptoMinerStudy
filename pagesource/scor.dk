					setValidation(jQuery("#FrontpageControl_CreateProfile_HairColour2DropDownList"), true);
					step = Math.min(step, 3);
				}
				if (jQuery.inArray(40, errors) != -1)
				{
					setValidation(jQuery("#FrontpageControl_CreateProfile_Children2DropDownList"), true);
					step = Math.min(step, 3);
				}
				if (jQuery.inArray(41, errors) != -1)
				{
					setValidation(jQuery("#FrontpageControl_CreateProfile_Build2DropDownList"), true);
					step = Math.min(step, 3);
				}

				if (jQuery.inArray(42, errors) != -1)
				{
					alert(getErrorMsg(42));
				}

				RegistrationWaitHide();
				jQuery(".rotator").cycle("goto", step - 1);

				var validation = jQuery(".create-profile").find(".step" + step).find(".validation.error").first();
				var element = validation.siblings(".wrong");
				var errorMsgs = element.data("errorMsgs");
				if (errorMsgs)
				{
					showInfoBubble(element, validation, validation.closest(".input-group"), errorMsgs);
				}

				return;
			}

			if (data.UGUID)
			{
				jQuery.cookie("UGUID", data.UGUID, {expires: 2 * 365 * 24 * 60});
			}

			goog_report_conversion();

			if (typeof adf !== "undefined")
			{
			    adf.Params.PageName = encodeURIComponent('Desktop/Signup Done');
				adf.Params.Divider = encodeURIComponent('|');
				adf.track(240624);
			}

			window.location = data.StartUrl;
		});
}

