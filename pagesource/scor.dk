	var annimation = jQuery("#RegistrationWaitPopup .annimation");
	var arrow = jQuery("#RegistrationWaitPopup .arrow");

	annimation.removeClass("left").addClass("right");
	arrow.css({left: 0});

	var pos = 0;
	var right = true;
	waitIntervalID = setInterval(function()
	{
		if (right)
		{
			pos += 0.5;
			arrow.css({left: pos + "%", right: ""});
			if (pos == 100)
			{
				right = false;
				annimation.removeClass("right").addClass("left");
			}
		}
		else
		{
			pos -= 0.5;
			arrow.css({left: "", right: 100 - pos + "%"});
			if (pos == 0)
			{
				right = true;
				annimation.removeClass("left").addClass("right");
			}
		}
	}, 50);
}

function RegistrationWaitHide()
{
	clearInterval(waitIntervalID);
	DialogHide("RegistrationWaitPopup");
}

