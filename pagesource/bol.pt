$(document).ready(function () {
	// Carregar carrossel
	$('#corpo-conteudo .seccoes .destaque').each(function (i, e) {
		var sl = $(this).find('.itens-container').show().bxSlider({ preloadImages: 'visible', pager: false, controls: false, slideWidth: 136, minSlides: 1, maxSlides: 100, moveSlides: 1, slideMargin: 12 });
		$(this).find('.destaque-controlos .destaque-controlos-prev').click(function () { sl.goToPrevSlide(); return false; });
		$(this).find('.destaque-controlos .destaque-controlos-next').click(function () { sl.goToNextSlide(); return false; });
	});

	// Função de resize de janela irá mostrar os controlos quando os itens do carrossel forem maiores
	var func = function () {
		$('#corpo-conteudo .seccoes .destaque').each(function (i, e) {
			var destaque_width = 0;
			$(this).find('.itens-container .item-montra:not(.bx-clone)').each(function(){
				destaque_width += $(this).outerWidth(true);
			});
			var bxslider_width = $(this).find('.bx-wrapper').outerWidth(true);
			if (bxslider_width < destaque_width) {
				$(this).addClass('navegacao');
			} else {
				$(this).removeClass('navegacao');
			}
		});
	};
	$(window).resize(func);
	func();
});if (window != window.top) window.top.location.href = window.location.href;//]]>
</script>
