				<!--サイドバーで広告を追尾させる-->
        <script type="text/javascript">
            jQuery(window).load(function(){
                var target = $("#fixd_block");
                var footer = $("#container_footer_1");
                var targetHeight = target.outerHeight(true);
                var targetTop = target.offset().top;
                var cmHeight = $("#container_main").height();
                var crHeight = $("#container_right").height();

                $(window).scroll(function(){
                    if(cmHeight > crHeight){
                        var scrollTop = $(this).scrollTop();

                        if(scrollTop > targetTop){
                            // 動的にコンテンツが追加されてもいいよう に、常に計算する
                            var footerTop = footer.offset().top;

                            if(scrollTop + targetHeight > footerTop){
                                customTopPosition = footerTop - (scrollTop + targetHeight);
                                target.css({position: "fixed", top: customTopPosition + "px"});
                            }else{
                                target.css({position: "fixed", top: "52px"});
                            }
                        }else{
                            target.css({position: "static", top: "auto"});
                        }
                    };
                });
            });
        </script>
