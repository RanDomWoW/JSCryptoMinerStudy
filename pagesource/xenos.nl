<script type="text/javascript">
	function submitNewsletterSubscribeForm() {
		jQuery("#NewsletterSubscribeForm").submit();
	}

	jQuery("#NewsletterSubscribeForm").submit(function(event) {
		/* stop form from submitting normally */
		event.preventDefault();

		
			dataLayer.push({
				'event': 'newsletterSubscribe'
			});
		

		/* get some values from elements on the page: */
		var $form = jQuery( this ),
			_nsf_email = $form.find( 'input[name="email"]' ).val(),
			_nsf_storeId = $form.find( 'input[name="storeId"]' ).val(),
			_nsf_langId = $form.find( 'input[name="langId"]' ).val(),
			_nsf_sourceForSalesForce = $form.find( 'input[name="sourceForSalesForce"]' ).val(),
			_nsf_subscriptionType = $form.find( 'input[name="subscriptionType"]' ).val(),	
			url = $form.attr( 'action' );

		var _nsf_ok = true;
			
		// jQuery("#newslettersubscribe_email_validation").hide();
		var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
			
		if (_nsf_email == '' || !emailReg.test( _nsf_email )) {
			// jQuery("#newslettersubscribe_email_validation").show();
			_nsf_ok = false;
		}	
		
		if (_nsf_ok) {
			/* Send the data using post */
			var posting = jQuery.post( url, { 
				'email': _nsf_email,
				'storeId': _nsf_storeId,
				'langId': _nsf_langId,
				'sourceForSalesForce':_nsf_sourceForSalesForce,
				'subscriptionType':_nsf_subscriptionType
			} );
			
			posting.done(function( data ) {
				var newsletterResponse = jQuery.parseJSON(data);

				jQuery("#newslettersocials_newsletter_input").val('');
				jQuery("#newslettersocials_newsletter_validation").hide();
				jQuery("#newslettersocials_newsletter_info").hide();
				jQuery("#newslettersocials_newsletter_complete").show();
				MessageHelper.displayStatusMessage('Bedankt voor het aanmelden <br/>Vanaf nu profiteert u van speciale acties en voordelen voor onze nieuwsbriefabonnees.');
				
				// Send data to Relay42
				try{
					if ('true' == 'true' && typeof(_st) != 'undefined'){
						_st('publishEvent', 'newsletter', 'subscribeNewsletter', [{'clangId' : newsletterResponse.customerClangId}]);
					}
				} catch (err) {} 	
			});
		} else {
			jQuery("#newslettersocials_newsletter_validation").show();
			jQuery("#newslettersocials_newsletter_complete").hide();
			jQuery("#newslettersocials_newsletter_info").hide();
		}
	});
</script><!-- END NewsletterSocials.jsp -->
