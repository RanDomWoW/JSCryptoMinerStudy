<script type="text/javascript">
$(document).ready(function()
{
    var param = {limit : 4, tentame_id : ""};
    $.ajax({
        type: "POST",
        url: "/ajax/request_tentame_enq.php",
        data: JSON.stringify(param),
        dataType: "json",

        success: function(data)
        {
            if (data.error == 0) {
                // HTML構築
                if (data.count > 0) {
                    // ミツけるの枠を表示
                    $("#mitsukeru").append('<div class="heading xx-large bd bd-pink enclosingBtn withIcon mb_10"><h1><span class="icoWrap"><img src="https://image.moratame.net/img2014/icons/ico15.png" alt="" width="81" height="77"></span><span class="fz_18 fw_b">お店でミツける。</span><span class="fz_14">参加受付中！</span></h1><p id="popupTriger04" class="btnWrap"><a href="https://www.moratame.net/detail/mitsukerulist.php" class="btnBean01 btnFind">ミツけるとは？</a></p><p id="popup04" class="popup" style="display: none;">対象商品を<br>お店で見つけて試そう♪</p>');
                    $("#mitsukeru").append('<div class="pickup mb_20"><div class="cardContainer tame ta-list mb_30"><div class="row" id="find-item">');

                    var x = 1;
                    for (index in data.list) {
                        var enq = data.list[index];
                        var zan = parseInt(enq.offer_count) - parseInt(enq.user_count);

                        // 現在日時取得
                        date = new Date();
                        yy = date.getYear();
                        mm = date.getMonth() + 1;
                        dd = date.getDate();
                        hh = date.getHours();
                        ii = date.getMinutes();
                        ss = date.getSeconds();
                        if (yy < 2000) { yy += 1900; }
                        if (mm < 10) { mm = "0" + mm; }
                        if (dd < 10) { dd = "0" + dd; }
                        if (hh < 10) { hh = "0" + hh; }
                        if (ii < 10) { ii = "0" + ii; }
                        if (ss < 10) { ss = "0" + ss; }
                        var now = yy + "-" + mm + "-" + dd + " " + hh + ":" + ii + ":" + ss;

                        // 開始日の翌日取得
                        var year  = enq.start_date.substr(0,4);
                        var month = enq.start_date.substr(5,2);
                        var date  = enq.start_date.substr(8,2);
                        var time  = enq.start_date.substr(11);
                        var obj = nextDate(year,month,date);
                        if (obj.month < 10) {
                            month = "0" + obj.month
                        } else {
                            month = obj.month;
                        }
                        if (obj.date  < 10) {
                            date  = "0" + obj.date
                        } else {
                            date = obj.date
                        }
                        var new_date = obj.year + "-" + month + "-" + date + " " + time;
                        if (x % 2 == 0) {
                            var control = 'r';
                        } else {
                            var control = 'l';
                        }

                        var html;
                        var icon;

                        if (enq.stat == "2") {
                            // 回答済
                            icon = '<p class="badge cardBadge already">回答済み</p>';
                        } else if (zan <= 100) {
                            // 残りわずか
                            icon = '<p class="badge cardBadge few">残りわずか</p>';
                        } else if (now < new_date) {
                            // NEW
                            icon = '<p class="badge cardBadge new">NEW</p>';
                        } else {
                            // アイコン無し
                            icon = '';

                        }

						var disp_count = enq.count;

						//カンマクギリ
						disp_count = String( enq.count ).replace( /(\d)(?=(\d\d\d)+(?!\d))/g, '$1,' );

                        html  = '<div class="card heightLine-group7 blockLink blockLinkSelf" title="' + enq.product_name + '">';
                        html += '  <p class="item">';
                        html += '    <a href="https://www.moratame.net/detail/mitsukeru.php?projectId=' + enq.id +'">';
                        html += '      <img style="width: 138px; height: 138px;" src="' + enq.product_image + '" height="320" width="320" alt="' + enq.product_name + '" class="item thumbnail" title="' + enq.product_name + '">';
                        html += '    </a>';
                        html += '  </p>';
                        html += '  <div>';
                        html += '  <p>' + enq.company + '</p>';
                        html += '  <p class="heightLine-group8"><a href="https://www.moratame.net/detail/mitsukeru.php?projectId=' + enq.id +'">' + enq.product_name + '</a></p>';
                        if(enq.id !== "DH0000001586" && enq.id !== "DH0000001603" && enq.id !== "DH0000001604" && enq.id !== "DH0000001605" && enq.id !== "DH0000001606" && enq.id !== "DH0000001612" && enq.id !== "DH0000001613" && enq.id !== "DH0000001614" && enq.id !== "DH0000001615" && enq.id !== "DH0000001616" && enq.id !== "DH0000001617" && enq.id !== "DH0000001618" && enq.id !== "DH0000001619" && enq.id !== "DH0000001620" && enq.id !== "DH0000001621" && enq.id !== "DH0000001622" && enq.id !== "DH0000001629"){
                        html += '  <p class="count">募集人数あと<span class="c_red">' + disp_count + '</span>名</p>';
                      }
                        html += icon;
                        html += '  </div>';
                        html += '</div>';
                        $("#find-item").append(html);

                        x++;
                    }
//                    $("#mitsukeru").append('</div></div></div>');
                    $("#mitsukeru").append('<!-- btnGroup --><div><p class="ta_c fz_14">今参加できる商品は <span class="c_red">' + data.count + '</span> 件</p><p class="w-60per-c btnSquare01wrap"><a href="https://www.moratame.net/detail/mitsukerulist.php" class="btnSquare01 btnBeige01 medium">お店でミツける商品一覧<span class="arrow"><img src="https://image.moratame.net/img2014/icons/arrow03.png" alt="" width="20" height="32" title=""></span></a></p></div><!-- /btnGroup -->');
                }
            }
        },
        error: function(XMLHttpRequest, textStatus, errorThrown)
        {
//            alert('Error : ' + errorThrown);
        }
    });

    return false;
});

function nextDate(year,month,date){
    var todayTime = new Date(year,month-1,date).getTime();
    var nextDateTime = todayTime + 60*60*24*1000;
    nextDateTime = new Date(nextDateTime);
    return {
        year:nextDateTime.getFullYear() ,
        month:nextDateTime.getMonth()+1 ,
        date:nextDateTime.getDate()
    }
}
