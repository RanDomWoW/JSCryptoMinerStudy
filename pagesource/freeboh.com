            if (!('sMap' in window)){
                sMapInit('//loc.freeboh.com/a','')
            }else{
                $('.maprect').show();var c=window.sMap.getCenter();google.maps.event.trigger(window.sMap,'resize'); window.sMap.setCenter(c); getPOI(); $('#search-box').focus();
            }
        })
        .on('click',function(){
            $('.maprect').show();var c=window.sMap.getCenter();google.maps.event.trigger(window.sMap,'resize'); window.sMap.setCenter(c); getPOI(); $('#search-box').focus();
        });
    if (matchMedia) {
            var mq = window.matchMedia("(max-width: 479px)");
            mq.addListener(WidthChange);
            WidthChange(mq);
    }
    function WidthChange(mq) {
        $('.search-form').insertAfter(mq.matches?'.btn-navbar':'.search-wrapper');
    }
    $('#where_ac').on('keyup',function(){
        $('#search-box').val($('#where_ac').val());
    });
    $('#search-box').on('keyup',function(){
        $('#where_ac').val($('#search-box').val());
