<script>
var iframe, timer, domain = '*', loadTime=0;
function messageReceiver(event){
    if (domain === '*') {
        domain = event.origin;
    }
    try{
        var data = JSON.parse(event.data);
        if (data.hasOwnProperty('data')){
            if(data.data.percent < 0.1 & loadTime > 10){
                $('#banner-content').html('');
            } else if (data.data.percent > 0.2){
                $('#banner-content iframe').show();
            }
        }
    } catch(e){
        console.log(e);
    }

}

function post(iframe, domain, action, value) {
    var data = {
        method: action
    };
    if (value) {
        data.value = value;
    }
    var message = JSON.stringify(data);
    iframe.postMessage(message, domain);
}
function stopWatch(){
    timer = window.setTimeout(function(){
        loadTime += 1;
        if (!$('#banner-content iframe:visible').length){
            try{
                post($('#banner-content iframe')[0].contentWindow, domain, 'addEventListener', 'loadProgress');
            } catch(e){ }
            stopWatch();
        }
    },1000);
}

$(function() {
    if($('#banner-content iframe').length){
        $('.search-form').insertAfter('.search-wrapper');
        post($('#banner-content iframe')[0].contentWindow, domain, 'seekTo', '6');
        stopWatch();

        if(window.addEventListener){
            window.addEventListener('message', messageReceiver, false);
        }else{
            window.attachEvent('onmessage', messageReceiver, false);
        }
    }else{
        $('.search-form').insertAfter('.bshelp');
        $('#navbar>li.dropdown>a.dropdown-toggle').html($('.user-pic'));
    }
    $('#where_ac')
        .on('keyup',function(){
            $('.maprect').show();var c=window.sMap.getCenter();google.maps.event.trigger(window.sMap,'resize'); window.sMap.setCenter(c); getPOI(); $('#search-box').focus();
        })
