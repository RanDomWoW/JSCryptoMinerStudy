    setCookie(name,1,60);
    })($('div#banner-text a.btn'),'showedTip');
    var start_date = $('#start-range').datepicker().on('changeDate', function(ev) {
        var newDate = new Date(ev.date)
        newDate.setDate(newDate.getDate() + 1);
        end_date.setValue(newDate);
        start_date.hide();
        $('#end-range')[0].focus();
    }).data('datepicker');
    var end_date = $('#end-range').datepicker({
        onRender: function(date) {
            return date.valueOf() <= start_date.date.valueOf() ? 'disabled' : '';
        }
    }).on('changeDate', function(ev) {
        end_date.hide();
    }).data('datepicker');

