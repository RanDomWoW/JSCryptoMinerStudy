<script>
	function window_hashchange(){
		switch(true){
			case window.location.hash == '#' || window.location.hash == '':
				console.log('#');
				document.body.setAttribute('data-hash','');

			break;
			case window.location.hash == '#edm_subscribe':
				console.log('edm_subscribe');
				document.body.setAttribute('data-hash',window.location.hash.split('#')[1]);
				document.getElementById('burger_check').checked = false;
			break;
		}
	}
	window.onhashchange = window_hashchange;
	window_hashchange();
	function close_lightbox(){
		window.location.hash = '';
	}

    function chk_edm_confirm() {
        var re = true;
        err_msg = '';
        if (re) {
            re = isemail("edm_mail_input");
        }
        if (!re) {
            alert(err_msg)
            return false;
        }
        return true;
    }

    function edm_ajax(type) {
        $.ajax({
            url: "do/edmdo.php",
            type: "POST",
            dataType: "json",
            data: {
                email: $('#edm_mail_input').val(),
                type:type
            },
            success: function (result) {
                if (result.state == true) {
                    if(result.type == '1'){
                        alert('訂閱成功!謝謝!');
                    }
                    else if(result.type == '2'){
                        alert('已取消訂閱!謝謝!');
                    }
                    if(result.type == '3'){
                    	alert(result.msg);
                    }
                    location.reload();

                }
                else {
                    alert(result.msg);
                }
            },
            error: function () {
                alert('資料傳送錯誤，請再試一次!!');
            }
        });
    }

	function edm_confirm(){
	    if(chk_edm_confirm()) {
            edm_ajax('1');
        }
	}
	function edm_cancel(){
        if(chk_edm_confirm()) {
            edm_ajax('2');
        }
	}
</script>
</body>
</html>