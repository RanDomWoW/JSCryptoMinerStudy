<script>
	function footer_search_change(device) {

		if(document.getElementById('footer_search_m').value.replace(/\s/g, '') ==''){
		    document.getElementById('footer_search_link').setAttribute('href','#');
			document.getElementById('footer_search_link').style.display = 'none';
		} else {
			document.getElementById('footer_search_link').setAttribute('href','search.php?keywords='+(isIE?encodeURIComponent(document.getElementById('footer_search_'+device).value):document.getElementById('footer_search_'+device).value));
			document.getElementById('footer_search_link').style.display = 'block';
		}

	}
	footer_search_change('m');
	footer_search_change('pc');
	function footer_search_click() {
		alert('請輸入搜尋關鍵字');
		trackEvent('footer','keyword_please');
	}
	function footer_search_keydown(event,device){
		if(event.keyCode == 13){
			if(document.getElementById('footer_search_m').value.replace(/\s/g, '') ==''){
			    document.getElementById('footer_search_link').setAttribute('href','#');
				document.getElementById('footer_search_link').style.display = 'none';
				trackEvent('footer','keyword_please');
				alert('請輸入搜尋關鍵字');
			} else {
				document.getElementById('footer_search_link').setAttribute('href','search.php?keywords='+(isIE?encodeURIComponent(document.getElementById('footer_search_'+device).value):document.getElementById('footer_search_'+device).value));
				document.getElementById('footer_search_link').style.display = 'block';
		        trackEvent('footer','search_enter');
				window.location.href = 'search.php?keywords='+(isIE?encodeURIComponent(document.getElementById('footer_search_'+device).value):document.getElementById('footer_search_'+device).value);
			}
		}
	}
</script>
