<script type="text/javascript">
    $.validate({
      modules : 'security'
    });
    refreshCaptcha=function(){
      $('.captcha_refresh').hide();
      $.ajax({
        type: "POST",
        url: "/engine/modules/captcha.php",
        data: "action=refresh_captcha",
        success: function(msg){
          console.log('p'+msg+'p');
          $('#captcha').html(msg);
          $('.captcha_refresh').show();
        }
      });
      return false;
    }
    testmd=function(){
      var input_hash=md5($('.captcha_input').val());
      var result_hash=$('#captcha_string').val();
      if(input_hash==result_hash){
        return true;
      }
      else{
        return false;
      }
    }
</script>


