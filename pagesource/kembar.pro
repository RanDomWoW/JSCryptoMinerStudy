}
.nav-menu2 a {
display:block;
padding:0 15px;
}
.nav-menu2 li {
position: relative;
margin:0 0;
}
.nav-menu2 > li {
float: left;
}
.nav-menu2 > li > a {
display: block;
height:50px;
line-height:50px;
color:#ffffff;
box-shadow: 0 4px 0 #434343 inset;
}
.nav-menu2 > li > a.active {
background:#434343;
box-shadow: 0 2px 0 #434343 inset;
}
.nav-menu2 > li:hover > a {
background:#131c25;
box-shadow: 0 2px 0 #087275 inset;
color:#ffffff;text-decoration:none;
}
}
.nav-menu2 li ul {
background:#fff;
display:block;
position:absolute;
left:0;
z-index:10;
visibility:hidden;
opacity:0;
-webkit-transition:all .25s ease-out;
-moz-transition:all .25s ease-out;
-ms-transition:all .25s ease-out;
-o-transition:all .25s ease-out;
transition:all .25s ease-out;
