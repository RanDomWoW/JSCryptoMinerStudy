      <script type="text/javascript">
       /*<![CDATA[*/
	   function share_click(greet_id, type) {
		   $.get('/api/share_click/'+'?greet_id=' + greet_id +'&sh='+type);
	   }
	  function show_qr(img_url) {
		  $('#qr_code_img').attr('src', img_url);
		  $('#qr_code_div').show('slow');
	  }
	  var timeOutId = 0;
	  var ajaxFn = function () {
	  }

	  function show_voice_form(url){
		  $('#voice_form').show();

		  $.get(url, function( data ) {
			  $('#voice_form').html(data);
		  });
	  }

	  function show_vk_form(greet_id){
		  $('#id_greet').val(greet_id);
		  $('.vk_form').show();
		  $('#vk_form').show();
		  $('#vk_sended').hide();
		  $('#vk_error').html('');
		  $('#vk_sended_good').hide();

	  }

	  $("#vk_form").submit(function(e) {

		  var url = "/api/send-vk-message/";

		  $.ajax({
			  type: "POST",
			  url: url,
			  data: $("#vk_form").serialize(), // serializes the form's elements.
			  success: function(data)
			  {
				  //data = JSON.parse(data);

				  if (data['success']) {
					  $('#vk_form').hide();
					  $('#vk_sended').show();
					  $('#vk_error').html('');
					  $.ajax({
						  url: '/api/check-vk-message/?mid=' + data['success'],
						  success: function (response) {
							  if (response['result'] == true) {//YAYA
								  $('#vk_sended').hide();
								  $('#vk_sended_good').show();
							  } else {//Fail check?
								  alert('Ошибка отправки, возможно у пользователя закрыта личка')
							  }
						  }
					  });


					  
				  } else {
					  error = data['error']
					  for (var key in error) {
							  $('#vk_error').html(error[key][0]);
					  }
				 }

			  }
          });

		  e.preventDefault(); // avoid to execute the actual submit of the form.
	  });
	  

