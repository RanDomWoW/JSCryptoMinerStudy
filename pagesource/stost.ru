      function v(g_id, v) {
		  var vote_url = "/vote/"+g_id+"?v="+v;
		  var v_id = "#vote-"+g_id;
		  $(v_id).html('<div><img src="/static/images/spinner.gif" /><br /></div>');
		  $.ajax({
			  url:vote_url,
			  dataType:'json',
			  success:function (data,b,c) {
				  if (data['success'] == '0') {
					  $('#already-'+g_id).show();
					  $('#already-'+g_id).hide(3500);
					  $(v_id).html(data['vote']+"%");
				  }
				  if (data['success'] == '1') {
					  $(v_id).html(data['vote']+"%");
					  $('#total-'+g_id).html(data['total']);
				  }
			  },
			  error:function(a,b,c){
				  console.log('error:',a,b,c);
			  }
		  });
      }
