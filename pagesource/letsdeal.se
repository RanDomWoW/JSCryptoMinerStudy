<script type="text/javascript">
    var pulse2opt = {
		respectDoNotTrack: true,
		clientId: 'letsdeal.se',
		userIdDomain: 'letsdeal.se',
		contentId: '/',
		contentType: 'Listing',
		category: 'start > alla',
		object: {"locality":null,"location":"start"}
	};

    (function () {
        var d = document, g = d.createElement('script'), s = d.getElementsByTagName('script')[0];
        g.src = "//jssdk.pulse.schibsted.com/autoTracker.min.js";
        s.parentNode.insertBefore(g, s);
    })();

    function pulseTrack(type) {
        if (typeof AutoTrack == 'object') {
            var activity = AutoTrack.activity();

            if (type == 'multibuy') {
                if (!activity.opts['object']) {
                    activity.opts['object'] = {};
                }
                var prices = [];
                $('.multibuy-price').each(
                    function () {
                        prices.push(parseInt($(this)[0].innerHTML));
                    }
                );
                activity.opts['object'].prices = prices;
                activity.events.trackContentView('Price popup').send();
            }
        }
    }
</script>


