            $(function(){

                admin_sidebar_checker();
                setInterval(admin_sidebar_checker, 1000*60*5);

                function admin_sidebar_checker() {

                    $.ajax({
                        type: "GET",
                        url: '//smak.berlin.bigmir.net/?_tpl=admin_sidebar&admin_sidebar_data=1&ajax=1&r='+Math.random(),
                        xhrFields: {
                            withCredentials: true
                        },
                        dataType: "json",
                        crossDomain: true,
                        success : function(data) {

                            var $bms = data['BMS'];
                            var $bmps = data['BMPS'];
                            var bms = $.cookie('BMS');
                            var bmps = $.cookie('BMPS');
                            if ( (document.location.host == 'smak.ua') &&  $bms!='' && $bms!='undefined' && $bms != undefined && bms != $bms) {
                                $.cookie('BMS', $bms, { expires: 1, domain:'.smak.ua', path: '/' });
                                console.log('need reload!');
                                //document.location.reload();
                            }

                            if ( (document.location.host == 'smak.ua') && $bmps!='' && $bmps!='undefined' && $bmps != undefined && bmps != $bmps) {
                                $.cookie('BMPS', $bmps, { expires: 1, domain:'.smak.ua', path: '/' });
                            }

                            if (bms == 'undefined' || bms == '') $.cookie('BMS', null);
                            if (bmps == 'undefined' || bmps == '') $.cookie('BMPS', null);
                        }
                    });
                }

            });
		</script>
		
		
			
	
	

	

