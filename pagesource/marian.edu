}

.herocontentimage {
    margin-bottom: .5em;
}

.herosummary {
    font-size: 2em;
    margin-bottom: 1em;
    color: white;
    line-height: 1.2em;
    font-family: copse;
}

.heretagline {
    text-align: right;
    position: absolute;
    bottom: 0;
    right: 0;
}


