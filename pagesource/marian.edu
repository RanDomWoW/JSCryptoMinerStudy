}

.bgimagecover {
    background-repeat: no-repeat;
    background-position: center top;
    background-size: cover;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.herocontent {
    position: relative;
    min-height: 420px;
}

.herotitle {
    color: white;
    line-height: 1.1;
    text-shadow: 0 0 10px #000000;
