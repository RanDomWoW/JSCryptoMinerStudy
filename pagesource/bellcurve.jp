<!--トップへ戻るボタン-->
$(function(){
    $(window).scroll(function(){
    //最上部から現在位置までの距離を取得して、変数[now]に格納
    var now = $(window).scrollTop();
 
    if(now > 400){
      //[#pagetop]をゆっくりフェードインする
      $('#pagetop').fadeIn('slow');
    //それ以外だったらフェードアウトする
    }else{
      $('#pagetop').fadeOut('slow');
    }
  });

$('#move-page-top').click(function(){
  //ページトップへ移動する
  $('html,body').animate({scrollTop:0},'slow');
});
});
