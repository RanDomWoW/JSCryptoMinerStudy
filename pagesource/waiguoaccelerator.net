        $mainNav.removeClass('js-hide');
        $navEvent.addClass('js-hide');
      }

      // Toggle control for main nav
      $toggler.on('click', function(e) {
        e.preventDefault();
        $mainNav.toggleClass('js-hide').toggleClass('is-active');

        if ( $utilNav.is(':visible') ) {
          $utilNav.toggleClass('js-hide').toggleClass('is-active');
          $mainNav.toggleClass('js-hide').removeClass('is-active');
        }
      });

      // More and Back Links
      $navEvent.on('click', function(e) {
        e.preventDefault();
        $mainNav.toggleClass('js-hide');
        $utilNav.toggleClass('js-hide').toggleClass('is-active');
      })

      // Resize window controls
      $(window).resize( function(){
        if ( $toggler.is(':hidden') ) {
          $mainNav.removeClass('js-hide');
          $utilNav.addClass('js-hide');
          $navEvent.addClass('js-hide');
        } else if ( $toggler.is(':visible') ) { 
          $mainNav.addClass('js-hide');
          $navEvent.removeClass('js-hide');
        }
      });
      
      // mark current page parent nav item
      var $subnav = $('#product-nav-main'),
